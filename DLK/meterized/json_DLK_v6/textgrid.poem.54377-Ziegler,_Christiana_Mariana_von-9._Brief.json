{"textgrid.poem.54377": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "9. Brief", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verwundre dich nur nicht, wo fern du meine Zeilen,", "tokens": ["Ver\u00b7wund\u00b7re", "dich", "nur", "nicht", ",", "wo", "fern", "du", "mei\u00b7ne", "Zei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "$,", "PWAV", "ADJD", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So dir die Antwort hier, wozu du mich verbindst,", "tokens": ["So", "dir", "die", "Ant\u00b7wort", "hier", ",", "wo\u00b7zu", "du", "mich", "ver\u00b7bindst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "ADV", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hochwohlgebohrne Frau, auf deinen Brief ertheilen,", "tokens": ["Hoch\u00b7wohl\u00b7ge\u00b7bohr\u00b7ne", "Frau", ",", "auf", "dei\u00b7nen", "Brief", "er\u00b7thei\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verwirrt, vom schlechtem Klang, und recht gezwungen findst.", "tokens": ["Ver\u00b7wirrt", ",", "vom", "schlech\u00b7tem", "Klang", ",", "und", "recht", "ge\u00b7zwun\u00b7gen", "findst", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPRART", "ADJA", "NN", "$,", "KON", "ADJD", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein Blat mu\u00df sich vor dir und deiner G\u00fcte sch\u00e4men,", "tokens": ["Mein", "Blat", "mu\u00df", "sich", "vor", "dir", "und", "dei\u00b7ner", "G\u00fc\u00b7te", "sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PRF", "APPR", "PPER", "KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nicht weniger als ich! ich werde feuerroth;", "tokens": ["Nicht", "we\u00b7ni\u00b7ger", "als", "ich", "!", "ich", "wer\u00b7de", "feu\u00b7er\u00b7roth", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KOUS", "PPER", "$.", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Furcht will mit Gewalt mir Hand und Finger l\u00e4hmen,", "tokens": ["Die", "Furcht", "will", "mit", "Ge\u00b7walt", "mir", "Hand", "und", "Fin\u00b7ger", "l\u00e4h\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "NN", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die mir mit deinem Zorn und Eifer billig droht,", "tokens": ["Die", "mir", "mit", "dei\u00b7nem", "Zorn", "und", "Ei\u00b7fer", "bil\u00b7lig", "droht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "KON", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dein Vorwurf ist gerecht, ich mu\u00df es selbst bekennen,", "tokens": ["Dein", "Vor\u00b7wurf", "ist", "ge\u00b7recht", ",", "ich", "mu\u00df", "es", "selbst", "be\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Weil mein Versprechen nicht von mir erf\u00fcllet ward.", "tokens": ["Weil", "mein", "Ver\u00b7spre\u00b7chen", "nicht", "von", "mir", "er\u00b7f\u00fcl\u00b7let", "ward", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "APPR", "PPER", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Du kannst, und zwar mit Recht, mich deinen Schuldner nennen;", "tokens": ["Du", "kannst", ",", "und", "zwar", "mit", "Recht", ",", "mich", "dei\u00b7nen", "Schuld\u00b7ner", "nen\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KON", "ADV", "APPR", "NN", "$,", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Jedoch dein Argwohn ist ganz falsch und klingt zu hart:", "tokens": ["Je\u00b7doch", "dein", "Arg\u00b7wohn", "ist", "ganz", "falsch", "und", "klingt", "zu", "hart", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "KON", "VVFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Du glaubst, ich h\u00e4tte dich die ganze Zeit vergessen,", "tokens": ["Du", "glaubst", ",", "ich", "h\u00e4t\u00b7te", "dich", "die", "gan\u00b7ze", "Zeit", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und der Gedanken Lauf nach andern hingericht,", "tokens": ["Und", "der", "Ge\u00b7dan\u00b7ken", "Lauf", "nach", "an\u00b7dern", "hin\u00b7ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "APPR", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Seit dem du weit entfernt in Carols Bad gesessen;", "tokens": ["Seit", "dem", "du", "weit", "ent\u00b7fernt", "in", "Ca\u00b7rols", "Bad", "ge\u00b7ses\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "ADJD", "APPR", "NE", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wie f\u00e4llt dir dieses ein? O Freundinn! Glaub es nicht,", "tokens": ["Wie", "f\u00e4llt", "dir", "die\u00b7ses", "ein", "?", "O", "Freun\u00b7dinn", "!", "Glaub", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PDAT", "PTKVZ", "$.", "NE", "NN", "$.", "NN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Es ist gewi\u00df kein Tag, ich schwer es dir, verstrichen,", "tokens": ["Es", "ist", "ge\u00b7wi\u00df", "kein", "Tag", ",", "ich", "schwer", "es", "dir", ",", "ver\u00b7stri\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "$,", "PPER", "ADJD", "PPER", "PPER", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Mein Herze muste dir auf jeden Tritt nachgehn,", "tokens": ["Mein", "Her\u00b7ze", "mus\u00b7te", "dir", "auf", "je\u00b7den", "Tritt", "nach\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Bey deiner Lust bin ich niemals von dir gewichen,", "tokens": ["Bey", "dei\u00b7ner", "Lust", "bin", "ich", "nie\u00b7mals", "von", "dir", "ge\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Und muste dir im Geist stets an der Seite stehn.", "tokens": ["Und", "mus\u00b7te", "dir", "im", "Geist", "stets", "an", "der", "Sei\u00b7te", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPRART", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So viel mal ich an dich und deine Cur gedachte,", "tokens": ["So", "viel", "mal", "ich", "an", "dich", "und", "dei\u00b7ne", "Cur", "ge\u00b7dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PPER", "APPR", "PPER", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So viel mal flog zu gleich ein frommer Wunsch nach dir,", "tokens": ["So", "viel", "mal", "flog", "zu", "gleich", "ein", "from\u00b7mer", "Wunsch", "nach", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "APPR", "ADV", "ART", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Den dir an meiner statt ein Seufzer \u00fcberbrachte,", "tokens": ["Den", "dir", "an", "mei\u00b7ner", "statt", "ein", "Seuf\u00b7zer", "\u00fc\u00b7berb\u00b7rach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und diesen wiederholt auch noch die Feder hier.", "tokens": ["Und", "die\u00b7sen", "wie\u00b7der\u00b7holt", "auch", "noch", "die", "Fe\u00b7der", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADV", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Doch soll ich dir im Ernst und offenherzig sagen", "tokens": ["Doch", "soll", "ich", "dir", "im", "Ernst", "und", "of\u00b7fen\u00b7her\u00b7zig", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PPER", "APPRART", "NN", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Was wohl die wahre Schuld an meinem Schweigen sey,", "tokens": ["Was", "wohl", "die", "wah\u00b7re", "Schuld", "an", "mei\u00b7nem", "Schwei\u00b7gen", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So kannst du dich mit Recht nicht \u00fcber mich beklagen,", "tokens": ["So", "kannst", "du", "dich", "mit", "Recht", "nicht", "\u00fc\u00b7ber", "mich", "be\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPR", "NN", "PTKNEG", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Leg alle meine Schuld blo\u00df dem Verh\u00e4ngni\u00df bey;", "tokens": ["Leg", "al\u00b7le", "mei\u00b7ne", "Schuld", "blo\u00df", "dem", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "bey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "PPOSAT", "NN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Dies risse leider! mir die Nachricht aus den H\u00e4nden,", "tokens": ["Dies", "ris\u00b7se", "lei\u00b7der", "!", "mir", "die", "Nach\u00b7richt", "aus", "den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$.", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "In welcher Gegend du dort deine Wohnung n\u00e4hmst;", "tokens": ["In", "wel\u00b7cher", "Ge\u00b7gend", "du", "dort", "dei\u00b7ne", "Woh\u00b7nung", "n\u00e4hmst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wie konnt ich dir also ein Bl\u00e4tchen \u00fcbersenden,", "tokens": ["Wie", "konnt", "ich", "dir", "al\u00b7so", "ein", "Bl\u00e4t\u00b7chen", "\u00fc\u00b7ber\u00b7sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So da\u00df du es gewi\u00df am rechten Ort bek\u00e4mst?", "tokens": ["So", "da\u00df", "du", "es", "ge\u00b7wi\u00df", "am", "rech\u00b7ten", "Ort", "be\u00b7k\u00e4mst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PPER", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Drum la\u00df doch den von mir gesch\u00f6pften Argwohn schwinden,", "tokens": ["Drum", "la\u00df", "doch", "den", "von", "mir", "ge\u00b7sch\u00f6pf\u00b7ten", "Arg\u00b7wohn", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "ART", "APPR", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Als w\u00e4r ich etwan nur ein solcher Flattergeist,", "tokens": ["Als", "w\u00e4r", "ich", "et\u00b7wan", "nur", "ein", "sol\u00b7cher", "Flat\u00b7ter\u00b7geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "ADV", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Der sich zu etwas bald und leicht pflegt zu verbinden,", "tokens": ["Der", "sich", "zu", "et\u00b7was", "bald", "und", "leicht", "pflegt", "zu", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PTKA", "PIS", "ADV", "KON", "ADJD", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Im Halten aber tr\u00e4g und ganz vergessen hei\u00dft:", "tokens": ["Im", "Hal\u00b7ten", "a\u00b7ber", "tr\u00e4g", "und", "ganz", "ver\u00b7ges\u00b7sen", "hei\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "KON", "ADV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ich weis, was ich an dir f\u00fcr eine Freundinn liebe;", "tokens": ["Ich", "weis", ",", "was", "ich", "an", "dir", "f\u00fcr", "ei\u00b7ne", "Freun\u00b7dinn", "lie\u00b7be", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "PWS", "PPER", "APPR", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Dein Umgang hat mir gleich im Anfang dargethan", "tokens": ["Dein", "Um\u00b7gang", "hat", "mir", "gleich", "im", "An\u00b7fang", "dar\u00b7ge\u00b7than"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Da\u00df man dich unverf\u00e4lscht, und deine Freundschaftstriebe", "tokens": ["Da\u00df", "man", "dich", "un\u00b7ver\u00b7f\u00e4lscht", ",", "und", "dei\u00b7ne", "Freund\u00b7schaft\u00b7strie\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "PRF", "ADJD", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Recht rein und ungef\u00e4rbt, dir \u00e4hnlich nennen kann.", "tokens": ["Recht", "rein", "und", "un\u00b7ge\u00b7f\u00e4rbt", ",", "dir", "\u00e4hn\u00b7lich", "nen\u00b7nen", "kann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$,", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wer der von A \u2013 \u2013 zur Hand und Seite sitzet,", "tokens": ["Wer", "der", "von", "A", "\u2013", "\u2013", "zur", "Hand", "und", "Sei\u00b7te", "sit\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "APPR", "NE", "$(", "$(", "APPRART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Der sieht, wofern er nur ein Aug im Kopfe tr\u00e4gt,", "tokens": ["Der", "sieht", ",", "wo\u00b7fern", "er", "nur", "ein", "Aug", "im", "Kop\u00b7fe", "tr\u00e4gt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Da\u00df eitel Redlichkeit aus ihrer Stirne blitzet,", "tokens": ["Da\u00df", "ei\u00b7tel", "Red\u00b7lich\u00b7keit", "aus", "ih\u00b7rer", "Stir\u00b7ne", "blit\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Und da\u00df ihr Herze nie List und Verstellung hegt.", "tokens": ["Und", "da\u00df", "ihr", "Her\u00b7ze", "nie", "List", "und", "Ver\u00b7stel\u00b7lung", "hegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ADV", "NE", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.45": {"text": "Man mu\u00df sie wirklich mit zu Biederfrauen stellen,", "tokens": ["Man", "mu\u00df", "sie", "wirk\u00b7lich", "mit", "zu", "Bie\u00b7der\u00b7frau\u00b7en", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADJD", "APPR", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Dies Wort geh\u00f6ret euch, ihr M\u00e4nner, nicht allein,", "tokens": ["Dies", "Wort", "ge\u00b7h\u00f6\u00b7ret", "euch", ",", "ihr", "M\u00e4n\u00b7ner", ",", "nicht", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Ihr \u00e4cht gesinnter Geist, ha\u00dft aller Falschheit Quellen,", "tokens": ["Ihr", "\u00e4cht", "ge\u00b7sinn\u00b7ter", "Geist", ",", "ha\u00dft", "al\u00b7ler", "Falschheit", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "NN", "$,", "VVFIN", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.48": {"text": "Den Worten mu\u00df die That stets gleich gestimmet seyn.", "tokens": ["Den", "Wor\u00b7ten", "mu\u00df", "die", "That", "stets", "gleich", "ge\u00b7stim\u00b7met", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "ADV", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "O! solche Freunde soll man hoch und heilig halten,", "tokens": ["O", "!", "sol\u00b7che", "Freun\u00b7de", "soll", "man", "hoch", "und", "hei\u00b7lig", "hal\u00b7ten", ","], "toke