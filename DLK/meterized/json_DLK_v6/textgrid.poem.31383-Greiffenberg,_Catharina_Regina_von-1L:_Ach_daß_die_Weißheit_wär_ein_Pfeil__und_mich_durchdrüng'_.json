{"textgrid.poem.31383": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach da\u00df die Wei\u00dfheit w\u00e4r ein Pfeil / und mich durchdr\u00fcng' /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach da\u00df die Wei\u00dfheit w\u00e4r ein Pfeil / und mich durchdr\u00fcng' /", "tokens": ["Ach", "da\u00df", "die", "Wei\u00df\u00b7heit", "w\u00e4r", "ein", "Pfeil", "/", "und", "mich", "durch\u00b7dr\u00fcng'", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "ART", "NN", "VAFIN", "ART", "NN", "$(", "KON", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ein glantz und mich erhellt'; ein wasser / und mich tr\u00e4nkte /", "tokens": ["ein", "glantz", "und", "mich", "er\u00b7hellt'", ";", "ein", "was\u00b7ser", "/", "und", "mich", "tr\u00e4nk\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPER", "VVFIN", "$.", "ART", "ADJA", "$(", "KON", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ein abgrunds-tieff' / und sie mich ganz in sie versenkte /", "tokens": ["ein", "ab\u00b7grunds\u00b7tief\u00b7f'", "/", "und", "sie", "mich", "ganz", "in", "sie", "ver\u00b7senk\u00b7te", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KON", "PPER", "PRF", "ADV", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+--+-+---+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "ein Adler / der mit mir sich zu der Sonne schw\u00fcng:", "tokens": ["ein", "Ad\u00b7ler", "/", "der", "mit", "mir", "sich", "zu", "der", "Son\u00b7ne", "schw\u00fcng", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "APPR", "PPER", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "ein helle Quell' / so in die Sinnen rinnend spr\u00fcng'!", "tokens": ["ein", "hel\u00b7le", "Quell'", "/", "so", "in", "die", "Sin\u00b7nen", "rin\u00b7nend", "spr\u00fcng'", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ADV", "APPR", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ach! da\u00df den Kunst- Geist sie mir aller Wei\u00dfen schenkte!", "tokens": ["Ach", "!", "da\u00df", "den", "Kunst", "Geist", "sie", "mir", "al\u00b7ler", "Wei\u00b7\u00dfen", "schenk\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "ART", "TRUNC", "NN", "PPER", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "da\u00df nur was w\u00fcrdigs ich zu Gottes Lob erdenkte", "tokens": ["da\u00df", "nur", "was", "w\u00fcr\u00b7digs", "ich", "zu", "Got\u00b7tes", "Lob", "er\u00b7denk\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PWS", "VAFIN", "PPER", "APPR", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und seiner Wunder Prei\u00df nach wunsch durch mich erkling!", "tokens": ["und", "sei\u00b7ner", "Wun\u00b7der", "Prei\u00df", "nach", "wunsch", "durch", "mich", "er\u00b7kling", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "APPR", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich such' je nicht mein Lob / die selbst-Ehr sey verflucht!", "tokens": ["Ich", "such'", "je", "nicht", "mein", "Lob", "/", "die", "selbst\u00b7Ehr", "sey", "ver\u00b7flucht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "PPOSAT", "NN", "$(", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Gott! Gott! Gott! ist der Zweck / den ihm mein kiel erkohren.", "tokens": ["Gott", "!", "Gott", "!", "Gott", "!", "ist", "der", "Zweck", "/", "den", "ihm", "mein", "kiel", "er\u00b7koh\u00b7ren", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$.", "VAFIN", "ART", "NN", "$(", "ART", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich bin der Pinsel nur: sein Hand mahlt selbst die Frucht;", "tokens": ["Ich", "bin", "der", "Pin\u00b7sel", "nur", ":", "sein", "Hand", "mahlt", "selbst", "die", "Frucht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$.", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ihr zimt die Ehr / wird was aus meinen Sinn gebohren.", "tokens": ["Ihr", "zimt", "die", "Ehr", "/", "wird", "was", "aus", "mei\u00b7nen", "Sinn", "ge\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "VAFIN", "PIS", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Aus Gottes trieb kan ja kein Teuffels Laster fliessen.", "tokens": ["Aus", "Got\u00b7tes", "trieb", "kan", "ja", "kein", "Teuf\u00b7fels", "Las\u00b7ter", "flies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "VMFIN", "ADV", "PIAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "mein einigs flugziel ist / zu Jesus Christus F\u00fcssen!", "tokens": ["mein", "ei\u00b7nigs", "flug\u00b7ziel", "ist", "/", "zu", "Je\u00b7sus", "Chris\u00b7tus", "F\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "$(", "APPR", "NE", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}