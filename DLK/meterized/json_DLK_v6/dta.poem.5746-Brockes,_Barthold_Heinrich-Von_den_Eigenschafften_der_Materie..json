{"dta.poem.5746": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Eigenschafften der  \n Materie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn wir nun aber recht, und wie wir sollen,", "tokens": ["Wenn", "wir", "nun", "a\u00b7ber", "recht", ",", "und", "wie", "wir", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "$,", "KON", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von ", "tokens": ["Von"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "So mu\u00df man nicht auf das, was sich ver\u00e4ndert, achten,", "tokens": ["So", "mu\u00df", "man", "nicht", "auf", "das", ",", "was", "sich", "ver\u00b7\u00e4n\u00b7dert", ",", "ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PTKNEG", "APPR", "ART", "$,", "PRELS", "PRF", "VVPP", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Man mu\u00df dieselbe nicht betrachten", "tokens": ["Man", "mu\u00df", "die\u00b7sel\u00b7be", "nicht", "be\u00b7trach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PDAT", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nach einem Zufall, welcher fl\u00fcchtig:", "tokens": ["Nach", "ei\u00b7nem", "Zu\u00b7fall", ",", "wel\u00b7cher", "fl\u00fcch\u00b7tig", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es wird, von unsrer Geister Krafft,", "tokens": ["Es", "wird", ",", "von", "uns\u00b7rer", "Geis\u00b7ter", "Krafft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht die Materie erkennet", "tokens": ["Nicht", "die", "Ma\u00b7te\u00b7rie", "er\u00b7ken\u00b7net"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Durch das, was man die ", "tokens": ["Durch", "das", ",", "was", "man", "die"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDS", "$,", "PRELS", "PIS", "ART"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "In C\u00f6rpern nennet,", "tokens": ["In", "C\u00f6r\u00b7pern", "nen\u00b7net", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+---", "measure": "dactylic.init"}, "line.10": {"text": "Kalt, Trocken, Warm, Weich, Hart und Feucht,", "tokens": ["Kalt", ",", "Tro\u00b7cken", ",", "Warm", ",", "Weich", ",", "Hart", "und", "Feucht", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "ADJD", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Geruch, Geschmack und Farben, Schwer und Leicht.", "tokens": ["Ge\u00b7ruch", ",", "Ge\u00b7schmack", "und", "Far\u00b7ben", ",", "Schwer", "und", "Leicht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Sie wird von uns allein erkannt", "tokens": ["Sie", "wird", "von", "uns", "al\u00b7lein", "er\u00b7kannt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Dadurch blo\u00df, da\u00df sie ", "tokens": ["Da\u00b7durch", "blo\u00df", ",", "da\u00df", "sie"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PAV", "ADV", "$,", "KOUS", "PPER"], "meter": "--+-+", "measure": "anapaest.init"}}}}}