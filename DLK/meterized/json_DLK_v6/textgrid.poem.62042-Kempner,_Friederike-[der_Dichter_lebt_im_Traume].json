{"textgrid.poem.62042": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[der Dichter lebt im Traume]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Dichter lebt im Traume,", "tokens": ["Der", "Dich\u00b7ter", "lebt", "im", "Trau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er spielt im Weltenraume", "tokens": ["Er", "spielt", "im", "Wel\u00b7ten\u00b7rau\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit Zeit und Ewigkeit \u2013", "tokens": ["Mit", "Zeit", "und", "E\u00b7wig\u00b7keit", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Verscherzet Gl\u00fcck und Zeit!", "tokens": ["Ver\u00b7scher\u00b7zet", "Gl\u00fcck", "und", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und wenn er nichts erzielet", "tokens": ["Und", "wenn", "er", "nichts", "er\u00b7zie\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PIS", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als das, was er gespielet,", "tokens": ["Als", "das", ",", "was", "er", "ge\u00b7spie\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "So ist's doch immer Viel,", "tokens": ["So", "ist's", "doch", "im\u00b7mer", "Viel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Denn wertvoll ist sein Spiel!", "tokens": ["Denn", "wert\u00b7voll", "ist", "sein", "Spiel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}