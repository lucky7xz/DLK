{"textgrid.poem.30434": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "48.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deine Charis ist der C\u00f6rper; du, Myrtillus, bist der Schaten;", "tokens": ["Dei\u00b7ne", "Cha\u00b7ris", "ist", "der", "C\u00f6r\u00b7per", ";", "du", ",", "Myr\u00b7til\u00b7lus", ",", "bist", "der", "Scha\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$.", "PPER", "$,", "NE", "$,", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wie wird aber, wann die Sonne geht zu Bette, dir gerathen?", "tokens": ["Wie", "wird", "a\u00b7ber", ",", "wann", "die", "Son\u00b7ne", "geht", "zu", "Bet\u00b7te", ",", "dir", "ge\u00b7ra\u00b7then", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "APPR", "NN", "$,", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "N\u00e4chte haben keinen Schaten; du hingegen gehst auff N\u00e4chte;", "tokens": ["N\u00e4ch\u00b7te", "ha\u00b7ben", "kei\u00b7nen", "Scha\u00b7ten", ";", "du", "hin\u00b7ge\u00b7gen", "gehst", "auff", "N\u00e4ch\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "$.", "PPER", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+--+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Weil die Nacht de\u00df Tages Schaten, k\u00fcmmstu so durch sie zu rechte.", "tokens": ["Weil", "die", "Nacht", "de\u00df", "Ta\u00b7ges", "Scha\u00b7ten", ",", "k\u00fcmm\u00b7stu", "so", "durch", "sie", "zu", "rech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "NN", "$,", "VVFIN", "ADV", "APPR", "PPER", "APPR", "ADJA", "$."], "meter": "--+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}}}}}