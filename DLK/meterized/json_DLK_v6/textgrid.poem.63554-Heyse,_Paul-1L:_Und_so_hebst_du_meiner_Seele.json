{"textgrid.poem.63554": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und so hebst du meiner Seele", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und so hebst du meiner Seele", "tokens": ["Und", "so", "hebst", "du", "mei\u00b7ner", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schleier mit der weichen Hand,", "tokens": ["Schlei\u00b7er", "mit", "der", "wei\u00b7chen", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sie nichts mehr dir verhehle,", "tokens": ["Da\u00df", "sie", "nichts", "mehr", "dir", "ver\u00b7heh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die err\u00f6tend vor dir stand.", "tokens": ["Die", "er\u00b7r\u00f6\u00b7tend", "vor", "dir", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, was ihr im \u00dcbermute", "tokens": ["Ach", ",", "was", "ihr", "im", "\u00dc\u00b7ber\u00b7mu\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWS", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieblich an ihr selber deucht',", "tokens": ["Lieb\u00b7lich", "an", "ihr", "sel\u00b7ber", "deucht'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seit darauf dein Auge ruhte,", "tokens": ["Seit", "da\u00b7rauf", "dein", "Au\u00b7ge", "ruh\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist der eitle Wahn verscheucht.", "tokens": ["Ist", "der", "eit\u00b7le", "Wahn", "ver\u00b7scheucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun entkleidet ihrer Flittern,", "tokens": ["Nun", "ent\u00b7klei\u00b7det", "ih\u00b7rer", "Flit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun so scheu in sich geschmiegt", "tokens": ["Nun", "so", "scheu", "in", "sich", "ge\u00b7schmiegt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "APPR", "PRF", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcberrieselt sie ein Zittern,", "tokens": ["\u00dc\u00b7berr\u00b7ie\u00b7selt", "sie", "ein", "Zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zwischen Gl\u00fcck und Scham gewiegt.", "tokens": ["Zwi\u00b7schen", "Gl\u00fcck", "und", "Scham", "ge\u00b7wiegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Bis sie sich mit heft'gem Triebe", "tokens": ["Bis", "sie", "sich", "mit", "heft'\u00b7gem", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dicht an ", "tokens": ["Dicht", "an"], "token_info": ["word", "word"], "pos": ["ADJD", "PTKVZ"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Und die F\u00fclle deiner Liebe", "tokens": ["Und", "die", "F\u00fcl\u00b7le", "dei\u00b7ner", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein Schleier sie umflie\u00dft.", "tokens": ["Wie", "ein", "Schlei\u00b7er", "sie", "um\u00b7flie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}