{"textgrid.poem.66278": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Die Thr\u00e4nen", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Thr\u00e4nenbilder, sei gepriesen,", "tokens": ["Thr\u00e4\u00b7nen\u00b7bil\u00b7der", ",", "sei", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcr die Thr\u00e4nen, die so s\u00fc\u00df", "tokens": ["F\u00fcr", "die", "Thr\u00e4\u00b7nen", ",", "die", "so", "s\u00fc\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von den Wangen niederflie\u00dfen,", "tokens": ["Von", "den", "Wan\u00b7gen", "nie\u00b7der\u00b7flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie der Thau im Paradies.", "tokens": ["Wie", "der", "Thau", "im", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn die hei\u00dfe Last der Qualen", "tokens": ["Wenn", "die", "hei\u00b7\u00dfe", "Last", "der", "Qua\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine m\u00fcde Seele dr\u00fcckt,", "tokens": ["Mei\u00b7ne", "m\u00fc\u00b7de", "See\u00b7le", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mein Aug' mit matten Strahlen", "tokens": ["Und", "mein", "Aug'", "mit", "mat\u00b7ten", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den Staub des Elends blickt;", "tokens": ["In", "den", "Staub", "des", "E\u00b7lends", "blickt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, so macht die Silberquelle,", "tokens": ["Ach", ",", "so", "macht", "die", "Sil\u00b7ber\u00b7quel\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Str\u00f6mend meinem Herzen Luft,", "tokens": ["Str\u00f6\u00b7mend", "mei\u00b7nem", "Her\u00b7zen", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mein Aug', von Thr\u00e4nen helle,", "tokens": ["Und", "mein", "Aug'", ",", "von", "Thr\u00e4\u00b7nen", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "APPR", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blickt hinauf durch Kerkerduft!", "tokens": ["Blickt", "hin\u00b7auf", "durch", "Ker\u00b7ker\u00b7duft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und den Himmel seh' ich wieder;", "tokens": ["Und", "den", "Him\u00b7mel", "seh'", "ich", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Engel schauen, d\u00e4ucht es mich,", "tokens": ["En\u00b7gel", "schau\u00b7en", ",", "d\u00e4ucht", "es", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVINF", "$,", "VVFIN", "PPER", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gnadel\u00e4chelnd auf mich nieder", "tokens": ["Gna\u00b7de\u00b7l\u00e4\u00b7chelnd", "auf", "mich", "nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "PPER", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mein Herz erleichtert sich.", "tokens": ["Und", "mein", "Herz", "er\u00b7leich\u00b7tert", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Oft sah ich in Thr\u00e4nenb\u00e4chen", "tokens": ["Oft", "sah", "ich", "in", "Thr\u00e4\u00b7nen\u00b7b\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gottes Sonne sch\u00f6n und mild", "tokens": ["Got\u00b7tes", "Son\u00b7ne", "sch\u00f6n", "und", "mild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich in tausend Strahlen brechen", "tokens": ["Sich", "in", "tau\u00b7send", "Strah\u00b7len", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "CARD", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und des Regenbogens Bild.", "tokens": ["Und", "des", "Re\u00b7gen\u00b7bo\u00b7gens", "Bild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach, da denk' ich, Gottes H\u00f6hen", "tokens": ["Ach", ",", "da", "denk'", "ich", ",", "Got\u00b7tes", "H\u00f6\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "$,", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Siehst du schon wie Stephanus!", "tokens": ["Siehst", "du", "schon", "wie", "Ste\u00b7pha\u00b7nus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KOKOM", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Siehst schon Jesum Christum stehen,", "tokens": ["Siehst", "schon", "Je\u00b7sum", "Chris\u00b7tum", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NE", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Giebst ihm schon den Trauungsku\u00df.", "tokens": ["Giebst", "ihm", "schon", "den", "Trau\u00b7ungs\u00b7ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wann ich im Gef\u00fchl der S\u00fcnde", "tokens": ["Wann", "ich", "im", "Ge\u00b7f\u00fchl", "der", "S\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Eine Thr\u00e4ne weinen kann,", "tokens": ["Ei\u00b7ne", "Thr\u00e4\u00b7ne", "wei\u00b7nen", "kann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den Wonnetrost empfinde:", "tokens": ["Und", "den", "Won\u00b7net\u00b7rost", "emp\u00b7fin\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jesus nimmt die S\u00fcnder an;", "tokens": ["Je\u00b7sus", "nimmt", "die", "S\u00fcn\u00b7der", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "O wie leicht wird's da dem Herzen,", "tokens": ["O", "wie", "leicht", "wird's", "da", "dem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn die Angst in Thr\u00e4nen schmelzt,", "tokens": ["Wenn", "die", "Angst", "in", "Thr\u00e4\u00b7nen", "schmelzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das Gef\u00fchl geh\u00e4ufter Schmerzen", "tokens": ["Das", "Ge\u00b7f\u00fchl", "ge\u00b7h\u00e4uf\u00b7ter", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird gleich H\u00fcgeln weggew\u00e4lzt.", "tokens": ["Wird", "gleich", "H\u00fc\u00b7geln", "weg\u00b7ge\u00b7w\u00e4lzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}