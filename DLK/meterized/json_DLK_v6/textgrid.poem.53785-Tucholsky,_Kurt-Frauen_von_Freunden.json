{"textgrid.poem.53785": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Frauen von Freunden", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frauen von Freunden zerst\u00f6ren die Freundschaft.", "tokens": ["Frau\u00b7en", "von", "Freun\u00b7den", "zer\u00b7st\u00f6\u00b7ren", "die", "Freund\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Sch\u00fcchtern erst besetzen sie einen Teil des Freundes,", "tokens": ["Sch\u00fcch\u00b7tern", "erst", "be\u00b7set\u00b7zen", "sie", "ei\u00b7nen", "Teil", "des", "Freun\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "nisten sich in ihm ein,", "tokens": ["nis\u00b7ten", "sich", "in", "ihm", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "warten,", "tokens": ["war\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "beobachten, und nehmen scheinbar teil am Freundesbund.", "tokens": ["be\u00b7ob\u00b7ach\u00b7ten", ",", "und", "neh\u00b7men", "schein\u00b7bar", "teil", "am", "Freun\u00b7des\u00b7bund", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "ADJD", "NN", "APPRART", "NN", "$."], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Dies St\u00fcck des Freundes hat uns nie geh\u00f6rt \u2013", "tokens": ["Dies", "St\u00fcck", "des", "Freun\u00b7des", "hat", "uns", "nie", "ge\u00b7h\u00f6rt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ART", "NN", "VAFIN", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "wir merken nichts.", "tokens": ["wir", "mer\u00b7ken", "nichts", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Aber bald \u00e4ndert sich das:", "tokens": ["A\u00b7ber", "bald", "\u00e4n\u00b7dert", "sich", "das", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "PDS", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Sie nehmen einen Hausfl\u00fcgel nach dem andern,", "tokens": ["Sie", "neh\u00b7men", "ei\u00b7nen", "Haus\u00b7fl\u00fc\u00b7gel", "nach", "dem", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "dringen tiefer ein,", "tokens": ["drin\u00b7gen", "tie\u00b7fer", "ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "haben bald den ganzen Freund.", "tokens": ["ha\u00b7ben", "bald", "den", "gan\u00b7zen", "Freund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Der ist ver\u00e4ndert; es ist, als sch\u00e4me er sich seiner Freundschaft.", "tokens": ["Der", "ist", "ver\u00b7\u00e4n\u00b7dert", ";", "es", "ist", ",", "als", "sch\u00e4\u00b7me", "er", "sich", "sei\u00b7ner", "Freund\u00b7schaft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "$.", "PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "So, wie er sich fr\u00fcher der Liebe vor uns gesch\u00e4mt hat,", "tokens": ["So", ",", "wie", "er", "sich", "fr\u00fc\u00b7her", "der", "Lie\u00b7be", "vor", "uns", "ge\u00b7sch\u00e4mt", "hat", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "PRF", "ADJD", "ART", "NN", "APPR", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "sch\u00e4mt er sich jetzt der Freundschaft vor ihr.", "tokens": ["sch\u00e4mt", "er", "sich", "jetzt", "der", "Freund\u00b7schaft", "vor", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ART", "NN", "APPR", "PPOSAT", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Er geh\u00f6rt uns nicht mehr.", "tokens": ["Er", "ge\u00b7h\u00f6rt", "uns", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Sie steht nicht zwischen uns \u2013 sie hat ihn weggezogen.", "tokens": ["Sie", "steht", "nicht", "zwi\u00b7schen", "uns", "\u2013", "sie", "hat", "ihn", "weg\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPER", "$(", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Er ist nicht mehr unser Freund:", "tokens": ["Er", "ist", "nicht", "mehr", "un\u00b7ser", "Freund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "er ist ihr Mann.", "tokens": ["er", "ist", "ihr", "Mann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Eine leise Verletzlichkeit bleibt \u00fcbrig.", "tokens": ["Ei\u00b7ne", "lei\u00b7se", "Ver\u00b7letz\u00b7lich\u00b7keit", "bleibt", "\u00fcb\u00b7rig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Traurig blicken wir ihm nach.", "tokens": ["Trau\u00b7rig", "bli\u00b7cken", "wir", "ihm", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Die im Bett beh\u00e4lt immer recht.", "tokens": ["Die", "im", "Bett", "be\u00b7h\u00e4lt", "im\u00b7mer", "recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}}}}