{"textgrid.poem.58599": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So f\u00fcrchte dich nun nicht; denn ich bin bei dir. Jes. 43, 5", "tokens": ["So", "f\u00fcrch\u00b7te", "dich", "nun", "nicht", ";", "denn", "ich", "bin", "bei", "dir", ".", "Jes", ".", "43", ",", "5"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "number", "punct", "number"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "$.", "KON", "PPER", "VAFIN", "APPR", "PPER", "$.", "NE", "$.", "CARD", "$,", "CARD"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Dies macht es, armer Mensch, da\u00df du in F\u00fcrchten schwebest,", "tokens": ["Dies", "macht", "es", ",", "ar\u00b7mer", "Mensch", ",", "da\u00df", "du", "in", "F\u00fcrch\u00b7ten", "schwe\u00b7best", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil Gott nicht bei dir ist und du mit Gott nicht lebest;", "tokens": ["Weil", "Gott", "nicht", "bei", "dir", "ist", "und", "du", "mit", "Gott", "nicht", "le\u00b7best", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKNEG", "APPR", "PPER", "VAFIN", "KON", "PPER", "APPR", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kommt dieses Wesen nah und wird man ihm gemein,", "tokens": ["Kommt", "die\u00b7ses", "We\u00b7sen", "nah", "und", "wird", "man", "ihm", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "ADJD", "KON", "VAFIN", "PIS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So mu\u00df das bl\u00f6dste Herz getrost und stille sein.", "tokens": ["So", "mu\u00df", "das", "bl\u00f6ds\u00b7te", "Herz", "ge\u00b7trost", "und", "stil\u00b7le", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "VVPP", "KON", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dies ist zwar so, mein Gott, doch bin ich oft in Sorgen,", "tokens": ["Dies", "ist", "zwar", "so", ",", "mein", "Gott", ",", "doch", "bin", "ich", "oft", "in", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "$,", "PPOSAT", "NN", "$,", "ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ob du auch bei mir bist, wann du dich mir verborgen;", "tokens": ["Ob", "du", "auch", "bei", "mir", "bist", ",", "wann", "du", "dich", "mir", "ver\u00b7bor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "VAFIN", "$,", "PWAV", "PPER", "PRF", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Drum f\u00fcrcht' ich Armer stets in dunkeln Wegen so.", "tokens": ["Drum", "f\u00fcrcht'", "ich", "Ar\u00b7mer", "stets", "in", "dun\u00b7keln", "We\u00b7gen", "so", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "NN", "ADV", "APPR", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Hilf glauben, da\u00df du nah, so werd' ich still und froh!", "tokens": ["Hilf", "glau\u00b7ben", ",", "da\u00df", "du", "nah", ",", "so", "werd'", "ich", "still", "und", "froh", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "$,", "ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}