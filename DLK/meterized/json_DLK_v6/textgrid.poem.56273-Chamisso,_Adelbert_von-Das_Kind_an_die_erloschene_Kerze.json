{"textgrid.poem.56273": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Das Kind an die erloschene Kerze", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du arme, arme Kerze,", "tokens": ["Du", "ar\u00b7me", ",", "ar\u00b7me", "Ker\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gibst f\u00fcrder keinen Schein,", "tokens": ["Gibst", "f\u00fcr\u00b7der", "kei\u00b7nen", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erloschen ist so schnelle", "tokens": ["Er\u00b7lo\u00b7schen", "ist", "so", "schnel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein Licht, das freud'ge, helle,", "tokens": ["Dein", "Licht", ",", "das", "freu\u00b7d'\u00b7ge", ",", "hel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "O mu\u00dft es also sein!", "tokens": ["O", "mu\u00dft", "es", "al\u00b7so", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADV", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Du arme, arme Kerze,", "tokens": ["Du", "ar\u00b7me", ",", "ar\u00b7me", "Ker\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Gibst f\u00fcrder keinen Schein!", "tokens": ["Gibst", "f\u00fcr\u00b7der", "kei\u00b7nen", "Schein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "'s ist nicht, weil ich nun weilen", "tokens": ["'s", "ist", "nicht", ",", "weil", "ich", "nun", "wei\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "$,", "KOUS", "PPER", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df in der Dunkelheit!", "tokens": ["Mu\u00df", "in", "der", "Dun\u00b7kel\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O brenntest du nur immer,", "tokens": ["O", "brenn\u00b7test", "du", "nur", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und g\u00e4b dein lieber Schimmer", "tokens": ["Und", "g\u00e4b", "dein", "lie\u00b7ber", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nur andern Freudigkeit!", "tokens": ["Nur", "an\u00b7dern", "Freu\u00b7dig\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "'s ist nicht, weil ich nun weilen", "tokens": ["'s", "ist", "nicht", ",", "weil", "ich", "nun", "wei\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "$,", "KOUS", "PPER", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mu\u00df in der Dunkelheit!", "tokens": ["Mu\u00df", "in", "der", "Dun\u00b7kel\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du arme, arme Kerze,", "tokens": ["Du", "ar\u00b7me", ",", "ar\u00b7me", "Ker\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gibst f\u00fcrder keinen Schein!", "tokens": ["Gibst", "f\u00fcr\u00b7der", "kei\u00b7nen", "Schein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "'s ist nicht, weil ich alleine", "tokens": ["'s", "ist", "nicht", ",", "weil", "ich", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "$,", "KOUS", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Im Dunkeln bin und weine,", "tokens": ["Im", "Dun\u00b7keln", "bin", "und", "wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich bin ja gern allein!", "tokens": ["Ich", "bin", "ja", "gern", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Du arme, arme Kerze,", "tokens": ["Du", "ar\u00b7me", ",", "ar\u00b7me", "Ker\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$,"], "meter"