{"textgrid.poem.38670": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Nachtst\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1791, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schaurig fl\u00fcstern die Gr\u00e4ber, melancholisch", "tokens": ["Schau\u00b7rig", "fl\u00fcs\u00b7tern", "die", "Gr\u00e4\u00b7ber", ",", "me\u00b7lan\u00b7cho\u00b7lisch"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "ADJD"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Blickt durch wehendes Laub der Vollmond; lichtscheu", "tokens": ["Blickt", "durch", "we\u00b7hen\u00b7des", "Laub", "der", "Voll\u00b7mond", ";", "licht\u00b7scheu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "$.", "NN"], "meter": "+-+--+-++-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Taucht im Dunkel des Uhus Flug; im Glanze", "tokens": ["Taucht", "im", "Dun\u00b7kel", "des", "U\u00b7hus", "Flug", ";", "im", "Glan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "NN", "$.", "APPRART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Schw\u00e4rmt die Phal\u00e4ne.", "tokens": ["Schw\u00e4rmt", "die", "Pha\u00b7l\u00e4\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "K\u00fchn am graulichen Kirchthurm kreucht der Efeu", "tokens": ["K\u00fchn", "am", "grau\u00b7li\u00b7chen", "Kirch\u00b7thurm", "kreucht", "der", "E\u00b7feu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPRART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Bis zum Glockengeb\u00e4lk; nur du verdorrtest,", "tokens": ["Bis", "zum", "Glo\u00b7cken\u00b7ge\u00b7b\u00e4lk", ";", "nur", "du", "ver\u00b7dorr\u00b7test", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "$.", "ADV", "PPER", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Die den H\u00fcgel des Freundes leis' umrauschte,", "tokens": ["Die", "den", "H\u00fc\u00b7gel", "des", "Freun\u00b7des", "leis'", "um\u00b7rauschte", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Weide der Thr\u00e4nen!", "tokens": ["Wei\u00b7de", "der", "Thr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Geisterlispel entwehn der Aetherw\u00fcste;", "tokens": ["Geis\u00b7ter\u00b7lis\u00b7pel", "ent\u00b7wehn", "der", "A\u00b7e\u00b7ther\u00b7w\u00fcs\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Von den Sternen durchd\u00e4mmert, graun im Mondstral", "tokens": ["Von", "den", "Ster\u00b7nen", "durch\u00b7d\u00e4m\u00b7mert", ",", "graun", "im", "Mond\u00b7stral"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "ADV", "APPRART", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Tr\u00fcbe Nebelgebild' am lichten Saume", "tokens": ["Tr\u00fc\u00b7be", "Ne\u00b7bel\u00b7ge\u00b7bild'", "am", "lich\u00b7ten", "Sau\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Schwarzer Gew\u00f6lke.", "tokens": ["Schwar\u00b7zer", "Ge\u00b7w\u00f6l\u00b7ke", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Wie so feierlich der Entschlafnen H\u00fcgel", "tokens": ["Wie", "so", "fei\u00b7er\u00b7lich", "der", "Ent\u00b7schlaf\u00b7nen", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "ART", "ADJA", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sich im schlummernden Lichte rings erheben!", "tokens": ["Sich", "im", "schlum\u00b7mern\u00b7den", "Lich\u00b7te", "rings", "er\u00b7he\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Die nur sparsam begr\u00fcnt, mit Ranken andre", "tokens": ["Die", "nur", "spar\u00b7sam", "be\u00b7gr\u00fcnt", ",", "mit", "Ran\u00b7ken", "and\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "VVPP", "$,", "APPR", "NN", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Dicht \u00fcberwildert.", "tokens": ["Dicht", "\u00fc\u00b7ber\u00b7wil\u00b7dert", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Kreuze weihte die Armuth hier zum stillen", "tokens": ["Kreu\u00b7ze", "weih\u00b7te", "die", "Ar\u00b7muth", "hier", "zum", "stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "APPRART", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Denkmal h\u00e4uslicher Tugend, werth des Marmors", "tokens": ["Denk\u00b7mal", "h\u00e4us\u00b7li\u00b7cher", "Tu\u00b7gend", ",", "werth", "des", "Mar\u00b7mors"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "ADJD", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Der Triumphe verherrlicht, werth der sch\u00f6nern", "tokens": ["Der", "Tri\u00b7um\u00b7phe", "ver\u00b7herr\u00b7licht", ",", "werth", "der", "sch\u00f6\u00b7nern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "ADJD", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Z\u00e4hre der Nachwelt.", "tokens": ["Z\u00e4h\u00b7re", "der", "Nach\u00b7welt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Hell am alternden Beinhaus gl\u00e4nzt ein Grabstein;", "tokens": ["Hell", "am", "al\u00b7tern\u00b7den", "Be\u00b7in\u00b7haus", "gl\u00e4nzt", "ein", "Grab\u00b7stein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Statt heraldischen Prunks winkt eine Rose,", "tokens": ["Statt", "her\u00b7al\u00b7di\u00b7schen", "Prunks", "winkt", "ei\u00b7ne", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Statt der Schriften von Gold liest mein bethr\u00e4ntes", "tokens": ["Statt", "der", "Schrif\u00b7ten", "von", "Gold", "liest", "mein", "be\u00b7thr\u00e4n\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPR", "NN", "VVFIN", "PPOSAT", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Auge: So war Sie!", "tokens": ["Au\u00b7ge", ":", "So", "war", "Sie", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VAFIN", "PPER", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}}, "stanza.7": {"line.1": {"text": "Dich, o l\u00e4ndlicher J\u00fcngling, der mit roher", "tokens": ["Dich", ",", "o", "l\u00e4nd\u00b7li\u00b7cher", "J\u00fcng\u00b7ling", ",", "der", "mit", "ro\u00b7her"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "FM", "ADJA", "NN", "$,", "PRELS", "APPR", "ADJA"], "meter": "+-+--+--++-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Kunst dem Grabe der Braut dies Mal gebildet,", "tokens": ["Kunst", "dem", "Gra\u00b7be", "der", "Braut", "dies", "Mal", "ge\u00b7bil\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "PDS", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Deckt ein H\u00fcgel zur Seite, von Zypressen", "tokens": ["Deckt", "ein", "H\u00fc\u00b7gel", "zur", "Sei\u00b7te", ",", "von", "Zyp\u00b7res\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,", "APPR", "NE"], "meter": "+-+--+--+--", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "D\u00fcster beschattet.", "tokens": ["D\u00fcs\u00b7ter", "be\u00b7schat\u00b7tet", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}