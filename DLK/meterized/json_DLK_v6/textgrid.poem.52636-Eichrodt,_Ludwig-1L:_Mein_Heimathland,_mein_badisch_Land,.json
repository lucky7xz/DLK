{"textgrid.poem.52636": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Heimathland, mein badisch Land,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Heimathland, mein badisch Land,", "tokens": ["Mein", "Hei\u00b7mat\u00b7hland", ",", "mein", "ba\u00b7disch", "Land", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer preiset dich und hat Verstand", "tokens": ["Wer", "prei\u00b7set", "dich", "und", "hat", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "KON", "VAFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und lobt nicht deinen Wein?", "tokens": ["Und", "lobt", "nicht", "dei\u00b7nen", "Wein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Markgr\u00e4fler schenket ein!", "tokens": ["Mark\u00b7gr\u00e4f\u00b7ler", "schen\u00b7ket", "ein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wohlan, der ist wie gelbes Gold,", "tokens": ["Wo\u00b7hlan", ",", "der", "ist", "wie", "gel\u00b7bes", "Gold", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "VAFIN", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Was aus dem Rhein der Landsmann holt,", "tokens": ["Was", "aus", "dem", "Rhein", "der", "Lands\u00b7mann", "holt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NE", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch schenkt ihr Rothen ein,", "tokens": ["Doch", "schenkt", "ihr", "Ro\u00b7then", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Solls Affenthaler sein!", "tokens": ["Solls", "Af\u00b7fent\u00b7ha\u00b7ler", "sein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "F\u00fcrwahr das hei\u00df ich Rebenblut!", "tokens": ["F\u00fcr\u00b7wahr", "das", "hei\u00df", "ich", "Re\u00b7ben\u00b7blut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Roth und Gelb das d\u00fcnk euch gut,", "tokens": ["Und", "Roth", "und", "Gelb", "das", "d\u00fcnk", "euch", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "ADJD", "PDS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Trinkt Landesfarb im Wein!", "tokens": ["Trinkt", "Lan\u00b7des\u00b7farb", "im", "Wein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie lustig blickt sie drein!", "tokens": ["Wie", "lus\u00b7tig", "blickt", "sie", "drein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ein muthiger, ein froher Sinn,", "tokens": ["Ein", "mut\u00b7hi\u00b7ger", ",", "ein", "fro\u00b7her", "Sinn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Ein treuer Sinn vom Rhein bis in", "tokens": ["Ein", "treu\u00b7er", "Sinn", "vom", "Rhein", "bis", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NE", "ADV", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Berge weit hinein", "tokens": ["Die", "Ber\u00b7ge", "weit", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Soll immerdar gedeihn!", "tokens": ["Soll", "im\u00b7mer\u00b7dar", "ge\u00b7deihn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Mein Heimathland, mein badisch Land,", "tokens": ["Mein", "Hei\u00b7mat\u00b7hland", ",", "mein", "ba\u00b7disch", "Land", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich preis ich laut, wer hat Verstand", "tokens": ["Dich", "preis", "ich", "laut", ",", "wer", "hat", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "PWS", "VAFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Herz, der stimme ein!", "tokens": ["Und", "Herz", ",", "der", "stim\u00b7me", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Gut leben ist am Rhein!", "tokens": ["Gut", "le\u00b7ben", "ist", "am", "Rhein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VAFIN", "APPRART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}