{"textgrid.poem.43880": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nur eine bleibet meine Taube,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur eine bleibet meine Taube,", "tokens": ["Nur", "ei\u00b7ne", "blei\u00b7bet", "mei\u00b7ne", "Tau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und diese, werthes Kind, bist du;", "tokens": ["Und", "die\u00b7se", ",", "wert\u00b7hes", "Kind", ",", "bist", "du", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "ADJA", "NN", "$,", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Welt hat nichts von s\u00fc\u00dfrem Schmerze,", "tokens": ["Die", "Welt", "hat", "nichts", "von", "s\u00fc\u00df\u00b7rem", "Schmer\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als wenn ich dir, vertrautes Herze,", "tokens": ["Als", "wenn", "ich", "dir", ",", "ver\u00b7trau\u00b7tes", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PPER", "$,", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Armen um den Nacken thu", "tokens": ["Die", "Ar\u00b7men", "um", "den", "Na\u00b7cken", "thu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und dort zwey Handvoll Blumen raube.", "tokens": ["Und", "dort", "zwey", "Hand\u00b7voll", "Blu\u00b7men", "rau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "CARD", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So wie uns oft nach warmen Regen", "tokens": ["So", "wie", "uns", "oft", "nach", "war\u00b7men", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein gr\u00fcnlichter Geruch erquickt,", "tokens": ["Ein", "gr\u00fcn\u00b7lich\u00b7ter", "Ge\u00b7ruch", "er\u00b7quickt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So geil, so kr\u00e4ftig und so s\u00fc\u00dfe", "tokens": ["So", "geil", ",", "so", "kr\u00e4f\u00b7tig", "und", "so", "s\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "ADV", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erfahr ich den Geruch der K\u00fc\u00dfe,", "tokens": ["Er\u00b7fahr", "ich", "den", "Ge\u00b7ruch", "der", "K\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die, wenn sich deine Zunge r\u00fcckt,", "tokens": ["Die", ",", "wenn", "sich", "dei\u00b7ne", "Zun\u00b7ge", "r\u00fcckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Herz, Nieren, Marck und Bein bewegen.", "tokens": ["Herz", ",", "Nie\u00b7ren", ",", "Marck", "und", "Bein", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So herrlich d\u00e4mpft Dianens Tempel", "tokens": ["So", "herr\u00b7lich", "d\u00e4mpft", "Di\u00b7a\u00b7nens", "Tem\u00b7pel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit seinem theuren R\u00e4uchwerk nicht,", "tokens": ["Mit", "sei\u00b7nem", "theu\u00b7ren", "R\u00e4uch\u00b7werk", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So liebreich wi\u00dfen keine Rosen", "tokens": ["So", "lieb\u00b7reich", "wi\u00b7\u00dfen", "kei\u00b7ne", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den schwachen Sinnen liebzukosen", "tokens": ["Den", "schwa\u00b7chen", "Sin\u00b7nen", "lieb\u00b7zu\u00b7ko\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Als dies, was hier die Regung spricht:", "tokens": ["Als", "dies", ",", "was", "hier", "die", "Re\u00b7gung", "spricht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Wollust leidet kein Exempel.", "tokens": ["Die", "Wol\u00b7lust", "lei\u00b7det", "kein", "Ex\u00b7em\u00b7pel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Mich deucht, es geht auf deinem Munde", "tokens": ["Mich", "deucht", ",", "es", "geht", "auf", "dei\u00b7nem", "Mun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der nechste Weg in Amors Reich.", "tokens": ["Der", "nechs\u00b7te", "Weg", "in", "A\u00b7mors", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Vorschmack von den reinen L\u00fcsten", "tokens": ["Der", "Vor\u00b7schmack", "von", "den", "rei\u00b7nen", "L\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fchrt mich durch Berge, Thal und W\u00fcsten;", "tokens": ["F\u00fchrt", "mich", "durch", "Ber\u00b7ge", ",", "Thal", "und", "W\u00fcs\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So denck ich oft, und irr ich gleich,", "tokens": ["So", "denck", "ich", "oft", ",", "und", "irr", "ich", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "$,", "KON", "NN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So irr ich doch mit gutem Grunde.", "tokens": ["So", "irr", "ich", "doch", "mit", "gu\u00b7tem", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Rei\u00dft, sanfte Lippen, rei\u00dft mein Leben", "tokens": ["Rei\u00dft", ",", "sanf\u00b7te", "Lip\u00b7pen", ",", "rei\u00dft", "mein", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch so ein warmes Gift dahin;", "tokens": ["Durch", "so", "ein", "war\u00b7mes", "Gift", "da\u00b7hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "ADJA", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So komm ich be\u00dfer und auch eher", "tokens": ["So", "komm", "ich", "be\u00b7\u00dfer", "und", "auch", "e\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADV", "ADV"], "meter": "-+-+--++-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ins Paradies der Elis\u00e4er,", "tokens": ["Ins", "Pa\u00b7ra\u00b7dies", "der", "E\u00b7li\u00b7s\u00e4\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Allwo ich schon im Traume bin,", "tokens": ["All\u00b7wo", "ich", "schon", "im", "Trau\u00b7me", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil Fried und Sch\u00f6nheit um mich schweben.", "tokens": ["Weil", "Fried", "und", "Sch\u00f6n\u00b7heit", "um", "mich", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ja, ja, du magst es auch belachen,", "tokens": ["Ja", ",", "ja", ",", "du", "magst", "es", "auch", "be\u00b7la\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich will mit deinen K\u00fc\u00dfen fliehn,", "tokens": ["Ich", "will", "mit", "dei\u00b7nen", "K\u00fc\u00b7\u00dfen", "fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wird mich dort Petrarch umfa\u00dfen,", "tokens": ["Und", "wird", "mich", "dort", "Pe\u00b7trarch", "um\u00b7fa\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn gleichfalls einen kosten la\u00dfen.", "tokens": ["Ihn", "gleich\u00b7falls", "ei\u00b7nen", "kos\u00b7ten", "la\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was gilt's? Er soll vor Sehnsucht gl\u00fchn", "tokens": ["Was", "gilt's", "?", "Er", "soll", "vor", "Sehn\u00b7sucht", "gl\u00fchn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$.", "PPER", "VMFIN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und viele Geister l\u00fcsternd machen.", "tokens": ["Und", "vie\u00b7le", "Geis\u00b7ter", "l\u00fcs\u00b7ternd", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}