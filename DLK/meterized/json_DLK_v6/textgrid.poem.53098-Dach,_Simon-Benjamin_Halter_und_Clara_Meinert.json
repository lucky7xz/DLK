{"textgrid.poem.53098": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Benjamin Halter und Clara Meinert", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So lang' ich noch das Leben hab',", "tokens": ["So", "lang'", "ich", "noch", "das", "Le\u00b7ben", "hab'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herr, deine Gab',", "tokens": ["Herr", ",", "dei\u00b7ne", "Gab'", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Alhie auff Erden,", "tokens": ["Al\u00b7hie", "auff", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Vnd eh' ich mu\u00df den W\u00fcrmen dort,", "tokens": ["Vnd", "eh'", "ich", "mu\u00df", "den", "W\u00fcr\u00b7men", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VMFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im finstern Ort',", "tokens": ["Im", "fins\u00b7tern", "Ort'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ein Gast-mahl werden,", "tokens": ["Ein", "Gast\u00b7mahl", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Bitt ich nur zweyerley von dir,", "tokens": ["Bitt", "ich", "nur", "zwey\u00b7er\u00b7ley", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Du woltest mir", "tokens": ["Du", "wol\u00b7test", "mir"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Die nicht versagen,", "tokens": ["Die", "nicht", "ver\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Auff falsche Lehr vnd L\u00fcgen la\u00df", "tokens": ["Auff", "fal\u00b7sche", "Lehr", "vnd", "L\u00fc\u00b7gen", "la\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Mich ewig Ha\u00df", "tokens": ["Mich", "e\u00b7wig", "Ha\u00df"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJD", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Vnd Feindschafft tragen.", "tokens": ["Vnd", "Feind\u00b7schafft", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "F\u00fcr Armut mich ja stets bewahr,", "tokens": ["F\u00fcr", "Ar\u00b7mut", "mich", "ja", "stets", "be\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch f\u00fcr zu gar", "tokens": ["Auch", "f\u00fcr", "zu", "gar"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "APPR", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Geh\u00e4ufften Sch\u00e4tzen,", "tokens": ["Ge\u00b7h\u00e4uff\u00b7ten", "Sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "La\u00df aber mich mein t\u00e4glich Brodt,", "tokens": ["La\u00df", "a\u00b7ber", "mich", "mein", "t\u00e4g\u00b7lich", "Brodt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPER", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So viel mir noth,", "tokens": ["So", "viel", "mir", "noth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "In Ruh' ergetzen,", "tokens": ["In", "Ruh'", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Sonst m\u00f6cht' ich bey so grossem Gutt'", "tokens": ["Sonst", "m\u00f6cht'", "ich", "bey", "so", "gros\u00b7sem", "Gutt'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Au\u00df Vbermuth", "tokens": ["Au\u00df", "Vber\u00b7muth"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Dich, Herr, verneinen,", "tokens": ["Dich", ",", "Herr", ",", "ver\u00b7nei\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Vnd sagen dir zu Hohn vnd Spott:", "tokens": ["Vnd", "sa\u00b7gen", "dir", "zu", "Hohn", "vnd", "Spott", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Gold ist mein Gott,", "tokens": ["Gold", "ist", "mein", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Sonst kenn' ich keinen.", "tokens": ["Sonst", "kenn'", "ich", "kei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "St\u00e4rckt auch zu sehr mein Mangel sich,", "tokens": ["St\u00e4rckt", "auch", "zu", "sehr", "mein", "Man\u00b7gel", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKA", "ADV", "PPOSAT", "NN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich m\u00f6chte mich", "tokens": ["Ich", "m\u00f6ch\u00b7te", "mich"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Zu betteln sch\u00e4men,", "tokens": ["Zu", "bet\u00b7teln", "sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Vnd, Arbeit-schew vnd Mittel-loh\u00df,", "tokens": ["Vnd", ",", "Ar\u00b7beit\u00b7schew", "vnd", "Mit\u00b7tel\u00b7loh\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zum Diebstal bloh\u00df", "tokens": ["Zum", "Diebs\u00b7tal", "bloh\u00df"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "NE"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Die Zuflucht nehmen,", "tokens": ["Die", "Zu\u00b7flucht", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "W\u00fcrd' also meiner S\u00fcnden Maa\u00df", "tokens": ["W\u00fcrd'", "al\u00b7so", "mei\u00b7ner", "S\u00fcn\u00b7den", "Maa\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ohn vnterla\u00df", "tokens": ["Ohn", "vn\u00b7ter\u00b7la\u00df"], "token_info": ["word", "word"], "pos": ["XY", "XY"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Mit S\u00fcnden h\u00e4uffen,", "tokens": ["Mit", "S\u00fcn\u00b7den", "h\u00e4uf\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Vnd mich an deines Nahmens Ehr'", "tokens": ["Vnd", "mich", "an", "dei\u00b7nes", "Nah\u00b7mens", "Ehr'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Vnleidlich sehr,", "tokens": ["Vn\u00b7leid\u00b7lich", "sehr", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Mein Gott, vergreiffen.", "tokens": ["Mein", "Gott", ",", "ver\u00b7greif\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}