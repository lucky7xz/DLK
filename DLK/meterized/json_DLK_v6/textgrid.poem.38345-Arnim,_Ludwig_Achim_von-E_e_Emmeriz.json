{"textgrid.poem.38345": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "E e Emmeriz", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Emmeriz bis zum Abend spat, singt \u00fcbel, \u00fcbel hin,", "tokens": ["Der", "Em\u00b7me\u00b7riz", "bis", "zum", "A\u00b7bend", "spat", ",", "singt", "\u00fc\u00b7bel", ",", "\u00fc\u00b7bel", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "APPRART", "NN", "VVFIN", "$,", "VVFIN", "ADJD", "$,", "ADJD", "PTKVZ", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Er sagt, wenns Feld nur Aehren hat, ich auch ein Schnitter bin,", "tokens": ["Er", "sagt", ",", "wenns", "Feld", "nur", "A\u00b7eh\u00b7ren", "hat", ",", "ich", "auch", "ein", "Schnit\u00b7ter", "bin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "NN", "ADV", "NN", "VAFIN", "$,", "PPER", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Im Feld thut er sich n\u00e4hren, bleibt Tag und Nacht darauf,", "tokens": ["Im", "Feld", "thut", "er", "sich", "n\u00e4h\u00b7ren", ",", "bleibt", "Tag", "und", "Nacht", "da\u00b7rauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PRF", "VVINF", "$,", "VVFIN", "NN", "KON", "NN", "PAV", "$,"], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Was Gott ihm thut beschehren, das klaubt er flei\u00dfig auf.", "tokens": ["Was", "Gott", "ihm", "thut", "be\u00b7scheh\u00b7ren", ",", "das", "klaubt", "er", "flei\u00b7\u00dfig", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PPER", "VVFIN", "VVINF", "$,", "PDS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}