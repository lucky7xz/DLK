{"textgrid.poem.60801": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Fruchtlose Zwangsanstalt", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlechtes verbietest du leicht; doch gegen des Genius Werke", "tokens": ["Schlech\u00b7tes", "ver\u00b7bie\u00b7test", "du", "leicht", ";", "doch", "ge\u00b7gen", "des", "Ge\u00b7nius", "Wer\u00b7ke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "$.", "ADV", "APPR", "ART", "NN", "NN"], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Sind ohnm\u00e4chtig und schwach Scherge, Minister, Despot:", "tokens": ["Sind", "ohn\u00b7m\u00e4ch\u00b7tig", "und", "schwach", "Scher\u00b7ge", ",", "Mi\u00b7nis\u00b7ter", ",", "Des\u00b7pot", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "W\u00e4hrend du Glaubst das Genie zu beherrschen, beherrschest du h\u00f6chstens", "tokens": ["W\u00e4h\u00b7rend", "du", "Glaubst", "das", "Ge\u00b7nie", "zu", "be\u00b7herr\u00b7schen", ",", "be\u00b7herr\u00b7schest", "du", "h\u00f6chs\u00b7tens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "PPER", "ADV"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Blo\u00df des Genies Leichnam, welchen die Seele verlie\u00df.", "tokens": ["Blo\u00df", "des", "Ge\u00b7nies", "Leich\u00b7nam", ",", "wel\u00b7chen", "die", "See\u00b7le", "ver\u00b7lie\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "PWAT", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}}}}