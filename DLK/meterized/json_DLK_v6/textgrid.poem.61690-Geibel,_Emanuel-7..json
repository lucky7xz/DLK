{"textgrid.poem.61690": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist es denn m\u00f6glich?", "tokens": ["Ist", "es", "denn", "m\u00f6g\u00b7lich", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und so viel Jahre", "tokens": ["Und", "so", "viel", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Lebt' ich schon fr\u00fcher?", "tokens": ["Lebt'", "ich", "schon", "fr\u00fc\u00b7her", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Sah Himmel und Erde", "tokens": ["Sah", "Him\u00b7mel", "und", "Er\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und lacht' und h\u00e4rmte mich", "tokens": ["Und", "lacht'", "und", "h\u00e4rm\u00b7te", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Um Schatten?", "tokens": ["Um", "Schat\u00b7ten", "?"], "token_info": ["word", "word", "punct"], "pos": ["KOUI", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Und nun, urpl\u00f6tzlich,", "tokens": ["Und", "nun", ",", "ur\u00b7pl\u00f6tz\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADJD", "$,"], "meter": "-+++-", "measure": "unknown.measure.tri"}, "line.2": {"text": "In dreien Tagen", "tokens": ["In", "drei\u00b7en", "Ta\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "CARD", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Lieben und Scheiden!", "tokens": ["Lie\u00b7ben", "und", "Schei\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "KON", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "O halte, mein Herz,", "tokens": ["O", "hal\u00b7te", ",", "mein", "Herz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Halte die F\u00fclle!", "tokens": ["Hal\u00b7te", "die", "F\u00fcl\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Nun erst brach ich", "tokens": ["Nun", "erst", "brach", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER"], "meter": "-++-", "measure": "unknown.measure.di"}, "line.4": {"text": "Vom Baume des Lebens,", "tokens": ["Vom", "Bau\u00b7me", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Hab' ich gekostet", "tokens": ["Hab'", "ich", "ge\u00b7kos\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "VVPP"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Vom Baum der Erkenntnis", "tokens": ["Vom", "Baum", "der", "Er\u00b7kennt\u00b7nis"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Und wei\u00df, was Freud' und was Leid ist.", "tokens": ["Und", "wei\u00df", ",", "was", "Freud'", "und", "was", "Leid", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "NN", "KON", "PWS", "NN", "VAFIN", "$."], "meter": "-+-+-+++", "measure": "unknown.measure.penta"}}}}}