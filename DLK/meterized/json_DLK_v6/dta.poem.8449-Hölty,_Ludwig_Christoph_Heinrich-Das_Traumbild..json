{"dta.poem.8449": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Traumbild.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Die Seele f\u00fcllt!", "tokens": ["Die", "See\u00b7le", "f\u00fcllt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wann werd' ich dich an meinen Busen dr\u00fccken,", "tokens": ["Wann", "werd'", "ich", "dich", "an", "mei\u00b7nen", "Bu\u00b7sen", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geliebtes Bild?", "tokens": ["Ge\u00b7lieb\u00b7tes", "Bild", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wann mich am Bach', im Wehn der Pappelweide,", "tokens": ["Wann", "mich", "am", "Bach'", ",", "im", "Wehn", "der", "Pap\u00b7pel\u00b7wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Schlaf umwallt,", "tokens": ["Der", "Schlaf", "um\u00b7wallt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Erscheinst du mir im weissen Abendkleide,", "tokens": ["Er\u00b7scheinst", "du", "mir", "im", "weis\u00b7sen", "A\u00b7bend\u00b7klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du Lichtgestalt!", "tokens": ["Du", "Licht\u00b7ge\u00b7stalt", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Du flatterst oft in fr\u00fcher Morgenstunde", "tokens": ["Du", "flat\u00b7terst", "oft", "in", "fr\u00fc\u00b7her", "Mor\u00b7gen\u00b7stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch mein Gemach,", "tokens": ["Durch", "mein", "Ge\u00b7mach", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "++-+", "measure": "iambic.di"}, "line.3": {"text": "Und k\u00fcssest mich mit deinem rothen Munde", "tokens": ["Und", "k\u00fcs\u00b7sest", "mich", "mit", "dei\u00b7nem", "ro\u00b7then", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom Schlummer wach.", "tokens": ["Vom", "Schlum\u00b7mer", "wach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Lang glaub' ich noch den Herzenskuss zu f\u00fchlen,", "tokens": ["Lang", "glaub'", "ich", "noch", "den", "Her\u00b7zens\u00b7kuss", "zu", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der mich entz\u00fcckt,", "tokens": ["Der", "mich", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und mit dem Strauss' an deiner Brust zu spielen,", "tokens": ["Und", "mit", "dem", "Straus\u00b7s'", "an", "dei\u00b7ner", "Brust", "zu", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der mir genickt.", "tokens": ["Der", "mir", "ge\u00b7nickt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Jezt seh' ich dich, im Rauschen gr\u00fcner Linden,", "tokens": ["Jezt", "seh'", "ich", "dich", ",", "im", "Rau\u00b7schen", "gr\u00fc\u00b7ner", "Lin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "APPRART", "NN", "ADJA", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein goldnes Band", "tokens": ["Ein", "gold\u00b7nes", "Band"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Um einen Kranz von Tausendsch\u00f6nchen winden", "tokens": ["Um", "ei\u00b7nen", "Kranz", "von", "Tau\u00b7send\u00b7sch\u00f6n\u00b7chen", "win\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit weisser Hand;", "tokens": ["Mit", "weis\u00b7ser", "Hand", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Und bald darauf im kleinen Blumengarten,", "tokens": ["Und", "bald", "da\u00b7rauf", "im", "klei\u00b7nen", "Blu\u00b7men\u00b7gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PAV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Eva sch\u00f6n,", "tokens": ["Wie", "E\u00b7va", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Des Rosenbaums, des Nelkenstrauchs zu warten,", "tokens": ["Des", "Ro\u00b7sen\u00b7baums", ",", "des", "Nel\u00b7ken\u00b7strauchs", "zu", "war\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am Beete gehn.", "tokens": ["Am", "Bee\u00b7te", "gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Erblick' ich dich, die ich vom Himmel bitte,", "tokens": ["Er\u00b7blick'", "ich", "dich", ",", "die", "ich", "vom", "Him\u00b7mel", "bit\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "$,", "PRELS", "PPER", "APPRART", "NN", "PTKANT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erblick' ich dich,", "tokens": ["Er\u00b7blick'", "ich", "dich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "So komm, so komm in meine Halmenh\u00fctte,", "tokens": ["So", "komm", ",", "so", "komm", "in", "mei\u00b7ne", "Hal\u00b7men\u00b7h\u00fct\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und tr\u00f6ste mich!", "tokens": ["Und", "tr\u00f6s\u00b7te", "mich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Dir soll ein Beet, wo tausend Blumen wanken,", "tokens": ["Dir", "soll", "ein", "Beet", ",", "wo", "tau\u00b7send", "Blu\u00b7men", "wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "$,", "PWAV", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Entgegenbl\u00fchn;", "tokens": ["Ent\u00b7ge\u00b7gen\u00b7bl\u00fchn", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ich will ein Dach von jungen Geisblattranken", "tokens": ["Ich", "will", "ein", "Dach", "von", "jun\u00b7gen", "Geis\u00b7blatt\u00b7ran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr dich erziehn;", "tokens": ["F\u00fcr", "dich", "er\u00b7ziehn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Ins Paradies an deiner Brust mich tr\u00e4umen,", "tokens": ["Ins", "Pa\u00b7ra\u00b7dies", "an", "dei\u00b7ner", "Brust", "mich", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein s\u00fcsses Kind;", "tokens": ["Mein", "s\u00fcs\u00b7ses", "Kind", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und froher sein, als unter Lebensb\u00e4umen", "tokens": ["Und", "fro\u00b7her", "sein", ",", "als", "un\u00b7ter", "Le\u00b7bens\u00b7b\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "VAINF", "$,", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Engel sind!", "tokens": ["Die", "En\u00b7gel", "sind", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}