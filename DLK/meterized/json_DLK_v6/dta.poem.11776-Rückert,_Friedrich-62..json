{"dta.poem.11776": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "62.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du das Ziel nur kennst, und bist auf rechten Wegen,", "tokens": ["Wenn", "du", "das", "Ziel", "nur", "kennst", ",", "und", "bist", "auf", "rech\u00b7ten", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$,", "KON", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gleichviel ists wie du rennst den Weg dem Ziel entgegen.", "tokens": ["Gleich\u00b7viel", "ists", "wie", "du", "rennst", "den", "Weg", "dem", "Ziel", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "KOKOM", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du magst zu Fu\u00dfe gehn, du magst auch reiten, fahren,", "tokens": ["Du", "magst", "zu", "Fu\u00b7\u00dfe", "gehn", ",", "du", "magst", "auch", "rei\u00b7ten", ",", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF", "$,", "PPER", "VMFIN", "ADV", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Ziel nur mu\u00dft du sehn, und deines Weges wahren.", "tokens": ["Dein", "Ziel", "nur", "mu\u00dft", "du", "sehn", ",", "und", "dei\u00b7nes", "We\u00b7ges", "wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VMFIN", "PPER", "VVINF", "$,", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nur vorwerts, nie zur\u00fcck! kein m\u00fc\u00dfiges Bedenken!", "tokens": ["Nur", "vor\u00b7werts", ",", "nie", "zu\u00b7r\u00fcck", "!", "kein", "m\u00fc\u00b7\u00dfi\u00b7ges", "Be\u00b7den\u00b7ken", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "PTKVZ", "$.", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Einzle mu\u00df das Gl\u00fcck, Gott mu\u00df das Ganze lenken.", "tokens": ["Das", "Einz\u00b7le", "mu\u00df", "das", "Gl\u00fcck", ",", "Gott", "mu\u00df", "das", "Gan\u00b7ze", "len\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "$,", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Schmal ist der rechte Weg, doch ist er nicht so schmal,", "tokens": ["Schmal", "ist", "der", "rech\u00b7te", "Weg", ",", "doch", "ist", "er", "nicht", "so", "schmal", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,", "ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df rechts und links zu gehn dir bliebe nicht die Wahl.", "tokens": ["Da\u00df", "rechts", "und", "links", "zu", "gehn", "dir", "blie\u00b7be", "nicht", "die", "Wahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "KON", "ADV", "PTKZU", "VVINF", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Auch eben ist der Weg, doch ist er nicht so eben,", "tokens": ["Auch", "e\u00b7ben", "ist", "der", "Weg", ",", "doch", "ist", "er", "nicht", "so", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df fortzukommen du den Fu\u00df nicht m\u00fc\u00dfest heben.", "tokens": ["Da\u00df", "fort\u00b7zu\u00b7kom\u00b7men", "du", "den", "Fu\u00df", "nicht", "m\u00fc\u00b7\u00dfest", "he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVIZU", "PPER", "ART", "NN", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Drum geh rechts oder links, wie's in den Sinn dir kommt,", "tokens": ["Drum", "geh", "rechts", "o\u00b7der", "links", ",", "wie's", "in", "den", "Sinn", "dir", "kommt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "KON", "ADV", "$,", "VVFIN", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und hebe so den Fu\u00df im Takte wie es frommt.", "tokens": ["Und", "he\u00b7be", "so", "den", "Fu\u00df", "im", "Tak\u00b7te", "wie", "es", "frommt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "KOKOM", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Im Wege magst du dich nach einer Blume b\u00fccken,", "tokens": ["Im", "We\u00b7ge", "magst", "du", "dich", "nach", "ei\u00b7ner", "Blu\u00b7me", "b\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht biegen aus dem Weg, um Blumen nur zu pfl\u00fccken.", "tokens": ["Nicht", "bie\u00b7gen", "aus", "dem", "Weg", ",", "um", "Blu\u00b7men", "nur", "zu", "pfl\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "APPR", "ART", "NN", "$,", "KOUI", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Stets eilen mu\u00dft du dich, doch nie dich \u00fcbereilen,", "tokens": ["Stets", "ei\u00b7len", "mu\u00dft", "du", "dich", ",", "doch", "nie", "dich", "\u00fc\u00b7be\u00b7rei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VMFIN", "PPER", "PRF", "$,", "ADV", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nie weilen ohne Noth, doch gern wo's Noth thut, weilen.", "tokens": ["Nie", "wei\u00b7len", "oh\u00b7ne", "Noth", ",", "doch", "gern", "wo's", "Noth", "thut", ",", "wei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$,", "ADV", "ADV", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Nie r\u00fcckwerts, wie gesagt, nur vorwerts mu\u00dft du gehn,", "tokens": ["Nie", "r\u00fcck\u00b7werts", ",", "wie", "ge\u00b7sagt", ",", "nur", "vor\u00b7werts", "mu\u00dft", "du", "gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "VVPP", "$,", "ADV", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und denken; doch erlaubt ist dir ein R\u00fcckwertssehn.", "tokens": ["Und", "den\u00b7ken", ";", "doch", "er\u00b7laubt", "ist", "dir", "ein", "R\u00fcck\u00b7werts\u00b7sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$.", "ADV", "VVPP", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Zum Vorwertskommen selbst mag das die Kraft dir st\u00e4rken,", "tokens": ["Zum", "Vor\u00b7werts\u00b7kom\u00b7men", "selbst", "mag", "das", "die", "Kraft", "dir", "st\u00e4r\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VMFIN", "PDS", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie weit du vorwerts schon gekommen bist, zu merken.", "tokens": ["Wie", "weit", "du", "vor\u00b7werts", "schon", "ge\u00b7kom\u00b7men", "bist", ",", "zu", "mer\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "ADV", "VVPP", "VAFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "So schreitest du von Schritt zu Schritt mit fester Ferse,", "tokens": ["So", "schrei\u00b7test", "du", "von", "Schritt", "zu", "Schritt", "mit", "fes\u00b7ter", "Fer\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Alswie ein Dichter r\u00fcckt vom Verse fort zum Verse.", "tokens": ["Als\u00b7wie", "ein", "Dich\u00b7ter", "r\u00fcckt", "vom", "Ver\u00b7se", "fort", "zum", "Ver\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Der auch nicht s\u00e4umen darf im steten Vorwertsdrang;", "tokens": ["Der", "auch", "nicht", "s\u00e4u\u00b7men", "darf", "im", "ste\u00b7ten", "Vor\u00b7werts\u00b7drang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKNEG", "VVINF", "VMFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und im Bewu\u00dftseyn geht, ein Gott lenk' ihm den Gang.", "tokens": ["Und", "im", "Be\u00b7wu\u00df\u00b7tseyn", "geht", ",", "ein", "Gott", "lenk'", "ihm", "den", "Gang", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}