{"dta.poem.13606": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Isegrimm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Aktenst\u00f6\u00dfe Nachts verschlingen,", "tokens": ["Ak\u00b7ten\u00b7st\u00f6\u00b7\u00dfe", "Nachts", "ver\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwatzen nach der Welt Gebrauch", "tokens": ["Schwat\u00b7zen", "nach", "der", "Welt", "Ge\u00b7brauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das gro\u00dfe Tret-Rad schwingen", "tokens": ["Und", "das", "gro\u00b7\u00dfe", "Tret\u00b7Rad", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein Ochs, das kann ich auch.", "tokens": ["Wie", "ein", "Ochs", ",", "das", "kann", "ich", "auch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PDS", "VMFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber glauben, da\u00df der Plunder", "tokens": ["A\u00b7ber", "glau\u00b7ben", ",", "da\u00df", "der", "Plun\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eben nicht der Plunder w\u00e4r',", "tokens": ["E\u00b7ben", "nicht", "der", "Plun\u00b7der", "w\u00e4r'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sondern ein hochwichtig Wunder,", "tokens": ["Son\u00b7dern", "ein", "hoch\u00b7wich\u00b7tig", "Wun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Das gelang mir nimmermehr.", "tokens": ["Das", "ge\u00b7lang", "mir", "nim\u00b7mer\u00b7mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber Andre \u00fcberwitzen,", "tokens": ["A\u00b7ber", "And\u00b7re", "\u00fc\u00b7ber\u00b7wit\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich mit dem Federkiel", "tokens": ["Da\u00df", "ich", "mit", "dem", "Fe\u00b7der\u00b7kiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nnt' den morschen Weltbau st\u00fctzen,", "tokens": ["K\u00f6nnt'", "den", "mor\u00b7schen", "Welt\u00b7bau", "st\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schien mir immer Narrenspiel.", "tokens": ["Schien", "mir", "im\u00b7mer", "Nar\u00b7ren\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und so, weil ich in dem Drehen", "tokens": ["Und", "so", ",", "weil", "ich", "in", "dem", "Dre\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dasteh' oft wie ein Pasquill,", "tokens": ["Da\u00b7steh'", "oft", "wie", "ein", "Pas\u00b7quill", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "L\u00e4\u00dft die Welt mich eben stehen \u2014", "tokens": ["L\u00e4\u00dft", "die", "Welt", "mich", "e\u00b7ben", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADV", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mag sie's halten, wie sie will!", "tokens": ["Mag", "sie's", "hal\u00b7ten", ",", "wie", "sie", "will", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}