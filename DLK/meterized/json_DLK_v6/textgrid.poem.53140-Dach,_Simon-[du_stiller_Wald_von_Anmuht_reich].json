{"textgrid.poem.53140": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[du stiller Wald von Anmuht reich]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du stiller Wald von Anmuht reich,", "tokens": ["Du", "stil\u00b7ler", "Wald", "von", "An\u00b7muht", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du freyes Feld, ihre klahre Quellen,", "tokens": ["Du", "frey\u00b7es", "Feld", ",", "ih\u00b7re", "klah\u00b7re", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die wie Christall vnd Bernstein hellen,", "tokens": ["Die", "wie", "Chris\u00b7tall", "vnd", "Bern\u00b7stein", "hel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "NE", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich halt am besten es mit euch:", "tokens": ["Ich", "halt", "am", "bes\u00b7ten", "es", "mit", "euch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKA", "ADJD", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr reisst mich von dem Hoffget\u00fcmmel", "tokens": ["Ihr", "reisst", "mich", "von", "dem", "Hoff\u00b7ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd zeigt mir hie den freyen Himmel.", "tokens": ["Vnd", "zeigt", "mir", "hie", "den", "frey\u00b7en", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Hie herbergt Lieb vnd Sicherheit,", "tokens": ["Hie", "her\u00b7bergt", "Lieb", "vnd", "Si\u00b7cher\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hie schl\u00e4gt vn\u00df mehr des H\u00f6chsten G\u00fcte", "tokens": ["Hie", "schl\u00e4gt", "vn\u00df", "mehr", "des", "H\u00f6chs\u00b7ten", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Al\u00df in den St\u00e4dten zu Gem\u00fchte,", "tokens": ["Al\u00df", "in", "den", "St\u00e4d\u00b7ten", "zu", "Ge\u00b7m\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da falsche Gunst, geschm\u00fcckter Neid,", "tokens": ["Da", "fal\u00b7sche", "Gunst", ",", "ge\u00b7schm\u00fcck\u00b7ter", "Neid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Stoltz, Eigennutz vnd tausend S\u00fcnden", "tokens": ["Stoltz", ",", "Ei\u00b7gen\u00b7nutz", "vnd", "tau\u00b7send", "S\u00fcn\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit tausent Straffen sich verbinden.", "tokens": ["Mit", "tau\u00b7sent", "Straf\u00b7fen", "sich", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du s\u00fcsse LandRuh, nimm mich an,", "tokens": ["Du", "s\u00fcs\u00b7se", "Land", "Ruh", ",", "nimm", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "NN", "$,", "VVIMP", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hie wil ich gern geruhig alten,", "tokens": ["Hie", "wil", "ich", "gern", "ge\u00b7ru\u00b7hig", "al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo stets von meinem Wolverhalten", "tokens": ["Wo", "stets", "von", "mei\u00b7nem", "Wol\u00b7ver\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Selbs Lufft vnd Himmel zeugen kan:", "tokens": ["Selbs", "Lufft", "vnd", "Him\u00b7mel", "zeu\u00b7gen", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In \u00e4rgerlichen St\u00e4dten leben", "tokens": ["In", "\u00e4r\u00b7ger\u00b7li\u00b7chen", "St\u00e4d\u00b7ten", "le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist zwischen Hell vnd Himmel schweben.", "tokens": ["Ist", "zwi\u00b7schen", "Hell", "vnd", "Him\u00b7mel", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NE", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}