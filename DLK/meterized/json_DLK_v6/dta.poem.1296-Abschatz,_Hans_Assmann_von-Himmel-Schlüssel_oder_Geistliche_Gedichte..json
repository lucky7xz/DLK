{"dta.poem.1296": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Himmel-Schl\u00fcssel  \n oder  \n Geistliche Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wir kommen auff die Welt befleckt/", "tokens": ["Wir", "kom\u00b7men", "auff", "die", "Welt", "be\u00b7fleckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit schn\u00f6der Erb-Seuch\u2019 angesteckt/", "tokens": ["Mit", "schn\u00f6\u00b7der", "Er\u00b7bSeuch'", "an\u00b7ge\u00b7steckt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und hegen des Verderbens Samen:", "tokens": ["Und", "he\u00b7gen", "des", "Ver\u00b7der\u00b7bens", "Sa\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch ist zu unsrer Reinigkeit", "tokens": ["Doch", "ist", "zu", "uns\u00b7rer", "Rei\u00b7nig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein heilsam Wasser-Bad bereit:", "tokens": ["Ein", "heil\u00b7sam", "Was\u00b7ser\u00b7Bad", "be\u00b7reit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NE", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Di\u00df brauchen wir auff GOttes Nahmen.", "tokens": ["Di\u00df", "brau\u00b7chen", "wir", "auff", "Got\u00b7tes", "Nah\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Des Vatern Gnade zum Gewinst/", "tokens": ["Des", "Va\u00b7tern", "Gna\u00b7de", "zum", "Ge\u00b7winst", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Sohnes blutiges Verdienst/", "tokens": ["Des", "Soh\u00b7nes", "blu\u00b7ti\u00b7ges", "Ver\u00b7dienst", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des reinen Geistes Ob-uns-schweben/", "tokens": ["Des", "rei\u00b7nen", "Geis\u00b7tes", "Ob\u00b7uns\u00b7schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "(gleichwie er sich der ersten Welt", "tokens": ["(", "gleich\u00b7wie", "er", "sich", "der", "ers\u00b7ten", "Welt"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "PPER", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nach Mosis Zeugnis f\u00fcrgestellt/)", "tokens": ["Nach", "Mo\u00b7sis", "Zeug\u00b7nis", "f\u00fcr\u00b7ge\u00b7stellt", "/", ")"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "NE", "VVFIN", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wird uns durch diesen Bund gegeben.", "tokens": ["Wird", "uns", "durch", "die\u00b7sen", "Bund", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Vernunfft/ die th\u00f6richte/ steht an/", "tokens": ["Ver\u00b7nunfft", "/", "die", "th\u00f6\u00b7rich\u00b7te", "/", "steht", "an", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "NN", "$(", "VVFIN", "PTKVZ", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Wie Wasser so viel w\u00fcrcken kan/", "tokens": ["Wie", "Was\u00b7ser", "so", "viel", "w\u00fcr\u00b7cken", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich geb\u2019 ihr aber zum Bescheide:", "tokens": ["Ich", "geb'", "ihr", "a\u00b7ber", "zum", "Be\u00b7schei\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df GOttes Wort und Krafft mit Flutt", "tokens": ["Da\u00df", "Got\u00b7tes", "Wort", "und", "Krafft", "mit", "Flutt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "NN", "KON", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verkn\u00fcpfft so grosse Wunder thut/", "tokens": ["Ver\u00b7kn\u00fcpfft", "so", "gros\u00b7se", "Wun\u00b7der", "thut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Glaube traut und sieht auff beyde.", "tokens": ["Der", "Glau\u00b7be", "traut", "und", "sieht", "auff", "bey\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "APPR", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Di\u00df Wasser soll ein Denck-Mahl seyn/", "tokens": ["Di\u00df", "Was\u00b7ser", "soll", "ein", "Den\u00b7ck\u00b7Mahl", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VMFIN", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Uns iederzeit zu halten rein/", "tokens": ["Uns", "ie\u00b7der\u00b7zeit", "zu", "hal\u00b7ten", "rein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Adams-L\u00fcste zu ers\u00e4uffen/", "tokens": ["Die", "A\u00b7dams\u00b7L\u00fcs\u00b7te", "zu", "er\u00b7s\u00e4uf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Damit ein neuer Mensch entsteh/", "tokens": ["Da\u00b7mit", "ein", "neu\u00b7er", "Mensch", "ent\u00b7steh", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und in desselben Wegen geh/", "tokens": ["Und", "in", "des\u00b7sel\u00b7ben", "We\u00b7gen", "geh", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der sich am Jordan selbst lie\u00df t\u00e4uffen.", "tokens": ["Der", "sich", "am", "Jor\u00b7dan", "selbst", "lie\u00df", "t\u00e4uf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "NE", "ADV", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Di\u00df Heyl-Bad hat mich auch genezt/", "tokens": ["Di\u00df", "Heyl\u00b7Bad", "hat", "mich", "auch", "ge\u00b7nezt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NE", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und in des H\u00f6chsten Huld gesezt/", "tokens": ["Und", "in", "des", "H\u00f6chs\u00b7ten", "Huld", "ge\u00b7sezt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gantz fest in seinen Bund geschlossen.", "tokens": ["Gantz", "fest", "in", "sei\u00b7nen", "Bund", "ge\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gott Vater/ Sohn und Geist verleih/", "tokens": ["Gott", "Va\u00b7ter", "/", "Sohn", "und", "Geist", "ver\u00b7leih", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$(", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Damit ich bleib/ und ewig sey", "tokens": ["Da\u00b7mit", "ich", "bleib", "/", "und", "e\u00b7wig", "sey"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$(", "KON", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gez\u00e4hlt zu deinen Reichs-Genossen.", "tokens": ["Ge\u00b7z\u00e4hlt", "zu", "dei\u00b7nen", "Reichs\u00b7Ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "$."], "mete