{"textgrid.poem.52507": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "27. Blume auf Guidos Grab", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ruh sanft, mit Erde leicht bedeckt,", "tokens": ["Ruh", "sanft", ",", "mit", "Er\u00b7de", "leicht", "be\u00b7deckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ruh sanft, du guter Knabe!", "tokens": ["Ruh", "sanft", ",", "du", "gu\u00b7ter", "Kna\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gott, dessen Hand die wei\u00dfe Rose weckt,", "tokens": ["Gott", ",", "des\u00b7sen", "Hand", "die", "wei\u00b7\u00dfe", "Ro\u00b7se", "weckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Weckt einst dich aus dem Grabe.", "tokens": ["Weckt", "einst", "dich", "aus", "dem", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PRF", "APPR", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Die Schaufel dort, in lockern Sand", "tokens": ["Die", "Schau\u00b7fel", "dort", ",", "in", "lo\u00b7ckern", "Sand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gesteckt von Totengr\u00e4bers Hand,", "tokens": ["Ge\u00b7steckt", "von", "To\u00b7ten\u00b7gr\u00e4\u00b7bers", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dein Totenkranz, der Liebe letzte Gabe,", "tokens": ["Dein", "To\u00b7ten\u00b7kranz", ",", "der", "Lie\u00b7be", "letz\u00b7te", "Ga\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sind nicht dein Ziel \u2013 nur Ziel von deiner Flucht;", "tokens": ["Sind", "nicht", "dein", "Ziel", "\u2013", "nur", "Ziel", "von", "dei\u00b7ner", "Flucht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PPOSAT", "NN", "$(", "ADV", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Erreicht hast du beim Morgenrot die Bucht,", "tokens": ["Er\u00b7reicht", "hast", "du", "beim", "Mor\u00b7gen\u00b7rot", "die", "Bucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Die mancher bang im Abendsturm erst sucht. \u2013", "tokens": ["Die", "man\u00b7cher", "bang", "im", "A\u00b7bend\u00b7sturm", "erst", "sucht", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PIAT", "ADJD", "APPRART", "NN", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Ruh sanft, du guter Knabe!", "tokens": ["Ruh", "sanft", ",", "du", "gu\u00b7ter", "Kna\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}