{"textgrid.poem.38438": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Wiegenlied", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6re mein Kindchen, was will ich dir singen,", "tokens": ["H\u00f6\u00b7re", "mein", "Kind\u00b7chen", ",", "was", "will", "ich", "dir", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "PWS", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+--+-++--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Aepfel und Birnen soll Vater mitbringen,", "tokens": ["A\u00b7e\u00b7pfel", "und", "Bir\u00b7nen", "soll", "Va\u00b7ter", "mit\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "NN", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Pflaumen, Rosinen und Feigen,", "tokens": ["Pflau\u00b7men", ",", "Ro\u00b7si\u00b7nen", "und", "Fei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Mein Kindchen soll schlafen und schweigen.", "tokens": ["Mein", "Kind\u00b7chen", "soll", "schla\u00b7fen", "und", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "KON", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}