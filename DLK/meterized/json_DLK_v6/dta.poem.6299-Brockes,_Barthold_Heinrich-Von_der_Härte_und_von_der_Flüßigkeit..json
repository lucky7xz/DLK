{"dta.poem.6299": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der H\u00e4rte und von der  \n Fl\u00fc\u00dfigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und also sind die C\u00f6rper fl\u00fc\u00dfig, hart,", "tokens": ["Und", "al\u00b7so", "sind", "die", "C\u00f6r\u00b7per", "fl\u00fc\u00b7\u00dfig", ",", "hart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sind fl\u00fc\u00dfend, feucht, sind trocken, d\u00fcrr, erstarrt,", "tokens": ["Sind", "fl\u00fc\u00b7\u00dfend", ",", "feucht", ",", "sind", "tro\u00b7cken", ",", "d\u00fcrr", ",", "er\u00b7starrt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "ADJD", "$,", "VAFIN", "ADJD", "$,", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nachdem die kleinen Theil, aus welchen sie bestehn,", "tokens": ["Nach\u00b7dem", "die", "klei\u00b7nen", "Theil", ",", "aus", "wel\u00b7chen", "sie", "be\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "APPR", "PWAT", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Getheilet und verbunden,", "tokens": ["Ge\u00b7thei\u00b7let", "und", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In ihnen sich gefunden.", "tokens": ["In", "ih\u00b7nen", "sich", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und werden sie schnell oder langsam gehn,", "tokens": ["Und", "wer\u00b7den", "sie", "schnell", "o\u00b7der", "lang\u00b7sam", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Nachdem die C\u00f6rper, die sie regen,", "tokens": ["Nach\u00b7dem", "die", "C\u00f6r\u00b7per", ",", "die", "sie", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und rings um ihnen sind, sich sanfft und starck bewegen.", "tokens": ["Und", "rings", "um", "ih\u00b7nen", "sind", ",", "sich", "sanfft", "und", "starck", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "VAFIN", "$,", "PRF", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}