{"dta.poem.3900": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "20) Schreiben an einen Freund,  \n dem er einen Brief der  Valiere  \u00fcbersendet.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Freund! da ich itzt mit Schrifft und Brief be-", "tokens": ["Freund", "!", "da", "ich", "itzt", "mit", "Schrifft", "und", "Brief", "be"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "KOUS", "PPER", "ADV", "APPR", "NN", "KON", "NN", "TRUNC"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "sch\u00e4fftigt bin,", "tokens": ["sch\u00e4ff\u00b7tigt", "bin", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "So nimm, wie du verlangst, auch diese Zeilen hin,", "tokens": ["So", "nimm", ",", "wie", "du", "ver\u00b7langst", ",", "auch", "die\u00b7se", "Zei\u00b7len", "hin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADV", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie ehmahls ", "tokens": ["Wie", "eh\u00b7mahls"], "token_info": ["word", "word"], "pos": ["PWAV", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Die letzt der S\u00fcnden-Last zur Busse angetrieben.", "tokens": ["Die", "letzt", "der", "S\u00fcn\u00b7den\u00b7Last", "zur", "Bus\u00b7se", "an\u00b7ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich habe dieses St\u00fcck von Wort zu Wort gesetzt,", "tokens": ["Ich", "ha\u00b7be", "die\u00b7ses", "St\u00fcck", "von", "Wort", "zu", "Wort", "ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDAT", "NN", "APPR", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Weil das Bewegliche dich offtermahls erg\u00f6tzt.", "tokens": ["Weil", "das", "Be\u00b7weg\u00b7li\u00b7che", "dich", "off\u00b7ter\u00b7mahls", "er\u00b7g\u00f6tzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Inhalt wird dir schon den alten Inhalt geben,", "tokens": ["Der", "In\u00b7halt", "wird", "dir", "schon", "den", "al\u00b7ten", "In\u00b7halt", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Indessen w\u00fcnsch ich dir vergn\u00fcgt und wohl zu leben.", "tokens": ["In\u00b7des\u00b7sen", "w\u00fcnsch", "ich", "dir", "ver\u00b7gn\u00fcgt", "und", "wohl", "zu", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "VVPP", "KON", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}