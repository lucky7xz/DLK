{"textgrid.poem.45824": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[weste s\u00e4useln; silbern wallen]", "genre": "verse", "period": "N.A.", "pub_year": 1799, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weste s\u00e4useln; silbern wallen", "tokens": ["Wes\u00b7te", "s\u00e4u\u00b7seln", ";", "sil\u00b7bern", "wal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVINF", "$.", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Locken um den Scheitel mir.", "tokens": ["Lo\u00b7cken", "um", "den", "Schei\u00b7tel", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meiner Harfe T\u00f6ne hallen", "tokens": ["Mei\u00b7ner", "Har\u00b7fe", "T\u00f6\u00b7ne", "hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sanfter durch die Felsen hier.", "tokens": ["Sanf\u00b7ter", "durch", "die", "Fel\u00b7sen", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus der ew'gen Ferne winken", "tokens": ["Aus", "der", "ew'\u00b7gen", "Fer\u00b7ne", "win\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tr\u00f6stend mir die Sterne zu.", "tokens": ["Tr\u00f6s\u00b7tend", "mir", "die", "Ster\u00b7ne", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Meine m\u00fcden Augen sinken", "tokens": ["Mei\u00b7ne", "m\u00fc\u00b7den", "Au\u00b7gen", "sin\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hin zur Erde, suchen Ruh'.", "tokens": ["Hin", "zur", "Er\u00b7de", ",", "su\u00b7chen", "Ruh'", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bald ach bald wird be\u00dfres Leben", "tokens": ["Bald", "ach", "bald", "wird", "be\u00df\u00b7res", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "VAFIN", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dieses m\u00fcde Herz erfreun,", "tokens": ["Die\u00b7ses", "m\u00fc\u00b7de", "Herz", "er\u00b7freun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Seele banges Streben", "tokens": ["Und", "der", "See\u00b7le", "ban\u00b7ges", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ewig dann gestillet sein.", "tokens": ["E\u00b7wig", "dann", "ge\u00b7stil\u00b7let", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schwarzer Grabesschatten dringet", "tokens": ["Schwar\u00b7zer", "Gra\u00b7bes\u00b7schat\u00b7ten", "drin\u00b7get"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Um den Tr\u00e4nenblick empor,", "tokens": ["Um", "den", "Tr\u00e4\u00b7nen\u00b7blick", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aus des Todes Asche ringet", "tokens": ["Aus", "des", "To\u00b7des", "A\u00b7sche", "rin\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sch\u00f6nre Hoffnung sich hervor.", "tokens": ["Sch\u00f6n\u00b7re", "Hoff\u00b7nung", "sich", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Meines Kindes Klage lallet", "tokens": ["Mei\u00b7nes", "Kin\u00b7des", "Kla\u00b7ge", "lal\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durchs Gew\u00f6lbe dumpf und hohl,", "tokens": ["Durchs", "Ge\u00b7w\u00f6l\u00b7be", "dumpf", "und", "hohl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Idolmios Zunge lallet", "tokens": ["I\u00b7dol\u00b7mi\u00b7os", "Zun\u00b7ge", "lal\u00b7let"], "token_info": ["word", "word", "word"], "pos": ["NE", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jammernd mir das Lebewohl", "tokens": ["Jam\u00b7mernd", "mir", "das", "Le\u00b7be\u00b7wohl"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zu der lang ersehnten Reise.", "tokens": ["Zu", "der", "lang", "er\u00b7sehn\u00b7ten", "Rei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Senkt mich in der Toten Reihn.", "tokens": ["Senkt", "mich", "in", "der", "To\u00b7ten", "Reihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Klaget nicht, denn sanft und leise", "tokens": ["Kla\u00b7get", "nicht", ",", "denn", "sanft", "und", "lei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$,", "KON", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wird des M\u00fcden Schlummer sein.", "tokens": ["Wird", "des", "M\u00fc\u00b7den", "Schlum\u00b7mer", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und du Gute nimmst die beiden", "tokens": ["Und", "du", "Gu\u00b7te", "nimmst", "die", "bei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "NN", "VVFIN", "ART", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00fctterlich in deinen Arm,", "tokens": ["M\u00fct\u00b7ter\u00b7lich", "in", "dei\u00b7nen", "Arm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Linderst meiner Tochter Leiden,", "tokens": ["Lin\u00b7derst", "mei\u00b7ner", "Toch\u00b7ter", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4chelst weg des Knaben Harm.", "tokens": ["L\u00e4\u00b7chelst", "weg", "des", "Kna\u00b7ben", "Harm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus des \u00c4thers lichter Ferne", "tokens": ["Aus", "des", "\u00c4\u00b7thers", "lich\u00b7ter", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Blickt dann Trost der Geist euch zu.", "tokens": ["Blickt", "dann", "Trost", "der", "Geist", "euch", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Es umarmen sich zwei Sterne", "tokens": ["Es", "um\u00b7ar\u00b7men", "sich", "zwei", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "CARD", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Und ihr Ku\u00df giebt allen Ruh'.", "tokens": ["Und", "ihr", "Ku\u00df", "giebt", "al\u00b7len", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Schwermut gl\u00e4nzt des Mondes Helle", "tokens": ["Schwer\u00b7mut", "gl\u00e4nzt", "des", "Mon\u00b7des", "Hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In mein tr\u00e4nenloses Aug',", "tokens": ["In", "mein", "tr\u00e4\u00b7nen\u00b7lo\u00b7ses", "Aug'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schatten schweben durch die Zelle,", "tokens": ["Schat\u00b7ten", "schwe\u00b7ben", "durch", "die", "Zel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seufzer lispeln, Geisterhauch", "tokens": ["Seuf\u00b7zer", "lis\u00b7peln", ",", "Geis\u00b7ter\u00b7hauch"], "token_info": ["word", "word", "punct", "word"], "pos": ["NN", "VVINF", "$,", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Rauschet bang durch meine Saiten,", "tokens": ["Rau\u00b7schet", "bang", "durch", "mei\u00b7ne", "Sai\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Horchend heb' ich nun die Hand,", "tokens": ["Hor\u00b7chend", "heb'", "ich", "nun", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und es pochen, Trost im Leiden,", "tokens": ["Und", "es", "po\u00b7chen", ",", "Trost", "im", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "$,", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Totenuhren in der Wand.", "tokens": ["To\u00b7te\u00b7nuh\u00b7ren", "in", "der", "Wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}