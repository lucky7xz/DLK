{"textgrid.poem.39587": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die veredelte Hexenzucht", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du willst in Furien die Hexen travestieren.", "tokens": ["Du", "willst", "in", "Fu\u00b7ri\u00b7en", "die", "He\u00b7xen", "tra\u00b7ves\u00b7tie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Meinst du, das sei die Art mit Hexen umzugehn?", "tokens": ["Meinst", "du", ",", "das", "sei", "die", "Art", "mit", "He\u00b7xen", "um\u00b7zu\u00b7gehn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PDS", "VAFIN", "ART", "NN", "APPR", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da werden beiderseits die Damen protestieren,", "tokens": ["Da", "wer\u00b7den", "bei\u00b7der\u00b7seits", "die", "Da\u00b7men", "pro\u00b7tes\u00b7tie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Shakspeare, Aeschylus, sich selbst nicht mehr verstehn.", "tokens": ["Und", "Shak\u00b7spea\u00b7re", ",", "A\u00b7esc\u00b7hy\u00b7lus", ",", "sich", "selbst", "nicht", "mehr", "ver\u00b7stehn", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "NE", "$,", "PRF", "ADV", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+---+-+-+-+-+", "measure": "dactylic.init"}}}}}