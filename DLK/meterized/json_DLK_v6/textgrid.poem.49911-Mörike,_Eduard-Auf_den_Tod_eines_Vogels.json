{"textgrid.poem.49911": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Tod eines Vogels", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Vogel, ist es aus mit dir?", "tokens": ["O", "Vo\u00b7gel", ",", "ist", "es", "aus", "mit", "dir", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VAFIN", "PPER", "APPR", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Krank \u00fcbergab ich dich Barmherzgen-Schwester-H\u00e4nden,", "tokens": ["Krank", "\u00fc\u00b7berg\u00b7ab", "ich", "dich", "Barm\u00b7her\u00b7zgen\u00b7Schwes\u00b7ter\u00b7H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ob sie vielleicht noch dein Verh\u00e4ngnis wenden;", "tokens": ["Ob", "sie", "viel\u00b7leicht", "noch", "dein", "Ver\u00b7h\u00e4ng\u00b7nis", "wen\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So war denn keine Hilfe hier?", "tokens": ["So", "war", "denn", "kei\u00b7ne", "Hil\u00b7fe", "hier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zwei Augen, schwarz als wie die deinen,", "tokens": ["Zwei", "Au\u00b7gen", ",", "schwarz", "als", "wie", "die", "dei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "ADJD", "KOKOM", "KOKOM", "ART", "PPOSAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sah ich mit deinem Blick sich einen,", "tokens": ["Sah", "ich", "mit", "dei\u00b7nem", "Blick", "sich", "ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PRF", "ART", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und gleich erlosch sein sch\u00f6nes Licht.", "tokens": ["Und", "gleich", "er\u00b7losch", "sein", "sch\u00f6\u00b7nes", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hast du von ihnen Leids erfahren?", "tokens": ["Hast", "du", "von", "ih\u00b7nen", "Leids", "er\u00b7fah\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "NE", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wohlan, wenn ", "tokens": ["Wo\u00b7hlan", ",", "wenn"], "token_info": ["word", "punct", "word"], "pos": ["ADV", "$,", "KOUS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "So war dein Tod so bitter nicht!", "tokens": ["So", "war", "dein", "Tod", "so", "bit\u00b7ter", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}