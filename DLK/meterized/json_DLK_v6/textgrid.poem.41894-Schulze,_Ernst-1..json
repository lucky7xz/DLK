{"textgrid.poem.41894": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus der Liebe raschen Tr\u00e4umen", "tokens": ["Aus", "der", "Lie\u00b7be", "ra\u00b7schen", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weckt mich strafend oft die Pflicht,", "tokens": ["Weckt", "mich", "stra\u00b7fend", "oft", "die", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das ernste Leben spricht:", "tokens": ["Und", "das", "erns\u00b7te", "Le\u00b7ben", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Willst du ewig hoffend s\u00e4umen,", "tokens": ["Willst", "du", "e\u00b7wig", "hof\u00b7fend", "s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo aus unfruchtbaren Keimen", "tokens": ["Wo", "aus", "un\u00b7frucht\u00b7ba\u00b7ren", "Kei\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nie die Bl\u00fcthe lohnend bricht?", "tokens": ["Nie", "die", "Bl\u00fc\u00b7the", "loh\u00b7nend", "bricht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sch\u00f6nre Myrten kannst du pfl\u00fccken,", "tokens": ["Sch\u00f6n\u00b7re", "Myr\u00b7ten", "kannst", "du", "pfl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dich mit sch\u00f6nerm Lorbeer schm\u00fccken,", "tokens": ["Dich", "mit", "sch\u00f6\u00b7nerm", "Lor\u00b7beer", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Viele werden hoch dich ehren,", "tokens": ["Vie\u00b7le", "wer\u00b7den", "hoch", "dich", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADJD", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Mag auch Eine dich verschm\u00e4hn. \u2013", "tokens": ["Mag", "auch", "Ei\u00b7ne", "dich", "ver\u00b7schm\u00e4hn", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "ADV", "ART", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Soll ich folgen? soll ich's h\u00f6ren?", "tokens": ["Soll", "ich", "fol\u00b7gen", "?", "soll", "ich's", "h\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$.", "VMFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Soll ich bleiben? soll ich gehn?", "tokens": ["Soll", "ich", "blei\u00b7ben", "?", "soll", "ich", "gehn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$.", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eurem Rufen, eurem Mahnen,", "tokens": ["Eu\u00b7rem", "Ru\u00b7fen", ",", "eu\u00b7rem", "Mah\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weise Stimmen, folgt' ich gern;", "tokens": ["Wei\u00b7se", "Stim\u00b7men", ",", "folgt'", "ich", "gern", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn verst\u00e4ndig r\u00e4th, wer fern", "tokens": ["Denn", "ver\u00b7st\u00e4n\u00b7dig", "r\u00e4\u00b7th", ",", "wer", "fern"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "$,", "PWS", "ADJD"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Stehet von des Unheils Bahnen;", "tokens": ["Ste\u00b7het", "von", "des", "Un\u00b7heils", "Bah\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch in tobenden Orkanen", "tokens": ["Doch", "in", "to\u00b7ben\u00b7den", "Or\u00b7ka\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Frommt nicht Anker mehr, noch Stern.", "tokens": ["Frommt", "nicht", "An\u00b7ker", "mehr", ",", "noch", "Stern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "NN", "ADV", "$,", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Stets gekr\u00e4nkt, mu\u00df ich vergeben,", "tokens": ["Stets", "ge\u00b7kr\u00e4nkt", ",", "mu\u00df", "ich", "ver\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "VMFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Stets verschm\u00e4ht, nur hei\u00dfer streben,", "tokens": ["Stets", "ver\u00b7schm\u00e4ht", ",", "nur", "hei\u00b7\u00dfer", "stre\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Mu\u00df die Geister selbst beschw\u00f6ren,", "tokens": ["Mu\u00df", "die", "Geis\u00b7ter", "selbst", "be\u00b7schw\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Die im Wirbelsturm mich drehn.", "tokens": ["Die", "im", "Wir\u00b7bels\u00b7turm", "mich", "drehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Ach, wenn G\u00f6tter uns beth\u00f6ren,", "tokens": ["Ach", ",", "wenn", "G\u00f6t\u00b7ter", "uns", "be\u00b7th\u00f6\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "K\u00f6nnen Menschen widerstehn?", "tokens": ["K\u00f6n\u00b7nen", "Men\u00b7schen", "wi\u00b7der\u00b7stehn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}