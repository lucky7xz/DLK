{"dta.poem.19047": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "XxVIII.  Auf der Frau  D.  Petersen Ein-  \n gang in die Freude.   Gedruckt zu Dre\u00dfden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "So gehest du dahin, vor unsers K\u00f6nigs Thron", "tokens": ["So", "ge\u00b7hest", "du", "da\u00b7hin", ",", "vor", "un\u00b7sers", "K\u00f6\u00b7nigs", "Thron"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "$,", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des lieben ", "tokens": ["Des", "lie\u00b7ben"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Gem\u00e4chlichkeit und Ruhm, Natur und Kunst verschertzen:", "tokens": ["Ge\u00b7m\u00e4ch\u00b7lich\u00b7keit", "und", "Ruhm", ",", "Na\u00b7tur", "und", "Kunst", "ver\u00b7schert\u00b7zen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das macht, sie sehen ihn in Lieb und Glauben schon,", "tokens": ["Das", "macht", ",", "sie", "se\u00b7hen", "ihn", "in", "Lieb", "und", "Glau\u00b7ben", "schon", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du siehst ihn nun, ", "tokens": ["Du", "siehst", "ihn", "nun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Wie ehemals am Creutz, zur rechten Hand der Kraft.", "tokens": ["Wie", "e\u00b7he\u00b7mals", "am", "Creutz", ",", "zur", "rech\u00b7ten", "Hand", "der", "Kraft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "O Freundin, g\u00f6nne uns, dir gl\u00e4ubig nachzusehn.", "tokens": ["O", "Freun\u00b7din", ",", "g\u00f6n\u00b7ne", "uns", ",", "dir", "gl\u00e4u\u00b7big", "nach\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "$,", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dir, deren Treue mich und andre mehr besch\u00e4met,", "tokens": ["Dir", ",", "de\u00b7ren", "Treu\u00b7e", "mich", "und", "and\u00b7re", "mehr", "be\u00b7sch\u00e4\u00b7met", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "PPER", "KON", "PIS", "ADV", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Die wir uns ebenfalls zum Creutze hin bequemet,", "tokens": ["Die", "wir", "uns", "e\u00b7ben\u00b7falls", "zum", "Creut\u00b7ze", "hin", "be\u00b7que\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADV", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und aber immer noch an einem Stecken gehn.", "tokens": ["Und", "a\u00b7ber", "im\u00b7mer", "noch", "an", "ei\u00b7nem", "Ste\u00b7cken", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach, m\u00f6cht ", "tokens": ["Ach", ",", "m\u00f6cht"], "token_info": ["word", "punct", "word"], "pos": ["ITJ", "$,", "VMFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "So gienge einst die Lust zu fremder Kraft verlohren.", "tokens": ["So", "gien\u00b7ge", "einst", "die", "Lust", "zu", "frem\u00b7der", "Kraft", "ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nicht ehe siegete der K\u00e4mpfer Jsrael,", "tokens": ["Nicht", "e\u00b7he", "sie\u00b7ge\u00b7te", "der", "K\u00e4mp\u00b7fer", "Js\u00b7rael", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Als bi\u00df ihm GOttes Kraft die H\u00fcfte ausgerencket,", "tokens": ["Als", "bi\u00df", "ihm", "Got\u00b7tes", "Kraft", "die", "H\u00fcf\u00b7te", "aus\u00b7ge\u00b7ren\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "NN", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo sich die gantze Macht des Leibes hingesencket:", "tokens": ["Wo", "sich", "die", "gant\u00b7ze", "Macht", "des", "Lei\u00b7bes", "hin\u00b7ge\u00b7sen\u00b7cket", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht ehe reisen wir mit Christo durch die H\u00f6ll;", "tokens": ["Nicht", "e\u00b7he", "rei\u00b7sen", "wir", "mit", "Chris\u00b7to", "durch", "die", "H\u00f6ll", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "PPER", "APPR", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es sey denn unsre Kunst und St\u00e4rcke gantz geschw\u00e4cht,", "tokens": ["Es", "sey", "denn", "uns\u00b7re", "Kunst", "und", "St\u00e4r\u00b7cke", "gantz", "ge\u00b7schw\u00e4cht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "KON", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wir elend und verdammt, und GOtt allein gerecht.", "tokens": ["Wir", "e\u00b7lend", "und", "ver\u00b7dammt", ",", "und", "Gott", "al\u00b7lein", "ge\u00b7recht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "VVPP", "$,", "KON", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}