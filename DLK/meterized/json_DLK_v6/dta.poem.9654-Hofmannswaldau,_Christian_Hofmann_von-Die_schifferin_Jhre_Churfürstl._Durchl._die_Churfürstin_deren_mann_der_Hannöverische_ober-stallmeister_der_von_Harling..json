{"dta.poem.9654": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die schifferin/ Jhre Churf\u00fcrstl. Durchl. die  \n Churf\u00fcrstin/ deren mann der Hann\u00f6verische  \n ober-stallmeister der von Harling.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der schiffer/ der bi\u00dfher nach Indien gefahren", "tokens": ["Der", "schif\u00b7fer", "/", "der", "bi\u00df\u00b7her", "nach", "In\u00b7di\u00b7en", "ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "$(", "ART", "ADV", "APPR", "NE", "VVPP"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und Mogols reichem schatz im handel nachgestellt/", "tokens": ["Und", "Mo\u00b7gols", "rei\u00b7chem", "schatz", "im", "han\u00b7del", "nach\u00b7ge\u00b7stellt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vergist nunmehr das gold aus jener fremden welt/", "tokens": ["Ver\u00b7gist", "nun\u00b7mehr", "das", "gold", "aus", "je\u00b7ner", "frem\u00b7den", "welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nun ihn des lo\u00dfes gunst so gl\u00fccklich wollen paaren.", "tokens": ["Nun", "ihn", "des", "lo\u00b7\u00dfes", "gunst", "so", "gl\u00fcck\u00b7lich", "wol\u00b7len", "paa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "ADJA", "NN", "ADV", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er spricht/ wie solt\u2019 ich mich den wellen anvertrauen/", "tokens": ["Er", "spricht", "/", "wie", "solt'", "ich", "mich", "den", "wel\u00b7len", "an\u00b7ver\u00b7trau\u00b7en", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWAV", "VMFIN", "PPER", "PRF", "ART", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was hat denn Indien das meinen sch\u00e4tzen gleich?", "tokens": ["Was", "hat", "denn", "In\u00b7di\u00b7en", "das", "mei\u00b7nen", "sch\u00e4t\u00b7zen", "gleich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "NE", "ART", "PPOSAT", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein blosser augenblick von meiner neuen frauen/", "tokens": ["Ein", "blos\u00b7ser", "au\u00b7gen\u00b7blick", "von", "mei\u00b7ner", "neu\u00b7en", "frau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vergn\u00fcgt und gilt auch mehr als Mogols gantzes reich.", "tokens": ["Ver\u00b7gn\u00fcgt", "und", "gilt", "auch", "mehr", "als", "Mo\u00b7gols", "gant\u00b7zes", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "ADV", "PIAT", "KOKOM", "NE", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}