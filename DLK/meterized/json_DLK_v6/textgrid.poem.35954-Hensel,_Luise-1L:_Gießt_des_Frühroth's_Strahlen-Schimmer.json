{"textgrid.poem.35954": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gie\u00dft des Fr\u00fchroth's Strahlen-Schimmer", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gie\u00dft des Fr\u00fchroth's Strahlen-Schimmer", "tokens": ["Gie\u00dft", "des", "Fr\u00fchro\u00b7th's", "Strah\u00b7len\u00b7Schim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rosen auf die Wangen mir,", "tokens": ["Ro\u00b7sen", "auf", "die", "Wan\u00b7gen", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Winkt der Sterne traut Geflimmer,", "tokens": ["Winkt", "der", "Ster\u00b7ne", "traut", "Ge\u00b7flim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gl\u00fcht der Mittag \u2013 nimmer, nimmer", "tokens": ["Gl\u00fcht", "der", "Mit\u00b7tag", "\u2013", "nim\u00b7mer", ",", "nim\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "ART", "NN", "$(", "ADV", "$,", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schweigt die Qual im Herzen hier.", "tokens": ["Schweigt", "die", "Qual", "im", "Her\u00b7zen", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn der Morgenr\u00f6the Strahlen", "tokens": ["Denn", "der", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wecken mir der Sehnsucht Weh'n,", "tokens": ["We\u00b7cken", "mir", "der", "Sehn\u00b7sucht", "Weh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! und der Erinn'rung Qualen:", "tokens": ["Ach", "!", "und", "der", "Er\u00b7inn'\u00b7rung", "Qua\u00b7len", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KON", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine Wangen mir zu malen", "tokens": ["Sei\u00b7ne", "Wan\u00b7gen", "mir", "zu", "ma\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gl\u00e4nzt der Morgen mir so sch\u00f6n.", "tokens": ["Gl\u00e4nzt", "der", "Mor\u00b7gen", "mir", "so", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und des gold'nen Mittags Schw\u00fcle", "tokens": ["Und", "des", "gold'\u00b7nen", "Mit\u00b7tags", "Schw\u00fc\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fand uns oft im Schattenthal,", "tokens": ["Fand", "uns", "oft", "im", "Schat\u00b7tent\u00b7hal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Strickend sa\u00df ich in der K\u00fchle,", "tokens": ["Stri\u00b7ckend", "sa\u00df", "ich", "in", "der", "K\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mein freundlicher Gespiele", "tokens": ["Und", "mein", "freund\u00b7li\u00b7cher", "Ge\u00b7spie\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Suchte Beeren mir zum Mahl. \u2013", "tokens": ["Such\u00b7te", "Bee\u00b7ren", "mir", "zum", "Mahl", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "NN", "PPER", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wehe! Seiner Augen Sonnen", "tokens": ["We\u00b7he", "!", "Sei\u00b7ner", "Au\u00b7gen", "Son\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seh' ich in der Sterne Licht; \u2013", "tokens": ["Seh'", "ich", "in", "der", "Ster\u00b7ne", "Licht", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles mahnt mich jener Wonnen \u2013", "tokens": ["Al\u00b7les", "mahnt", "mich", "je\u00b7ner", "Won\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PDAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine Treu' ist zwar zerronnen;", "tokens": ["Sei\u00b7ne", "Treu'", "ist", "zwar", "zer\u00b7ron\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch vergessen kann ich nicht. \u2013", "tokens": ["Doch", "ver\u00b7ges\u00b7sen", "kann", "ich", "nicht", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}