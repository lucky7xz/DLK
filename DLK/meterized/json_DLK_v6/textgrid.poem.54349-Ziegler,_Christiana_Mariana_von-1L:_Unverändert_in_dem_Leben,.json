{"textgrid.poem.54349": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Unver\u00e4ndert in dem Leben,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unver\u00e4ndert in dem Leben,", "tokens": ["Un\u00b7ver\u00b7\u00e4n\u00b7dert", "in", "dem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unver\u00e4ndert in dem Tod!", "tokens": ["Un\u00b7ver\u00b7\u00e4n\u00b7dert", "in", "dem", "Tod", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was mir Zeit und Gl\u00fcck wird geben,", "tokens": ["Was", "mir", "Zeit", "und", "Gl\u00fcck", "wird", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "KON", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Es sey Freude, es sey Noth;", "tokens": ["Es", "sey", "Freu\u00b7de", ",", "es", "sey", "Noth", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "PPER", "VAFIN", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Alles ist mir einerley.", "tokens": ["Al\u00b7les", "ist", "mir", "ei\u00b7ner\u00b7ley", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich bin immer froh und frey.", "tokens": ["Ich", "bin", "im\u00b7mer", "froh", "und", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Unver\u00e4ndert in den Worten,", "tokens": ["Un\u00b7ver\u00b7\u00e4n\u00b7dert", "in", "den", "Wor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unver\u00e4ndert in der That;", "tokens": ["Un\u00b7ver\u00b7\u00e4n\u00b7dert", "in", "der", "That", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gegen den an allen Orten,", "tokens": ["Ge\u00b7gen", "den", "an", "al\u00b7len", "Or\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "PIAT", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Welcher meine Freundschaft hat.", "tokens": ["Wel\u00b7cher", "mei\u00b7ne", "Freund\u00b7schaft", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn ich liebe nicht den Schein,", "tokens": ["Denn", "ich", "lie\u00b7be", "nicht", "den", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Falsch und hinterlistig seyn.", "tokens": ["Falsch", "und", "hin\u00b7ter\u00b7lis\u00b7tig", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Unver\u00e4ndert in der Liebe,", "tokens": ["Un\u00b7ver\u00b7\u00e4n\u00b7dert", "in", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unver\u00e4ndert bleibt die Lust;", "tokens": ["Un\u00b7ver\u00b7\u00e4n\u00b7dert", "bleibt", "die", "Lust", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn der Ursprung reiner Triebe", "tokens": ["Denn", "der", "Ur\u00b7sprung", "rei\u00b7ner", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Herrschet \u00fcber Geist und Brust.", "tokens": ["Herr\u00b7schet", "\u00fc\u00b7ber", "Geist", "und", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich verschenke nicht zum Scherz", "tokens": ["Ich", "ver\u00b7schen\u00b7ke", "nicht", "zum", "Scherz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Meine Freyheit und mein Herz.", "tokens": ["Mei\u00b7ne", "Frey\u00b7heit", "und", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Unver\u00e4ndert in dem Leiden,", "tokens": ["Un\u00b7ver\u00b7\u00e4n\u00b7dert", "in", "dem", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unver\u00e4ndert, ist der Schlu\u00df;", "tokens": ["Un\u00b7ver\u00b7\u00e4n\u00b7dert", ",", "ist", "der", "Schlu\u00df", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn man mich will schm\u00e4hen, neiden,", "tokens": ["Wenn", "man", "mich", "will", "schm\u00e4\u00b7hen", ",", "nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VMFIN", "VVINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weis ich doch nichts von Verdru\u00df.", "tokens": ["Weis", "ich", "doch", "nichts", "von", "Ver\u00b7dru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn nichts st\u00f6ret meine Ruh,", "tokens": ["Denn", "nichts", "st\u00f6\u00b7ret", "mei\u00b7ne", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ich lache noch darzu.", "tokens": ["Und", "ich", "la\u00b7che", "noch", "dar\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Unver\u00e4ndert in dem Gl\u00fccke,", "tokens": ["Un\u00b7ver\u00b7\u00e4n\u00b7dert", "in", "dem", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unver\u00e4ndert bleibt mein Sinn;", "tokens": ["Un\u00b7ver\u00b7\u00e4n\u00b7dert", "bleibt", "mein", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bey dem widrigsten Geschicke", "tokens": ["Bey", "dem", "wid\u00b7rigs\u00b7ten", "Ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Bleib ich immer wie ich bin.", "tokens": ["Bleib", "ich", "im\u00b7mer", "wie", "ich", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "KOKOM", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da verdoppelt sich mein Geist,", "tokens": ["Da", "ver\u00b7dop\u00b7pelt", "sich", "mein", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wann er sich dem Schmerz entrei\u00dft.", "tokens": ["Wann", "er", "sich", "dem", "Schmerz", "ent\u00b7rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Unver\u00e4ndert in den Sitten,", "tokens": ["Un\u00b7ver\u00b7\u00e4n\u00b7dert", "in", "den", "Sit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unver\u00e4ndert in dem Muth;", "tokens": ["Un\u00b7ver\u00b7\u00e4n\u00b7dert", "in", "dem", "Muth", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sollt ich um Vergebung bitten,", "tokens": ["Sollt", "ich", "um", "Ver\u00b7ge\u00b7bung", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00e4r die Handlung noch so gut?", "tokens": ["W\u00e4r", "die", "Hand\u00b7lung", "noch", "so", "gut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn ich mir nichts zeigen kann,", "tokens": ["Wenn", "ich", "mir", "nichts", "zei\u00b7gen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PIS", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "H\u00f6r ich keinen Tadler an.", "tokens": ["H\u00f6r", "ich", "kei\u00b7nen", "Tad\u00b7ler", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Unver\u00e4ndert in Gedanken,", "tokens": ["Un\u00b7ver\u00b7\u00e4n\u00b7dert", "in", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unver\u00e4ndert mu\u00df man seyn;", "tokens": ["Un\u00b7ver\u00b7\u00e4n\u00b7dert", "mu\u00df", "man", "seyn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PIS", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn die Gro\u00dfmuth l\u00e4\u00dft nicht wanken;", "tokens": ["Denn", "die", "Gro\u00df\u00b7muth", "l\u00e4\u00dft", "nicht", "wan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie r\u00e4umt keine Grillen ein.", "tokens": ["Sie", "r\u00e4umt", "kei\u00b7ne", "Gril\u00b7len", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ihre Wirkung ist zu sch\u00f6n,", "tokens": ["Ih\u00b7re", "Wir\u00b7kung", "ist", "zu", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie kann sich in nichts vergehn.", "tokens": ["Sie", "kann", "sich", "in", "nichts", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "PIS", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Unver\u00e4ndert in dem Hoffen,", "tokens": ["Un\u00b7ver\u00b7\u00e4n\u00b7dert", "in", "dem", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unver\u00e4ndert \u00fcberhaupt;", "tokens": ["Un\u00b7ver\u00b7\u00e4n\u00b7dert", "\u00fc\u00b7ber\u00b7haupt", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dadurch wird das Ziel getroffen,", "tokens": ["Da\u00b7durch", "wird", "das", "Ziel", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Das uns kein Vergn\u00fcgen raubt.", "tokens": ["Das", "uns", "kein", "Ver\u00b7gn\u00fc\u00b7gen", "raubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nun, ich sage frey heraus:", "tokens": ["Nun", ",", "ich", "sa\u00b7ge", "frey", "he\u00b7raus", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Dieser Schlu\u00df macht alles aus.", "tokens": ["Die\u00b7ser", "Schlu\u00df", "macht", "al\u00b7les", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}