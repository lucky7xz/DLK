{"dta.poem.12940": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "* * *", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es satzte Sylvio sich zu Lisetten nieder,", "tokens": ["Es", "satz\u00b7te", "Syl\u00b7vio", "sich", "zu", "Li\u00b7set\u00b7ten", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "PRF", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und sprach: Du hassest mich, weil mir ein schatz gebricht;", "tokens": ["Und", "sprach", ":", "Du", "has\u00b7sest", "mich", ",", "weil", "mir", "ein", "schatz", "ge\u00b7bricht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VAFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Allein wei\u00dft du denn nicht,", "tokens": ["Al\u00b7lein", "wei\u00dft", "du", "denn", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df die lieb\u2019 ein lohn der liebe, nicht ein lohn des goldes ist?", "tokens": ["Da\u00df", "die", "lieb'", "ein", "lohn", "der", "lie\u00b7be", ",", "nicht", "ein", "lohn", "des", "gol\u00b7des", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "VVFIN", "ART", "NN", "ART", "ADJA", "$,", "PTKNEG", "ART", "NN", "ART", "ADJA", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Wie? oder bin ich dir deswegen nur zuwider,", "tokens": ["Wie", "?", "o\u00b7der", "bin", "ich", "dir", "des\u00b7we\u00b7gen", "nur", "zu\u00b7wi\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KON", "VAFIN", "PPER", "PPER", "PAV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dieweil dein aug\u2019 an mir nichts zierliches erkiest?", "tokens": ["Die\u00b7weil", "dein", "aug'", "an", "mir", "nichts", "zier\u00b7li\u00b7ches", "er\u00b7kiest", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPER", "PIS", "ADJA", "VVFIN", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "So f\u00fchr es doch nur auf mein hertze,", "tokens": ["So", "f\u00fchr", "es", "doch", "nur", "auf", "mein", "hert\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das sich so sch\u00f6n, als dein gesichte, sch\u00e4tzt,", "tokens": ["Das", "sich", "so", "sch\u00f6n", ",", "als", "dein", "ge\u00b7sich\u00b7te", ",", "sch\u00e4tzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "PRF", "ADV", "ADJD", "$,", "KOUS", "PPOSAT", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Nachdem der liebe faust, die offtmals pfeil und kertze,", "tokens": ["Nach\u00b7dem", "der", "lie\u00b7be", "faust", ",", "die", "offt\u00b7mals", "pfeil", "und", "kert\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Zu kiel und griffel macht, dein bild darein ge\u00e4tzt.", "tokens": ["Zu", "kiel", "und", "grif\u00b7fel", "macht", ",", "dein", "bild", "da\u00b7rein", "ge\u00b7\u00e4tzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VVFIN", "$,", "PPOSAT", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Steht dir nun dieses, was dir gleichet,", "tokens": ["Steht", "dir", "nun", "die\u00b7ses", ",", "was", "dir", "glei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PDAT", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und dem sonst alle sch\u00f6nheit weichet,", "tokens": ["Und", "dem", "sonst", "al\u00b7le", "sch\u00f6n\u00b7heit", "wei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Lisette! nicht an meinem hertzen an;", "tokens": ["Li\u00b7set\u00b7te", "!", "nicht", "an", "mei\u00b7nem", "hert\u00b7zen", "an", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PTKNEG", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Was ist denn in der welt, so dir gefallen kan?", "tokens": ["Was", "ist", "denn", "in", "der", "welt", ",", "so", "dir", "ge\u00b7fal\u00b7len", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "APPR", "ART", "NN", "$,", "ADV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}