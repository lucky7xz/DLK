{"textgrid.poem.44409": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Himmel grau, die Erde wei\u00df,", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel grau, die Erde wei\u00df,", "tokens": ["Der", "Him\u00b7mel", "grau", ",", "die", "Er\u00b7de", "wei\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die B\u00e4ume kahl, die B\u00fcsche Gereis,", "tokens": ["Die", "B\u00e4u\u00b7me", "kahl", ",", "die", "B\u00fc\u00b7sche", "Ge\u00b7reis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ihr L\u00e4cheln den Fluren genommen.", "tokens": ["Ihr", "L\u00e4\u00b7cheln", "den", "Flu\u00b7ren", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mag zagen, wer will, mir wallet es hei\u00df,", "tokens": ["Mag", "za\u00b7gen", ",", "wer", "will", ",", "mir", "wal\u00b7let", "es", "hei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "PWS", "VMFIN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-++-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Ich nenne willkommen dich, blinkendes Eis,", "tokens": ["Ich", "nen\u00b7ne", "will\u00b7kom\u00b7men", "dich", ",", "blin\u00b7ken\u00b7des", "Eis", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Dich, starrender Winter, willkommen.", "tokens": ["Dich", ",", "star\u00b7ren\u00b7der", "Win\u00b7ter", ",", "will\u00b7kom\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Als noch die Menschheit im Lenze lag,", "tokens": ["Als", "noch", "die", "Menschheit", "im", "Len\u00b7ze", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da stand ihr wohl ein Fr\u00fchlingstag,", "tokens": ["Da", "stand", "ihr", "wohl", "ein", "Fr\u00fch\u00b7lings\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun mag sie sich anders erweisen.", "tokens": ["Nun", "mag", "sie", "sich", "an\u00b7ders", "er\u00b7wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Willkommen, ihr Felder, erstarrt und beschneit,", "tokens": ["Will\u00b7kom\u00b7men", ",", "ihr", "Fel\u00b7der", ",", "er\u00b7starrt", "und", "be\u00b7schneit", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Wir leben ja doch in eiserner Zeit,", "tokens": ["Wir", "le\u00b7ben", "ja", "doch", "in", "ei\u00b7ser\u00b7ner", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wohl paaret sich Eis zu dem Eisen.", "tokens": ["Wohl", "paa\u00b7ret", "sich", "Eis", "zu", "dem", "Ei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Des Dichters Leier verklingt, verstummt,", "tokens": ["Des", "Dich\u00b7ters", "Lei\u00b7er", "ver\u00b7klingt", ",", "ver\u00b7stummt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kaum da\u00df noch die Klage wie Heimchen summt,", "tokens": ["Kaum", "da\u00df", "noch", "die", "Kla\u00b7ge", "wie", "Heim\u00b7chen", "summt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ADV", "ART", "NN", "KOKOM", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Kein Spiel, kein Preis, kein Sieger.", "tokens": ["Kein", "Spiel", ",", "kein", "Preis", ",", "kein", "Sie\u00b7ger", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Drum fort ins Freie, die Waffe zur Hand,", "tokens": ["Drum", "fort", "ins", "Frei\u00b7e", ",", "die", "Waf\u00b7fe", "zur", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "APPRART", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Das Rohr gehoben, den Hahn gespannt,", "tokens": ["Das", "Rohr", "ge\u00b7ho\u00b7ben", ",", "den", "Hahn", "ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Als J\u00e4ger, wenn nicht als Krieger!", "tokens": ["Als", "J\u00e4\u00b7ger", ",", "wenn", "nicht", "als", "Krie\u00b7ger", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "KOUS", "PTKNEG", "KOUS", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Und wenn es knallt und wenn es trifft,", "tokens": ["Und", "wenn", "es", "knallt", "und", "wenn", "es", "trifft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "KON", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So denkt, es seien, die kochten das Gift,", "tokens": ["So", "denkt", ",", "es", "sei\u00b7en", ",", "die", "koch\u00b7ten", "das", "Gift", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPER", "VAFIN", "$,", "PRELS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Im Finstern horchen und harren.", "tokens": ["Im", "Fins\u00b7tern", "hor\u00b7chen", "und", "har\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "O Winter der Fluren! st\u00fcrme nur zu,", "tokens": ["O", "Win\u00b7ter", "der", "Flu\u00b7ren", "!", "st\u00fcr\u00b7me", "nur", "zu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$.", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der Geister Winter ist k\u00e4lter als du,", "tokens": ["Der", "Geis\u00b7ter", "Win\u00b7ter", "ist", "k\u00e4l\u00b7ter", "als", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "KOKOM", "PPER", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Er t\u00f6tet, du machest nur starren!", "tokens": ["Er", "t\u00f6\u00b7tet", ",", "du", "ma\u00b7chest", "nur", "star\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Nur abends, daheim am Feuerherd,", "tokens": ["Nur", "a\u00b7bends", ",", "da\u00b7heim", "am", "Feu\u00b7er\u00b7herd", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da sei euch ein einziger Seufzer gew\u00e4hrt", "tokens": ["Da", "sei", "euch", "ein", "ein\u00b7zi\u00b7ger", "Seuf\u00b7zer", "ge\u00b7w\u00e4hrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Nach Lenz und Bl\u00fcten und Fr\u00fcchten;", "tokens": ["Nach", "Lenz", "und", "Bl\u00fc\u00b7ten", "und", "Fr\u00fcch\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Des Morgens aber von neuem hinaus,", "tokens": ["Des", "Mor\u00b7gens", "a\u00b7ber", "von", "neu\u00b7em", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPR", "ADJA", "APZR", "$,"], "meter": "-+-+--+---", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "In Jagdgetos und Sturmgebraus,", "tokens": ["In", "Jagd\u00b7ge\u00b7tos", "und", "Sturm\u00b7ge\u00b7braus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Zwietracht des Innern zu schlichten.", "tokens": ["Die", "Zwiet\u00b7racht", "des", "In\u00b7nern", "zu", "schlich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}