{"textgrid.poem.29190": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00e4ntel zum bedecken,", "tokens": ["M\u00e4n\u00b7tel", "zum", "be\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Larven zum verstecken,", "tokens": ["Lar\u00b7ven", "zum", "ver\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "R\u00f6cke zum verkleiden,", "tokens": ["R\u00f6\u00b7cke", "zum", "ver\u00b7klei\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Scheren zum beschneiden,", "tokens": ["Sche\u00b7ren", "zum", "be\u00b7schnei\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Zangen zum verzwicken,", "tokens": ["Zan\u00b7gen", "zum", "ver\u00b7zwi\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Pressen, au\u00df zu dr\u00fccken,", "tokens": ["Pres\u00b7sen", ",", "au\u00df", "zu", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Pensel zum vergolden,", "tokens": ["Pen\u00b7sel", "zum", "ver\u00b7gol\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Blasen zum besolden,", "tokens": ["Bla\u00b7sen", "zum", "be\u00b7sol\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Pulster, ein zu wiegen,", "tokens": ["Puls\u00b7ter", ",", "ein", "zu", "wie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Brillen zu vergn\u00fcgen,", "tokens": ["Bril\u00b7len", "zu", "ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Fechel, Wind zu machen:", "tokens": ["Fe\u00b7chel", ",", "Wind", "zu", "ma\u00b7chen", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Mehr noch solche Sachen", "tokens": ["Mehr", "noch", "sol\u00b7che", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Sind bey Hof im Hauffen;", "tokens": ["Sind", "bey", "Hof", "im", "Hauf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NE", "APPRART", "NN", "$."], "meter": "--+-+-", "measure": "anapaest.init"}, "line.14": {"text": "Niemand darff sie kauffen.", "tokens": ["Nie\u00b7mand", "darff", "sie", "kauf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}