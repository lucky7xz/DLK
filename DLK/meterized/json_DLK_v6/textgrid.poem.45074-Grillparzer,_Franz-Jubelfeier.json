{"textgrid.poem.45074": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Jubelfeier", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mann bracht es auf siebzig gar;", "tokens": ["Der", "Mann", "bracht", "es", "auf", "sieb\u00b7zig", "gar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "CARD", "ADV", "$."], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das hei\u00dft: er blieb stets sieben Jahr,", "tokens": ["Das", "hei\u00dft", ":", "er", "blieb", "stets", "sie\u00b7ben", "Jahr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und all sein Leben von Kind bis jetzt", "tokens": ["Und", "all", "sein", "Le\u00b7ben", "von", "Kind", "bis", "jetzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "APPR", "NN", "ADV", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hat nur eine Null ihm zugesetzt.", "tokens": ["Hat", "nur", "ei\u00b7ne", "Null", "ihm", "zu\u00b7ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}