{"dta.poem.8486": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Gr\u00fc\u00dfe .", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Steigt mir in diesem fremden Lande", "tokens": ["Steigt", "mir", "in", "die\u00b7sem", "frem\u00b7den", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die allbekannte Nacht empor,", "tokens": ["Die", "all\u00b7be\u00b7kann\u00b7te", "Nacht", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Klatscht es, wie Hufesschlag vom Strande,", "tokens": ["Klatscht", "es", ",", "wie", "Hu\u00b7fes\u00b7schlag", "vom", "Stran\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PWAV", "NN", "APPRART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Rollt sich die D\u00e4mmerung hervor,", "tokens": ["Rollt", "sich", "die", "D\u00e4m\u00b7me\u00b7rung", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gleich Staubeswolken mir entgegen", "tokens": ["Gleich", "Stau\u00b7bes\u00b7wol\u00b7ken", "mir", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von meinem lieben starken Nord,", "tokens": ["Von", "mei\u00b7nem", "lie\u00b7ben", "star\u00b7ken", "Nord", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und f\u00fchl\u2019 ich meine Locken regen", "tokens": ["Und", "f\u00fchl'", "ich", "mei\u00b7ne", "Lo\u00b7cken", "re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Luft geheimni\u00dfvolles Wort.", "tokens": ["Der", "Luft", "ge\u00b7heim\u00b7ni\u00df\u00b7vol\u00b7les", "Wort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dann ist es mir, als h\u00f6r\u2019 ich reiten", "tokens": ["Dann", "ist", "es", "mir", ",", "als", "h\u00f6r'", "ich", "rei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$,", "KOUS", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und klirren und entgegenzieh\u2019n", "tokens": ["Und", "klir\u00b7ren", "und", "ent\u00b7ge\u00b7gen\u00b7zieh'n"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVINF", "KON", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Vaterland von allen Seiten,", "tokens": ["Mein", "Va\u00b7ter\u00b7land", "von", "al\u00b7len", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und seine K\u00fcsse f\u00fchl\u2019 ich gl\u00fch\u2019n;", "tokens": ["Und", "sei\u00b7ne", "K\u00fcs\u00b7se", "f\u00fchl'", "ich", "gl\u00fch'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann wird des Windes leises Munkeln", "tokens": ["Dann", "wird", "des", "Win\u00b7des", "lei\u00b7ses", "Mun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mir zu verworrnen Stimmen bald,", "tokens": ["Mir", "zu", "ver\u00b7worr\u00b7nen", "Stim\u00b7men", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und jede schwache Form im Dunkeln", "tokens": ["Und", "je\u00b7de", "schwa\u00b7che", "Form", "im", "Dun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zur tiefvertrautesten Gestalt.", "tokens": ["Zur", "tief\u00b7ver\u00b7trau\u00b7tes\u00b7ten", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und meine Arme mu\u00df ich strecken,", "tokens": ["Und", "mei\u00b7ne", "Ar\u00b7me", "mu\u00df", "ich", "stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00df K\u00fcsse, K\u00fcsse hauchen aus,", "tokens": ["Mu\u00df", "K\u00fcs\u00b7se", ",", "K\u00fcs\u00b7se", "hau\u00b7chen", "aus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "$,", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie sie die Leiber k\u00f6nnten recken,", "tokens": ["Wie", "sie", "die", "Lei\u00b7ber", "k\u00f6nn\u00b7ten", "re\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die modernden, im gr\u00fcnen Haus;", "tokens": ["Die", "mo\u00b7dern\u00b7den", ",", "im", "gr\u00fc\u00b7nen", "Haus", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mu\u00df jeden Waldeswipfel gr\u00fc\u00dfen,", "tokens": ["Mu\u00df", "je\u00b7den", "Wal\u00b7des\u00b7wip\u00b7fel", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und jede Haid\u2019 und jeden Bach,", "tokens": ["Und", "je\u00b7de", "Haid'", "und", "je\u00b7den", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und alle Tropfen, die da flie\u00dfen,", "tokens": ["Und", "al\u00b7le", "Trop\u00b7fen", ",", "die", "da", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und jedes H\u00e4lmchen, das noch wach.", "tokens": ["Und", "je\u00b7des", "H\u00e4lm\u00b7chen", ",", "das", "noch", "wach", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dir, Vaterhaus, mit deinen Th\u00fcrmen,", "tokens": ["Dir", ",", "Va\u00b7ter\u00b7haus", ",", "mit", "dei\u00b7nen", "Th\u00fcr\u00b7men", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom stillen Weiher eingewiegt,", "tokens": ["Vom", "stil\u00b7len", "Wei\u00b7her", "ein\u00b7ge\u00b7wiegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo ich in meines Lebens St\u00fcrmen,", "tokens": ["Wo", "ich", "in", "mei\u00b7nes", "Le\u00b7bens", "St\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So oft erlegen und gesiegt; \u2014", "tokens": ["So", "oft", "er\u00b7le\u00b7gen", "und", "ge\u00b7siegt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "VVPP", "KON", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr breiten, laubgew\u00f6lbten Hallen,", "tokens": ["Ihr", "brei\u00b7ten", ",", "laub\u00b7ge\u00b7w\u00f6lb\u00b7ten", "Hal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die jung und fr\u00f6hlich mich gesehn,", "tokens": ["Die", "jung", "und", "fr\u00f6h\u00b7lich", "mich", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo ewig meine Seufzer wallen,", "tokens": ["Wo", "e\u00b7wig", "mei\u00b7ne", "Seuf\u00b7zer", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und meines Fu\u00dfes Spuren stehn.", "tokens": ["Und", "mei\u00b7nes", "Fu\u00b7\u00dfes", "Spu\u00b7ren", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du feuchter Wind von meinen Haiden,", "tokens": ["Du", "feuch\u00b7ter", "Wind", "von", "mei\u00b7nen", "Hai\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der wie versch\u00e4mte Klage weint, \u2014", "tokens": ["Der", "wie", "ver\u00b7sch\u00e4m\u00b7te", "Kla\u00b7ge", "weint", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "KOKOM", "ADJA", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du Sonnenstrahl, der so bescheiden", "tokens": ["Du", "Son\u00b7nen\u00b7strahl", ",", "der", "so", "be\u00b7schei\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PRELS", "ADV", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf ihre Kr\u00e4uter niederscheint; \u2014", "tokens": ["Auf", "ih\u00b7re", "Kr\u00e4u\u00b7ter", "nie\u00b7der\u00b7scheint", ";"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr Gleise, die mich fortgetragen,", "tokens": ["Ihr", "Glei\u00b7se", ",", "die", "mich", "fort\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr Augen, die mir nachgeblinkt,", "tokens": ["Ihr", "Au\u00b7gen", ",", "die", "mir", "nach\u00b7ge\u00b7blinkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr Herzen, die mir nachgeschlagen,", "tokens": ["Ihr", "Her\u00b7zen", ",", "die", "mir", "nach\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ihr H\u00e4nde, die mir nachgewinkt.", "tokens": ["Ihr", "H\u00e4n\u00b7de", ",", "die", "mir", "nach\u00b7ge\u00b7winkt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und Gr\u00fc\u00dfe, Gr\u00fc\u00dfe, Dach, wo nimmer", "tokens": ["Und", "Gr\u00fc\u00b7\u00dfe", ",", "Gr\u00fc\u00b7\u00dfe", ",", "Dach", ",", "wo", "nim\u00b7mer"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "NN", "$,", "NN", "$,", "NN", "$,", "PWAV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die treu\u2019ste Seele mein vergi\u00dft,", "tokens": ["Die", "treu'\u00b7ste", "See\u00b7le", "mein", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und jetzt bei ihres L\u00e4mpchens Schimmer", "tokens": ["Und", "jetzt", "bei", "ih\u00b7res", "L\u00e4mp\u00b7chens", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr mich den Abendsegen liest,", "tokens": ["F\u00fcr", "mich", "den", "A\u00b7bend\u00b7se\u00b7gen", "liest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo bei des Hahnes erstem Kr\u00e4hen", "tokens": ["Wo", "bei", "des", "Hah\u00b7nes", "ers\u00b7tem", "Kr\u00e4\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie matt die graue Wimper streicht,", "tokens": ["Sie", "matt", "die", "grau\u00b7e", "Wim\u00b7per", "streicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und einmal noch vor Schlafengehen", "tokens": ["Und", "ein\u00b7mal", "noch", "vor", "Schla\u00b7fen\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "An mein verlassnes Lager schleicht.", "tokens": ["An", "mein", "ver\u00b7lass\u00b7nes", "La\u00b7ger", "schleicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}