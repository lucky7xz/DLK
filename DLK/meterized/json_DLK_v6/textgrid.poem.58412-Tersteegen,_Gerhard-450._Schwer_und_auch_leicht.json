{"textgrid.poem.58412": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "450. Schwer und auch leicht", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie tief ist unser gro\u00df Verderben,", "tokens": ["Wie", "tief", "ist", "un\u00b7ser", "gro\u00df", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es kostet bittres Kreuz und Sterben,", "tokens": ["Es", "kos\u00b7tet", "bitt\u00b7res", "Kreuz", "und", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Eh' man wird gr\u00fcndlich frei und rein!", "tokens": ["Eh'", "man", "wird", "gr\u00fcnd\u00b7lich", "frei", "und", "rein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VAFIN", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch wer in Gottes Hand sich giebet,", "tokens": ["Doch", "wer", "in", "Got\u00b7tes", "Hand", "sich", "gie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sich selbst vergi\u00dft und Gott nur liebet,", "tokens": ["Sich", "selbst", "ver\u00b7gi\u00dft", "und", "Gott", "nur", "lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVFIN", "KON", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der mag auch leicht erl\u00f6set sein.", "tokens": ["Der", "mag", "auch", "leicht", "er\u00b7l\u00f6\u00b7set", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}