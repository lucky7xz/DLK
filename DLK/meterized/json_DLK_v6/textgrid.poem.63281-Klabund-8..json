{"textgrid.poem.63281": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Anarchie ist unser Gl\u00fcck.", "tokens": ["Die", "An\u00b7ar\u00b7chie", "ist", "un\u00b7ser", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich reiss vom Leib mir St\u00fcck f\u00fcr St\u00fcck.", "tokens": ["Ich", "reiss", "vom", "Leib", "mir", "St\u00fcck", "f\u00fcr", "St\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPER", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zuerst den Rock, danach das Hemd \u2013", "tokens": ["Zu\u00b7erst", "den", "Rock", ",", "da\u00b7nach", "das", "Hemd", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PAV", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie war ich vor mir selber fremd.", "tokens": ["Wie", "war", "ich", "vor", "mir", "sel\u00b7ber", "fremd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "APPR", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich reiss die Augen aus der Stirn,", "tokens": ["Ich", "reiss", "die", "Au\u00b7gen", "aus", "der", "Stirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie sollen nicht das Licht verwirrn.", "tokens": ["Sie", "sol\u00b7len", "nicht", "das", "Licht", "ver\u00b7wirrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich zerr an Darm und Samenstrang:", "tokens": ["Ich", "zerr", "an", "Darm", "und", "Sa\u00b7mens\u00b7trang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein neuer Mensch mein Untergang.", "tokens": ["Kein", "neu\u00b7er", "Mensch", "mein", "Un\u00b7ter\u00b7gang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich reisse mir die Haut herab,", "tokens": ["Ich", "reis\u00b7se", "mir", "die", "Haut", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es f\u00e4llt der Plunder von mir ab.", "tokens": ["Es", "f\u00e4llt", "der", "Plun\u00b7der", "von", "mir", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich stehe nackt vor Tod und Grab.", "tokens": ["Ich", "ste\u00b7he", "nackt", "vor", "Tod", "und", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}