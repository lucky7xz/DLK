{"dta.poem.11741": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du sch\u00e4me dich vor Gott und dir in deinen Zellen,", "tokens": ["Du", "sch\u00e4\u00b7me", "dich", "vor", "Gott", "und", "dir", "in", "dei\u00b7nen", "Zel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "KON", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie in Gesellschaft du dich sch\u00e4mest vor Gesellen.", "tokens": ["Wie", "in", "Ge\u00b7sell\u00b7schaft", "du", "dich", "sch\u00e4\u00b7mest", "vor", "Ge\u00b7sel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "PPER", "PRF", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der unversch\u00e4mte sagt: da Gott es sieht in mir;", "tokens": ["Der", "un\u00b7ver\u00b7sch\u00e4m\u00b7te", "sagt", ":", "da", "Gott", "es", "sieht", "in", "mir", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$.", "KOUS", "NN", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Scheut' ich dich mehr als ihn, um es zu bergen dir?", "tokens": ["Scheut'", "ich", "dich", "mehr", "als", "ihn", ",", "um", "es", "zu", "ber\u00b7gen", "dir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "KOUS", "PPER", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "PPER", "$."], "meter": "+--+--+----+", "measure": "dactylic.tri.plus"}}, "stanza.3": {"line.1": {"text": "Doch der besch\u00e4mte sagt: da Gott in mir es schaut,", "tokens": ["Doch", "der", "be\u00b7sch\u00e4m\u00b7te", "sagt", ":", "da", "Gott", "in", "mir", "es", "schaut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "$.", "KOUS", "NN", "APPR", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und es verzeiht, sei dir's auch zum Verzeihn vertraut.", "tokens": ["Und", "es", "ver\u00b7zeiht", ",", "sei", "dir's", "auch", "zum", "Ver\u00b7zeihn", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "VAFIN", "PIS", "ADV", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}