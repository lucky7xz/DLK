{"textgrid.poem.36050": {"metadata": {"author": {"name": "Bechstein, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Xi. Der Lustgarten.", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bblasst Schellen hell erklingen bei buntem Mummenschanz!", "tokens": ["\u00bb", "lasst", "Schel\u00b7len", "hell", "er\u00b7klin\u00b7gen", "bei", "bun\u00b7tem", "Mum\u00b7men\u00b7schanz", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "NN", "ADJD", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Lasst muntre G\u00e4ste singen und springen froh im Tanz!\u00ab", "tokens": ["Lasst", "mun\u00b7tre", "G\u00e4s\u00b7te", "sin\u00b7gen", "und", "sprin\u00b7gen", "froh", "im", "Tanz", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADJA", "NN", "VVINF", "KON", "VVFIN", "ADJD", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "So Faustus ruft, der Festlust als Lebenslust erk\u00fcrt,", "tokens": ["So", "Faus\u00b7tus", "ruft", ",", "der", "Fest\u00b7lust", "als", "Le\u00b7bens\u00b7lust", "er\u00b7k\u00fcrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "$,", "ART", "NN", "KOUS", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und, jeden Schmerz bet\u00e4ubend, oft bunte Reigen f\u00fchrt.", "tokens": ["Und", ",", "je\u00b7den", "Schmerz", "be\u00b7t\u00e4u\u00b7bend", ",", "oft", "bun\u00b7te", "Rei\u00b7gen", "f\u00fchrt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PIAT", "NN", "VVPP", "$,", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Jetzt nah'n in vollen Schlitten sich Masken sonder Zahl,", "tokens": ["Jetzt", "nah'n", "in", "vol\u00b7len", "Schlit\u00b7ten", "sich", "Mas\u00b7ken", "son\u00b7der", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "PRF", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die gr\u00fcsset Wagner h\u00f6flich, weit \u00f6ffnet er den Saal.", "tokens": ["Die", "gr\u00fcs\u00b7set", "Wag\u00b7ner", "h\u00f6f\u00b7lich", ",", "weit", "\u00f6ff\u00b7net", "er", "den", "Saal", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "ADJD", "$,", "ADJD", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Die Winterh\u00fcllen fallen, manch sch\u00f6nes Auge strahlt,", "tokens": ["Die", "Win\u00b7ter\u00b7h\u00fcl\u00b7len", "fal\u00b7len", ",", "manch", "sch\u00f6\u00b7nes", "Au\u00b7ge", "strahlt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und r\u00f6ther unter Larven sich manche Wange malt.", "tokens": ["Und", "r\u00f6\u00b7ther", "un\u00b7ter", "Lar\u00b7ven", "sich", "man\u00b7che", "Wan\u00b7ge", "malt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "PRF", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00bbwillkommen, froh willkommen, Freundinnen, hold und fein!", "tokens": ["\u00bb", "will\u00b7kom\u00b7men", ",", "froh", "will\u00b7kom\u00b7men", ",", "Freun\u00b7din\u00b7nen", ",", "hold", "und", "fein", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "ADJD", "ADJD", "$,", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Willkommen edle Frauen und Herren! Nur herein!", "tokens": ["Will\u00b7kom\u00b7men", "ed\u00b7le", "Frau\u00b7en", "und", "Her\u00b7ren", "!", "Nur", "her\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "KON", "NN", "$.", "ADV", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Auch Gruss den Musens\u00f6hnen, von edlem Blut entstammt!", "tokens": ["Auch", "Gruss", "den", "Mu\u00b7sen\u00b7s\u00f6h\u00b7nen", ",", "von", "ed\u00b7lem", "Blut", "ent\u00b7stammt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "NN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Zu dienen hier dem Sch\u00f6nen ist einzig Euer Amt!\u00ab", "tokens": ["Zu", "die\u00b7nen", "hier", "dem", "Sch\u00f6\u00b7nen", "ist", "ein\u00b7zig", "Eu\u00b7er", "Amt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKZU", "VVINF", "ADV", "ART", "NN", "VAFIN", "ADJD", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Jungen wie die Alten gr\u00fcsst so der Herr vom Haus;", "tokens": ["Die", "Jun\u00b7gen", "wie", "die", "Al\u00b7ten", "gr\u00fcs\u00b7st", "so", "der", "Herr", "vom", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "\u00bbden Winter nur, den kalten, ihr Freunde, lasst mir draus!", "tokens": ["\u00bb", "den", "Win\u00b7ter", "nur", ",", "den", "kal\u00b7ten", ",", "ihr", "Freun\u00b7de", ",", "lasst", "mir", "draus", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ADV", "$,", "ART", "ADJA", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "PAV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Er ist ein gr\u00e4mlichfinstrer, murrk\u00f6pfiger Gesell;", "tokens": ["Er", "ist", "ein", "gr\u00e4m\u00b7lich\u00b7finst\u00b7rer", ",", "murr\u00b7k\u00f6p\u00b7fi\u00b7ger", "Ge\u00b7sell", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "Ich mag ihn nicht, und schaffte den Sommer uns zur Stell!\u00ab", "tokens": ["Ich", "mag", "ihn", "nicht", ",", "und", "schaff\u00b7te", "den", "Som\u00b7mer", "uns", "zur", "Stell", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "$,", "KON", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Und durch die weite Halle die G\u00e4ste staunend gehn;", "tokens": ["Und", "durch", "die", "wei\u00b7te", "Hal\u00b7le", "die", "G\u00e4s\u00b7te", "stau\u00b7nend", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Verwundrung fesselt alle, wie sie das Wunder sehn.", "tokens": ["Ver\u00b7wund\u00b7rung", "fes\u00b7selt", "al\u00b7le", ",", "wie", "sie", "das", "Wun\u00b7der", "sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "$,", "PWAV", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Hier gr\u00fcnen B\u00e4ume, Lauben, der Rasen bunt geschm\u00fcckt;", "tokens": ["Hier", "gr\u00fc\u00b7nen", "B\u00e4u\u00b7me", ",", "Lau\u00b7ben", ",", "der", "Ra\u00b7sen", "bunt", "ge\u00b7schm\u00fcckt", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "NN", "$,", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Dort harren Purpurtrauben des Winzers, der sie pfl\u00fcckt.", "tokens": ["Dort", "har\u00b7ren", "Pur\u00b7pur\u00b7trau\u00b7ben", "des", "Win\u00b7zers", ",", "der", "sie", "pfl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Dort gl\u00fchen saft'ge Pfirschen rings an der Mauerwand,", "tokens": ["Dort", "gl\u00fc\u00b7hen", "saft'\u00b7ge", "Pfir\u00b7schen", "rings", "an", "der", "Mau\u00b7er\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dort neigen rothe Kirschen sich schwellend in die Hand.", "tokens": ["Dort", "nei\u00b7gen", "ro\u00b7the", "Kir\u00b7schen", "sich", "schwel\u00b7lend", "in", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "PRF", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dort gl\u00e4nzen, hell wie Sterne, Orangen licht im Gr\u00fcn,", "tokens": ["Dort", "gl\u00e4n\u00b7zen", ",", "hell", "wie", "Ster\u00b7ne", ",", "O\u00b7ran\u00b7gen", "licht", "im", "Gr\u00fcn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "ADJD", "KOKOM", "NN", "$,", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Dort flammt in h\u00f6h'rer R\u00f6the Granatengluthrubin.", "tokens": ["Dort", "flammt", "in", "h\u00f6h'\u00b7rer", "R\u00f6\u00b7the", "Gra\u00b7na\u00b7ten\u00b7glut\u00b7hru\u00b7bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Und Blumen bl\u00fchn in F\u00fclle, wie sie der Sommer zeugt.", "tokens": ["Und", "Blu\u00b7men", "bl\u00fchn", "in", "F\u00fcl\u00b7le", ",", "wie", "sie", "der", "Som\u00b7mer", "zeugt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dort Tulpen, stolzaufragend, Narzissen, sanft gebeugt.", "tokens": ["Dort", "Tul\u00b7pen", ",", "stolz\u00b7auf\u00b7ra\u00b7gend", ",", "Nar\u00b7zis\u00b7sen", ",", "sanft", "ge\u00b7beugt", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADJD", "$,", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "-+--+---+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wo hoch auf schlankem Stengel sich K\u00f6n'gin Rose wiegt,", "tokens": ["Wo", "hoch", "auf", "schlan\u00b7kem", "Sten\u00b7gel", "sich", "K\u00f6n'\u00b7gin", "Ro\u00b7se", "wiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "ADJA", "NN", "PRF", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ist ihr als Mitregentin die Lilie zugef\u00fcgt.", "tokens": ["Ist", "ihr", "als", "Mit\u00b7re\u00b7gen\u00b7tin", "die", "Li\u00b7lie", "zu\u00b7ge\u00b7f\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KOUS", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Und heitres Leben regt sich ringsum in Busch und Baum,", "tokens": ["Und", "heit\u00b7res", "Le\u00b7ben", "regt", "sich", "ring\u00b7sum", "in", "Busch", "und", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PRF", "ADV", "APPR", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Singt, flattert, schwirrt und gaukelt umher im gr\u00fcnen Raum.", "tokens": ["Singt", ",", "flat\u00b7tert", ",", "schwirrt", "und", "gau\u00b7kelt", "um\u00b7her", "im", "gr\u00fc\u00b7nen", "Raum", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "PTKVZ", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dort V\u00f6glein, goldgefiedert, dort seltner K\u00e4fer Glanz;", "tokens": ["Dort", "V\u00f6\u00b7glein", ",", "gold\u00b7ge\u00b7fie\u00b7dert", ",", "dort", "selt\u00b7ner", "K\u00e4\u00b7fer", "Glanz", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "VVFIN", "$,", "ADV", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Dort buhlen Schmetterlinge, dort M\u00fccken froh im Tanz.", "tokens": ["Dort", "buh\u00b7len", "Schmet\u00b7ter\u00b7lin\u00b7ge", ",", "dort", "M\u00fc\u00b7cken", "froh", "im", "Tanz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADV", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Die V\u00f6gel singen freudig, die Quellen rieseln traut;", "tokens": ["Die", "V\u00f6\u00b7gel", "sin\u00b7gen", "freu\u00b7dig", ",", "die", "Quel\u00b7len", "rie\u00b7seln", "traut", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die M\u00e4dchen winden Kr\u00e4nze, die M\u00e4nner plaudern laut.", "tokens": ["Die", "M\u00e4d\u00b7chen", "win\u00b7den", "Kr\u00e4n\u00b7ze", ",", "die", "M\u00e4n\u00b7ner", "plau\u00b7dern", "laut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und rings ist Scherz und Jubel, und Sommersonnenschein,", "tokens": ["Und", "rings", "ist", "Scherz", "und", "Ju\u00b7bel", ",", "und", "Som\u00b7mer\u00b7son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "KON", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Nur von beschneiten D\u00e4chern schaut ernst der Winter drein.", "tokens": ["Nur", "von", "be\u00b7schnei\u00b7ten", "D\u00e4\u00b7chern", "schaut", "ernst", "der", "Win\u00b7ter", "drein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-++-+-+", "measure": "unknown.measure.septa"}}, "stanza.10": {"line.1": {"text": "Und sich vor langen Zeiten befreundet mit der ", "tokens": ["Und", "sich", "vor", "lan\u00b7gen", "Zei\u00b7ten", "be\u00b7freun\u00b7det", "mit", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "APPR", "ADJA", "NN", "VVFIN", "APPR", "ART"], "meter": "---+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die blickt, ein hartes Steinbild, erstarrend und erstarrt,", "tokens": ["Die", "blickt", ",", "ein", "har\u00b7tes", "Stein\u00b7bild", ",", "er\u00b7star\u00b7rend", "und", "er\u00b7starrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ART", "ADJA", "NN", "$,", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Tr\u00fcb in den Sommernachttraum der kurzen Gegenwart. \u2013", "tokens": ["Tr\u00fcb", "in", "den", "Som\u00b7mer