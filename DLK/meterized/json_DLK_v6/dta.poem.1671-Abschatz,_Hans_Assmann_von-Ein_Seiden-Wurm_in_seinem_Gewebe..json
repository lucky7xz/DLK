{"dta.poem.1671": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ein Seiden-Wurm in seinem  \n Gewebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Mir zum Grabe/ dir zum Kleide", "tokens": ["Mir", "zum", "Gra\u00b7be", "/", "dir", "zum", "Klei\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "$(", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spinn ich die ber\u00fchmte Seyde", "tokens": ["Spinn", "ich", "die", "be\u00b7r\u00fchm\u00b7te", "Sey\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Selbst aus meinem Eingeweyde:", "tokens": ["Selbst", "aus", "mei\u00b7nem", "Ein\u00b7ge\u00b7wey\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Andern dienen/ sich verzehren/", "tokens": ["An\u00b7dern", "die\u00b7nen", "/", "sich", "ver\u00b7zeh\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "VVINF", "$(", "PRF", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kan dem Grabe zwar gew\u00e4hren", "tokens": ["Kan", "dem", "Gra\u00b7be", "zwar", "ge\u00b7w\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch im Tode Lob geb\u00e4hren.", "tokens": ["Doch", "im", "To\u00b7de", "Lob", "ge\u00b7b\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}