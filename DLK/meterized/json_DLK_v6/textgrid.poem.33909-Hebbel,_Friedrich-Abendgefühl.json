{"textgrid.poem.33909": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Abendgef\u00fchl", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Friedlich bek\u00e4mpfen", "tokens": ["Fried\u00b7lich", "be\u00b7k\u00e4mp\u00b7fen"], "token_info": ["word", "word"], "pos": ["ADJD", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Nacht sich und Tag.", "tokens": ["Nacht", "sich", "und", "Tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "KON", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wie das zu d\u00e4mpfen,", "tokens": ["Wie", "das", "zu", "d\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wie das zu l\u00f6sen vermag!", "tokens": ["Wie", "das", "zu", "l\u00f6\u00b7sen", "ver\u00b7mag", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Der mich bedr\u00fcckte,", "tokens": ["Der", "mich", "be\u00b7dr\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,"], "meter": "++-+-", "measure": "iambic.di"}, "line.2": {"text": "Schl\u00e4fst du schon, Schmerz?", "tokens": ["Schl\u00e4fst", "du", "schon", ",", "Schmerz", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Was mich begl\u00fcckte,", "tokens": ["Was", "mich", "be\u00b7gl\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Sage, was war's doch, mein Herz?", "tokens": ["Sa\u00b7ge", ",", "was", "wa\u00b7r's", "doch", ",", "mein", "Herz", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VAFIN", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Freude, wie Kummer,", "tokens": ["Freu\u00b7de", ",", "wie", "Kum\u00b7mer", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "F\u00fchl' ich, zerrann,", "tokens": ["F\u00fchl'", "ich", ",", "zer\u00b7rann", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Aber den Schlummer", "tokens": ["A\u00b7ber", "den", "Schlum\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "F\u00fchrten sie leise heran.", "tokens": ["F\u00fchr\u00b7ten", "sie", "lei\u00b7se", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Und im Entschweben,", "tokens": ["Und", "im", "Ent\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Immer empor,", "tokens": ["Im\u00b7mer", "em\u00b7por", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Kommt mir das Leben", "tokens": ["Kommt", "mir", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ganz, wie ein Schlummerlied vor.", "tokens": ["Ganz", ",", "wie", "ein", "Schlum\u00b7mer\u00b7lied", "vor", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}