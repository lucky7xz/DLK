{"dta.poem.3892": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Vor der Liebe mich zu schmiegen,", "tokens": ["Vor", "der", "Lie\u00b7be", "mich", "zu", "schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kleine Kinder einzuwiegen,", "tokens": ["Klei\u00b7ne", "Kin\u00b7der", "ein\u00b7zu\u00b7wie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieses ist mir h\u00f6chst verha\u00dft,", "tokens": ["Die\u00b7ses", "ist", "mir", "h\u00f6chst", "ver\u00b7ha\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Das dient mir zum Zeitvertreibe,", "tokens": ["Das", "dient", "mir", "zum", "Zeit\u00b7ver\u00b7trei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wenn ich jung und ledig bleibe,", "tokens": ["Wenn", "ich", "jung", "und", "le\u00b7dig", "blei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lieben ist mir eine Last.", "tokens": ["Lie\u00b7ben", "ist", "mir", "ei\u00b7ne", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Weicht, ihr Fr\u00fcchte falscher Liebe,", "tokens": ["Weicht", ",", "ihr", "Fr\u00fcch\u00b7te", "fal\u00b7scher", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eitle Tr\u00e4ume banger Triebe!", "tokens": ["Eit\u00b7le", "Tr\u00e4u\u00b7me", "ban\u00b7ger", "Trie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Lieben kan und will ich nicht.", "tokens": ["Lie\u00b7ben", "kan", "und", "will", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VMFIN", "KON", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Wer wird Sch\u00e4fer-Worten trauen?", "tokens": ["Wer", "wird", "Sch\u00e4\u00b7fer\u00b7Wor\u00b7ten", "trau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wer wird auf Versprechen bauen?", "tokens": ["Wer", "wird", "auf", "Ver\u00b7spre\u00b7chen", "bau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Da die Treu so leichte bricht,", "tokens": ["Da", "die", "Treu", "so", "leich\u00b7te", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJA", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.13": {"text": "Weicht, ihr Fr\u00fcchte falscher Liebe,", "tokens": ["Weicht", ",", "ihr", "Fr\u00fcch\u00b7te", "fal\u00b7scher", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Eitle Tr\u00e4ume banger Triebe,", "tokens": ["Eit\u00b7le", "Tr\u00e4u\u00b7me", "ban\u00b7ger", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Lieben kan und will ich nicht.", "tokens": ["Lie\u00b7ben", "kan", "und", "will", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VMFIN", "KON", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}