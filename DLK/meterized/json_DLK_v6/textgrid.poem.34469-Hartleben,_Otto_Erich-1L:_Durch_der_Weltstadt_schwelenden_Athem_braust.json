{"textgrid.poem.34469": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Durch der Weltstadt schwelenden Athem braust", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch der Weltstadt schwelenden Athem braust", "tokens": ["Durch", "der", "Welt\u00b7stadt", "schwe\u00b7len\u00b7den", "A\u00b7them", "braust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "vorw\u00e4rts mit ehernen Schl\u00e4gen der Zug.", "tokens": ["vor\u00b7w\u00e4rts", "mit", "e\u00b7her\u00b7nen", "Schl\u00e4\u00b7gen", "der", "Zug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Da lehnt ein Mann am Fenster und starrt", "tokens": ["Da", "lehnt", "ein", "Mann", "am", "Fens\u00b7ter", "und", "starrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "KON", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "hoch \u00fcber die Stadt in die Ferne:", "tokens": ["hoch", "\u00fc\u00b7ber", "die", "Stadt", "in", "die", "Fer\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "D\u00fcsterroth, hart aneinander, endlos", "tokens": ["D\u00fcs\u00b7ter\u00b7roth", ",", "hart", "an\u00b7ein\u00b7an\u00b7der", ",", "end\u00b7los"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "$,", "ADJD", "ADV", "$,", "ADJD"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "scharen sich die Wolkenreihn dort oben!", "tokens": ["scha\u00b7ren", "sich", "die", "Wol\u00b7ken\u00b7reihn", "dort", "o\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADV", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Roth hinein dr\u00e4ngt sich der Rauch der Schlote ...", "tokens": ["Roth", "hin\u00b7ein", "dr\u00e4ngt", "sich", "der", "Rauch", "der", "Schlo\u00b7te", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APZR", "VVFIN", "PRF", "ART", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Blut trieft von des Himmels harter W\u00f6lbung!", "tokens": ["Blut", "trieft", "von", "des", "Him\u00b7mels", "har\u00b7ter", "W\u00f6l\u00b7bung", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}