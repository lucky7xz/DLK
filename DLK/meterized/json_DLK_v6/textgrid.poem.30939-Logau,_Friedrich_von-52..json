{"textgrid.poem.30939": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "52.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Andre L\u00e4nder geben Steuer nach dem Kopff und nach Genie\u00df;", "tokens": ["And\u00b7re", "L\u00e4n\u00b7der", "ge\u00b7ben", "Steu\u00b7er", "nach", "dem", "Kopff", "und", "nach", "Ge\u00b7nie\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "NN", "APPR", "ART", "NN", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wir, nach dem sich unbesonnen weiland einer sch\u00e4tzen lie\u00df.", "tokens": ["Wir", ",", "nach", "dem", "sich", "un\u00b7be\u00b7son\u00b7nen", "wei\u00b7land", "ei\u00b7ner", "sch\u00e4t\u00b7zen", "lie\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "PRELS", "PRF", "ADJD", "VVFIN", "PIS", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wer das Leben kaum noch hat, wer sonst alles hat versetzet,", "tokens": ["Wer", "das", "Le\u00b7ben", "kaum", "noch", "hat", ",", "wer", "sonst", "al\u00b7les", "hat", "ver\u00b7set\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADV", "ADV", "VAFIN", "$,", "PWS", "ADV", "PIS", "VAFIN", "VVPP", "$,"], "meter": "--+--+---+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Mu\u00df noch dennoch tragen Last, darum da\u00df er ist gesch\u00e4tzet.", "tokens": ["Mu\u00df", "noch", "den\u00b7noch", "tra\u00b7gen", "Last", ",", "da\u00b7rum", "da\u00df", "er", "ist", "ge\u00b7sch\u00e4t\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "ADJA", "NN", "$,", "PAV", "KOUS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+--+-+----+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ists dann billich, ists dann christlich? O, es sey gleich, wie es wil,", "tokens": ["Ists", "dann", "bil\u00b7lich", ",", "ists", "dann", "christ\u00b7lich", "?", "O", ",", "es", "sey", "gleich", ",", "wie", "es", "wil", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$,", "VAFIN", "ADV", "ADJD", "$.", "NE", "$,", "PPER", "VAFIN", "ADV", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Fromts nur einem und dem andren, hats zu deuten sonst nicht viel!", "tokens": ["Fromts", "nur", "ei\u00b7nem", "und", "dem", "an\u00b7dren", ",", "hats", "zu", "deu\u00b7ten", "sonst", "nicht", "viel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "KON", "ART", "ADJA", "$,", "VAFIN", "PTKZU", "VVINF", "ADV", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}