{"textgrid.poem.32417": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "95. An die Fuska", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei nicht mit deinem roten Haar", "tokens": ["Sei", "nicht", "mit", "dei\u00b7nem", "ro\u00b7ten", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So \u00e4u\u00dferst, Fuska, unzufrieden!", "tokens": ["So", "\u00e4u\u00b7\u00dferst", ",", "Fus\u00b7ka", ",", "un\u00b7zu\u00b7frie\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "$,", "NE", "$,", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ward dir nicht sch\u00f6nes braunes Haar,", "tokens": ["Ward", "dir", "nicht", "sch\u00f6\u00b7nes", "brau\u00b7nes", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So ward dir braune Haut beschieden.", "tokens": ["So", "ward", "dir", "brau\u00b7ne", "Haut", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}