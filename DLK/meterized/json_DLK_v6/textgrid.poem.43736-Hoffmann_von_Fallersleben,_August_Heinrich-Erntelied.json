{"textgrid.poem.43736": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Erntelied", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Sommer bleibt nicht lange mehr,", "tokens": ["Der", "Som\u00b7mer", "bleibt", "nicht", "lan\u00b7ge", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Tag wird k\u00fcrzer, die Nacht wird l\u00e4nger,", "tokens": ["Der", "Tag", "wird", "k\u00fcr\u00b7zer", ",", "die", "Nacht", "wird", "l\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das Korn ist gem\u00e4ht, das Feld wird leer,", "tokens": ["Das", "Korn", "ist", "ge\u00b7m\u00e4ht", ",", "das", "Feld", "wird", "leer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Es schweigen schon des Waldes S\u00e4nger.", "tokens": ["Es", "schwei\u00b7gen", "schon", "des", "Wal\u00b7des", "S\u00e4n\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch eh' uns der Sommer ganz verl\u00e4\u00dft,", "tokens": ["Doch", "eh'", "uns", "der", "Som\u00b7mer", "ganz", "ver\u00b7l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "So giebt er uns noch ein fr\u00f6hlich Fest.", "tokens": ["So", "giebt", "er", "uns", "noch", "ein", "fr\u00f6h\u00b7lich", "Fest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Seht da! sie bringen den Erntekranz", "tokens": ["Seht", "da", "!", "sie", "brin\u00b7gen", "den", "Ern\u00b7te\u00b7kranz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit bunten B\u00e4ndern und Flittern,", "tokens": ["Mit", "bun\u00b7ten", "B\u00e4n\u00b7dern", "und", "Flit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie eilen Alle zum fr\u00f6hlichen Tanz,", "tokens": ["Sie", "ei\u00b7len", "Al\u00b7le", "zum", "fr\u00f6h\u00b7li\u00b7chen", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die M\u00e4dchen mit den Schnittern.", "tokens": ["Die", "M\u00e4d\u00b7chen", "mit", "den", "Schnit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und Alles tanzt und springt,", "tokens": ["Und", "Al\u00b7les", "tanzt", "und", "springt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und Alles jubelt und singt:", "tokens": ["Und", "Al\u00b7les", "ju\u00b7belt", "und", "singt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Juchhei\u00dfa juchhei!", "tokens": ["Juch\u00b7hei\u00b7\u00dfa", "juch\u00b7hei", "!"], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Die Ernt' ist vorbei.", "tokens": ["Die", "Ernt'", "ist", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}