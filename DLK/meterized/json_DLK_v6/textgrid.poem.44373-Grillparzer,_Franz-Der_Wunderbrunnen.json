{"textgrid.poem.44373": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Der Wunderbrunnen", "genre": "verse", "period": "N.A.", "pub_year": 1820, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seit ich von dir gekostet,", "tokens": ["Seit", "ich", "von", "dir", "ge\u00b7kos\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du labend heller Born!", "tokens": ["Du", "la\u00b7bend", "hel\u00b7ler", "Born", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "D\u00fcnkt jedes Na\u00df mir tr\u00fcbe", "tokens": ["D\u00fcnkt", "je\u00b7des", "Na\u00df", "mir", "tr\u00fc\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "PPER", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und leer der Freude Horn.", "tokens": ["Und", "leer", "der", "Freu\u00b7de", "Horn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zu dir geht meine Liebe,", "tokens": ["Zu", "dir", "geht", "mei\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von dir aus all mein Zorn;", "tokens": ["Von", "dir", "aus", "all", "mein", "Zorn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "O, da\u00df du immer fl\u00f6ssest,", "tokens": ["O", ",", "da\u00df", "du", "im\u00b7mer", "fl\u00f6s\u00b7sest", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Du leichtversiegter Born!", "tokens": ["Du", "leicht\u00b7ver\u00b7sieg\u00b7ter", "Born", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}