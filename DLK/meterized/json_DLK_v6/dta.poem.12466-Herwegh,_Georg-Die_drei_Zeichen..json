{"dta.poem.12466": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die drei Zeichen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Drei Zeichen hat uns Gott bestellt,", "tokens": ["Drei", "Zei\u00b7chen", "hat", "uns", "Gott", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df wir die Herren dieser Welt:", "tokens": ["Da\u00df", "wir", "die", "Her\u00b7ren", "die\u00b7ser", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das ist der goldne Wein,", "tokens": ["Das", "ist", "der", "gold\u00b7ne", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das ist durch's Land der gr\u00fcne Strom,", "tokens": ["Das", "ist", "durch's", "Land", "der", "gr\u00fc\u00b7ne", "Strom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das ist der hohe heil'ge Dom,", "tokens": ["Das", "ist", "der", "ho\u00b7he", "heil'\u00b7ge", "Dom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Dom zu K\u00f6ln am Rhein.", "tokens": ["Der", "Dom", "zu", "K\u00f6ln", "am", "Rhein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "APPRART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O Traubenblut, o adlig Blut!", "tokens": ["O", "Trau\u00b7ben\u00b7blut", ",", "o", "ad\u00b7lig", "Blut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer schafft wie du so k\u00fchnen Muth,", "tokens": ["Wer", "schafft", "wie", "du", "so", "k\u00fch\u00b7nen", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KOKOM", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So frisch und froh Gedeihn?", "tokens": ["So", "frisch", "und", "froh", "Ge\u00b7deihn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Meister, der den Plan gemacht,", "tokens": ["Der", "Meis\u00b7ter", ",", "der", "den", "Plan", "ge\u00b7macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hat sicher ihn beim Wein erdacht,", "tokens": ["Hat", "si\u00b7cher", "ihn", "beim", "Wein", "er\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Dom zu K\u00f6ln am Rhein.", "tokens": ["Den", "Dom", "zu", "K\u00f6ln", "am", "Rhein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "APPRART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dir, deutscher Strom, den zweiten Gru\u00df!", "tokens": ["Dir", ",", "deut\u00b7scher", "Strom", ",", "den", "zwei\u00b7ten", "Gru\u00df", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von freien Alpen kommt der Flu\u00df", "tokens": ["Von", "frei\u00b7en", "Al\u00b7pen", "kommt", "der", "Flu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um deutsches Land zu ", "tokens": ["Um", "deut\u00b7sches", "Land", "zu"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ADJA", "NN", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Kann ich sein Rauschen recht verstehn,", "tokens": ["Kann", "ich", "sein", "Rau\u00b7schen", "recht", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So hei\u00dft's: Ich will ihn fertig sehn,", "tokens": ["So", "hei\u00dft's", ":", "Ich", "will", "ihn", "fer\u00b7tig", "sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$.", "PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Dom zu K\u00f6ln am Rhein.", "tokens": ["Den", "Dom", "zu", "K\u00f6ln", "am", "Rhein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "APPRART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ja, wie der Meister dich erschaut,", "tokens": ["Ja", ",", "wie", "der", "Meis\u00b7ter", "dich", "er\u00b7schaut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bis zu den Sternen auferbaut", "tokens": ["Bis", "zu", "den", "Ster\u00b7nen", "au\u00b7fer\u00b7baut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sollst du, o Tempel, sein!", "tokens": ["Sollst", "du", ",", "o", "Tem\u00b7pel", ",", "sein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "FM", "NN", "$,", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Damit man einst am j\u00fcngsten Tag", "tokens": ["Da\u00b7mit", "man", "einst", "am", "j\u00fcng\u00b7sten", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Noch singen und noch sagen mag", "tokens": ["Noch", "sin\u00b7gen", "und", "noch", "sa\u00b7gen", "mag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "ADV", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vom Dom zu K\u00f6ln am Rhein.", "tokens": ["Vom", "Dom", "zu", "K\u00f6ln", "am", "Rhein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "APPRART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was will des Teufels Witz und Spott?", "tokens": ["Was", "will", "des", "Teu\u00b7fels", "Witz", "und", "Spott", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es kehret schon der rechte Gott", "tokens": ["Es", "keh\u00b7ret", "schon", "der", "rech\u00b7te", "Gott"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch bei den Deutschen ein;", "tokens": ["Auch", "bei", "den", "Deut\u00b7schen", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nur frisch, Gesellen, frisch zur Hand!", "tokens": ["Nur", "frisch", ",", "Ge\u00b7sel\u00b7len", ",", "frisch", "zur", "Hand", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "NN", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Macht Platz f\u00fcr's ", "tokens": ["Macht", "Platz", "f\u00fcr's"], "token_info": ["word", "word", "word"], "pos": ["NN", "NN", "NE"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Im Dom zu K\u00f6ln am Rhein.", "tokens": ["Im", "Dom", "zu", "K\u00f6ln", "am", "Rhein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "APPRART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}