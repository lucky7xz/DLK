{"dta.poem.8343": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Lehre.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Wespen sind ein Bild, von\nden unn\u00fczen Leuten,", "tokens": ["Die", "Wes\u00b7pen", "sind", "ein", "Bild", ",", "von", "den", "un\u00b7n\u00fc\u00b7zen", "Leu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die mit der L\u00e4sterzung, als", "tokens": ["Die", "mit", "der", "L\u00e4s\u00b7ter\u00b7zung", ",", "als"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "APPR", "ART", "NN", "$,", "KOUS"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Die Bienen aber sind, den Sittenlehrern", "tokens": ["Die", "Bie\u00b7nen", "a\u00b7ber", "sind", ",", "den", "Sit\u00b7ten\u00b7leh\u00b7rern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "VAFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Die zwar empfindlich sind, doch auch am", "tokens": ["Die", "zwar", "emp\u00b7find\u00b7lich", "sind", ",", "doch", "auch", "am"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "VAFIN", "$,", "ADV", "ADV", "APPRART"], "meter": "-+-+-+++-", "measure": "unknown.measure.penta"}, "line.5": {"text": "Die L\u00e4strer stechen nur, das andern mu\u00df ver-", "tokens": ["Die", "L\u00e4st\u00b7rer", "ste\u00b7chen", "nur", ",", "das", "an\u00b7dern", "mu\u00df", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PRELS", "PIS", "VMFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein Sittenlehrer weis die Stiche zu vers\u00fcssen.", "tokens": ["Ein", "Sit\u00b7ten\u00b7leh\u00b7rer", "weis", "die", "Sti\u00b7che", "zu", "ver\u00b7s\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}