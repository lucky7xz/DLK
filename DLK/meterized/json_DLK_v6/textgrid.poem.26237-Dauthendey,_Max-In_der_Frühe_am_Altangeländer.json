{"textgrid.poem.26237": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "In der Fr\u00fche am Altangel\u00e4nder", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In der Fr\u00fche am Altangel\u00e4nder,", "tokens": ["In", "der", "Fr\u00fc\u00b7he", "am", "Al\u00b7tan\u00b7ge\u00b7l\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ehe die Sonne noch aufgegangen", "tokens": ["E\u00b7he", "die", "Son\u00b7ne", "noch", "auf\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "VVPP"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Und die gelbglitzernden Wolkenr\u00e4nder", "tokens": ["Und", "die", "gelb\u00b7glit\u00b7zern\u00b7den", "Wol\u00b7ken\u00b7r\u00e4n\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "An den rauchenden Bergketten hangen,", "tokens": ["An", "den", "rau\u00b7chen\u00b7den", "Berg\u00b7ket\u00b7ten", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Frage ich stumm: Wann kommt das Wort \u00bbFriede\u00ab,", "tokens": ["Fra\u00b7ge", "ich", "stumm", ":", "Wann", "kommt", "das", "Wort", "\u00bb", "Frie\u00b7de", "\u00ab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "PPER", "ADJD", "$.", "PWAV", "VVFIN", "ART", "NN", "$(", "NN", "$(", "$,"], "meter": "+--+-+-++-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wie dort der Strahl aus dem Morgengrauen,", "tokens": ["Wie", "dort", "der", "Strahl", "aus", "dem", "Mor\u00b7gen\u00b7grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dem Aug' zur Freude, dem Ohr zum Liede,", "tokens": ["Dem", "Aug'", "zur", "Freu\u00b7de", ",", "dem", "Ohr", "zum", "Lie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und dem Blut zu neuem Vertrauen?", "tokens": ["Und", "dem", "Blut", "zu", "neu\u00b7em", "Ver\u00b7trau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Frage: Wann lernt der Geist wieder fliegen", "tokens": ["Fra\u00b7ge", ":", "Wann", "lernt", "der", "Geist", "wie\u00b7der", "flie\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PWAV", "VVFIN", "ART", "NN", "ADV", "VVINF"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Leicht in Gedanken, sorglos im Hoffen,", "tokens": ["Leicht", "in", "Ge\u00b7dan\u00b7ken", ",", "sorg\u00b7los", "im", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Wie sich V\u00f6gel im G\u00f6tterbaum wiegen,", "tokens": ["Wie", "sich", "V\u00f6\u00b7gel", "im", "G\u00f6t\u00b7ter\u00b7baum", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Wie der Garten der Fr\u00fchsonne offen?", "tokens": ["Wie", "der", "Gar\u00b7ten", "der", "Fr\u00fch\u00b7son\u00b7ne", "of\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "--+--++-+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Steine klappern mit lebhaftem Schalle,", "tokens": ["Stei\u00b7ne", "klap\u00b7pern", "mit", "leb\u00b7haf\u00b7tem", "Schal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Munter springt dort der Rappe zum Grasen,", "tokens": ["Mun\u00b7ter", "springt", "dort", "der", "Rap\u00b7pe", "zum", "Gra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Kollernd flattern Truthennen vom Stalle,", "tokens": ["Kol\u00b7lernd", "flat\u00b7tern", "Tru\u00b7then\u00b7nen", "vom", "Stal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Freigelassen zum tauigen Rasen.", "tokens": ["Frei\u00b7ge\u00b7las\u00b7sen", "zum", "tau\u00b7i\u00b7gen", "Ra\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Dr\u00fcben beim Nachbarn lernt laut ein Knabe", "tokens": ["Dr\u00fc\u00b7ben", "beim", "Nach\u00b7barn", "lernt", "laut", "ein", "Kna\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Aus dem Koran die tausendste Sure;", "tokens": ["Aus", "dem", "Ko\u00b7ran", "die", "tau\u00b7sends\u00b7te", "Su\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "In den Palmen jagt kr\u00e4chzend ein Rabe", "tokens": ["In", "den", "Pal\u00b7men", "jagt", "kr\u00e4ch\u00b7zend", "ein", "Ra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADJD", "ART", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Und \u00fcberschreit der Tauben Gegurre.", "tokens": ["Und", "\u00fc\u00b7bersc\u00b7hreit", "der", "Tau\u00b7ben", "Ge\u00b7gur\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Frisch in das Weltall klingt lautes Leben,", "tokens": ["Frisch", "in", "das", "Wel\u00b7tall", "klingt", "lau\u00b7tes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Harmlos wachsend zur H\u00f6he der Stunden.", "tokens": ["Harm\u00b7los", "wach\u00b7send", "zur", "H\u00f6\u00b7he", "der", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ich nur stehe beklommen daneben,", "tokens": ["Ich", "nur", "ste\u00b7he", "be\u00b7klom\u00b7men", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "PAV", "$,"], "meter": "--+--+-+--", "measure": "anapaest.di.plus"}, "line.4": {"text": "An die Frage: Wer siegt? stumm gebunden.", "tokens": ["An", "die", "Fra\u00b7ge", ":", "Wer", "siegt", "?", "stumm", "ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "PWS", "VVFIN", "$.", "ADJD", "VVPP", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}}}}