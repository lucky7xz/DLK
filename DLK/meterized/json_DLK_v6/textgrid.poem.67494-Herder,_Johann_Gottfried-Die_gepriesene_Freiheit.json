{"textgrid.poem.67494": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die gepriesene Freiheit", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rt, Ihr M\u00e4chtigen, h\u00f6rt! Der Feder gr\u00f6\u00dfeste Freiheit", "tokens": ["H\u00f6rt", ",", "Ihr", "M\u00e4ch\u00b7ti\u00b7gen", ",", "h\u00f6rt", "!", "Der", "Fe\u00b7der", "gr\u00f6\u00b7\u00dfes\u00b7te", "Frei\u00b7heit"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$.", "ART", "NN", "ADJA", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Herrschet anjetzt; es schreibt jede, was jeder gef\u00e4llt.", "tokens": ["Herr\u00b7schet", "an\u00b7jetzt", ";", "es", "schreibt", "je\u00b7de", ",", "was", "je\u00b7der", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "PPER", "VVFIN", "PIAT", "$,", "PRELS", "PIS", "VVPP", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Loben und tadeln d\u00fcrfen wir laut ohn' alle Besorgni\u00df;", "tokens": ["Lo\u00b7ben", "und", "ta\u00b7deln", "d\u00fcr\u00b7fen", "wir", "laut", "ohn'", "al\u00b7le", "Be\u00b7sorg\u00b7ni\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "VMFIN", "PPER", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Was Pasquino gedenkt, spricht er und findet Geh\u00f6r.", "tokens": ["Was", "Pas\u00b7qui\u00b7no", "ge\u00b7denkt", ",", "spricht", "er", "und", "fin\u00b7det", "Ge\u00b7h\u00f6r", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "VVFIN", "$,", "VVFIN", "PPER", "KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.5": {"text": "Eins nur wagen wir nicht: reinaus zu sagen ", "tokens": ["Eins", "nur", "wa\u00b7gen", "wir", "nicht", ":", "rein\u00b7aus", "zu", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "PPER", "PTKNEG", "$.", "ADV", "PTKZU", "VVINF"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Weihrauch liebet man wol, aber kein w\u00fcrziges Salz.", "tokens": ["Weih\u00b7rauch", "lie\u00b7bet", "man", "wol", ",", "a\u00b7ber", "kein", "w\u00fcr\u00b7zi\u00b7ges", "Salz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "ADV", "$,", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "+-+--+--++--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "H\u00f6rt, Ihr M\u00e4chtigen, h\u00f6rt! Die hochgepriesene Freiheit", "tokens": ["H\u00f6rt", ",", "Ihr", "M\u00e4ch\u00b7ti\u00b7gen", ",", "h\u00f6rt", "!", "Die", "hoch\u00b7ge\u00b7prie\u00b7se\u00b7ne", "Frei\u00b7heit"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Unsrer Feder, sie ist knechtischer, schmeichelnder Dienst.", "tokens": ["Uns\u00b7rer", "Fe\u00b7der", ",", "sie", "ist", "knech\u00b7ti\u00b7scher", ",", "schmei\u00b7cheln\u00b7der", "Dienst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}