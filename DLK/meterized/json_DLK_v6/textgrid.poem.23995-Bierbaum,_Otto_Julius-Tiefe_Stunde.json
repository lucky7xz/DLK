{"textgrid.poem.23995": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Tiefe Stunde", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne ist gegangen.", "tokens": ["Die", "Son\u00b7ne", "ist", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein letzter roter Schein", "tokens": ["Ein", "letz\u00b7ter", "ro\u00b7ter", "Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Liegt auf den h\u00f6chsten Gipfeln,", "tokens": ["Liegt", "auf", "den", "h\u00f6chs\u00b7ten", "Gip\u00b7feln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die gl\u00fchen wie von Wein.", "tokens": ["Die", "gl\u00fc\u00b7hen", "wie", "von", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KOKOM", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Luft ist voller Bangen.", "tokens": ["Die", "Luft", "ist", "vol\u00b7ler", "Ban\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auf leicht bewegten Wipfeln", "tokens": ["Auf", "leicht", "be\u00b7weg\u00b7ten", "Wip\u00b7feln"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Schlafen die V\u00f6gel ein,", "tokens": ["Schla\u00b7fen", "die", "V\u00f6\u00b7gel", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Die eben noch aus voller Kehle sangen.", "tokens": ["Die", "e\u00b7ben", "noch", "aus", "vol\u00b7ler", "Keh\u00b7le", "san\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie tief ist diese Stunde!", "tokens": ["Wie", "tief", "ist", "die\u00b7se", "Stun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus unsichtbarem Munde", "tokens": ["Aus", "un\u00b7sicht\u00b7ba\u00b7rem", "Mun\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Trifft mich ein seltsam Wort:", "tokens": ["Trifft", "mich", "ein", "selt\u00b7sam", "Wort", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Gegeben und genommen,", "tokens": ["Ge\u00b7ge\u00b7ben", "und", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gegangen und gekommen,", "tokens": ["Ge\u00b7gan\u00b7gen", "und", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo ist dein Hier, dein Dort?", "tokens": ["Wo", "ist", "dein", "Hier", ",", "dein", "Dort", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Ein Schweben in der Runde \u2013", "tokens": ["Ein", "Schwe\u00b7ben", "in", "der", "Run\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dein Leben geht zu Grunde", "tokens": ["Dein", "Le\u00b7ben", "geht", "zu", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und lebt doch fort und fort.", "tokens": ["Und", "lebt", "doch", "fort", "und", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nun in den Wipfeln \u2013 Ruhe,", "tokens": ["Nun", "in", "den", "Wip\u00b7feln", "\u2013", "Ru\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$(", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf allen Gipfeln \u2013 Dunkelheit.", "tokens": ["Auf", "al\u00b7len", "Gip\u00b7feln", "\u2013", "Dun\u00b7kel\u00b7heit", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$(", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf thut sich schwarz und weit", "tokens": ["Auf", "thut", "sich", "schwarz", "und", "weit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "PRF", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die ungeheure Truhe:", "tokens": ["Die", "un\u00b7ge\u00b7heu\u00b7re", "Tru\u00b7he", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nacht und Vergessenheit.", "tokens": ["Nacht", "und", "Ver\u00b7ges\u00b7sen\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}