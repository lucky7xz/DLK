{"dta.poem.18836": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sah, mit Staunen und Vergn\u00fcgen,", "tokens": ["Ich", "sah", ",", "mit", "Stau\u00b7nen", "und", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Eine Pfauenfeder im Coran liegen,", "tokens": ["Ei\u00b7ne", "Pfau\u00b7en\u00b7fe\u00b7der", "im", "Co\u00b7ran", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Willkommen an dem heilgen Platz!", "tokens": ["Will\u00b7kom\u00b7men", "an", "dem", "heil\u00b7gen", "Platz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Erdgebilde h\u00f6chster Schatz.", "tokens": ["Der", "Erd\u00b7ge\u00b7bil\u00b7de", "h\u00f6chs\u00b7ter", "Schatz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "An dir wie an des Himmels Sternen", "tokens": ["An", "dir", "wie", "an", "des", "Him\u00b7mels", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "KOKOM", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist Gottes Gr\u00f6sse im Kleinen zu lernen.", "tokens": ["Ist", "Got\u00b7tes", "Gr\u00f6s\u00b7se", "im", "Klei\u00b7nen", "zu", "ler\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Dass er, der Welten \u00fcberblickt,", "tokens": ["Dass", "er", ",", "der", "Wel\u00b7ten", "\u00fc\u00b7berb\u00b7lickt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Sein Auge hier hat aufgedr\u00fcckt,", "tokens": ["Sein", "Au\u00b7ge", "hier", "hat", "auf\u00b7ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und so den leichten Flaum geschm\u00fcckt", "tokens": ["Und", "so", "den", "leich\u00b7ten", "Flaum", "ge\u00b7schm\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Dass K\u00f6nige kaum unternahmen", "tokens": ["Dass", "K\u00f6\u00b7ni\u00b7ge", "kaum", "un\u00b7ter\u00b7nah\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die Pracht des Vogels nachzuahmen.", "tokens": ["Die", "Pracht", "des", "Vo\u00b7gels", "nach\u00b7zu\u00b7ah\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Bescheiden freue dich des Ruhms,", "tokens": ["Be\u00b7schei\u00b7den", "freu\u00b7e", "dich", "des", "Ruhms", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "So bist du werth des Heiligthums.", "tokens": ["So", "bist", "du", "werth", "des", "Hei\u00b7lig\u00b7thums", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}