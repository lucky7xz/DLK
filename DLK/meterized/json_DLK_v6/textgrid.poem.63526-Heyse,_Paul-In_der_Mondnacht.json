{"textgrid.poem.63526": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "In der Mondnacht", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In der Mondnacht, in der Fr\u00fchlingsmondnacht", "tokens": ["In", "der", "Mond\u00b7nacht", ",", "in", "der", "Fr\u00fch\u00b7lings\u00b7mond\u00b7nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Gehen Engel um auf leisen Sohlen;", "tokens": ["Ge\u00b7hen", "En\u00b7gel", "um", "auf", "lei\u00b7sen", "Soh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Blonde Engel, innig und verstohlen", "tokens": ["Blon\u00b7de", "En\u00b7gel", ",", "in\u00b7nig", "und", "ver\u00b7stoh\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJD", "KON", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "K\u00fcssen sie die sch\u00f6nsten Menschenblumen.", "tokens": ["K\u00fcs\u00b7sen", "sie", "die", "sch\u00f6ns\u00b7ten", "Men\u00b7schen\u00b7blu\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Tausendsch\u00f6nchen, allerliebste Blume,", "tokens": ["Tau\u00b7send\u00b7sch\u00f6n\u00b7chen", ",", "al\u00b7ler\u00b7liebs\u00b7te", "Blu\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wei\u00df es wohl, woher der Schimmer stammet,", "tokens": ["Wei\u00df", "es", "wohl", ",", "wo\u00b7her", "der", "Schim\u00b7mer", "stam\u00b7met", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Der dir heut das Antlitz \u00fcberflammet:", "tokens": ["Der", "dir", "heut", "das", "Ant\u00b7litz", "\u00fc\u00b7berf\u00b7lam\u00b7met", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Bist noch in den Traum der Nacht verloren.", "tokens": ["Bist", "noch", "in", "den", "Traum", "der", "Nacht", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Denkst der Engel, die durchs kleine Fenster", "tokens": ["Denkst", "der", "En\u00b7gel", ",", "die", "durchs", "klei\u00b7ne", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sich auf Mondesstrahlen zu dir schwangen,", "tokens": ["Sich", "auf", "Mon\u00b7des\u00b7strah\u00b7len", "zu", "dir", "schwan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Leise dir zu k\u00fcssen Mund und Wangen", "tokens": ["Lei\u00b7se", "dir", "zu", "k\u00fcs\u00b7sen", "Mund", "und", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "In der Mondnacht, in der Fr\u00fchlingsmondnacht.", "tokens": ["In", "der", "Mond\u00b7nacht", ",", "in", "der", "Fr\u00fch\u00b7lings\u00b7mond\u00b7nacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-++", "measure": "unknown.measure.hexa"}}}}}