"}, "line.40": {"text": "Doch kan sie dadurch nicht der Rache widerstre-", "tokens": ["Doch", "kan", "sie", "da\u00b7durch", "nicht", "der", "Ra\u00b7che", "wi\u00b7der\u00b7stre"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PAV", "PTKNEG", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Die auf das B\u00f6se z\u00fcrnt; bei ", "tokens": ["Die", "auf", "das", "B\u00f6\u00b7se", "z\u00fcrnt", ";", "bei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$.", "APPR"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.42": {"text": "Steht die Gerechtigkeit und Lieb im Gleichgewicht.", "tokens": ["Steht", "die", "Ge\u00b7rech\u00b7tig\u00b7keit", "und", "Lieb", "im", "Gleich\u00b7ge\u00b7wicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Der ewge Gnadenbund kan uns dies kl\u00e4rlich wei-", "tokens": ["Der", "ew\u00b7ge", "Gna\u00b7den\u00b7bund", "kan", "uns", "dies", "kl\u00e4r\u00b7lich", "wei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "PDS", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Darin wir seine G\u00fct, die \u00fcberschwenglich preisen:", "tokens": ["Da\u00b7rin", "wir", "sei\u00b7ne", "G\u00fct", ",", "die", "\u00fc\u00b7bersc\u00b7hweng\u00b7lich", "prei\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text