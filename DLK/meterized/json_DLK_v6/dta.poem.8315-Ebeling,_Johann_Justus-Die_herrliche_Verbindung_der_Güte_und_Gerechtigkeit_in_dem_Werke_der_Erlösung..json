{"dta.poem.8315": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die herrliche Verbindung der  \n G\u00fcte und Gerechtigkeit in dem  \n Werke der Erl\u00f6sung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott ist das h\u00f6chste Gut, das ist gewis-\nlich wahr,", "tokens": ["Gott", "ist", "das", "h\u00f6chs\u00b7te", "Gut", ",", "das", "ist", "ge\u00b7wis", "lich", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,", "PDS", "VAFIN", "TRUNC", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jm Buche der Natur und Offenbah-", "tokens": ["Jm", "Bu\u00b7che", "der", "Na\u00b7tur", "und", "Of\u00b7fen\u00b7bah"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir sehen, was wir sehn, es dr\u00fckt", "tokens": ["Wir", "se\u00b7hen", ",", "was", "wir", "sehn", ",", "es", "dr\u00fckt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PPER", "VVINF", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ja allenthalben ab, in ein ger\u00fchrt Gem\u00fcte.", "tokens": ["Ja", "al\u00b7len\u00b7thal\u00b7ben", "ab", ",", "in", "ein", "ge\u00b7r\u00fchrt", "Ge\u00b7m\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "PTKVZ", "$,", "APPR", "ART", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er hat ja alles woll, zu unsern Gl\u00fck bedacht,", "tokens": ["Er", "hat", "ja", "al\u00b7les", "woll", ",", "zu", "un\u00b7sern", "Gl\u00fck", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "ADV", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und herrlich uns zur Lust, in dieser Welt gemacht,", "tokens": ["Und", "herr\u00b7lich", "uns", "zur", "Lust", ",", "in", "die\u00b7ser", "Welt", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "APPRART", "NN", "$,", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und in dem Gnadenreich bemerken wir die Triebe,", "tokens": ["Und", "in", "dem", "Gna\u00b7den\u00b7reich", "be\u00b7mer\u00b7ken", "wir", "die", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df unser Sch\u00f6pfer sey die wesentliche Liebe.", "tokens": ["Da\u00df", "un\u00b7ser", "Sch\u00f6p\u00b7fer", "sey", "die", "we\u00b7sent\u00b7li\u00b7che", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.9": {"text": "Die Menschen sehen dies, wie die Barmherzigkeit,", "tokens": ["Die", "Men\u00b7schen", "se\u00b7hen", "dies", ",", "wie", "die", "Barm\u00b7her\u00b7zig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PDS", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Aus reiner Lieb ger\u00fchrt, uns ihre Huld anbeut,", "tokens": ["Aus", "rei\u00b7ner", "Lieb", "ge\u00b7r\u00fchrt", ",", "uns", "ih\u00b7re", "Huld", "an\u00b7beut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sich nur darum bem\u00fcht uns aus den Elends-", "tokens": ["Sich", "nur", "da\u00b7rum", "be\u00b7m\u00fcht", "uns", "aus", "den", "E\u00b7lends"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "PAV", "VVFIN", "PPER", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Worin die S\u00fcnd uns schliest, uns wieder zu er-", "tokens": ["Wo\u00b7rin", "die", "S\u00fcnd", "uns", "schliest", ",", "uns", "wie\u00b7der", "zu", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PPER", "VVFIN", "$,", "PPER", "ADV", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Der S\u00fcnder denkt daran, und macht daher den", "tokens": ["Der", "S\u00fcn\u00b7der", "denkt", "da\u00b7ran", ",", "und", "macht", "da\u00b7her", "den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PAV", "$,", "KON", "VVFIN", "PAV", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Weil ", "tokens": ["Weil"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Misbraucht die G\u00fctigkeit, die ihn der Sch\u00f6pfer", "tokens": ["Mis\u00b7braucht", "die", "G\u00fc\u00b7tig\u00b7keit", ",", "die", "ihn", "der", "Sch\u00f6p\u00b7fer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und meint die Liebe sey, auch dem was B\u00f6\u00df ge-", "tokens": ["Und", "meint", "die", "Lie\u00b7be", "sey", ",", "auch", "dem", "was", "B\u00f6\u00df", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "VAFIN", "$,", "ADV", "ART", "PRELS", "NE", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Das ist ein falscher Schlus, der seiner Sicherheit,", "tokens": ["Das", "ist", "ein", "fal\u00b7scher", "Schlus", ",", "der", "sei\u00b7ner", "Si\u00b7cher\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Nur falsche Hofnung giebt, und sanfte Polster", "tokens": ["Nur", "fal\u00b7sche", "Hof\u00b7nung", "giebt", ",", "und", "sanf\u00b7te", "Pols\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Worauf er ganz getrost, ohn sein Bem\u00fchn und", "tokens": ["Wo\u00b7rauf", "er", "ganz", "ge\u00b7trost", ",", "ohn", "sein", "Be\u00b7m\u00fchn", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "ADV", "VVPP", "$,", "KOUI", "PPOSAT", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Wird blos durch eigne Schuld, die Seeligkeit ver-", "tokens": ["Wird", "blos", "durch", "eig\u00b7ne", "Schuld", ",", "die", "See\u00b7lig\u00b7keit", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "ADJA", "NN", "$,", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Der Jrthum fliest daher, dieweil er f\u00e4lschlich meint,", "tokens": ["Der", "Jr\u00b7thum", "fliest", "da\u00b7her", ",", "die\u00b7weil", "er", "f\u00e4lschlich", "meint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Da\u00df ", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.23": {"text": "Ohnm\u00f6glich ihn noch k\u00f6nn, zum ewgen Feuers-", "tokens": ["Ohn\u00b7m\u00f6g\u00b7lich", "ihn", "noch", "k\u00f6nn", ",", "zum", "ew\u00b7gen", "Feu\u00b7er\u00b7s"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "ADV", "VMFIN", "$,", "APPRART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da er die Liebe sey, ob er gleich B\u00f6\u00df, verdammen.", "tokens": ["Da", "er", "die", "Lie\u00b7be", "sey", ",", "ob", "er", "gleich", "B\u00f6\u00df", ",", "ver\u00b7dam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,", "KOUS", "PPER", "ADV", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Er denkt der Heiland sei, ja f\u00fcr die ganze Welt,", "tokens": ["Er", "denkt", "der", "Hei\u00b7land", "sei", ",", "ja", "f\u00fcr", "die", "gan\u00b7ze", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VAFIN", "$,", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Als ein Erl\u00f6ser auch f\u00fcr ihm mit dargestellt;", "tokens": ["Als", "ein", "Er\u00b7l\u00f6\u00b7ser", "auch", "f\u00fcr", "ihm", "mit", "dar\u00b7ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "APPR", "PPER", "APPR", "VVPP", "$."], "meter": "-+-+-++-+--+", "measure": "iambic.hexa.chol"}, "line.27": {"text": "Und darum sey er frei von seiner Schuld der S\u00fcn-", "tokens": ["Und", "da\u00b7rum", "sey", "er", "frei", "von", "sei\u00b7ner", "Schuld", "der", "S\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VAFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Weil in desselben Blut Bezahlung gnug zu finden.", "tokens": ["Weil", "in", "des\u00b7sel\u00b7ben", "Blut", "Be\u00b7zah\u00b7lung", "gnug", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PDAT", "NN", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Doch diese Meinung f\u00e4llt, so bald man nur erwegt,", "tokens": ["Doch", "die\u00b7se", "Mei\u00b7nung", "f\u00e4llt", ",", "so", "bald", "man", "nur", "er\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "$,", "ADV", "ADV", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Was ", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.31": {"text": "So bald man nur bedenkt, da\u00df die Vollkommen-", "tokens": ["So", "bald", "man", "nur", "be\u00b7denkt", ",", "da\u00df", "die", "Voll\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "PIS", "ADV", "VVFIN", "$,", "KOUS", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Gleich herrlich und also nicht mit einander streiten.", "tokens": ["Gleich", "herr\u00b7lich", "und", "al\u00b7so", "nicht", "mit", "ein\u00b7an\u00b7der", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "PTKNEG", "APPR", "PRF", "VVFIN", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "Barmherzig ist er stets, doch auch dabei gerecht,", "tokens": ["Barm\u00b7her\u00b7zig", "ist", "er", "stets", ",", "doch", "auch", "da\u00b7bei", "ge\u00b7recht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "$,", "ADV", "ADV", "PAV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Er liebt den Menschen woll, doch keinen S\u00fcnden-", "tokens": ["Er", "liebt", "den", "Men\u00b7schen", "woll", ",", "doch", "kei\u00b7nen", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$,", "ADV", "PIAT", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Der seine G\u00fct misbraucht. Die Liebe ist bem\u00fchet,", "tokens": ["Der", "sei\u00b7ne", "G\u00fct", "mis\u00b7braucht", ".", "Die", "Lie\u00b7be", "ist", "be\u00b7m\u00