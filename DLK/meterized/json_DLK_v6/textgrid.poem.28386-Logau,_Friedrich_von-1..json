{"textgrid.poem.28386": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1647, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer einen Herren hat, darff keinen mehr begehren,", "tokens": ["Wer", "ei\u00b7nen", "Her\u00b7ren", "hat", ",", "darff", "kei\u00b7nen", "mehr", "be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "$,", "VMFIN", "PIAT", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sonst wird er Ehr und Leib mit Schmach und Pein beschweren.", "tokens": ["Sonst", "wird", "er", "Ehr", "und", "Leib", "mit", "Schmach", "und", "Pein", "be\u00b7schwe\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Welt h\u00e4lt mich in sich; doch ist nur Christus mein,", "tokens": ["Die", "Welt", "h\u00e4lt", "mich", "in", "sich", ";", "doch", "ist", "nur", "Chris\u00b7tus", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PRF", "$.", "ADV", "VAFIN", "ADV", "NE", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und solt ich tausendmal der Welt Rebelle seyn.", "tokens": ["Und", "solt", "ich", "tau\u00b7send\u00b7mal", "der", "Welt", "Re\u00b7bel\u00b7le", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}