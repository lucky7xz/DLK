{"textgrid.poem.60908": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Villa Ricciardi", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "R\u00f6tlich erbl\u00fcht Oleander in \u00fcppigen Hecken, es schlingt sich", "tokens": ["R\u00f6t\u00b7lich", "er\u00b7bl\u00fcht", "O\u00b7lean\u00b7der", "in", "\u00fcp\u00b7pi\u00b7gen", "He\u00b7cken", ",", "es", "schlingt", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "NN", "APPR", "ADJA", "NN", "$,", "PPER", "VVFIN", "PRF"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Duftiges Rosengeflecht hoch an die B\u00e4ume hinauf;", "tokens": ["Duf\u00b7ti\u00b7ges", "Ro\u00b7sen\u00b7ge\u00b7flecht", "hoch", "an", "die", "B\u00e4u\u00b7me", "hin\u00b7auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Pinie ragt auf wiesigem Grund, und es \u00f6ffnet das Tal sich", "tokens": ["Pi\u00b7nie", "ragt", "auf", "wie\u00b7si\u00b7gem", "Grund", ",", "und", "es", "\u00f6ff\u00b7net", "das", "Tal", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "$,", "KON", "PPER", "VVFIN", "ART", "NN", "PRF"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.4": {"text": "Lachend, in das du so k\u00fchn, hohes Camaldoli, schaust!", "tokens": ["La\u00b7chend", ",", "in", "das", "du", "so", "k\u00fchn", ",", "ho\u00b7hes", "Ca\u00b7mal\u00b7do\u00b7li", ",", "schaust", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "PRELS", "PPER", "ADV", "ADJD", "$,", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+---+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Doch von der Zinne des Hauses erblick ich das gro\u00dfe Neapel,", "tokens": ["Doch", "von", "der", "Zin\u00b7ne", "des", "Hau\u00b7ses", "er\u00b7blick", "ich", "das", "gro\u00b7\u00dfe", "Nea\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "---+--+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Oder des bajischen Golfs ewigen Lenz, und Misen.", "tokens": ["O\u00b7der", "des", "ba\u00b7jis\u00b7chen", "Golfs", "e\u00b7wi\u00b7gen", "Lenz", ",", "und", "Mi\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADJA", "NN", "$,", "KON", "NN", "$."], "meter": "+-+-+-++--+-+-", "measure": "trochaic.septa.relaxed"}}}}}