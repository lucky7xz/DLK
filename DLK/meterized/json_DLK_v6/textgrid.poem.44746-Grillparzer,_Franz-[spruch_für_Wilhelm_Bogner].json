{"textgrid.poem.44746": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[spruch f\u00fcr Wilhelm Bogner]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn heut ich warm dich liebe, und wie ichs f\u00fchle sag;", "tokens": ["Wenn", "heut", "ich", "warm", "dich", "lie\u00b7be", ",", "und", "wie", "ichs", "f\u00fch\u00b7le", "sag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ADJD", "PPER", "VVFIN", "$,", "KON", "PWAV", "PIS", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dann, Vater, ist ja t\u00e4glich, nicht heut dein Namenstag!", "tokens": ["Dann", ",", "Va\u00b7ter", ",", "ist", "ja", "t\u00e4g\u00b7lich", ",", "nicht", "heut", "dein", "Na\u00b7mens\u00b7tag", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VAFIN", "ADV", "ADJD", "$,", "PTKNEG", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}}}}}