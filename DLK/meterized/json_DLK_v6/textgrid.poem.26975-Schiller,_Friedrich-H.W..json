{"textgrid.poem.26975": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "H.W.", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n erhebt sich der Aglei und senkt das K\u00f6pfchen herunter.", "tokens": ["Sch\u00f6n", "er\u00b7hebt", "sich", "der", "Ag\u00b7lei", "und", "senkt", "das", "K\u00f6pf\u00b7chen", "her\u00b7un\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "ART", "NN", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+---+-+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ist es Gef\u00fchl? Oder ists Mutwill? Wir wissen es nicht.", "tokens": ["Ist", "es", "Ge\u00b7f\u00fchl", "?", "O\u00b7der", "ists", "Mut\u00b7will", "?", "Wir", "wis\u00b7sen", "es", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$.", "NE", "VAFIN", "NN", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+--++--+--+--+", "measure": "trochaic.hexa.relaxed"}}}}}