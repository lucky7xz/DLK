{"textgrid.poem.49353": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Errettung aus gro\u00dfer Not zur See", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df itz mit s\u00fc\u00dfen Weisen,", "tokens": ["La\u00df", "itz", "mit", "s\u00fc\u00b7\u00dfen", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Herr Gott, du starker Held,", "tokens": ["Herr", "Gott", ",", "du", "star\u00b7ker", "Held", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mich deine Wunder preisen", "tokens": ["Mich", "dei\u00b7ne", "Wun\u00b7der", "prei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr alles in der Welt;", "tokens": ["F\u00fcr", "al\u00b7les", "in", "der", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dein Lob sol immerdar", "tokens": ["Dein", "Lob", "sol", "im\u00b7mer\u00b7dar"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In meinem Mund' erklingen,", "tokens": ["In", "mei\u00b7nem", "Mund'", "er\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dir wil ich, Herr, lobsingen,", "tokens": ["Dir", "wil", "ich", ",", "Herr", ",", "lob\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der du hilfst aus Gefahr.", "tokens": ["Der", "du", "hilfst", "aus", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Wie sol ich dir vergelten,", "tokens": ["Wie", "sol", "ich", "dir", "ver\u00b7gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Herr, solche Wunderthat,", "tokens": ["Herr", ",", "sol\u00b7che", "Wun\u00b7der\u00b7that", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Die deine Hand nicht selten", "tokens": ["Die", "dei\u00b7ne", "Hand", "nicht", "sel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "PTKNEG", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Im Meer' erwiesen hat?", "tokens": ["Im", "Meer'", "er\u00b7wie\u00b7sen", "hat", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Wie sol ich deine Treu'", "tokens": ["Wie", "sol", "ich", "dei\u00b7ne", "Treu'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Dir dankbar gnug bezahlen,", "tokens": ["Dir", "dank\u00b7bar", "gnug", "be\u00b7zah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Der ich zu tausend malen", "tokens": ["Der", "ich", "zu", "tau\u00b7send", "ma\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PTKA", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Dein Schuldner werd' aufs Neu?", "tokens": ["Dein", "Schuld\u00b7ner", "werd'", "aufs", "Neu", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Viel Angst hab' ich erfahren", "tokens": ["Viel", "Angst", "hab'", "ich", "er\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Auf dem erz\u00f6rnten Meer,", "tokens": ["Auf", "dem", "er\u00b7z\u00f6rn\u00b7ten", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Das so viel stolze Baren", "tokens": ["Das", "so", "viel", "stol\u00b7ze", "Ba\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Warf grausamlich daher;", "tokens": ["Warf", "grau\u00b7sam\u00b7lich", "da\u00b7her", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Ach Gott, das Schifflein floh", "tokens": ["Ach", "Gott", ",", "das", "Schif\u00b7flein", "floh"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "Erschrecklich schnell gen Himmel,", "tokens": ["Er\u00b7schreck\u00b7lich", "schnell", "gen", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Drauf ward ein gro\u00df Get\u00fcmmel,", "tokens": ["Drauf", "ward", "ein", "gro\u00df", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Der wolt es so, der so.", "tokens": ["Der", "wolt", "es", "so", ",", "der", "so", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "$,", "PRELS", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Bald fiel das Schiff zu Grunde,", "tokens": ["Bald", "fiel", "das", "Schiff", "zu", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Bald sprang es wieder auf", "tokens": ["Bald", "sprang", "es", "wie\u00b7der", "auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Und hielt in einer Stunde", "tokens": ["Und", "hielt", "in", "ei\u00b7ner", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "So manchen harten Lauf,", "tokens": ["So", "man\u00b7chen", "har\u00b7ten", "Lauf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Da\u00df wir den Trunknen gleich", "tokens": ["Da\u00df", "wir", "den", "Trunk\u00b7nen", "gleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.30": {"text": "Bald taumelten, bald fielen,", "tokens": ["Bald", "tau\u00b7mel\u00b7ten", ",", "bald", "fie\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.31": {"text": "Ja, wurden durch die\u00df W\u00fchlen", "tokens": ["Ja", ",", "wur\u00b7den", "durch", "die\u00df", "W\u00fch\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "VAFIN", "APPR", "PDS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Wie Todte bla\u00df und bleich.", "tokens": ["Wie", "Tod\u00b7te", "bla\u00df", "und", "bleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Da must', Herr, unser Leben", "tokens": ["Da", "must'", ",", "Herr", ",", "un\u00b7ser", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VMFIN", "$,", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Recht in der Grausamkeit", "tokens": ["Recht", "in", "der", "Grau\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.35": {"text": "Des tiefen Abgrunds schweben,", "tokens": ["Des", "tie\u00b7fen", "Ab\u00b7grunds", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Ja, machen sich bereit,", "tokens": ["Ja", ",", "ma\u00b7chen", "sich", "be\u00b7reit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.37": {"text": "Zu fahren in ein Grab", "tokens": ["Zu", "fah\u00b7ren", "in", "ein", "Grab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.38": {"text": "Von Wasser, nicht von Erden,", "tokens": ["Von", "Was\u00b7ser", ",", "nicht", "von", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Den Fischen da zu werden", "tokens": ["Den", "Fi\u00b7schen", "da", "zu", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "PTKZU", "VAINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "Ein' angenehme Gab'.", "tokens": ["Ein'", "an\u00b7ge\u00b7neh\u00b7me", "Gab'", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.41": {"text": "Ach, wie das T\u00e4ublein girret,", "tokens": ["Ach", ",", "wie", "das", "T\u00e4ub\u00b7lein", "gir\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.42": {"text": "So winselt' ich im Schiff,", "tokens": ["So", "win\u00b7selt'", "ich", "im", "Schiff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.43": {"text": "Ich lag doch gar verwirret,", "tokens": ["Ich", "lag", "doch", "gar", "ver\u00b7wir\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.44": {"text": "Als uns der Sturm ergriff;", "tokens": ["Als", "uns", "der", "Sturm", "er\u00b7griff", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.45": {"text": "Um Trost war mir sehr bang',", "tokens": ["Um", "Trost", "war", "mir", "sehr", "bang'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.46": {"text": "Ich rief in solchem Grauen:", "tokens": ["Ich", "rief", "in", "sol\u00b7chem", "Grau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.47": {"text": "Das Land werd' ich nicht schauen", "tokens": ["Das", "Land", "werd'", "ich", "nicht", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.48": {"text": "Hinfort mein Leben lang!", "tokens": ["Hin\u00b7fort", "mein", "Le\u00b7ben", "lang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.49": {"text": "Doch, der du liebst das Leben,", "tokens": ["Doch", ",", "der", "du", "liebst", "das", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.50": {"text": "Du Menschenh\u00fcter du,", "tokens": ["Du", "Men\u00b7schen\u00b7h\u00fc\u00b7ter", "du", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.51": {"text": "Du hast nicht zugegeben,", "tokens": ["Du", "hast", "nicht", "zu\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.52": {"text": "Da\u00df wir noch immerzu", "tokens": ["Da\u00df", "wir", "noch", "im\u00b7mer\u00b7zu"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.53": {"text": "Verlassen solten sein;", "tokens": ["Ver\u00b7las\u00b7sen", "sol\u00b7ten", "sein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.54": {"text": "Du lie\u00dfest H\u00fclfe kommen,", "tokens": ["Du", "lie\u00b7\u00dfest", "H\u00fcl\u00b7fe", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.55": {"text": "Du hast uns aufgenommen", "tokens": ["Du", "hast", "uns", "auf\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.56": {"text": "Durch deinen Schutz allein.", "tokens": ["Durch", "dei\u00b7nen", "Schutz", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.57": {"text": "Das Brausen ward gestillet,", "tokens": ["Das", "Brau\u00b7sen", "ward", "ge\u00b7stil\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.58": {"text": "Die Wellen legten sich,", "tokens": ["Die", "Wel\u00b7len", "leg\u00b7ten", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.59": {"text": "Der Himmel, der verh\u00fcllet", "tokens": ["Der", "Him\u00b7mel", ",", "der", "ver\u00b7h\u00fcl\u00b7let"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.60": {"text": "Gestanden j\u00e4mmerlich,", "tokens": ["Ge\u00b7stan\u00b7den", "j\u00e4m\u00b7mer\u00b7lich", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.61": {"text": "Ward wiedrum hell und klar.", "tokens": ["Ward", "wie\u00b7drum", "hell", "und", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.62": {"text": "So hast du, Herr, das Leben", "tokens": ["So", "hast", "du", ",", "Herr", ",", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.63": {"text": "Mir gleichsam neu gegeben,", "tokens": ["Mir", "gleich\u00b7sam", "neu", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.64": {"text": "Das schier verloren war.", "tokens": ["Das", "schier", "ver\u00b7lo\u00b7ren", "war", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.65": {"text": "Daf\u00fcr wil ich dich preisen,", "tokens": ["Da\u00b7f\u00fcr", "wil", "ich", "dich", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.66": {"text": "So lang' ich leb' und bin;", "tokens": ["So", "lang'", "ich", "leb'", "und", "bin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "VVFIN", "KON", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.67": {"text": "Ich wil dir Dank erweisen,", "tokens": ["Ich", "wil", "dir", "Dank", "er\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.68": {"text": "Herr, nimm die\u00df Opfer hin!", "tokens": ["Herr", ",", "nimm", "die\u00df", "Op\u00b7fer", "hin", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PDS", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.69": {"text": "Du bist mein st\u00e4rkster Hort,", "tokens": ["Du", "bist", "mein", "st\u00e4rks\u00b7ter", "Hort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.70": {"text": "Drum sol dein Lob f\u00fcr allen", "tokens": ["Drum", "sol", "dein", "Lob", "f\u00fcr", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPOSAT", "NN", "APPR", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.71": {"text": "In meinem Mund' erschallen", "tokens": ["In", "mei\u00b7nem", "Mund'", "er\u00b7schal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.72": {"text": "Recht freudig hier und dort.", "tokens": ["Recht", "freu\u00b7dig", "hier", "und", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}