{"textgrid.poem.61090": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Das Zauberschlo\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es gibt ein K\u00f6nigsschlo\u00df in alten Sagen,", "tokens": ["Es", "gibt", "ein", "K\u00f6\u00b7nigs\u00b7schlo\u00df", "in", "al\u00b7ten", "Sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch Zauberbann in w\u00fcsten Schutt zerfallen,", "tokens": ["Durch", "Zau\u00b7ber\u00b7bann", "in", "w\u00fcs\u00b7ten", "Schutt", "zer\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch wenn die rechten L\u00f6sungsworte schallen,", "tokens": ["Doch", "wenn", "die", "rech\u00b7ten", "L\u00f6\u00b7sungs\u00b7wor\u00b7te", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So steigt's empor wie in der Vorzeit Tagen.", "tokens": ["So", "steigt's", "em\u00b7por", "wie", "in", "der", "Vor\u00b7zeit", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "KOKOM", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da gl\u00e4nzt der Saal, die goldnen Zinnen ragen,", "tokens": ["Da", "gl\u00e4nzt", "der", "Saal", ",", "die", "gold\u00b7nen", "Zin\u00b7nen", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jasmin und Ros' umbl\u00fchn die S\u00e4ulenhallen,", "tokens": ["Jas\u00b7min", "und", "Ros'", "um\u00b7bl\u00fchn", "die", "S\u00e4u\u00b7len\u00b7hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es tanzen M\u00e4dchen, Purpurkleider wallen,", "tokens": ["Es", "tan\u00b7zen", "M\u00e4d\u00b7chen", ",", "Pur\u00b7pur\u00b7klei\u00b7der", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Silberharfen h\u00f6rst du lieblich schlagen.", "tokens": ["Und", "Sil\u00b7ber\u00b7har\u00b7fen", "h\u00f6rst", "du", "lieb\u00b7lich", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Den Tr\u00fcmmern glich mein Herz. Es mu\u00dfte lange", "tokens": ["Den", "Tr\u00fcm\u00b7mern", "glich", "mein", "Herz", ".", "Es", "mu\u00df\u00b7te", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$.", "PPER", "VMFIN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Graus und Finsternis ver\u00f6det liegen,", "tokens": ["In", "Graus", "und", "Fins\u00b7ter\u00b7nis", "ver\u00b7\u00f6\u00b7det", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und drinnen war es leer und dumpf und bange.", "tokens": ["Und", "drin\u00b7nen", "war", "es", "leer", "und", "dumpf", "und", "ban\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da sprachest du, den Bannfluch zu besiegen,", "tokens": ["Da", "spra\u00b7chest", "du", ",", "den", "Bann\u00b7fluch", "zu", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das L\u00f6sungswort, und sieh, mit hellem Klange", "tokens": ["Das", "L\u00f6\u00b7sungs\u00b7wort", ",", "und", "sieh", ",", "mit", "hel\u00b7lem", "Klan\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist draus der Liebe Zauberschlo\u00df gestiegen.", "tokens": ["Ist", "draus", "der", "Lie\u00b7be", "Zau\u00b7ber\u00b7schlo\u00df", "ge\u00b7stie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}