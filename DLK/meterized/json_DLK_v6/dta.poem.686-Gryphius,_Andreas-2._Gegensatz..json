{"dta.poem.686": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "2. Gegensatz.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.57", "af:0.42"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Dv hast mein GOtt die jmmer nassen wangen/ ", "tokens": ["Dv", "hast", "mein", "Gott", "die", "jm\u00b7mer", "nas\u00b7sen", "wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Getrucknet mit liebreicher handt/", "tokens": ["Ge\u00b7truck\u00b7net", "mit", "lieb\u00b7rei\u00b7cher", "handt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein arm hat mich erhalten ", "tokens": ["Dein", "arm", "hat", "mich", "er\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn ich den m\u00fcden ", "tokens": ["Wenn", "ich", "den", "m\u00fc\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Denn hast du lassen walten", "tokens": ["Denn", "hast", "du", "las\u00b7sen", "wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "VVINF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die s\u00fcsse gunst/ durch die ich bin entgangen/ ", "tokens": ["Die", "s\u00fcs\u00b7se", "gunst", "/", "durch", "die", "ich", "bin", "ent\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "APPR", "PRELS", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wenn mancher must\u2019 erkalten/", "tokens": ["Wenn", "man\u00b7cher", "must'", "er\u00b7kal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wenn mancher fiel/ hab ich von dir empfangen/", "tokens": ["Wenn", "man\u00b7cher", "fiel", "/", "hab", "ich", "von", "dir", "emp\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$(", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Manch\u2019 ewig trewer liebe pfandt.", "tokens": ["Man\u00b7ch'", "e\u00b7wig", "tre\u00b7wer", "lie\u00b7be", "pfandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}