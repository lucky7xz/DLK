{"textgrid.poem.38578": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An Laura", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freud' umbl\u00fche dich auf allen Wegen,", "tokens": ["Freud'", "um\u00b7bl\u00fc\u00b7he", "dich", "auf", "al\u00b7len", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sch\u00f6ner als sie je die Unschuld fand,", "tokens": ["Sch\u00f6\u00b7ner", "als", "sie", "je", "die", "Un\u00b7schuld", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Seelenruh, des Himmels bester Segen", "tokens": ["See\u00b7len\u00b7ruh", ",", "des", "Him\u00b7mels", "bes\u00b7ter", "Se\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Walle dir wie Fr\u00fchlingshauch entgegen,", "tokens": ["Wal\u00b7le", "dir", "wie", "Fr\u00fch\u00b7lings\u00b7hauch", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KOKOM", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Bis zum Wiedersehn im Lichtgewand!", "tokens": ["Bis", "zum", "Wie\u00b7der\u00b7sehn", "im", "Licht\u00b7ge\u00b7wand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "L\u00e4chelnd wird der Seraph niederschweben,", "tokens": ["L\u00e4\u00b7chelnd", "wird", "der", "Se\u00b7raph", "nie\u00b7der\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der die Palme der Vergeltung tr\u00e4gt,", "tokens": ["Der", "die", "Pal\u00b7me", "der", "Ver\u00b7gel\u00b7tung", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Aus dem dunkeln Thal zu jenem Leben", "tokens": ["Aus", "dem", "dun\u00b7keln", "Thal", "zu", "je\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Deine sch\u00f6ne Seele zu erheben,", "tokens": ["Dei\u00b7ne", "sch\u00f6\u00b7ne", "See\u00b7le", "zu", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wo der Richter unsre Thaten w\u00e4gt.", "tokens": ["Wo", "der", "Rich\u00b7ter", "uns\u00b7re", "Tha\u00b7ten", "w\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "O dann t\u00f6ne Gottes ernste Waage", "tokens": ["O", "dann", "t\u00f6\u00b7ne", "Got\u00b7tes", "erns\u00b7te", "Waa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wonne dir, von jedem Misklang frei,", "tokens": ["Won\u00b7ne", "dir", ",", "von", "je\u00b7dem", "Mis\u00b7klang", "frei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und der Freund an deinem Grabe sage:", "tokens": ["Und", "der", "Freund", "an", "dei\u00b7nem", "Gra\u00b7be", "sa\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Gl\u00fcckliche! der lezte deiner Tage", "tokens": ["Gl\u00fcck\u00b7li\u00b7che", "!", "der", "lez\u00b7te", "dei\u00b7ner", "Ta\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ART", "ADJA", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "War ein Sonnenuntergang im Mai!", "tokens": ["War", "ein", "Son\u00b7nen\u00b7un\u00b7ter\u00b7gang", "im", "Mai", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}