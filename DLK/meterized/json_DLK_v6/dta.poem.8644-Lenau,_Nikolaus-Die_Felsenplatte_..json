{"dta.poem.8644": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Die Felsenplatte .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Dort am steilen Klippenhange,", "tokens": ["Dort", "am", "stei\u00b7len", "Klip\u00b7pen\u00b7han\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo der Wildbach niedersch\u00e4umt,", "tokens": ["Wo", "der", "Wild\u00b7bach", "nie\u00b7der\u00b7sch\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lehnt beim Sonnenuntergange", "tokens": ["Lehnt", "beim", "Son\u00b7nen\u00b7un\u00b7ter\u00b7gan\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einsam still ein Mann \u2014 und tr\u00e4umt.", "tokens": ["Ein\u00b7sam", "still", "ein", "Mann", "und", "tr\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ART", "NN", "$(", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hingesenkt das gramesmatte", "tokens": ["Hin\u00b7ge\u00b7senkt", "das", "gra\u00b7mes\u00b7mat\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Angesicht, so fr\u00fch verbl\u00fcht,", "tokens": ["An\u00b7ge\u00b7sicht", ",", "so", "fr\u00fch", "ver\u00b7bl\u00fcht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Starrt er auf die Felsenplatte,", "tokens": ["Starrt", "er", "auf", "die", "Fel\u00b7sen\u00b7plat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die vom Abendrothe gl\u00fcht.", "tokens": ["Die", "vom", "A\u00b7ben\u00b7dro\u00b7the", "gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie er also unabwendig", "tokens": ["Wie", "er", "al\u00b7so", "un\u00b7ab\u00b7wen\u00b7dig"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Starret auf den hellen Stein,", "tokens": ["Star\u00b7ret", "auf", "den", "hel\u00b7len", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Werden pl\u00f6tzlich d'rauf lebendig", "tokens": ["Wer\u00b7den", "pl\u00f6tz\u00b7lich", "d'\u00b7rauf", "le\u00b7ben\u00b7dig"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "PAV", "ADJD"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Seine lieben Fantasei'n.", "tokens": ["Sei\u00b7ne", "lie\u00b7ben", "Fan\u00b7ta\u00b7sei'", "n."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["PPOSAT", "ADJA", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Seiner Kindheit Spielgenossen", "tokens": ["Sei\u00b7ner", "Kind\u00b7heit", "Spiel\u00b7ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tanzen lustig dr\u00fcber hin", "tokens": ["Tan\u00b7zen", "lus\u00b7tig", "dr\u00fc\u00b7ber", "hin"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "PAV", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit der Unschuld s\u00fc\u00dfen Possen,", "tokens": ["Mit", "der", "Un\u00b7schuld", "s\u00fc\u00b7\u00dfen", "Pos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Laden ein zu Spielen ihn.", "tokens": ["La\u00b7den", "ein", "zu", "Spie\u00b7len", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PTKZU", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Auch sein M\u00fctterlein, die gute,", "tokens": ["Auch", "sein", "M\u00fct\u00b7ter\u00b7lein", ",", "die", "gu\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wandelt l\u00e4chelnd auf dem Stein,", "tokens": ["Wan\u00b7delt", "l\u00e4\u00b7chelnd", "auf", "dem", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die so manches Jahr schon ruhte", "tokens": ["Die", "so", "man\u00b7ches", "Jahr", "schon", "ruh\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PIAT", "NN", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem \u00f6den Todtenschrein.", "tokens": ["In", "dem", "\u00f6\u00b7den", "Tod\u00b7ten\u00b7schrein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und nun sieht er unter ihnen", "tokens": ["Und", "nun", "sieht", "er", "un\u00b7ter", "ih\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Klar sein eignes Jugendbild,", "tokens": ["Klar", "sein", "eig\u00b7nes", "Ju\u00b7gend\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit den frohen Fremdlingsmienen", "tokens": ["Mit", "den", "fro\u00b7hen", "Fremd\u00b7lings\u00b7mie\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf der Erde Schmerzgefild.", "tokens": ["Auf", "der", "Er\u00b7de", "Schmerz\u00b7ge\u00b7fild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und er h\u00f6rt das laute Klopfen", "tokens": ["Und", "er", "h\u00f6rt", "das", "lau\u00b7te", "Klop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In des J\u00fcnglings hei\u00dfer Brust,", "tokens": ["In", "des", "J\u00fcng\u00b7lings", "hei\u00b7\u00dfer", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieht vom Aug' ihm niedertropfen", "tokens": ["Sieht", "vom", "Aug'", "ihm", "nie\u00b7der\u00b7trop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Thr\u00e4nen, selig, unbewu\u00dft;", "tokens": ["Thr\u00e4\u00b7nen", ",", "se\u00b7lig", ",", "un\u00b7be\u00b7wu\u00dft", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "M\u00f6chte mit dem J\u00fcngling greinen,", "tokens": ["M\u00f6ch\u00b7te", "mit", "dem", "J\u00fcng\u00b7ling", "grei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df er traut der holden M\u00e4hr;", "tokens": ["Da\u00df", "er", "traut", "der", "hol\u00b7den", "M\u00e4hr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und auch wieder bitter weinen,", "tokens": ["Und", "auch", "wie\u00b7der", "bit\u00b7ter", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er nicht der J\u00fcngling mehr. \u2014", "tokens": ["Da\u00df", "er", "nicht", "der", "J\u00fcng\u00b7ling", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}