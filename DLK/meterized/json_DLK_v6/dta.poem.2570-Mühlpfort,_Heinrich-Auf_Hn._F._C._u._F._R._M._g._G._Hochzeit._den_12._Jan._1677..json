, "in", "den", "Wein\u00b7berg", "sich", "so", "th\u00e4\u00b7tig", "nun", "er\u00b7wei\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "PRF", "ADV", "ADJD", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Und seines Lebens Ziel nach ", "tokens": ["Und", "sei\u00b7nes", "Le\u00b7bens", "Ziel", "nach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.49": {"text": "Der hat gewi\u00df auch nichts von seiner Hand zu hoffen/", "tokens": ["Der", "hat", "ge\u00b7wi\u00df", "auch", "nichts", "von", "sei\u00b7ner", "Hand", "zu", "hof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "PIS", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Als \u00fcberschwenglich Heil und Gnade fr\u00fch und spat.", "tokens": ["Als", "\u00fc\u00b7bersc\u00b7hweng\u00b7lich", "Heil", "und", "Gna\u00b7de", "fr\u00fch", "und", "spat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "NN", "KON", "NN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Wohl ihm ", "tokens": ["Wohl", "ihm"], "token_info": ["word", "word"], "pos": ["ADV", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.52": {"text": "Und ihm das beste Theil zuvor erwehlet hat.", "tokens": ["Und", "ihm", "das", "bes\u00b7te", "Theil", "zu\u00b7vor", "er\u00b7weh\u00b7let", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Gesetzt/ es rufft ihn Gott auch um die letzte Stunde/", "tokens": ["Ge\u00b7setzt", "/", "es", "rufft", "ihn", "Gott", "auch", "um", "die", "letz\u00b7te", "Stun\u00b7de", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$(", "PPER", "VVFIN", "PPER", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.55": {"text": "Wer nur bey seinem Gott steht im Genaden-Bunde/", "tokens": ["Wer", "nur", "bey", "sei\u00b7nem", "Gott", "steht", "im", "Ge\u00b7na\u00b7den\u00b7Bun\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Dem bringt das letzte Lo\u00df den besten Vortheil ein.", "tokens": ["Dem", "bringt", "das", "letz\u00b7te", "Lo\u00df", "den", "bes\u00b7ten", "Vor\u00b7theil", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Sein Wahlspruch bleibet wahr. Es geht nach ", "tokens": ["Sein", "Wahl\u00b7spruch", "blei\u00b7bet", "wahr", ".", "Es", "geht", "nach"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.58": {"text": "Hier nntzet ihm der Mensch mit seinem rathen nicht/", "tokens": ["Hier",