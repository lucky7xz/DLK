{"dta.poem.2570": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf Hn. F. C. u. F. R. M. g. G. Hochzeit.  \n den 12. Jan. 1677.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Herr Chremitz dieser Tag ist einer von den besten/", "tokens": ["Herr", "Chre\u00b7mitz", "die\u00b7ser", "Tag", "ist", "ei\u00b7ner", "von", "den", "bes\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PDAT", "NN", "VAFIN", "ART", "APPR", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ihm des Himmels Gunst auff dieser Erden schenckt.", "tokens": ["So", "ihm", "des", "Him\u00b7mels", "Gunst", "auff", "die\u00b7ser", "Er\u00b7den", "schenckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er setz\u2019 ietzt auff sein Haar die Kron aus Myrten-Aesten/", "tokens": ["Er", "setz'", "ietzt", "auff", "sein", "Haar", "die", "Kron", "aus", "Myr\u00b7ten\u00b7Aes\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und was betr\u00fcben kan sey in die See gesenckt.", "tokens": ["Und", "was", "be\u00b7tr\u00fc\u00b7ben", "kan", "sey", "in", "die", "See", "ge\u00b7senckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVINF", "VMFIN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Auff heute mu\u00df sein Hertz in wahren Freuden gl\u00fchen/", "tokens": ["Auff", "heu\u00b7te", "mu\u00df", "sein", "Hertz", "in", "wah\u00b7ren", "Freu\u00b7den", "gl\u00fc\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VMFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es ist die Einsamkeit von seiner Brust verbannt/", "tokens": ["Es", "ist", "die", "Ein\u00b7sam\u00b7keit", "von", "sei\u00b7ner", "Brust", "ver\u00b7bannt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nun Liebe/ Lust und Schertz in allen Zimmern bl\u00fchen", "tokens": ["Nun", "Lie\u00b7be", "/", "Lust", "und", "Schertz", "in", "al\u00b7len", "Zim\u00b7mern", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "$(", "NN", "KON", "NN", "APPR", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und seine Seele f\u00fchlt den heissen Liebes-Brand.", "tokens": ["Und", "sei\u00b7ne", "See\u00b7le", "f\u00fchlt", "den", "heis\u00b7sen", "Lie\u00b7bes\u00b7Brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er hat zwar lange Zeit den Flammen widerstrebet/", "tokens": ["Er", "hat", "zwar", "lan\u00b7ge", "Zeit", "den", "Flam\u00b7men", "wi\u00b7der\u00b7stre\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Jetzt sp\u00fcrt er da\u00df der Zug von einer h\u00f6hern Macht.", "tokens": ["Jetzt", "sp\u00fcrt", "er", "da\u00df", "der", "Zug", "von", "ei\u00b7ner", "h\u00f6\u00b7hern", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Er sieht/ da\u00df er in nichts als W\u00fcsteney gelebet", "tokens": ["Er", "sieht", "/", "da\u00df", "er", "in", "nichts", "als", "W\u00fcs\u00b7te\u00b7ney", "ge\u00b7le\u00b7bet"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "APPR", "PIS", "KOKOM", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nun ihn Cupido hat ins Garn der Lust gebracht.", "tokens": ["Nun", "ihn", "Cu\u00b7pi\u00b7do", "hat", "ins", "Garn", "der", "Lust", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "NE", "VAFIN", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wer ruffet nicht Gl\u00fcck zu? nun auch ", "tokens": ["Wer", "ruf\u00b7fet", "nicht", "Gl\u00fcck", "zu", "?", "nun", "auch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "PTKNEG", "NN", "PTKVZ", "$.", "ADV", "ADV"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.14": {"text": "Und seines Namens Ruhm und Ewigkeit bedenckt", "tokens": ["Und", "sei\u00b7nes", "Na\u00b7mens", "Ruhm", "und", "E\u00b7wig\u00b7keit", "be\u00b7denckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Nun er der keuschen Eh sich willig untergiebet/", "tokens": ["Nun", "er", "der", "keu\u00b7schen", "Eh", "sich", "wil\u00b7lig", "un\u00b7ter\u00b7gie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "ADJA", "NN", "PRF", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und den sich lencken l\u00e4st/ der Welt und Himmel lenckt.", "tokens": ["Und", "den", "sich", "len\u00b7cken", "l\u00e4st", "/", "der", "Welt", "und", "Him\u00b7mel", "lenckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PRF", "VVINF", "VVFIN", "$(", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich mag von Buhlerey und K\u00fcssen ihm nichts schreiben/", "tokens": ["Ich", "mag", "von", "Buh\u00b7le\u00b7rey", "und", "K\u00fcs\u00b7sen", "ihm", "nichts", "schrei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "KON", "NN", "PPER", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ich wei\u00df sein reiner Geist verlacht die Leckerey.", "tokens": ["Ich", "wei\u00df", "sein", "rei\u00b7ner", "Geist", "ver\u00b7lacht", "die", "Le\u00b7cke\u00b7rey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Mein Gl\u00fcckwunsch soll allein bey diesem End-Zweck bleiben/", "tokens": ["Mein", "Gl\u00fcck\u00b7wunsch", "soll", "al\u00b7lein", "bey", "die\u00b7sem", "En\u00b7dZweck", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da\u00df sein gefaster Schlu\u00df des H\u00f6chsten Satzung sey.", "tokens": ["Da\u00df", "sein", "ge\u00b7fas\u00b7ter", "Schlu\u00df", "des", "H\u00f6chs\u00b7ten", "Sat\u00b7zung", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Denn/ wie er ", "tokens": ["Denn", "/", "wie", "er"], "token_info": ["word", "punct", "word", "word"], "pos": ["KON", "$(", "PWAV", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.22": {"text": "Und mit Gedancken stets dem Himmel ist verwand.", "tokens": ["Und", "mit", "Ge\u00b7dan\u00b7cken", "stets", "dem", "Him\u00b7mel", "ist", "ver\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So will auch Gottes Huld mit Segen ihn erf\u00fcllen/", "tokens": ["So", "will", "auch", "Got\u00b7tes", "Huld", "mit", "Se\u00b7gen", "ihn", "er\u00b7f\u00fcl\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "NN", "NN", "APPR", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und Gl\u00fcck und Wolfarth wird bekr\u00f6nen seinen Stand.", "tokens": ["Und", "Gl\u00fcck", "und", "Wolf\u00b7arth", "wird", "be\u00b7kr\u00f6\u00b7nen", "sei\u00b7nen", "Stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wie treu er Gott geliebt/ bef\u00f6rdert was zur Ehre/", "tokens": ["Wie", "treu", "er", "Gott", "ge\u00b7liebt", "/", "be\u00b7f\u00f6r\u00b7dert", "was", "zur", "Eh\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "NN", "VVPP", "$(", "VVPP", "PRELS", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und seines Namens Prei\u00df bey Kirchen dienen kan/", "tokens": ["Und", "sei\u00b7nes", "Na\u00b7mens", "Prei\u00df", "bey", "Kir\u00b7chen", "die\u00b7nen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "APPR", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ist unnoth da\u00df mein Reim bey diesem Fest es lehre/", "tokens": ["Ist", "un\u00b7noth", "da\u00df", "mein", "Reim", "bey", "die\u00b7sem", "Fest", "es", "leh\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOUS", "PPOSAT", "NN", "APPR", "PDAT", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der Nachruff hat es l\u00e4ngst ruhmw\u00fcrdig kund gethan.", "tokens": ["Der", "Nach\u00b7ruff", "hat", "es", "l\u00e4ngst", "ruhm\u00b7w\u00fcr\u00b7dig", "kund", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ADJD", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Er f\u00fchrt die Gottesfurcht in keinem Heuchel-Hertzen/", "tokens": ["Er", "f\u00fchrt", "die", "Got\u00b7tes\u00b7furcht", "in", "kei\u00b7nem", "Heu\u00b7chel\u00b7Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und seine Redligkeit ist auf die Stirn gepr\u00e4gt/", "tokens": ["Und", "sei\u00b7ne", "Red\u00b7lig\u00b7keit", "ist", "auf", "die", "Stirn", "ge\u00b7pr\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "L\u00e4st andrer Wanckelmuth mit Eyd und Schw\u00fcren schertzen/", "tokens": ["L\u00e4st", "an\u00b7drer", "Wan\u00b7ckel\u00b7muth", "mit", "Eyd", "und", "Schw\u00fc\u00b7ren", "schert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Sein Anfrecht-deutscher Sinn wird nicht dadurch bewegt.", "tokens": ["Sein", "An\u00b7frecht\u00b7deut\u00b7scher", "Sinn", "wird", "nicht", "da\u00b7durch", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAFIN", "PTKNEG", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ja ist der Christen Kirch\u2019 ein Weinberg zu benennen/", "tokens": ["Ja", "ist", "der", "Chris\u00b7ten", "Kirch'", "ein", "Wein\u00b7berg", "zu", "be\u00b7nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wie embsig hat er nicht denselben angebaut/", "tokens": ["Wie", "emb\u00b7sig", "hat", "er", "nicht", "den\u00b7sel\u00b7ben", "an\u00b7ge\u00b7baut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "PTKNEG", "PDS", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Man wird nach seinem Tod auch seine Wolthat kennen/", "tokens": ["Man", "wird", "nach", "sei\u00b7nem", "Tod", "auch", "sei\u00b7ne", "Wolt\u00b7hat", "ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So oft ein frembdes Aug ", "tokens": ["So", "oft", "ein", "fremb\u00b7des", "Aug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.37": {"text": "Es klingen ihm zum Ruhm des Vaterlandes Glocken/", "tokens": ["Es", "klin\u00b7gen", "ihm", "zum", "Ruhm", "des", "Va\u00b7ter\u00b7lan\u00b7des", "Glo\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die seine Mildigkeit zu Bautzen hat geweyht.", "tokens": ["Die", "sei\u00b7ne", "Mil\u00b7dig\u00b7keit", "zu", "Baut\u00b7zen", "hat", "ge\u00b7weyht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und seiner Andacht Fest kan sichre Menschen locken/", "tokens": ["Und", "sei\u00b7ner", "An\u00b7dacht", "Fest", "kan", "sich\u00b7re", "Men\u00b7schen", "lo\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Da\u00df sie den Fu\u00df entziehn der schn\u00f6den Eitelkeit:", "tokens": ["Da\u00df", "sie", "den", "Fu\u00df", "ent\u00b7ziehn", "der", "schn\u00f6\u00b7den", "Ei\u00b7tel\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Was schenckt uns nicht sein Buch f\u00fcr sch\u00f6ne Todten-Lieder/", "tokens": ["Was", "schenckt", "uns", "nicht", "sein", "Buch", "f\u00fcr", "sch\u00f6\u00b7ne", "Tod\u00b7ten\u00b7Lie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Wann vor den Schauplatz er den Kirchhof ihm erkiest/", "tokens": ["Wann", "vor", "den", "Schau\u00b7platz", "er", "den", "Kirch\u00b7hof", "ihm", "er\u00b7kiest", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["P