{"textgrid.poem.24656": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was ist die Welt? Ein ewiges Gedicht,", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist die Welt? Ein ewiges Gedicht,", "tokens": ["Was", "ist", "die", "Welt", "?", "Ein", "e\u00b7wi\u00b7ges", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Daraus der Geist der Gottheit strahlt und gl\u00fcht,", "tokens": ["Da\u00b7raus", "der", "Geist", "der", "Got\u00b7theit", "strahlt", "und", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Daraus der Wein der Weisheit sch\u00e4umt und spr\u00fcht,", "tokens": ["Da\u00b7raus", "der", "Wein", "der", "Weis\u00b7heit", "sch\u00e4umt", "und", "spr\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Daraus der Laut der Liebe zu uns spricht", "tokens": ["Da\u00b7raus", "der", "Laut", "der", "Lie\u00b7be", "zu", "uns", "spricht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "APPR", "ART", "NN", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und jedes Menschen wechselndes Gem\u00fct,", "tokens": ["Und", "je\u00b7des", "Men\u00b7schen", "wech\u00b7seln\u00b7des", "Ge\u00b7m\u00fct", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Strahl ists, der aus dieser Sonne bricht,", "tokens": ["Ein", "Strahl", "ists", ",", "der", "aus", "die\u00b7ser", "Son\u00b7ne", "bricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Vers, der sich an tausend andre flicht,", "tokens": ["Ein", "Vers", ",", "der", "sich", "an", "tau\u00b7send", "and\u00b7re", "flicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "APPR", "CARD", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der unbemerkt verhallt, verlischt, verbl\u00fcht.", "tokens": ["Der", "un\u00b7be\u00b7merkt", "ver\u00b7hallt", ",", "ver\u00b7lischt", ",", "ver\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "$,", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und doch auch eine Welt f\u00fcr sich allein,", "tokens": ["Und", "doch", "auch", "ei\u00b7ne", "Welt", "f\u00fcr", "sich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "APPR", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Voll s\u00fc\u00df-geheimer, nievernommner T\u00f6ne,", "tokens": ["Voll", "s\u00fc\u00df\u00b7ge\u00b7hei\u00b7mer", ",", "nie\u00b7ver\u00b7nomm\u00b7ner", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Begabt mit eigner, unentweihter Sch\u00f6ne,", "tokens": ["Be\u00b7gabt", "mit", "eig\u00b7ner", ",", "un\u00b7ent\u00b7weih\u00b7ter", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und keines Andern Nachhall, Widerschein.", "tokens": ["Und", "kei\u00b7nes", "An\u00b7dern", "Nach\u00b7hall", ",", "Wi\u00b7der\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und wenn du gar zu lesen drin verst\u00fcndest,", "tokens": ["Und", "wenn", "du", "gar", "zu", "le\u00b7sen", "drin", "ver\u00b7st\u00fcn\u00b7dest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PTKZU", "VVINF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Buch, das du im Leben nicht ergr\u00fcndest.", "tokens": ["Ein", "Buch", ",", "das", "du", "im", "Le\u00b7ben", "nicht", "er\u00b7gr\u00fcn\u00b7dest", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}