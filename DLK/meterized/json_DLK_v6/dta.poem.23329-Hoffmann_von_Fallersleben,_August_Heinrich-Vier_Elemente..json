{"dta.poem.23329": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vier Elemente.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wollt ihr uns repr\u00e4sentieren,", "tokens": ["Wollt", "ihr", "uns", "re\u00b7pr\u00e4\u00b7sen\u00b7tie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Haltet fest an diesen vieren:", "tokens": ["Hal\u00b7tet", "fest", "an", "die\u00b7sen", "vie\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PDAT", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Geist und Arbeit, Gut und Geld", "tokens": ["Geist", "und", "Ar\u00b7beit", ",", "Gut", "und", "Geld"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "ADJD", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind die St\u00e4nde dieser Welt.", "tokens": ["Sind", "die", "St\u00e4n\u00b7de", "die\u00b7ser", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "G\u00f6nnet jedem auszusprechen", "tokens": ["G\u00f6n\u00b7net", "je\u00b7dem", "aus\u00b7zu\u00b7spre\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seine Leiden und Gebrechen!", "tokens": ["Sei\u00b7ne", "Lei\u00b7den", "und", "Ge\u00b7bre\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lernt, da\u00df zu vertreten ist", "tokens": ["Lernt", ",", "da\u00df", "zu", "ver\u00b7tre\u00b7ten", "ist"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PTKZU", "VVINF", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mehr als Adel nur und Mist.", "tokens": ["Mehr", "als", "A\u00b7del", "nur", "und", "Mist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "NN", "ADV", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}