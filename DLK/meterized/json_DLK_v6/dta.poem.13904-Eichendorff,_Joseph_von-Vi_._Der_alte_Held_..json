{"dta.poem.13904": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Vi .  \n  Der alte Held .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewie rauschen nun W\u00e4lder und Quellen", "tokens": ["\u201e", "wie", "rau\u00b7schen", "nun", "W\u00e4l\u00b7der", "und", "Quel\u00b7len"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "VVFIN", "ADV", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und singen vom ewigen Port:", "tokens": ["Und", "sin\u00b7gen", "vom", "e\u00b7wi\u00b7gen", "Port", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Schon seh' ich Morgenroth schwellen,", "tokens": ["Schon", "seh'", "ich", "Mor\u00b7gen\u00b7roth", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und ihr dort, ihr jungen Gesellen,", "tokens": ["Und", "ihr", "dort", ",", "ihr", "jun\u00b7gen", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Fahrt immer immerfort!\u201c", "tokens": ["Fahrt", "im\u00b7mer", "im\u00b7mer\u00b7fort", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}