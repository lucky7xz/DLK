{"textgrid.poem.65067": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sei stark, mein Freund! und la\u00df' kein weichlich Zagen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei stark, mein Freund! und la\u00df' kein weichlich Zagen", "tokens": ["Sei", "stark", ",", "mein", "Freund", "!", "und", "la\u00df'", "kein", "weich\u00b7lich", "Za\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "$,", "PPOSAT", "NN", "$.", "KON", "VVIMP", "PIAT", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor d\u00fcst'rer Wahrheit dich die Blicke wenden,", "tokens": ["Vor", "d\u00fcst'\u00b7rer", "Wahr\u00b7heit", "dich", "die", "Bli\u00b7cke", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was sie begannen, will ich selbst vollenden:", "tokens": ["Was", "sie", "be\u00b7gan\u00b7nen", ",", "will", "ich", "selbst", "voll\u00b7en\u00b7den", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Stimmen l\u00fcgen nicht, die mich verklagen.", "tokens": ["Die", "Stim\u00b7men", "l\u00fc\u00b7gen", "nicht", ",", "die", "mich", "ver\u00b7kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$,", "PRELS", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nach falschen G\u00fctern ging mein wildes Jagen,", "tokens": ["Nach", "fal\u00b7schen", "G\u00fc\u00b7tern", "ging", "mein", "wil\u00b7des", "Ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Schuld erlaubt' ich, mit verruchten H\u00e4nden", "tokens": ["Der", "Schuld", "er\u00b7laubt'", "ich", ",", "mit", "ver\u00b7ruch\u00b7ten", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Innern stilles Heiligthum zu sch\u00e4nden,", "tokens": ["Des", "In\u00b7nern", "stil\u00b7les", "Hei\u00b7lig\u00b7thum", "zu", "sch\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Unw\u00fcrd'ge Fesseln hat mein Herz getragen.", "tokens": ["Un\u00b7w\u00fcrd'\u00b7ge", "Fes\u00b7seln", "hat", "mein", "Herz", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Und l\u00e4hmen lie\u00df ich meines Geistes Schwingen,", "tokens": ["Und", "l\u00e4h\u00b7men", "lie\u00df", "ich", "mei\u00b7nes", "Geis\u00b7tes", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ergab der Welt mich, von dem Wahn betrogen,", "tokens": ["Er\u00b7gab", "der", "Welt", "mich", ",", "von", "dem", "Wahn", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch den Genu\u00df zum Gl\u00fccke hinzudringen.", "tokens": ["Durch", "den", "Ge\u00b7nu\u00df", "zum", "Gl\u00fc\u00b7cke", "hin\u00b7zu\u00b7drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}, "stanza.4": {"line.1": {"text": "Bis ich, da jene T\u00e4uschung mir entflogen,", "tokens": ["Bis", "ich", ",", "da", "je\u00b7ne", "T\u00e4u\u00b7schung", "mir", "ent\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PDAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dastand, umg\u00fcrtet von den Schlangenringen", "tokens": ["Das\u00b7tand", ",", "um\u00b7g\u00fcr\u00b7tet", "von", "den", "Schlan\u00b7gen\u00b7rin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Ungeheuer, die ich gro\u00dfgezogen.", "tokens": ["Der", "Un\u00b7ge\u00b7heu\u00b7er", ",", "die", "ich", "gro\u00df\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}