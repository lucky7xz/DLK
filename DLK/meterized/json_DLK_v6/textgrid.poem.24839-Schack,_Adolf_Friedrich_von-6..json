{"textgrid.poem.24839": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie sind es, ja! im Wasserfall", "tokens": ["Sie", "sind", "es", ",", "ja", "!", "im", "Was\u00b7ser\u00b7fall"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "$,", "ADV", "$.", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vernehm' ich ihrer Stimmen Schall", "tokens": ["Ver\u00b7nehm'", "ich", "ih\u00b7rer", "Stim\u00b7men", "Schall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und in den Murmelquellen;", "tokens": ["Und", "in", "den", "Mur\u00b7mel\u00b7quel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie rufen mich im Abendwind,", "tokens": ["Sie", "ru\u00b7fen", "mich", "im", "Ab\u00b7end\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mich ihnen, so wie einst als Kind,", "tokens": ["Mich", "ih\u00b7nen", ",", "so", "wie", "einst", "als", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "$,", "ADV", "KOKOM", "ADV", "KOUS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Beim Mondlicht zu gesellen.", "tokens": ["Beim", "Mond\u00b7licht", "zu", "ge\u00b7sel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So fern, ihr Geister, jene Zeit,", "tokens": ["So", "fern", ",", "ihr", "Geis\u00b7ter", ",", "je\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PPOSAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als ich in Waldeseinsamkeit", "tokens": ["Als", "ich", "in", "Wal\u00b7de\u00b7sein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Euch meine Br\u00fcder nannte,", "tokens": ["Euch", "mei\u00b7ne", "Br\u00fc\u00b7der", "nann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und euer Blick, so sanft, so mild,", "tokens": ["Und", "eu\u00b7er", "Blick", ",", "so", "sanft", ",", "so", "mild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie Schein, der aus den Sternen quillt,", "tokens": ["Wie", "Schein", ",", "der", "aus", "den", "Ster\u00b7nen", "quillt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Herz an euch mir bannte!", "tokens": ["Das", "Herz", "an", "euch", "mir", "bann\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Als wir umhergeschweift am See,", "tokens": ["Als", "wir", "um\u00b7her\u00b7ge\u00b7schweift", "am", "See", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo auf dem Lager sich das Reh,", "tokens": ["Wo", "auf", "dem", "La\u00b7ger", "sich", "das", "Reh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Waldlust tr\u00e4umend, regte,", "tokens": ["Von", "Wald\u00b7lust", "tr\u00e4u\u00b7mend", ",", "reg\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Indes der n\u00e4cht'ge Schmetterling,", "tokens": ["In\u00b7des", "der", "n\u00e4cht'\u00b7ge", "Schmet\u00b7ter\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der an der Wei\u00dfdornbl\u00fcte hing,", "tokens": ["Der", "an", "der", "Wei\u00df\u00b7dorn\u00b7bl\u00fc\u00b7te", "hing", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Schwingen sanft bewegte!", "tokens": ["Die", "Schwin\u00b7gen", "sanft", "be\u00b7weg\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O nie ward in der Menschenwelt,", "tokens": ["O", "nie", "ward", "in", "der", "Men\u00b7schen\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihrer Schw\u00fcre keinen h\u00e4lt,", "tokens": ["Die", "ih\u00b7rer", "Schw\u00fc\u00b7re", "kei\u00b7nen", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PIAT", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So wie bei euch mir Friede!", "tokens": ["So", "wie", "bei", "euch", "mir", "Frie\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "PPER", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nehmt neu mich auf in euern Kreis,", "tokens": ["Nehmt", "neu", "mich", "auf", "in", "eu\u00b7ern", "Kreis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und k\u00fc\u00dft den Lebenstraum mir leis", "tokens": ["Und", "k\u00fc\u00dft", "den", "Le\u00b7ben\u00b7straum", "mir", "leis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hinweg vom Augenlide!", "tokens": ["Hin\u00b7weg", "vom", "Au\u00b7gen\u00b7li\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}