{"dta.poem.15005": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Historische Nachricht.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1741", "urn": "urn:nbn:de:kobv:b4-200905198526", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das that der grosse Geist, der mehr als tausend n\u00fctzet,", "tokens": ["Das", "that", "der", "gros\u00b7se", "Geist", ",", "der", "mehr", "als", "tau\u00b7send", "n\u00fct\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PIS", "KOKOM", "CARD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Tr-ll-r, dessen Kiel die Ehre Deutschlands st\u00fctzet,", "tokens": ["Mein", "Tr\u00b7ll\u00b7\u00b7r", ",", "des\u00b7sen", "Kiel", "die", "Eh\u00b7re", "Deutschlands", "st\u00fct\u00b7zet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELAT", "NN", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dem einst noch Gronov, Clerc, Gr\u00e4v u. Salmasius,", "tokens": ["Dem", "einst", "noch", "Gro\u00b7nov", ",", "Clerc", ",", "Gr\u00e4v", "u.", "Sal\u00b7ma\u00b7sius", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "abbreviation", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "NE", "$,", "NE", "$,", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ja Grot' und Scaliger den Vortritt g\u00f6nnen mu\u00df.", "tokens": ["Ja", "Grot'", "und", "Sca\u00b7li\u00b7ger", "den", "Vor\u00b7tritt", "g\u00f6n\u00b7nen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "KON", "NE", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}