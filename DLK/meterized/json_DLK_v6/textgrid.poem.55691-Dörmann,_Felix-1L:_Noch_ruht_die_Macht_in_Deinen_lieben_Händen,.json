{"textgrid.poem.55691": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Noch ruht die Macht in Deinen lieben H\u00e4nden,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch ruht die Macht in Deinen lieben H\u00e4nden,", "tokens": ["Noch", "ruht", "die", "Macht", "in", "Dei\u00b7nen", "lie\u00b7ben", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Herzens wildes Toben einzud\u00e4mmen,", "tokens": ["Des", "Her\u00b7zens", "wil\u00b7des", "To\u00b7ben", "ein\u00b7zu\u00b7d\u00e4m\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Zweifels Bohren kinderleicht zu hemmen \u2013", "tokens": ["Des", "Zwei\u00b7fels", "Boh\u00b7ren", "kin\u00b7der\u00b7leicht", "zu", "hem\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Sehnsuchtsqualen, dunkle, zu beenden.", "tokens": ["Und", "Sehn\u00b7suchts\u00b7qua\u00b7len", ",", "dunk\u00b7le", ",", "zu", "be\u00b7en\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJA", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich aber f\u00fchle schon die Stunde kommen,", "tokens": ["Ich", "a\u00b7ber", "f\u00fch\u00b7le", "schon", "die", "Stun\u00b7de", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo machtlos wird Dein liebevolles M\u00fchen,", "tokens": ["Wo", "macht\u00b7los", "wird", "Dein", "lie\u00b7be\u00b7vol\u00b7les", "M\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo f\u00fcr des Herzens wild und wildres Gl\u00fchen", "tokens": ["Wo", "f\u00fcr", "des", "Her\u00b7zens", "wild", "und", "wild\u00b7res", "Gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJD", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kein sittigsanfter H\u00e4ndedruck will frommen.", "tokens": ["Kein", "sit\u00b7tigs\u00b7anf\u00b7ter", "H\u00e4n\u00b7de\u00b7druck", "will", "from\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich w\u00fcrde l\u00e4ngst in jener Stunde Ahnen", "tokens": ["Ich", "w\u00fcr\u00b7de", "l\u00e4ngst", "in", "je\u00b7ner", "Stun\u00b7de", "Ah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr immerdar von Dir die Schritte wenden", "tokens": ["F\u00fcr", "im\u00b7mer\u00b7dar", "von", "Dir", "die", "Schrit\u00b7te", "wen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "APPR", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wieder wandern windumbrauste Bahnen,", "tokens": ["Und", "wie\u00b7der", "wan\u00b7dern", "win\u00b7dum\u00b7braus\u00b7te", "Bah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "F\u00e4nd' ich die Kraft zu l\u00f6sen und zu enden,", "tokens": ["F\u00e4nd'", "ich", "die", "Kraft", "zu", "l\u00f6\u00b7sen", "und", "zu", "en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und w\u00fcrde nicht mein Herz best\u00e4ndig mahnen,", "tokens": ["Und", "w\u00fcr\u00b7de", "nicht", "mein", "Herz", "be\u00b7st\u00e4n\u00b7dig", "mah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df Dich verlassen hei\u00dft: den Tod Dir senden.", "tokens": ["Da\u00df", "Dich", "ver\u00b7las\u00b7sen", "hei\u00dft", ":", "den", "Tod", "Dir", "sen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VVFIN", "$.", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}