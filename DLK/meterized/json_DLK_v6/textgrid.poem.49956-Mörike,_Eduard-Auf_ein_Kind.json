{"textgrid.poem.49956": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Auf ein Kind", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Kind, in welchem Krieg hast du", "tokens": ["Mein", "Kind", ",", "in", "wel\u00b7chem", "Krieg", "hast", "du"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PWAT", "NN", "VAFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die gelben Haare lassen m\u00fcssen?", "tokens": ["Die", "gel\u00b7ben", "Haa\u00b7re", "las\u00b7sen", "m\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Rosenzweig hat sie im Sprunge dir entrissen!", "tokens": ["Ein", "Ro\u00b7sen\u00b7zweig", "hat", "sie", "im", "Sprun\u00b7ge", "dir", "ent\u00b7ris\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du wei\u00dft es kaum und lachst dazu.", "tokens": ["Du", "wei\u00dft", "es", "kaum", "und", "lachst", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gott gebe, da\u00df in k\u00fcnftger Zeit", "tokens": ["Gott", "ge\u00b7be", ",", "da\u00df", "in", "k\u00fcnft\u00b7ger", "Zeit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Nie kein Verlust, noch ander Leid", "tokens": ["Nie", "kein", "Ver\u00b7lust", ",", "noch", "an\u00b7der", "Leid"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "$,", "ADV", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dich bitterer im jungen Herzen", "tokens": ["Dich", "bit\u00b7te\u00b7rer", "im", "jun\u00b7gen", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als dieser leichte Raub mag schmerzen!", "tokens": ["Als", "die\u00b7ser", "leich\u00b7te", "Raub", "mag", "schmer\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}