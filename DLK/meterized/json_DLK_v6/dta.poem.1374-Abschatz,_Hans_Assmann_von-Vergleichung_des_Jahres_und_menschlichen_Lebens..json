{"dta.poem.1374": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vergleichung des Jahres und menschlichen  \n Lebens.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Der Winter ist hin/ die Blumen bezieren", "tokens": ["Der", "Win\u00b7ter", "ist", "hin", "/", "die", "Blu\u00b7men", "be\u00b7zie\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "$(", "ART", "NN", "VVINF"], "meter": "-+-++-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "H\u00fcgel/ Gr\u00fcnde!", "tokens": ["H\u00fc\u00b7gel", "/", "Gr\u00fcn\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$(", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Sanffte Winde", "tokens": ["Sanff\u00b7te", "Win\u00b7de"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Durch bisamte L\u00fcffte sind itzo zu sp\u00fcren.", "tokens": ["Durch", "bi\u00b7sam\u00b7te", "L\u00fcff\u00b7te", "sind", "it\u00b7zo", "zu", "sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Mit Diamanten", "tokens": ["Mit", "Di\u00b7a\u00b7man\u00b7ten"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Des nassen Zolls bem\u00fchn sich einzustellen", "tokens": ["Des", "nas\u00b7sen", "Zolls", "be\u00b7m\u00fchn", "sich", "ein\u00b7zu\u00b7stel\u00b7len"], "token_info": ["word", "word", 