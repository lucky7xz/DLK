, "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "So wird er Staub/ der Geist als Schatten schwebet.", "tokens": ["So", "wird", "er", "Staub", "/", "der", "Geist", "als", "Schat\u00b7ten", "schwe\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$(", "ART", "NN", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Er liegt im Grab/ als h\u00e4tt\u2019 er nie gelebet.", "tokens": ["Er", "liegt", "im", "Grab", "/", "als", "h\u00e4tt'", "er", "nie", "ge\u00b7le\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$(", "KOKOM", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Lentz ist hin/ man f\u00fchlet nicht spielen", "tokens": ["Der", "Lentz", "ist", "hin", "/", "man", "f\u00fch\u00b7let", "nicht", "spie\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "$(", "PIS", "VVFIN", "PTKNEG", "VVINF"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "K\u00fchle L\u00fcffte:", "tokens": ["K\u00fch\u00b7le", "L\u00fcff\u00b7te", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Heisse D\u00fcffte", "tokens": ["Heis\u00b7se", "D\u00fcff\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Mit brennenden D\u00fcnsten beschweren im schw\u00fclen.", "tokens": ["Mit", "bren\u00b7nen\u00b7den", "D\u00fcns\u00b7ten", "be\u00b7schwe\u00b7ren", "im", "schw\u00fc\u00b7len", "."], "token_info": ["word", "word", "word", "word",