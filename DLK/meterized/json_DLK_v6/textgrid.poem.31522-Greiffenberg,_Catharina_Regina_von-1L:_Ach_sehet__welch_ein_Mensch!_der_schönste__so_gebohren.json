{"textgrid.poem.31522": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach sehet / welch ein Mensch! der sch\u00f6nste / so gebohren", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach sehet / welch ein Mensch! der sch\u00f6nste / so gebohren", "tokens": ["Ach", "se\u00b7het", "/", "welch", "ein", "Mensch", "!", "der", "sch\u00f6ns\u00b7te", "/", "so", "ge\u00b7boh\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "VVFIN", "$(", "PWAT", "ART", "NN", "$.", "ART", "ADJA", "$(", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "in und auch vor der Zeit / des Vatters Herzen-Lust!", "tokens": ["in", "und", "auch", "vor", "der", "Zeit", "/", "des", "Vat\u00b7ters", "Her\u00b7zen\u00b7Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "ADV", "APPR", "ART", "NN", "$(", "ART", "NN", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "der Engel Wunder-Zweck / vor unsre S\u00fcnde must", "tokens": ["der", "En\u00b7gel", "Wun\u00b7der\u00b7Zweck", "/", "vor", "uns\u00b7re", "S\u00fcn\u00b7de", "must"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$(", "APPR", "PPOSAT", "NN", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "der Schmerzen Schie\u00dfziel seyn / damit wir unverlohren.", "tokens": ["der", "Schmer\u00b7zen", "Schie\u00df\u00b7ziel", "seyn", "/", "da\u00b7mit", "wir", "un\u00b7ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAINF", "$(", "KOUS", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Gott-vereinigt Leib zum Abgrund wird erkohren /", "tokens": ["Der", "Got\u00b7tver\u00b7ei\u00b7nigt", "Leib", "zum", "Ab\u00b7grund", "wird", "er\u00b7koh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "wo alle Qual hinrinn / von unsern S\u00fcnden-Wust.", "tokens": ["wo", "al\u00b7le", "Qual", "hin\u00b7rinn", "/", "von", "un\u00b7sern", "S\u00fcn\u00b7den\u00b7Wust", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADV", "$(", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es bricht / ob diesen Platz / des Zornes Wolkenbrust /", "tokens": ["Es", "bricht", "/", "ob", "die\u00b7sen", "Platz", "/", "des", "Zor\u00b7nes", "Wol\u00b7ken\u00b7brust", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PDAT", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "das ganze S\u00fcnden Heer sticht ihm mit seinen Sporen.", "tokens": ["das", "gan\u00b7ze", "S\u00fcn\u00b7den", "Heer", "sticht", "ihm", "mit", "sei\u00b7nen", "Spo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach Edler Aethna du! du flammest lauter Liebe /", "tokens": ["Ach", "Ed\u00b7ler", "A\u00b7e\u00b7th\u00b7na", "du", "!", "du", "flam\u00b7mest", "lau\u00b7ter", "Lie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "NE", "$.", "PPER", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "ein jedes Tr\u00f6pflein ist ein Lieb-entgl\u00fchte Kohl.", "tokens": ["ein", "je\u00b7des", "Tr\u00f6pf\u00b7lein", "ist", "ein", "Lie\u00b7bent\u00b7gl\u00fch\u00b7te", "Kohl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ach! da\u00df ich nicht mein Herz in deine Striemen schiebe /", "tokens": ["Ach", "!", "da\u00df", "ich", "nicht", "mein", "Herz", "in", "dei\u00b7ne", "Strie\u00b7men", "schie\u00b7be", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "PPER", "PTKNEG", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "und einen Gold-Geist mir aus deinen Wunden hol.", "tokens": ["und", "ei\u00b7nen", "Gold\u00b7Geist", "mir", "aus", "dei\u00b7nen", "Wun\u00b7den", "hol", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Seht Wunder! welch ein Mensch / der Gott und uns vers\u00fchnet /", "tokens": ["Seht", "Wun\u00b7der", "!", "welch", "ein", "Mensch", "/", "der", "Gott", "und", "uns", "ver\u00b7s\u00fch\u00b7net", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$.", "PWAT", "ART", "NN", "$(", "ART", "NN", "KON", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "mit h\u00f6chster Schmerzen-Schmach uns Himmels-Ehr verdienet!", "tokens": ["mit", "h\u00f6chs\u00b7ter", "Schmer\u00b7zen\u00b7Schmach", "uns", "Him\u00b7mels\u00b7Ehr", "ver\u00b7die\u00b7net", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}