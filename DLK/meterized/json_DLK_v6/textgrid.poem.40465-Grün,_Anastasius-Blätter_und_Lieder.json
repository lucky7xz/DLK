{"textgrid.poem.40465": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Bl\u00e4tter und Lieder", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fchling ist's in allen R\u00e4umen!", "tokens": ["Fr\u00fch\u00b7ling", "ist's", "in", "al\u00b7len", "R\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00fcth' und Blume taucht empor,", "tokens": ["Bl\u00fcth'", "und", "Blu\u00b7me", "taucht", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und aus Standen und aus B\u00e4umen", "tokens": ["Und", "aus", "Stan\u00b7den", "und", "aus", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sprie\u00dfen Bl\u00e4tter gr\u00fcn hervor.", "tokens": ["Sprie\u00b7\u00dfen", "Bl\u00e4t\u00b7ter", "gr\u00fcn", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jugend bl\u00fcht auf meiner Wange,", "tokens": ["Ju\u00b7gend", "bl\u00fcht", "auf", "mei\u00b7ner", "Wan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jugend gl\u00fcht in meiner Brust;", "tokens": ["Ju\u00b7gend", "gl\u00fcht", "in", "mei\u00b7ner", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bl\u00e4ttern gleich im Fr\u00fchlingsdrange", "tokens": ["Bl\u00e4t\u00b7tern", "gleich", "im", "Fr\u00fch\u00b7lings\u00b7dran\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bl\u00fchn mir Lieder aus der Brust.", "tokens": ["Bl\u00fchn", "mir", "Lie\u00b7der", "aus", "der", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Bl\u00e4tter sangen aus der Erde", "tokens": ["Bl\u00e4t\u00b7ter", "san\u00b7gen", "aus", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leben, Farbe, Glanz und Saft,", "tokens": ["Le\u00b7ben", ",", "Far\u00b7be", ",", "Glanz", "und", "Saft", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Flattern wieder zu der Erde,", "tokens": ["Flat\u00b7tern", "wie\u00b7der", "zu", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn sie knickt des Sturmes Kraft.", "tokens": ["Wenn", "sie", "knickt", "des", "Stur\u00b7mes", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aus der Lieb' erbl\u00fchen Lieder,", "tokens": ["Aus", "der", "Lieb'", "er\u00b7bl\u00fc\u00b7hen", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00fchn und sprossen auf zum Licht,", "tokens": ["Bl\u00fchn", "und", "spros\u00b7sen", "auf", "zum", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fl\u00fcchten zu der Liebe wieder,", "tokens": ["Fl\u00fcch\u00b7ten", "zu", "der", "Lie\u00b7be", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn der Zeiten Arm sie bricht.", "tokens": ["Wenn", "der", "Zei\u00b7ten", "Arm", "sie", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn ein neuer Lenztag blinket,", "tokens": ["Wenn", "ein", "neu\u00b7er", "Lenz\u00b7tag", "blin\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00fchn die Bl\u00e4tter wieder auf,", "tokens": ["Bl\u00fchn", "die", "Bl\u00e4t\u00b7ter", "wie\u00b7der", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wenn neue Liebe winket,", "tokens": ["Und", "wenn", "neu\u00b7e", "Lie\u00b7be", "win\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leben neu die Lieder auf.", "tokens": ["Le\u00b7ben", "neu", "die", "Lie\u00b7der", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}