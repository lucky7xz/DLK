{"textgrid.poem.37001": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mathilde!", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mathilde!", "tokens": ["Ma\u00b7thil\u00b7de", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Du, dieser Ego-Gilde", "tokens": ["Du", ",", "die\u00b7ser", "E\u00b7go\u00b7Gil\u00b7de"], "token_info": ["word", "punct", "word", "word"], "pos": ["PPER", "$,", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Liebreizendstes Gebilde", "tokens": ["Lieb\u00b7rei\u00b7zends\u00b7tes", "Ge\u00b7bil\u00b7de"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Mit Engelshuld und Milde:", "tokens": ["Mit", "En\u00b7gels\u00b7huld", "und", "Mil\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Trotz Deiner Tugend Schilde,", "tokens": ["Trotz", "Dei\u00b7ner", "Tu\u00b7gend", "Schil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Warf Dich der Tod, der wilde,", "tokens": ["Warf", "Dich", "der", "Tod", ",", "der", "wil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "In dieses Schmerzgefilde!", "tokens": ["In", "die\u00b7ses", "Schmerz\u00b7ge\u00b7fil\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dein Grab ist mein's:", "tokens": ["Dein", "Grab", "ist", "mein's", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIS", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Mathilde!", "tokens": ["Ma\u00b7thil\u00b7de", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}