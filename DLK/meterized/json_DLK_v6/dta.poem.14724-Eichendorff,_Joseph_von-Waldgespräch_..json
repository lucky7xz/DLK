{"dta.poem.14724": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Waldgespr\u00e4ch .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "\u201egro\u00df ist der M\u00e4nner Trug und List,", "tokens": ["\u201e", "gro\u00df", "ist", "der", "M\u00e4n\u00b7ner", "Trug", "und", "List", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor Schmerz mein Herz gebrochen ist,", "tokens": ["Vor", "Schmerz", "mein", "Herz", "ge\u00b7bro\u00b7chen", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl irrt das Waldhorn her und hin,", "tokens": ["Wohl", "irrt", "das", "Wald\u00b7horn", "her", "und", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O flieh! Du wei\u00dft nicht, wer ich bin.\u201c", "tokens": ["O", "flieh", "!", "Du", "wei\u00dft", "nicht", ",", "wer", "ich", "bin", ".", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "$.", "PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VAFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}