{"textgrid.poem.43167": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie kniet mit verschleiertem Antlitz", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie kniet mit verschleiertem Antlitz", "tokens": ["Sie", "kniet", "mit", "ver\u00b7schlei\u00b7er\u00b7tem", "Ant\u00b7litz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "In der Kirche am Altar,", "tokens": ["In", "der", "Kir\u00b7che", "am", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Erz\u00e4hlt dem geduld'gen Herrgott,", "tokens": ["Er\u00b7z\u00e4hlt", "dem", "ge\u00b7duld'\u00b7gen", "Herr\u00b7gott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wie tugendhaft sie war:", "tokens": ["Wie", "tu\u00b7gend\u00b7haft", "sie", "war", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fcr seine Krieger gesammelt", "tokens": ["F\u00fcr", "sei\u00b7ne", "Krie\u00b7ger", "ge\u00b7sam\u00b7melt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hat sie an der Kirchenth\u00fcr,", "tokens": ["Hat", "sie", "an", "der", "Kir\u00b7chent\u00b7h\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Manch' schlanken J\u00fcngling geworben \u2013", "tokens": ["Man\u00b7ch'", "schlan\u00b7ken", "J\u00fcng\u00b7ling", "ge\u00b7wor\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVPP", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und wirbt noch f\u00fcr und f\u00fcr.", "tokens": ["Und", "wirbt", "noch", "f\u00fcr", "und", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}