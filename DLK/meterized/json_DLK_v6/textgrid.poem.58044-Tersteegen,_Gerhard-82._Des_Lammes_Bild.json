{"textgrid.poem.58044": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "82. Des Lammes Bild", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, w\u00e4r' ich wie ein Lamm unschuldig,", "tokens": ["Ach", ",", "w\u00e4r'", "ich", "wie", "ein", "Lamm", "un\u00b7schul\u00b7dig", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "KOKOM", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sanft, rein, einf\u00e4ltig und geduldig!", "tokens": ["Sanft", ",", "rein", ",", "ein\u00b7f\u00e4l\u00b7tig", "und", "ge\u00b7dul\u00b7dig", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ach, m\u00f6cht' man Christi Bild und Wesen", "tokens": ["Ach", ",", "m\u00f6cht'", "man", "Chris\u00b7ti", "Bild", "und", "We\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VMFIN", "PIS", "NE", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus meinem ganzen Wandel lesen!", "tokens": ["Aus", "mei\u00b7nem", "gan\u00b7zen", "Wan\u00b7del", "le\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}