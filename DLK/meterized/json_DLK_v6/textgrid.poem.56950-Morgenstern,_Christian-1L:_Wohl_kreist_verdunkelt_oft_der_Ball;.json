{"textgrid.poem.56950": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohl kreist verdunkelt oft der Ball;", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl kreist verdunkelt oft der Ball;", "tokens": ["Wohl", "kreist", "ver\u00b7dun\u00b7kelt", "oft", "der", "Ball", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "doch \u00fcber den paar Wolken droben,", "tokens": ["doch", "\u00fc\u00b7ber", "den", "paar", "Wol\u00b7ken", "dro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "da blaut das sterndurchtanzte All", "tokens": ["da", "blaut", "das", "stern\u00b7durch\u00b7tanz\u00b7te", "All"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und l\u00e4\u00dft sich von den G\u00f6ttern loben.", "tokens": ["und", "l\u00e4\u00dft", "sich", "von", "den", "G\u00f6t\u00b7tern", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die liegen auf den Wolkenbergen,", "tokens": ["Die", "lie\u00b7gen", "auf", "den", "Wol\u00b7ken\u00b7ber\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "wie Hirten einer Fabelwelt,", "tokens": ["wie", "Hir\u00b7ten", "ei\u00b7ner", "Fa\u00b7bel\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und wissen kaum von all den Zwergen,", "tokens": ["und", "wis\u00b7sen", "kaum", "von", "all", "den", "Zwer\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "die das Gebirg im Scho\u00dfe h\u00e4lt.", "tokens": ["die", "das", "Ge\u00b7birg", "im", "Scho\u00b7\u00dfe", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie lachen mit den wei\u00dfen Z\u00e4hnen", "tokens": ["Sie", "la\u00b7chen", "mit", "den", "wei\u00b7\u00dfen", "Z\u00e4h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "den G\u00f6ttern andrer Sterne zu \u2013.", "tokens": ["den", "G\u00f6t\u00b7tern", "an\u00b7drer", "Ster\u00b7ne", "zu", "\u2013", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKZU", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Komm, Bruder, la\u00df die leeren Tr\u00e4nen,", "tokens": ["Komm", ",", "Bru\u00b7der", ",", "la\u00df", "die", "lee\u00b7ren", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVIMP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "wir sind auch G\u00f6tter, ich und du!", "tokens": ["wir", "sind", "auch", "G\u00f6t\u00b7ter", ",", "ich", "und", "du", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$,", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}