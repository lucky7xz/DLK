"+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Deines Liedes Ton.", "tokens": ["Dei\u00b7nes", "Lie\u00b7des", "Ton", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Hast mit deinen Lippen", "tokens": ["Hast", "mit", "dei\u00b7nen", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Mir es angetan;", "tokens": ["Mir", "es", "an\u00b7ge\u00b7tan", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Selig in die Klippen", "tokens": ["Se\u00b7lig", "in", "die", "Klip\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Steur' ich meinen Kahn.", "tokens": ["Steur'", "ich", "mei\u00b7nen", "Kahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}