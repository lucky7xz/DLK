{"textgrid.poem.59740": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Gottes Geist und Christi Geist,", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Gottes Geist und Christi Geist,", "tokens": ["O", "Got\u00b7tes", "Geist", "und", "Chris\u00b7ti", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NN", "KON", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der uns den Weg zum Himmel weist,", "tokens": ["Der", "uns", "den", "Weg", "zum", "Him\u00b7mel", "weist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der uns die dunkle Erdennacht", "tokens": ["Der", "uns", "die", "dunk\u00b7le", "Er\u00b7den\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch seine Lichter helle macht.", "tokens": ["Durch", "sei\u00b7ne", "Lich\u00b7ter", "hel\u00b7le", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du Hauch, der durch das Weltall weht", "tokens": ["Du", "Hauch", ",", "der", "durch", "das", "Wel\u00b7tall", "weht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als Gottes stille Majest\u00e4t,", "tokens": ["Als", "Got\u00b7tes", "stil\u00b7le", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du, aller Lichter reinstes Licht,", "tokens": ["Du", ",", "al\u00b7ler", "Lich\u00b7ter", "reins\u00b7tes", "Licht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erleucht' uns Herz und Angesicht.", "tokens": ["Er\u00b7leucht'", "uns", "Herz", "und", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Komm, leuchte mit dem Gnadenschein", "tokens": ["Komm", ",", "leuch\u00b7te", "mit", "dem", "Gna\u00b7den\u00b7schein"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hell in die weite Welt hinein,", "tokens": ["Hell", "in", "die", "wei\u00b7te", "Welt", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Komm, mach' uns in der Finsternis", "tokens": ["Komm", ",", "mach'", "uns", "in", "der", "Fins\u00b7ter\u00b7nis"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des lichten Himmelswegs gewi\u00df.", "tokens": ["Des", "lich\u00b7ten", "Him\u00b7mels\u00b7wegs", "ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach! Hier ist alles Staub und Nacht,", "tokens": ["Ach", "!", "Hier", "ist", "al\u00b7les", "Staub", "und", "Nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VAFIN", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wahn und S\u00fcnde tr\u00fcbe macht,", "tokens": ["Die", "Wahn", "und", "S\u00fcn\u00b7de", "tr\u00fc\u00b7be", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach! Hier ist alles Not und Tod,", "tokens": ["Ach", "!", "Hier", "ist", "al\u00b7les", "Not", "und", "Tod", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VAFIN", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geht uns nicht auf dein Morgenrot.", "tokens": ["Geht", "uns", "nicht", "auf", "dein", "Mor\u00b7gen\u00b7rot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Das Morgenrot der bessern Welt,", "tokens": ["Das", "Mor\u00b7gen\u00b7rot", "der", "bes\u00b7sern", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das wie ein Strahl vom Himmel f\u00e4llt,", "tokens": ["Das", "wie", "ein", "Strahl", "vom", "Him\u00b7mel", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KOKOM", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als Gottes Macht und Gottes Lust", "tokens": ["Als", "Got\u00b7tes", "Macht", "und", "Got\u00b7tes", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "NN", "KON", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durchblitzt die kranke Menschenbrust.", "tokens": ["Durch\u00b7blitzt", "die", "kran\u00b7ke", "Men\u00b7schen\u00b7brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O Gottes Geist und Christi Geist,", "tokens": ["O", "Got\u00b7tes", "Geist", "und", "Chris\u00b7ti", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NN", "KON", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der uns wie Kinder beten hei\u00dft,", "tokens": ["Der", "uns", "wie", "Kin\u00b7der", "be\u00b7ten", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOKOM", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der uns wie Kinder glauben hei\u00dft,", "tokens": ["Der", "uns", "wie", "Kin\u00b7der", "glau\u00b7ben", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOKOM", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O komm! o komm, du Heil'ger Geist!", "tokens": ["O", "komm", "!", "o", "komm", ",", "du", "Heil'\u00b7ger", "Geist", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "FM", "VVFIN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Komm, Gottes Frieden, Gottes Mut!", "tokens": ["Komm", ",", "Got\u00b7tes", "Frie\u00b7den", ",", "Got\u00b7tes", "Mut", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "NN", "$,", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Komm, stille Kraft, die nimmer ruht!", "tokens": ["Komm", ",", "stil\u00b7le", "Kraft", ",", "die", "nim\u00b7mer", "ruht", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Komm, gie\u00dfe deinen Gnadenschein", "tokens": ["Komm", ",", "gie\u00b7\u00dfe", "dei\u00b7nen", "Gna\u00b7den\u00b7schein"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Seele, Sinn und Herz mir ein.", "tokens": ["In", "See\u00b7le", ",", "Sinn", "und", "Herz", "mir", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Dann wandl' ich wie ein Kind des Lichts", "tokens": ["Dann", "wandl'", "ich", "wie", "ein", "Kind", "des", "Lichts"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Glanze deines Angesichts", "tokens": ["Im", "Glan\u00b7ze", "dei\u00b7nes", "An\u00b7ge\u00b7sichts"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schon meinen kurzen Erdenlauf", "tokens": ["Schon", "mei\u00b7nen", "kur\u00b7zen", "Er\u00b7den\u00b7lauf"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Stets himmelein und himmelauf.", "tokens": ["Stets", "him\u00b7mel\u00b7ein", "und", "him\u00b7me\u00b7lauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}