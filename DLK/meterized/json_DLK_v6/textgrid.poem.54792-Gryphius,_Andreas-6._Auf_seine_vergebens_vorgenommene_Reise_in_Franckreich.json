{"textgrid.poem.54792": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "6. Auf seine vergebens vorgenommene Reise in Franckreich", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Francken freyes Reich hast du zu sehn begehrt:", "tokens": ["Der", "Fran\u00b7cken", "frey\u00b7es", "Reich", "hast", "du", "zu", "sehn", "be\u00b7gehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "PPER", "PTKZU", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Umbsonst! es ist mit Dinst und hartem Joch beschwert.", "tokens": ["Um\u00b7bsonst", "!", "es", "ist", "mit", "Dinst", "und", "har\u00b7tem", "Joch", "be\u00b7schwert", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VAFIN", "APPR", "NN", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Freyheit die der Welt l\u00e4ngst gutte Nacht gesagt/", "tokens": ["Die", "Frey\u00b7heit", "die", "der", "Welt", "l\u00e4ngst", "gut\u00b7te", "Nacht", "ge\u00b7sagt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ART", "NN", "ADV", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hat dich/ den sie gelibt/ in ihrem Hoff betagt.", "tokens": ["Hat", "dich", "/", "den", "sie", "ge\u00b7libt", "/", "in", "ih\u00b7rem", "Hoff", "be\u00b7tagt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$(", "ART", "PPER