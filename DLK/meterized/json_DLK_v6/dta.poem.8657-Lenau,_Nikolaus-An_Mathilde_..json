{"dta.poem.8657": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "An Mathilde .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Schon verrauscht der Tag, und des Abends sanftere Seele", "tokens": ["Schon", "ver\u00b7rauscht", "der", "Tag", ",", "und", "des", "A\u00b7bends", "sanf\u00b7te\u00b7re", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "ART", "NN", "ADJA", "NN"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Flie\u00dft wie s\u00fc\u00dfe Musik s\u00e4nftigend uns in die Brust.", "tokens": ["Flie\u00dft", "wie", "s\u00fc\u00b7\u00dfe", "Mu\u00b7sik", "s\u00e4nf\u00b7ti\u00b7gend", "uns", "in", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ADJA", "NN", "VVPP", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+---+-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Horch, Mathilde, wie leise der West durch Bl\u00fcthen da-", "tokens": ["Horch", ",", "Ma\u00b7thil\u00b7de", ",", "wie", "lei\u00b7se", "der", "West", "durch", "Bl\u00fc\u00b7then", "da"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "NE", "$,", "PWAV", "ADJD", "ART", "NN", "APPR", "NN", "TRUNC"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "hinseufzt,", "tokens": ["hin\u00b7seufzt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Leiser noch weht sein Hauch, kost er um deine Gestalt.", "tokens": ["Lei\u00b7ser", "noch", "weht", "sein", "Hauch", ",", "kost", "er", "um", "dei\u00b7ne", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Sieh, die Biene, sie wandelt von Blume zu Blume gesch\u00e4ftig,", "tokens": ["Sieh", ",", "die", "Bie\u00b7ne", ",", "sie", "wan\u00b7delt", "von", "Blu\u00b7me", "zu", "Blu\u00b7me", "ge\u00b7sch\u00e4f\u00b7tig", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,", "PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.7": {"text": "S\u00fc\u00dfe Bereicherung lockt weiter die Summende stets;", "tokens": ["S\u00fc\u00b7\u00dfe", "Be\u00b7rei\u00b7che\u00b7rung", "lockt", "wei\u00b7ter", "die", "Sum\u00b7men\u00b7de", "stets", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "ADV", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Also wandelt die Seele dereinst von Blume zu Blume,", "tokens": ["Al\u00b7so", "wan\u00b7delt", "die", "See\u00b7le", "de\u00b7reinst", "von", "Blu\u00b7me", "zu", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "NN", "APPR", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.9": {"text": "Welche zum strahlenden Kranz sich der Unendliche wand,", "tokens": ["Wel\u00b7che", "zum", "strah\u00b7len\u00b7den", "Kranz", "sich", "der", "Un\u00b7end\u00b7li\u00b7che", "wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPRART", "ADJA", "NN", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.10": {"text": "Wandelt die Seele dereinst von Welten weiter zu Welten,", "tokens": ["Wan\u00b7delt", "die", "See\u00b7le", "de\u00b7reinst", "von", "Wel\u00b7ten", "wei\u00b7ter", "zu", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPR", "NN", "ADV", "APPR", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.11": {"text": "N\u00e4her dem liebenden Gott, liebender, g\u00f6ttlicher stets.", "tokens": ["N\u00e4\u00b7her", "dem", "lie\u00b7ben\u00b7den", "Gott", ",", "lie\u00b7ben\u00b7der", ",", "g\u00f6tt\u00b7li\u00b7cher", "stets", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "ADJA", "$,", "ADJA", "ADV", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Aber die Wechselgestalten des Lebens, sie theilen nicht alle", "tokens": ["A\u00b7ber", "die", "Wech\u00b7sel\u00b7ge\u00b7stal\u00b7ten", "des", "Le\u00b7bens", ",", "sie", "thei\u00b7len", "nicht", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "PIAT"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.13": {"text": "Gleich der Unsterblichkeit Loos, wenn uns der Ewige winkt;", "tokens": ["Gleich", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", "Loos", ",", "wenn", "uns", "der", "E\u00b7wi\u00b7ge", "winkt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NE", "$,", "KOUS", "PPER", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+---+-+-+--+", "measure": "iambic.hexa.chol"}, "line.14": {"text": "Nur das Sch\u00f6nste des Lebens, worin der Himmel uns", "tokens": ["Nur", "das", "Sch\u00f6ns\u00b7te", "des", "Le\u00b7bens", ",", "wo\u00b7rin", "der", "Him\u00b7mel", "uns"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$,", "PWAV", "ART", "NN", "PPER"], "meter": "+-+-+++-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.15": {"text": "kund wird,", "tokens": ["kund", "wird", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKVZ", "VAFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.16": {"text": "Nimmt die Seele mit fort, schwingt sie den Sternen", "tokens": ["Nimmt", "die", "See\u00b7le", "mit", "fort", ",", "schwingt", "sie", "den", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "PTKVZ", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "sich zu.", "tokens": ["sich", "zu", "."], "token_info": ["word", "word", "punct"], "pos": ["PRF", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.18": {"text": "Doch die tr\u00fcben Gestalten verh\u00fcllt Nacht, ewige Nacht", "tokens": ["Doch", "die", "tr\u00fc\u00b7ben", "Ge\u00b7stal\u00b7ten", "ver\u00b7h\u00fcllt", "Nacht", ",", "e\u00b7wi\u00b7ge", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "NN", "$,", "ADJA", "NN"], "meter": "--+--+--+-+--+", "measure": "anapaest.tri.plus"}, "line.19": {"text": "dann.", "tokens": ["dann", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-", "measure": "single.down"}, "line.20": {"text": "Heil der Stunde, die selbst dann noch uns wonnig", "tokens": ["Heil", "der", "Stun\u00b7de", ",", "die", "selbst", "dann", "noch", "uns", "won\u00b7nig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "ADV", "ADV", "ADV", "PPER", "ADJD"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.21": {"text": "umstrahlt!", "tokens": ["um\u00b7strahlt", "!"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.22": {"text": "O Mathilde, dein Auge voll himmlischer, tiefer Bedeutung,", "tokens": ["O", "Ma\u00b7thil\u00b7de", ",", "dein", "Au\u00b7ge", "voll", "himm\u00b7li\u00b7scher", ",", "tie\u00b7fer", "Be\u00b7deu\u00b7tung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPOSAT", "NN", "ADJD", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.23": {"text": "Blickt mir ins Auge so ernst, und so entz\u00fcckend zugleich,", "tokens": ["Blickt", "mir", "ins", "Au\u00b7ge", "so", "ernst", ",", "und", "so", "ent\u00b7z\u00fc\u00b7ckend", "zu\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV", "ADJD", "$,", "KON", "ADV", "ADJD", "ADV", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.24": {"text": "Da\u00df die Seele mir bebt, o Geliebte! ahnet dir etwa,", "tokens": ["Da\u00df", "die", "See\u00b7le", "mir", "bebt", ",", "o", "Ge\u00b7lieb\u00b7te", "!", "ah\u00b7net", "dir", "et\u00b7wa", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,", "FM", "NN", "$.", "VVFIN", "PPER", "ADV", "$,"], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.25": {"text": "Da\u00df auch diesen Moment h\u00fcllen nicht werde die Nacht?", "tokens": ["Da\u00df", "auch", "die\u00b7sen", "Mo\u00b7ment", "h\u00fcl\u00b7len", "nicht", "wer\u00b7de", "die", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PDAT", "NN", "VVFIN", "PTKNEG", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}}}}