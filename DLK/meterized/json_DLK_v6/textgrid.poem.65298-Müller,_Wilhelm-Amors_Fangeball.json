{"textgrid.poem.65298": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Amors Fangeball", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Amor wollte Fangeb\u00e4llchen", "tokens": ["A\u00b7mor", "woll\u00b7te", "Fan\u00b7ge\u00b7b\u00e4ll\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["NE", "VMFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Neulich mit den Nymphen spielen.", "tokens": ["Neu\u00b7lich", "mit", "den", "Nym\u00b7phen", "spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese lie\u00dfen Knabenherzen,", "tokens": ["Die\u00b7se", "lie\u00b7\u00dfen", "Kna\u00b7ben\u00b7her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die in Tr\u00e4umen sie gestohlen,", "tokens": ["Die", "in", "Tr\u00e4u\u00b7men", "sie", "ge\u00b7stoh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch die L\u00fcft', als B\u00e4lle, fliegen.", "tokens": ["Durch", "die", "L\u00fcft'", ",", "als", "B\u00e4l\u00b7le", ",", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "NN", "$,", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Amor hatte nichts zu werfen;", "tokens": ["A\u00b7mor", "hat\u00b7te", "nichts", "zu", "wer\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PIS", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Alsobald sandt' er die Blicke", "tokens": ["Al\u00b7so\u00b7bald", "sandt'", "er", "die", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Durch die weiten Himmelsr\u00e4ume,", "tokens": ["Durch", "die", "wei\u00b7ten", "Him\u00b7mels\u00b7r\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und das Erste, was er sahe,", "tokens": ["Und", "das", "Ers\u00b7te", ",", "was", "er", "sa\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "War der Weltkreis, welcher ruhte", "tokens": ["War", "der", "Welt\u00b7kreis", ",", "wel\u00b7cher", "ruh\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "In des G\u00f6tterk\u00f6nigs Rechten.", "tokens": ["In", "des", "G\u00f6t\u00b7ter\u00b7k\u00f6\u00b7nigs", "Rech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Amor zielt' und traf die Kugel", "tokens": ["A\u00b7mor", "zielt'", "und", "traf", "die", "Ku\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Grade durch die beiden Pole,", "tokens": ["Gra\u00b7de", "durch", "die", "bei\u00b7den", "Po\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Da\u00df sie flugs vom hohen \u00c4ther", "tokens": ["Da\u00df", "sie", "flugs", "vom", "ho\u00b7hen", "\u00c4\u00b7ther"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Niederfiel zu seinen F\u00fc\u00dfen.", "tokens": ["Nie\u00b7der\u00b7fiel", "zu", "sei\u00b7nen", "F\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Jetzt, ihr Nymphen, kann er spielen!", "tokens": ["Jetzt", ",", "ihr", "Nym\u00b7phen", ",", "kann", "er", "spie\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}