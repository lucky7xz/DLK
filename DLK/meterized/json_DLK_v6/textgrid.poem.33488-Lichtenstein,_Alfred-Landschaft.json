{"textgrid.poem.33488": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Landschaft", "genre": "verse", "period": "N.A.", "pub_year": 1902, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie alte Knochen liegen in dem Topf", "tokens": ["Wie", "al\u00b7te", "Kno\u00b7chen", "lie\u00b7gen", "in", "dem", "Topf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Mittags die verfluchten Stra\u00dfen da.", "tokens": ["Des", "Mit\u00b7tags", "die", "ver\u00b7fluch\u00b7ten", "Stra\u00b7\u00dfen", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Schon lange ist es her, da\u00df ich dich sah.", "tokens": ["Schon", "lan\u00b7ge", "ist", "es", "her", ",", "da\u00df", "ich", "dich", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Junge zupft ein M\u00e4dchen an dem Zopf.", "tokens": ["Ein", "Jun\u00b7ge", "zupft", "ein", "M\u00e4d\u00b7chen", "an", "dem", "Zopf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und ein paar Hunde sielen sich im Dreck.", "tokens": ["Und", "ein", "paar", "Hun\u00b7de", "sie\u00b7len", "sich", "im", "Dreck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "NN", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich ginge gerne Arm in Arm mit dir.", "tokens": ["Ich", "gin\u00b7ge", "ger\u00b7ne", "Arm", "in", "Arm", "mit", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "APPR", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Himmel ist ein graues Packpapier,", "tokens": ["Der", "Him\u00b7mel", "ist", "ein", "grau\u00b7es", "Pack\u00b7pa\u00b7pier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf dem die Sonne klebt \u2013 ein Butterfleck.", "tokens": ["Auf", "dem", "die", "Son\u00b7ne", "klebt", "\u2013", "ein", "But\u00b7ter\u00b7fleck", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$(", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}