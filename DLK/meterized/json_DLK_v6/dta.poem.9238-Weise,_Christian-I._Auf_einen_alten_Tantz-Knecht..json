{"dta.poem.9238": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "I.  \n Auf einen alten Tantz-Knecht.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Ach weh! wir armen junggesellen/", "tokens": ["Ach", "weh", "!", "wir", "ar\u00b7men", "jung\u00b7ge\u00b7sel\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$.", "PPER", "ADJA", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir werden gar zu langsam klug/", "tokens": ["Wir", "wer\u00b7den", "gar", "zu", "lang\u00b7sam", "klug", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKA", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil wir uns tumm und alber stellen/", "tokens": ["Weil", "wir", "uns", "tumm", "und", "al\u00b7ber", "stel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "KON", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So f\u00e4hrt die zeit den schnellen flug/", "tokens": ["So", "f\u00e4hrt", "die", "zeit", "den", "schnel\u00b7len", "flug", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Eh wir uns eigentlich besinnen/", "tokens": ["Eh", "wir", "uns", "ei\u00b7gent\u00b7lich", "be\u00b7sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVINF", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Mit aller lustigkeit von hinnen.", "tokens": ["Mit", "al\u00b7ler", "lus\u00b7tig\u00b7keit", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Der anfang von den jungen jahren", "tokens": ["Der", "an\u00b7fang", "von", "den", "jun\u00b7gen", "jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sol auch der liebe wachsthum seyn/", "tokens": ["Sol", "auch", "der", "lie\u00b7be", "wach\u00b7sthum", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sol das zarte volck zu paaren/", "tokens": ["Da", "sol", "das", "zar\u00b7te", "volck", "zu", "paa\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die stillen k\u00fcsse von sich streun/", "tokens": ["Die", "stil\u00b7len", "k\u00fcs\u00b7se", "von", "sich", "streun", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PRF", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da sol die junge sehnsucht spielen/", "tokens": ["Da", "sol", "die", "jun\u00b7ge", "sehn\u00b7sucht", "spie\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und flammen in der seele f\u00fchlen/", "tokens": ["Und", "flam\u00b7men", "in", "der", "see\u00b7le", "f\u00fch\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Gleich wie ein sch\u00e4fgen seiner weyde/", "tokens": ["Gleich", "wie", "ein", "sch\u00e4f\u00b7gen", "sei\u00b7ner", "wey\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Begierig nachzugehen pflegt/", "tokens": ["Be\u00b7gie\u00b7rig", "nach\u00b7zu\u00b7ge\u00b7hen", "pflegt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVIZU", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So sucht die jugend ihre freude/", "tokens": ["So", "sucht", "die", "ju\u00b7gend", "ih\u00b7re", "freu\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da wird nichts \u00fcbels ausgelegt/", "tokens": ["Da", "wird", "nichts", "\u00fc\u00b7bels", "aus\u00b7ge\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da schn\u00e4beln sich die glatten m\u00e4ulgen/", "tokens": ["Da", "schn\u00e4\u00b7beln", "sich", "die", "glat\u00b7ten", "m\u00e4ul\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und pflicken ihre mertzen-veilgen.", "tokens": ["Und", "pfli\u00b7cken", "ih\u00b7re", "mert\u00b7zen\u00b7veil\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Doch/ wann man schon an seinen wangen", "tokens": ["Doch", "/", "wann", "man", "schon", "an", "sei\u00b7nen", "wan\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$(", "PWAV", "PIS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den bart mit tausend stacheln f\u00fchlt/", "tokens": ["Den", "bart", "mit", "tau\u00b7send", "sta\u00b7cheln", "f\u00fchlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "CARD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und hat es noch nicht angefangen/", "tokens": ["Und", "hat", "es", "noch", "nicht", "an\u00b7ge\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So hat man mehr als halb verspielt/", "tokens": ["So", "hat", "man", "mehr", "als", "halb", "ver\u00b7spielt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PIAT", "KOKOM", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und will man mehr auf kurtzweil harren/", "tokens": ["Und", "will", "man", "mehr", "auf", "kurt\u00b7zweil", "har\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So wird man mehrentheils zum narren.", "tokens": ["So", "wird", "man", "meh\u00b7ren\u00b7theils", "zum", "nar\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Es heist die blumen sind vergangen/", "tokens": ["Es", "heist", "die", "blu\u00b7men", "sind", "ver\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die knospe sol zusammen stehn/", "tokens": ["Die", "knos\u00b7pe", "sol", "zu\u00b7sam\u00b7men", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VMFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn man alles kan erlangen/", "tokens": ["Und", "wenn", "man", "al\u00b7les", "kan", "er\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PIS", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So mu\u00df man ins gef\u00e4ngnis gehn/", "tokens": ["So", "mu\u00df", "man", "ins", "ge\u00b7f\u00e4ng\u00b7nis", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da sich das elend in den decken", "tokens": ["Da", "sich", "das", "e\u00b7lend", "in", "den", "de\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der eitlen liebe sol verstecken.", "tokens": ["Der", "eit\u00b7len", "lie\u00b7be", "sol", "ver\u00b7ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Wohl dem/ der seinen liebs-beruffe", "tokens": ["Wohl", "dem", "/", "der", "sei\u00b7nen", "liebs\u00b7be\u00b7ruf\u00b7fe"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "$(", "ART", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Still und gehorsam folgen lernt/", "tokens": ["Still", "und", "ge\u00b7hor\u00b7sam", "fol\u00b7gen", "lernt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Eh ihm der jugend letzte stuffe", "tokens": ["Eh", "ihm", "der", "ju\u00b7gend", "letz\u00b7te", "stuf\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Von der gelegenheit entfernt/", "tokens": ["Von", "der", "ge\u00b7le\u00b7gen\u00b7heit", "ent\u00b7fernt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da kriegt man sorgen mit den jahren/", "tokens": ["Da", "kriegt", "man", "sor\u00b7gen", "mit", "den", "jah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und mu\u00df die lust dann ewig spahren.", "tokens": ["Und", "mu\u00df", "die", "lust", "dann", "e\u00b7wig", "spah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "7. Wann ich mich hin und her besinne/", "tokens": ["Wann", "ich", "mich", "hin", "und", "her", "be\u00b7sin\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "PTKVZ", "KON", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So hab ich auch die zeit verseumt/", "tokens": ["So", "hab", "ich", "auch", "die", "zeit", "ver\u00b7se\u00b7umt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.3": {"text": "Und finde was an meinem kinne/", "tokens": ["Und", "fin\u00b7de", "was", "an", "mei\u00b7nem", "kin\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das sich zu keiner liebe reimt/", "tokens": ["Das", "sich", "zu", "kei\u00b7ner", "lie\u00b7be", "reimt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "PTKA", "PIS", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum w\u00fcnsch ich offt in diesem st\u00fccke", "tokens": ["Drum", "w\u00fcnsch", "ich", "offt", "in", "die\u00b7sem", "st\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ach h\u00e4tt ich noch zehn jahr zur\u00fccke.", "tokens": ["Ach", "h\u00e4tt", "ich", "noch", "zehn", "jahr", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "ADV", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}