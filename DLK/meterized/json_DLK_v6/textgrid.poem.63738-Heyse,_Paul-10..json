{"textgrid.poem.63738": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut da kommt mir ein fremdes Gesicht aufs Zimmer.\u00bbIch bin ein", "tokens": ["Heut", "da", "kommt", "mir", "ein", "frem\u00b7des", "Ge\u00b7sicht", "aufs", "Zim\u00b7mer", ".", "\u00bb", "Ich", "bin", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "$.", "$(", "PPER", "VAFIN", "ART"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Deutscher, verzeihn's\u00ab. \u2013 Nun, dies scheint mir verzeihlich zu sein.", "tokens": ["Deut\u00b7scher", ",", "ver\u00b7zeihn's", "\u00ab", ".", "\u2013", "Nun", ",", "dies", "scheint", "mir", "ver\u00b7zeih\u00b7lich", "zu", "sein", "."], "token_info": ["word", "punct", "word", "punct", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "$(", "$.", "$(", "ADV", "$,", "PDS", "VVFIN", "PPER", "ADJD", "PTKZU", "VAINF", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "Nehmen Sie Platz, mein Teurer. In wieviel Tagen vernahm ich", "tokens": ["Neh\u00b7men", "Sie", "Platz", ",", "mein", "Teu\u00b7rer", ".", "In", "wie\u00b7viel", "Ta\u00b7gen", "ver\u00b7nahm", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "$,", "PPOSAT", "NN", "$.", "APPR", "PWAT", "NN", "VVFIN", "PPER"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Kein heimatliches Wort! \u2013 \u00bbSchauen's, so ging mir es halt", "tokens": ["Kein", "hei\u00b7mat\u00b7li\u00b7ches", "Wort", "!", "\u2013", "\u00bb", "Schau\u00b7en's", ",", "so", "ging", "mir", "es", "halt"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$.", "$(", "$(", "NE", "$,", "ADV", "VVFIN", "PPER", "PPER", "VVFIN"], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Auch; drum bin ich so frei, als Deutscher \u2013 wenn Sie erlauben.\u00ab", "tokens": ["Auch", ";", "drum", "bin", "ich", "so", "frei", ",", "als", "Deut\u00b7scher", "\u2013", "wenn", "Sie", "er\u00b7lau\u00b7ben", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$.", "PAV", "VAFIN", "PPER", "ADV", "ADJD", "$,", "KOUS", "NN", "$(", "KOUS", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Nehmen Sie Platz! Wie s\u00fc\u00df t\u00f6nst du, mein m\u00fctterlich Deutsch!", "tokens": ["Neh\u00b7men", "Sie", "Platz", "!", "Wie", "s\u00fc\u00df", "t\u00f6nst", "du", ",", "mein", "m\u00fct\u00b7ter\u00b7lich", "Deutsch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$.", "PWAV", "ADJD", "VVFIN", "PPER", "$,", "PPOSAT", "ADJD", "NN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "All das welsche Gemunkel, zumal das Napoletanisch,", "tokens": ["All", "das", "wel\u00b7sche", "Ge\u00b7mun\u00b7kel", ",", "zu\u00b7mal", "das", "Na\u00b7po\u00b7le\u00b7ta\u00b7nisch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "S\u00fc\u00df wie die Feige, doch auch weichlich entartet wie sie.", "tokens": ["S\u00fc\u00df", "wie", "die", "Fei\u00b7ge", ",", "doch", "auch", "weich\u00b7lich", "ent\u00b7ar\u00b7tet", "wie", "sie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$,", "ADV", "ADV", "ADJD", "VVPP", "KOKOM", "PPER", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Endlich wieder ein kr\u00e4ftiges Wort!", "tokens": ["End\u00b7lich", "wie\u00b7der", "ein", "kr\u00e4f\u00b7ti\u00b7ges", "Wort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Aber, Sie stehn noch immer? \u2013 \u00bbVerzeihn's, ich komme direkte", "tokens": ["A\u00b7ber", ",", "Sie", "stehn", "noch", "im\u00b7mer", "?", "\u2013", "\u00bb", "Ver\u00b7zeihn's", ",", "ich", "kom\u00b7me", "di\u00b7rek\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "PPER", "VVFIN", "ADV", "ADV", "$.", "$(", "$(", "NN", "$,", "PPER", "VVFIN", "ADJA"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.11": {"text": "Vom Vesuvio her, wo ich verwichene Nacht", "tokens": ["Vom", "Ve\u00b7su\u00b7vio", "her", ",", "wo", "ich", "ver\u00b7wi\u00b7che\u00b7ne", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NE", "PTKVZ", "$,", "PWAV", "PPER", "ADJA", "NN"], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.12": {"text": "Beim Einsiedler geschlafen. Der Sakrische! der Malefizkerl", "tokens": ["Beim", "Ein\u00b7sied\u00b7ler", "ge\u00b7schla\u00b7fen", ".", "Der", "Sak\u00b7ri\u00b7sche", "!", "der", "Ma\u00b7le\u00b7fiz\u00b7kerl"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "$.", "ART", "NN", "$.", "ART", "NN"], "meter": "--+--+--+-+-+--+", "measure": "anapaest.tri.plus"}, "line.13": {"text": "Lie\u00df sich zahlen. Zuletzt nahm ich noch Wanzen in Kauf.", "tokens": ["Lie\u00df", "sich", "zah\u00b7len", ".", "Zu\u00b7letzt", "nahm", "ich", "noch", "Wan\u00b7zen", "in", "Kauf", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVINF", "$.", "ADV", "VVFIN", "PPER", "ADV", "NN", "APPR", "NN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "Und nun mein' ich, es sitzt mir im Rock ein Rest des Geziefers,", "tokens": ["Und", "nun", "mein'", "ich", ",", "es", "sitzt", "mir", "im", "Rock", "ein", "Rest", "des", "Ge\u00b7zie\u00b7fers", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.15": {"text": "Und die Racker auch hier nisten sich ein in den Stuhl.", "tokens": ["Und", "die", "Ra\u00b7cker", "auch", "hier", "nis\u00b7ten", "sich", "ein", "in", "den", "Stuhl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADV", "VVFIN", "PRF", "ART", "APPR", "ART", "NN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Ich vor allen, ich bin sehr an das Propre gew\u00f6hnt,", "tokens": ["Ich", "vor", "al\u00b7len", ",", "ich", "bin", "sehr", "an", "das", "Prop\u00b7re", "ge\u00b7w\u00f6hnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PIAT", "$,", "PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.17": {"text": "Erst seit kurzem. Ich komme von Gr\u00e4fenberg, und die Reise", "tokens": ["Erst", "seit", "kur\u00b7zem", ".", "Ich", "kom\u00b7me", "von", "Gr\u00e4\u00b7fen\u00b7berg", ",", "und", "die", "Rei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "$.", "PPER", "VVFIN", "APPR", "NE", "$,", "KON", "ART", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.18": {"text": "Sollte die Nachkur sein.\u00ab \u2013 Hm! ich begreife! ja ja!", "tokens": ["Soll\u00b7te", "die", "Nach\u00b7kur", "sein", ".", "\u00ab", "\u2013", "Hm", "!", "ich", "be\u00b7grei\u00b7fe", "!", "ja", "ja", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VAINF", "$.", "$(", "$(", "ITJ", "$.", "PPER", "VVFIN", "$.", "ADV", "ADV", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.19": {"text": "Dann ist's freilich ein anderes Ding. \u2013 \u00bbJa, schauen's, ich hatt' ein", "tokens": ["Dann", "ist's", "frei\u00b7lich", "ein", "an\u00b7de\u00b7res", "Ding", ".", "\u2013", "\u00bb", "Ja", ",", "schau\u00b7en's", ",", "ich", "hatt'", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ART", "ADJA", "NN", "$.", "$(", "$(", "PTKANT", "$,", "ADJA", "$,", "PPER", "VAFIN", "ART"], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.20": {"text": "Magenleiden, und zehn \u00c4rzte, die ersten in Wien,", "tokens": ["Ma\u00b7gen\u00b7lei\u00b7den", ",", "und", "zehn", "\u00c4rz\u00b7te", ",", "die", "ers\u00b7ten", "in", "Wi\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "CARD", "NN", "$,", "ART", "ADJA", "APPR", "NE", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.21": {"text": "Setzten mir zu. Was half's? Da ging ich zuletzt zu dem Prie\u00dfnitz,", "tokens": ["Setz\u00b7ten", "mir", "zu", ".", "Was", "ha\u00b7lf's", "?", "Da", "ging", "ich", "zu\u00b7letzt", "zu", "dem", "Prie\u00df\u00b7nitz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "PWS", "VAFIN", "$.", "ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.22": {"text": "Mitten im Winter; es war letzten Dezember ein Jahr.\u00ab \u2013", "tokens": ["Mit\u00b7ten", "im", "Win\u00b7ter", ";", "es", "war", "letz\u00b7ten", "De\u00b7zem\u00b7ber", "ein", "Jahr", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "APPRART", "NN", "$.", "PPER", "VAFIN", "ADJA", "NN", "ART", "NN", "$.", "$(", "$("], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.23": {"text": "So! \u2013 \u00bbJa wissen's, ich fror wie ein richtiger Schneider. Es ist dort", "tokens": ["So", "!", "\u2013", "\u00bb", "Ja", "wis\u00b7sen's", ",", "ich", "fror", "wie", "ein", "rich\u00b7ti\u00b7ger", "Schnei\u00b7der", ".", "Es", "ist", "dort"], "token_info": ["word", "punct", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$.", "$(", "$(", "PTKANT", "VVFIN", "$,", "PPER", "VVFIN", "KOKOM", "ART", "ADJA", "NE", "$.", "PPER", "VAFIN", "ADV"], "meter": "--+--+--+--+-+-+", "measure": "anapaest.tetra.plus"}, "line.24": {"text": "Regel, man deckt in der Nacht nur mit dem Kotzen sich zu.", "tokens": ["Re\u00b7gel", ",", "man", "deckt", "in", "der", "Nacht", "nur", "mit", "dem", "Kot\u00b7zen", "sich", "zu", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PIS", "VVFIN", "APPR", "ART", "NN", "ADV", "APPR", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.25": {"text": "Solcher ist d\u00fcnn nur und schmal. Ich kroch im Sommer und Winter,", "tokens": ["Sol\u00b7cher", "ist", "d\u00fcnn", "nur", "und", "schmal", ".", "Ich", "kroch", "im", "Som\u00b7mer", "und", "Win\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADJD", "ADV", "KON", "ADJD", "$.", "PPER", "VVFIN", "APPRART", "NN", "KON", "NN", "$,"], "meter": "+---+-+-+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.26": {"text": "Eh' ich zu Prie\u00dfnitz kam, unter die Federn zu Nacht.", "tokens": ["Eh'", "ich", "zu", "Prie\u00df\u00b7nitz", "kam", ",", "un\u00b7ter", "die", "Fe\u00b7dern", "zu", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "VVFIN", "$,", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Seine Gewohnheit hat doch ein jeglicher.\u00ab \u2013 Wahr! zum Exempel", "tokens": ["Sei\u00b7ne", "Ge\u00b7wohn\u00b7heit", "hat", "doch", "ein", "jeg\u00b7li\u00b7cher", ".", "\u00ab", "\u2013", "Wahr", "!", "zum", "Ex\u00b7em\u00b7pel"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ART", "PIAT", "$.", "$(", "$(", "NE", "$.", "APPRART", "NN"], "meter": "+--+-+--+--+-+-+", "measure": "iambic.septa.invert"}, "line.28": {"text": "Ich, um die jetzige Zeit sch\u00f6pf' ich ein weniges Luft", "tokens": ["Ich", ",", "um", "die", "jet\u00b7zi\u00b7ge", "Zeit", "sch\u00f6pf'", "ich", "ein", "we\u00b7ni\u00b7ges", "Luft"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "KOUI", "ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "PIAT", "NN"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "Auf dem Balkon, sonst schlaf' ich die Nacht nicht. (Freilich, die Stunde", "tokens": ["Auf", "dem", "Bal\u00b7kon", ",", "sonst", "schlaf'", "ich", "die", "Nacht", "nicht", ".", "(", "Frei\u00b7lich", ",", "die", "Stun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "$.", "$(", "ADV", "$,", "ART", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.30": {"text": "War's, wo ihren Balkon auch Mariuccia betrat,", "tokens": ["Wa\u00b7r's", ",", "wo", "ih\u00b7ren", "Bal\u00b7kon", "auch", "Ma\u00b7riuc\u00b7cia", "be\u00b7trat", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWAV", "PPOSAT", "NN", "ADV", "NE", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "Nur ein Haus von dem meinen getrennt. Streng hielt sie die Mutter", "tokens": ["Nur", "ein", "Haus", "von", "dem", "mei\u00b7nen", "ge\u00b7trennt", ".", "Streng", "hielt", "sie", "die", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "PPOSAT", "VVPP", "$.", "NE", "VVFIN", "PPER", "ART", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.32": {"text": "Tags am Webstuhl fest. Aber sie kam in der Nacht.", "tokens": ["Tags", "am", "Web\u00b7stuhl", "fest", ".", "A\u00b7ber", "sie", "kam", "in", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PTKVZ", "$.", "KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.33": {"text": "War's auch immer zum Reden zu weit, zum Blicken zu dunkel,", "tokens": ["Wa\u00b7r's", "auch", "im\u00b7mer", "zum", "Re\u00b7den", "zu", "weit", ",", "zum", "Bli\u00b7cken", "zu", "dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "APPRART", "NN", "PTKA", "ADJD", "$,", "APPRART", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "Gr\u00fc\u00dfte sie doch mit Gesang, winkte sie doch mit der Hand.)", "tokens": ["Gr\u00fc\u00df\u00b7te", "sie", "doch", "mit", "Ge\u00b7sang", ",", "wink\u00b7te", "sie", "doch", "mit", "der", "Hand", ".", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "$,", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$.", "$("], "meter": "+--++-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.35": {"text": "\u00bbgehn's nur,\u00ab bat mich der Wiener. \u00bbDie T\u00fcr ist offen so k\u00f6nnen's", "tokens": ["\u00bb", "gehn's", "nur", ",", "\u00ab", "bat", "mich", "der", "Wie\u00b7ner", ".", "\u00bb", "Die", "T\u00fcr", "ist", "of\u00b7fen", "so", "k\u00f6n\u00b7nen's"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADJA", "ADV", "$,", "$(", "VVFIN", "PPER", "ART", "NN", "$.", "$(", "ART", "NN", "VAFIN", "ADJD", "ADV", "VMFIN"], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.36": {"text": "Mich von drau\u00dfen verstehn. Also wo blieb ich? Ich fror,", "tokens": ["Mich", "von", "drau\u00b7\u00dfen", "ver\u00b7stehn", ".", "Al\u00b7so", "wo", "blieb", "ich", "?", "Ich", "fror", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADV", "VVINF", "$.", "ADV", "PWAV", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "$,"], "meter": "+-+--+--++--+", "measure": "trochaic.hexa.relaxed"}, "line.37": {"text": "Und so geb' ich dem Hausknecht Geld, er soll mir ein Deckbett", "tokens": ["Und", "so", "geb'", "ich", "dem", "Haus\u00b7knecht", "Geld", ",", "er", "soll", "mir", "ein", "Deck\u00b7bett"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$,", "PPER", "VMFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.38": {"text": "Schaffen. Er schafft es, und ich schlafe die Nacht wie ein Dachs.", "tokens": ["Schaf\u00b7fen", ".", "Er", "schafft", "es", ",", "und", "ich", "schla\u00b7fe", "die", "Nacht", "wie", "ein", "Dachs", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPER", "$,", "KON", "PPER", "VVFIN", "ART", "NN", "KOKOM", "ART", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.39": {"text": "Aber was wird mein Prie\u00dfnitz tun? Was denken's? Die Runde", "tokens": ["A\u00b7ber", "was", "wird", "mein", "Prie\u00df\u00b7nitz", "tun", "?", "Was", "den\u00b7ken's", "?", "Die", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWS", "VAFIN", "PPOSAT", "NN", "VVINF", "$.", "PWS", "PDS", "$.", "ART", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.40": {"text": "Macht er und schaut, ob keins wider die Regel verst\u00f6\u00dft.", "tokens": ["Macht", "er", "und", "schaut", ",", "ob", "keins", "wi\u00b7der", "die", "Re\u00b7gel", "ver\u00b7st\u00f6\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "VVFIN", "$,", "KOUS", "PIS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.41": {"text": "Nun, wie gesagt, ich schlief und ich ahnt's nicht. Morgens \u2013 wie wird mir? \u2013", "tokens": ["Nun", ",", "wie", "ge\u00b7sagt", ",", "ich", "schlief", "und", "ich", "ahnt's", "nicht", ".", "Mor\u00b7gens", "\u2013", "wie", "wird", "mir", "?", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$,", "PWAV", "VVPP", "$,", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "PTKNEG", "$.", "ADV", "$(", "KOKOM", "VAFIN", "PPER", "$.", "$("], "meter": "-+-+-+--+-+-++-", "measure": "iambic.septa.relaxed"}, "line.42": {"text": "Lieg' ich \u2013 und klappre vor Frost \u2013 unter dem Kotzen allein.", "tokens": ["Lieg'", "ich", "\u2013", "und", "klapp\u00b7re", "vor", "Frost", "\u2013", "un\u00b7ter", "dem", "Kot\u00b7zen", "al\u00b7lein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$(", "KON", "VVFIN", "APPR", "NN", "$(", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.43": {"text": "Aber mein Hausknecht kl\u00e4rte mich auf! Der Schlingel! Er wu\u00dft' es", "tokens": ["A\u00b7ber", "mein", "Haus\u00b7knecht", "kl\u00e4r\u00b7te", "mich", "auf", "!", "Der", "Schlin\u00b7gel", "!", "Er", "wu\u00dft'", "es"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "ART", "NN", "$.", "PPER", "VVFIN", "PPER"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.44": {"text": "Alles voraus, und doch steckt' er das Geld in den Sack.", "tokens": ["Al\u00b7les", "vo\u00b7raus", ",", "und", "doch", "steckt'", "er", "das", "Geld", "in", "den", "Sack", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKVZ", "$,", "KON", "ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.45": {"text": "Nicht acht Tage, so war ich's gewohnt. Jetzt sei mir ein Bette", "tokens": ["Nicht", "acht", "Ta\u00b7ge", ",", "so", "war", "ich's", "ge\u00b7wohnt", ".", "Jetzt", "sei", "mir", "ein", "Bet\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "CARD", "NN", "$,", "ADV", "VAFIN", "PIS", "VVPP", "$.", "ADV", "VAFIN", "PPER", "ART", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.46": {"text": "Kalt wie es will, nur sei's sauber, so ist mir es recht.", "tokens": ["Kalt", "wie", "es", "will", ",", "nur", "sei's", "sau\u00b7ber", ",", "so", "ist", "mir", "es", "recht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPER", "VMFIN", "$,", "ADV", "VAFIN", "ADJD", "$,", "ADV", "VAFIN", "PPER", "PPER", "ADJD", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}, "line.47": {"text": "Sehr ein erfahrener Doktor, der Prie\u00dfnitz!\u00ab \u2013 ", "tokens": ["Sehr", "ein", "er\u00b7fah\u00b7re\u00b7ner", "Dok\u00b7tor", ",", "der", "Prie\u00df\u00b7nitz", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "ART", "NN", "$.", "$(", "$("], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.48": {"text": "Denn jetzt trat sie heraus, ein L\u00e4mpchen in H\u00e4nden, und h\u00e4ngt' es", "tokens": ["Denn", "jetzt", "trat", "sie", "he\u00b7raus", ",", "ein", "L\u00e4mp\u00b7chen", "in", "H\u00e4n\u00b7den", ",", "und", "h\u00e4ngt'", "es"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "APPR", "NN", "$,", "KON", "VVFIN", "PPER"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.49": {"text": "\u00dcber den Sims des Balkons. Sch\u00f6ner erschien sie als je.", "tokens": ["\u00dc\u00b7ber", "den", "Sims", "des", "Bal\u00b7kons", ".", "Sch\u00f6\u00b7ner", "er\u00b7schien", "sie", "als", "je", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$.", "NN", "VVFIN", "PPER", "KOKOM", "ADV", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.50": {"text": "S\u00e4\u00dfe der l\u00e4stige Mensch nur jetzt in der Tiefe des Kraters", "tokens": ["S\u00e4\u00b7\u00dfe", "der", "l\u00e4s\u00b7ti\u00b7ge", "Mensch", "nur", "jetzt", "in", "der", "Tie\u00b7fe", "des", "Kra\u00b7ters"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "ADV", "ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.51": {"text": "Oder der H\u00f6lle, ein Wort rief' ich hin\u00fcber zu ihr!", "tokens": ["O\u00b7der", "der", "H\u00f6l\u00b7le", ",", "ein", "Wort", "rie\u00b7f'", "ich", "hin\u00b7\u00fc\u00b7ber", "zu", "ihr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "$."], "meter": "+--+--+--+-+--+", "measure": "dactylic.tri.plus"}, "line.52": {"text": "Doch da sitzt er und schwatzt. Das abscheuliche Deutsch! Wie wohl tut", "tokens": ["Doch", "da", "sitzt", "er", "und", "schwatzt", ".", "Das", "ab\u00b7scheu\u00b7li\u00b7che", "Deutsch", "!", "Wie", "wohl", "tut"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "KON", "VVFIN", "$.", "ART", "ADJA", "NN", "$.", "PWAV", "ADV", "NN"], "meter": "--+--+-+---+--+", "measure": "anapaest.di.plus"}, "line.53": {"text": "Ein land\u00fcblicher Fluch, hinter den Z\u00e4hnen gebrummt.", "tokens": ["Ein", "lan\u00b7d\u00fcb\u00b7li\u00b7cher", "Fluch", ",", "hin\u00b7ter", "den", "Z\u00e4h\u00b7nen", "ge\u00b7brummt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+---++--+--+", "measure": "iambic.penta.relaxed"}, "line.54": {"text": "Still! jetzt \u00f6ffnet sie wahrlich den Mund. Was aber best\u00fcrzt sie,", "tokens": ["Still", "!", "jetzt", "\u00f6ff\u00b7net", "sie", "wahr\u00b7lich", "den", "Mund", ".", "Was", "a\u00b7ber", "be\u00b7st\u00fcrzt", "sie", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$.", "PWS", "ADV", "VVFIN", "PPER