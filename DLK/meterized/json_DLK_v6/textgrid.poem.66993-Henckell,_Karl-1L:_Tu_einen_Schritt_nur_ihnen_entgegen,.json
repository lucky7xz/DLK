{"textgrid.poem.66993": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tu einen Schritt nur ihnen entgegen,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tu einen Schritt nur ihnen entgegen,", "tokens": ["Tu", "ei\u00b7nen", "Schritt", "nur", "ih\u00b7nen", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie geben dir zwiefach ihren Segen,", "tokens": ["Sie", "ge\u00b7ben", "dir", "zwie\u00b7fach", "ih\u00b7ren", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Aber sie werden schrecklich verstimmt,", "tokens": ["A\u00b7ber", "sie", "wer\u00b7den", "schreck\u00b7lich", "ver\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wenn man wieder mehr Zwischenraum nimmt.", "tokens": ["Wenn", "man", "wie\u00b7der", "mehr", "Zwi\u00b7schen\u00b7raum", "nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}}}}