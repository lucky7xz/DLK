{"textgrid.poem.42101": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Gesang an die Stadt", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du brachst im sch\u00f6nen Leib der Landschaft auf.", "tokens": ["Du", "brachst", "im", "sch\u00f6\u00b7nen", "Leib", "der", "Land\u00b7schaft", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie eine Eiterbeule aufbricht, brachst du", "tokens": ["Wie", "ei\u00b7ne", "Ei\u00b7ter\u00b7beu\u00b7le", "auf\u00b7bricht", ",", "brachst", "du"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVPP", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Auf in den Tag und liegst du glei\u00dfend nun", "tokens": ["Auf", "in", "den", "Tag", "und", "liegst", "du", "glei\u00b7\u00dfend", "nun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "KON", "VVFIN", "PPER", "ADJD", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Unter den Sternen nachts.", "tokens": ["Un\u00b7ter", "den", "Ster\u00b7nen", "nachts", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Nun gehen alle gro\u00dfen Stra\u00dfen zu dir hin:", "tokens": ["Nun", "ge\u00b7hen", "al\u00b7le", "gro\u00b7\u00dfen", "Stra\u00b7\u00dfen", "zu", "dir", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu dir die M\u00fchsal-Stra\u00dfen, staubbedeckt,", "tokens": ["Zu", "dir", "die", "M\u00fch\u00b7sa\u00b7lStra\u00b7\u00dfen", ",", "staub\u00b7be\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Eisen-Adern, und die unsichtbaren", "tokens": ["Die", "Ei\u00b7sen\u00b7A\u00b7dern", ",", "und", "die", "un\u00b7sicht\u00b7ba\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Stra\u00dfen der Luft spannen sich nach dir hin.", "tokens": ["Stra\u00b7\u00dfen", "der", "Luft", "span\u00b7nen", "sich", "nach", "dir", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "PRF", "APPR", "PPER", "PTKVZ", "$."], "meter": "+--++--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Und alle gro\u00dfen Str\u00f6me m\u00fcnden in dich ein:", "tokens": ["Und", "al\u00b7le", "gro\u00b7\u00dfen", "Str\u00f6\u00b7me", "m\u00fcn\u00b7den", "in", "dich", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VMFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dich die breiten, breiten Str\u00f6me Blut,", "tokens": ["In", "dich", "die", "brei\u00b7ten", ",", "brei\u00b7ten", "Str\u00f6\u00b7me", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "ADJA", "$,", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Str\u00f6me Lust, die Str\u00f6me Schmerz, es st\u00fcrzen", "tokens": ["Die", "Str\u00f6\u00b7me", "Lust", ",", "die", "Str\u00f6\u00b7me", "Schmerz", ",", "es", "st\u00fcr\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$,", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu dir die tausendfachen Str\u00f6me Tod.", "tokens": ["Zu", "dir", "die", "tau\u00b7send\u00b7fa\u00b7chen", "Str\u00f6\u00b7me", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da\u00df, wer in dich trieb, Wirbel, sieht zur\u00fcck", "tokens": ["Da\u00df", ",", "wer", "in", "dich", "trieb", ",", "Wir\u00b7bel", ",", "sieht", "zu\u00b7r\u00fcck"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "$,", "PWS", "APPR", "PPER", "VVFIN", "$,", "NN", "$,", "VVFIN", "PTKVZ"], "meter": "-+-+++-+-+", "measure": "zehnsilber"}, "line.2": {"text": "Und sieht nur Weg: sieht noch in Kindheitsjahren", "tokens": ["Und", "sieht", "nur", "Weg", ":", "sieht", "noch", "in", "Kind\u00b7heits\u00b7jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "NN", "$.", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von Kindheitsdrachen sehnsuchtsvoll befahren", "tokens": ["Von", "Kind\u00b7heits\u00b7dra\u00b7chen", "sehn\u00b7suchts\u00b7voll", "be\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nach dir ein ziellos reines Blau.", "tokens": ["Nach", "dir", "ein", "ziel\u00b7los", "rei\u00b7nes", "Blau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}