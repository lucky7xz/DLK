{"textgrid.poem.31217": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00fcmbzw\u00e4nckt sein Hau\u00df au\u00df Ey\u00df/", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00fcmbzw\u00e4nckt sein Hau\u00df au\u00df Ey\u00df/", "tokens": ["\u00fcmb\u00b7zw\u00e4nckt", "sein", "Hau\u00df", "au\u00df", "Ey\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "itzt sizzt es sich so r\u00e4cht geheuer", "tokens": ["itzt", "sizzt", "es", "sich", "so", "r\u00e4cht", "ge\u00b7heu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "VVFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u00fcmb unser lihbes Schornstein-Feuer!", "tokens": ["\u00fcmb", "un\u00b7ser", "lih\u00b7bes", "Schorn\u00b7stein\u00b7Feu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch den nichts al\u00df blancken Frost", "tokens": ["Durch", "den", "nichts", "al\u00df", "blan\u00b7cken", "Frost"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PIS", "KOKOM", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "klirrt der ", "tokens": ["klirrt", "der"], "token_info": ["word", "word"], "pos": ["VVFIN", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "in den krau\u00df befrohrnen Scheiben", "tokens": ["in", "den", "krau\u00df", "be\u00b7fr\u00b7ohr\u00b7nen", "Schei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "siht man kleine Bl\u00fchmckens kleiben.", "tokens": ["siht", "man", "klei\u00b7ne", "Bl\u00fch\u00b7mckens", "klei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "durch den dikken Dannicht streifft/", "tokens": ["durch", "den", "dik\u00b7ken", "Dan\u00b7nicht", "streifft", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hirsche/ W\u00f6lffe/ B\u00e4ren/ L\u00fcxe/", "tokens": ["Hir\u00b7sche", "/", "W\u00f6lf\u00b7fe", "/", "B\u00e4\u00b7ren", "/", "L\u00fc\u00b7xe", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "$(", "NE", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "knallt sich seine Kugel-B\u00fcxe.", "tokens": ["knallt", "sich", "sei\u00b7ne", "Ku\u00b7gel\u00b7B\u00fc\u00b7xe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "sinnt itzt nur auffs Schlitten-Fahren.", "tokens": ["sinnt", "itzt", "nur", "auffs", "Schlit\u00b7ten\u00b7Fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00f6mbt er Abends froh nach Hau\u00df/", "tokens": ["K\u00f6mbt", "er", "A\u00b7bends", "froh", "nach", "Hau\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "APPR", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "ziht sie ihm den Harnisch au\u00df/", "tokens": ["ziht", "sie", "ihm", "den", "Har\u00b7nisch", "au\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hasel-H\u00fchner/ L\u00f6ffel-Kraut", "tokens": ["Ha\u00b7sel\u00b7H\u00fch\u00b7ner", "/", "L\u00f6f\u00b7fel\u00b7Kraut"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$(", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "stehn schon for ihn auff-gebaut", "tokens": ["stehn", "schon", "for", "ihn", "auf\u00b7fge\u00b7baut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "VVFIN", "PPER", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "und zu einem Gl\u00e4sgen Wein", "tokens": ["und", "zu", "ei\u00b7nem", "Gl\u00e4s\u00b7gen", "Wein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Sauer-Kohl mit Pfl\u00fckk-H\u00e4cht dreyn.", "tokens": ["Sau\u00b7e\u00b7rKohl", "mit", "Pfl\u00fc\u00b7kk\u00b7\u00b7H\u00e4cht", "dreyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Beyde Br\u00fcste nakkt und bloh\u00df/", "tokens": ["Bey\u00b7de", "Br\u00fcs\u00b7te", "nakkt", "und", "bloh\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "KON", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "sizzt sie sich auff seinen Schoo\u00df;", "tokens": ["sizzt", "sie", "sich", "auff", "sei\u00b7nen", "Schoo\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "alles ist ihm frey gestellt/", "tokens": ["al\u00b7les", "ist", "ihm", "frey", "ge\u00b7stellt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADJD", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "s\u00e4lbst ihr g\u00f6ldnes Rohsen-Zelt!", "tokens": ["s\u00e4lbst", "ihr", "g\u00f6ld\u00b7nes", "Roh\u00b7sen\u00b7Zelt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "l\u00e4\u00dft sich die beyden lihben", "tokens": ["l\u00e4\u00dft", "sich", "die", "bey\u00b7den", "lih\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "PIAT", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "und stopfft sich seinen schwartzen Bauch", "tokens": ["und", "stopfft", "sich", "sei\u00b7nen", "schwart\u00b7zen", "Bauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "f\u00e4st voll G\u00e4nse-Grihben.", "tokens": ["f\u00e4st", "voll", "G\u00e4n\u00b7se\u00b7Grih\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "saufft sihben Eymer Bacharacher.", "tokens": ["saufft", "sih\u00b7ben", "Ey\u00b7mer", "Bac\u00b7ha\u00b7ra\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Drauff so rukken beyde dicht", "tokens": ["Drauff", "so", "ruk\u00b7ken", "bey\u00b7de", "dicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "VVFIN", "PIS", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "in das g\u00f6ldne Lampen-Licht", "tokens": ["in", "das", "g\u00f6ld\u00b7ne", "Lam\u00b7pen\u00b7Licht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "und zu einer Pfeiff Thobakk", "tokens": ["und", "zu", "ei\u00b7ner", "Pfeiff", "Tho\u00b7bakk"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "spihlen sie dan Dikke-Dakk.", "tokens": ["spih\u00b7len", "sie", "dan", "Dik\u00b7ke\u00b7Dakk", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Unterde\u00df pfaucht immer gr\u00f6ber", "tokens": ["Un\u00b7ter\u00b7de\u00df", "pfaucht", "im\u00b7mer", "gr\u00f6\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "durch die dikk begraute Lufft", "tokens": ["durch", "die", "dikk", "be\u00b7grau\u00b7te", "Lufft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "zirculirt ein Schnee-Gest\u00f6ber.", "tokens": ["zir\u00b7cu\u00b7lirt", "ein", "Schnee\u00b7Ge\u00b7st\u00f6\u00b7ber", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Puh Teuffel/ fegt das kolt!", "tokens": ["Puh", "Teuf\u00b7fel", "/", "fegt", "das", "kolt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "VVFIN", "PDS", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich acht/ ein guht Gla\u00df Wein", "tokens": ["Ich", "acht", "/", "ein", "guht", "Gla\u00df", "Wein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "CARD", "$(", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "sampt einer Braht-Worst sollt", "tokens": ["sampt", "ei\u00b7ner", "Braht\u00b7Worst", "sollt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "ihm angenehm itzt seyn!", "tokens": ["ihm", "an\u00b7ge\u00b7nehm", "itzt", "seyn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "verk\u00e4ufft bloh\u00df Honig-Fladen.", "tokens": ["ver\u00b7k\u00e4ufft", "bloh\u00df", "Ho\u00b7nig\u00b7Fla\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Mit Hertzgens gantz au\u00df Kuchen", "tokens": ["Mit", "Hertz\u00b7gens", "gantz", "au\u00df", "Ku\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "macht er die Kindgens juchen/", "tokens": ["macht", "er", "die", "Kind\u00b7gens", "ju\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.11": {"text": "mit Prillekens und Prindten", "tokens": ["mit", "Pril\u00b7le\u00b7kens", "und", "Prind\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "stopfft er sie voll bi\u00df Hindten.", "tokens": ["stopfft", "er", "sie", "voll", "bi\u00df", "Hind\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Mit Obst und Confect\u00fcren", "tokens": ["Mit", "Obst", "und", "Con\u00b7fec\u00b7t\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "kan er sie durchau\u00df r\u00fchren;", "tokens": ["kan", "er", "sie", "durch\u00b7au\u00df", "r\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Knakk-Mandeln und Mors\u00f6llen \u2013", "tokens": ["Knak\u00b7k\u00b7Man\u00b7deln", "und", "Mor\u00b7s\u00f6l\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "er wei\u00df schon/ wa\u00df sie w\u00f6llen!", "tokens": ["er", "wei\u00df", "schon", "/", "wa\u00df", "sie", "w\u00f6l\u00b7len", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "KOUS", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Heimlich streichen zu ihm hin", "tokens": ["Heim\u00b7lich", "strei\u00b7chen", "zu", "ihm", "hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPER", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "auch die kleinen J\u00fcngffrichin.", "tokens": ["auch", "die", "klei\u00b7nen", "J\u00fcngf\u00b7fri\u00b7chin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Sie sind for Feffergens und W\u00fcrtzgens/", "tokens": ["Sie", "sind", "for", "Fef\u00b7fer\u00b7gens", "und", "W\u00fcrtz\u00b7gens", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "sie knuppen gerne Nonnen-F\u00fcrtzgens/", "tokens": ["sie", "knup\u00b7pen", "ger\u00b7ne", "Non\u00b7nen\u00b7F\u00fcrtz\u00b7gens", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "sie sind mit wenig Worten", "tokens": ["sie", "sind", "mit", "we\u00b7nig", "Wor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "for angemachte Torten.", "tokens": ["for", "an\u00b7ge\u00b7mach\u00b7te", "Tor\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "dr\u00fckkt die Ofen-Banck und spinnt.", "tokens": ["dr\u00fckkt", "die", "O\u00b7fen\u00b7Banck", "und", "spinnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Jeden Morgen kombt sie nider/", "tokens": ["Je\u00b7den", "Mor\u00b7gen", "kombt", "sie", "ni\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "denn sie tichtet \u2013 Mayen-Lider!", "tokens": ["denn", "sie", "tich\u00b7tet", "\u2013", "Mayen\u00b7Li\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$(", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.5": {"line.1": {"text": "Bringstu mir schon Sp\u00e4kk und Worst/", "tokens": ["Brings\u00b7tu", "mir", "schon", "Sp\u00e4kk", "und", "Worst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "bringstu mir schon Schincken?", "tokens": ["brings\u00b7tu", "mir", "schon", "Schin\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mein/ wa\u00df blagt mich for ein Dorst/", "tokens": ["Mein", "/", "wa\u00df", "blagt", "mich", "for", "ein", "Dorst", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$(", "PWAV", "VVFIN", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "schaff mir auch zu drincken!", "tokens": ["schaff", "mir", "auch", "zu", "drin\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Lilgen/ Tulipen und Klee", "tokens": ["Lil\u00b7gen", "/", "Tu\u00b7li\u00b7pen", "und", "Klee"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "d\u00e4kkt itzt Elen-tieffer Schnee/", "tokens": ["d\u00e4kkt", "itzt", "E\u00b7len\u00b7tief\u00b7fer", "Schnee", "/"], "token_info": ["word", "word", "word", "word", "punct"], "po