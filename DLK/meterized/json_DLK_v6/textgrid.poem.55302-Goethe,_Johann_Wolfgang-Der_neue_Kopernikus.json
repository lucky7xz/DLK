{"textgrid.poem.55302": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Der neue Kopernikus", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Art'ges H\u00e4uschen hab ich klein,", "tokens": ["Art'\u00b7ges", "H\u00e4usc\u00b7hen", "hab", "ich", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und, darin verstecket,", "tokens": ["Und", ",", "da\u00b7rin", "ver\u00b7ste\u00b7cket", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "PAV", "VVFIN", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Bin ich vor der Sonne Schein", "tokens": ["Bin", "ich", "vor", "der", "Son\u00b7ne", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gar bequem bedecket.", "tokens": ["Gar", "be\u00b7quem", "be\u00b7de\u00b7cket", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Denn da gibt es Schalterlein,", "tokens": ["Denn", "da", "gibt", "es", "Schal\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Federchen und L\u00e4dchen,", "tokens": ["Fe\u00b7der\u00b7chen", "und", "L\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Finde mich so wohl allein", "tokens": ["Fin\u00b7de", "mich", "so", "wohl", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als mit h\u00fcbschen M\u00e4dchen.", "tokens": ["Als", "mit", "h\u00fcb\u00b7schen", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Denn, o Wunder! mir zur Lust", "tokens": ["Denn", ",", "o", "Wun\u00b7der", "!", "mir", "zur", "Lust"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "FM", "NN", "$.", "PPER", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Regen sich die W\u00e4lder,", "tokens": ["Re\u00b7gen", "sich", "die", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "N\u00e4her kommen meiner Brust", "tokens": ["N\u00e4\u00b7her", "kom\u00b7men", "mei\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die entfernten Felder.", "tokens": ["Die", "ent\u00b7fern\u00b7ten", "Fel\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Und so tanzen auch vorbei", "tokens": ["Und", "so", "tan\u00b7zen", "auch", "vor\u00b7bei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die bewachsnen Berge,", "tokens": ["Die", "be\u00b7wachs\u00b7nen", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Fehlet nur das Lustgeschrei", "tokens": ["Feh\u00b7let", "nur", "das", "Lust\u00b7ge\u00b7schrei"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aufgeregter Zwerge.", "tokens": ["Auf\u00b7ge\u00b7reg\u00b7ter", "Zwer\u00b7ge", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Doch so g\u00e4nzlich still und stumm", "tokens": ["Doch", "so", "g\u00e4nz\u00b7lich", "still", "und", "stumm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "PTKVZ", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Rennt es mir vor\u00fcber,", "tokens": ["Rennt", "es", "mir", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Meistens grad und oft auch krumm,", "tokens": ["Meis\u00b7tens", "grad", "und", "oft", "auch", "krumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und so ist mir's lieber.", "tokens": ["Und", "so", "ist", "mir's", "lie\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NE", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Wenn ich's recht betrachten will", "tokens": ["Wenn", "ich's", "recht", "be\u00b7trach\u00b7ten", "will"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADJD", "VVINF", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es ernst gewahre,", "tokens": ["Und", "es", "ernst", "ge\u00b7wah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Steht vielleicht das alles still,", "tokens": ["Steht", "viel\u00b7leicht", "das", "al\u00b7les", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "PIS", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich selber fahre.", "tokens": ["Und", "ich", "sel\u00b7ber", "fah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}