{"textgrid.poem.53171": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Christoph Kerstein und Maria von Weinbeer", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tantz, der du Gesetze", "tokens": ["Tantz", ",", "der", "du", "Ge\u00b7set\u00b7ze"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPER", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Vnsern F\u00fcssen giebst,", "tokens": ["Vn\u00b7sern", "F\u00fcs\u00b7sen", "giebst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Hand-dr\u00fcck, Huld-Geschw\u00e4tze,", "tokens": ["Han\u00b7ddr\u00fcck", ",", "Huld\u00b7Ge\u00b7schw\u00e4t\u00b7ze", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Schertz und Liebe liebst,", "tokens": ["Schertz", "und", "Lie\u00b7be", "liebst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Einig deinetwegen", "tokens": ["Ei\u00b7nig", "dei\u00b7net\u00b7we\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADJD", "PPOSAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ist die Jugend hier,", "tokens": ["Ist", "die", "Ju\u00b7gend", "hier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "W\u00fcnscht, du woltest regen", "tokens": ["W\u00fcnscht", ",", "du", "wol\u00b7test", "re\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VMFIN", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Deiner Lust Panier.", "tokens": ["Dei\u00b7ner", "Lust", "Pa\u00b7nier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Weder Tranck noch Essen", "tokens": ["We\u00b7der", "Tranck", "noch", "Es\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "ADV", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "K\u00f6nnen bey ihr ein,", "tokens": ["K\u00f6n\u00b7nen", "bey", "ihr", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Alles wird vergessen,", "tokens": ["Al\u00b7les", "wird", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Hat sie dich allein,", "tokens": ["Hat", "sie", "dich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Sinnen, Augen, Ohren", "tokens": ["Sin\u00b7nen", ",", "Au\u00b7gen", ",", "Oh\u00b7ren"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Werden uns zuhauff", "tokens": ["Wer\u00b7den", "uns", "zu\u00b7hauff"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Gleichsam wie beschworen,", "tokens": ["Gleich\u00b7sam", "wie", "be\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Zeucht dein L\u00e4ger auff.", "tokens": ["Zeucht", "dein", "L\u00e4\u00b7ger", "auff", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wie die B\u00e4um' im Lentzen", "tokens": ["Wie", "die", "B\u00e4um'", "im", "Lent\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Von der Bl\u00fcthe schwer,", "tokens": ["Von", "der", "Bl\u00fc\u00b7the", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wie die Tauben gl\u00e4ntzen,", "tokens": ["Wie", "die", "Tau\u00b7ben", "gl\u00e4nt\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wie ein Krieges-Heer:", "tokens": ["Wie", "ein", "Krie\u00b7ge\u00b7sHeer", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "So bist du zu schawen,", "tokens": ["So", "bist", "du", "zu", "scha\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Tantz, wenn du dich r\u00fchrst", "tokens": ["Tantz", ",", "wenn", "du", "dich", "r\u00fchrst"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Vnd an die Jungfrawen", "tokens": ["Vnd", "an", "die", "Jung\u00b7fra\u00b7wen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Die Gesellen f\u00fchrst.", "tokens": ["Die", "Ge\u00b7sel\u00b7len", "f\u00fchrst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "--+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Auff, such zu begn\u00fcgen", "tokens": ["Auff", ",", "such", "zu", "be\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["APPR", "$,", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Dieses edle Paar,", "tokens": ["Die\u00b7ses", "ed\u00b7le", "Paar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Das sich jetzt wil f\u00fcgen", "tokens": ["Das", "sich", "jetzt", "wil", "f\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PRF", "ADV", "VMFIN", "VVINF"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Vmb das Newe Jahr,", "tokens": ["Vmb", "das", "Ne\u00b7we", "Jahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Reg in ihren Sinnen", "tokens": ["Reg", "in", "ih\u00b7ren", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Dich mit newer Gunst,", "tokens": ["Dich", "mit", "ne\u00b7wer", "Gunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "La\u00df sie stets gewinnen", "tokens": ["La\u00df", "sie", "stets", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Keusche Gegen-Brunst.", "tokens": ["Keu\u00b7sche", "Ge\u00b7gen\u00b7Brunst", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Schaff, da\u00df ihre Sachen", "tokens": ["Schaff", ",", "da\u00df", "ih\u00b7re", "Sa\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wie im Tantze gehn,", "tokens": ["Wie", "im", "Tant\u00b7ze", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "La\u00df nur Lieb' und Lachen", "tokens": ["La\u00df", "nur", "Lieb'", "und", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Allzeit umb sie stehn!", "tokens": ["All\u00b7zeit", "umb", "sie", "stehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Nichts so reich an G\u00fcte", "tokens": ["Nichts", "so", "reich", "an", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "ADV", "ADJD", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wird f\u00fcr sie begehrt,", "tokens": ["Wird", "f\u00fcr", "sie", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Ihrer Tugend Bl\u00fcte", "tokens": ["Ih\u00b7rer", "Tu\u00b7gend", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Ist desselben wehrt.", "tokens": ["Ist", "des\u00b7sel\u00b7ben", "wehrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Hierauff stimm Schalmeyen", "tokens": ["Hier\u00b7auff", "stimm", "Schal\u00b7me\u00b7yen"], "token_info": ["word", "word", "word"], "pos": ["PAV", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vnd Trompetten an,", "tokens": ["Vnd", "Trom\u00b7pet\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKVZ", "$,"], "meter": "-++-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "La\u00df an deinen Reyen", "tokens": ["La\u00df", "an", "dei\u00b7nen", "Re\u00b7yen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Gehen was nur kan,", "tokens": ["Ge\u00b7hen", "was", "nur", "kan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "ADV", "VMFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Leb' uns zu gefallen,", "tokens": ["Leb'", "uns", "zu", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Angesehn, da\u00df Welt,", "tokens": ["An\u00b7ge\u00b7sehn", ",", "da\u00df", "Welt", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Zeit und Tod sampt allen", "tokens": ["Zeit", "und", "Tod", "sampt", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "APPR", "PIAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Seinen Reyen h\u00e4lt.", "tokens": ["Sei\u00b7nen", "Re\u00b7yen", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}