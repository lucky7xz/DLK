{"textgrid.poem.55253": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Vergebliche M\u00fch", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du der getreue Eckart sein", "tokens": ["Willst", "du", "der", "ge\u00b7treu\u00b7e", "E\u00b7ckart", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "PPOSAT"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und jedermann vor Schaden warnen,", "tokens": ["Und", "je\u00b7der\u00b7mann", "vor", "Scha\u00b7den", "war\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "'s ist auch eine Rolle, sie tr\u00e4gt nichts ein:", "tokens": ["'s", "ist", "auch", "ei\u00b7ne", "Rol\u00b7le", ",", "sie", "tr\u00e4gt", "nichts", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$,", "PPER", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sie laufen dennoch nach den Garnen.", "tokens": ["Sie", "lau\u00b7fen", "den\u00b7noch", "nach", "den", "Gar\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}