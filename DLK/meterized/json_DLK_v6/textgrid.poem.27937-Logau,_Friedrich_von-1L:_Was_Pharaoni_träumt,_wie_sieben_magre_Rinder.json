{"textgrid.poem.27937": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was Pharaoni tr\u00e4umt, wie sieben magre Rinder", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was Pharaoni tr\u00e4umt, wie sieben magre Rinder", "tokens": ["Was", "Pha\u00b7rao\u00b7ni", "tr\u00e4umt", ",", "wie", "sie\u00b7ben", "mag\u00b7re", "Rin\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "NE", "VVFIN", "$,", "PWAV", "CARD", "ADJA", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Verschlungen sieben fett, ereignet sich nicht minder", "tokens": ["Ver\u00b7schlun\u00b7gen", "sie\u00b7ben", "fett", ",", "er\u00b7eig\u00b7net", "sich", "nicht", "min\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "CARD", "ADJD", "$,", "VVFIN", "PRF", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bey uns und in der That; dann mancher Hunger-leider", "tokens": ["Bey", "uns", "und", "in", "der", "That", ";", "dann", "man\u00b7cher", "Hun\u00b7ger\u00b7lei\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "KON", "APPR", "ART", "NN", "$.", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist fett vom Raube-Brot und gl\u00e4ntzt durch fremde Kleider.", "tokens": ["Ist", "fett", "vom", "Rau\u00b7be\u00b7Brot", "und", "gl\u00e4ntzt", "durch", "frem\u00b7de", "Klei\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPRART", "NN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-