{"textgrid.poem.34464": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "[wies dem Hunde geht]", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wies dem Hunde geht,", "tokens": ["Wies", "dem", "Hun\u00b7de", "geht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "der von dem Diener draussen", "tokens": ["der", "von", "dem", "Die\u00b7ner", "draus\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "geschlagen und getreten wird,", "tokens": ["ge\u00b7schla\u00b7gen", "und", "ge\u00b7tre\u00b7ten", "wird", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und dem die Herrin drinnen schmeichelt", "tokens": ["und", "dem", "die", "Her\u00b7rin", "drin\u00b7nen", "schmei\u00b7chelt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "und den sie liebkost \u2013", "tokens": ["und", "den", "sie", "lieb\u00b7kost", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "VVFIN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "so geht es mir.", "tokens": ["so", "geht", "es", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Verschwende deine G\u00fcte nicht,", "tokens": ["Ver\u00b7schwen\u00b7de", "dei\u00b7ne", "G\u00fc\u00b7te", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "du s\u00fcsse kleine Herrin,", "tokens": ["du", "s\u00fcs\u00b7se", "klei\u00b7ne", "Her\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "verw\u00f6hne nicht mich Armen!", "tokens": ["ver\u00b7w\u00f6h\u00b7ne", "nicht", "mich", "Ar\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich muss ja doch hinaus von dir,", "tokens": ["Ich", "muss", "ja", "doch", "hin\u00b7aus", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APZR", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und wieder fasst mich dann", "tokens": ["und", "wie\u00b7der", "fasst", "mich", "dann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "das kalte Leben draussen,", "tokens": ["das", "kal\u00b7te", "Le\u00b7ben", "draus\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "und um so h\u00e4rter nur", "tokens": ["und", "um", "so", "h\u00e4r\u00b7ter", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADV", "ADJD", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "werd ich es f\u00fchlen,", "tokens": ["werd", "ich", "es", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "je mehr du Gute", "tokens": ["je", "mehr", "du", "Gu\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "zuvor mir Liebe schenktest.", "tokens": ["zu\u00b7vor", "mir", "Lie\u00b7be", "schenk\u00b7test", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}