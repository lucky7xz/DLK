"so", "bist", "du", "kein", "Deut\u00b7scher", "!", "ein", "Nac\u00b7hah\u00b7mer"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "ADV", "VAFIN", "PPER", "PIAT", "NN", "$.", "ART", "NN"], "meter": "+--+--+--+--", "measure": "dactylic.tetra"}, "line.2": {"text": "Belastet vom Joche, verkenst du dich selber!", "tokens": ["Be\u00b7las\u00b7tet", "vom", "Jo\u00b7che", ",", "ver\u00b7kenst", "du", "dich", "sel\u00b7ber", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$,", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Keines Gesang ward dir Marathons Schlacht!", "tokens": ["Kei\u00b7nes", "Ge\u00b7sang", "ward", "dir", "Ma\u00b7ra\u00b7thons", "Schlacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "NE", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "N\u00e4cht' ohne Schlaf hattest du nie!", "tokens": ["N\u00e4cht'", "oh\u00b7ne", "Schlaf", "hat\u00b7test", "du", "nie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}