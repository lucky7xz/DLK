{"textgrid.poem.54145": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Eisenbahner", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Stellwerk wachen in der Nacht \u2013", "tokens": ["Im", "Stell\u00b7werk", "wa\u00b7chen", "in", "der", "Nacht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Marsch \u2013 Marsch! Zehn Stunden Dienst gemacht!", "tokens": ["Marsch", "\u2013", "Marsch", "!", "Zehn", "Stun\u00b7den", "Dienst", "ge\u00b7macht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NE", "$.", "CARD", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die schweren Hebel an der Hand,", "tokens": ["Die", "schwe\u00b7ren", "He\u00b7bel", "an", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hitze und Zugwind am F\u00fchrerstand.", "tokens": ["Hit\u00b7ze", "und", "Zug\u00b7wind", "am", "F\u00fch\u00b7rer\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Im Bauch kalten Kaffee, im Kopf das Signal,", "tokens": ["Im", "Bauch", "kal\u00b7ten", "Kaf\u00b7fee", ",", "im", "Kopf", "das", "Sig\u00b7nal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "die Strecke abgehen, hundertmal \u2013:", "tokens": ["die", "Stre\u00b7cke", "ab\u00b7ge\u00b7hen", ",", "hun\u00b7dert\u00b7mal", "\u2013", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ADV", "$(", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "das macht das Unterpersonal.", "tokens": ["das", "macht", "das", "Un\u00b7ter\u00b7per\u00b7so\u00b7nal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Verf\u00fcgungen schmieren, wie die dienstlichen M\u00fctzen", "tokens": ["Ver\u00b7f\u00fc\u00b7gun\u00b7gen", "schmie\u00b7ren", ",", "wie", "die", "dienst\u00b7li\u00b7chen", "M\u00fct\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "auf dem Proletenkopf sollen sitzen;", "tokens": ["auf", "dem", "Pro\u00b7le\u00b7ten\u00b7kopf", "sol\u00b7len", "sit\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "nur die eigene Beh\u00f6rde kennen;", "tokens": ["nur", "die", "ei\u00b7ge\u00b7ne", "Be\u00b7h\u00f6r\u00b7de", "ken\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "sich gegenseitig zum Geheimrat ernennen;", "tokens": ["sich", "ge\u00b7gen\u00b7sei\u00b7tig", "zum", "Ge\u00b7heim\u00b7rat", "er\u00b7nen\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.5": {"text": "vom gr\u00fcnen Tisch den gemeinen Haufen", "tokens": ["vom", "gr\u00fc\u00b7nen", "Tisch", "den", "ge\u00b7mei\u00b7nen", "Hau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "regieren, da\u00df alle in Akten versaufen;", "tokens": ["re\u00b7gie\u00b7ren", ",", "da\u00df", "al\u00b7le", "in", "Ak\u00b7ten", "ver\u00b7sau\u00b7fen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "$,", "KOUS", "PIS", "APPR", "NN", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "auf Wersalljes schimpfen, aufs Material \u2013:", "tokens": ["auf", "Wer\u00b7sall\u00b7jes", "schimp\u00b7fen", ",", "aufs", "Ma\u00b7te\u00b7ri\u00b7al", "\u2013", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "VVFIN", "$,", "APPRART", "NN", "$(", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "das tut das Oberpersonal.", "tokens": ["das", "tut", "das", "O\u00b7ber\u00b7per\u00b7so\u00b7nal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Den Kopf hinhalten vor Gericht;", "tokens": ["Den", "Kopf", "hin\u00b7hal\u00b7ten", "vor", "Ge\u00b7richt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Maul halten, wenn der Richter spricht;", "tokens": ["Maul", "hal\u00b7ten", ",", "wenn", "der", "Rich\u00b7ter", "spricht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die Brust hinhalten, wenn es spr\u00fcht,", "tokens": ["die", "Brust", "hin\u00b7hal\u00b7ten", ",", "wenn", "es", "spr\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "undichtes Rohr . . . der Dampf verbr\u00fcht . . .", "tokens": ["un\u00b7dich\u00b7tes", "Rohr", ".", ".", ".", "der", "Dampf", "ver\u00b7br\u00fcht", ".", ".", "."], "token_info": ["word", "word", "punct", "punct", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$.", "$.", "ART", "NN", "VVPP", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "ein hei\u00dfer Strahl . . . weg, ins Spital . . .", "tokens": ["ein", "hei\u00b7\u00dfer", "Strahl", ".", ".", ".", "weg", ",", "ins", "Spi\u00b7tal", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$.", "$.", "PTKVZ", "$,", "APPRART", "NN", "$.", "$.", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "So f\u00fcnfzig\u2013, hundert\u2013, tausendmal \u2013:", "tokens": ["So", "f\u00fcnf\u00b7zig", "\u2013", ",", "hun\u00b7dert", "\u2013", ",", "tau\u00b7send\u00b7mal", "\u2013", ":"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["ADV", "CARD", "$(", "$,", "CARD", "$(", "$,", "ADV", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "das macht das Unterpersonal.", "tokens": ["das", "macht", "das", "Un\u00b7ter\u00b7per\u00b7so\u00b7nal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Intrigieren und organisieren \u2013", "tokens": ["Int\u00b7ri\u00b7gie\u00b7ren", "und", "or\u00b7ga\u00b7ni\u00b7sie\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "paragraphieren und reglementieren.", "tokens": ["pa\u00b7ra\u00b7gra\u00b7phie\u00b7ren", "und", "re\u00b7gle\u00b7men\u00b7tie\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVINF", "KON", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Geht es bei Katastrophen ans Leben,", "tokens": ["Geht", "es", "bei", "Ka\u00b7tast\u00b7ro\u00b7phen", "ans", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "APPRART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "sich \u00bbpers\u00f6nlich auf den Schauplatz begeben\u00ab;", "tokens": ["sich", "\u00bb", "per\u00b7s\u00f6n\u00b7lich", "auf", "den", "Schau\u00b7platz", "be\u00b7ge\u00b7ben", "\u00ab", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PRF", "$(", "ADJD", "APPR", "ART", "NN", "VVPP", "$(", "$."], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "an Vorschriften und Verf\u00fcgungen polken,", "tokens": ["an", "Vor\u00b7schrif\u00b7ten", "und", "Ver\u00b7f\u00fc\u00b7gun\u00b7gen", "pol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "(wie ein Mond leuchtet Dorpm\u00fcller aus den Wolken).", "tokens": ["(", "wie", "ein", "Mond", "leuch\u00b7tet", "Dorp\u00b7m\u00fcl\u00b7ler", "aus", "den", "Wol\u00b7ken", ")", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOKOM", "ART", "NN", "VVFIN", "NE", "APPR", "ART", "NN", "$(", "$."], "meter": "+-+--++-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "F\u00fcr die andern: Kommi\u00df. F\u00fcr sich selber: sozial.", "tokens": ["F\u00fcr", "die", "an\u00b7dern", ":", "Kom\u00b7mi\u00df", ".", "F\u00fcr", "sich", "sel\u00b7ber", ":", "so\u00b7zi\u00b7al", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$.", "NN", "$.", "APPR", "PRF", "ADV", "$.", "ADV", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.8": {"text": "Das macht das Oberpersonal.", "tokens": ["Das", "macht", "das", "O\u00b7ber\u00b7per\u00b7so\u00b7nal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wir rufen ihm zu, so wie es da ist,", "tokens": ["Wir", "ru\u00b7fen", "ihm", "zu", ",", "so", "wie", "es", "da", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "KOKOM", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "ein Signal, das kein Proletarier vergi\u00dft:", "tokens": ["ein", "Sig\u00b7nal", ",", "das", "kein", "Pro\u00b7le\u00b7ta\u00b7rier", "ver\u00b7gi\u00dft", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.11": {"text": "Abfahren! Abfahren! Abfahren \u2013!", "tokens": ["Ab\u00b7fah\u00b7ren", "!", "Ab\u00b7fah\u00b7ren", "!", "Ab\u00b7fah\u00b7ren", "\u2013", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$(", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}}}}}