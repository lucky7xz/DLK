{"dta.poem.22748": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "52. Die Liebe hats erfunden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df GOtt gekreutzigt wird! da\u00df man jhn kan ver-", "tokens": ["Da\u00df", "Gott", "ge\u00b7kreut\u00b7zigt", "wird", "!", "da\u00df", "man", "jhn", "kan", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VVPP", "VAFIN", "$.", "KOUS", "PIS", "PPER", "VMFIN", "TRUNC"], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "wunden!", "tokens": ["wun\u00b7den", "!"], "token_info": ["word", "punct"], "pos": ["ADJA", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Da\u00df Er die Schmach vertr\u00e4gt/ die man jhm ange-", "tokens": ["Da\u00df", "Er", "die", "Schmach", "ver\u00b7tr\u00e4gt", "/", "die", "man", "jhm", "an\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$(", "PRELS", "PIS", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "than!", "tokens": ["than", "!"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Da\u00df Er solch\u2019 Angst au\u00dfsteht! un\u0303 da\u00df Er sterben kan!", "tokens": ["Da\u00df", "Er", "solch'", "Angst", "au\u00df\u00b7steht", "!", "u\u00f1", "da\u00df", "Er", "ster\u00b7ben", "kan", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$.", "KON", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Verwundere dich nicht/ die Liebe hats erfunden.", "tokens": ["Ver\u00b7wun\u00b7de\u00b7re", "dich", "nicht", "/", "die", "Lie\u00b7be", "hats", "er\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$(", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}