{"textgrid.poem.57876": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein ganz unglaubliches Ger\u00fccht", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein ganz unglaubliches Ger\u00fccht", "tokens": ["Ein", "ganz", "un\u00b7glaub\u00b7li\u00b7ches", "Ge\u00b7r\u00fccht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ist Montag zu mir gedrungen:", "tokens": ["Ist", "Mon\u00b7tag", "zu", "mir", "ge\u00b7drun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Simonsbrunnen, so sagte man mir,", "tokens": ["Der", "Si\u00b7mons\u00b7brun\u00b7nen", ",", "so", "sag\u00b7te", "man", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVFIN", "PIS", "PPER", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der habe am Sonntag gesprungen.", "tokens": ["Der", "ha\u00b7be", "am", "Sonn\u00b7tag", "ge\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Als eingeweiht man mit Prunk und Pomp", "tokens": ["Als", "ein\u00b7ge\u00b7weiht", "man", "mit", "Prunk", "und", "Pomp"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PIS", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Kirche am Goetheplatze,", "tokens": ["Die", "Kir\u00b7che", "am", "Goe\u00b7the\u00b7plat\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da renommierte Hannover auch", "tokens": ["Da", "re\u00b7nom\u00b7mier\u00b7te", "Han\u00b7no\u00b7ver", "auch"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "ADV"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Mit seinem Wasserschatze.", "tokens": ["Mit", "sei\u00b7nem", "Was\u00b7ser\u00b7schat\u00b7ze", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch nahm man die traurigen Fische fort", "tokens": ["Doch", "nahm", "man", "die", "trau\u00b7ri\u00b7gen", "Fi\u00b7sche", "fort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIS", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und hatte dazu seine Gr\u00fcnde,", "tokens": ["Und", "hat\u00b7te", "da\u00b7zu", "sei\u00b7ne", "Gr\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sonst h\u00e4tte der eifrige Tierschutzverein", "tokens": ["Sonst", "h\u00e4t\u00b7te", "der", "eif\u00b7ri\u00b7ge", "Tier\u00b7schutz\u00b7ver\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Geklagt \u00fcber Frevel und S\u00fcnde.", "tokens": ["Ge\u00b7klagt", "\u00fc\u00b7ber", "Fre\u00b7vel", "und", "S\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Im letzten Momente, da h\u00f6rte man", "tokens": ["Im", "letz\u00b7ten", "Mo\u00b7men\u00b7te", ",", "da", "h\u00f6r\u00b7te", "man"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "ADV", "VVFIN", "PIS"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.2": {"text": "Ein Mitglied erschrocken sagen:", "tokens": ["Ein", "Mit\u00b7glied", "er\u00b7schro\u00b7cken", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbdie Fische, nehmt doch die Fische fort,", "tokens": ["\u00bb", "die", "Fi\u00b7sche", ",", "nehmt", "doch", "die", "Fi\u00b7sche", "fort", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die k\u00f6nnen kein Wasser vertragen!\u00ab", "tokens": ["Die", "k\u00f6n\u00b7nen", "kein", "Was\u00b7ser", "ver\u00b7tra\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VMFIN", "PIAT", "NN", "VVFIN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}