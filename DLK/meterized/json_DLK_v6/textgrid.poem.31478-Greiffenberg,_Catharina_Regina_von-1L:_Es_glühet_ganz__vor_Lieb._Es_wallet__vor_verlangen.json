{"textgrid.poem.31478": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es gl\u00fchet ganz / vor Lieb. Es wallet / vor verlangen", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es gl\u00fchet ganz / vor Lieb. Es wallet / vor verlangen", "tokens": ["Es", "gl\u00fc\u00b7het", "ganz", "/", "vor", "Lieb", ".", "Es", "wal\u00b7let", "/", "vor", "ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$(", "APPR", "NN", "$.", "PPER", "VVFIN", "$(", "APPR", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "den Menschen guts zu thun. Es webet lauter Gunst.", "tokens": ["den", "Men\u00b7schen", "guts", "zu", "thun", ".", "Es", "we\u00b7bet", "lau\u00b7ter", "Gunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$.", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es h\u00e4nget voller Fr\u00fccht der Wunderschickungs Kunst.", "tokens": ["Es", "h\u00e4n\u00b7get", "vol\u00b7ler", "Fr\u00fccht", "der", "Wun\u00b7der\u00b7schi\u00b7ckungs", "Kunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Trost-und H\u00fclffes Heer / mit Freuden ausher drangen.", "tokens": ["Die", "Trost\u00b7\u00b7und", "H\u00fclf\u00b7fes", "Heer", "/", "mit", "Freu\u00b7den", "aus\u00b7her", "dran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "NN", "$(", "APPR", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit lauter G\u00fct' und Treu wird drinnen umgegangen.", "tokens": ["Mit", "lau\u00b7ter", "G\u00fct'", "und", "Treu", "wird", "drin\u00b7nen", "um\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Begierde flammet an / ein' Erzerbarmungs-Brunst:", "tokens": ["Be\u00b7gier\u00b7de", "flam\u00b7met", "an", "/", "ein'", "Er\u00b7zer\u00b7bar\u00b7mungs\u00b7Brunst", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$(", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "die laufft und steigt zum Ziel' ohn' Eitler Ehren dunst.", "tokens": ["die", "laufft", "und", "steigt", "zum", "Ziel'", "ohn'", "Eit\u00b7ler", "Eh\u00b7ren", "dunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "APPRART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein Gnad' ist / in die wett der andern / ausher gangen.", "tokens": ["Ein", "Gnad'", "ist", "/", "in", "die", "wett", "der", "an\u00b7dern", "/", "aus\u00b7her", "gan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$(", "APPR", "ART", "NN", "ART", "ADJA", "$(", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ey was? es ringen sich der mittel reiche M\u00e4ng' /", "tokens": ["Ey", "was", "?", "es", "rin\u00b7gen", "sich", "der", "mit\u00b7tel", "rei\u00b7che", "M\u00e4ng'", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "$.", "PPER", "VVFIN", "PRF", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "um / welches zu dem Heil der Menschen mehr solt dienen.", "tokens": ["um", "/", "wel\u00b7ches", "zu", "dem", "Heil", "der", "Men\u00b7schen", "mehr", "solt", "die\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "$(", "PRELS", "APPR", "ART", "NN", "ART", "NN", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "des Sohnes Menschgeburt / h\u00e4lt dort ihr Sieggepr\u00e4ng:", "tokens": ["des", "Soh\u00b7nes", "Menschge\u00b7burt", "/", "h\u00e4lt", "dort", "ihr", "Sieg\u00b7ge\u00b7pr\u00e4ng", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$(", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "sie war das K\u00f6stlichste / die S\u00fcnder zu vers\u00fchnen.", "tokens": ["sie", "war", "das", "K\u00f6st\u00b7lichs\u00b7te", "/", "die", "S\u00fcn\u00b7der", "zu", "ver\u00b7s\u00fch\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der H\u00f6chste sagt': Ich kan nun l\u00e4nger warten nicht:", "tokens": ["Der", "H\u00f6chs\u00b7te", "sagt'", ":", "Ich", "kan", "nun", "l\u00e4n\u00b7ger", "war\u00b7ten", "nicht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VMFIN", "ADV", "ADJD", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "vor Lieb-Erbarmung mir mein Herz / zu helffen / bricht!", "tokens": ["vor", "Lie\u00b7bEr\u00b7bar\u00b7mung", "mir", "mein", "Herz", "/", "zu", "helf\u00b7fen", "/", "bricht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "PPER", "PPOSAT", "NN", "$(", "PTKZU", "VVINF", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}