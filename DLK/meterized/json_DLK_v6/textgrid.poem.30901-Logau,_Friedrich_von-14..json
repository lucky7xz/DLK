{"textgrid.poem.30901": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Angelica ist wie ein Engel, und englisch sind auch ihre S\u00fcnden,", "tokens": ["An\u00b7ge\u00b7li\u00b7ca", "ist", "wie", "ein", "En\u00b7gel", ",", "und", "eng\u00b7lisch", "sind", "auch", "ih\u00b7re", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "KOKOM", "ART", "NN", "$,", "KON", "ADJD", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+------+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Wie Engel, die zu Teuffeln worden, voll Hoffart waren zu befinden.", "tokens": ["Wie", "En\u00b7gel", ",", "die", "zu", "Teuf\u00b7feln", "wor\u00b7den", ",", "voll", "Hof\u00b7fart", "wa\u00b7ren", "zu", "be\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "PRELS", "APPR", "NN", "VAPP", "$,", "ADJD", "NN", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}}}}