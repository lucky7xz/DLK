{"textgrid.poem.50941": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Flackre, fernes Licht im Tal,", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Flackre, fernes Licht im Tal,", "tokens": ["Flack\u00b7re", ",", "fer\u00b7nes", "Licht", "im", "Tal", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die Nacht mit leisem Blinken:", "tokens": ["Durch", "die", "Nacht", "mit", "lei\u00b7sem", "Blin\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch vor Morgen wird dein Strahl", "tokens": ["Noch", "vor", "Mor\u00b7gen", "wird", "dein", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Endlich in sich selbst versinken!", "tokens": ["End\u00b7lich", "in", "sich", "selbst", "ver\u00b7sin\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Rausche, singe, sch\u00f6ner Flu\u00df!", "tokens": ["Rau\u00b7sche", ",", "sin\u00b7ge", ",", "sch\u00f6\u00b7ner", "Flu\u00df", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dein Gesang wird fortbestehen;", "tokens": ["Dein", "Ge\u00b7sang", "wird", "fort\u00b7be\u00b7ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber jede Welle mu\u00df", "tokens": ["A\u00b7ber", "je\u00b7de", "Wel\u00b7le", "mu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Endlich doch im Meer vergehen.", "tokens": ["End\u00b7lich", "doch", "im", "Meer", "ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nachtviolen, s\u00fc\u00df und stark", "tokens": ["Nacht\u00b7vi\u00b7o\u00b7len", ",", "s\u00fc\u00df", "und", "stark"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Duftet ihr durch diese Lauben;", "tokens": ["Duf\u00b7tet", "ihr", "durch", "die\u00b7se", "Lau\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Oh, wie wi\u00dft das feinste Mark", "tokens": ["Oh", ",", "wie", "wi\u00dft", "das", "feins\u00b7te", "Mark"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr der Erde schnell zu rauben!", "tokens": ["Ihr", "der", "Er\u00b7de", "schnell", "zu", "rau\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Von der warmen Nacht gek\u00fc\u00dft,", "tokens": ["Von", "der", "war\u00b7men", "Nacht", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wi\u00dft ihr schnell es auszuhauchen,", "tokens": ["Wi\u00dft", "ihr", "schnell", "es", "aus\u00b7zu\u00b7hau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPER", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eh ihr selber wieder m\u00fc\u00dft", "tokens": ["Eh", "ihr", "sel\u00b7ber", "wie\u00b7der", "m\u00fc\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eure K\u00f6pflein untertauchen!", "tokens": ["Eu\u00b7re", "K\u00f6\u00b7pflein", "un\u00b7ter\u00b7tau\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aus dem tiefen blauen Raum", "tokens": ["Aus", "dem", "tie\u00b7fen", "blau\u00b7en", "Raum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Perlt ihr leuchtend, goldne Sonnen,", "tokens": ["Perlt", "ihr", "leuch\u00b7tend", ",", "gold\u00b7ne", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kommt und schwindet, wie ein Traum;", "tokens": ["Kommt", "und", "schwin\u00b7det", ",", "wie", "ein", "Traum", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$,", "PWAV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch gef\u00fcllt bleibt stets der Bronnen.", "tokens": ["Doch", "ge\u00b7f\u00fcllt", "bleibt", "stets", "der", "Bron\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und nur du, mein armes Herz,", "tokens": ["Und", "nur", "du", ",", "mein", "ar\u00b7mes", "Herz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Du allein willst ewig schlagen,", "tokens": ["Du", "al\u00b7lein", "willst", "e\u00b7wig", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Lust und deinen Schmerz", "tokens": ["Dei\u00b7ne", "Lust", "und", "dei\u00b7nen", "Schmerz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ewig durch die Himmel tragen?", "tokens": ["E\u00b7wig", "durch", "die", "Him\u00b7mel", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Andre Blumen, andre Wellen,", "tokens": ["And\u00b7re", "Blu\u00b7men", ",", "and\u00b7re", "Wel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Andre Sterne, andre Herzen,", "tokens": ["And\u00b7re", "Ster\u00b7ne", ",", "and\u00b7re", "Her\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Andre Freuden, andre Schmerzen", "tokens": ["And\u00b7re", "Freu\u00b7den", ",", "and\u00b7re", "Schmer\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werden unersch\u00f6pflich quellen", "tokens": ["Wer\u00b7den", "un\u00b7er\u00b7sch\u00f6pf\u00b7lich", "quel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und, eh wir noch gar verglommen,", "tokens": ["Und", ",", "eh", "wir", "noch", "gar", "ver\u00b7glom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ganz uns auszul\u00f6schen kommen.", "tokens": ["Ganz", "uns", "aus\u00b7zu\u00b7l\u00f6\u00b7schen", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ewig ist, begreifst es du,", "tokens": ["E\u00b7wig", "ist", ",", "be\u00b7greifst", "es", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sehnend Herz? nur deine Ruh!", "tokens": ["Seh\u00b7nend", "Herz", "?", "nur", "dei\u00b7ne", "Ruh", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$.", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}