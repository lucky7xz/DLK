{"textgrid.poem.37772": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Das Heidenm\u00e4dchen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Sohn des Himmels und der Erde", "tokens": ["Der", "Sohn", "des", "Him\u00b7mels", "und", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sah, aus der Weihnacht Abendroth,", "tokens": ["Sah", ",", "aus", "der", "Weih\u00b7nacht", "A\u00b7ben\u00b7droth", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein sch\u00f6nes Kind bei einer Heerde,", "tokens": ["Ein", "sch\u00f6\u00b7nes", "Kind", "bei", "ei\u00b7ner", "Heer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und keiner da Geschenke bot.", "tokens": ["Und", "kei\u00b7ner", "da", "Ge\u00b7schen\u00b7ke", "bot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Glaube war noch nicht gedrungen", "tokens": ["Der", "Glau\u00b7be", "war", "noch", "nicht", "ge\u00b7drun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKNEG", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu diesen sp\u00e4t erschaffnen Aun,", "tokens": ["Zu", "die\u00b7sen", "sp\u00e4t", "er\u00b7schaff\u00b7nen", "Aun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn von den Felsen ganz umschlungen,", "tokens": ["Denn", "von", "den", "Fel\u00b7sen", "ganz", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Konnt' wenig Sonne \u00fcberschaun.", "tokens": ["Konnt'", "we\u00b7nig", "Son\u00b7ne", "\u00fc\u00b7bersc\u00b7haun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Doch freut die Kleine sich am Lichte,", "tokens": ["Doch", "freut", "die", "Klei\u00b7ne", "sich", "am", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das neu durch Felsenschatten strahlt,", "tokens": ["Das", "neu", "durch", "Fel\u00b7sen\u00b7schat\u00b7ten", "strahlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie hat so gar ein lieb Gesichte,", "tokens": ["Sie", "hat", "so", "gar", "ein", "lieb", "Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein edles Blut die Wangen malt.", "tokens": ["Ein", "ed\u00b7les", "Blut", "die", "Wan\u00b7gen", "malt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie mu\u00df im Lichte zierlich springen", "tokens": ["Sie", "mu\u00df", "im", "Lich\u00b7te", "zier\u00b7lich", "sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So glatt und weich schien ihr das Gr\u00fcn", "tokens": ["So", "glatt", "und", "weich", "schien", "ihr", "das", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und zu dem holden Echo singen;", "tokens": ["Und", "zu", "dem", "hol\u00b7den", "E\u00b7cho", "sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Herr will sie zum Glauben ziehn.", "tokens": ["Der", "Herr", "will", "sie", "zum", "Glau\u00b7ben", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Es sprengt der Herr mit Strahlenz\u00fcgen", "tokens": ["Es", "sprengt", "der", "Herr", "mit", "Strah\u00b7len\u00b7z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Ziegen ihr weit auf den Fels,", "tokens": ["Die", "Zie\u00b7gen", "ihr", "weit", "auf", "den", "Fels", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie klettert sorgsam nach den Ziegen,", "tokens": ["Sie", "klet\u00b7tert", "sorg\u00b7sam", "nach", "den", "Zie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er zeigt den Weg im Blick des Hells.", "tokens": ["Er", "zeigt", "den", "Weg", "im", "Blick", "des", "Hells", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Hin \u00fcber die bemoosten Platten", "tokens": ["Hin", "\u00fc\u00b7ber", "die", "be\u00b7moos\u00b7ten", "Plat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie wagt sich, schaut ein andres Land,", "tokens": ["Sie", "wagt", "sich", ",", "schaut", "ein", "and\u00b7res", "Land", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da will ihr Herz vor Schreck ermatten,", "tokens": ["Da", "will", "ihr", "Herz", "vor", "Schreck", "er\u00b7mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn alles scheint vor ihr in Brand.", "tokens": ["Denn", "al\u00b7les", "scheint", "vor", "ihr", "in", "Brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "PPOSAT", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Da stehen tausend kleine Tische", "tokens": ["Da", "ste\u00b7hen", "tau\u00b7send", "klei\u00b7ne", "Ti\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit bunten Lichtern rings besteckt,", "tokens": ["Mit", "bun\u00b7ten", "Lich\u00b7tern", "rings", "be\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Brodt und Wein steht im Gemische,", "tokens": ["Und", "Brodt", "und", "Wein", "steht", "im", "Ge\u00b7mi\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sch\u00f6n Me\u00dfgewand die Tische deckt.", "tokens": ["Sch\u00f6n", "Me\u00df\u00b7ge\u00b7wand", "die", "Ti\u00b7sche", "deckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und statt der Puppen heilge Bilder", "tokens": ["Und", "statt", "der", "Pup\u00b7pen", "heil\u00b7ge", "Bil\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bewohnen dieses Paradies,", "tokens": ["Be\u00b7woh\u00b7nen", "die\u00b7ses", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Kinder ziehen sanft und milder", "tokens": ["Und", "Kin\u00b7der", "zie\u00b7hen", "sanft", "und", "mil\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sehn wie dies so herrlich lie\u00df.", "tokens": ["Und", "sehn", "wie", "dies", "so", "herr\u00b7lich", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "KOKOM", "PDS", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Das M\u00e4dchen sieht's und meint ihr eigen,", "tokens": ["Das", "M\u00e4d\u00b7chen", "sieht's", "und", "meint", "ihr", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was ihr kein andrer wehren will,", "tokens": ["Was", "ihr", "kein", "an\u00b7drer", "weh\u00b7ren", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PIAT", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch bald sich viele Knaben zeigen,", "tokens": ["Doch", "bald", "sich", "vie\u00b7le", "Kna\u00b7ben", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PRF", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die bitten drum in Demuth still.", "tokens": ["Die", "bit\u00b7ten", "drum", "in", "De\u00b7muth", "still", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PAV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Der eine will ihr H\u00e4ndchen k\u00fcssen,", "tokens": ["Der", "ei\u00b7ne", "will", "ihr", "H\u00e4nd\u00b7chen", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem wirft sie Aepfel ins Gesicht;", "tokens": ["Dem", "wirft", "sie", "A\u00b7e\u00b7pfel", "ins", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der will sie sch\u00f6n mit Reden gr\u00fc\u00dfen,", "tokens": ["Der", "will", "sie", "sch\u00f6n", "mit", "Re\u00b7den", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADJD", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem h\u00e4lt sie in den Mund das Licht.", "tokens": ["Dem", "h\u00e4lt", "sie", "in", "den", "Mund", "das", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Doch einer kommt mit Witz zu streiten,", "tokens": ["Doch", "ei\u00b7ner", "kommt", "mit", "Witz", "zu", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da nimmt sie alle heilgen Bild',", "tokens": ["Da", "nimmt", "sie", "al\u00b7le", "heil\u00b7gen", "Bild'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Beginnt sie n\u00e4rrisch umzukleiden,", "tokens": ["Be\u00b7ginnt", "sie", "n\u00e4r\u00b7risch", "um\u00b7zu\u00b7klei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verliert sie dann im Spiele wild.", "tokens": ["Ver\u00b7liert", "sie", "dann", "im", "Spie\u00b7le", "wild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Was so viel tausend Engel s\u00e4ten,", "tokens": ["Was", "so", "viel", "tau\u00b7send", "En\u00b7gel", "s\u00e4\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zerst\u00f6rt das Kind aus Unverstand,", "tokens": ["Zer\u00b7st\u00f6rt", "das", "Kind", "aus", "Un\u00b7ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Warum viel fromme Kinder beten,", "tokens": ["Wa\u00b7rum", "viel", "from\u00b7me", "Kin\u00b7der", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geschenk des Herren ist ihr Tand.", "tokens": ["Ge\u00b7schenk", "des", "Her\u00b7ren", "ist", "ihr", "Tand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Da kam der Herr zu ihr gegangen,", "tokens": ["Da", "kam", "der", "Herr", "zu", "ihr", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als armes Kindlein angethan,", "tokens": ["Als", "ar\u00b7mes", "Kin\u00b7dlein", "an\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und th\u00e4t nach etwas nur verlangen,", "tokens": ["Und", "th\u00e4t", "nach", "et\u00b7was", "nur", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was sie verworfen und verthan.", "tokens": ["Was", "sie", "ver\u00b7wor\u00b7fen", "und", "ver\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Da fand sie leer die reichen Tische,", "tokens": ["Da", "fand", "sie", "leer", "die", "rei\u00b7chen", "Ti\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Lichter waren fast verbrannt,", "tokens": ["Die", "Lich\u00b7ter", "wa\u00b7ren", "fast", "ver\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "