{"dta.poem.5511": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gedancken bey einer Mond-Finsterni\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da ich den Mond verfinstert seh',", "tokens": ["Da", "ich", "den", "Mond", "ver\u00b7fins\u00b7tert", "seh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Versp\u00fchr ich, da\u00df, in meinem Hertzen,", "tokens": ["Ver\u00b7sp\u00fchr", "ich", ",", "da\u00df", ",", "in", "mei\u00b7nem", "Hert\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Aus seiner Finsterni\u00df ein Licht entsteh\u2019", "tokens": ["Aus", "sei\u00b7ner", "Fins\u00b7ter\u00b7ni\u00df", "ein", "Licht", "ent\u00b7steh'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das keine falbe Schatten schw\u00e4rtzen.", "tokens": ["Das", "kei\u00b7ne", "fal\u00b7be", "Schat\u00b7ten", "schw\u00e4rt\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie zeigt mir \u00fcberzeuglich klar", "tokens": ["Sie", "zeigt", "mir", "\u00fc\u00b7berz\u00b7eug\u00b7lich", "klar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Wei\u00dfheit G\u00f6ttlicher Regierung,", "tokens": ["Die", "Wei\u00df\u00b7heit", "G\u00f6tt\u00b7li\u00b7cher", "Re\u00b7gie\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und macht zugleich mir offenbahr", "tokens": ["Und", "macht", "zu\u00b7gleich", "mir", "of\u00b7fen\u00b7bahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Richtigkeiten seiner F\u00fchrung;", "tokens": ["Die", "Rich\u00b7tig\u00b7kei\u00b7ten", "sei\u00b7ner", "F\u00fch\u00b7rung", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Da nicht nur, um kein eintzigs Haar,", "tokens": ["Da", "nicht", "nur", ",", "um", "kein", "eint\u00b7zigs", "Haar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "$,", "KOUI", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die Lichter, die viel tausend Jahr,", "tokens": ["Die", "Lich\u00b7ter", ",", "die", "viel", "tau\u00b7send", "Jahr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "In ungest\u00f6rter Ordnung, gehn,", "tokens": ["In", "un\u00b7ge\u00b7st\u00f6r\u00b7ter", "Ord\u00b7nung", ",", "gehn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sich aus den festen Angeln drehn;", "tokens": ["Sich", "aus", "den", "fes\u00b7ten", "An\u00b7geln", "drehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Auch da\u00df der Sch\u00f6pfer uns so gar,", "tokens": ["Auch", "da\u00df", "der", "Sch\u00f6p\u00b7fer", "uns", "so", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Solch eine F\u00e4higkeit geschencket,", "tokens": ["Solch", "ei\u00b7ne", "F\u00e4\u00b7hig\u00b7keit", "ge\u00b7schen\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "So viele Jahr vorher zu sehn,", "tokens": ["So", "vie\u00b7le", "Jahr", "vor\u00b7her", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Wie alles sich so richtig lencket.", "tokens": ["Wie", "al\u00b7les", "sich", "so", "rich\u00b7tig", "len\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PRF", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "HeRR! la\u00df uns des Verstandes Gaben,", "tokens": ["HeRR", "!", "la\u00df", "uns", "des", "Ver\u00b7stan\u00b7des", "Ga\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVIMP", "PPER", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Das Pfund, das du in uns gesenckt,", "tokens": ["Das", "Pfund", ",", "das", "du", "in", "uns", "ge\u00b7senckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gebrauchen und es nicht vergraben!", "tokens": ["Ge\u00b7brau\u00b7chen", "und", "es", "nicht", "ver\u00b7gra\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df uns, so oft wir, wie so sch\u00f6n", "tokens": ["La\u00df", "uns", ",", "so", "oft", "wir", ",", "wie", "so", "sch\u00f6n"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "ADV", "ADV", "PPER", "$,", "PWAV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Monden-Licht uns scheinet, sehn,", "tokens": ["Das", "Mon\u00b7den\u00b7Licht", "uns", "schei\u00b7net", ",", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit allen Kr\u00e4ften des Gesichts", "tokens": ["Mit", "al\u00b7len", "Kr\u00e4f\u00b7ten", "des", "Ge\u00b7sichts"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zu ihm; von ihm zur Sonnen steigen,", "tokens": ["Zu", "ihm", ";", "von", "ihm", "zur", "Son\u00b7nen", "stei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$.", "APPR", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zur wahren Quelle seines Lichts;", "tokens": ["Zur", "wah\u00b7ren", "Quel\u00b7le", "sei\u00b7nes", "Lichts", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da denn der Seele sich wird zeigen,", "tokens": ["Da", "denn", "der", "See\u00b7le", "sich", "wird", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PRF", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Da\u00df man, mit Recht, die Sonne selbst wird k\u00f6nnen", "tokens": ["Da\u00df", "man", ",", "mit", "Recht", ",", "die", "Son\u00b7ne", "selbst", "wird", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "$,", "APPR", "NN", "$,", "ART", "NN", "ADV", "VAFIN", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Des Sch\u00f6pfers Mond, ja seinen Schatten, nennen.", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "Mond", ",", "ja", "sei\u00b7nen", "Schat\u00b7ten", ",", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADV", "PPOSAT", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}