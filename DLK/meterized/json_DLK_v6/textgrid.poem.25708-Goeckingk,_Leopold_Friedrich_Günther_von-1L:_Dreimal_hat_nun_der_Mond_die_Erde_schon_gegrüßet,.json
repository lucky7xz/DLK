{"textgrid.poem.25708": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dreimal hat nun der Mond die Erde schon gegr\u00fc\u00dfet,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dreimal hat nun der Mond die Erde schon gegr\u00fc\u00dfet,", "tokens": ["Drei\u00b7mal", "hat", "nun", "der", "Mond", "die", "Er\u00b7de", "schon", "ge\u00b7gr\u00fc\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Seit sich der Schmerz um eure Nerven schlang,", "tokens": ["Seit", "sich", "der", "Schmerz", "um", "eu\u00b7re", "Ner\u00b7ven", "schlang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Thr\u00e4nen, die ihr sonst so fest in sie verschlie\u00dfet,", "tokens": ["Und", "Thr\u00e4\u00b7nen", ",", "die", "ihr", "sonst", "so", "fest", "in", "sie", "ver\u00b7schlie\u00b7\u00dfet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Aus euren Dr\u00fcsen rang.", "tokens": ["Aus", "eu\u00b7ren", "Dr\u00fc\u00b7sen", "rang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr blickt auf meine Hand, da\u00df sie vor Sonnenpfeilen", "tokens": ["Ihr", "blickt", "auf", "mei\u00b7ne", "Hand", ",", "da\u00df", "sie", "vor", "Son\u00b7nen\u00b7pfei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mitleidig euch zum Schilde werden soll,", "tokens": ["Mit\u00b7lei\u00b7dig", "euch", "zum", "Schil\u00b7de", "wer\u00b7den", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "APPRART", "NN", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und auf den gr\u00fcnen Schirm, die Feuerglut zu theilen,", "tokens": ["Und", "auf", "den", "gr\u00fc\u00b7nen", "Schirm", ",", "die", "Feu\u00b7er\u00b7glut", "zu", "thei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die aus dem Scharlach quoll.", "tokens": ["Die", "aus", "dem", "Schar\u00b7lach", "quoll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was habt ihr doch gethan, ihr M\u00e4rtyrer! ihr Augen!", "tokens": ["Was", "habt", "ihr", "doch", "ge\u00b7than", ",", "ihr", "M\u00e4r\u00b7ty\u00b7rer", "!", "ihr", "Au\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VVPP", "$,", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Lie\u00df ich euch meines Feindes Ungl\u00fcck freun?", "tokens": ["Lie\u00df", "ich", "euch", "mei\u00b7nes", "Fein\u00b7des", "Un\u00b7gl\u00fcck", "freun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus euch, ein M\u00e4dchen Gift f\u00fcr ihre Tugend saugen?", "tokens": ["Aus", "euch", ",", "ein", "M\u00e4d\u00b7chen", "Gift", "f\u00fcr", "ih\u00b7re", "Tu\u00b7gend", "sau\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist's so: dann trocknet ein!", "tokens": ["Ist's", "so", ":", "dann", "trock\u00b7net", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$.", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie? oder lockte je des Wuchrers goldner Wagen", "tokens": ["Wie", "?", "o\u00b7der", "lock\u00b7te", "je", "des", "Wuc\u00b7hrers", "gold\u00b7ner", "Wa\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "KON", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur einen Blick voll Neid von euch heraus?", "tokens": ["Nur", "ei\u00b7nen", "Blick", "voll", "Neid", "von", "euch", "he\u00b7raus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und konntet nicht den Glanz des Ordenssterns ertragen?", "tokens": ["Und", "konn\u00b7tet", "nicht", "den", "Glanz", "des", "Or\u00b7denss\u00b7terns", "er\u00b7tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "W\u00fc\u00dft ich's: Ich ri\u00df' euch aus!", "tokens": ["W\u00fc\u00dft", "ich's", ":", "Ich", "ri\u00df'", "euch", "aus", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$.", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das nicht! Ihr habet oft mit Traurigen geweinet,", "tokens": ["Das", "nicht", "!", "Ihr", "ha\u00b7bet", "oft", "mit", "Trau\u00b7ri\u00b7gen", "ge\u00b7wei\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "$.", "PPER", "VAFIN", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mit den Fr\u00f6hlichen so gern gelacht!", "tokens": ["Und", "mit", "den", "Fr\u00f6h\u00b7li\u00b7chen", "so", "gern", "ge\u00b7lacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und nun ist euch der Blick nach beiden dick verz\u00e4unet,", "tokens": ["Und", "nun", "ist", "euch", "der", "Blick", "nach", "bei\u00b7den", "dick", "ver\u00b7z\u00e4u\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "APPR", "PIAT", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und nun kommt solche Nacht?", "tokens": ["Und", "nun", "kommt", "sol\u00b7che", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sprich, Sonne! sahst du nicht, wenn aller Augen schliefen,", "tokens": ["Sprich", ",", "Son\u00b7ne", "!", "sahst", "du", "nicht", ",", "wenn", "al\u00b7ler", "Au\u00b7gen", "schlie\u00b7fen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$.", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die meinen schon auf unsers Harzes H\u00f6hn,", "tokens": ["Die", "mei\u00b7nen", "schon", "auf", "un\u00b7sers", "Har\u00b7zes", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie Z\u00e4hren dir entz\u00fcckt darin entgegen liefen,", "tokens": ["Wie", "Z\u00e4h\u00b7ren", "dir", "ent\u00b7z\u00fcckt", "da\u00b7rin", "ent\u00b7ge\u00b7gen", "lie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "VVFIN", "PAV", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Im Morgenschimmer stehn?", "tokens": ["Im", "Mor\u00b7gen\u00b7schim\u00b7mer", "stehn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Verweiltest du dich nicht noch gern am Horizonte,", "tokens": ["Ver\u00b7weil\u00b7test", "du", "dich", "nicht", "noch", "gern", "am", "Ho\u00b7ri\u00b7zon\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn ich am Bach' zu Laurens F\u00fc\u00dfen sa\u00df,", "tokens": ["Wenn", "ich", "am", "Bach'", "zu", "Lau\u00b7rens", "F\u00fc\u00b7\u00dfen", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo dieses Auge selbst, so einsam! sprechen konnte,", "tokens": ["Wo", "die\u00b7ses", "Au\u00b7ge", "selbst", ",", "so", "ein\u00b7sam", "!", "spre\u00b7chen", "konn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "ADV", "$,", "ADV", "ADJD", "$.", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und nur in ihrem las?", "tokens": ["Und", "nur", "in", "ih\u00b7rem", "las", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Jetzt aber meidet scheu mein Auge deine Strahlen,", "tokens": ["Jetzt", "a\u00b7ber", "mei\u00b7det", "scheu", "mein", "Au\u00b7ge", "dei\u00b7ne", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADJD", "PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es fl\u00fcchtet schon vor meiner Lampe Schein.", "tokens": ["Es", "fl\u00fcch\u00b7tet", "schon", "vor", "mei\u00b7ner", "Lam\u00b7pe", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O! sieh mich nicht mehr an! du mehrest meine Qualen!", "tokens": ["O", "!", "sieh", "mich", "nicht", "mehr", "an", "!", "du", "meh\u00b7rest", "mei\u00b7ne", "Qua\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVIMP", "PPER", "PTKNEG", "ADV", "PTKVZ", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "La\u00df mich in Nacht allein.", "tokens": ["La\u00df", "mich", "in", "Nacht", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Doch, la\u00df nur, selbst mit Schmerz mich Laurens Antlitz sehen!", "tokens": ["Doch", ",", "la\u00df", "nur", ",", "selbst", "mit", "Schmerz", "mich", "Lau\u00b7rens", "Ant\u00b7litz", "se\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVIMP", "ADV", "$,", "ADV", "APPR", "NN", "PPER", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "(was seh' ich sonst liebreichers au\u00dfer ihr?)", "tokens": ["(", "was", "seh'", "ich", "sonst", "lieb\u00b7rei\u00b7chers", "au\u00b7\u00dfer", "ihr", "?", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht Einen Seufzer soll der Wind dann mir entwehen,", "tokens": ["Nicht", "Ei\u00b7nen", "Seuf\u00b7zer", "soll", "der", "Wind", "dann", "mir", "ent\u00b7we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VMFIN", "ART", "NN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Entziehst gleich du dich mir.", "tokens": ["Ent\u00b7ziehst", "gleich", "du", "dich", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "PRF", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}