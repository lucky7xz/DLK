{"textgrid.poem.59499": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herrliche Sonne, du leuchtest hier oben,", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herrliche Sonne, du leuchtest hier oben,", "tokens": ["Herr\u00b7li\u00b7che", "Son\u00b7ne", ",", "du", "leuch\u00b7test", "hier", "o\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Mond und Gestirne, ihr wandelt herab \u2013", "tokens": ["Mond", "und", "Ge\u00b7stir\u00b7ne", ",", "ihr", "wan\u00b7delt", "her\u00b7ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PPER", "VVFIN", "ADV", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Weil ich denn liebe, so will ich euch loben,", "tokens": ["Weil", "ich", "denn", "lie\u00b7be", ",", "so", "will", "ich", "euch", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,", "ADV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Fr\u00fche bescheint ihr des Schlummernden Grab.", "tokens": ["Fr\u00fc\u00b7he", "be\u00b7scheint", "ihr", "des", "Schlum\u00b7mern\u00b7den", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Singe mir, heilige Stimme der Liebe,", "tokens": ["Sin\u00b7ge", "mir", ",", "hei\u00b7li\u00b7ge", "Stim\u00b7me", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Nachtigallkehle, den kurzen Gesang,", "tokens": ["Nach\u00b7ti\u00b7gall\u00b7keh\u00b7le", ",", "den", "kur\u00b7zen", "Ge\u00b7sang", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Riesle mir, murmelnde Quelle, bis tr\u00fcbe", "tokens": ["Ries\u00b7le", "mir", ",", "mur\u00b7meln\u00b7de", "Quel\u00b7le", ",", "bis", "tr\u00fc\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "KOUS", "ADJA"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Lethe mir reichet den traurigen Trank;", "tokens": ["Le\u00b7the", "mir", "rei\u00b7chet", "den", "trau\u00b7ri\u00b7gen", "Trank", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Spielet, ihr G\u00f6tter der Freude, ihr losen", "tokens": ["Spie\u00b7let", ",", "ihr", "G\u00f6t\u00b7ter", "der", "Freu\u00b7de", ",", "ihr", "lo\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "ART", "NN", "$,", "PPER", "VVINF"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Freundlichen Knaben, im holden Verein,", "tokens": ["Freund\u00b7li\u00b7chen", "Kna\u00b7ben", ",", "im", "hol\u00b7den", "Ver\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Kr\u00e4nzet die rosige Stirne mit Rosen,", "tokens": ["Kr\u00e4n\u00b7zet", "die", "ro\u00b7si\u00b7ge", "Stir\u00b7ne", "mit", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "F\u00fcllet den Becher mit goldenem Wein!", "tokens": ["F\u00fcl\u00b7let", "den", "Be\u00b7cher", "mit", "gol\u00b7de\u00b7nem", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "F\u00fchrt in der Sch\u00f6nheit umschmeichelnden Banden,", "tokens": ["F\u00fchrt", "in", "der", "Sch\u00f6n\u00b7heit", "um\u00b7schmei\u00b7cheln\u00b7den", "Ban\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Musen und Grazien, f\u00fchrt mich die Bahn.", "tokens": ["Mu\u00b7sen", "und", "Gra\u00b7zi\u00b7en", ",", "f\u00fchrt", "mich", "die", "Bahn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Reich mit den Bl\u00fcten Mnemosynens landen", "tokens": ["Reich", "mit", "den", "Bl\u00fc\u00b7ten", "Mne\u00b7mo\u00b7sy\u00b7nens", "lan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NE", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Eure Geweihten im stygischen Kahn.", "tokens": ["Eu\u00b7re", "Ge\u00b7weih\u00b7ten", "im", "sty\u00b7gi\u00b7schen", "Kahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}