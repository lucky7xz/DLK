{"dta.poem.3844": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Auf drey eintzige Kinder ihrer Eltern/  \n so zugleich begraben worden.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.85", "sv:0.14"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "So geh, du Kleeblatt frommer S\u00f6hne,", "tokens": ["So", "geh", ",", "du", "Klee\u00b7blatt", "from\u00b7mer", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPER", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ins ewig frohe Canaan,", "tokens": ["Ins", "e\u00b7wig", "fro\u00b7he", "Ca\u00b7naan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "ADJA", "NE", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wir singen Jammer-volle Th\u00f6ne,", "tokens": ["Wir", "sin\u00b7gen", "Jam\u00b7mer\u00b7vol\u00b7le", "Th\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da GOtt den harten Ri\u00df gethan.", "tokens": ["Da", "Gott", "den", "har\u00b7ten", "Ri\u00df", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jhr eintzig Hoffnungs-vollen Zweige,", "tokens": ["Ihr", "eint\u00b7zig", "Hoff\u00b7nungs\u00b7vol\u00b7len", "Zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wir m\u00fcssen zwar bey eurer Leiche", "tokens": ["Wir", "m\u00fcs\u00b7sen", "zwar", "bey", "eu\u00b7rer", "Lei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Viel traurige Cypressen streun;", "tokens": ["Viel", "trau\u00b7ri\u00b7ge", "Cyp\u00b7res\u00b7sen", "streun", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Doch GOtt wird unser Tr\u00f6ster seyn.", "tokens": ["Doch", "Gott", "wird", "un\u00b7ser", "Tr\u00f6s\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}