{"textgrid.poem.37771": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Heldenlauf", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frisch zu! das ist mein erster Ruf,", "tokens": ["Frisch", "zu", "!", "das", "ist", "mein", "ers\u00b7ter", "Ruf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$.", "PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Will rasch die Welt beschau'n:", "tokens": ["Will", "rasch", "die", "Welt", "be\u00b7schau'n", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie sie der frische Morgen schuf,", "tokens": ["Wie", "sie", "der", "fri\u00b7sche", "Mor\u00b7gen", "schuf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mich daran erbau'n.", "tokens": ["Und", "mich", "da\u00b7ran", "er\u00b7bau'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["KON", "PPER", "PAV", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich schwing' mich auf mein Ritterpferd", "tokens": ["Ich", "schwing'", "mich", "auf", "mein", "Rit\u00b7ter\u00b7pferd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und werfe weg den Zaum,", "tokens": ["Und", "wer\u00b7fe", "weg", "den", "Zaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So rennt und gras't es auf der Erd'", "tokens": ["So", "rennt", "und", "gras't", "es", "auf", "der", "Erd'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bei meinem Morgentraum.", "tokens": ["Bei", "mei\u00b7nem", "Mor\u00b7gen\u00b7traum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Geht auch mein Pferdchen mit mir durch,", "tokens": ["Geht", "auch", "mein", "Pferd\u00b7chen", "mit", "mir", "durch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es geht nicht zu geschwind,", "tokens": ["Es", "geht", "nicht", "zu", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn unten dr\u00f6hnt noch feste Burg", "tokens": ["Denn", "un\u00b7ten", "dr\u00f6hnt", "noch", "fes\u00b7te", "Burg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und oben saust der Wind;", "tokens": ["Und", "o\u00b7ben", "saust", "der", "Wind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Wind, der durch die Haare saust,", "tokens": ["Der", "Wind", ",", "der", "durch", "die", "Haa\u00b7re", "saust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der singt mein Morgenlied:", "tokens": ["Der", "singt", "mein", "Mor\u00b7gen\u00b7lied", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vom Adler, der sich kaum gemaust", "tokens": ["Vom", "Ad\u00b7ler", ",", "der", "sich", "kaum", "ge\u00b7maust"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PRELS", "PRF", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und schon zur Sonne zieht.", "tokens": ["Und", "schon", "zur", "Son\u00b7ne", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gern dr\u00e4ng' ich in den Morgenglanz,", "tokens": ["Gern", "dr\u00e4ng'", "ich", "in", "den", "Mor\u00b7gen\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch steh ich schon am Meer;", "tokens": ["Doch", "steh", "ich", "schon", "am", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O wie so golden eins und ganz", "tokens": ["O", "wie", "so", "gol\u00b7den", "eins", "und", "ganz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADV", "ADJD", "CARD", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Scheint Meer und Himmel her;", "tokens": ["Scheint", "Meer", "und", "Him\u00b7mel", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie einer Muschel offnes Haus", "tokens": ["Wie", "ei\u00b7ner", "Mu\u00b7schel", "off\u00b7nes", "Haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist Meer und Luft vereint", "tokens": ["Ist", "Meer", "und", "Luft", "ver\u00b7eint"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "KON", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und eine Perle steigt heraus:", "tokens": ["Und", "ei\u00b7ne", "Per\u00b7le", "steigt", "he\u00b7raus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Schifflein so erscheint.", "tokens": ["Ein", "Schif\u00b7flein", "so", "er\u00b7scheint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Vom Schiffe treten auf den Strand", "tokens": ["Vom", "Schif\u00b7fe", "tre\u00b7ten", "auf", "den", "Strand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zwei Frau'n von edlem Blick,", "tokens": ["Zwei", "Frau'n", "von", "ed\u00b7lem", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als w\u00e4ren sie von heil'gem Stand,", "tokens": ["Als", "w\u00e4\u00b7ren", "sie", "von", "heil'\u00b7gem", "Stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Zeichen vom Geschick.", "tokens": ["Und", "Zei\u00b7chen", "vom", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Eine stets voraus gesehn,", "tokens": ["Die", "Ei\u00b7ne", "stets", "vo\u00b7raus", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Tritt erst in meinen Lauf;", "tokens": ["Tritt", "erst", "in", "mei\u00b7nen", "Lauf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es mi\u00dft ihr Schritt den Weg im Gehn,", "tokens": ["Es", "mi\u00dft", "ihr", "Schritt", "den", "Weg", "im", "Gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie schreibt sich Alles auf.", "tokens": ["Sie", "schreibt", "sich", "Al\u00b7les", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIS", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der tret' ich neckend ins Geheg,", "tokens": ["Der", "tret'", "ich", "ne\u00b7ckend", "ins", "Ge\u00b7heg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Sie reizt den Eigensinn;", "tokens": ["Sie", "reizt", "den", "Ei\u00b7gen\u00b7sinn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach, w\u00e4re ich nur wieder weg,", "tokens": ["Ach", ",", "w\u00e4\u00b7re", "ich", "nur", "wie\u00b7der", "weg", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sonst bin ich wahrlich hin.", "tokens": ["Sonst", "bin", "ich", "wahr\u00b7lich", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vom Pferde zieht sie mich herab,", "tokens": ["Vom", "Pfer\u00b7de", "zieht", "sie", "mich", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PRF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Schleppe trag' ich ihr;", "tokens": ["Die", "Schlep\u00b7pe", "trag'", "ich", "ihr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ihr Auge tief, so wie ein Grab,", "tokens": ["Ihr", "Au\u00b7ge", "tief", ",", "so", "wie", "ein", "Grab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$,", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Blickt selten nur nach mir.", "tokens": ["Blickt", "sel\u00b7ten", "nur", "nach", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie nennet sich die Wissenschaft,", "tokens": ["Sie", "nen\u00b7net", "sich", "die", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie nimmt mich in die Lehr',", "tokens": ["Sie", "nimmt", "mich", "in", "die", "Lehr'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und giebt nichts meiner Leidenschaft,", "tokens": ["Und", "giebt", "nichts", "mei\u00b7ner", "Lei\u00b7den\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In der ich mich verzehr'.", "tokens": ["In", "der", "ich", "mich", "ver\u00b7zehr'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich schleiche sanft von ihr mich fort", "tokens": ["Ich", "schlei\u00b7che", "sanft", "von", "ihr", "mich", "fort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPER", "PRF", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zur andern, die mir singt:", "tokens": ["Zur", "an\u00b7dern", ",", "die", "mir", "singt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "\u00bbf\u00fcr jeden Sinn ist nur ", "tokens": ["\u00bb", "f\u00fcr", "je\u00b7den", "Sinn", "ist", "nur"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "PIAT", "NN", "VAFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ein Klang nur, der ihn zwingt.", "tokens": ["Ein", "Klang", "nur", ",", "der", "ihn", "zwingt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wer dieses Wort im Herzen h\u00e4lt,", "tokens": ["Wer", "die\u00b7ses", "Wort", "im", "Her\u00b7zen", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und singt es nicht heraus,", "tokens": ["Und", "singt", "es", "nicht", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der ist wohl unst\u00e4t auf der Welt", "tokens": ["Der", "ist", "wohl", "un\u00b7st\u00e4t", "auf", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Qual ist ihm sein Haus.", "tokens": ["Und", "Qual", "ist", "ihm", "sein", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du armes Herz, sag mir dein Wort,", "tokens": ["Du", "ar\u00b7mes", "Herz", ",", "sag", "mir", "dein", "Wort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu meinem hellen Klang,", "tokens": ["Zu", "mei\u00b7nem", "hel\u00b7len", "Klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein gutes Wort hat seinen Ort", "tokens": ["Ein", "gu\u00b7tes", "Wort", "hat", "sei\u00b7nen", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Unsterblich im Gesang.\u00ab", "tokens": ["U\u00b7nsterb\u00b7lich", "im", "Ge\u00b7sang", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPRART", "NN", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "Wie wird mir da auf einmal leicht", "tokens": ["Wie", "wird", "mir", "da", "auf", "ein\u00b7mal", "leicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "APPR", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Herzen, im Verstand;", "tokens": ["Im", "Her\u00b7zen", ",", "im", "Ver\u00b7stand", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein heller Strahl zum Himmel steigt:", "tokens": ["Ein", "hel\u00b7ler", "Strahl", "zum", "Him\u00b7mel", "steigt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "