{"dta.poem.5518": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1763", "urn": "urn:nbn:de:kobv:b4-20678-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erwarte nicht, verehrungsw\u00fcrdger", "tokens": ["Er\u00b7war\u00b7te", "nicht", ",", "ver\u00b7eh\u00b7rungs\u00b7w\u00fcrd\u00b7ger"], "token_info": ["word", "word", "punct", "word"], "pos": ["VVFIN", "PTKNEG", "$,", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Held,", "tokens": ["Held", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Du Zier der ietzigen, du Ruhm der", "tokens": ["Du", "Zier", "der", "iet\u00b7zi\u00b7gen", ",", "du", "Ruhm", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "ART", "PPOSS", "$,", "PPER", "NN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "sp\u00e4tsten Welt,", "tokens": ["sp\u00e4ts\u00b7ten", "Welt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Der Muse schwaches Lob. Wie k\u00f6nnt\u2019", "tokens": ["Der", "Mu\u00b7se", "schwa\u00b7ches", "Lob", ".", "Wie", "k\u00f6nnt'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "PWAV", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "es ihr gelingen,", "tokens": ["es", "ihr", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "O Ferdinand, Dich w\u00fcrdig zu besin-", "tokens": ["O", "Fer\u00b7di\u00b7nand", ",", "Dich", "w\u00fcr\u00b7dig", "zu", "be\u00b7sin"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PPER", "ADJD", "APPR", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "gen,", "tokens": ["gen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Da Deine Thaten selbst ein gr\u00f6\u00dfrer", "tokens": ["Da", "Dei\u00b7ne", "Tha\u00b7ten", "selbst", "ein", "gr\u00f6\u00df\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Lobspruch sind,", "tokens": ["Lob\u00b7spruch", "sind", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Als alles, was der Witz der Dichter", "tokens": ["Als", "al\u00b7les", ",", "was", "der", "Witz", "der", "Dich\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "$,", "PRELS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "sich ersinnt.", "tokens": ["sich", "er\u00b7sinnt", "."], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Wofern indes den Beyfall sp\u00e4ter", "tokens": ["Wo\u00b7fern", "in\u00b7des", "den", "Bey\u00b7fall", "sp\u00e4\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Zeiten", "tokens": ["Zei\u00b7ten"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Die Muse hoffen darf: so sollen ihre", "tokens": ["Die", "Mu\u00b7se", "hof\u00b7fen", "darf", ":", "so", "sol\u00b7len", "ih\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$.", "ADV", "VMFIN", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Saiten", "tokens": ["Sai\u00b7ten"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Bekr\u00e4ftigen, wie Du ein ganzes Land,", "tokens": ["Be\u00b7kr\u00e4f\u00b7ti\u00b7gen", ",", "wie", "Du", "ein", "gan\u00b7zes", "Land", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Gefesselt schon von seiner Feinde Hand,", "tokens": ["Ge\u00b7fes\u00b7selt", "schon", "von", "sei\u00b7ner", "Fein\u00b7de", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "So schnell befreyt, da\u00df schon vor Dei-", "tokens": ["So", "schnell", "be\u00b7freyt", ",", "da\u00df", "schon", "vor", "Dei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVPP", "$,", "KOUS", "ADV", "APPR", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "nem Drohen", "tokens": ["nem", "Dro\u00b7hen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "Vom Elbstrand an des Feindes Fah-", "tokens": ["Vom", "Elbst\u00b7rand", "an", "des", "Fein\u00b7des", "Fah"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "nen flohen", "tokens": ["nen", "flo\u00b7hen"], "token_info": ["word", "word"], "pos": ["VVFIN", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.23": {"text": "Bis zum erstaunten Rhein. Die klei-", "tokens": ["Bis", "zum", "er\u00b7staun\u00b7ten", "Rhein", ".", "Die", "klei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "APPRART", "ADJA", "NE", "$.", "ART", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "ne Kriegerschaar,", "tokens": ["ne", "Krie\u00b7ger\u00b7schaar", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.25": {"text": "Die kaum bey Stade noch ein Spott", "tokens": ["Die", "kaum", "bey", "Sta\u00b7de", "noch", "ein", "Spott"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "des Feindes war,", "tokens": ["des", "Fein\u00b7des", "war", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.27": {"text": "Schlug nun bey Creveld schon den Kern", "tokens": ["Schlug", "nun", "bey", "Cre\u00b7veld", "schon", "den", "Kern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "von Frankreichs Kriegern,", "tokens": ["von", "Fran\u00b7kreichs", "Krie\u00b7gern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.29": {"text": "Und ward, gef\u00fchrt durch Dich, ein", "tokens": ["Und", "ward", ",", "ge\u00b7f\u00fchrt", "durch", "Dich", ",", "ein"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "VAFIN", "$,", "VVPP", "APPR", "PPER", "$,", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "furchtbar Heer von Siegern.", "tokens": ["furcht\u00b7bar", "Heer", "von", "Sie\u00b7gern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.31": {"text": "O Tag bey Minden! Du, uns ewig", "tokens": ["O", "Tag", "bey", "Min\u00b7den", "!", "Du", ",", "uns", "e\u00b7wig"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "NN", "APPR", "NN", "$.", "PPER", "$,", "PPER", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "theurer Tag!", "tokens": ["theu\u00b7rer", "Tag", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.33": {"text": "Wie vom Olymp ein schneller Don-", "tokens": ["Wie", "vom", "O\u00b7lymp", "ein", "schnel\u00b7ler", "Don"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ART", "ADJA", "TRUNC"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.34": {"text": "nerschlag", "tokens": ["ner\u00b7schlag"], "token_info": ["word"], "pos": ["NN"], "meter": "-+", "measure": "iambic.single"}, "line.35": {"text": "Herniederf\u00e4hrt: so sah in wenig Stun-", "tokens": ["Her\u00b7nie\u00b7der\u00b7f\u00e4hrt", ":", "so", "sah", "in", "we\u00b7nig", "Stun"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "ADV", "VVFIN", "APPR", "PIAT", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "den", "tokens": ["den"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.37": {"text": "Der Gallier von dir sich \u00fcberwunden;", "tokens": ["Der", "Gal\u00b7lier", "von", "dir", "sich", "\u00fc\u00b7berw\u00b7un\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PRF", "VVPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.38": {"text": "Contades floh. Du gabst zum zwey-", "tokens": ["Con\u00b7ta\u00b7des", "floh", ".", "Du", "gabst", "zum", "zwey"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$.", "PPER", "VVFIN", "APPRART", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "tenmal", "tokens": ["ten\u00b7mal"], "token_info": ["word"], "pos": ["ADV"], "meter": "-+", "measure": "iambic.single"}, "line.40": {"text": "Die Freyheit uns durch Deinen Hel-", "tokens": ["Die", "Frey\u00b7heit", "uns", "durch", "Dei\u00b7nen", "Hel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "denstahl.", "tokens": ["den\u00b7stahl", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.42": {"text": "Doch wie das Meer, das durch die", "tokens": ["Doch", "wie", "das", "Meer", ",", "das", "durch", "die"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "PRELS", "APPR", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.43": {"text": "D\u00e4mme dringet,", "tokens": ["D\u00e4m\u00b7me", "drin\u00b7get", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.44": {"text": "Auf neuem Sturm auch neue Wellen", "tokens": ["Auf", "neu\u00b7em", "Sturm", "auch", "neu\u00b7e", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "bringet;", "tokens": ["brin\u00b7get", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.46": {"text": "So drang aus Gallien, auf ein ge-", "tokens": ["So", "drang", "aus", "Gal\u00b7li\u00b7en", ",", "auf", "ein", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NE", "$,", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "schlagnes Heer,", "tokens": ["schlag\u00b7nes", "Heer", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.48": {"text": "Ein st\u00e4rkeres herzu. Welch eine Ge-", "tokens": ["Ein", "st\u00e4r\u00b7ke\u00b7res", "her\u00b7zu", ".", "Welch", "ei\u00b7ne", "Ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "ADV", "$.", "PIAT", "ART", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.49": {"text": "genwehr", "tokens": ["gen\u00b7wehr"], "token_info": ["word"], "pos": ["ADV"], "meter": "-+", "measure": "iambic.single"}, "line.50": {"text": "Von Dir, o Ferdinand, in Fielinghau-", "tokens": ["Von", "Dir", ",", "o", "Fer\u00b7di\u00b7nand", ",", "in", "Fie\u00b7ling\u00b7hau"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "$,", "FM", "NE", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.51": {"text": "sens Fl\u00e4chen!", "tokens": ["sens", "Fl\u00e4\u00b7chen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.52": {"text": "Die Feinde konnten nicht in Deine", "tokens": ["Die", "Fein\u00b7de", "konn\u00b7ten", "nicht", "in", "Dei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "Schaaren brechen;", "tokens": ["Schaa\u00b7ren", "bre\u00b7chen", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.54": {"text": "Du standest Tage lang in Arbeit,", "tokens": ["Du", "stan\u00b7dest", "Ta\u00b7ge", "lang", "in", "Ar\u00b7beit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.55": {"text": "Dampf und Gluth;", "tokens": ["Dampf", "und", "Gluth", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.56": {"text": "Der Feind entfloh, und hier auch siegte", "tokens": ["Der", "Feind", "ent\u00b7floh", ",", "und", "hier", "auch", "sieg\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.57": {"text": "Muth.", "tokens": ["Muth", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.58": {"text": "So eiltest Du von Siegen fort zu", "tokens": ["So", "eil\u00b7test", "Du", "von", "Sie\u00b7gen", "fort", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "PTKZU"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.59": {"text": "Siegen;", "tokens": ["Sie\u00b7gen", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.60": {"text": "Schienst oft dem Feind mit Deinem", "tokens": ["Schienst", "oft", "dem", "Feind", "mit", "Dei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.61": {"text": "Heer zu fliegen,", "tokens": ["Heer", "zu", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.62": {"text": "Wenn er bedeckt im sichern Lager stand,", "tokens": ["Wenn", "er", "be\u00b7deckt", "im", "si\u00b7chern", "La\u00b7ger", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.63": {"text": "Und auf einmal Dein R\u00e4cherschwerdt", "tokens": ["Und", "auf", "ein\u00b7mal", "Dein", "R\u00e4\u00b7cher\u00b7schwerdt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.64": {"text": "empfand.", "tokens": ["emp\u00b7fand", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.65": {"text": "Sprich, Grebenstein, wie er bey dir ge-", "tokens": ["Sprich", ",", "Gre\u00b7ben\u00b7stein", ",", "wie", "er", "bey", "dir", "ge"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "NN", "$,", "PWAV", "PPER", "APPR", "PPER", "TRUNC"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.66": {"text": "stritten!", "tokens": ["strit\u00b7ten", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.67": {"text": "Sein Heer zog fort mit festgeschlo\u00dfnen", "tokens": ["Sein", "Heer", "zog", "fort", "mit", "fest\u00b7ge\u00b7schlo\u00df\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.68": {"text": "Schritten;", "tokens": ["Schrit\u00b7ten", ";"], "token_info": ["word", "p