{"textgrid.poem.53848": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Angst des Kapitalisten", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fcher hatte ich ", "tokens": ["Fr\u00fc\u00b7her", "hat\u00b7te", "ich"], "token_info": ["word", "word", "word"], "pos": ["NN", "VAFIN", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Die verdammten Proleten!", "tokens": ["Die", "ver\u00b7damm\u00b7ten", "Pro\u00b7le\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Wie waren die Luder feste geeint \u2013", "tokens": ["Wie", "wa\u00b7ren", "die", "Lu\u00b7der", "fes\u00b7te", "ge\u00b7eint", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADJA", "VVPP", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Spitze \u2013 kurz treten!", "tokens": ["Spit\u00b7ze", "\u2013", "kurz", "tre\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "ADJD", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ein Stand \u2013 ein Kommando \u2013", "tokens": ["Ein", "Stand", "\u2013", "ein", "Kom\u00b7man\u00b7do", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ein Wille \u2013 ein Schritt \u2013", "tokens": ["Ein", "Wil\u00b7le", "\u2013", "ein", "Schritt", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und alle mit \u2013", "tokens": ["Und", "al\u00b7le", "mit", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Im November hing ich an einem Haar.", "tokens": ["Im", "No\u00b7vem\u00b7ber", "hing", "ich", "an", "ei\u00b7nem", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die verdammten Proleten!", "tokens": ["Die", "ver\u00b7damm\u00b7ten", "Pro\u00b7le\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Meine Bank, mein Heiligstes, war in Gefahr \u2013", "tokens": ["Mei\u00b7ne", "Bank", ",", "mein", "Hei\u00b7ligs\u00b7tes", ",", "war", "in", "Ge\u00b7fahr", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "VAFIN", "APPR", "NN", "$("], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Kopf ab \u2013 zum Beten . . . ?", "tokens": ["Kopf", "ab", "\u2013", "zum", "Be\u00b7ten", ".", ".", ".", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["NN", "PTKVZ", "$(", "APPRART", "NN", "$.", "$.", "$.", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Ein Tag \u2013 eine Welle \u2013", "tokens": ["Ein", "Tag", "\u2013", "ei\u00b7ne", "Wel\u00b7le", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ein Volk \u2013 ein Ri\u00df \u2013 \u2013", "tokens": ["Ein", "Volk", "\u2013", "ein", "Ri\u00df", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "$(", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und ich hatte Schi\u00df.", "tokens": ["Und", "ich", "hat\u00b7te", "Schi\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Heute gibt es viele Sozialistenpartein,", "tokens": ["Heu\u00b7te", "gibt", "es", "vie\u00b7le", "So\u00b7zi\u00b7a\u00b7lis\u00b7ten\u00b7par\u00b7tein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Die dummen Proleten!", "tokens": ["Die", "dum\u00b7men", "Pro\u00b7le\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "La\u00df sie doch durcheinander schrein,", "tokens": ["La\u00df", "sie", "doch", "durch\u00b7ein\u00b7an\u00b7der", "schrein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann kann ich sie besser treten!", "tokens": ["Dann", "kann", "ich", "sie", "bes\u00b7ser", "tre\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADJD", "VVINF", "$."], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Ein Chaos \u2013 ein Kampf \u2013", "tokens": ["Ein", "Chaos", "\u2013", "ein", "Kampf", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ein Krach \u2013 ein Gerauf \u2013 Gottseidank:", "tokens": ["Ein", "Krach", "\u2013", "ein", "Ger\u00b7auf", "\u2013", "Gott\u00b7sei\u00b7dank", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "$(", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und ich obenauf! \u2013", "tokens": ["Und", "ich", "o\u00b7ben\u00b7auf", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "ADV", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}