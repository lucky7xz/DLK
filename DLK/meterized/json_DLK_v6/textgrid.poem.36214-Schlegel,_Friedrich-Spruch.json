{"textgrid.poem.36214": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Spruch", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer gew\u00e4hrt nur Edlen Gunst?", "tokens": ["Wer", "ge\u00b7w\u00e4hrt", "nur", "Ed\u00b7len", "Gunst", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die hohe Kunst.", "tokens": ["Die", "ho\u00b7he", "Kunst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wo verliert man nie die Spur?", "tokens": ["Wo", "ver\u00b7liert", "man", "nie", "die", "Spur", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Natur.", "tokens": ["In", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wie gewinnst du sichres Gut?", "tokens": ["Wie", "ge\u00b7winnst", "du", "sich\u00b7res", "Gut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch eignen Mut.", "tokens": ["Durch", "eig\u00b7nen", "Mut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Tapfer also heil'ge Glut,", "tokens": ["Tap\u00b7fer", "al\u00b7so", "heil'\u00b7ge", "Glut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hoch hinan zum ewig Sch\u00f6nen!", "tokens": ["Hoch", "hi\u00b7nan", "zum", "e\u00b7wig", "Sch\u00f6\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPRART", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Flamme k\u00fchn, und la\u00df sie h\u00f6hnen,", "tokens": ["Flam\u00b7me", "k\u00fchn", ",", "und", "la\u00df", "sie", "h\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "KON", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eins in Kunst, Natur und Mut.", "tokens": ["Eins", "in", "Kunst", ",", "Na\u00b7tur", "und", "Mut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}