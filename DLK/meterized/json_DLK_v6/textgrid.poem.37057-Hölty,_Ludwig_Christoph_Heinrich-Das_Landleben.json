{"textgrid.poem.37057": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Landleben", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n ist die Flur, mit Perlen \u00fcberhangen,", "tokens": ["Sch\u00f6n", "ist", "die", "Flur", ",", "mit", "Per\u00b7len", "\u00fc\u00b7berh\u00b7an\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Worin das Bild der Sonne strahlt,", "tokens": ["Wo\u00b7rin", "das", "Bild", "der", "Son\u00b7ne", "strahlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sch\u00f6n ist das Volk der Blumen, deren Wangen", "tokens": ["Sch\u00f6n", "ist", "das", "Volk", "der", "Blu\u00b7men", ",", "de\u00b7ren", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "ART", "NN", "$,", "PRELAT", "NN"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Die Abendr\u00f6the mahlt.", "tokens": ["Die", "A\u00b7bend\u00b7r\u00f6\u00b7the", "mahlt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6n ist das Thal, und die bebl\u00fcmte Weide,", "tokens": ["Sch\u00f6n", "ist", "das", "Thal", ",", "und", "die", "be\u00b7bl\u00fcm\u00b7te", "Wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "KON", "ART", "ADJA", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Wo manche Wollenheerde geht,", "tokens": ["Wo", "man\u00b7che", "Wol\u00b7len\u00b7heer\u00b7de", "geht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sobald der Tag, im purpurrothen Kleide,", "tokens": ["So\u00b7bald", "der", "Tag", ",", "im", "pur\u00b7pur\u00b7ro\u00b7then", "Klei\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf den Gebirgen steht.", "tokens": ["Auf", "den", "Ge\u00b7bir\u00b7gen", "steht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6n ist der Hayn, der einen gr\u00fcnen Schleyer", "tokens": ["Sch\u00f6n", "ist", "der", "Hayn", ",", "der", "ei\u00b7nen", "gr\u00fc\u00b7nen", "Schle\u00b7yer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "++-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.2": {"text": "Von D\u00e4mmrung um die Hirtin zieht,", "tokens": ["Von", "D\u00e4mm\u00b7rung", "um", "die", "Hir\u00b7tin", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn Sirius die Luft beherrscht und Feuer", "tokens": ["Wenn", "Si\u00b7rius", "die", "Luft", "be\u00b7herrscht", "und", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ART", "NN", "VVFIN", "KON", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Aus ofnem Schlunde spr\u00fcht.", "tokens": ["Aus", "of\u00b7nem", "Schlun\u00b7de", "spr\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sch\u00f6n ist der Bach, der pl\u00e4tschernd durchs Gewimmel", "tokens": ["Sch\u00f6n", "ist", "der", "Bach", ",", "der", "pl\u00e4t\u00b7schernd", "durchs", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN"], "meter": "+--+-+++-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Der Bl\u00fcmchen, das ihm Reize leyht,", "tokens": ["Der", "Bl\u00fcm\u00b7chen", ",", "das", "ihm", "Rei\u00b7ze", "leyht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wellchen rollt, wenn ihn der Abendhimmel", "tokens": ["Die", "Well\u00b7chen", "rollt", ",", "wenn", "ihn", "der", "A\u00b7bend\u00b7him\u00b7mel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Purpur \u00fcberstreut.", "tokens": ["Mit", "Pur\u00b7pur", "\u00fc\u00b7bers\u00b7treut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Garten, den ein Hayn voll Apfelb\u00e4ume", "tokens": ["Der", "Gar\u00b7ten", ",", "den", "ein", "Hayn", "voll", "Ap\u00b7fel\u00b7b\u00e4u\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In seine gr\u00fcnen Arme schlingt,", "tokens": ["In", "sei\u00b7ne", "gr\u00fc\u00b7nen", "Ar\u00b7me", "schlingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie reizt er nicht! Wie str\u00f6men nicht die Reime,", "tokens": ["Wie", "reizt", "er", "nicht", "!", "Wie", "str\u00f6\u00b7men", "nicht", "die", "Rei\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKNEG", "$.", "PWAV", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn hier ein Dichter singt!", "tokens": ["Wenn", "hier", "ein", "Dich\u00b7ter", "singt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ein jedes Kind der bunt bemahlten Flore", "tokens": ["Ein", "je\u00b7des", "Kind", "der", "bunt", "be\u00b7mahl\u00b7ten", "Flo\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ergie\u00dft hier einen Strom von Duft,", "tokens": ["Er\u00b7gie\u00dft", "hier", "ei\u00b7nen", "Strom", "von", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und lacht dem Tag entgegen, den Aurore", "tokens": ["Und", "lacht", "dem", "Tag", "ent\u00b7ge\u00b7gen", ",", "den", "Au\u00b7ro\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus Thetis Armen ruft.", "tokens": ["Aus", "The\u00b7tis", "Ar\u00b7men", "ruft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Hier wirbelt, wenn der Abendstern im Westen", "tokens": ["Hier", "wir\u00b7belt", ",", "wenn", "der", "A\u00b7bends\u00b7tern", "im", "Wes\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "ART", "NN", "APPRART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den Saum des Horizonts besteigt,", "tokens": ["Den", "Saum", "des", "Ho\u00b7ri\u00b7zonts", "be\u00b7steigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Nachtigall, und klaget auf den Aesten,", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", ",", "und", "kla\u00b7get", "auf", "den", "A\u00b7es\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.4": {"text": "Bis Ph\u00f6bus sie verscheucht.", "tokens": ["Bis", "Ph\u00f6\u00b7bus", "sie", "ver\u00b7scheucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wie lieb ich dich, du Flur nach meinem Herzen,", "tokens": ["Wie", "lieb", "ich", "dich", ",", "du", "Flur", "nach", "mei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PRF", "$,", "PPER", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo bl\u00fchende Gesundheit thront,", "tokens": ["Wo", "bl\u00fc\u00b7hen\u00b7de", "Ge\u00b7sund\u00b7heit", "thront", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Wo alles scherzt, wie Sommerl\u00fcftgen scherzen,", "tokens": ["Wo", "al\u00b7les", "scherzt", ",", "wie", "Som\u00b7mer\u00b7l\u00fcft\u00b7gen", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo noch die Tugend wohnt.", "tokens": ["Wo", "noch", "die", "Tu\u00b7gend", "wohnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Du bist mein Wunsch, o Hayn, voll Rasenbetten,", "tokens": ["Du", "bist", "mein", "Wunsch", ",", "o", "Hayn", ",", "voll", "Ra\u00b7sen\u00b7bet\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "FM", "NN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch den ein Bach die Urne gie\u00dft,", "tokens": ["Durch", "den", "ein", "Bach", "die", "Ur\u00b7ne", "gie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht Gold, das dich, o Geiz, mit Sklavenketten,", "tokens": ["Nicht", "Gold", ",", "das", "dich", ",", "o", "Geiz", ",", "mit", "Skla\u00b7ven\u00b7ket\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PRELS", "PPER", "$,", "FM", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An deinen Kasten schlie\u00dft.", "tokens": ["An", "dei\u00b7nen", "Kas\u00b7ten", "schlie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Nimt mich ein Thal, vom silbernen Geschw\u00e4tze", "tokens": ["Nimt", "mich", "ein", "Thal", ",", "vom", "sil\u00b7ber\u00b7nen", "Ge\u00b7schw\u00e4t\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Bachs durchfl\u00fcstert, in den Schoo\u00df,", "tokens": ["Des", "Bachs", "durch\u00b7fl\u00fcs\u00b7tert", ",", "in", "den", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Webt der Jasmin um meine Lauben Netze,", "tokens": ["Webt", "der", "Jas\u00b7min", "um", "mei\u00b7ne", "Lau\u00b7ben", "Net\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Wie gl\u00fccklich ist mein Loo\u00df!", "tokens": ["Wie", "gl\u00fcck\u00b7lich", "ist", "mein", "Loo\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Der Abend sieht mich oft in meiner Laube,", "tokens": ["Der", "A\u00b7bend", "sieht", "mich", "oft", "in", "mei\u00b7ner", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn ich dem Thomson und Virgil,", "tokens": ["Wenn", "ich", "dem", "Thom\u00b7son", "und", "Vir\u00b7gil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "KON", "NE", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Der Biene gleich, die s\u00fc\u00dfen Sch\u00e4tze raube,", "tokens": ["Der", "Bie\u00b7ne", "gleich", ",", "die", "s\u00fc\u00b7\u00dfen", "Sch\u00e4t\u00b7ze", "rau\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ganz Wollust, ganz Gef\u00fchl.", "tokens": ["Ganz", "Wol\u00b7lust", ",", "ganz", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Ruft einst der Tod mich weg von meinem H\u00fcgel,", "tokens": ["Ruft", "einst", "der", "Tod", "mich", "weg", "von", "mei\u00b7nem", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von meiner Flur, ich zittre nicht,", "tokens": ["Von", "mei\u00b7ner", "Flur", ",", "ich", "zitt\u00b7re", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er kommt als Freund, giebt meiner Seele Fl\u00fcgel,", "tokens": ["Er", "kommt", "als", "Freund", ",", "giebt", "mei\u00b7ner", "See\u00b7le", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "$,", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Giebt ihr ein Kleid von Licht.", "tokens": ["Giebt", "ihr", "ein", "Kleid", "von", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Er f\u00fchret mich in Gegenden voll Wonne,", "tokens": ["Er", "f\u00fch\u00b7ret", "mich", "in", "Ge\u00b7gen\u00b7den", "voll", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo mit der Flora Hand in Hand", "tokens": ["Wo", "mit", "der", "Flo\u00b7ra", "Hand", "in", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Fr\u00fchling h\u00fcpft, und eine mildre Sonne", "tokens": ["Der", "Fr\u00fch\u00b7ling", "h\u00fcpft", ",", "und", 