{"textgrid.poem.62352": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Der Scheidenden", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus dem Tempel willst du fliehen,", "tokens": ["Aus", "dem", "Tem\u00b7pel", "willst", "du", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den dir hier die Liebe baut?", "tokens": ["Den", "dir", "hier", "die", "Lie\u00b7be", "baut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meinen Armen dich entziehen,", "tokens": ["Mei\u00b7nen", "Ar\u00b7men", "dich", "ent\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meines Geistes holde Braut?", "tokens": ["Mei\u00b7nes", "Geis\u00b7tes", "hol\u00b7de", "Braut", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Richtest du nach deiner Heimat,", "tokens": ["Rich\u00b7test", "du", "nach", "dei\u00b7ner", "Hei\u00b7mat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Pilgerin, den m\u00fcden Lauf?", "tokens": ["Pil\u00b7ge\u00b7rin", ",", "den", "m\u00fc\u00b7den", "Lauf", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Fleuchst du schon in deinen Himmel,", "tokens": ["Fleuchst", "du", "schon", "in", "dei\u00b7nen", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sch\u00f6ner Engel, wieder auf?", "tokens": ["Sch\u00f6\u00b7ner", "En\u00b7gel", ",", "wie\u00b7der", "auf", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nein, du weilest noch hienieden", "tokens": ["Nein", ",", "du", "wei\u00b7lest", "noch", "hien\u00b7ie\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Voll erhabner Gottesruh,", "tokens": ["Voll", "er\u00b7hab\u00b7ner", "Got\u00b7tes\u00b7ruh", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tr\u00e4gst den Himmel und den Frieden", "tokens": ["Tr\u00e4gst", "den", "Him\u00b7mel", "und", "den", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun entfernten Fluren zu;", "tokens": ["Nun", "ent\u00b7fern\u00b7ten", "Flu\u00b7ren", "zu", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Willst dem Kranken Labung spenden,", "tokens": ["Willst", "dem", "Kran\u00b7ken", "La\u00b7bung", "spen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Den der Himmel dir vertraut,", "tokens": ["Den", "der", "Him\u00b7mel", "dir", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Willst des Lebens dich erfreuen,", "tokens": ["Willst", "des", "Le\u00b7bens", "dich", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Das aus deinen Blicken thaut.", "tokens": ["Das", "aus", "dei\u00b7nen", "Bli\u00b7cken", "thaut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Folge denn der sch\u00f6nen Sendung,", "tokens": ["Fol\u00b7ge", "denn", "der", "sch\u00f6\u00b7nen", "Sen\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Folge nur des Geistes Ruf,", "tokens": ["Fol\u00b7ge", "nur", "des", "Geis\u00b7tes", "Ruf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der zur Krone der Vollendung", "tokens": ["Der", "zur", "Kro\u00b7ne", "der", "Vol\u00b7len\u00b7dung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "ART", "NN"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.4": {"text": "Dich mit solcher Sch\u00f6nheit schuf!", "tokens": ["Dich", "mit", "sol\u00b7cher", "Sch\u00f6n\u00b7heit", "schuf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mag ein Andrer deiner N\u00e4he,", "tokens": ["Mag", "ein", "A\u00b7ndrer", "dei\u00b7ner", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deines Strahles sich erfreun, \u2013", "tokens": ["Dei\u00b7nes", "Strah\u00b7les", "sich", "er\u00b7freun", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVINF", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach! die Liebe kann entsagen,", "tokens": ["Ach", "!", "die", "Lie\u00b7be", "kann", "ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und entsagend selig sein.", "tokens": ["Und", "ent\u00b7sa\u00b7gend", "se\u00b7lig", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schweigen sollen alle Klagen,", "tokens": ["Schwei\u00b7gen", "sol\u00b7len", "al\u00b7le", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und kein treuer Zephyr soll", "tokens": ["Und", "kein", "treu\u00b7er", "Ze\u00b7phyr", "soll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diesen Seufzer zu dir tragen,", "tokens": ["Die\u00b7sen", "Seuf\u00b7zer", "zu", "dir", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welcher hier der Brust entquoll.", "tokens": ["Wel\u00b7cher", "hier", "der", "Brust", "ent\u00b7quoll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ADV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "N\u00e4her, unaussprechlich n\u00e4her", "tokens": ["N\u00e4\u00b7her", ",", "un\u00b7aus\u00b7sprech\u00b7lich", "n\u00e4\u00b7her"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJD", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bist du doch, Entfernte, mir,", "tokens": ["Bist", "du", "doch", ",", "Ent\u00b7fern\u00b7te", ",", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$,", "VVFIN", "$,", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und im Geisterreiche schweiget", "tokens": ["Und", "im", "Geis\u00b7ter\u00b7rei\u00b7che", "schwei\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Jede st\u00fcrmische Begier.", "tokens": ["Je\u00b7de", "st\u00fcr\u00b7mi\u00b7sche", "Be\u00b7gier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}