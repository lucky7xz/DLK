{"dta.poem.5821": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Gesetzen der Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dasjenige, nun was im Kreyse gehet,", "tokens": ["Das\u00b7je\u00b7ni\u00b7ge", ",", "nun", "was", "im", "Krey\u00b7se", "ge\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ADV", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Beweisst, da\u00df solch ein C\u00f6rper sich", "tokens": ["Be\u00b7weisst", ",", "da\u00df", "solch", "ein", "C\u00f6r\u00b7per", "sich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PIAT", "ART", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf tausend tausend Arten drehet.", "tokens": ["Auf", "tau\u00b7send", "tau\u00b7send", "Ar\u00b7ten", "dre\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein C\u00f6rper der ihm hinderlich,", "tokens": ["Ein", "C\u00f6r\u00b7per", "der", "ihm", "hin\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "H\u00e4lt durch den Widerstand ihn auf,", "tokens": ["H\u00e4lt", "durch", "den", "Wi\u00b7der\u00b7stand", "ihn", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und bieget stets den sonst geraden Lauff.", "tokens": ["Und", "bie\u00b7get", "stets", "den", "sonst", "ge\u00b7ra\u00b7den", "Lauff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}