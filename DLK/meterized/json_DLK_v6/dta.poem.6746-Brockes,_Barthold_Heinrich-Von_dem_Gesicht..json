{"dta.poem.6746": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die ihr nat\u00fcrlich sind. Nachdem sie Staffel-Weise", "tokens": ["Die", "ihr", "na\u00b7t\u00fcr\u00b7lich", "sind", ".", "Nach\u00b7dem", "sie", "Staf\u00b7fel\u00b7Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "VAFIN", "$.", "KOUS", "PPER", "NN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sich \u00e4ndert, wei\u00df sie, da\u00df sich m\u00fcsse", "tokens": ["Sich", "\u00e4n\u00b7dert", ",", "wei\u00df", "sie", ",", "da\u00df", "sich", "m\u00fcs\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "VVFIN", "$,", "VVFIN", "PPER", "$,", "KOUS", "PRF", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Sehungs-Axe gleichfalls biegen.", "tokens": ["Die", "Se\u00b7hungs\u00b7A\u00b7xe", "gleich\u00b7falls", "bie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Die Winckel, die sie macht, die enge bald, bald weit,", "tokens": ["Die", "Win\u00b7ckel", ",", "die", "sie", "macht", ",", "die", "en\u00b7ge", "bald", ",", "bald", "weit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "ART", "ADJA", "ADV", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Veranlasst, da\u00df der Geist, durch diesen Unterscheid,", "tokens": ["Ver\u00b7an\u00b7lasst", ",", "da\u00df", "der", "Geist", ",", "durch", "die\u00b7sen", "Un\u00b7ter\u00b7scheid", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sein Urtheil von der Gr\u00f6\u00df\u2019 und von der Weite f\u00e4llt,", "tokens": ["Sein", "Ur\u00b7theil", "von", "der", "Gr\u00f6\u00df'", "und", "von", "der", "Wei\u00b7te", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der C\u00f6rper, die ihm vorgestellt.", "tokens": ["Der", "C\u00f6r\u00b7per", ",", "die", "ihm", "vor\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}