{"dta.poem.19620": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Der Landgraf.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Mir sitzt zu Hause jung gez\u00e4hmt", "tokens": ["Mir", "sitzt", "zu", "Hau\u00b7se", "jung", "ge\u00b7z\u00e4hmt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und leicht gel\u00e4hmt", "tokens": ["Und", "leicht", "ge\u00b7l\u00e4hmt"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVPP"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ein Steinaar im Verlie\u00dfe,", "tokens": ["Ein", "Stei\u00b7naar", "im", "Ver\u00b7lie\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der martert sich den Hals zu drehn,", "tokens": ["Der", "mar\u00b7tert", "sich", "den", "Hals", "zu", "drehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ins Blau zu sehn,", "tokens": ["Ins", "Blau", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Aus dem er gerne stie\u00dfe.", "tokens": ["Aus", "dem", "er", "ger\u00b7ne", "stie\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So streck' ich Landgraf ebenfalls", "tokens": ["So", "streck'", "ich", "Land\u00b7graf", "e\u00b7ben\u00b7falls"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Kopf und Hals", "tokens": ["Den", "Kopf", "und", "Hals"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wohl durch das Kerkergitter,", "tokens": ["Wohl", "durch", "das", "Ker\u00b7ker\u00b7git\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ob etwas auf der Stra\u00dfe zieht", "tokens": ["Ob", "et\u00b7was", "auf", "der", "Stra\u00b7\u00dfe", "zieht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcr mein Gem\u00fct,", "tokens": ["F\u00fcr", "mein", "Ge\u00b7m\u00fct", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ein Sch\u00fcler oder Ritter.", "tokens": ["Ein", "Sch\u00fc\u00b7ler", "o\u00b7der", "Rit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Kaiser, der vergichtet ist,", "tokens": ["Der", "Kai\u00b7ser", ",", "der", "ver\u00b7gich\u00b7tet", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum gerne mi\u00dft", "tokens": ["Drum", "ger\u00b7ne", "mi\u00dft"], "token_info": ["word", "word", "word"], "pos": ["PAV", "ADV", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die Kost der harschen L\u00fcfte,", "tokens": ["Die", "Kost", "der", "har\u00b7schen", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verga\u00df wie schwer ein ganzer Mann", "tokens": ["Ver\u00b7ga\u00df", "wie", "schwer", "ein", "gan\u00b7zer", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Entrathen kann", "tokens": ["Ent\u00b7ra\u00b7then", "kann"], "token_info": ["word", "word"], "pos": ["NN", "VMFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Das Jagdhorn an der H\u00fcfte.", "tokens": ["Das", "Jagd\u00b7horn", "an", "der", "H\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich wurde hinterr\u00fccks gef\u00e4llt,", "tokens": ["Ich", "wur\u00b7de", "hin\u00b7ter\u00b7r\u00fccks", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Netz gestellt", "tokens": ["Ein", "Netz", "ge\u00b7stellt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ward mir mit falschen Schriften!", "tokens": ["Ward", "mir", "mit", "fal\u00b7schen", "Schrif\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer mir mit l\u00e4chelndem Gesicht", "tokens": ["Wer", "mir", "mit", "l\u00e4\u00b7cheln\u00b7dem", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Treue bricht,", "tokens": ["Die", "Treu\u00b7e", "bricht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Der kann mich auch vergiften!", "tokens": ["Der", "kann", "mich", "auch", "ver\u00b7gif\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "W\u00e4r' ich ein r\u00f6misch bl\u00f6der Mann,", "tokens": ["W\u00e4r'", "ich", "ein", "r\u00f6\u00b7misch", "bl\u00f6\u00b7der", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich w\u00e4hnte dann:", "tokens": ["Ich", "w\u00e4hn\u00b7te", "dann", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Damit h\u00e4tt' ich's verbrochen,", "tokens": ["Da\u00b7mit", "h\u00e4tt'", "ich's", "ver\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df triumphirend ich hinaus", "tokens": ["Da\u00df", "tri\u00b7um\u00b7phi\u00b7rend", "ich", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "PPER", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zum Gotteshaus", "tokens": ["Zum", "Got\u00b7tes\u00b7haus"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Schmi\u00df M\u00fchmchen Lisbeths Knochen! ", "tokens": ["Sch\u00b7mi\u00df", "M\u00fchm\u00b7chen", "Lis\u00b7beths", "Kno\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "J\u00fcngst warf ich auf den Festungsrain", "tokens": ["J\u00fcngst", "warf", "ich", "auf", "den", "Fes\u00b7tungs\u00b7rain"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein St\u00fcberlein", "tokens": ["Ein", "St\u00fc\u00b7berl\u00b7ein"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Dem Bettler hin, dem lahmen:", "tokens": ["Dem", "Bett\u00b7ler", "hin", ",", "dem", "lah\u00b7men", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PRELS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den schlug der Spanier bis aufs Blut \u2014", "tokens": ["Den", "schlug", "der", "Spa\u00b7nier", "bis", "aufs", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "APPRART", "NN", "$("], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Mich fra\u00df die Wuth \u2014", "tokens": ["Mich", "fra\u00df", "die", "Wuth"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Der Teufel hol' ihn! Amen!", "tokens": ["Der", "Teu\u00b7fel", "hol'", "ihn", "!", "A\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wohl l\u00e4g' ich besser auf dem Feld \u2014", "tokens": ["Wohl", "l\u00e4g'", "ich", "bes\u00b7ser", "auf", "dem", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eade, du Welt!\u201c \u2014", "tokens": ["\u201e", "a\u00b7de", ",", "du", "Welt", "!", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "ADV", "$,", "PPER", "NN", "$.", "$(", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Gewundet und erstochen!", "tokens": ["Ge\u00b7wun\u00b7det", "und", "er\u00b7sto\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie Meister Ulrich Zwingli lag,", "tokens": ["Wie", "Meis\u00b7ter", "Ul\u00b7rich", "Zwing\u00b7li", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Am gr\u00fcnen Hag,", "tokens": ["Am", "gr\u00fc\u00b7nen", "Hag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Den hellen Blick gebrochen!", "tokens": ["Den", "hel\u00b7len", "Blick", "ge\u00b7bro\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Nun tr\u00f6stet mich das Eine doch:", "tokens": ["Nun", "tr\u00f6s\u00b7tet", "mich", "das", "Ei\u00b7ne", "doch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "ART", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das p\u00e4pstlich Joch", "tokens": ["Das", "p\u00e4pst\u00b7lich", "Joch"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ist in den Dreck getreten!", "tokens": ["Ist", "in", "den", "Dreck", "ge\u00b7tre\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wir d\u00fcrfen ohne Clerisei", "tokens": ["Wir", "d\u00fcr\u00b7fen", "oh\u00b7ne", "Cle\u00b7ri\u00b7sei"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Heuchelei", "tokens": ["Und", "Heu\u00b7che\u00b7lei"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Getrost zum Herrgott beten!", "tokens": ["Ge\u00b7trost", "zum", "Herr\u00b7gott", "be\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}