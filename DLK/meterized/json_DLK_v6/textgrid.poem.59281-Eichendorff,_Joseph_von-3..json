{"textgrid.poem.59281": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch schwankende Wipfel", "tokens": ["Durch", "schwan\u00b7ken\u00b7de", "Wip\u00b7fel"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Schie\u00dft g\u00fcldener Strahl,", "tokens": ["Schie\u00dft", "g\u00fcl\u00b7de\u00b7ner", "Strahl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Tief unter den Gipfeln", "tokens": ["Tief", "un\u00b7ter", "den", "Gip\u00b7feln"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Das neblige Tal.", "tokens": ["Das", "neb\u00b7li\u00b7ge", "Tal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Fern hallt es am Schlosse,", "tokens": ["Fern", "hallt", "es", "am", "Schlos\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.6": {"text": "Das Waldhorn ruft,", "tokens": ["Das", "Wald\u00b7horn", "ruft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Es wiehern die Rosse,", "tokens": ["Es", "wie\u00b7hern", "die", "Ros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "In die Luft, in die Luft!", "tokens": ["In", "die", "Luft", ",", "in", "die", "Luft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Bald L\u00e4nder und Seen", "tokens": ["Bald", "L\u00e4n\u00b7der", "und", "Seen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Durch Wolkenzug", "tokens": ["Durch", "Wol\u00b7ken\u00b7zug"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Tief schimmernd zu sehen", "tokens": ["Tief", "schim\u00b7mernd", "zu", "se\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "PTKZU", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "In schwindelndem Flug,", "tokens": ["In", "schwin\u00b7deln\u00b7dem", "Flug", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Bald Dunkel wieder", "tokens": ["Bald", "Dun\u00b7kel", "wie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ADV", "NN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "H\u00fcllt Reiter und Ro\u00df,", "tokens": ["H\u00fcllt", "Rei\u00b7ter", "und", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "O Lieb, o Liebe", "tokens": ["O", "Lieb", ",", "o", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "FM", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "So la\u00df mich los! \u2013", "tokens": ["So", "la\u00df", "mich", "los", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Immer weiter und weiter", "tokens": ["Im\u00b7mer", "wei\u00b7ter", "und", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "KON", "ADV"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Die Kl\u00e4nge ziehn,", "tokens": ["Die", "Kl\u00e4n\u00b7ge", "ziehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Durch W\u00e4lder und Heiden", "tokens": ["Durch", "W\u00e4l\u00b7der", "und", "Hei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wohin, ach wohin?", "tokens": ["Wo\u00b7hin", ",", "ach", "wo\u00b7hin", "?"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PWAV", "$,", "XY", "PWAV", "$."], "meter": "--+-+", "measure": "anapaest.init"}, "line.5": {"text": "Erquickliche Frische", "tokens": ["Er\u00b7quick\u00b7li\u00b7che", "Fri\u00b7sche"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "S\u00fc\u00df-schaurige Lust!", "tokens": ["S\u00fc\u00df\u00b7schau\u00b7ri\u00b7ge", "Lust", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Hoch flattern die B\u00fcsche,", "tokens": ["Hoch", "flat\u00b7tern", "die", "B\u00fc\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Frei schl\u00e4gt die Brust.", "tokens": ["Frei", "schl\u00e4gt", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}