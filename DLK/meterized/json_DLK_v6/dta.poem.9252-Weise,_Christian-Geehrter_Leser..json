{"dta.poem.9252": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Geehrter Leser.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl dem der noch in seinem leben", "tokens": ["Wohl", "dem", "der", "noch", "in", "sei\u00b7nem", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ART", "ADV", "APPR", "PPOSAT", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein lustig st\u00fcndgen haben kan/", "tokens": ["Ein", "lus\u00b7tig", "st\u00fcnd\u00b7gen", "ha\u00b7ben", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "VAINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wann andre sich dem schmertz ergeben/", "tokens": ["Wann", "and\u00b7re", "sich", "dem", "schmertz", "er\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PRF", "ART", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und stecken zwischen furcht und wahn/", "tokens": ["Und", "ste\u00b7cken", "zwi\u00b7schen", "furcht", "und", "wahn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So bleibt der trost der ihn ergetzt", "tokens": ["So", "bleibt", "der", "trost", "der", "ihn", "er\u00b7getzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auff einen festen grund gesetzt.", "tokens": ["Auff", "ei\u00b7nen", "fes\u00b7ten", "grund", "ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Gesetzt ich liesse tag und nacht", "tokens": ["Ge\u00b7setzt", "ich", "lies\u00b7se", "tag", "und", "nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den kalten safft in magen lauffen/", "tokens": ["Den", "kal\u00b7ten", "safft", "in", "ma\u00b7gen", "lauf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der nur den scheidel hitzig macht.", "tokens": ["Der", "nur", "den", "schei\u00b7del", "hit\u00b7zig", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So sagt ich endlich morgens fr\u00fch/", "tokens": ["So", "sagt", "ich", "end\u00b7lich", "mor\u00b7gens", "fr\u00fch", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die lust verlohnt sich nicht der m\u00fch.", "tokens": ["Die", "lust", "ver\u00b7lohnt", "sich", "nicht", "der", "m\u00fch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKNEG", "ART", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "3. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Es macht mich reich und wieder arm/", "tokens": ["Es", "macht", "mich", "reich", "und", "wie\u00b7der", "arm", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sol ich meinen schaden f\u00fchlen/", "tokens": ["Und", "sol", "ich", "mei\u00b7nen", "scha\u00b7den", "f\u00fch\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So wird die stube gar zu warm/", "tokens": ["So", "wird", "die", "stu\u00b7be", "gar", "zu", "warm", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "PTKA", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gleichwie man spricht: das spielen hitzt.", "tokens": ["Gleich\u00b7wie", "man", "spricht", ":", "das", "spie\u00b7len", "hitzt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$.", "PDS", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wann man in dem keller sitzt.", "tokens": ["Und", "wann", "man", "in", "dem", "kel\u00b7ler", "sitzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "4. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Giebt endlich schlechten \u00fcberdru\u00df/", "tokens": ["Giebt", "end\u00b7lich", "schlech\u00b7ten", "\u00fc\u00b7berd\u00b7ru\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch weil man offt zur\u00fccke stehen", "tokens": ["Doch", "weil", "man", "offt", "zu\u00b7r\u00fc\u00b7cke", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIS", "ADV", "VVFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und in gedancken wuchern mu\u00df/", "tokens": ["Und", "in", "ge\u00b7dan\u00b7cken", "wu\u00b7chern", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So ist auch dieses nicht die bahn", "tokens": ["So", "ist", "auch", "die\u00b7ses", "nicht", "die", "bahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PDS", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Darauff man sich vergn\u00fcgen kan.", "tokens": ["Dar\u00b7auff", "man", "sich", "ver\u00b7gn\u00fc\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "5. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Der angenehmen Poesi/", "tokens": ["Der", "an\u00b7ge\u00b7neh\u00b7men", "Poe\u00b7si", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die gilt mir in den h\u00f6chsten leide", "tokens": ["Die", "gilt", "mir", "in", "den", "h\u00f6chs\u00b7ten", "lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das beste labsal vor die m\u00fch:", "tokens": ["Das", "bes\u00b7te", "lab\u00b7sal", "vor", "die", "m\u00fch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da schick ich meine sorgen hin", "tokens": ["Da", "schick", "ich", "mei\u00b7ne", "sor\u00b7gen", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wann ich im tiefsten kummer bin.", "tokens": ["Wann", "ich", "im", "tiefs\u00b7ten", "kum\u00b7mer", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "6. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Was andre mit beschweren thun:", "tokens": ["Was", "and\u00b7re", "mit", "be\u00b7schwe\u00b7ren", "thun", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPR", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich darff in meiner stuben bleiben/", "tokens": ["Ich", "darff", "in", "mei\u00b7ner", "stu\u00b7ben", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ja auch wohl gar im bette ruhn/", "tokens": ["Ja", "auch", "wohl", "gar", "im", "bet\u00b7te", "ruhn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ADV", "ADV", "APPRART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So f\u00e4llt mir doch in solcher ruh/", "tokens": ["So", "f\u00e4llt", "mir", "doch", "in", "sol\u00b7cher", "ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das allersch\u00f6nste liedgen zu.", "tokens": ["Das", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "lied\u00b7gen", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "7. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Ich f\u00fchle keinen schmertz davon:", "tokens": ["Ich", "f\u00fch\u00b7le", "kei\u00b7nen", "schmertz", "da\u00b7von", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er geht die gassen auff und nieder/", "tokens": ["Er", "geht", "die", "gas\u00b7sen", "auff", "und", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "KON", "PTKVZ", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und hat nur qvaal und noth zu lohn:", "tokens": ["Und", "hat", "nur", "qvaal", "und", "noth", "zu", "lohn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hingegen ich leb unbetr\u00fcbt/", "tokens": ["Hin\u00b7ge\u00b7gen", "ich", "leb", "un\u00b7be\u00b7tr\u00fcbt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und bin nur ins papier verliebt.", "tokens": ["Und", "bin", "nur", "ins", "pa\u00b7pier", "ver\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "8. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Jhr habt mich offtermahls erqvickt:", "tokens": ["Ihr", "habt", "mich", "off\u00b7ter\u00b7mahls", "er\u00b7qvickt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Derhalben bin ich euch verbunden/", "tokens": ["Der\u00b7hal\u00b7ben", "bin", "ich", "euch", "ver\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und werde gleichsam gantz entz\u00fcckt/", "tokens": ["Und", "wer\u00b7de", "gleich\u00b7sam", "gantz", "ent\u00b7z\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So bald mein geist auff dieses gut", "tokens": ["So", "bald", "mein", "geist", "auff", "die\u00b7ses", "gut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "APPR", "PDAT", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Noch einen blick zur\u00fccke thut.", "tokens": ["Noch", "ei\u00b7nen", "blick", "zu\u00b7r\u00fc\u00b7cke", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}