{"textgrid.poem.66240": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Soll wieder unsre Welt in Blute schwimmen,", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Soll wieder unsre Welt in Blute schwimmen,", "tokens": ["Soll", "wie\u00b7der", "uns\u00b7re", "Welt", "in", "Blu\u00b7te", "schwim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weil euer Herrscherstolz gebeut,", "tokens": ["Weil", "eu\u00b7er", "Herr\u00b7scher\u00b7stolz", "ge\u00b7beut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und euer Donnerruf die Stimmen", "tokens": ["Und", "eu\u00b7er", "Don\u00b7ner\u00b7ruf", "die", "Stim\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Friedenss\u00f6hne \u00fcberschreit?", "tokens": ["Der", "Frie\u00b7dens\u00b7s\u00f6h\u00b7ne", "\u00fc\u00b7bersc\u00b7hreit", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach, schrecklich ist's, der Menschen Mark vergeuden,", "tokens": ["Ach", ",", "schreck\u00b7lich", "ist's", ",", "der", "Men\u00b7schen", "Mark", "ver\u00b7geu\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJD", "VAFIN", "$,", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und mit der W\u00fcrgehand", "tokens": ["Und", "mit", "der", "W\u00fcr\u00b7ge\u00b7hand"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Umw\u00fchlen in der Menschen Eingeweiden,", "tokens": ["Um\u00b7w\u00fch\u00b7len", "in", "der", "Men\u00b7schen", "Ein\u00b7ge\u00b7wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Vom Schlachtendurst entbrannt!", "tokens": ["Vom", "Schlach\u00b7ten\u00b7durst", "ent\u00b7brannt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Steckt eure Schwerter in die Scheide,", "tokens": ["Steckt", "eu\u00b7re", "Schwer\u00b7ter", "in", "die", "Schei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "La\u00dft eure Donnerschl\u00fcnde ruh'n!", "tokens": ["La\u00dft", "eu\u00b7re", "Don\u00b7ner\u00b7schl\u00fcn\u00b7de", "ruh'n", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Gibt's gr\u00f6\u00dfern Ruhm, gibt's rein're Freude,", "tokens": ["Gibt's", "gr\u00f6\u00b7\u00dfern", "Ruhm", ",", "gibt's", "rein'\u00b7re", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Als Friede geben, Gutes thun?", "tokens": ["Als", "Frie\u00b7de", "ge\u00b7ben", ",", "Gu\u00b7tes", "thun", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVINF", "$,", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}