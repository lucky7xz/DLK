{"textgrid.poem.34819": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es treibt dich fort von Ort zu Ort,", "tokens": ["Es", "treibt", "dich", "fort", "von", "Ort", "zu", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du wei\u00dft nicht mal warum;", "tokens": ["Du", "wei\u00dft", "nicht", "mal", "wa\u00b7rum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "PWAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Winde klingt ein sanftes Wort,", "tokens": ["Im", "Win\u00b7de", "klingt", "ein", "sanf\u00b7tes", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schaust dich verwundert um.", "tokens": ["Schaust", "dich", "ver\u00b7wun\u00b7dert", "um", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Liebe, die dahinten blieb,", "tokens": ["Die", "Lie\u00b7be", ",", "die", "da\u00b7hin\u00b7ten", "blieb", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie ruft dich sanft zur\u00fcck:", "tokens": ["Sie", "ruft", "dich", "sanft", "zu\u00b7r\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O komm zur\u00fcck, ich hab dich lieb,", "tokens": ["O", "komm", "zu\u00b7r\u00fcck", ",", "ich", "hab", "dich", "lieb", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$,", "PPER", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du bist mein einz'ges Gl\u00fcck!", "tokens": ["Du", "bist", "mein", "einz'\u00b7ges", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch weiter, weiter, sonder Rast,", "tokens": ["Doch", "wei\u00b7ter", ",", "wei\u00b7ter", ",", "son\u00b7der", "Rast", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du darfst nicht stillestehn;", "tokens": ["Du", "darfst", "nicht", "stil\u00b7les\u00b7tehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was du so sehr geliebet hast,", "tokens": ["Was", "du", "so", "sehr", "ge\u00b7lie\u00b7bet", "hast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sollst du nicht wiedersehn.", "tokens": ["Sollst", "du", "nicht", "wie\u00b7der\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}