{"textgrid.poem.56673": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Die Sprache", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sprachlos willst du die nackte Welt genie\u00dfen", "tokens": ["Sprach\u00b7los", "willst", "du", "die", "nack\u00b7te", "Welt", "ge\u00b7nie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und tief einf\u00fchlend dich in ihr verlieren,", "tokens": ["und", "tief", "ein\u00b7f\u00fch\u00b7lend", "dich", "in", "ihr", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "ohne in Worten sie zu portr\u00e4tieren", "tokens": ["oh\u00b7ne", "in", "Wor\u00b7ten", "sie", "zu", "por\u00b7tr\u00e4\u00b7tie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "APPR", "NN", "PPER", "PTKZU", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "und sie in hohle Kl\u00e4nge umzugie\u00dfen?", "tokens": ["und", "sie", "in", "hoh\u00b7le", "Kl\u00e4n\u00b7ge", "um\u00b7zu\u00b7gie\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch aus der Sprache deine Wunder sprie\u00dfen,", "tokens": ["Doch", "aus", "der", "Spra\u00b7che", "dei\u00b7ne", "Wun\u00b7der", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "in deiner Sprache nur kristallisieren", "tokens": ["in", "dei\u00b7ner", "Spra\u00b7che", "nur", "kris\u00b7tal\u00b7li\u00b7sie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "die j\u00e4hen Bilder, die gleich wilden Tieren", "tokens": ["die", "j\u00e4\u00b7hen", "Bil\u00b7der", ",", "die", "gleich", "wil\u00b7den", "Tie\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "chaotisch w\u00fctend durcheinander schie\u00dfen,", "tokens": ["chao\u00b7tisch", "w\u00fc\u00b7tend", "durch\u00b7ein\u00b7an\u00b7der", "schie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "zu deiner schimmernd festgef\u00fcgten Welt.", "tokens": ["zu", "dei\u00b7ner", "schim\u00b7mernd", "fest\u00b7ge\u00b7f\u00fcg\u00b7ten", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und da\u00df dich diese Worte selbst nur malen,", "tokens": ["Und", "da\u00df", "dich", "die\u00b7se", "Wor\u00b7te", "selbst", "nur", "ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PDAT", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "klag sie nicht an, denn ohne sie zerf\u00e4llt", "tokens": ["klag", "sie", "nicht", "an", ",", "denn", "oh\u00b7ne", "sie", "zer\u00b7f\u00e4llt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,", "KON", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "des Daseins Klang und siebenfarbig Strahlen", "tokens": ["des", "Da\u00b7seins", "Klang", "und", "sie\u00b7ben\u00b7far\u00b7big", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "in ewig w\u00fcste Nacht, schaurig erhellt", "tokens": ["in", "e\u00b7wig", "w\u00fcs\u00b7te", "Nacht", ",", "schau\u00b7rig", "er\u00b7hellt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,", "ADJD", "VVFIN"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "von aller N\u00f6te flammenden Fanalen.", "tokens": ["von", "al\u00b7ler", "N\u00f6\u00b7te", "flam\u00b7men\u00b7den", "Fa\u00b7na\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}