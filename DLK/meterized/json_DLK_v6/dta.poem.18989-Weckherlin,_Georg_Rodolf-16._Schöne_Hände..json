{"dta.poem.18989": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "16.  \n  Sch\u00f6ne H\u00e4nde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.85", "af:0.14"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Glickseelig bin ich wol/ weil sie mir jhre hand/", "tokens": ["Glick\u00b7see\u00b7lig", "bin", "ich", "wol", "/", "weil", "sie", "mir", "jhre", "hand", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "$(", "KOUS", "PPER", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+----+-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Hand vnsers vertrags zeug/ vnd vnsers fridens", "tokens": ["Hand", "vn\u00b7sers", "ver\u00b7trags", "zeug", "/", "vnd", "vn\u00b7sers", "fri\u00b7dens"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$(", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "zaichen/", "tokens": ["zai\u00b7chen", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Auch jhrer gunst vnd lieb vnverw\u00fcrfliches pfand", "tokens": ["Auch", "jhrer", "gunst", "vnd", "lieb", "vn\u00b7ver\u00b7w\u00fcrf\u00b7li\u00b7ches", "pfand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "KON", "ADJD", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Numehr/ nach meinem wunsch zu k\u00fcssen/ will", "tokens": ["Nu\u00b7mehr", "/", "nach", "mei\u00b7nem", "wunsch", "zu", "k\u00fcs\u00b7sen", "/", "will"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "$(", "APPR", "PPOSAT", "ADJD", "PTKZU", "VVINF", "$(", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "darraichen.", "tokens": ["dar\u00b7rai\u00b7chen", "."], "token_info": ["word", "punct"], "pos": ["PAV", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "O hand/ die du zuvor mein hertz mit taussent straiche\u0304", "tokens": ["O", "hand", "/", "die", "du", "zu\u00b7vor", "mein", "hertz", "mit", "taus\u00b7sent", "straich\u0113"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$(", "PRELS", "PPER", "ADV", "PPOSAT", "NN", "APPR", "CARD", "XY"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gefoltert/ du bist nu f\u00fcr meine wund ein band;", "tokens": ["Ge\u00b7fol\u00b7tert", "/", "du", "bist", "nu", "f\u00fcr", "mei\u00b7ne", "wund", "ein", "band", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$(", "PPER", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O hand/ ab deren schnee man mich oft sah ver-", "tokens": ["O", "hand", "/", "ab", "de\u00b7ren", "schnee", "man", "mich", "oft", "sah", "ver"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$(", "APPR", "PDS", "VVFIN", "PIS", "PPER", "ADV", "VVFIN", "TRUNC"], "meter": "--+--+-+++-", "measure": "anapaest.di.plus"}, "line.4": {"text": "blaichen/", "tokens": ["blai\u00b7chen", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Zuvor ein bra\u0303d der lieb/ du bist nu mein wolstand.", "tokens": ["Zu\u00b7vor", "ein", "br\u00e3d", "der", "lieb", "/", "du", "bist", "nu", "mein", "wol\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "ADJD", "$(", "PPER", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wan du dan so wol kanst beleben vnd entleiben/", "tokens": ["Wan", "du", "dan", "so", "wol", "kanst", "be\u00b7le\u00b7ben", "vnd", "ent\u00b7lei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ADV", "VMFIN", "VVINF", "KON", "VVINF", "$("], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Vnd kanst den G\u00f6ttern selbs der Lieb vnd Ehr", "tokens": ["Vnd", "kanst", "den", "G\u00f6t\u00b7tern", "selbs", "der", "Lieb", "vnd", "Ehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "NN", "ADV", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "gesatz", "tokens": ["ge\u00b7satz"], "token_info": ["word"], "pos": ["NE"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "F\u00fcrschreibend/ frewd vnd layd mitthailen vnd", "tokens": ["F\u00fcrsc\u00b7hrei\u00b7bend", "/", "frewd", "vnd", "layd", "mitt\u00b7hai\u00b7len", "vnd"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$(", "NN", "KON", "NN", "VVINF", "KON"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "vertreiben:", "tokens": ["ver\u00b7trei\u00b7ben", ":"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "So schw\u00f6r ich dir/ O hand/ ich schw\u00f6r bey disem", "tokens": ["So", "schw\u00f6r", "ich", "dir", "/", "O", "hand", "/", "ich", "schw\u00f6r", "bey", "di\u00b7sem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$(", "NE", "NN", "$(", "PPER", "VVFIN", "APPR", "PDAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "schmatz/", "tokens": ["schmatz", "/"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Du sanfftes helfenbein solt f\u00fcrhin stehts mein", "tokens": ["Du", "sanff\u00b7tes", "hel\u00b7fen\u00b7bein", "solt", "f\u00fcr\u00b7hin", "stehts", "mein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "VMFIN", "ADV", "VVFIN", "PPOSAT"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "schatz/", "tokens": ["schatz", "/"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Vnd meines lebens hand/ band/ brand/ pfand/", "tokens": ["Vnd", "mei\u00b7nes", "le\u00b7bens", "hand", "/", "band", "/", "brand", "/", "pfand", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$(", "VVFIN", "$(", "VVFIN", "$(", "VVFIN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "wolstand bleiben.", "tokens": ["wol\u00b7stand", "blei\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}