{"dta.poem.8624": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Bitte .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Weil' auf mir, du dunkles Auge,", "tokens": ["Weil'", "auf", "mir", ",", "du", "dunk\u00b7les", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Uebe deine ganze Macht,", "tokens": ["Ue\u00b7be", "dei\u00b7ne", "gan\u00b7ze", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ernste, milde, tr\u00e4umerische,", "tokens": ["Erns\u00b7te", ",", "mil\u00b7de", ",", "tr\u00e4u\u00b7me\u00b7ri\u00b7sche", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unergr\u00fcndlich s\u00fc\u00dfe Nacht!", "tokens": ["Un\u00b7er\u00b7gr\u00fcnd\u00b7lich", "s\u00fc\u00b7\u00dfe", "Nacht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nimm mit deinem Zauberdunkel", "tokens": ["Nimm", "mit", "dei\u00b7nem", "Zau\u00b7ber\u00b7dun\u00b7kel"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diese Welt von hinnen mir,", "tokens": ["Die\u00b7se", "Welt", "von", "hin\u00b7nen", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "ADV", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df du \u00fcber meinem Leben", "tokens": ["Da\u00df", "du", "\u00fc\u00b7ber", "mei\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einsam schwebest f\u00fcr und f\u00fcr.", "tokens": ["Ein\u00b7sam", "schwe\u00b7best", "f\u00fcr", "und", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "KON", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}