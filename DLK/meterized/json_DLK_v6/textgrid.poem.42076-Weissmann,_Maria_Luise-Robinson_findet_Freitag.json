{"textgrid.poem.42076": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Robinson findet Freitag", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er blieb, da er mich sah, erschrocken stehn,", "tokens": ["Er", "blieb", ",", "da", "er", "mich", "sah", ",", "er\u00b7schro\u00b7cken", "stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich stand, der ihn erblickte, Stein, verblieben", "tokens": ["Ich", "stand", ",", "der", "ihn", "er\u00b7blick\u00b7te", ",", "Stein", ",", "ver\u00b7blie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In der Geb\u00e4rde: himmlischem Vergehn.", "tokens": ["In", "der", "Ge\u00b7b\u00e4r\u00b7de", ":", "himm\u00b7li\u00b7schem", "Ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "O Menschen-Wohlgestalt! O Gl\u00fcck, zu lieben", "tokens": ["O", "Men\u00b7schen\u00b7Wohl\u00b7ge\u00b7stalt", "!", "O", "Gl\u00fcck", ",", "zu", "lie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$.", "NE", "NN", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Im Blick Verwandtes: Auge, Lippe, Knie,", "tokens": ["Im", "Blick", "Ver\u00b7wand\u00b7tes", ":", "Au\u00b7ge", ",", "Lip\u00b7pe", ",", "Knie", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$.", "NN", "$,", "NE", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein Ohr wie meines, F\u00fc\u00dfe, f\u00fcnfgespalten,", "tokens": ["Ein", "Ohr", "wie", "mei\u00b7nes", ",", "F\u00fc\u00b7\u00dfe", ",", "f\u00fcnf\u00b7ge\u00b7spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "PPOSAT", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und H\u00e4nde, ganz vertraute Form, wie die", "tokens": ["Und", "H\u00e4n\u00b7de", ",", "ganz", "ver\u00b7trau\u00b7te", "Form", ",", "wie", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "$,", "ADV", "ADJA", "NN", "$,", "PWAV", "ART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sich breitend jetzt, der Schritt, nun aufzuhalten", "tokens": ["Sich", "brei\u00b7tend", "jetzt", ",", "der", "Schritt", ",", "nun", "auf\u00b7zu\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PRF", "ADJD", "ADV", "$,", "ART", "NN", "$,", "ADV", "VVIZU"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "L\u00e4nger nicht mehr und dort erwidert, scheu...", "tokens": ["L\u00e4n\u00b7ger", "nicht", "mehr", "und", "dort", "er\u00b7wi\u00b7dert", ",", "scheu", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "KON", "ADV", "VVPP", "$,", "ADJD", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.10": {"text": "Antwort jetzt meinem Ruf. O s\u00fc\u00df Get\u00f6n", "tokens": ["Ant\u00b7wort", "jetzt", "mei\u00b7nem", "Ruf", ".", "O", "s\u00fc\u00df", "Ge\u00b7t\u00f6n"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "PPOSAT", "NN", "$.", "NE", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Von Stimme! Schuf mir zum Gespielen neu", "tokens": ["Von", "Stim\u00b7me", "!", "Schuf", "mir", "zum", "Ge\u00b7spie\u00b7len", "neu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$.", "NN", "PPER", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Spiegelnd mich Einsamkeit?", "tokens": ["Spie\u00b7gelnd", "mich", "Ein\u00b7sam\u00b7keit", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.13": {"text": "Bin ich so sch\u00f6n?!", "tokens": ["Bin", "ich", "so", "sch\u00f6n", "?!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}