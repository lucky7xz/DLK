{"textgrid.poem.33039": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und wieder kehre ich aus fremdem Lande", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wieder kehre ich aus fremdem Lande", "tokens": ["Und", "wie\u00b7der", "keh\u00b7re", "ich", "aus", "frem\u00b7dem", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und seines Lebens buntes Bilderspiel", "tokens": ["und", "sei\u00b7nes", "Le\u00b7bens", "bun\u00b7tes", "Bil\u00b7der\u00b7spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "vergl\u00fcht zu stiller, wei\u00dfer Flamme:", "tokens": ["ver\u00b7gl\u00fcht", "zu", "stil\u00b7ler", ",", "wei\u00b7\u00dfer", "Flam\u00b7me", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du in dir nur tr\u00e4gst den Punkt,", "tokens": ["Du", "in", "dir", "nur", "tr\u00e4gst", "den", "Punkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "in dem sich alles fa\u00dft und findet", "tokens": ["in", "dem", "sich", "al\u00b7les", "fa\u00dft", "und", "fin\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PRF", "PIS", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "und l\u00f6st und bindet ...", "tokens": ["und", "l\u00f6st", "und", "bin\u00b7det", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Du bist die Welt und nicht das laute", "tokens": ["Du", "bist", "die", "Welt", "und", "nicht", "das", "lau\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "PTKNEG", "PDS", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "vieldeutig immer andere Ding,", "tokens": ["viel\u00b7deu\u00b7tig", "im\u00b7mer", "an\u00b7de\u00b7re", "Ding", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "das sich so nennt, das niemand kennt", "tokens": ["das", "sich", "so", "nennt", ",", "das", "nie\u00b7mand", "kennt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "PRF", "ADV", "VVFIN", "$,", "PRELS", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "und nichts und alles ist! ... du bist die Welt", "tokens": ["und", "nichts", "und", "al\u00b7les", "ist", "!", "...", "du", "bist", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIS", "KON", "PIS", "VAFIN", "$.", "$(", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "und nicht die L\u00e4nder, nicht die Meere,", "tokens": ["und", "nicht", "die", "L\u00e4n\u00b7der", ",", "nicht", "die", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "$,", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "die du durchquerst in raschem Flug,", "tokens": ["die", "du", "durch\u00b7querst", "in", "ra\u00b7schem", "Flug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "auch nicht was Menschenk\u00f6nnen schuf ...", "tokens": ["auch", "nicht", "was", "Men\u00b7schen\u00b7k\u00f6n\u00b7nen", "schuf", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "PWS", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du bist die Welt und du allein ...", "tokens": ["Du", "bist", "die", "Welt", "und", "du", "al\u00b7lein", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "PPER", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und bist du Gottes, wird sie Gottes sein!", "tokens": ["und", "bist", "du", "Got\u00b7tes", ",", "wird", "sie", "Got\u00b7tes", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "$,", "VAFIN", "PPER", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}