{"dta.poem.19104": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "LxXXVI.   Auf den Superintendenten Jo-  \n sephi in Sorau.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Joseph! mein verborgner Bruder, l\u00e4nger halt ich mich\nnun nicht,", "tokens": ["Jo\u00b7se\u00b7ph", "!", "mein", "ver\u00b7borg\u00b7ner", "Bru\u00b7der", ",", "l\u00e4n\u00b7ger", "halt", "ich", "mich", "nun", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPOSAT", "ADJA", "NN", "$,", "ADJD", "VVFIN", "PPER", "PRF", "ADV", "PTKNEG", "$,"], "meter": "+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.invert"}, "line.2": {"text": "Dich f\u00fcr allen \u00f6ffentlich einen Knecht des HErrn zu nennen,", "tokens": ["Dich", "f\u00fcr", "al\u00b7len", "\u00f6f\u00b7fent\u00b7lich", "ei\u00b7nen", "Knecht", "des", "Herrn", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PIAT", "ADJD", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Deine Liebe gegen den, der die Lieb ist, zu bekennen;", "tokens": ["Dei\u00b7ne", "Lie\u00b7be", "ge\u00b7gen", "den", ",", "der", "die", "Lieb", "ist", ",", "zu", "be\u00b7ken\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "$,", "PRELS", "ART", "NN", "VAFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Mu\u00df ich denn der Seligkeit, mich mit Br\u00fcdern zu erquicken,", "tokens": ["Mu\u00df", "ich", "denn", "der", "Se\u00b7lig\u00b7keit", ",", "mich", "mit", "Br\u00fc\u00b7dern", "zu", "er\u00b7qui\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "$,", "PRF", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und mit ihnen aus dem Glauben zu verst\u00e4rcken, m\u00fc\u00dfig gehn:", "tokens": ["Und", "mit", "ih\u00b7nen", "aus", "dem", "Glau\u00b7ben", "zu", "ver\u00b7st\u00e4r\u00b7cken", ",", "m\u00fc\u00b7\u00dfig", "gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Will ich endlich meine Fl\u00fcgel bi\u00df zu GOttes Stuhl erh\u00f6hn,", "tokens": ["Will", "ich", "end\u00b7lich", "mei\u00b7ne", "Fl\u00fc\u00b7gel", "bi\u00df", "zu", "Got\u00b7tes", "Stuhl", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "NN", "APPR", "APPR", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Wo sie all erseits im Geist nach der Streiter L\u00e4gern blicken.", "tokens": ["Wo", "sie", "all", "er\u00b7seits", "im", "Geist", "nach", "der", "Strei\u00b7ter", "L\u00e4\u00b7gern", "bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "ADV", "APPRART", "NN", "APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Die du lange schon gef\u00fchlt, la\u00df ich alle sehn und h\u00f6ren,", "tokens": ["Die", "du", "lan\u00b7ge", "schon", "ge\u00b7f\u00fchlt", ",", "la\u00df", "ich", "al\u00b7le", "sehn", "und", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVPP", "$,", "VVIMP", "PPER", "PIS", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.9": {"text": "Deren Uberlegungs-Kraft nicht die Vorurtheile st\u00f6hren.", "tokens": ["De\u00b7ren", "U\u00b7ber\u00b7le\u00b7gungs\u00b7Kraft", "nicht", "die", "Vor\u00b7urt\u00b7hei\u00b7le", "st\u00f6h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "+-+--++--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Ich vergesse nimmermehr, was du an des HErren Tage", "tokens": ["Ich", "ver\u00b7ges\u00b7se", "nim\u00b7mer\u00b7mehr", ",", "was", "du", "an", "des", "Her\u00b7ren", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Gantz geheim mit mir gesprochen, wie du deinen Wandel", "tokens": ["Gantz", "ge\u00b7heim", "mit", "mir", "ge\u00b7spro\u00b7chen", ",", "wie", "du", "dei\u00b7nen", "Wan\u00b7del"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "PPER", "VVPP", "$,", "PWAV", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.12": {"text": "Wie du deiner Br\u00fcder Hertz gerne in einander r\u00fchrt\u2019st,", "tokens": ["Wie", "du", "dei\u00b7ner", "Br\u00fc\u00b7der", "Hertz", "ger\u00b7ne", "in", "ein\u00b7an\u00b7der", "r\u00fchrt'st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "NN", "ADV", "APPR", "PRF", "VVFIN", "$,"], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Wo du aber inne hielt\u2019st: ", "tokens": ["Wo", "du", "a\u00b7ber", "in\u00b7ne", "hielt'st", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PTKVZ", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Joseph! deinen Hirten-Stecken kanst du mit getrostem Sinn", "tokens": ["Jo\u00b7se\u00b7ph", "!", "dei\u00b7nen", "Hir\u00b7ten\u00b7Ste\u00b7cken", "kanst", "du", "mit", "ge\u00b7tros\u00b7tem", "Sinn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PPOSAT", "NN", "VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.invert"}, "line.15": {"text": "Jesu, deinem Ober-HErrn, der dich einhohlt, \u00fcberreichen.", "tokens": ["Je\u00b7su", ",", "dei\u00b7nem", "O\u00b7ber\u00b7HErrn", ",", "der", "dich", "ein\u00b7hohlt", ",", "\u00fc\u00b7berr\u00b7ei\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.16": {"text": "Sorau! unser Ertz-Bischof gebe dir bald einen gleichen,", "tokens": ["So\u00b7rau", "!", "un\u00b7ser", "Ertz\u00b7Bi\u00b7schof", "ge\u00b7be", "dir", "bald", "ei\u00b7nen", "glei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "ART", "ADJA", "$,"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Da\u00df es hier nicht heissen m\u00f6ge: ", "tokens": ["Da\u00df", "es", "hier", "nicht", "heis\u00b7sen", "m\u00f6\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}