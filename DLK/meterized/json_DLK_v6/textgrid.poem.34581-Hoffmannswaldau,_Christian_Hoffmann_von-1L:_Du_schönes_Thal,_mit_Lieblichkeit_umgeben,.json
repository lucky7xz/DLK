{"textgrid.poem.34581": {"metadata": {"author": {"name": "Hoffmannswaldau, Christian Hoffmann von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du sch\u00f6nes Thal, mit Lieblichkeit umgeben,", "genre": "verse", "period": "N.A.", "pub_year": 1647, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du sch\u00f6nes Thal, mit Lieblichkeit umgeben,", "tokens": ["Du", "sch\u00f6\u00b7nes", "Thal", ",", "mit", "Lieb\u00b7lich\u00b7keit", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In dessen Schoo\u00df viel tausend Blumen weben,", "tokens": ["In", "des\u00b7sen", "Schoo\u00df", "viel", "tau\u00b7send", "Blu\u00b7men", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ADV", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df meine Klagen ein!", "tokens": ["La\u00df", "mei\u00b7ne", "Kla\u00b7gen", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "La\u00df, was du siehst aus meinen Augen schie\u00dfen,", "tokens": ["La\u00df", ",", "was", "du", "siehst", "aus", "mei\u00b7nen", "Au\u00b7gen", "schie\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Durch Laub und Gras der sch\u00f6nen Gegend flie\u00dfen", "tokens": ["Durch", "Laub", "und", "Gras", "der", "sch\u00f6\u00b7nen", "Ge\u00b7gend", "flie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und ihren Schmelz damit gewaschen sein!", "tokens": ["Und", "ih\u00b7ren", "Schmelz", "da\u00b7mit", "ge\u00b7wa\u00b7schen", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PAV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du stolzer Berg, mit B\u00e4umen wohl besetzet,", "tokens": ["Du", "stol\u00b7zer", "Berg", ",", "mit", "B\u00e4u\u00b7men", "wohl", "be\u00b7set\u00b7zet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So keine Hand der M\u00e4nner hat verletzet,", "tokens": ["So", "kei\u00b7ne", "Hand", "der", "M\u00e4n\u00b7ner", "hat", "ver\u00b7let\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ART", "NN", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Jungfrau'n sind, wie ich,", "tokens": ["Und", "Jung\u00b7frau'n", "sind", ",", "wie", "ich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "$,", "PWAV", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Verachte nicht, was meine Wehmuth bringet,", "tokens": ["Ver\u00b7ach\u00b7te", "nicht", ",", "was", "mei\u00b7ne", "Weh\u00b7muth", "brin\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und so sie dich nicht auch zu klagen zwinget,", "tokens": ["Und", "so", "sie", "dich", "nicht", "auch", "zu", "kla\u00b7gen", "zwin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "PRF", "PTKNEG", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So mu\u00df ich billig trauern \u00fcber dich.", "tokens": ["So", "mu\u00df", "ich", "bil\u00b7lig", "trau\u00b7ern", "\u00fc\u00b7ber", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es wird mein Fu\u00df dich k\u00fcnftig nicht beschreiten,", "tokens": ["Es", "wird", "mein", "Fu\u00df", "dich", "k\u00fcnf\u00b7tig", "nicht", "be\u00b7schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "PPER", "ADJD", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Wiederschall wird nicht mehr mit mir streiten,", "tokens": ["Der", "Wie\u00b7der\u00b7schall", "wird", "nicht", "mehr", "mit", "mir", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Mund spricht: gute Nacht!", "tokens": ["Mein", "Mund", "spricht", ":", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ihr Bl\u00e4tter, kommt und werdet mir zu Zungen,", "tokens": ["Ihr", "Bl\u00e4t\u00b7ter", ",", "kommt", "und", "wer\u00b7det", "mir", "zu", "Zun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "KON", "VAFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und weil ich euch vor diesem viel gesungen,", "tokens": ["Und", "weil", "ich", "euch", "vor", "die\u00b7sem", "viel", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "APPR", "PDAT", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So singt nun ihr, was mir den Tod gebracht.", "tokens": ["So", "singt", "nun", "ihr", ",", "was", "mir", "den", "Tod", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "$,", "PWS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Du sch\u00f6ner Flu\u00df, der du die Gegend zierest", "tokens": ["Du", "sch\u00f6\u00b7ner", "Flu\u00df", ",", "der", "du", "die", "Ge\u00b7gend", "zie\u00b7rest"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und mehr Kristall, als Wasserfluthen f\u00fchrest,", "tokens": ["Und", "mehr", "Kris\u00b7tall", ",", "als", "Was\u00b7ser\u00b7flut\u00b7hen", "f\u00fch\u00b7rest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Nimm an mein Ach und Weh!", "tokens": ["Nimm", "an", "mein", "Ach", "und", "Weh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du reiner Flu\u00df, nimm meine reinen Z\u00e4hren \u2013", "tokens": ["Du", "rei\u00b7ner", "Flu\u00df", ",", "nimm", "mei\u00b7ne", "rei\u00b7nen", "Z\u00e4h\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "VVIMP", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich wei\u00df nichts itzo Reiner's zu gew\u00e4hren \u2013", "tokens": ["Ich", "wei\u00df", "nichts", "it\u00b7zo", "Rei\u00b7ner's", "zu", "ge\u00b7w\u00e4h\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "NE", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und schenke sie alsdann der w\u00fcsten See!", "tokens": ["Und", "schen\u00b7ke", "sie", "als\u00b7dann", "der", "w\u00fcs\u00b7ten", "See", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Was aber will ich Arme doch beginnen?", "tokens": ["Was", "a\u00b7ber", "will", "ich", "Ar\u00b7me", "doch", "be\u00b7gin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "PPER", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was plag' ich doch durch Klagen meine Sinnen?", "tokens": ["Was", "plag'", "ich", "doch", "durch", "Kla\u00b7gen", "mei\u00b7ne", "Sin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es ist um mich gethan.", "tokens": ["Es", "ist", "um", "mich", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Jugend hei\u00dft mich ferner sein und leben,", "tokens": ["Die", "Ju\u00b7gend", "hei\u00dft", "mich", "fer\u00b7ner", "sein", "und", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "VAINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und der, so mir das Leben hat gegeben,", "tokens": ["Und", "der", ",", "so", "mir", "das", "Le\u00b7ben", "hat", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "ADV", "PPER", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Macht, da\u00df ich nicht mehr leben kann.", "tokens": ["Macht", ",", "da\u00df", "ich", "nicht", "mehr", "le\u00b7ben", "kann", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "O schwerer Sieg! o ungl\u00fcckselig Streiten!", "tokens": ["O", "schwe\u00b7rer", "Sieg", "!", "o", "un\u00b7gl\u00fcck\u00b7se\u00b7lig", "Strei\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "FM", "ADJD", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des Vaters Ruhm mu\u00df mir das Grab bereiten;", "tokens": ["Des", "Va\u00b7ters", "Ruhm", "mu\u00df", "mir", "das", "Grab", "be\u00b7rei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Liebe bringt Gefahr.", "tokens": ["Die", "Lie\u00b7be", "bringt", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mein Untergang vermehrt der Feinde Haufen;", "tokens": ["Mein", "Un\u00b7ter\u00b7gang", "ver\u00b7mehrt", "der", "Fein\u00b7de", "Hau\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es mu\u00df mein Blut zu ihrem Blute laufen;", "tokens": ["Es", "mu\u00df", "mein", "Blut", "zu", "ih\u00b7rem", "Blu\u00b7te", "lau\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Tochter Tod vermehrt der Feinde Schaar.", "tokens": ["Der", "Toch\u00b7ter", "Tod", "ver\u00b7mehrt", "der", "Fein\u00b7de", "Schaar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ganz Ammon wird des Vaters Sieg belachen", "tokens": ["Ganz", "Am\u00b7mon", "wird", "des", "Va\u00b7ters", "Sieg", "be\u00b7la\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "VAFIN", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und einen Scherz aus Jephta's Tochter machen;", "tokens": ["Und", "ei\u00b7nen", "Scherz", "aus", "Je\u00b7phta's", "Toch\u00b7ter", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NE", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hier ist kein Unterscheid.", "tokens": ["Hier", "ist", "kein", "Un\u00b7ter\u00b7scheid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ganz Ammon trotzt und mu\u00df durch's Schwert verderben;", "tokens": ["Ganz", "Am\u00b7mon", "trotzt", "und", "mu\u00df", "durch's", "Schwert", "ver\u00b7der\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "KON", "VMFIN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Tochter liebt und mu\u00df, wie Ammon, sterben;", "tokens": ["Die", "Toch\u00b7ter", "liebt", "und", "mu\u00df", ",", "wie", "Am\u00b7mon", ",", "ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VMFIN", "$,", "PWAV", "NE", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Aus Ammon's Blut bl\u00fcht Angst und Herzeleid.", "tokens": ["Aus", "Am\u00b7mon's", "Blut", "bl\u00fcht", "Angst", "und", "Her\u00b7ze\u00b7leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Der Vater schlug der Feinde Trotz danieder;", "tokens": ["Der", "Va\u00b7ter", "schlug", "der", "Fein\u00b7de", "Trotz", "da\u00b7nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jetzt r\u00e4cht der Feind sich an dem Vater wieder,", "tokens": ["Jetzt", "r\u00e4cht", "der", "Feind", "sich", "an", "dem", "Va\u00b7ter", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Jetzt fleu\u00dft sein eigen Blut,", "tokens": ["Jetzt", "fleu\u00dft", "sein", "ei\u00b7gen", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sein eigen Blut, aus seinen Adern kommen,", "tokens": ["Sein", "ei\u00b7gen", "Blut", ",", "aus", "sei\u00b7nen", "A\u00b7dern", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sein eigen Blut, davon ich bin genommen,", "tokens": ["Sein", "ei\u00b7gen", "Blut", ",", "da\u00b7von", "ich", "bin", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PAV", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sein eigen Blut, sein Schatz, sein gr\u00f6\u00dftes Gut.", "tokens": ["Sein", "ei\u00b7ge