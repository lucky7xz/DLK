{"dta.poem.1703": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Umwechslung aller Dinge.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Das Trappeliren ist der Welt gemeines Spiel/", "tokens": ["Das", "Trap\u00b7pe\u00b7li\u00b7ren", "ist", "der", "Welt", "ge\u00b7mei\u00b7nes", "Spiel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr meistes Absehn ist ein vortheilhafftig Du.", "tokens": ["Ihr", "meis\u00b7tes", "Ab\u00b7sehn", "ist", "ein", "vor\u00b7theil\u00b7haff\u00b7tig", "Du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ART", "ADJD", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie offt betr\u00fcget sich/ wer bessers w\u00e4hlen will?", "tokens": ["Wie", "offt", "be\u00b7tr\u00fc\u00b7get", "sich", "/", "wer", "bes\u00b7sers", "w\u00e4h\u00b7len", "will", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PRF", "$(", "PWS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die herrlichste Figur vergeht in einem Nu.", "tokens": ["Die", "herr\u00b7lichs\u00b7te", "Fi\u00b7gur", "ver\u00b7geht", "in", "ei\u00b7nem", "Nu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADV", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Wie mancher wird von ", "tokens": ["Wie", "man\u00b7cher", "wird", "von"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VAFIN", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Doch irrt nicht/ wer allzeit auffs lezte Blatt wohl zielt.", "tokens": ["Doch", "irrt", "nicht", "/", "wer", "all\u00b7zeit", "auffs", "lez\u00b7te", "Blatt", "wohl", "zielt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$(", "PWS", "ADV", "APPRART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}