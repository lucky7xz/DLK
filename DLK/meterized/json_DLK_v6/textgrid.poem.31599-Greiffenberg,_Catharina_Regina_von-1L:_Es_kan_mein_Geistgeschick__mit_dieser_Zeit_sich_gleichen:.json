{"textgrid.poem.31599": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es kan mein Geistgeschick / mit dieser Zeit sich gleichen:", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es kan mein Geistgeschick / mit dieser Zeit sich gleichen:", "tokens": ["Es", "kan", "mein", "Geist\u00b7ge\u00b7schick", "/", "mit", "die\u00b7ser", "Zeit", "sich", "glei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "$(", "APPR", "PDAT", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wann Vngedult schier macht aus Jahren Ewigkeit /", "tokens": ["wann", "Vn\u00b7ge\u00b7dult", "schier", "macht", "aus", "Jah\u00b7ren", "E\u00b7wig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "VVFIN", "APPR", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und sich nach l\u00e4ngster K\u00e4lt erzeigt ein F\u00fcnklein Freud /", "tokens": ["und", "sich", "nach", "l\u00e4ngs\u00b7ter", "K\u00e4lt", "er\u00b7zeigt", "ein", "F\u00fcn\u00b7klein", "Freud", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "da\u00df man nichts gwissers hofft / als Schnee und Weh werd weichen", "tokens": ["da\u00df", "man", "nichts", "gwis\u00b7sers", "hofft", "/", "als", "Schnee", "und", "Weh", "werd", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PIS", "ADV", "VVFIN", "$(", "KOUS", "NN", "KON", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "und pflegt bey kalter Sonn und kleinem Trost / zu schleichen", "tokens": ["und", "pflegt", "bey", "kal\u00b7ter", "Sonn", "und", "klei\u00b7nem", "Trost", "/", "zu", "schlei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$(", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "ins halb-entbl\u00f6ste Feld / und bleiche frischungs-Heyd /", "tokens": ["ins", "halb\u00b7ent\u00b7bl\u00f6s\u00b7te", "Feld", "/", "und", "blei\u00b7che", "fri\u00b7schungs\u00b7Heyd", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$(", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "voll Trosts / es komm nun bald die Freud und Blumen Zeit /", "tokens": ["voll", "Trosts", "/", "es", "komm", "nun", "bald", "die", "Freud", "und", "Blu\u00b7men", "Zeit", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$(", "PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "KON", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "den frohen Fr\u00fclings-Port nun ehest zu erreichen.", "tokens": ["den", "fro\u00b7hen", "Fr\u00fc\u00b7lings\u00b7Port", "nun", "e\u00b7hest", "zu", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach Schmerz-verkehrter Schlu\u00df! jetzt kommet erst geflogen", "tokens": ["Ach", "Schmer\u00b7zver\u00b7kehr\u00b7ter", "Schlu\u00df", "!", "jetzt", "kom\u00b7met", "erst", "ge\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "NN", "$.", "ADV", "VVFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "das wei\u00dfe Wolken-Heer / der gr\u00fcnen Hoffnung Grab", "tokens": ["das", "wei\u00b7\u00dfe", "Wol\u00b7ken\u00b7Heer", "/", "der", "gr\u00fc\u00b7nen", "Hoff\u00b7nung", "Grab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "doch weicht ein standhaffts Herz / dem Widerstand nicht ab /", "tokens": ["doch", "weicht", "ein", "stand\u00b7haffts", "Herz", "/", "dem", "Wi\u00b7der\u00b7stand", "nicht", "ab", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$(", "ART", "NN", "PTKNEG", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "bi\u00df / nach zugeben / ihn der Himmel selbst bewogen:", "tokens": ["bi\u00df", "/", "nach", "zu\u00b7ge\u00b7ben", "/", "ihn", "der", "Him\u00b7mel", "selbst", "be\u00b7wo\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$(", "APPR", "VVINF", "$(", "PPER", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mann mu\u00df so lang mit ihm / durch Glaub und beten streiten /", "tokens": ["Mann", "mu\u00df", "so", "lang", "mit", "ihm", "/", "durch", "Glaub", "und", "be\u00b7ten", "strei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "ADJD", "APPR", "PPER", "$(", "APPR", "NN", "KON", "VVINF", "VVFIN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.14": {"text": "bi\u00df \u00fcberwunden er selbst tritt auf unsre Seiten.", "tokens": ["bi\u00df", "\u00fc\u00b7berw\u00b7un\u00b7den", "er", "selbst", "tritt", "auf", "uns\u00b7re", "Sei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}