{"textgrid.poem.66147": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Julie, mein Ebenbild,", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Julie, mein Ebenbild,", "tokens": ["O", "Ju\u00b7lie", ",", "mein", "E\u00b7ben\u00b7bild", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nur sanfter noch, und nicht so wild;", "tokens": ["Nur", "sanf\u00b7ter", "noch", ",", "und", "nicht", "so", "wild", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "$,", "KON", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An jeder Engelanmuth reich,", "tokens": ["An", "je\u00b7der", "En\u00b7ge\u00b7lan\u00b7muth", "reich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An Gro\u00dfgef\u00fchl dem Vater gleich.", "tokens": ["An", "Gro\u00df\u00b7ge\u00b7f\u00fchl", "dem", "Va\u00b7ter", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O Julie, bedaure mich;", "tokens": ["O", "Ju\u00b7lie", ",", "be\u00b7dau\u00b7re", "mich", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "PPER", "$."], "meter": "++--+-+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Denn ach! wie schwer entbehr' ich dich!", "tokens": ["Denn", "ach", "!", "wie", "schwer", "ent\u00b7behr'", "ich", "dich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PWAV", "ADJD", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem L\u00e4mmlein gleich h\u00fcpfst du nicht mehr", "tokens": ["Dem", "L\u00e4mm\u00b7lein", "gleich", "h\u00fcpfst", "du", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um deinen armen Vater her.", "tokens": ["Um", "dei\u00b7nen", "ar\u00b7men", "Va\u00b7ter", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du hemmst nicht mehr mit deinem Ku\u00df", "tokens": ["Du", "hemmst", "nicht", "mehr", "mit", "dei\u00b7nem", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Thr\u00e4nen m\u00e4chtigen Ergu\u00df,", "tokens": ["Der", "Thr\u00e4\u00b7nen", "m\u00e4ch\u00b7ti\u00b7gen", "Er\u00b7gu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und scheuchst mit deinem L\u00e4cheln nicht,", "tokens": ["Und", "scheuchst", "mit", "dei\u00b7nem", "L\u00e4\u00b7cheln", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Gram von meinem Angesicht.", "tokens": ["Den", "Gram", "von", "mei\u00b7nem", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O Julie, wann kommt die Zeit,", "tokens": ["O", "Ju\u00b7lie", ",", "wann", "kommt", "die", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PWAV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich mit Vaterz\u00e4rtlichkeit", "tokens": ["Da\u00df", "ich", "mit", "Va\u00b7ter\u00b7z\u00e4rt\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erst an mein Herz dich dr\u00fccken kann?", "tokens": ["Erst", "an", "mein", "Herz", "dich", "dr\u00fc\u00b7cken", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich armer, ich gefangner Mann.", "tokens": ["Ich", "ar\u00b7mer", ",", "ich", "ge\u00b7fang\u00b7ner", "Mann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wann lenk' ich deiner Stimme Ton", "tokens": ["Wann", "lenk'", "ich", "dei\u00b7ner", "Stim\u00b7me", "Ton"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am goldbezognen Pantalon,", "tokens": ["Am", "gold\u00b7be\u00b7zog\u00b7nen", "Pan\u00b7ta\u00b7lon", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und lehre dich mit Saitenklang", "tokens": ["Und", "leh\u00b7re", "dich", "mit", "Sai\u00b7ten\u00b7klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den deutschen herzlichen Gesang?", "tokens": ["Den", "deut\u00b7schen", "herz\u00b7li\u00b7chen", "Ge\u00b7sang", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Welsche ", "tokens": ["Der", "Wel\u00b7sche"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Durchs Labyrinth der T\u00f6n' und ", "tokens": ["Durchs", "La\u00b7by\u00b7rinth", "der", "T\u00f6n'", "und"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "KON"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Deutsche aber ", "tokens": ["Der", "Deut\u00b7sche", "a\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Da\u00df sein Gesang das Herz durchdringt.", "tokens": ["Da\u00df", "sein", "Ge\u00b7sang", "das", "Herz", "durch\u00b7dringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O Julie, wann kommt die Zeit,", "tokens": ["O", "Ju\u00b7lie", ",", "wann", "kommt", "die", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PWAV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich mit 