{"textgrid.poem.49482": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn E.C. Weise", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was h\u00f6r ich hier vor Symphonien?", "tokens": ["Was", "h\u00f6r", "ich", "hier", "vor", "Sym\u00b7pho\u00b7ni\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und welche freudenvolle Schaar", "tokens": ["Und", "wel\u00b7che", "freu\u00b7den\u00b7vol\u00b7le", "Schaar"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Liegt auf dem Antlitz und den Knien", "tokens": ["Liegt", "auf", "dem", "Ant\u00b7litz", "und", "den", "Kni\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor dem geheiligten Altar?", "tokens": ["Vor", "dem", "ge\u00b7hei\u00b7lig\u00b7ten", "Al\u00b7tar", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Welch eine Gottheit l\u00e4st sich sp\u00fcren?", "tokens": ["Welch", "ei\u00b7ne", "Got\u00b7theit", "l\u00e4st", "sich", "sp\u00fc\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Steine scheinen sich zu r\u00fchren,", "tokens": ["Die", "Stei\u00b7ne", "schei\u00b7nen", "sich", "zu", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und jener weisse Marmor haucht.", "tokens": ["Und", "je\u00b7ner", "weis\u00b7se", "Mar\u00b7mor", "haucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "O dreymal seeliges Gesichte!", "tokens": ["O", "drey\u00b7mal", "see\u00b7li\u00b7ges", "Ge\u00b7sich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Gott zeiget mir in seinem Lichte,", "tokens": ["Gott", "zei\u00b7get", "mir", "in", "sei\u00b7nem", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wen er zu seinem Werckzeug braucht.", "tokens": ["Wen", "er", "zu", "sei\u00b7nem", "Wer\u00b7ck\u00b7zeug", "braucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O Dichtkunst, Freundin frommer Thronen,", "tokens": ["O", "Dicht\u00b7kunst", ",", "Freun\u00b7din", "from\u00b7mer", "Thro\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was schwebt dort f\u00fcr ein edles Bild?", "tokens": ["Was", "schwebt", "dort", "f\u00fcr", "ein", "ed\u00b7les", "Bild", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Schutzgeist meiner Vangionen", "tokens": ["Der", "Schutz\u00b7geist", "mei\u00b7ner", "Van\u00b7gi\u00b7o\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In einen goldnen Duft verh\u00fcllt.", "tokens": ["In", "ei\u00b7nen", "gold\u00b7nen", "Duft", "ver\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch wen von seinen klugen S\u00f6hnen", "tokens": ["Doch", "wen", "von", "sei\u00b7nen", "klu\u00b7gen", "S\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Scheint seine rechte Hand zu kr\u00f6nen?", "tokens": ["Scheint", "sei\u00b7ne", "rech\u00b7te", "Hand", "zu", "kr\u00f6\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Welch ist sein Namen und sein Lob?", "tokens": ["Welch", "ist", "sein", "Na\u00b7men", "und", "sein", "Lob", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich trag ihn \u00fcber Feld und H\u00fcgel,", "tokens": ["Ich", "trag", "ihn", "\u00fc\u00b7ber", "Feld", "und", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "So hoch, als ehmahls Pindars Fl\u00fcgel", "tokens": ["So", "hoch", ",", "als", "eh\u00b7mahls", "Pin\u00b7dars", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KOUS", "ADV", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Den k\u00f6niglichen K\u00e4mpfer hob.", "tokens": ["Den", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "K\u00e4mp\u00b7fer", "hob", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Erkenn ihn hier an dem Geleite,", "tokens": ["Er\u00b7kenn", "ihn", "hier", "an", "dem", "Ge\u00b7lei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Das sich besch\u00e4ftigt um ihn dreht.", "tokens": ["Das", "sich", "be\u00b7sch\u00e4f\u00b7tigt", "um", "ihn", "dreht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "VVPP", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Klugheit geht zur rechten Seite,", "tokens": ["Die", "Klug\u00b7heit", "geht", "zur", "rech\u00b7ten", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zur lincken Huld und Majest\u00e4t;", "tokens": ["Zur", "lin\u00b7cken", "Huld", "und", "Ma\u00b7jes\u00b7t\u00e4t", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und die Entschlossenheit und Treue", "tokens": ["Und", "die", "Ent\u00b7schlos\u00b7sen\u00b7heit", "und", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bereiten in der sch\u00f6nsten Reihe", "tokens": ["Be\u00b7rei\u00b7ten", "in", "der", "sch\u00f6ns\u00b7ten", "Rei\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Den Weg vor ihm zu unserm Wohl.", "tokens": ["Den", "Weg", "vor", "ihm", "zu", "un\u00b7serm", "Wohl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die sinds, die ihn so sch\u00f6n formiret;", "tokens": ["Die", "sinds", ",", "die", "ihn", "so", "sch\u00f6n", "for\u00b7mi\u00b7ret", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sein Hertze, durch sie angef\u00fchret,", "tokens": ["Sein", "Hert\u00b7ze", ",", "durch", "sie", "an\u00b7ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ist ihrer edlen Lehren voll.", "tokens": ["Ist", "ih\u00b7rer", "ed\u00b7len", "Leh\u00b7ren", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er weis in Winden und in St\u00fcrmen,", "tokens": ["Er", "weis", "in", "Win\u00b7den", "und", "in", "St\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wann andre von dem Steuer fliehn,", "tokens": ["Wann", "and\u00b7re", "von", "dem", "Steu\u00b7er", "fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Schif des Staates zu beschirmen,", "tokens": ["Das", "Schif", "des", "Staa\u00b7tes", "zu", "be\u00b7schir\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00fchrt es durch die Syrten hin.", "tokens": ["Und", "f\u00fchrt", "es", "durch", "die", "Syr\u00b7ten", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erschaffen, B\u00fcrger zu erhalten,", "tokens": ["Er\u00b7schaf\u00b7fen", ",", "B\u00fcr\u00b7ger", "zu", "er\u00b7hal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und f\u00e4hig, Scepter zu verwalten,", "tokens": ["Und", "f\u00e4\u00b7hig", ",", "Scep\u00b7ter", "zu", "ver\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Weis er von keiner Niedrigkeit.", "tokens": ["Weis", "er", "von", "kei\u00b7ner", "Nied\u00b7rig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wann ihn die B\u00fcrger z\u00fcrnen h\u00f6ren,", "tokens": ["Wann", "ihn", "die", "B\u00fcr\u00b7ger", "z\u00fcr\u00b7nen", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "O Gott! wie fliesen ihre Z\u00e4hren?", "tokens": ["O", "Gott", "!", "wie", "flie\u00b7sen", "ih\u00b7re", "Z\u00e4h\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wie quillt ihr Hertz von bitterm Leyd?", "tokens": ["Wie", "quillt", "ihr", "Hertz", "von", "bit\u00b7term", "Leyd", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Als ehmals auf Pang\u00e4us H\u00f6hen", "tokens": ["Als", "eh\u00b7mals", "auf", "Pan\u00b7g\u00e4us", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "NE", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Des Orpheus krumme Leyer klang,", "tokens": ["Des", "Or\u00b7pheus", "krum\u00b7me", "Le\u00b7yer", "klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Konnt man an Fels und Fl\u00fcssen sehen,", "tokens": ["Konnt", "man", "an", "Fels", "und", "Fl\u00fcs\u00b7sen", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sie die Macht der Thonkunst zwang;", "tokens": ["Wie", "sie", "die", "Macht", "der", "Thon\u00b7kunst", "zwang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da sah man auf der Ceder Spitzen,", "tokens": ["Da", "sah", "man", "auf", "der", "Ce\u00b7der", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Adler still und lauschend sitzen,", "tokens": ["Den", "Ad\u00b7ler", "still", "und", "lau\u00b7schend", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Lieder Reitz berauschte ihn;", "tokens": ["Der", "Lie\u00b7der", "Reitz", "be\u00b7rauschte", "ihn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Er senckte nickend sein Gefieder,", "tokens": ["Er", "senck\u00b7te", "ni\u00b7ckend", "sein", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und \u00fcber seine Augenlieder", "tokens": ["Und", "\u00fc\u00b7ber", "sei\u00b7ne", "Au\u00b7gen\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Warf sich des Schlafes Wolcke hin:", "tokens": ["Warf", "sich", "des", "Schla\u00b7fes", "Wol\u00b7cke", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So zwingt die Kraft von seinem Gr\u00fcnden", "tokens": ["So", "zwingt", "die", "Kraft", "von", "sei\u00b7nem", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der H\u00f6rer wiederspenstig Hertz.", "tokens": ["Der", "H\u00f6\u00b7rer", "wie\u00b7der\u00b7spens\u00b7tig", "Hertz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er spricht; sein Wort mu\u00df \u00fcberwinden.", "tokens": ["Er", "spricht", ";", "sein", "Wort", "mu\u00df", "\u00fc\u00b7berw\u00b7in\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er tr\u00f6stet, und es fleucht der Schmertz;", "tokens": ["Er", "tr\u00f6s\u00b7tet", ",", "und", "es", "fleucht", "der", "Schmertz", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Durch seine hohe Art zu dencken,", "tokens": 