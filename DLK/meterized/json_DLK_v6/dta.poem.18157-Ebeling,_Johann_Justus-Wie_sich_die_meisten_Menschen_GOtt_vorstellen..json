{"dta.poem.18157": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Wie sich die meisten Menschen  \n GOtt vorstellen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Einfalt bildet sich ein G\u00f6ttlich We-", "tokens": ["Die", "Ein\u00b7falt", "bil\u00b7det", "sich", "ein", "G\u00f6tt\u00b7lich", "We"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "sen ein,", "tokens": ["sen", "ein", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Und mu\u00df doch in der That ein blos-", "tokens": ["Und", "mu\u00df", "doch", "in", "der", "That", "ein", "blos"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "APPR", "ART", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ser Mensche sein:", "tokens": ["ser", "Men\u00b7sche", "sein", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Gott ist der h\u00f6chste Geist; doch an dergleichen Lehren,", "tokens": ["Gott", "ist", "der", "h\u00f6chs\u00b7te", "Geist", ";", "doch", "an", "derg\u00b7lei\u00b7chen", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$.", "ADV", "APPR", "PIS", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Pflegt sich nicht ihr Begrif, der anders denkt, zu", "tokens": ["Pflegt", "sich", "nicht", "ihr", "Be\u00b7grif", ",", "der", "an\u00b7ders", "denkt", ",", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PRF", "PTKNEG", "PPOSAT", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,", "APPR"], "meter": "+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.7": {"text": "kehren.", "tokens": ["keh\u00b7ren", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Wie sieht die gr\u00f6\u00dfte Zahl von Sterblichen ", "tokens": ["Wie", "sieht", "die", "gr\u00f6\u00df\u00b7te", "Zahl", "von", "Sterb\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Auf seinen hohen Thron? Als einen alten Mann", "tokens": ["Auf", "sei\u00b7nen", "ho\u00b7hen", "Thron", "?", "Als", "ei\u00b7nen", "al\u00b7ten", "Mann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mit einen grauen Bart, wie auf dem Kirchenbildern,", "tokens": ["Mit", "ei\u00b7nen", "grau\u00b7en", "Bart", ",", "wie", "auf", "dem", "Kir\u00b7chen\u00b7bil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PWAV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Mahler sichtbarlich den h\u00f6chsten Vater schildern.", "tokens": ["Die", "Mah\u00b7ler", "sicht\u00b7bar\u00b7lich", "den", "h\u00f6chs\u00b7ten", "Va\u00b7ter", "schil\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der gros gemahlte Bart mit Reif und Schnee be-", "tokens": ["Der", "gros", "ge\u00b7mahl\u00b7te", "Bart", "mit", "Reif", "und", "Schnee", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "APPR", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "streut,", "tokens": ["streut", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.14": {"text": "Macht ihnen den Begrif der Ehrerbietigkeit,", "tokens": ["Macht", "ih\u00b7nen", "den", "Be\u00b7grif", "der", "Ehr\u00b7er\u00b7bie\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die dieser alte ", "tokens": ["Die", "die\u00b7ser", "al\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "PDAT", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Von jederman verdient, der in sein Reich geh\u00f6ret.", "tokens": ["Von", "je\u00b7der\u00b7man", "ver\u00b7dient", ",", "der", "in", "sein", "Reich", "ge\u00b7h\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVPP", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Des Russen blinder Wahn, der hievon Zeugnis", "tokens": ["Des", "Rus\u00b7sen", "blin\u00b7der", "Wahn", ",", "der", "hie\u00b7von", "Zeug\u00b7nis"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PRELS", "PAV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "giebt,", "tokens": ["giebt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.19": {"text": "Und einen grauen Bart, mehr als das Leben liebt", "tokens": ["Und", "ei\u00b7nen", "grau\u00b7en", "Bart", ",", "mehr", "als", "das", "Le\u00b7ben", "liebt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PIAT", "KOKOM", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wird darum nimmermehr die langen B\u00e4rte missen,", "tokens": ["Wird", "da\u00b7rum", "nim\u00b7mer\u00b7mehr", "die", "lan\u00b7gen", "B\u00e4r\u00b7te", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Weil ihre Aehnlichkeit mit ", "tokens": ["Weil", "ih\u00b7re", "A\u00b7ehn\u00b7lich\u00b7keit", "mit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.22": {"text": "Und woher komt es woll, da\u00df jetzt die kluge Welt,", "tokens": ["Und", "wo\u00b7her", "komt", "es", "woll", ",", "da\u00df", "jetzt", "die", "klu\u00b7ge", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Das h\u00f6chste Wesen noch vor einen K\u00f6rper h\u00e4lt,", "tokens": ["Das", "h\u00f6chs\u00b7te", "We\u00b7sen", "noch", "vor", "ei\u00b7nen", "K\u00f6r\u00b7per", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der in der Ewigkeit im g\u00fcldnen Schimmer wohnet,", "tokens": ["Der", "in", "der", "E\u00b7wig\u00b7keit", "im", "g\u00fcld\u00b7nen", "Schim\u00b7mer", "woh\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und wie ein Welt Monarch im Himmels Schlosse", "tokens": ["Und", "wie", "ein", "Welt", "Mon\u00b7arch", "im", "Him\u00b7mels", "Schlos\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "NN", "APPRART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "thronet?", "tokens": ["thro\u00b7net", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.27": {"text": "Daher weil ihr Verstand sich zu der Gottheit schwingt,", "tokens": ["Da\u00b7her", "weil", "ihr", "Ver\u00b7stand", "sich", "zu", "der", "Got\u00b7theit", "schwingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und mit verwegnen Trieb in ihre Tiefen dringt;", "tokens": ["Und", "mit", "ver\u00b7weg\u00b7nen", "Trieb", "in", "ih\u00b7re", "Tie\u00b7fen", "dringt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und weil sie einen Geist nicht recht begreiffen k\u00f6nnen,", "tokens": ["Und", "weil", "sie", "ei\u00b7nen", "Geist", "nicht", "recht", "be\u00b7greif\u00b7fen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "PTKNEG", "ADJD", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "So werden sie ihn nie von einen K\u00f6rper trennen.", "tokens": ["So", "wer\u00b7den", "sie", "ihn", "nie", "von", "ei\u00b7nen", "K\u00f6r\u00b7per", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Je mehr der arme Mensch an ", "tokens": ["Je", "mehr", "der", "ar\u00b7me", "Mensch", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Das man ergr\u00fcnden will, je mehr wird man gelenkt", "tokens": ["Das", "man", "er\u00b7gr\u00fcn\u00b7den", "will", ",", "je", "mehr", "wird", "man", "ge\u00b7lenkt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "PIS", "VVINF", "VMFIN", "$,", "ADV", "ADV", "VAFIN", "PIS", "VVPP"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "Auf das, was k\u00f6rperlich, da wir das sehen wollen,", "tokens": ["Auf", "das", ",", "was", "k\u00f6r\u00b7per\u00b7lich", ",", "da", "wir", "das", "se\u00b7hen", "wol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADJD", "$,", "KOUS", "PPER", "PDS", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Was wir in dieser Zeit, mit Ehrfurcht glauben sollen.", "tokens": ["Was", "wir", "in", "die\u00b7ser", "Zeit", ",", "mit", "Ehr\u00b7furcht", "glau\u00b7ben", "sol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PDAT", "NN", "$,", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Doch dieser grobe Sinn wird niemahls abgelegt,", "tokens": ["Doch", "die\u00b7ser", "gro\u00b7be", "Sinn", "wird", "nie\u00b7mahls", "ab\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Bis da\u00df man, was wir sein, und ", "tokens": ["Bis", "da\u00df", "man", ",", "was", "wir", "sein", ",", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "KOUS", "PIS", "$,", "PRELS", "PPER", "VAINF", "$,", "KON"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "erwegt:", "tokens": ["er\u00b7wegt", ":"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.38": {"text": "Denn werden wir ersehn, da\u00df unsers Geistes Denken,", "tokens": ["Denn", "wer\u00b7den", "wir", "er\u00b7sehn", ",", "da\u00df", "un\u00b7sers", "Geis\u00b7tes", "Den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVINF", "$,", "KOUS", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Was unbegreiflich ist, wird nimmermehr umschr\u00e4nken.", "tokens": ["Was", "un\u00b7be\u00b7greif\u00b7lich", "ist", ",", "wird", "nim\u00b7mer\u00b7mehr", "um\u00b7schr\u00e4n\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Genug f\u00fcr unsern Stand auf dieser Unterwelt,", "tokens": ["Ge\u00b7nug", "f\u00fcr", "un\u00b7sern", "Stand", "auf", "die\u00b7ser", "Un\u00b7ter\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wenn man das h\u00f6chste ", "tokens": ["Wenn", "man", "das", "h\u00f6chs\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.42": {"text": "Als einen ", "tokens": ["Als", "ei\u00b7nen"], "token_info": ["word", "word"], "pos": ["KOUS", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.43": {"text": "Uns nicht mehr sehen l\u00e4st, als unvollkomne Spuren.", "tokens": ["Uns", "nicht", "mehr", "se\u00b7hen", "l\u00e4st", ",", "als", "un\u00b7voll\u00b7kom\u00b7ne", "Spu\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "VVINF", "VVFIN", "$,", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Genug wenn unser Herz ein h\u00f6chstes Wesen nennt", "tokens": ["Ge\u00b7nug", "wenn", "un\u00b7ser", "Herz", "ein", "h\u00f6chs\u00b7tes", "We\u00b7sen", "nennt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Da\u00df hie kein Sterblicher, so wie es ist erkennt;", "tokens": ["Da\u00df", "hie", "kein", "Sterb\u00b7li\u00b7cher", ",", "so", "wie", "es", "ist", "er\u00b7kennt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "$,", "ADV", "KOKOM", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Und diese Warheit gl\u00e4ubt der Eigenschaften Gr\u00f6ssen", "tokens": ["Und", "die\u00b7se", "War\u00b7heit", "gl\u00e4ubt", "der", "Ei\u00b7gen\u00b7schaf\u00b7ten", "Gr\u00f6s\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Jm grossen Jehovah, k\u00f6n kein Verstand ausmessen:", "tokens": ["Jm", "gros\u00b7sen", "Je\u00b7ho\u00b7vah", ",", "k\u00f6n", "kein", "Ver\u00b7stand", "aus\u00b7mes\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "VMFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Genug wenn seine Macht, die Weisheit, G\u00fctigkeit,", "tokens": ["Ge\u00b7nug", "wenn", "sei\u00b7ne", "Macht", ",", "die", "Weis\u00b7heit", ",", "G\u00fc\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "KOUS", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Sich allemahl uns zeigt mit der Unentlichkeit;", "tokens": ["Sich", "al\u00b7le\u00b7mahl", "uns", "zeigt", "mit", "der", "Un\u