{"dta.poem.2067": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Luchs.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auch der Luchs ist sch\u00f6n und sch\u00e4dlich. Er ist voller", "tokens": ["Auch", "der", "Luchs", "ist", "sch\u00f6n", "und", "sch\u00e4d\u00b7lich", ".", "Er", "ist", "vol\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$.", "PPER", "VAFIN", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Raubbegier;", "tokens": ["Raub\u00b7be\u00b7gier", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Aber dennoch ist es uns ebenfalls ein n\u00fctzlichs Thier.", "tokens": ["A\u00b7ber", "den\u00b7noch", "ist", "es", "uns", "e\u00b7ben\u00b7falls", "ein", "n\u00fctz\u00b7lichs", "Thier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Zwischen Katzen und dem Tiger scheints ein Mittelthier", "tokens": ["Zwi\u00b7schen", "Kat\u00b7zen", "und", "dem", "Ti\u00b7ger", "scheints", "ein", "Mit\u00b7tel\u00b7thier"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "zu seyn:", "tokens": ["zu", "seyn", ":"], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VAINF", "$."], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Seine Haut ist gelblichfleckig, auch wohl etwas grau zu-", "tokens": ["Sei\u00b7ne", "Haut", "ist", "gelb\u00b7li\u00b7ch\u00b7flec\u00b7kig", ",", "auch", "wohl", "et\u00b7was", "grau", "zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "ADV", "ADV", "ADV", "ADJD", "TRUNC"], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "weilen:", "tokens": ["wei\u00b7len", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Sie sind aus der Maa\u00dfen fertig, ihre Speise zu ereilen,", "tokens": ["Sie", "sind", "aus", "der", "Maa\u00b7\u00dfen", "fer\u00b7tig", ",", "ih\u00b7re", "Spei\u00b7se", "zu", "er\u00b7ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "ADJD", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.9": {"text": "Sehn so scharf, als sonst kein Thier. Zwischen Bergen,", "tokens": ["Sehn", "so", "scharf", ",", "als", "sonst", "kein", "Thier", ".", "Zwi\u00b7schen", "Ber\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$,", "KOUS", "ADV", "PIAT", "NN", "$.", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Fels und Stein", "tokens": ["Fels", "und", "Stein"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Leben meist die Katzenl\u00fcchse, wenn die K\u00e4lberl\u00fcchs\u2019 hin-", "tokens": ["Le\u00b7ben", "meist", "die", "Kat\u00b7zen\u00b7l\u00fcch\u00b7se", ",", "wenn", "die", "K\u00e4l\u00b7ber\u00b7l\u00fcchs'", "hin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "KOUS", "ART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.12": {"text": "gegen", "tokens": ["ge\u00b7gen"], "token_info": ["word"], "pos": ["APPR"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "In den dickverwachs\u2019nen W\u00e4ldern insgemein zu wohnen", "tokens": ["In", "den", "dick\u00b7ver\u00b7wachs'\u00b7nen", "W\u00e4l\u00b7dern", "ins\u00b7ge\u00b7mein", "zu", "woh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.14": {"text": "pflegen.", "tokens": ["pfle\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "F\u00fcr die Schwerenoth und Krampf wird die Luchsklau", "tokens": ["F\u00fcr", "die", "Schwe\u00b7re\u00b7noth", "und", "Krampf", "wird", "die", "Luchs\u00b7klau"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "NN", "VAFIN", "ART", "NE"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.16": {"text": "uns verschrieben;", "tokens": ["uns", "ver\u00b7schrie\u00b7ben", ";"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Und mit ihren B\u00e4lgen werden gro\u00dfe Handlungen ge-", "tokens": ["Und", "mit", "ih\u00b7ren", "B\u00e4l\u00b7gen", "wer\u00b7den", "gro\u00b7\u00dfe", "Hand\u00b7lun\u00b7gen", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VAFIN", "ADJA", "NN", "TRUNC"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.18": {"text": "trieben.", "tokens": ["trie\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}