{"textgrid.poem.44078": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gute Nacht, ihr liebsten Br\u00fcder!", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gute Nacht, ihr liebsten Br\u00fcder!", "tokens": ["Gu\u00b7te", "Nacht", ",", "ihr", "liebs\u00b7ten", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das Verh\u00e4ngn\u00fc\u00df trennt uns wieder", "tokens": ["Das", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "trennt", "uns", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und verg\u00e4llt uns manche Nacht,", "tokens": ["Und", "ver\u00b7g\u00e4llt", "uns", "man\u00b7che", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die wir freudig zugebracht,", "tokens": ["Die", "wir", "freu\u00b7dig", "zu\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn die Wei\u00dfheit nebst den Musen", "tokens": ["Wenn", "die", "Wei\u00df\u00b7heit", "nebst", "den", "Mu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und auch, wie es manchmahl kam,", "tokens": ["Und", "auch", ",", "wie", "es", "manch\u00b7mahl", "kam", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": ". . . . allgemeiner Busen", "tokens": [".", ".", ".", ".", "all\u00b7ge\u00b7mei\u00b7ner", "Bu\u00b7sen"], "token_info": ["punct", "punct", "punct", "punct", "word", "word"], "pos": ["$.", "$.", "$.", "$.", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Aller Grillen Macht benahm.", "tokens": ["Al\u00b7ler", "Gril\u00b7len", "Macht", "be\u00b7nahm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denckt an diesen guten Winter,", "tokens": ["Denckt", "an", "die\u00b7sen", "gu\u00b7ten", "Win\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da euch der verfolgte G\u00fcnther", "tokens": ["Da", "euch", "der", "ver\u00b7folg\u00b7te", "G\u00fcn\u00b7ther"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Erstlich auf die Spur gelenckt,", "tokens": ["Erst\u00b7lich", "auf", "die", "Spur", "ge\u00b7lenckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die mit edler Wollust tr\u00e4nckt;", "tokens": ["Die", "mit", "ed\u00b7ler", "Wol\u00b7lust", "tr\u00e4nckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denckt an meine treue Lehren", "tokens": ["Denckt", "an", "mei\u00b7ne", "treu\u00b7e", "Leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Von Verstand und Wi\u00dfenschaft,", "tokens": ["Von", "Ver\u00b7stand", "und", "Wi\u00b7\u00dfen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denckt auch nicht allein ans H\u00f6ren,", "tokens": ["Denckt", "auch", "nicht", "al\u00b7lein", "ans", "H\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sondern bringt sie selbst zur Kraft.", "tokens": ["Son\u00b7dern", "bringt", "sie", "selbst", "zur", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Gott und Gl\u00fccke weis, wie lange,", "tokens": ["Gott", "und", "Gl\u00fc\u00b7cke", "weis", ",", "wie", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKVZ", "$,", "PWAV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo, wie weit, wie froh, wie bange", "tokens": ["Wo", ",", "wie", "weit", ",", "wie", "froh", ",", "wie", "ban\u00b7ge"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "$,", "PWAV", "ADJD", "$,", "PWAV", "ADJD", "$,", "PWAV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mein herumgezogner Fu\u00df", "tokens": ["Mein", "her\u00b7um\u00b7ge\u00b7zog\u00b7ner", "Fu\u00df"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Noch den Ruhplaz suchen mu\u00df.", "tokens": ["Noch", "den", "Ruh\u00b7plaz", "su\u00b7chen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O wie manches Schnein und Regnen,", "tokens": ["O", "wie", "man\u00b7ches", "Schnein", "und", "Reg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "PIAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "O wie mancher saurer Wind", "tokens": ["O", "wie", "man\u00b7cher", "sau\u00b7rer", "Wind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Werden mir noch wohl begegnen,", "tokens": ["Wer\u00b7den", "mir", "noch", "wohl", "be\u00b7geg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Eh mein Flei\u00df die Frucht empfindt!", "tokens": ["Eh", "mein", "Flei\u00df", "die", "Frucht", "emp\u00b7findt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "W\u00fcntscht mir weiter nichts auf Erden,", "tokens": ["W\u00fcnt\u00b7scht", "mir", "wei\u00b7ter", "nichts", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als nur wohl gepr\u00fcft zu werden", "tokens": ["Als", "nur", "wohl", "ge\u00b7pr\u00fcft", "zu", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "VVPP", "PTKZU", "VAINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bey der Noth nicht eigner Schuld,", "tokens": ["Bey", "der", "Noth", "nicht", "eig\u00b7ner", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKNEG", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem Kummer stets Gedult.", "tokens": ["In", "dem", "Kum\u00b7mer", "stets", "Ge\u00b7dult", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich bin allemahl zufrieden,", "tokens": ["Ich", "bin", "al\u00b7le\u00b7mahl", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie es auch die Vorsicht schickt;", "tokens": ["Wie", "es", "auch", "die", "Vor\u00b7sicht", "schickt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Lebt ich nicht von euch geschieden,", "tokens": ["Lebt", "ich", "nicht", "von", "euch", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lebt ich ganz und gar begl\u00fcckt.", "tokens": ["Lebt", "ich", "ganz", "und", "gar", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KON", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "H\u00f6rt ihr meinen Tod berichten,", "tokens": ["H\u00f6rt", "ihr", "mei\u00b7nen", "Tod", "be\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O so klagt und weint mit nichten!", "tokens": ["O", "so", "klagt", "und", "weint", "mit", "nich\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "KON", "VVFIN", "APPR", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Last es euch vielmehr erfreun,", "tokens": ["Last", "es", "euch", "viel\u00b7mehr", "er\u00b7freun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dadurch scharf erweckt zu seyn,", "tokens": ["Da\u00b7durch", "scharf", "er\u00b7weckt", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Da\u00df ihr dieses kurze Leben", "tokens": ["Da\u00df", "ihr", "die\u00b7ses", "kur\u00b7ze", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Desto be\u00dfer brauchen lernt", "tokens": ["Des\u00b7to", "be\u00b7\u00dfer", "brau\u00b7chen", "lernt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVINF", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und durch eifriges Bestreben", "tokens": ["Und", "durch", "eif\u00b7ri\u00b7ges", "Be\u00b7stre\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Euch dem P\u00f6bel bald entfernt.", "tokens": ["Euch", "dem", "P\u00f6\u00b7bel", "bald", "ent\u00b7fernt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Seh ich jezt mit na\u00dfem Blicke", "tokens": ["Seh", "ich", "jezt", "mit", "na\u00b7\u00dfem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So betr\u00fcbt wie der zur\u00fccke,", "tokens": ["So", "be\u00b7tr\u00fcbt", "wie", "der", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "ART", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem sich dort das Paradies", "tokens": ["Dem", "sich", "dort", "das", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PRF", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kl\u00e4glich auf dem R\u00fccken wies,", "tokens": ["Kl\u00e4g\u00b7lich", "auf", "dem", "R\u00fc\u00b7cken", "wies", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Z\u00e4hm ich doch die weichen Triebe", "tokens": ["Z\u00e4hm", "ich", "doch", "die", "wei\u00b7chen", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch Gewohnheit und Verstand;", "tokens": ["Durch", "Ge\u00b7wohn\u00b7heit", "und", "Ver\u00b7stand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo ich mich in K\u00fcnsten \u00fcbe,", "tokens": ["Wo", "ich", "mich", "in", "K\u00fcns\u00b7ten", "\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da, da ist mein Vaterland.", "tokens": ["Da", ",", "da", "ist", "mein", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Gute Nacht, ihr Berg und Tiefen,", "tokens": ["Gu\u00b7te", "Nacht", ",", "ihr", "Berg", "und", "Tie\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo wir oft dem Tage riefen,", "tokens": ["Wo", "wir", "oft", "dem", "Ta\u00b7ge", "rie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der uns einmahl wei\u00dfheitsvoll", "tokens": ["Der", "uns", "ein\u00b7mahl", "wei\u00df\u00b7heits\u00b7voll"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Au\u00dfer Landes cr\u00f6nen soll.", "tokens": ["Au\u00b7\u00dfer", "Lan\u00b7des", "cr\u00f6\u00b7nen", "soll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00f6rt auch: Haltet meine Br\u00fcder", "tokens": ["H\u00f6rt", "auch", ":", "Hal\u00b7tet", "mei\u00b7ne", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$.", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht zu lange nach mir auf;", "tokens": ["Nicht", "zu", "lan\u00b7ge", "nach", "mir", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKA", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schickt ihr mir die drey nicht wieder,", "tokens": ["Schickt", "ihr", "mir", "die", "drey", "nicht", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ART", "CARD", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Leg ich schon den Fluch darauf.", "tokens": ["Leg", "ich", "schon", "den", "Fluch", "da\u00b7rauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}