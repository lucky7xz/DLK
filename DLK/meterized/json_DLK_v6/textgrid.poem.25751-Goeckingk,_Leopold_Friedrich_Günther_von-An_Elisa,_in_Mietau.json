{"textgrid.poem.25751": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "An Elisa, in Mietau", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah dich nie, werd' auch wohl nie dich sehen,", "tokens": ["Ich", "sah", "dich", "nie", ",", "werd'", "auch", "wohl", "nie", "dich", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "VAFIN", "ADV", "ADV", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als bis die Erd' uns beide nicht mehr fa\u00dft,", "tokens": ["Als", "bis", "die", "Erd'", "uns", "bei\u00b7de", "nicht", "mehr", "fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "PIS", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als bis mein Geist, frei von des K\u00f6rpers Last,", "tokens": ["Als", "bis", "mein", "Geist", ",", "frei", "von", "des", "K\u00f6r\u00b7pers", "Last", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "$,", "ADJD", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hinauf sich schwingt zu den bestirnten H\u00f6hen,", "tokens": ["Hin\u00b7auf", "sich", "schwingt", "zu", "den", "bes\u00b7tirn\u00b7ten", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wo er nichts mehr bef\u00fcrchtet, hofft und ha\u00dft.", "tokens": ["Wo", "er", "nichts", "mehr", "be\u00b7f\u00fcrch\u00b7tet", ",", "hofft", "und", "ha\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIS", "ADV", "VVPP", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich werde wohl voran dir gehen;", "tokens": ["Ich", "wer\u00b7de", "wohl", "vo\u00b7ran", "dir", "ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als Freundschaftspfand lass' ich die\u00df B\u00fcchelchen zur\u00fcck,", "tokens": ["Als", "Freund\u00b7schafts\u00b7pfand", "lass'", "ich", "die\u00df", "B\u00fc\u00b7chel\u00b7chen", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "PDS", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Bis sp\u00e4ter du mir nachkommst, und, o Gl\u00fcck!", "tokens": ["Bis", "sp\u00e4\u00b7ter", "du", "mir", "nach\u00b7kommst", ",", "und", ",", "o", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJD", "PPER", "PPER", "VVFIN", "$,", "KON", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wir uns zum erstenmale sehen.", "tokens": ["Wir", "uns", "zum", "ers\u00b7ten\u00b7ma\u00b7le", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Dort wird uns weder Land noch See,", "tokens": ["Dort", "wird", "uns", "we\u00b7der", "Land", "noch", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "KON", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Kein Strom und kein Gebirge trennen,", "tokens": ["Kein", "Strom", "und", "kein", "Ge\u00b7bir\u00b7ge", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Dort werden, wie Amint und Galathee,", "tokens": ["Dort", "wer\u00b7den", ",", "wie", "A\u00b7mint", "und", "Ga\u00b7la\u00b7thee", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAINF", "$,", "PWAV", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Wir unschuldsvoll uns lieben k\u00f6nnen.", "tokens": ["Wir", "un\u00b7schulds\u00b7voll", "uns", "lie\u00b7ben", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PPER", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Der Thr\u00e4nen, die wir hier geweint,", "tokens": ["Der", "Thr\u00e4\u00b7nen", ",", "die", "wir", "hier", "ge\u00b7weint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Gedenkt man dort nicht mehr, Elise!", "tokens": ["Ge\u00b7denkt", "man", "dort", "nicht", "mehr", ",", "E\u00b7li\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PTKNEG", "ADV", "$,", "NE", "$."], "meter": "-+-+-++--", "measure": "unknown.measure.tetra"}, "line.16": {"text": "Wie heut uns hold die Sonne scheint,", "tokens": ["Wie", "heut", "uns", "hold", "die", "Son\u00b7ne", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Scheint ewig sie im Paradiese.", "tokens": ["Scheint", "e\u00b7wig", "sie", "im", "Pa\u00b7ra\u00b7die\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Sey hier denn auch mein Horizont", "tokens": ["Sey", "hier", "denn", "auch", "mein", "Ho\u00b7ri\u00b7zont"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Noch \u00f6fter in der Zukunft tr\u00fcbe,", "tokens": ["Noch", "\u00f6f\u00b7ter", "in", "der", "Zu\u00b7kunft", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Wenn aus der Ferne deine Liebe", "tokens": ["Wenn", "aus", "der", "Fer\u00b7ne", "dei\u00b7ne", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Nur meinen dunkeln Pfad besonnt!", "tokens": ["Nur", "mei\u00b7nen", "dun\u00b7keln", "Pfad", "be\u00b7sonnt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}