{"dta.poem.12362": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Die Frau von Weissenburg .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was wolln wir aber singen,               ", "tokens": ["Was", "wolln", "wir", "a\u00b7ber", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was wollt ihr f\u00fcr ein Lied,", "tokens": ["Was", "wollt", "ihr", "f\u00fcr", "ein", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Lied von der Frauen von Weissenburg,", "tokens": ["Ein", "Lied", "von", "der", "Frau\u00b7en", "von", "Weis\u00b7sen\u00b7burg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wie sie ihren Herrn verrieth.", "tokens": ["Wie", "sie", "ih\u00b7ren", "Herrn", "ver\u00b7rieth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie lie\u00df ein Briefelein schreiben,", "tokens": ["Sie", "lie\u00df", "ein", "Brie\u00b7fel\u00b7ein", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gar fern ins Th\u00fcringer Land,", "tokens": ["Gar", "fern", "ins", "Th\u00fc\u00b7rin\u00b7ger", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Zu ihrem Ludewig Buhlen,", "tokens": ["Zu", "ih\u00b7rem", "Lu\u00b7de\u00b7wig", "Buh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NE", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da\u00df er da k\u00e4m zur Hand.", "tokens": ["Da\u00df", "er", "da", "k\u00e4m", "zur", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er sprach zu seinem Knechte:", "tokens": ["Er", "sprach", "zu", "sei\u00b7nem", "Knech\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du, sattel mir mein Pferd,", "tokens": ["Du", ",", "sat\u00b7tel", "mir", "mein", "Pferd", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVIMP", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir wollen zur Weissenburg reiten,", "tokens": ["Wir", "wol\u00b7len", "zur", "Weis\u00b7sen\u00b7burg", "rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Es ist nun Reitens werth.", "tokens": ["Es", "ist", "nun", "Rei\u00b7tens", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u201egott gr\u00fcs euch Adelheid sch\u00f6ne,", "tokens": ["\u201e", "gott", "gr\u00fcs", "euch", "A\u00b7del\u00b7heid", "sch\u00f6\u00b7ne", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "NE", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "\u201ew\u00fcnsch euch ein guten Tag:", "tokens": ["\u201e", "w\u00fcnsch", "euch", "ein", "gu\u00b7ten", "Tag", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "\u201ewo ist eur edler Herre,", "tokens": ["\u201e", "wo", "ist", "eur", "ed\u00b7ler", "Her\u00b7re", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201emit dem ich k\u00e4mpfen mag?\u201c", "tokens": ["\u201e", "mit", "dem", "ich", "k\u00e4mp\u00b7fen", "mag", "?", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "PRELS", "PPER", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Frau lenkt ihren Herren,", "tokens": ["Die", "Frau", "lenkt", "ih\u00b7ren", "Her\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Schein falsches Gem\u00fcths,", "tokens": ["Im", "Schein", "fal\u00b7sches", "Ge\u00b7m\u00fcths", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er reitet Nachts ganz sp\u00e4te", "tokens": ["Er", "rei\u00b7tet", "Nachts", "ganz", "sp\u00e4\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Hunden nach dem Ried.", "tokens": ["Mit", "Hun\u00b7den", "nach", "dem", "Ried", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da Ludewig unter die Linde kam,", "tokens": ["Da", "Lu\u00b7de\u00b7wig", "un\u00b7ter", "die", "Lin\u00b7de", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "ART", "NE", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ja unter die Linde so gr\u00fcn,", "tokens": ["Ja", "un\u00b7ter", "die", "Lin\u00b7de", "so", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "ART", "NE", "ADV", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da kam der Herr von der Weissenburg", "tokens": ["Da", "kam", "der", "Herr", "von", "der", "Weis\u00b7sen\u00b7burg"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit seinen Winden so k\u00fchn.", "tokens": ["Mit", "sei\u00b7nen", "Win\u00b7den", "so", "k\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "\u201ewillkommen Herr von der Weissenburg,", "tokens": ["\u201e", "will\u00b7kom\u00b7men", "Herr", "von", "der", "Weis\u00b7sen\u00b7burg", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u201egott geb euch guten Muth,", "tokens": ["\u201e", "gott", "geb", "euch", "gu\u00b7ten", "Muth", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201eihr sollt nicht l\u00e4nger leben,", "tokens": ["\u201e", "ihr", "sollt", "nicht", "l\u00e4n\u00b7ger", "le\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201edenn heut diesen halben Tag.\u201c", "tokens": ["\u201e", "denn", "heut", "die\u00b7sen", "hal\u00b7ben", "Tag", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "ADV", "PDAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "\u201esoll ich nicht l\u00e4nger leben,", "tokens": ["\u201e", "soll", "ich", "nicht", "l\u00e4n\u00b7ger", "le\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201edenn diesen halben Tag,", "tokens": ["\u201e", "denn", "die\u00b7sen", "hal\u00b7ben", "Tag", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201eso klag ichs Christo vom Himmel,", "tokens": ["\u201e", "so", "klag", "ichs", "Chris\u00b7to", "vom", "Him\u00b7mel", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PIS", "NE", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "\u201eder all Ding wenden mag.\u201c", "tokens": ["\u201e", "der", "all", "Ding", "wen\u00b7den", "mag.", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "abbreviation", "punct"], "pos": ["$(", "ART", "PIAT", "NN", "PIAT", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Sie kamen hart zusammen,", "tokens": ["Sie", "ka\u00b7men", "hart", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Wort und Zorn so gro\u00df,", "tokens": ["Mit", "Wort", "und", "Zorn", "so", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df einer zu dem andern", "tokens": ["Da\u00df", "ei\u00b7ner", "zu", "dem", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein Armbrust abe scho\u00df.", "tokens": ["Sein", "Arm\u00b7brust", "a\u00b7be", "scho\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Er sprach zu seinem Knechte:", "tokens": ["Er", "sprach", "zu", "sei\u00b7nem", "Knech\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201enun spann dein Armbrust ein,", "tokens": ["\u201e", "nun", "spann", "dein", "Arm\u00b7brust", "ein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201eund schie\u00df den Herrn von der Weissenburg,", "tokens": ["\u201e", "und", "schie\u00df", "den", "Herrn", "von", "der", "Weis\u00b7sen\u00b7burg", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u201ezur linken Seiten ein.\u201c", "tokens": ["\u201e", "zur", "lin\u00b7ken", "Sei\u00b7ten", "ein", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPRART", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "\u201ewarum soll ich ihn schie\u00dfen,", "tokens": ["\u201e", "wa\u00b7rum", "soll", "ich", "ihn", "schie\u00b7\u00dfen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VMFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201eund morden auf dem Plan,", "tokens": ["\u201e", "und", "mor\u00b7den", "auf", "dem", "Plan", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201ehat er mir doch sein Lebelang,", "tokens": ["\u201e", "hat", "er", "mir", "doch", "sein", "Le\u00b7be\u00b7lang", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "\u201enoch nie kein Leid gethan.\u201c", "tokens": ["\u201e", "noch", "nie", "ke