{"textgrid.poem.66316": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Das Weltgericht", "genre": "verse", "period": "N.A.", "pub_year": 1767, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Blinder Geist, entrei\u00dfe heut", "tokens": ["Blin\u00b7der", "Geist", ",", "ent\u00b7rei\u00b7\u00dfe", "heut"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deinem Blick die Decke,", "tokens": ["Dei\u00b7nem", "Blick", "die", "De\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df Gericht und Ewigkeit", "tokens": ["Da\u00df", "Ge\u00b7richt", "und", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich einmal erwecke.", "tokens": ["Dich", "ein\u00b7mal", "er\u00b7we\u00b7cke", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Schau mit ernstem Angesicht", "tokens": ["Schau", "mit", "erns\u00b7tem", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Am Entscheidungstage", "tokens": ["Am", "Ent\u00b7schei\u00b7dungs\u00b7ta\u00b7ge"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Himmel, H\u00f6lle und Gericht,", "tokens": ["Him\u00b7mel", ",", "H\u00f6l\u00b7le", "und", "Ge\u00b7richt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Richter, Schwert und Wage.", "tokens": ["Rich\u00b7ter", ",", "Schwert", "und", "Wa\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Sieh, \u2013 Verstockte wollen nur", "tokens": ["Sieh", ",", "\u2013", "Ver\u00b7stock\u00b7te", "wol\u00b7len", "nur"], "token_info": ["word", "punct", "punct", "word", "word", "word"], "pos": ["NE", "$,", "$(", "NN", "VMFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses Bild nicht sehen \u2013", "tokens": ["Die\u00b7ses", "Bild", "nicht", "se\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKNEG", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Auf den Tr\u00fcmmern der Natur", "tokens": ["Auf", "den", "Tr\u00fcm\u00b7mern", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Br\u00fcder stehen.", "tokens": ["Dei\u00b7ne", "Br\u00fc\u00b7der", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Welch ein unz\u00e4hlbares Heer", "tokens": ["Welch", "ein", "un\u00b7z\u00e4hl\u00b7ba\u00b7res", "Heer"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wimmelt in der Ferne!", "tokens": ["Wim\u00b7melt", "in", "der", "Fer\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Viel, wie Tropfen in dem Meer,", "tokens": ["Viel", ",", "wie", "Trop\u00b7fen", "in", "dem", "Meer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00e4ufiger, als Sterne.", "tokens": ["H\u00e4u\u00b7fi\u00b7ger", ",", "als", "Ster\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wie er schon von ferne schreckt,", "tokens": ["Wie", "er", "schon", "von", "fer\u00b7ne", "schreckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der entflammte Richter!", "tokens": ["Der", "ent\u00b7flamm\u00b7te", "Rich\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schrecken und Verzweiflung deckt", "tokens": ["Schre\u00b7cken", "und", "Ver\u00b7zwei\u00b7flung", "deckt"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tausend Angesichter.", "tokens": ["Tau\u00b7send", "An\u00b7ge\u00b7sich\u00b7ter", "."], "token_info": ["word", "word", "punct"], "pos": ["CARD", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Seht den gro\u00dfen Menschensohn", "tokens": ["Seht", "den", "gro\u00b7\u00dfen", "Men\u00b7schen\u00b7sohn"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Seinem Donner winken,", "tokens": ["Sei\u00b7nem", "Don\u00b7ner", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und die S\u00fcnder schl\u00e4gt Er schon", "tokens": ["Und", "die", "S\u00fcn\u00b7der", "schl\u00e4gt", "Er", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Hin zu seiner Linken.", "tokens": ["Hin", "zu", "sei\u00b7ner", "Lin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Sein erz\u00fcrntes Auge blitzt,", "tokens": ["Sein", "er\u00b7z\u00fcrn\u00b7tes", "Au\u00b7ge", "blitzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und er spricht im Grimme:", "tokens": ["Und", "er", "spricht", "im", "Grim\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "(sieben Donner reden itzt", "tokens": ["(", "sie\u00b7ben", "Don\u00b7ner", "re\u00b7den", "itzt"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "CARD", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihre Todesstimme!)", "tokens": ["Ih\u00b7re", "To\u00b7dess\u00b7tim\u00b7me", "!", ")"], "token_info": ["word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "S\u00fcnder, weicht! ich kenn' euch nicht!", "tokens": ["S\u00fcn\u00b7der", ",", "weicht", "!", "ich", "kenn'", "euch", "nicht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Flieht vor diesem Stuhle,", "tokens": ["Flieht", "vor", "die\u00b7sem", "Stuh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Vor des Himmels Angesicht!", "tokens": ["Vor", "des", "Him\u00b7mels", "An\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Heult im Schwefelpfuhle!", "tokens": ["Heult", "im", "Schwe\u00b7fel\u00b7pfuh\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Wilde Stimmen heben sich", "tokens": ["Wil\u00b7de", "Stim\u00b7men", "he\u00b7ben", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PRF"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Aus verruchten H\u00e4lsen:", "tokens": ["Aus", "ver\u00b7ruch\u00b7ten", "H\u00e4l\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Berge, fallet \u00fcber mich!", "tokens": ["Ber\u00b7ge", ",", "fal\u00b7let", "\u00fc\u00b7ber", "mich", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Decket mich, ihr Felsen!", "tokens": ["De\u00b7cket", "mich", ",", "ihr", "Fel\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Doch die Berge h\u00f6ren nicht:", "tokens": ["Doch", "die", "Ber\u00b7ge", "h\u00f6\u00b7ren", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn sie sind zerst\u00f6ret,", "tokens": ["Denn", "sie", "sind", "zer\u00b7st\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "VVPP", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Wenn der S\u00fcnder am Gericht", "tokens": ["Wenn", "der", "S\u00fcn\u00b7der", "am", "Ge\u00b7richt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihren Trost begehret.", "tokens": ["Ih\u00b7ren", "Trost", "be\u00b7ge\u00b7hret", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Nur die Tugend zittert nicht,", "tokens": ["Nur", "die", "Tu\u00b7gend", "zit\u00b7tert", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn der Richter t\u00f6dtet:", "tokens": ["Wenn", "der", "Rich\u00b7ter", "t\u00f6d\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Denn sie schauet ein Gesicht,", "tokens": ["Denn", "sie", "schau\u00b7et", "ein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo die Gnade redet.", "tokens": ["Wo", "die", "Gna\u00b7de", "re\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Muthig forscht des Frommen Blick", "tokens": ["Mut\u00b7hig", "forscht", "des", "From\u00b7men", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In des Richters Mienen;", "tokens": ["In", "des", "Rich\u00b7ters", "Mie\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und er findet nur sein Gl\u00fcck,", "tokens": ["Und", "er", "fin\u00b7det", "nur", "sein", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Nicht Gericht in ihnen.", "tokens": ["Nicht", "Ge\u00b7richt", "in", "ih\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Sanft, wie Gottes Harfenton,", "tokens": ["Sanft", ",", "wie", "Got\u00b7tes", "Har\u00b7fen\u00b7ton", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wallt die Stimme nieder:", "tokens": ["Wallt", "die", "Stim\u00b7me", "nie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Kommt, ererbet euren Lohn,", "tokens": ["Kommt", ",", "er\u00b7er\u00b7bet", "eu\u00b7ren", "Lohn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kommt, ihr meine Br\u00fcder.", "tokens": ["Kommt", ",", "ihr", "mei\u00b7ne", "Br\u00fc\u00b7der", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und sie steigen auf zum Licht,", "tokens": ["Und", "sie", "stei\u00b7gen", "auf", "zum", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Leicht, wie Adler steigen,", "tokens": ["Leicht", ",", "wie", "Ad\u00b7ler", "stei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Fallen auf ihr Angesicht,", "tokens": ["Fal\u00b7len", "auf", "ihr", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Danken Gott und schweigen.", "tokens": ["Dan\u00b7ken", "Gott", "und", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Richter, ach! an jenem Tag", "tokens": ["Rich\u00b7ter", ",", "ach", "!", "an", "je\u00b7nem", "Tag"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ITJ", "$.", "APPR", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wirst du mich verdammen?", "tokens": ["Wirst", "du", "mich", "ver\u00b7dam\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schl\u00e4gt der Rache Donnerschlag", "tokens": ["Schl\u00e4gt", "der", "Ra\u00b7che", "Don\u00b7ner\u00b7schlag"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich in H\u00f6llenflammen?", "tokens": ["Mich", "in", "H\u00f6l\u00b7len\u00b7flam\u00b7men", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Gingst du denn nicht ins Gericht?", "tokens": ["Gingst", "du", "denn", "nicht", "ins", "Ge\u00b7richt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "APPRART", "NN", "$."], "meter": "+--++-+", "measure": "dactylic.init"}, "line.6": {"text": "Bist du nicht gestorben?", "tokens": ["Bist", "du", "nicht", "ge\u00b7stor\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Hast du mir den Himmel nicht", "tokens": ["Hast", "du", "mir", "den", "Him\u00b7mel", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "ART", "NN", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Durch dein Blut erworben?", "tokens": ["Durch", "dein", "Blut", "er\u00b7wor\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Heut, Erl\u00f6ser! stehet mir", "tokens": ["Heut", ",", "Er\u00b7l\u00f6\u00b7ser", "!", "ste\u00b7het", "mir"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "NN", "$.", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch der Himmel offen;", "tokens": ["Noch", "der", "Him\u00b7mel", "of\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Heute kann ich noch von dir", "tokens": ["Heu\u00b7te", "kann", "ich", "noch", "von", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Vergebung hoffen.", "tokens": ["Die", "Ver\u00b7ge\u00b7bung", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ach! so heilige mein Herz,", "tokens": ["Ach", "!", "so", "hei\u00b7li\u00b7ge", "mein", "Herz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "ADJA", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Salbe diese Seele,", "tokens": ["Sal\u00b7be", "die\u00b7se", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Da\u00df Verzweiflung, Reue, Schmerz", "tokens": ["Da\u00df", "Ver\u00b7zwei\u00b7flung", ",", "Reu\u00b7e", ",", "Schmerz"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["KOUS", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sie nicht ewig qu\u00e4le