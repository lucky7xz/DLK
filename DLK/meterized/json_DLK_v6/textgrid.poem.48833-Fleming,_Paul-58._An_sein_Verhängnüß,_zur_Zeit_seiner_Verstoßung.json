{"textgrid.poem.48833": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "58. An sein Verh\u00e4ngn\u00fc\u00df, zur Zeit seiner Versto\u00dfung", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, wenn ich etwa nicht mit dir zufrieden w\u00e4re", "tokens": ["Ja", ",", "wenn", "ich", "et\u00b7wa", "nicht", "mit", "dir", "zu\u00b7frie\u00b7den", "w\u00e4\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "APPR", "PPER", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und schm\u00e4hte deinen Rat, so lie\u00df ichs billich sein,", "tokens": ["und", "schm\u00e4h\u00b7te", "dei\u00b7nen", "Rat", ",", "so", "lie\u00df", "ichs", "bil\u00b7lich", "sein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PIS", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da\u00df du mir legtest auf so eine harte Pein,", "tokens": ["da\u00df", "du", "mir", "leg\u00b7test", "auf", "so", "ei\u00b7ne", "har\u00b7te", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "APPR", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "f\u00fcr der ich lieber tot, als krank zu sein begehre.", "tokens": ["f\u00fcr", "der", "ich", "lie\u00b7ber", "tot", ",", "als", "krank", "zu", "sein", "be\u00b7geh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADJD", "$,", "KOUS", "ADJD", "PTKZU", "VAINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Schau, doch die Marter an, in der ich mich verzehre.", "tokens": ["Schau", ",", "doch", "die", "Mar\u00b7ter", "an", ",", "in", "der", "ich", "mich", "ver\u00b7zeh\u00b7re", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ART", "NN", "PTKVZ", "$,", "APPR", "PRELS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie lange soll denn noch di\u00df Wetter schlagen ein?", "tokens": ["Wie", "lan\u00b7ge", "soll", "denn", "noch", "di\u00df", "Wet\u00b7ter", "schla\u00b7gen", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "ADV", "ADV", "PDS", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und hast du denn f\u00fcr mich ganz keinen Sonnenschein?", "tokens": ["Und", "hast", "du", "denn", "f\u00fcr", "mich", "ganz", "kei\u00b7nen", "Son\u00b7nen\u00b7schein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich bin darzu versehn, da\u00df ich mich stets beschwere.", "tokens": ["Ich", "bin", "dar\u00b7zu", "ver\u00b7sehn", ",", "da\u00df", "ich", "mich", "stets", "be\u00b7schwe\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "VVINF", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ach mir! Du und mein Lieb seid eins nicht nachzugeben,", "tokens": ["Ach", "mir", "!", "Du", "und", "mein", "Lieb", "seid", "eins", "nicht", "nach\u00b7zu\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "$.", "PPER", "KON", "PPOSAT", "NN", "VAFIN", "PIS", "PTKNEG", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "bis ihr mir t\u00f6tet ganz mein schon gestorbnes Leben.", "tokens": ["bis", "ihr", "mir", "t\u00f6\u00b7tet", "ganz", "mein", "schon", "ge\u00b7storb\u00b7nes", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "ADV", "PPOSAT", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie aber kanst du dir so stets zuwider sein?", "tokens": ["Wie", "a\u00b7ber", "kanst", "du", "dir", "so", "stets", "zu\u00b7wi\u00b7der", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "PPER", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sonst Alles hei\u00dfest du in stetem Wechsel gehen.", "tokens": ["Sonst", "Al\u00b7les", "hei\u00b7\u00dfest", "du", "in", "ste\u00b7tem", "Wech\u00b7sel", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drei Dinge sind es nur, die ohne Wandel stehen:", "tokens": ["Drei", "Din\u00b7ge", "sind", "es", "nur", ",", "die", "oh\u00b7ne", "Wan\u00b7del", "ste\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "ADV", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "dein Ha\u00df, der Liebsten Zorn, und diese meine Pein.", "tokens": ["dein", "Ha\u00df", ",", "der", "Liebs\u00b7ten", "Zorn", ",", "und", "die\u00b7se", "mei\u00b7ne", "Pein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,", "KON", "PDS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}