{"textgrid.poem.67808": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "4. Abschiedslied eines M\u00e4dchens", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.85", "no:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort im Garten bl\u00fchten Majorane,", "tokens": ["Dort", "im", "Gar\u00b7ten", "bl\u00fch\u00b7ten", "Ma\u00b7jo\u00b7ra\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hier im Garten bl\u00fchten Tymiane,", "tokens": ["Hier", "im", "Gar\u00b7ten", "bl\u00fch\u00b7ten", "Ty\u00b7mi\u00b7a\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und wo unser Schwesterchen sich lehnte,", "tokens": ["Und", "wo", "un\u00b7ser", "Schwes\u00b7ter\u00b7chen", "sich", "lehn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da die allerbesten Bl\u00fcmlein bl\u00fchten.", "tokens": ["Da", "die", "al\u00b7ler\u00b7bes\u00b7ten", "Bl\u00fcm\u00b7lein", "bl\u00fch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Warum liegst du hingelehnt, mein M\u00e4dchen?", "tokens": ["Wa\u00b7rum", "liegst", "du", "hin\u00b7ge\u00b7lehnt", ",", "mein", "M\u00e4d\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "VVPP", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Warum hingelehnt, mein junges M\u00e4dchen?", "tokens": ["Wa\u00b7rum", "hin\u00b7ge\u00b7lehnt", ",", "mein", "jun\u00b7ges", "M\u00e4d\u00b7chen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ist nicht Jugend noch dein liebes Leben?", "tokens": ["Ist", "nicht", "Ju\u00b7gend", "noch", "dein", "lie\u00b7bes", "Le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "NN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und noch leicht und frisch dein junges Herzchen?", "tokens": ["Und", "noch", "leicht", "und", "frisch", "dein", "jun\u00b7ges", "Herz\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "KON", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Ist gleich Jugend noch mein liebes Leben,", "tokens": ["Ist", "gleich", "Ju\u00b7gend", "noch", "mein", "lie\u00b7bes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und noch frisch und leicht mein junges Herzchen,", "tokens": ["Und", "noch", "frisch", "und", "leicht", "mein", "jun\u00b7ges", "Herz\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "KON", "ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Dennoch f\u00fchl' ich junges M\u00e4dchen Schmerzen,", "tokens": ["Den\u00b7noch", "f\u00fchl'", "ich", "jun\u00b7ges", "M\u00e4d\u00b7chen", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Heute geht zu Ende meine Jugend.", "tokens": ["Heu\u00b7te", "geht", "zu", "En\u00b7de", "mei\u00b7ne", "Ju\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Durch die gr\u00fcne Hofflur geht das M\u00e4dchen,", "tokens": ["Durch", "die", "gr\u00fc\u00b7ne", "Hof\u00b7flur", "geht", "das", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ihren Brautkranz in dem weissen H\u00e4ndchen,", "tokens": ["Ih\u00b7ren", "Braut\u00b7kranz", "in", "dem", "weis\u00b7sen", "H\u00e4nd\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "O mein Kr\u00e4nzel! o mein schwarzes Kr\u00e4nzel,", "tokens": ["O", "mein", "Kr\u00e4n\u00b7zel", "!", "o", "mein", "schwar\u00b7zes", "Kr\u00e4n\u00b7zel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$.", "FM", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Weit von hinnen wirst du mit mir gehen!", "tokens": ["Weit", "von", "hin\u00b7nen", "wirst", "du", "mit", "mir", "ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADV", "VAFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Lebe wohl nun, Mutter, liebe Mutter!", "tokens": ["Le\u00b7be", "wohl", "nun", ",", "Mut\u00b7ter", ",", "lie\u00b7be", "Mut\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "$,", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Lebe wohl nun, Vater, lieber Vater!", "tokens": ["Le\u00b7be", "wohl", "nun", ",", "Va\u00b7ter", ",", "lie\u00b7ber", "Va\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "$,", "NN", "$,", "ADV", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Lebt wohl, liebe Br\u00fcder!", "tokens": ["Lebt", "wohl", ",", "lie\u00b7be", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Lebt wohl, liebe Schwestern!", "tokens": ["Lebt", "wohl", ",", "lie\u00b7be", "Schwes\u00b7tern", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}