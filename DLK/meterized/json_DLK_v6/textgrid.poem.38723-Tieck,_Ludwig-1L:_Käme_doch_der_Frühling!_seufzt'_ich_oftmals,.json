{"textgrid.poem.38723": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: K\u00e4me doch der Fr\u00fchling! seufzt' ich oftmals,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00e4me doch der Fr\u00fchling! seufzt' ich oftmals,", "tokens": ["K\u00e4\u00b7me", "doch", "der", "Fr\u00fch\u00b7ling", "!", "seufzt'", "ich", "oft\u00b7mals", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$.", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df der s\u00fc\u00dfe Blumenduft, das Fl\u00fcstern", "tokens": ["Da\u00df", "der", "s\u00fc\u00b7\u00dfe", "Blu\u00b7men\u00b7duft", ",", "das", "Fl\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Holder Birken und das Lied der Lerchen", "tokens": ["Hol\u00b7der", "Bir\u00b7ken", "und", "das", "Lied", "der", "Ler\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Meine hei\u00dfen Thr\u00e4nen trocknen m\u00f6chten! \u2013", "tokens": ["Mei\u00b7ne", "hei\u00b7\u00dfen", "Thr\u00e4\u00b7nen", "trock\u00b7nen", "m\u00f6ch\u00b7ten", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "VMFIN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und in jedem Jahre kam der Fr\u00fchling,", "tokens": ["Und", "in", "je\u00b7dem", "Jah\u00b7re", "kam", "der", "Fr\u00fch\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und in jedem Jahre weint' ich Thr\u00e4nen:", "tokens": ["Und", "in", "je\u00b7dem", "Jah\u00b7re", "weint'", "ich", "Thr\u00e4\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "T\u00f6ne, Blumen, holdes Baumgefl\u00fcster,", "tokens": ["T\u00f6\u00b7ne", ",", "Blu\u00b7men", ",", "hol\u00b7des", "Baum\u00b7ge\u00b7fl\u00fcs\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Alles ging wie scheu mir aus dem Wege,", "tokens": ["Al\u00b7les", "ging", "wie", "scheu", "mir", "aus", "dem", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KOKOM", "ADJD", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Nichts, das meinen hei\u00dfen Busen k\u00fchlte:", "tokens": ["Nichts", ",", "das", "mei\u00b7nen", "hei\u00b7\u00dfen", "Bu\u00b7sen", "k\u00fchl\u00b7te", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Und ich flehte nicht mehr um den Fr\u00fchling.", "tokens": ["Und", "ich", "fleh\u00b7te", "nicht", "mehr", "um", "den", "Fr\u00fch\u00b7ling", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ADV", "APPR", "ART", "NN", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.11": {"text": "Kl\u00e4glich kam er, kaum da\u00df ich's bemerkte,", "tokens": ["Kl\u00e4g\u00b7lich", "kam", "er", ",", "kaum", "da\u00df", "ich's", "be\u00b7merk\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "ADV", "KOUS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "D\u00fcster blickt' ich in sein gr\u00fcn Gewebe,", "tokens": ["D\u00fcs\u00b7ter", "blickt'", "ich", "in", "sein", "gr\u00fcn", "Ge\u00b7we\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Dachte: bist nicht besser als die andern! \u2013", "tokens": ["Dach\u00b7te", ":", "bist", "nicht", "bes\u00b7ser", "als", "die", "an\u00b7dern", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$.", "VAFIN", "PTKNEG", "ADJD", "KOKOM", "ART", "ADJA", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Hinter mir h\u00f6rt' ich ein leises Rieseln,", "tokens": ["Hin\u00b7ter", "mir", "h\u00f6rt'", "ich", "ein", "lei\u00b7ses", "Rie\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie wenn B\u00e4chlein \u00fcber Kiesel jauchzen,", "tokens": ["Wie", "wenn", "B\u00e4ch\u00b7lein", "\u00fc\u00b7ber", "Kie\u00b7sel", "jauch\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Hinter mir lief Wind durch das Geb\u00fcsche,", "tokens": ["Hin\u00b7ter", "mir", "lief", "Wind", "durch", "das", "Ge\u00b7b\u00fc\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Seitw\u00e4rts nickten alle Blumen freundlich,", "tokens": ["Seit\u00b7w\u00e4rts", "nick\u00b7ten", "al\u00b7le", "Blu\u00b7men", "freund\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und in sanften r\u00f6thern Strahlen spielte", "tokens": ["Und", "in", "sanf\u00b7ten", "r\u00f6\u00b7thern", "Strah\u00b7len", "spiel\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sonnenschein zum gr\u00fcnen Boden nieder.", "tokens": ["Son\u00b7nen\u00b7schein", "zum", "gr\u00fc\u00b7nen", "Bo\u00b7den", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Sinnend stand ich jetzt, ein Weilchen zweifelnd", "tokens": ["Sin\u00b7nend", "stand", "ich", "jetzt", ",", "ein", "Weil\u00b7chen", "zwei\u00b7felnd"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Was die holde T\u00e4uschung um mich zaubre.", "tokens": ["Was", "die", "hol\u00b7de", "T\u00e4u\u00b7schung", "um", "mich", "zaub\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Als ich wieder auf vom Boden blickte,", "tokens": ["Als", "ich", "wie\u00b7der", "auf", "vom", "Bo\u00b7den", "blick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Stand ein holder Knabe mir zur Seiten,", "tokens": ["Stand", "ein", "hol\u00b7der", "Kna\u00b7be", "mir", "zur", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Goldne Locken hingen um die Schl\u00e4fe,", "tokens": ["Gold\u00b7ne", "Lo\u00b7cken", "hin\u00b7gen", "um", "die", "Schl\u00e4\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Um die Lippen spielte schalkisch L\u00e4cheln,", "tokens": ["Um", "die", "Lip\u00b7pen", "spiel\u00b7te", "schal\u00b7kisch", "L\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVFIN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Sah mich an mit keckem blauen Auge:", "tokens": ["Sah", "mich", "an", "mit", "ke\u00b7ckem", "blau\u00b7en", "Au\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbtr\u00e4umer du! zertritt nicht alle Freuden,", "tokens": ["\u00bb", "tr\u00e4u\u00b7mer", "du", "!", "zer\u00b7tritt", "nicht", "al\u00b7le", "Freu\u00b7den", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "PPER", "$.", "VVFIN", "PTKNEG", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die so zart in deinem Wege liegen!\u00ab \u2013", "tokens": ["Die", "so", "zart", "in", "dei\u00b7nem", "We\u00b7ge", "lie\u00b7gen", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$.", "$(", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Rief er, hob den Zeigefinger drohend. \u2013", "tokens": ["Rief", "er", ",", "hob", "den", "Zei\u00b7ge\u00b7fin\u00b7ger", "dro\u00b7hend", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sieh, wie sich auf mein Gebot die Waldung", "tokens": ["Sieh", ",", "wie", "sich", "auf", "mein", "Ge\u00b7bot", "die", "Wal\u00b7dung"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "PRF", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Neu begr\u00fcnt, wie Glanz und s\u00fc\u00dfes Leben", "tokens": ["Neu", "be\u00b7gr\u00fcnt", ",", "wie", "Glanz", "und", "s\u00fc\u00b7\u00dfes", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "$,", "PWAV", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sich auf jedem Zweige schaukelt; Blumen,", "tokens": ["Sich", "auf", "je\u00b7dem", "Zwei\u00b7ge", "schau\u00b7kelt", ";", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "APPR", "PIAT", "NN", "VVFIN", "$.", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Nachtigallen, D\u00fcfte, alles ruft dich", "tokens": ["Nach\u00b7ti\u00b7gal\u00b7len", ",", "D\u00fcf\u00b7te", ",", "al\u00b7les", "ruft", "dich"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "PIS", "VVFIN", "PPER"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "An mit wunderbar-holdseel'gen T\u00f6nen;", "tokens": ["An", "mit", "wun\u00b7der\u00b7ba\u00b7rhold\u00b7seel'\u00b7gen", "T\u00f6\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.9": {"text": "Gehst du nicht in deinem eignen Schatten?", "tokens": ["Gehst", "du", "nicht", "in", "dei\u00b7nem", "eig\u00b7nen", "Schat\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Bist du, Thor, nicht selber dir im Wege?", "tokens": ["Bist", "du", ",", "Thor", ",", "nicht", "sel\u00b7ber", "dir", "im", "We\u00b7ge", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "NN", "$,", "PTKNEG", "ADV", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Stracks voll Mismuth ward mein banger Busen:", "tokens": ["Stracks", "voll", "Mis\u00b7muth", "ward", "mein", "ban\u00b7ger", "Bu\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Kinder, sagt' ich, sollten nicht so sprechen,", "tokens": ["Kin\u00b7der", ",", "sagt'", "ich", ",", "soll\u00b7ten", "nicht", "so", "spre\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Th\u00f6richt sind sie, haben nichts erfahren,", "tokens": ["Th\u00f6\u00b7richt", "sind", "sie", ",", "ha\u00b7ben", "nichts", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "VAFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Leben ohne Sorge, unbefangen,", "tokens": ["Le\u00b7ben", "oh\u00b7ne", "Sor\u00b7ge", ",", "un\u00b7be\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wissen \u00fcber Spielger\u00e4th zu urtheln,", "tokens": ["Wis\u00b7sen", "\u00fc\u00b7ber", "Spiel\u00b7ge\u00b7r\u00e4\u00b7th", "zu", "ur\u00b7theln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "M\u00fcssen aber \u00fcber Kummer schweigen.", "tokens": ["M\u00fcs\u00b7sen", "a\u00b7ber", "\u00fc\u00b7ber", "Kum\u00b7mer", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Also sagt' ich ernsthaftlich vermahnend,", "tokens": ["Al\u00b7so", "sagt'", "ich", "ernst\u00b7haft\u00b7lich", "ver\u00b7mah\u00b7nend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Meinte, da\u00df er sich wohl sch\u00e4men d\u00fcrfte,", "tokens": ["Mein\u00b7te", ",", "da\u00df", "er", "sich", "wohl", "sch\u00e4\u00b7men", "d\u00fcrf\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PRF", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Aber laut auf lachte nun der Bube", "tokens": ["A\u00b7ber", "laut", "auf", "lach\u00b7te", "nun", "der", "Bu\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und die Fassung w\u00e4r' mir fast entgangen.", "tokens": ["Und", "die", "Fas\u00b7sung", "w\u00e4r'", "mir", "fast", "ent\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Aber als ich herzlich z\u00fcrnen wollte,", "tokens": ["A\u00b7ber", "als", "ich", "herz\u00b7lich", "z\u00fcr\u00b7nen", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "War Besinnung so wie Zorn entschwunden,", "tokens": ["War", "Be\u00b7sin\u00b7nung", "so", "wie", "Zorn", "ent\u00b7schwun\u00b7den"