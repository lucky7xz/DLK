{"textgrid.poem.27662": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "76.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Manche Braut bleibt nicht zufrieden,", "tokens": ["Man\u00b7che", "Braut", "bleibt", "nicht", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ietzt der Brauch bleibt vermieden,", "tokens": ["Da\u00df", "ietzt", "der", "Brauch", "bleibt", "ver\u00b7mie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da\u00df nicht thun am Hochzeit-Feste,", "tokens": ["Da\u00df", "nicht", "thun", "am", "Hoch\u00b7zeit\u00b7Fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "VVINF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was der Br\u00e4utgam thut, die G\u00e4ste.", "tokens": ["Was", "der", "Br\u00e4ut\u00b7gam", "thut", ",", "die", "G\u00e4s\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ART", "NE", "VVFIN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}