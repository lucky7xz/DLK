{"textgrid.poem.33850": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Requiem", "genre": "verse", "period": "N.A.", "pub_year": 1840, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seele, vergi\u00df sie nicht,", "tokens": ["See\u00b7le", ",", "ver\u00b7gi\u00df", "sie", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "PTKNEG", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Seele, vergi\u00df nicht die Todten!", "tokens": ["See\u00b7le", ",", "ver\u00b7gi\u00df", "nicht", "die", "Tod\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PTKNEG", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Sieh, sie umschweben dich,", "tokens": ["Sieh", ",", "sie", "um\u00b7schwe\u00b7ben", "dich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Schauernd, verlassen,", "tokens": ["Schau\u00b7ernd", ",", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und in den heiligen Gluten,", "tokens": ["Und", "in", "den", "hei\u00b7li\u00b7gen", "Glu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Die den Armen die Liebe sch\u00fcrt,", "tokens": ["Die", "den", "Ar\u00b7men", "die", "Lie\u00b7be", "sch\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Athmen sie auf und erwarmen,", "tokens": ["Ath\u00b7men", "sie", "auf", "und", "er\u00b7war\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "KON", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Und genie\u00dfen zum letzten Mal", "tokens": ["Und", "ge\u00b7nie\u00b7\u00dfen", "zum", "letz\u00b7ten", "Mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Ihr verglimmendes Leben.", "tokens": ["Ihr", "ver\u00b7glim\u00b7men\u00b7des", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Seele, vergi\u00df sie nicht,", "tokens": ["See\u00b7le", ",", "ver\u00b7gi\u00df", "sie", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "PTKNEG", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Seele, vergi\u00df nicht die Todten!", "tokens": ["See\u00b7le", ",", "ver\u00b7gi\u00df", "nicht", "die", "Tod\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PTKNEG", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Sieh, sie umschweben dich,", "tokens": ["Sieh", ",", "sie", "um\u00b7schwe\u00b7ben", "dich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Schauernd, verlassen,", "tokens": ["Schau\u00b7ernd", ",", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und wenn du dich erkaltend", "tokens": ["Und", "wenn", "du", "dich", "er\u00b7kal\u00b7tend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihnen verschlie\u00dfest, erstarren sie", "tokens": ["Ih\u00b7nen", "ver\u00b7schlie\u00b7\u00dfest", ",", "er\u00b7star\u00b7ren", "sie"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPER"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Bis hinein in das Tiefste.", "tokens": ["Bis", "hin\u00b7ein", "in", "das", "Tiefs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ART", "ADJA", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Dann ergreift sie der Sturm der Nacht,", "tokens": ["Dann", "er\u00b7greift", "sie", "der", "Sturm", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Dem sie, zusammengekrampft in sich,", "tokens": ["Dem", "sie", ",", "zu\u00b7sam\u00b7men\u00b7ge\u00b7krampft", "in", "sich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "VVFIN", "APPR", "PRF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Trotzten im Schoo\u00dfe der Liebe,", "tokens": ["Trotz\u00b7ten", "im", "Schoo\u00b7\u00dfe", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.9": {"text": "Und er jagt sie mit Ungest\u00fcm", "tokens": ["Und", "er", "jagt", "sie", "mit", "Un\u00b7ge\u00b7st\u00fcm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Durch die unendliche W\u00fcste hin,", "tokens": ["Durch", "die", "un\u00b7end\u00b7li\u00b7che", "W\u00fcs\u00b7te", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Wo nicht Leben mehr ist, nur Kampf", "tokens": ["Wo", "nicht", "Le\u00b7ben", "mehr", "ist", ",", "nur", "Kampf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PTKNEG", "NN", "ADV", "VAFIN", "$,", "ADV", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.12": {"text": "Losgelassener Kr\u00e4fte", "tokens": ["Los\u00b7ge\u00b7las\u00b7se\u00b7ner", "Kr\u00e4f\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.13": {"text": "Um erneuertes Sein!", "tokens": ["Um", "er\u00b7neu\u00b7er\u00b7tes", "Sein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Seele, vergi\u00df sie nicht,", "tokens": ["See\u00b7le", ",", "ver\u00b7gi\u00df", "sie", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "PTKNEG", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Seele, vergi\u00df nicht die Todten!", "tokens": ["See\u00b7le", ",", "ver\u00b7gi\u00df", "nicht", "die", "Tod\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PTKNEG", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}