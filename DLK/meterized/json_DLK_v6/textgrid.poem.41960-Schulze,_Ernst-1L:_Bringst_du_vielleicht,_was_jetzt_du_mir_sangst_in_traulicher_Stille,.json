{"textgrid.poem.41960": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bringst du vielleicht, was jetzt du mir sangst in traulicher Stille,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bringst du vielleicht, was jetzt du mir sangst in traulicher Stille,", "tokens": ["Bringst", "du", "viel\u00b7leicht", ",", "was", "jetzt", "du", "mir", "sangst", "in", "trau\u00b7li\u00b7cher", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PRELS", "ADV", "PPER", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+---+-+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Einst in die H\u00e4nde des Volks, zu der Gebildeten Ohr,", "tokens": ["Einst", "in", "die", "H\u00e4n\u00b7de", "des", "Volks", ",", "zu", "der", "Ge\u00b7bil\u00b7de\u00b7ten", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "O dann tilge den Namen hinweg der Geliebten und jedes", "tokens": ["O", "dann", "til\u00b7ge", "den", "Na\u00b7men", "hin\u00b7weg", "der", "Ge\u00b7lieb\u00b7ten", "und", "je\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VVFIN", "ART", "NN", "APZR", "ART", "NN", "KON", "PIAT"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Deutende Wort, denn hart richtet der kalte Verstand.", "tokens": ["Deu\u00b7ten\u00b7de", "Wort", ",", "denn", "hart", "rich\u00b7tet", "der", "kal\u00b7te", "Ver\u00b7stand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.5": {"text": "Also sprachst du zu mir, und ich z\u00fcrnt' und sagte: du liebst mich,", "tokens": ["Al\u00b7so", "sprachst", "du", "zu", "mir", ",", "und", "ich", "z\u00fcrnt'", "und", "sag\u00b7te", ":", "du", "liebst", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "$,", "KON", "PPER", "VVFIN", "KON", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Und doch scheust du den Spott, welcher nur Schuldige kr\u00e4nkt,", "tokens": ["Und", "doch", "scheust", "du", "den", "Spott", ",", "wel\u00b7cher", "nur", "Schul\u00b7di\u00b7ge", "kr\u00e4nkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "ADV", "NN", "VVFIN", "$,"], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "G\u00f6nnst die Freude mir nicht der Grazie Dichter zu hei\u00dfen,", "tokens": ["G\u00f6nnst", "die", "Freu\u00b7de", "mir", "nicht", "der", "Gra\u00b7zie", "Dich\u00b7ter", "zu", "hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "PTKNEG", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Nicht das verstohlene Gl\u00fcck, rings mich beneidet zu sehn?", "tokens": ["Nicht", "das", "ver\u00b7stoh\u00b7le\u00b7ne", "Gl\u00fcck", ",", "rings", "mich", "be\u00b7nei\u00b7det", "zu", "sehn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "$,", "ADV", "PPER", "VVPP", "PTKZU", "VVINF", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Feindlich wandt' ich mich ab und schwieg. Du senktest das K\u00f6pfchen,", "tokens": ["Feind\u00b7lich", "wandt'", "ich", "mich", "ab", "und", "schwieg", ".", "Du", "senk\u00b7test", "das", "K\u00f6pf\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "PTKVZ", "KON", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Doch bald hobst du den Blick sch\u00fcchtern von neuem empor,", "tokens": ["Doch", "bald", "hobst", "du", "den", "Blick", "sch\u00fcch\u00b7tern", "von", "neu\u00b7em", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "VVFIN", "APPR", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-++--+--+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Regtest den rosigen Mund und wolltest reden, doch pl\u00f6tzlich", "tokens": ["Reg\u00b7test", "den", "ro\u00b7si\u00b7gen", "Mund", "und", "woll\u00b7test", "re\u00b7den", ",", "doch", "pl\u00f6tz\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "KON", "VMFIN", "VVINF", "$,", "ADV", "ADJD"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Flo\u00df dir ein h\u00f6heres Roth \u00fcber die Wange; du schwiegst.", "tokens": ["Flo\u00df", "dir", "ein", "h\u00f6\u00b7he\u00b7res", "Roth", "\u00fc\u00b7ber", "die", "Wan\u00b7ge", ";", "du", "schwiegst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.13": {"text": "Rasch nun ging ich hinweg, und z\u00fcrnen wollt' ich, doch immer", "tokens": ["Rasch", "nun", "ging", "ich", "hin\u00b7weg", ",", "und", "z\u00fcr\u00b7nen", "wollt'", "ich", ",", "doch", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVINF", "VMFIN", "PPER", "$,", "ADV", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "Traf mich der eigene Pfeil, immer nur z\u00fcrnt' ich mir selbst.", "tokens": ["Traf", "mich", "der", "ei\u00b7ge\u00b7ne", "Pfeil", ",", "im\u00b7mer", "nur", "z\u00fcrnt'", "ich", "mir", "selbst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "ADV", "ADV", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "+--+--+--++--+", "measure": "dactylic.tri.plus"}, "line.15": {"text": "Sieh, da erhellte den d\u00fcsteren Geist ein pl\u00f6tzlicher Lichtglanz,", "tokens": ["Sieh", ",", "da", "er\u00b7hell\u00b7te", "den", "d\u00fcs\u00b7te\u00b7ren", "Geist", "ein", "pl\u00f6tz\u00b7li\u00b7cher", "Licht\u00b7glanz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.16": {"text": "Und ich erkannte des Worts s\u00fc\u00dfen, verborgenen Sinn.", "tokens": ["Und", "ich", "er\u00b7kann\u00b7te", "des", "Worts", "s\u00fc\u00b7\u00dfen", ",", "ver\u00b7bor\u00b7ge\u00b7nen", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Ha, du reizendes Weib, wie f\u00fchlst du den Zauber der Liebe", "tokens": ["Ha", ",", "du", "rei\u00b7zen\u00b7des", "Weib", ",", "wie", "f\u00fchlst", "du", "den", "Zau\u00b7ber", "der", "Lie\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "ADJA", "NN", "$,", "PWAV", "VVFIN", "PPER", "ART", "NN", "ART", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Und ihr inneres Gl\u00fcck tiefer und zarter als ich!", "tokens": ["Und", "ihr", "in\u00b7ne\u00b7res", "Gl\u00fcck", "tie\u00b7fer", "und", "zar\u00b7ter", "als", "ich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ADJD", "KON", "ADJA", "KOUS", "PPER", "$."], "meter": "--+--++--+-+-", "measure": "anapaest.di.plus"}, "line.19": {"text": "Nur in der D\u00e4mmerung weht der Viol' erquickender Aushauch,", "tokens": ["Nur", "in", "der", "D\u00e4m\u00b7me\u00b7rung", "weht", "der", "Vi\u00b7ol'", "er\u00b7qui\u00b7cken\u00b7der", "Aus\u00b7hauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+-+-+", "measure": "dactylic.tri.plus"}, "line.20": {"text": "Und im dunkelsten Hain finget die Nachtigall nur.", "tokens": ["Und", "im", "dun\u00b7kels\u00b7ten", "Hain", "fin\u00b7get", "die", "Nach\u00b7ti\u00b7gall", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.21": {"text": "So auch meidet die Liebe das Licht; im mondlichen Glanz nur", "tokens": ["So", "auch", "mei\u00b7det", "die", "Lie\u00b7be", "das", "Licht", ";", "im", "mond\u00b7li\u00b7chen", "Glanz", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$.", "APPRART", "ADJA", "NN", "ADV"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.22": {"text": "Und im Gesausel des Hains bl\u00fcht sie verstohlen empor.", "tokens": ["Und", "im", "Ge\u00b7sau\u00b7sel", "des", "Hains", "bl\u00fcht", "sie", "ver\u00b7stoh\u00b7len", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Wohnt doch Idalia selbst in verschwiegenem Schatten, und deckt doch", "tokens": ["Wohnt", "doch", "I\u00b7da\u00b7lia", "selbst", "in", "ver\u00b7schwie\u00b7ge\u00b7nem", "Schat\u00b7ten", ",", "und", "deckt", "doch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "NE", "ADV", "APPR", "ADJA", "NN", "$,", "KON", "VVFIN", "ADV"], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.24": {"text": "Amorn, wenn er entschlief, immer ein Rosengeb\u00fcsch.", "tokens": ["A\u00b7morn", ",", "wenn", "er", "ent\u00b7schlief", ",", "im\u00b7mer", "ein", "Ro\u00b7sen\u00b7ge\u00b7b\u00fcsch", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "VVFIN", "$,", "ADV", "ART", "NN", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}, "line.25": {"text": "Richtig erscheint mir ein Ku\u00df und geschmacklos, wenn du im Kreise", "tokens": ["Rich\u00b7tig", "er\u00b7scheint", "mir", "ein", "Ku\u00df", "und", "ge\u00b7schmack\u00b7los", ",", "wenn", "du", "im", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "KON", "ADJD", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "+--+--+--+--+-+-", "measure": "dactylic.tetra.plus"}, "line.26": {"text": "Sp\u00e4hender Augen mir ihn giebst, im Get\u00e4ndel des Spiels:", "tokens": ["Sp\u00e4\u00b7hen\u00b7der", "Au\u00b7gen", "mir", "ihn", "giebst", ",", "im", "Ge\u00b7t\u00e4n\u00b7del", "des", "Spiels", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "PPER", "VVFIN", "$,", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.27": {"text": "Doch wenn ein heimlicher Druck ihn vers\u00fc\u00dft, wenn h\u00f6her die Lippen,", "tokens": ["Doch", "wenn", "ein", "heim\u00b7li\u00b7cher", "Druck", "ihn", "ver\u00b7s\u00fc\u00dft", ",", "wenn", "h\u00f6\u00b7her", "die", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "PPER", "VVPP", "$,", "KOUS", "ADJD", "ART", "NN", "$,"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Eng an die meinen gepre\u00dft, schwellen, dann bin ich ein Gott.", "tokens": ["Eng", "an", "die", "mei\u00b7nen", "ge\u00b7pre\u00dft", ",", "schwel\u00b7len", ",", "dann", "bin", "ich", "ein", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "PPOSAT", "VVPP", "$,", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+---+-++--+", "measure": "iambic.hexa.chol"}, "line.29": {"text": "O verzeih mir den th\u00f6richten Wahn, o strafe mich! schuldig", "tokens": ["O", "ver\u00b7zeih", "mir", "den", "th\u00f6\u00b7rich\u00b7ten", "Wahn", ",", "o", "stra\u00b7fe", "mich", "!", "schul\u00b7dig"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NE", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "FM", "VVFIN", "PPER", "$.", "ADJD"], "meter": "+-+--++-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.30": {"text": "Steh' ich vor dir, doch sey streng und gelinde zugleich;", "tokens": ["Steh'", "ich", "vor", "dir", ",", "doch", "sey", "streng", "und", "ge\u00b7lin\u00b7de", "zu\u00b7gleich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,", "ADV", "VAFIN", "ADJD", "KON", "ADJA", "ADV", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.31": {"text": "Banne mich fort aus deinem Gesicht zw\u00f6lf traurige Monden,", "tokens": ["Ban\u00b7ne", "mich", "fort", "aus", "dei\u00b7nem", "Ge\u00b7sicht", "zw\u00f6lf", "trau\u00b7ri\u00b7ge", "Mon\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "APPR", "PPOSAT", "NN", "CARD", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.32": {"text": "Gro\u00df zwar w\u00e4re der Schmerz, aber ich duldet' ihn doch:", "tokens": ["Gro\u00df", "zwar", "w\u00e4\u00b7re", "der", "Schmerz", ",", "a\u00b7ber", "ich", "dul\u00b7det'", "ihn", "doch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.33": {"text": "Nur la\u00df deinen Namen mich nie, f\u00fcr solch ein Vergehen", "tokens": ["Nur", "la\u00df", "dei\u00b7nen", "Na\u00b7men", "mich", "nie", ",", "f\u00fcr", "solch", "ein", "Ver\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPOSAT", "NN", "PPER", "ADV", "$,", "APPR", "PIAT", "ART", "NN"], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.34": {"text": "W\u00e4re die Strafe zu hart, nennen der sp\u00e4henden Welt.", "tokens": ["W\u00e4\u00b7re", "die", "Stra\u00b7fe", "zu", "hart", ",", "nen\u00b7nen", "der", "sp\u00e4\u00b7hen\u00b7den", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKA", "ADJD", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}}}}}