{"textgrid.poem.55077": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Mignon", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kennst du das Land, wo die Zitronen bl\u00fchn,", "tokens": ["Kennst", "du", "das", "Land", ",", "wo", "die", "Zit\u00b7ro\u00b7nen", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im dunkeln Laub die Goldorangen gl\u00fchn,", "tokens": ["Im", "dun\u00b7keln", "Laub", "die", "Gol\u00b7do\u00b7ran\u00b7gen", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein sanfter Wind vom blauen Himmel weht,", "tokens": ["Ein", "sanf\u00b7ter", "Wind", "vom", "blau\u00b7en", "Him\u00b7mel", "weht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Myrte still und hoch der Lorbeer steht,", "tokens": ["Die", "Myr\u00b7te", "still", "und", "hoch", "der", "Lor\u00b7beer", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Kennst du es wohl?", "tokens": ["Kennst", "du", "es", "wohl", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Dahin! Dahin", "tokens": ["Da\u00b7hin", "!", "Da\u00b7hin"], "token_info": ["word", "punct", "word"], "pos": ["PAV", "$.", "PAV"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "M\u00f6cht ich mit dir, o mein Geliebter, ziehn.", "tokens": ["M\u00f6cht", "ich", "mit", "dir", ",", "o", "mein", "Ge\u00b7lieb\u00b7ter", ",", "ziehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "$,", "FM", "PPOSAT", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Kennst du das Haus? Auf S\u00e4ulen ruht sein Dach,", "tokens": ["Kennst", "du", "das", "Haus", "?", "Auf", "S\u00e4u\u00b7len", "ruht", "sein", "Dach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "APPR", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es gl\u00e4nzt der Saal, es schimmert das Gemach,", "tokens": ["Es", "gl\u00e4nzt", "der", "Saal", ",", "es", "schim\u00b7mert", "das", "Ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Marmorbilder stehn und sehn mich an:", "tokens": ["Und", "Mar\u00b7mor\u00b7bil\u00b7der", "stehn", "und", "sehn", "mich", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was hat man dir, du armes Kind, getan?", "tokens": ["Was", "hat", "man", "dir", ",", "du", "ar\u00b7mes", "Kind", ",", "ge\u00b7tan", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "PIS", "PPER", "$,", "PPER", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Kennst du es wohl?", "tokens": ["Kennst", "du", "es", "wohl", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Dahin! Dahin", "tokens": ["Da\u00b7hin", "!", "Da\u00b7hin"], "token_info": ["word", "punct", "word"], "pos": ["PAV", "$.", "PAV"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "M\u00f6cht ich mit dir, o mein Besch\u00fctzer, ziehn.", "tokens": ["M\u00f6cht", "ich", "mit", "dir", ",", "o", "mein", "Be\u00b7sch\u00fct\u00b7zer", ",", "ziehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "$,", "FM", "PPOSAT", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Kennst du den Berg und seinen Wolkensteg?", "tokens": ["Kennst", "du", "den", "Berg", "und", "sei\u00b7nen", "Wol\u00b7kens\u00b7teg", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Maultier sucht im Nebel seinen Weg;", "tokens": ["Das", "Maul\u00b7tier", "sucht", "im", "Ne\u00b7bel", "sei\u00b7nen", "Weg", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In H\u00f6hlen wohnt der Drachen alte Brut;", "tokens": ["In", "H\u00f6h\u00b7len", "wohnt", "der", "Dra\u00b7chen", "al\u00b7te", "Brut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es st\u00fcrzt der Fels und \u00fcber ihn die Flut,", "tokens": ["Es", "st\u00fcrzt", "der", "Fels", "und", "\u00fc\u00b7ber", "ihn", "die", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Kennst du ihn wohl?", "tokens": ["Kennst", "du", "ihn", "wohl", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Dahin! Dahin", "tokens": ["Da\u00b7hin", "!", "Da\u00b7hin"], "token_info": ["word", "punct", "word"], "pos": ["PAV", "$.", "PAV"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Geht unser Weg! o Vater, la\u00df uns ziehn!", "tokens": ["Geht", "un\u00b7ser", "Weg", "!", "o", "Va\u00b7ter", ",", "la\u00df", "uns", "ziehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "FM", "NN", "$,", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}