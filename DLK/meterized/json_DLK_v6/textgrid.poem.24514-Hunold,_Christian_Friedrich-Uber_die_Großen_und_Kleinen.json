{"textgrid.poem.24514": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Uber die Gro\u00dfen und Kleinen", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der kleine hei\u00dft ein Zwerg/ und gro\u00dfe hei\u00dfen Riesen.", "tokens": ["Der", "klei\u00b7ne", "hei\u00dft", "ein", "Zwerg", "/", "und", "gro\u00b7\u00dfe", "hei\u00b7\u00dfen", "Rie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "$(", "KON", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr Sp\u00f6tter redet doch von Kleinen nicht zu frey:", "tokens": ["Ihr", "Sp\u00f6t\u00b7ter", "re\u00b7det", "doch", "von", "Klei\u00b7nen", "nicht", "zu", "frey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "NN", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil Hande der Natur/ die Kunst und Mahlerey", "tokens": ["Weil", "Han\u00b7de", "der", "Na\u00b7tur", "/", "die", "Kunst", "und", "Mah\u00b7le\u00b7rey"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ART", "NN", "$(", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein gr\u00f6\u00dfer Meisterst\u00fcck in kleinen stets erwiesen.", "tokens": ["Ein", "gr\u00f6\u00b7\u00dfer", "Meis\u00b7ter\u00b7st\u00fcck", "in", "klei\u00b7nen", "stets", "er\u00b7wie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "A