{"textgrid.poem.35329": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich sagte dem Tauber: Fleug', bis du entdeckt", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sagte dem Tauber: Fleug', bis du entdeckt", "tokens": ["Ich", "sag\u00b7te", "dem", "Tau\u00b7ber", ":", "Fleug'", ",", "bis", "du", "ent\u00b7deckt"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "NN", "$,", "KOUS", "PPER", "VVPP"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Blume mit wonnigem Duft,", "tokens": ["Die", "Blu\u00b7me", "mit", "won\u00b7ni\u00b7gem", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die Herzen bezaubert und Liebe erweckt!", "tokens": ["Die", "Her\u00b7zen", "be\u00b7zau\u00b7bert", "und", "Lie\u00b7be", "er\u00b7weckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Doch der Tauber: \u00bbZu schwer ist die Luft.\u00ab", "tokens": ["Doch", "der", "Tau\u00b7ber", ":", "\u00bb", "Zu", "schwer", "ist", "die", "Luft", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "$.", "$(", "PTKA", "ADJD", "VAFIN", "ART", "NN", "$.", "$("], "meter": "--+-+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Dem Falken: Ersp\u00e4he, was Herzen bezwingt;", "tokens": ["Dem", "Fal\u00b7ken", ":", "Er\u00b7sp\u00e4\u00b7he", ",", "was", "Her\u00b7zen", "be\u00b7zwingt", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "VVFIN", "$,", "PWS", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und ist es versengendes Licht,", "tokens": ["Und", "ist", "es", "ver\u00b7sen\u00b7gen\u00b7des", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sei der Retter, der es dem Himmel entringt!", "tokens": ["Sei", "der", "Ret\u00b7ter", ",", "der", "es", "dem", "Him\u00b7mel", "ent\u00b7ringt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Doch der Falke: \u00bbSo hoch steig' ich nicht.\u00ab", "tokens": ["Doch", "der", "Fal\u00b7ke", ":", "\u00bb", "So", "hoch", "steig'", "ich", "nicht", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "$.", "$(", "ADV", "ADJD", "VVFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Da sprach ich zum Geier: Nicht l\u00e4nger verwehrt", "tokens": ["Da", "sprach", "ich", "zum", "Gei\u00b7er", ":", "Nicht", "l\u00e4n\u00b7ger", "ver\u00b7wehrt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$.", "PTKNEG", "ADJD", "VVPP"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sei dies Herz dir mit seinem Leid;", "tokens": ["Sei", "dies", "Herz", "dir", "mit", "sei\u00b7nem", "Leid", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "NN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Nur lasse den Teil, der noch unversehrt!", "tokens": ["Nur", "las\u00b7se", "den", "Teil", ",", "der", "noch", "un\u00b7ver\u00b7sehrt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Doch: \u2013 \u00bbZu sp\u00e4t\u00ab \u2013 war des Geiers Bescheid.", "tokens": ["Doch", ":", "\u2013", "\u00bb", "Zu", "sp\u00e4t", "\u00ab", "\u2013", "war", "des", "Gei\u00b7ers", "Be\u00b7scheid", "."], "token_info": ["word", "punct", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$.", "$(", "$(", "PTKA", "ADJD", "$(", "$(", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}