{"textgrid.poem.62160": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Thadd\u00e4us Gora", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Purpurn gl\u00e4nzt die Abendr\u00f6te,", "tokens": ["Pur\u00b7purn", "gl\u00e4nzt", "die", "A\u00b7bend\u00b7r\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Still der Prosna zugekehrt,", "tokens": ["Still", "der", "Pros\u00b7na", "zu\u00b7ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Frauenbild man beten h\u00f6rt.", "tokens": ["Frau\u00b7en\u00b7bild", "man", "be\u00b7ten", "h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Warum weint Frau Margarete?", "tokens": ["Wa\u00b7rum", "weint", "Frau", "Mar\u00b7ga\u00b7re\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NN", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Fremdling, fr\u00e4gst, warum ich bete?", "tokens": ["Fremd\u00b7ling", ",", "fr\u00e4gst", ",", "wa\u00b7rum", "ich", "be\u00b7te", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hast von der Legion geh\u00f6rt,", "tokens": ["Hast", "von", "der", "Le\u00b7gi\u00b7on", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die, vom Schwerte aufgezehrt", "tokens": ["Die", ",", "vom", "Schwer\u00b7te", "auf\u00b7ge\u00b7zehrt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Polens Boden blutig s\u00e4te?", "tokens": ["Po\u00b7lens", "Bo\u00b7den", "blu\u00b7tig", "s\u00e4\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Lieber Sohn war mit dabei,", "tokens": ["Lie\u00b7ber", "Sohn", "war", "mit", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VAFIN", "APPR", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hochgelehrt und achtzehn Jahr,", "tokens": ["Hoch\u00b7ge\u00b7lehrt", "und", "acht\u00b7zehn", "Jahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "CARD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "St\u00e4rker war die Tyrannei,", "tokens": ["St\u00e4r\u00b7ker", "war", "die", "Ty\u00b7ran\u00b7nei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Reicht' das Schwert ihm selber dar", "tokens": ["Reicht'", "das", "Schwert", "ihm", "sel\u00b7ber", "dar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADV", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fremdling, eine Tr\u00e4ne weih:", "tokens": ["Fremd\u00b7ling", ",", "ei\u00b7ne", "Tr\u00e4\u00b7ne", "weih", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Polens Asche, sei fruchtbar! \u2013", "tokens": ["Po\u00b7lens", "A\u00b7sche", ",", "sei", "frucht\u00b7bar", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "$,", "VAFIN", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}