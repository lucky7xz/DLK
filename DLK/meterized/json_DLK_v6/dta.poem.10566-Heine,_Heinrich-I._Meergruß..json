{"dta.poem.10566": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "I.  \n  Meergru\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.71", "af:0.28"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Sey mir gegr\u00fc\u00dft, du ewiges Meer!", "tokens": ["Sey", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "du", "e\u00b7wi\u00b7ges", "Meer", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sey mir gegr\u00fc\u00dft zehntausendmal", "tokens": ["Sey", "mir", "ge\u00b7gr\u00fc\u00dft", "zehn\u00b7tau\u00b7send\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus jauchzendem Herzen", "tokens": ["Aus", "jauch\u00b7zen\u00b7dem", "Her\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wie einst dich begr\u00fc\u00dften", "tokens": ["Wie", "einst", "dich", "be\u00b7gr\u00fc\u00df\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Zehntausend Griechenherzen,", "tokens": ["Zehn\u00b7tau\u00b7send", "Grie\u00b7chen\u00b7her\u00b7zen", ","], "token_info": ["word", "word", "punct"], "pos": ["CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ungl\u00fcckbek\u00e4mpfende, heimathverlangende,", "tokens": ["Un\u00b7gl\u00fcck\u00b7be\u00b7k\u00e4mp\u00b7fen\u00b7de", ",", "hei\u00b7math\u00b7ver\u00b7lan\u00b7gen\u00b7de", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,"], "meter": "+--+--+--+--", "measure": "dactylic.tetra"}, "line.7": {"text": "Weltber\u00fchmte Griechenherzen.", "tokens": ["Welt\u00b7be\u00b7r\u00fchm\u00b7te", "Grie\u00b7chen\u00b7her\u00b7zen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Es wogten die Fluthen,", "tokens": ["Es", "wog\u00b7ten", "die", "Flut\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sie wogten und brausten,", "tokens": ["Sie", "wog\u00b7ten", "und", "braus\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die Sonne go\u00df eilig herunter", "tokens": ["Die", "Son\u00b7ne", "go\u00df", "ei\u00b7lig", "her\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die spielenden Rosenlichter,", "tokens": ["Die", "spie\u00b7len\u00b7den", "Ro\u00b7sen\u00b7lich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Die aufgescheuchten M\u00f6venz\u00fcge", "tokens": ["Die", "auf\u00b7ge\u00b7scheuch\u00b7ten", "M\u00f6\u00b7ven\u00b7z\u00fc\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Flatterten fort, lautschreiend,", "tokens": ["Flat\u00b7ter\u00b7ten", "fort", ",", "laut\u00b7schrei\u00b7end", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ADJD", "$,"], "meter": "+--++-+", "measure": "dactylic.init"}, "line.3": {"text": "Es stampften die Rosse, es klirrten die Schilde,", "tokens": ["Es", "stampf\u00b7ten", "die", "Ros\u00b7se", ",", "es", "klirr\u00b7ten", "die", "Schil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und weithin erscholl es, wie Siegesruf:", "tokens": ["Und", "weit\u00b7hin", "er\u00b7scholl", "es", ",", "wie", "Sie\u00b7ges\u00b7ruf", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "PWAV", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Thalatta! Thalatta!", "tokens": ["Tha\u00b7lat\u00b7ta", "!", "Tha\u00b7lat\u00b7ta", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Sey mir gegr\u00fc\u00dft, du ewiges Meer,", "tokens": ["Sey", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "du", "e\u00b7wi\u00b7ges", "Meer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wie Sprache der Heimath rauscht mir dein Wasser,", "tokens": ["Wie", "Spra\u00b7che", "der", "Hei\u00b7math", "rauscht", "mir", "dein", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie Tr\u00e4ume der Kindheit seh' ich es flimmern", "tokens": ["Wie", "Tr\u00e4u\u00b7me", "der", "Kind\u00b7heit", "seh'", "ich", "es", "flim\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ART", "NN", "VVFIN", "PPER", "PPER", "VVINF"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auf deinem wogenden Wellengebiet,", "tokens": ["Auf", "dei\u00b7nem", "wo\u00b7gen\u00b7den", "Wel\u00b7len\u00b7ge\u00b7biet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und alte Erinn'rung erz\u00e4hlt mir auf's neue,", "tokens": ["Und", "al\u00b7te", "Er\u00b7inn'\u00b7rung", "er\u00b7z\u00e4hlt", "mir", "auf's", "neu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "ADJA", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Von all dem lieben, herrlichen Spielzeug,", "tokens": ["Von", "all", "dem", "lie\u00b7ben", ",", "herr\u00b7li\u00b7chen", "Spiel\u00b7zeug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Von all den blinkenden Weihnachtsgaben,", "tokens": ["Von", "all", "den", "blin\u00b7ken\u00b7den", "Weih\u00b7nachts\u00b7ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Von all den rothen Corallenb\u00e4umen,", "tokens": ["Von", "all", "den", "ro\u00b7then", "Co\u00b7ral\u00b7len\u00b7b\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Goldfischchen, Perlen und bunten Muscheln,", "tokens": ["Gold\u00b7fi\u00b7schchen", ",", "Per\u00b7len", "und", "bun\u00b7ten", "Mu\u00b7scheln", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.10": {"text": "Die du geheimni\u00dfvoll bewahrest", "tokens": ["Die", "du", "ge\u00b7heim\u00b7ni\u00df\u00b7voll", "be\u00b7wah\u00b7rest"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Dort unten im klaren Kristallhaus.", "tokens": ["Dort", "un\u00b7ten", "im", "kla\u00b7ren", "Kris\u00b7tall\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "O! wie hab' ich geschmachtet in \u00f6der Fremde!", "tokens": ["O", "!", "wie", "hab'", "ich", "ge\u00b7schmach\u00b7tet", "in", "\u00f6\u00b7der", "Frem\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "VAFIN", "PPER", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Gleich einer welken Blume", "tokens": ["Gleich", "ei\u00b7ner", "wel\u00b7ken", "Blu\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In des Botanikers blecherner Kapsel,", "tokens": ["In", "des", "Bo\u00b7ta\u00b7ni\u00b7kers", "ble\u00b7cher\u00b7ner", "Kap\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Lag mir das Herz in der Brust;", "tokens": ["Lag", "mir", "das", "Herz", "in", "der", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Mir ist, als sa\u00df ich winterlange,", "tokens": ["Mir", "ist", ",", "als", "sa\u00df", "ich", "win\u00b7ter\u00b7lan\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Kranker, in dunkler Krankenstube,", "tokens": ["Ein", "Kran\u00b7ker", ",", "in", "dunk\u00b7ler", "Kran\u00b7ken\u00b7stu\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und nun verla\u00df ich sie pl\u00f6tzlich,", "tokens": ["Und", "nun", "ver\u00b7la\u00df", "ich", "sie", "pl\u00f6tz\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Und blendend strahlt mir entgegen", "tokens": ["Und", "blen\u00b7dend", "strahlt", "mir", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der schmaragdne Fr\u00fchling, der sonnengeweckte,", "tokens": ["Der", "schma\u00b7ragd\u00b7ne", "Fr\u00fch\u00b7ling", ",", "der", "son\u00b7nen\u00b7ge\u00b7weck\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und es rauschen die wei\u00dfen Bl\u00fcthenb\u00e4ume,", "tokens": ["Und", "es", "rau\u00b7schen", "die", "wei\u00b7\u00dfen", "Bl\u00fc\u00b7then\u00b7b\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und die jungen Blumen schauen mich an,", "tokens": ["Und", "die", "jun\u00b7gen", "Blu\u00b7men", "schau\u00b7en", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Mit bunten, duftenden Augen,", "tokens": ["Mit", "bun\u00b7ten", ",", "duf\u00b7ten\u00b7den", "Au\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und es duftet und summt, und athmet und lacht,", "tokens": ["Und", "es", "duf\u00b7tet", "und", "summt", ",", "und", "ath\u00b7met", "und", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "$,", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Und im blauen Himmel singen die V\u00f6glein \u2014", "tokens": ["Und", "im", "blau\u00b7en", "Him\u00b7mel", "sin\u00b7gen", "die", "V\u00f6\u00b7glein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$("], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Thalatta! Thalatta!", "tokens": ["Tha\u00b7lat\u00b7ta", "!", "Tha\u00b7lat\u00b7ta", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "Du tapferes R\u00fcckzugherz!", "tokens": ["Du", "tap\u00b7fe\u00b7res", "R\u00fcck\u00b7zu\u00b7gherz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie oft, wie bitteroft", "tokens": ["Wie", "oft", ",", "wie", "bit\u00b7te\u00b7roft"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADV", "$,", "PWAV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bedr\u00e4ngten dich des Nordens Barbarinnen!", "tokens": ["Be\u00b7dr\u00e4ng\u00b7ten", "dich", "des", "Nor\u00b7dens", "Bar\u00b7ba\u00b7rin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus gro\u00dfen, siegenden Augen", "tokens": ["Aus", "gro\u00b7\u00dfen", ",", "sie\u00b7gen\u00b7den", "Au\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Schossen sie brennende Pfeile;", "tokens": ["Schos\u00b7sen", "sie", "bren\u00b7nen\u00b7de", "Pfei\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Mit krummgeschliffenen Worten", "tokens": ["Mit", "krumm\u00b7ge\u00b7schlif\u00b7fe\u00b7nen", "Wor\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Drohten sie mir die Brust zu spalten,", "tokens": ["Droh\u00b7ten", "sie", "mir", "die", "Brust", "zu", "spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Mit Keilschriftbillets zerschlugen sie mir", "tokens": ["Mit", "Keil\u00b7schrift\u00b7bil\u00b7lets", "zer\u00b7schlu\u00b7gen", "sie", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "PPER"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Das arme, bet\u00e4ubte Gehirn \u2014", "tokens": ["Das", "ar\u00b7me", ",", "be\u00b7t\u00e4ub\u00b7te", "Ge\u00b7hirn"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "Vergebens hielt ich den Schild entgegen,", "tokens": ["Ver\u00b7ge\u00b7bens", "hielt", "ich", "den", "Schild", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Die Pfeile zischten, die Hiebe krachten,", "tokens": ["Die", "Pfei\u00b7le", "zischten", ",", "die", "Hie\u00b7be", "krach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und von des Nordens Barbarinnen", "tokens": ["Und", "von", "des", "Nor\u00b7dens", "Bar\u00b7ba\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Ward ich gedr\u00e4ngt bis an's Meer,", "tokens": ["Ward", "ich", "ge\u00b7dr\u00e4ngt", "bis", "an's", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.14": {"text": "Und freiaufathmend begr\u00fc\u00df' ich das Meer,", "tokens": ["Und", "frei\u00b7au\u00b7fath\u00b7mend", "be\u00b7gr\u00fc\u00df'", "ich", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Das liebe, rettende Meer,", "tokens": ["Das", "lie\u00b7be", ",", "ret\u00b7ten\u00b7de", "Meer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.16": {"text": "Thalatta! Thalatta!", "tokens": ["Tha\u00b7lat\u00b7ta", "!", "Tha\u00b7lat\u00b7ta", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}