{"textgrid.poem.63896": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "An Theodor Fontane", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du, der \u00dcbersetzergilde Meister,", "tokens": ["O", "du", ",", "der", "\u00dc\u00b7bers\u00b7et\u00b7zer\u00b7gil\u00b7de", "Meis\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der schon so jung Don Juan nachgesungen,", "tokens": ["Der", "schon", "so", "jung", "Don", "Juan", "nach\u00b7ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "NN", "NE", "VVIZU", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Gereift mit Shakespeares hoher Kraft gerungen", "tokens": ["Ge\u00b7reift", "mit", "Sha\u00b7ke\u00b7spe\u00b7a\u00b7res", "ho\u00b7her", "Kraft", "ge\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NE", "ADJA", "NN", "VVPP"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und k\u00fchn besiegt Ariostos wilde Geister;", "tokens": ["Und", "k\u00fchn", "be\u00b7siegt", "A\u00b7rios\u00b7tos", "wil\u00b7de", "Geis\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zuletzt, durch dein Gelingen dreist und dreister,", "tokens": ["Zu\u00b7letzt", ",", "durch", "dein", "Ge\u00b7lin\u00b7gen", "dreist", "und", "dreis\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Dantes H\u00f6llenschlund hinabgedrungen", "tokens": ["In", "Dan\u00b7tes", "H\u00f6l\u00b7len\u00b7schlund", "hin\u00b7ab\u00b7ge\u00b7drun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und dann gel\u00e4utert dich emporgeschwungen", "tokens": ["Und", "dann", "ge\u00b7l\u00e4u\u00b7tert", "dich", "em\u00b7por\u00b7ge\u00b7schwun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVPP", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum Paradies \u2013 Gru\u00df dir, Freund Gildemeister!", "tokens": ["Zum", "Pa\u00b7ra\u00b7dies", "\u2013", "Gru\u00df", "dir", ",", "Freund", "Gil\u00b7de\u00b7meis\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$(", "NN", "PPER", "$,", "NN", "NE", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Zu selbstlos bargst du stets den eignen Wert,", "tokens": ["Zu", "selbst\u00b7los", "bargst", "du", "stets", "den", "eig\u00b7nen", "Wert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da doch an goldner Weisheit, Witz und Wissen", "tokens": ["Da", "doch", "an", "gold\u00b7ner", "Weis\u00b7heit", ",", "Witz", "und", "Wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So reiche Lebensfrucht dir ward beschert.", "tokens": ["So", "rei\u00b7che", "Le\u00b7bens\u00b7frucht", "dir", "ward", "be\u00b7schert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nun, da der Herbst erschien, la\u00df uns nicht missen", "tokens": ["Nun", ",", "da", "der", "Herbst", "er\u00b7schien", ",", "la\u00df", "uns", "nicht", "mis\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "VVFIN", "$,", "VVIMP", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Segen deines Werks, und hundertfarben", "tokens": ["Den", "Se\u00b7gen", "dei\u00b7nes", "Werks", ",", "und", "hun\u00b7dert\u00b7far\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "KON", "CARD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum Erntefeste sammle deine Garben!", "tokens": ["Zum", "Ern\u00b7te\u00b7fes\u00b7te", "samm\u00b7le", "dei\u00b7ne", "Gar\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}