{"textgrid.poem.26384": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[nie f\u00fcrchte vor der Heimat dich]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nie f\u00fcrchte vor der Heimat dich,", "tokens": ["Nie", "f\u00fcrch\u00b7te", "vor", "der", "Hei\u00b7mat", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Fremde, die zehrt f\u00fcrchterlich,", "tokens": ["Die", "Frem\u00b7de", ",", "die", "zehrt", "f\u00fcrch\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nur Heimat nimmt dich in den Arm", "tokens": ["Nur", "Hei\u00b7mat", "nimmt", "dich", "in", "den", "Arm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ist wie Muttermilch so warm.", "tokens": ["Und", "ist", "wie", "Mut\u00b7ter\u00b7milch", "so", "warm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "KOKOM", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und Heimat ist wie Honigwaben,", "tokens": ["Und", "Hei\u00b7mat", "ist", "wie", "Ho\u00b7nig\u00b7wa\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Herzen meist was S\u00fc\u00dfes haben.", "tokens": ["Wo", "Her\u00b7zen", "meist", "was", "S\u00fc\u00b7\u00dfes", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PIS", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dort sind die langweiligsten Tage,", "tokens": ["Dort", "sind", "die", "lang\u00b7wei\u00b7ligs\u00b7ten", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erf\u00fcllt von deiner Kindheit Sage,", "tokens": ["Er\u00b7f\u00fcllt", "von", "dei\u00b7ner", "Kind\u00b7heit", "Sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dort tun die Stunden schal und hohl", "tokens": ["Dort", "tun", "die", "Stun\u00b7den", "schal", "und", "hohl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "KON", "XY"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch ihr Gedankenloses wohl.", "tokens": ["Durch", "ihr", "Ge\u00b7dan\u00b7ken\u00b7lo\u00b7ses", "wohl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Erkennst du sie als Himmelreich,", "tokens": ["Er\u00b7kennst", "du", "sie", "als", "Him\u00b7mel\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "KOUS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann bist du erst den G\u00f6ttern gleich.", "tokens": ["Dann", "bist", "du", "erst", "den", "G\u00f6t\u00b7tern", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Gut ist dort jeder Pflasterstein,", "tokens": ["Gut", "ist", "dort", "je\u00b7der", "Pflas\u00b7ter\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kennt deine Stiefel, als sie klein,", "tokens": ["Kennt", "dei\u00b7ne", "Stie\u00b7fel", ",", "als", "sie", "klein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und von den Sperlingskindern da", "tokens": ["Und", "von", "den", "Sper\u00b7lings\u00b7kin\u00b7dern", "da"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Pfiff auf dich die Urgro\u00dfmama.", "tokens": ["Pfiff", "auf", "dich", "die", "Ur\u00b7gro\u00df\u00b7ma\u00b7ma", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Jeder nicht gleiche Heimat hat,", "tokens": ["Je\u00b7der", "nicht", "glei\u00b7che", "Hei\u00b7mat", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKNEG", "ADJA", "NN", "VAFIN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Doch irgendwo findet sie statt,", "tokens": ["Doch", "ir\u00b7gend\u00b7wo", "fin\u00b7det", "sie", "statt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.10": {"line.1": {"text": "Und auch im kleinsten B\u00fcrgernest", "tokens": ["Und", "auch", "im", "kleins\u00b7ten", "B\u00fcr\u00b7ger\u00b7nest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Lieb' du auf dich pfeifen l\u00e4\u00dft.", "tokens": ["Aus", "Lieb'", "du", "auf", "dich", "pfei\u00b7fen", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "APPR", "PPER", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "In meiner Stadt regiert der Wein,", "tokens": ["In", "mei\u00b7ner", "Stadt", "re\u00b7giert", "der", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach Wein riecht jeder Pflasterstein,", "tokens": ["Nach", "Wein", "riecht", "je\u00b7der", "Pflas\u00b7ter\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Keller sind dort wie Katakomben,", "tokens": ["Kel\u00b7ler", "sind", "dort", "wie", "Ka\u00b7ta\u00b7kom\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "KOKOM", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Drin summen F\u00e4sser wie die Bomben.", "tokens": ["Drin", "sum\u00b7men", "F\u00e4s\u00b7ser", "wie", "die", "Bom\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Wenn man im Keller selig ist,", "tokens": ["Wenn", "man", "im", "Kel\u00b7ler", "se\u00b7lig", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPRART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Leib man wie im Grab vergi\u00dft,", "tokens": ["Den", "Leib", "man", "wie", "im", "Grab", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "KOKOM", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Der Kater rei\u00dft dich leicht nach oben,", "tokens": ["Der", "Ka\u00b7ter", "rei\u00dft", "dich", "leicht", "nach", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Kirchendache hocherhoben,", "tokens": ["Zum", "Kir\u00b7chen\u00b7da\u00b7che", "ho\u00b7cher\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Und meine kleine Vaterstadt,", "tokens": ["Und", "mei\u00b7ne", "klei\u00b7ne", "Va\u00b7ter\u00b7stadt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Unz\u00e4hlig viele Kirchen hat.", "tokens": ["Un\u00b7z\u00e4h\u00b7lig", "vie\u00b7le", "Kir\u00b7chen", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Und in den Kirchen ist es sch\u00f6n,", "tokens": ["Und", "in", "den", "Kir\u00b7chen", "ist", "es", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo schw\u00e4rmerisch Madonnen stehn,", "tokens": ["Wo", "schw\u00e4r\u00b7me\u00b7risch", "Ma\u00b7don\u00b7nen", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Doch in den Kirchen ist's auch kalt,", "tokens": ["Doch", "in", "den", "Kir\u00b7chen", "ist's", "auch", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und man verl\u00e4\u00dft sie wieder bald.", "tokens": ["Und", "man", "ver\u00b7l\u00e4\u00dft", "sie", "wie\u00b7der", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "In warmen Stra\u00dfen brennt die Sonnen,", "tokens": ["In", "war\u00b7men", "Stra\u00b7\u00dfen", "brennt", "die", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch in der Sonn' wandeln Madonnen.", "tokens": ["Auch", "in", "der", "Sonn'", "wan\u00b7deln", "Ma\u00b7don\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}}, "stanza.19": {"line.1": {"text": "Ich kam an zum Frohnleichnamstag,", "tokens": ["Ich", "kam", "an", "zum", "Frohn\u00b7leich\u00b7nams\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wo alles auf den Stra\u00dfen lag,", "tokens": ["Wo", "al\u00b7les", "auf", "den", "Stra\u00b7\u00dfen", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.20": {"line.1": {"text": "Aus Teppichen baut man Alt\u00e4re,", "tokens": ["Aus", "Tep\u00b7pi\u00b7chen", "baut", "man", "Al\u00b7t\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Weil Gott mal gern im Freien w\u00e4re,", "tokens": ["Weil", "Gott", "mal", "gern", "im", "Frei\u00b7en", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ADV", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.21": {"line.1": {"text": "Sah wei\u00dfgekleidet M\u00e4dchen ziehn,", "tokens": ["Sah", "wei\u00df\u00b7ge\u00b7klei\u00b7det", "M\u00e4d\u00b7chen", "ziehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schleppten auf Bahren Goldmarien.", "tokens": ["Schlepp\u00b7ten", "auf", "Bah\u00b7ren", "Gold\u00b7ma\u00b7ri\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "NE", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.22": {"line.1": {"text": "Ach, dachte ich, in meiner Stadt", "tokens": ["Ach", ",", "dach\u00b7te", "ich", ",", "in", "mei\u00b7ner", "Stadt"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Kirche viele Frauen hat,", "tokens": ["Die", "Kir\u00b7che", "vie\u00b7le", "Frau\u00b7en", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.23": {"line.1": {"text": "Darum ist es wohl einerlei", "tokens": ["Da\u00b7rum", "ist", "es", "wohl", "ei\u00b7ner\u00b7lei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "PIAT"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Nimmt sich ein Mensch auf Erden zwei.", "tokens": ["Nimmt", "sich", "ein", "Mensch", "auf", "Er\u00b7den", "zwei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "NN", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.24": {"line.1": {"text": "In einem Kirchenkeller steht", "tokens": ["In", "ei\u00b7nem", "Kir\u00b7chen\u00b7kel\u00b7ler", "steht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Br\u00fcnnlein, wo man gern hingeht,", "tokens": ["Ein", "Br\u00fcnn\u00b7lein", ",", "wo", "man", "gern", "hin\u00b7geht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.25": {"line.1": {"text": "Denn wer von seinem Wasser trinkt,", "tokens": ["Denn", "wer", "von", "sei\u00b7nem", "Was\u00b7ser", "trinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es dann gar leicht zu Kindern bringt.", "tokens": ["Es", "dann", "gar", "leicht", "zu", "Kin\u00b7dern", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.26": {"line.1": {"text": "Frauen dr\u00e4ngten zum Kellerloch,", "tokens": ["Frau\u00b7en", "dr\u00e4ng\u00b7ten", "zum", "Kel\u00b7ler\u00b7loch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Denn Kinder w\u00fcnscht sich manche noch,", "tokens": ["Denn", "Kin\u00b7der", "w\u00fcnscht", "sich", "man\u00b7che", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PRF", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.27": {"line.1": {"text": "Und da ich keine Kinder kriege,", "tokens": ["Und", "da", "ich", "kei\u00b7ne", "Kin\u00b7der", "krie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Trank ich und w\u00fcnscht eins in die Wiege.", "tokens": ["Trank", "ich", "und", "w\u00fcnscht", "eins", "in", "die", "Wie\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.28": {"line.1": {"text": "Wie ich vom Becher ernst aufsah,", "tokens": ["Wie", "ich", "vom", "Be\u00b7cher", "ernst", "auf\u00b7sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stand schon das Kind leibhaftig da.", "tokens": ["Stand", "schon", "das", "Kind", "leib\u00b7haf\u00b7tig", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.29": {"line.1": {"text": "Das Mohrle stand am Brunnentrog,", "tokens": ["Das", "Mohr\u00b7le", "stand", "am", "Brun\u00b7nen\u00b7trog", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "And\u00e4chtig es am Eimer zog.", "tokens": ["An\u00b7d\u00e4ch\u00b7tig", "es", "am", "Ei\u00b7mer", "zog", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.30": {"line.1": {"text": "F\u00fcr Augen soll's ja auch gut sein,", "tokens": ["F\u00fcr", "Au\u00b7gen", "soll's", "ja", "auch", "gut", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "ADV", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Wasser, nicht f\u00fcr's Haus allein.", "tokens": ["Das", "Was\u00b7ser", ",", "nicht", "f\u00fcr's", "Haus", "al\u00b7lein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKNEG", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.31": {"line.1": {"text": "Das Mohrle stand mir gegen\u00fcber,", "tokens": ["Das", "Mohr\u00b7le", "stand", "mir", "ge\u00b7gen\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und meine Augen schielten \u00fcber.", "tokens": ["Und", "mei\u00b7ne", "Au\u00b7gen", "schiel\u00b7ten", "\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.32": {"line.1": {"text": "Ich tauchte in die Menschenmenge", "tokens": ["Ich", "tauch\u00b7te", "in", "die", "Men\u00b7schen\u00b7men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und zog mein Schicksal in die L\u00e4nge,", "tokens": ["Und", "zog", "mein", "Schick\u00b7sal", "in", "die", "L\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.33": {"line.1": {"text": "Ich floh und machte nirgends halt,", "tokens": ["Ich", "floh", "und", "mach\u00b7te", "nir\u00b7gends", "halt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am liebsten lief' ich, bis ich alt.", "tokens": ["Am", "liebs\u00b7ten", "lie\u00b7f'", "ich", ",", "bis", "ich", "alt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.34": {"line.1": {"text": "Und hinter mir rannten die Glocken", "tokens": ["Und", "hin\u00b7ter", "mir", "rann\u00b7ten", "die", "Glo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und wollten mich zur Jungfrau locken.", "tokens": ["Und", "woll\u00b7ten", "mich", "zur", "Jung\u00b7frau", "lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.35": {"line.1": {"text": "Die Landschaft roch nach Rosensalben,", "tokens": ["Die", "Land\u00b7schaft", "roch", "nach", "Ro\u00b7sen\u00b7sal\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sonne, selber tat sie kalben,", "tokens": ["Die", "Son\u00b7ne", ",", "sel\u00b7ber", "tat", "sie", "kal\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.36": {"line.1": {"text": "Es lag in jeder Fensterscheib'", "tokens": ["Es", "lag", "in", "je\u00b7der", "Fens\u00b7ter\u00b7scheib'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gl\u00e4nzend ein runder Sonnenleib,", "tokens": ["Gl\u00e4n\u00b7zend", "ein", "run\u00b7der", "Son\u00b7nen\u00b7leib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.37": {"line.1": {"text": "Und Sprungfedern auf allen Wegen,", "tokens": ["Und", "Sprung\u00b7fe\u00b7dern", "auf", "al\u00b7len", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und \u00fcberall sprang was entgegen.", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", "sprang", "was", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.38": {"line.1": {"text": "In B\u00fcschen, die vor Glut ganz mager,", "tokens": ["In", "B\u00fc\u00b7schen", ",", "die", "vor", "Glut", "ganz", "ma\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "APPR", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hielten die Schnecken Liebeslager,", "tokens": ["Hiel\u00b7ten", "die", "Schne\u00b7cken", "Lie\u00b7bes\u00b7la\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.39": {"line.1": {"text": "Der letzte S\u00fcnder war sich gut", "tokens": ["Der", "letz\u00b7te", "S\u00fcn\u00b7der", "war", "sich", "gut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PRF", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und f\u00fchlte Heiligkeit im Blut.", "tokens": ["Und", "f\u00fchl\u00b7te", "Hei\u00b7lig\u00b7keit", "im", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.40": {"line.1": {"text": "Lieb' kam selbst zum Holzapfelbaum,", "tokens": ["Lieb'", "kam", "selbst", "zum", "Holz\u00b7ap\u00b7fel\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Er sah die Welt vers\u00fc\u00dfter kaum.", "tokens": ["Er", "sah", "die", "Welt", "ver\u00b7s\u00fc\u00df\u00b7ter", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.41": {"line.1": {"text": "Nur ich wu\u00dfte nicht, was ich will,", "tokens": ["Nur", "ich", "wu\u00df\u00b7te", "nicht", ",", "was", "ich", "will", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VMFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Ach st\u00fcnd' mein Schalkherze doch still!", "tokens": ["Ach", "st\u00fcnd'", "mein", "Schal\u00b7kher\u00b7ze", "doch", "still", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPOSAT", "NN", "ADV", "PTKVZ", "$."], "meter": "++-++--+", "measure": "iambic.penta.chol"}}, "stanza.42": {"line.1": {"text": "Beruhigend w\u00e4r es zu sehn,", "tokens": ["Be\u00b7ru\u00b7hi\u00b7gend", "w\u00e4r", "es", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "T\u00e4t im Weinberg ein Haus mir stehn,", "tokens": ["T\u00e4t", "im", "Wein\u00b7berg", "ein", "Haus", "mir", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.43": {"line.1": {"text": "Und K\u00f6nigin schaute he