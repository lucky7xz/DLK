{"textgrid.poem.55850": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Die Aschanti", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Keine Vision von fremden L\u00e4ndern,", "tokens": ["Kei\u00b7ne", "Vi\u00b7si\u00b7on", "von", "frem\u00b7den", "L\u00e4n\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "kein Gef\u00fchl von braunen Frauen, die", "tokens": ["kein", "Ge\u00b7f\u00fchl", "von", "brau\u00b7nen", "Frau\u00b7en", ",", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PIAT", "NN", "APPR", "ADJA", "NN", "$,", "PRELS"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "tanzen aus den fallenden Gew\u00e4ndern.", "tokens": ["tan\u00b7zen", "aus", "den", "fal\u00b7len\u00b7den", "Ge\u00b7w\u00e4n\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Keine wilde fremde Melodie.", "tokens": ["Kei\u00b7ne", "wil\u00b7de", "frem\u00b7de", "Me\u00b7lo\u00b7die", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Keine Lieder, die vom Blute stammten,", "tokens": ["Kei\u00b7ne", "Lie\u00b7der", ",", "die", "vom", "Blu\u00b7te", "stamm\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "und kein Blut, das aus den Tiefen schrie.", "tokens": ["und", "kein", "Blut", ",", "das", "aus", "den", "Tie\u00b7fen", "schrie", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Keine braunen M\u00e4dchen, die sich samten", "tokens": ["Kei\u00b7ne", "brau\u00b7nen", "M\u00e4d\u00b7chen", ",", "die", "sich", "sam\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$,", "PRELS", "PRF", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "breiteten in Tropenm\u00fcdigkeit;", "tokens": ["brei\u00b7te\u00b7ten", "in", "Tro\u00b7pen\u00b7m\u00fc\u00b7dig\u00b7keit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "keine Augen, die wie Waffen flammten,", "tokens": ["kei\u00b7ne", "Au\u00b7gen", ",", "die", "wie", "Waf\u00b7fen", "flamm\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "KOKOM", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "und die Munde zum Gel\u00e4chter breit.", "tokens": ["und", "die", "Mun\u00b7de", "zum", "Ge\u00b7l\u00e4ch\u00b7ter", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und ein wunderliches Sich-verstehen", "tokens": ["Und", "ein", "wun\u00b7der\u00b7li\u00b7ches", "Sich\u00b7ver\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "mit der hellen Menschen Eitelkeit.", "tokens": ["mit", "der", "hel\u00b7len", "Men\u00b7schen", "Ei\u00b7tel\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Und mir war so bange hinzusehen.", "tokens": ["Und", "mir", "war", "so", "ban\u00b7ge", "hin\u00b7zu\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "O wie sind die Tiere so viel treuer,", "tokens": ["O", "wie", "sind", "die", "Tie\u00b7re", "so", "viel", "treu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VAFIN", "ART", "NN", "ADV", "PIAT", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "die in Gittern auf und niedergehn,", "tokens": ["die", "in", "Git\u00b7tern", "auf", "und", "nie\u00b7der\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PTKVZ", "KON", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "ohne Eintracht mit dem Treiben neuer", "tokens": ["oh\u00b7ne", "Ein\u00b7tracht", "mit", "dem", "Trei\u00b7ben", "neu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "fremder Dinge, die sie nicht verstehn;", "tokens": ["frem\u00b7der", "Din\u00b7ge", ",", "die", "sie", "nicht", "ver\u00b7stehn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "und sie brennen wie ein stilles Feuer", "tokens": ["und", "sie", "bren\u00b7nen", "wie", "ein", "stil\u00b7les", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVINF", "KOKOM", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "leise aus und sinken in sich ein,", "tokens": ["lei\u00b7se", "aus", "und", "sin\u00b7ken", "in", "sich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "VVFIN", "APPR", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "teilnahmslos dem neuen Abenteuer", "tokens": ["teil\u00b7nahms\u00b7los", "dem", "neu\u00b7en", "A\u00b7bent\u00b7eu\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "und mit ihrem gro\u00dfen Blut allein.", "tokens": ["und", "mit", "ih\u00b7rem", "gro\u00b7\u00dfen", "Blut", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}