{"textgrid.poem.57881": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Immer langsam voran", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Winter ist es jetzt geworden,", "tokens": ["Win\u00b7ter", "ist", "es", "jetzt", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Putz' die Schlittschuh', junger Mann!", "tokens": ["Putz'", "die", "Schlitt\u00b7schuh'", ",", "jun\u00b7ger", "Mann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df es lieber, bei Hannover", "tokens": ["La\u00df", "es", "lie\u00b7ber", ",", "bei", "Han\u00b7no\u00b7ver"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "$,", "APPR", "NE"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Man ja doch nicht laufen kann.", "tokens": ["Man", "ja", "doch", "nicht", "lau\u00b7fen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eine Eisbahn hat man freilich", "tokens": ["Ei\u00b7ne", "Eis\u00b7bahn", "hat", "man", "frei\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PIS", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Masch, jedoch man wei\u00df,", "tokens": ["In", "der", "Masch", ",", "je\u00b7doch", "man", "wei\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn es friert, dann ist sie trocken,", "tokens": ["Wenn", "es", "friert", ",", "dann", "ist", "sie", "tro\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist sie na\u00df, dann friert's kein Eis.", "tokens": ["Ist", "sie", "na\u00df", ",", "dann", "friert's", "kein", "Eis", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jetzt, wo's friert, da m\u00fc\u00dft' man sie doch", "tokens": ["Jetzt", ",", "wo's", "friert", ",", "da", "m\u00fc\u00dft'", "man", "sie", "doch"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "VVFIN", "$,", "KOUS", "VMFIN", "PIS", "PPER", "ADV"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "\u00dcberschwemmen, damit dort", "tokens": ["\u00dc\u00b7bersc\u00b7hwem\u00b7men", ",", "da\u00b7mit", "dort"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "KOUS", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf der breiten, weiten Fl\u00e4che", "tokens": ["Auf", "der", "brei\u00b7ten", ",", "wei\u00b7ten", "Fl\u00e4\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bl\u00fchen k\u00f6nnt' der Schlittschuhsport.", "tokens": ["Bl\u00fc\u00b7hen", "k\u00f6nnt'", "der", "Schlitt\u00b7schuh\u00b7sport", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Beh\u00f6rde aber handelt", "tokens": ["Die", "Be\u00b7h\u00f6r\u00b7de", "a\u00b7ber", "han\u00b7delt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruhig stets, nie kindisch rasch:", "tokens": ["Ru\u00b7hig", "stets", ",", "nie", "kin\u00b7disch", "rasch", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "ADV", "ADJD", "ADJD", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Wenn die Schwalben wiederkommen,", "tokens": ["Wenn", "die", "Schwal\u00b7ben", "wie\u00b7der\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcberschwemmt man auch die Masch.", "tokens": ["\u00dc\u00b7bersc\u00b7hwemmt", "man", "auch", "die", "Masch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}