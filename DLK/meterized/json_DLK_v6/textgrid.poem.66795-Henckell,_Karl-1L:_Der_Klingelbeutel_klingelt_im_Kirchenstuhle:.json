{"textgrid.poem.66795": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Klingelbeutel klingelt im Kirchenstuhle:", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Klingelbeutel klingelt im Kirchenstuhle:", "tokens": ["Der", "Klin\u00b7gel\u00b7beu\u00b7tel", "klin\u00b7gelt", "im", "Kir\u00b7chen\u00b7stuh\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Almosen den Hungerleidern in unsrer Schule!", "tokens": ["Al\u00b7mo\u00b7sen", "den", "Hun\u00b7ger\u00b7lei\u00b7dern", "in", "uns\u00b7rer", "Schu\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-++-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Viel hundert Kinder hungern von Tag zu Tage,", "tokens": ["Viel", "hun\u00b7dert", "Kin\u00b7der", "hun\u00b7gern", "von", "Tag", "zu", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die \u00bbPresse\u00ab schreibt es, das Faktum ist au\u00dfer Frage;", "tokens": ["Die", "\u00bb", "Pres\u00b7se", "\u00ab", "schreibt", "es", ",", "das", "Fak\u00b7tum", "ist", "au\u00b7\u00dfer", "Fra\u00b7ge", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "NN", "$(", "VVFIN", "PPER", "$,", "ART", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Barmherzig war der biedere B\u00fcrger von je,", "tokens": ["Barm\u00b7her\u00b7zig", "war", "der", "bie\u00b7de\u00b7re", "B\u00fcr\u00b7ger", "von", "je", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "APPR", "ADV", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Mit eurem goldenen Herzen, o stillt das Weh!", "tokens": ["Mit", "eu\u00b7rem", "gol\u00b7de\u00b7nen", "Her\u00b7zen", ",", "o", "stillt", "das", "Weh", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "FM", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Den braven B\u00fcrger kitzelt's g\u00fctig und gruselt's,", "tokens": ["Den", "bra\u00b7ven", "B\u00fcr\u00b7ger", "kit\u00b7zelt's", "g\u00fc\u00b7tig", "und", "gru\u00b7selt's", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In seinem faulen Hirne d\u00e4mmert's und duselt's.", "tokens": ["In", "sei\u00b7nem", "fau\u00b7len", "Hir\u00b7ne", "d\u00e4m\u00b7mert's", "und", "du\u00b7selt'", "s."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NE", "KON", "XY", "XY"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Am Hungertuche \u2013 kaum glaublich scheint die Geschichte \u2013", "tokens": ["Am", "Hun\u00b7ger\u00b7tu\u00b7che", "\u2013", "kaum", "glaub\u00b7lich", "scheint", "die", "Ge\u00b7schich\u00b7te", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$(", "ADV", "ADJD", "VVFIN", "ART", "NN", "$("], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Vorl\u00e4ufig gebe man ihnen Erbsengerichte!", "tokens": ["Vor\u00b7l\u00e4u\u00b7fig", "ge\u00b7be", "man", "ih\u00b7nen", "Erb\u00b7sen\u00b7ge\u00b7rich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "PPER", "NN", "$."], "meter": "++--+-+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Drei Deziliter! H\u00fclsenfr\u00fcchte sind gut,", "tokens": ["Drei", "De\u00b7zi\u00b7li\u00b7ter", "!", "H\u00fcl\u00b7sen\u00b7fr\u00fcch\u00b7te", "sind", "gut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$.", "NN", "VAFIN", "ADJD", "$,"], "meter": "-----+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Fleischkost, ja, ja, verdickt und verdirbt das Blut.", "tokens": ["Fleischkost", ",", "ja", ",", "ja", ",", "ver\u00b7dickt", "und", "ver\u00b7dirbt", "das", "Blut", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKANT", "$,", "PTKANT", "$,", "VVPP", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Nun wird der Rahm der Humanit\u00e4t gebuttert,", "tokens": ["Nun", "wird", "der", "Rahm", "der", "Hu\u00b7ma\u00b7ni\u00b7t\u00e4t", "ge\u00b7but\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die armen Kinder privatwohlt\u00e4tig gefuttert:", "tokens": ["Die", "ar\u00b7men", "Kin\u00b7der", "pri\u00b7vat\u00b7wohl\u00b7t\u00e4\u00b7tig", "ge\u00b7fut\u00b7tert", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des echten Christen Wohltat mu\u00df sich verzinsen,", "tokens": ["Des", "ech\u00b7ten", "Chris\u00b7ten", "Wohl\u00b7tat", "mu\u00df", "sich", "ver\u00b7zin\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VMFIN", "PRF", "VVPP", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Drum opfern mild wir Erbsen, Bohnen und Linsen.", "tokens": ["Drum", "op\u00b7fern", "mild", "wir", "Erb\u00b7sen", ",", "Boh\u00b7nen", "und", "Lin\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADJD", "PPER", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Der Fabrikant bekreuzt sich und denkt: Parbleu!", "tokens": ["Der", "Fab\u00b7ri\u00b7kant", "be\u00b7kreuzt", "sich", "und", "denkt", ":", "Par\u00b7bleu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "KON", "VVFIN", "$.", "NE", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Helft, helft! Die industrielle Reservearmee!", "tokens": ["Helft", ",", "helft", "!", "Die", "in\u00b7dust\u00b7ri\u00b7el\u00b7le", "Re\u00b7ser\u00b7ve\u00b7ar\u00b7mee", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$.", "ART", "ADJA", "NN", "$."], "meter": "----+-+-+-+-+", "measure": "unknown.measure.penta"}}, "stanza.4": {"line.1": {"text": "Ich aber sage euch: Alles mu\u00df anders werden,", "tokens": ["Ich", "a\u00b7ber", "sa\u00b7ge", "euch", ":", "Al\u00b7les", "mu\u00df", "an\u00b7ders", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$.", "PIS", "VMFIN", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein gro\u00df Ger\u00e4usch wird fahren \u00fcber die Erden!", "tokens": ["Ein", "gro\u00df", "Ge\u00b7r\u00e4usch", "wird", "fah\u00b7ren", "\u00fc\u00b7ber", "die", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Aus allen Winkeln h\u00f6r' ich es heimlich brausen,", "tokens": ["Aus", "al\u00b7len", "Win\u00b7keln", "h\u00f6r'", "ich", "es", "heim\u00b7lich", "brau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Meine dunkle Seele durchzuckt ein leuchtend Grausen:", "tokens": ["Mei\u00b7ne", "dunk\u00b7le", "See\u00b7le", "durch\u00b7zuckt", "ein", "leuch\u00b7tend", "Grau\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Der Klingelbeutel emp\u00f6rter Natur geht um,", "tokens": ["Der", "Klin\u00b7gel\u00b7beu\u00b7tel", "em\u00b7p\u00f6r\u00b7ter", "Na\u00b7tur", "geht", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Ihren Kreuzer die Dirne opfert und weinet stumm.", "tokens": ["Ih\u00b7ren", "Kreu\u00b7zer", "die", "Dir\u00b7ne", "op\u00b7fert", "und", "wei\u00b7net", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN", "KON", "VVFIN", "ADJD", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}}}}}