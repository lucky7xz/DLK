{"dta.poem.7956": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An den Unsichtbaren .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Du, den wir suchen auf so finstern Wegen,", "tokens": ["Du", ",", "den", "wir", "su\u00b7chen", "auf", "so", "fins\u00b7tern", "We\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVFIN", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit forschenden Gedanken nicht erfassen,", "tokens": ["Mit", "for\u00b7schen\u00b7den", "Ge\u00b7dan\u00b7ken", "nicht", "er\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du hast dein heilig Dunkel einst verlassen", "tokens": ["Du", "hast", "dein", "hei\u00b7lig", "Dun\u00b7kel", "einst", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJD", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und tratest sichtbar deinem Volk entgegen.", "tokens": ["Und", "tra\u00b7test", "sicht\u00b7bar", "dei\u00b7nem", "Volk", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Welch s\u00fc\u00dfes Heil, dein Bild sich einzupr\u00e4gen,", "tokens": ["Welch", "s\u00fc\u00b7\u00dfes", "Heil", ",", "dein", "Bild", "sich", "ein\u00b7zu\u00b7pr\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Worte deines Mundes aufzufassen!", "tokens": ["Die", "Wor\u00b7te", "dei\u00b7nes", "Mun\u00b7des", "auf\u00b7zu\u00b7fas\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O selig, die an deinem Mahle sa\u00dfen!", "tokens": ["O", "se\u00b7lig", ",", "die", "an", "dei\u00b7nem", "Mah\u00b7le", "sa\u00b7\u00dfen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O selig, der an deiner Brust gelegen!", "tokens": ["O", "se\u00b7lig", ",", "der", "an", "dei\u00b7ner", "Brust", "ge\u00b7le\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Drum war es auch kein seltsames Gel\u00fcste,", "tokens": ["Drum", "war", "es", "auch", "kein", "selt\u00b7sa\u00b7mes", "Ge\u00b7l\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn Pilger ohne Zahl vom Strande stie\u00dfen,", "tokens": ["Wenn", "Pil\u00b7ger", "oh\u00b7ne", "Zahl", "vom", "Stran\u00b7de", "stie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn Heere k\u00e4mpften an der fernsten K\u00fcste:", "tokens": ["Wenn", "Hee\u00b7re", "k\u00e4mpf\u00b7ten", "an", "der", "ferns\u00b7ten", "K\u00fcs\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}