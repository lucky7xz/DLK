{"textgrid.poem.55007": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "An die Erw\u00e4hlte", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hand in Hand! und Lipp auf Lippe!", "tokens": ["Hand", "in", "Hand", "!", "und", "Lipp", "auf", "Lip\u00b7pe", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$.", "KON", "NE", "APPR", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebes M\u00e4dchen, bleibe treu!", "tokens": ["Lie\u00b7bes", "M\u00e4d\u00b7chen", ",", "blei\u00b7be", "treu", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lebe wohl! und manche Klippe", "tokens": ["Le\u00b7be", "wohl", "!", "und", "man\u00b7che", "Klip\u00b7pe"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$.", "KON", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00e4hrt dein Liebster noch vorbei;", "tokens": ["F\u00e4hrt", "dein", "Liebs\u00b7ter", "noch", "vor\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber wenn er einst den Hafen,", "tokens": ["A\u00b7ber", "wenn", "er", "einst", "den", "Ha\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach dem Sturme, wieder gr\u00fc\u00dft,", "tokens": ["Nach", "dem", "Stur\u00b7me", ",", "wie\u00b7der", "gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00f6gen ihn die G\u00f6tter strafen,", "tokens": ["M\u00f6\u00b7gen", "ihn", "die", "G\u00f6t\u00b7ter", "stra\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn er ohne dich genie\u00dft.", "tokens": ["Wenn", "er", "oh\u00b7ne", "dich", "ge\u00b7nie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Frisch gewagt ist schon gewonnen,", "tokens": ["Frisch", "ge\u00b7wagt", "ist", "schon", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Halb ist schon mein Werk vollbracht!", "tokens": ["Halb", "ist", "schon", "mein", "Werk", "voll\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sterne leuchten mir wie Sonnen,", "tokens": ["Ster\u00b7ne", "leuch\u00b7ten", "mir", "wie", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur dem Feigen ist es Nacht.", "tokens": ["Nur", "dem", "Fei\u00b7gen", "ist", "es", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "W\u00e4r ich m\u00fc\u00dfig dir zur Seite,", "tokens": ["W\u00e4r", "ich", "m\u00fc\u00b7\u00dfig", "dir", "zur", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VMFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dr\u00fcckte noch der Kummer mich;", "tokens": ["Dr\u00fcck\u00b7te", "noch", "der", "Kum\u00b7mer", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch in aller dieser Weite", "tokens": ["Doch", "in", "al\u00b7ler", "die\u00b7ser", "Wei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wirk ich rasch und nur f\u00fcr dich.", "tokens": ["Wirk", "ich", "rasch", "und", "nur", "f\u00fcr", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "KON", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schon ist mir das Tal gefunden,", "tokens": ["Schon", "ist", "mir", "das", "Tal", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo wir einst zusammen gehn", "tokens": ["Wo", "wir", "einst", "zu\u00b7sam\u00b7men", "gehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den Strom in Abendstunden", "tokens": ["Und", "den", "Strom", "in", "A\u00b7bends\u00b7tun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sanft hinuntergleiten sehn.", "tokens": ["Sanft", "hin\u00b7un\u00b7ter\u00b7glei\u00b7ten", "sehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Diese Pappeln auf den Wiesen,", "tokens": ["Die\u00b7se", "Pap\u00b7peln", "auf", "den", "Wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Diese Buchen in dem Hain!", "tokens": ["Die\u00b7se", "Bu\u00b7chen", "in", "dem", "Hain", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach, und hinter allen diesen", "tokens": ["Ach", ",", "und", "hin\u00b7ter", "al\u00b7len", "die\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KON", "APPR", "PIAT", "PDAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wird doch auch ein H\u00fcttchen sein.", "tokens": ["Wird", "doch", "auch", "ein", "H\u00fctt\u00b7chen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}