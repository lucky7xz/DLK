{"textgrid.poem.63461": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "Don un laten", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oll Paster Reuter g\u00fcng spazieren,", "tokens": ["Oll", "Pas\u00b7ter", "Reu\u00b7ter", "g\u00fcng", "spa\u00b7zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NE", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "dat W\u00e4der dat was gor to sch\u00f6n \u2013", "tokens": ["dat", "W\u00e4\u00b7der", "dat", "was", "gor", "to", "sch\u00f6n", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "FM", "FM", "FM", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "dunn kem dor sacht wat antof\u00fchren,", "tokens": ["dunn", "kem", "dor", "sacht", "wat", "an\u00b7to\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "un dat was Buer Stoffer D\u00f6n.", "tokens": ["un", "dat", "was", "Bu\u00b7er", "Stof\u00b7fer", "D\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "NN", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "De was all wedder eens to Stadt west", "tokens": ["De", "was", "all", "wed\u00b7der", "e\u00b7ens", "to", "Stadt", "west"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PRELS", "PIAT", "KON", "FM", "FM", "NN", "VVFIN"], "meter": "-----+-+-+", "measure": "unknown.measure.tri"}, "line.6": {"text": "un kem tor\u00fcgg nu dick un dun,", "tokens": ["un", "kem", "to\u00b7r\u00fcgg", "nu", "dick", "un", "dun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "de K\u0153m was wedder em to glatt west,", "tokens": ["de", "K\u0153m", "was", "wed\u00b7der", "em", "to", "glatt", "west", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "sin Snut all wedder blag un brun.", "tokens": ["sin", "Snut", "all", "wed\u00b7der", "blag", "un", "brun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "As he dunn rankem, s\u00e4d oll Reuter:", "tokens": ["As", "he", "dunn", "ran\u00b7kem", ",", "s\u00e4d", "oll", "Reu\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "VVFIN", "ADJD", "NE", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "\u00bbkriggt He denn, Stoffer, nie eens nog!", "tokens": ["\u00bb", "kriggt", "He", "denn", ",", "Stof\u00b7fer", ",", "nie", "e\u00b7ens", "nog", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "NE", "KON", "$,", "NN", "$,", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "De Pr\u00e4digt jidwermal \u00fcmgeiht He,", "tokens": ["De", "Pr\u00e4\u00b7digt", "jid\u00b7wer\u00b7mal", "\u00fcm\u00b7geiht", "He", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "VVFIN", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "man nie un keenen Dag den Krog!", "tokens": ["man", "nie", "un", "ke\u00b7e\u00b7nen", "Dag", "den", "Krog", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "FM", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Versteiht He sick denn nich to faten,", "tokens": ["Ver\u00b7steiht", "He", "sick", "denn", "nich", "to", "fa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "KON", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "verspr\u00f6k He mi nich fast un stif:", "tokens": ["ver\u00b7spr\u00f6k", "He", "mi", "nich", "fast", "un", "stif", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "PTKNEG", "ADV", "FM", "FM", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Wenn He dat ded, He k\u00fcnn't ok laten \u2013", "tokens": ["Wenn", "He", "dat", "ded", ",", "He", "k\u00fcnn't", "ok", "la\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ART", "NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "un wedder hett He vull dat Lif?!\u00ab", "tokens": ["un", "wed\u00b7der", "hett", "He", "vull", "dat", "Lif", "?!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM", "KON", "VAFIN", "NE", "NE", "ART", "NN", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "\u00bbprr!\u00ab s\u00e4d dunn D\u00f6n. De Pir de st\u00fcnnen,", "tokens": ["\u00bb", "prr", "!", "\u00ab", "s\u00e4d", "dunn", "D\u00f6n", ".", "De", "Pir", "de", "st\u00fcn\u00b7nen", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$.", "$(", "FM.la", "FM.la", "FM.la", "$.", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "de Wag de h\u00f6ll, un Stoffer sch\u00f6w", "tokens": ["de", "Wag", "de", "h\u00f6ll", ",", "un", "Stof\u00b7fer", "sch\u00f6w"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["FM.da", "FM.da", "FM.da", "FM.da", "$,", "FM", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "sin'n Hot sick vun sin Snut nah hinnen", "tokens": ["sin'n", "Hot", "sick", "vun", "sin", "Snut", "nah", "hin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.nl", "FM.nl", "FM.nl", "FM.nl", "FM.nl", "FM.nl", "ADJD", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "un richt't sick up un harkt un sn\u00f6w", "tokens": ["un", "richt't", "sick", "up", "un", "harkt", "un", "sn\u00f6w"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "un hickupt irst sin G\u00f6rgel klor sick,", "tokens": ["un", "hic\u00b7kupt", "irst", "sin", "G\u00f6r\u00b7gel", "klor", "sick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "as ob de Frag he woll verst\u00fcnn,", "tokens": ["as", "ob", "de", "Frag", "he", "woll", "ver\u00b7st\u00fcnn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "NE", "NE", "NE", "ADV", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "man up de Antwurt nich so drad sick", "tokens": ["man", "up", "de", "Ant\u00b7wurt", "nich", "so", "drad", "sick"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "NE", "NE", "NE", "PTKNEG", "ADV", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "un uppe St\u00e4d besinnen k\u00fcnn.", "tokens": ["un", "up\u00b7pe", "St\u00e4d", "be\u00b7sin\u00b7nen", "k\u00fcnn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbje, Herr Propo\u00df, je\u00ab, s\u00e4d dunn Stoffer,", "tokens": ["\u00bb", "je", ",", "Herr", "Pro\u00b7po\u00df", ",", "je", "\u00ab", ",", "s\u00e4d", "dunn", "Stof\u00b7fer", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "NN", "NN", "$,", "ADV", "$(", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "\u00bbje, wat ick seggt heww, dat heww'ck seggt!", "tokens": ["\u00bb", "je", ",", "wat", "ick", "seggt", "heww", ",", "dat", "hew\u00b7w'ck", "seggt", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "PWS", "PPER", "VVFIN", "NE", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ick mag keen Warmbier un keen Koffee,", "tokens": ["Ick", "mag", "ke\u00b7en", "Warm\u00b7bier", "un", "ke\u00b7en", "Kof\u00b7fee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "FM", "VVFIN", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "un Melkgr\u00fctt is mi gor nich recht;", "tokens": ["un", "Melk\u00b7gr\u00fctt", "is", "mi", "gor", "nich", "recht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "de Brannwin is'n grotes Oewel,", "tokens": ["de", "Brann\u00b7win", "is'n", "gro\u00b7tes", "O\u00b7e\u00b7wel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "ADJA", "NN", "$,"], "meter": "-++-+-+-+", "measure": "unknown.measure.penta"}, "line.6": {"text": "dat's wohr un dat is gor keen Frag \u2013", "tokens": ["dat's", "wohr", "un", "dat", "is", "gor", "ke\u00b7en", "Frag", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "man Water mag'ck nich in min St\u00e4wel,", "tokens": ["man", "Wa\u00b7ter", "mag'ck", "nich", "in", "min", "St\u00e4\u00b7wel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "NE", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "un unnod n\u00e4hm ick't in de Mag.", "tokens": ["un", "un\u00b7nod", "n\u00e4hm", "ick't", "in", "de", "Mag."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Un dorbi bliw ick, wat ick seggt heww,", "tokens": ["Un", "dor\u00b7bi", "bliw", "ick", ",", "wat", "ick", "seggt", "heww", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "PWS", "PPER", "VVFIN", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "un to min W\u00fcr stah'ck as'n Mann:", "tokens": ["un", "to", "min", "W\u00fcr", "stah'ck", "as'n", "Mann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Don kann'ck, ick weet woll, wat ick recht heww,", "tokens": ["Don", "kann'ck", ",", "ick", "weet", "woll", ",", "wat", "ick", "recht", "heww", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "VVFIN", "ADV", "$,", "PWS", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "un Gott weet, wat ick't laten kann!", "tokens": ["un", "Gott", "weet", ",", "wat", "ick't", "la\u00b7ten", "kann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "NN", "VVFIN", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "He hett man nich de rechte Kenntnis,", "tokens": ["He", "hett", "man", "nich", "de", "rech\u00b7te", "Kennt\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PIS", "PTKNEG", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Herr Paster, as ick nu woll seh \u2013", "tokens": ["Herr", "Pas\u00b7ter", ",", "as", "ick", "nu", "woll", "seh", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "dat is'n rennlich Unverst\u00e4ndnis,", "tokens": ["dat", "is'n", "renn\u00b7lich", "Un\u00b7ver\u00b7st\u00e4nd\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "un anners as ick meent dat He.", "tokens": ["un", "an\u00b7ners", "as", "ick", "meent", "dat", "He", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+---", "measure": "unknown.measure.tri"}}, "stanza.6": {"line.1": {"text": "Un denn so een f\u00f6r alle Malen:", "tokens": ["Un", "denn", "so", "e\u00b7en", "f\u00f6r", "al\u00b7le", "Ma\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADV", "ART", "NE", "PIAT", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ick s\u00e4d, Herr Paster, ick k\u00fcnn't don,", "tokens": ["Ick", "s\u00e4d", ",", "Herr", "Pas\u00b7ter", ",", "ick", "k\u00fcnn't", "don", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "$,", "NN", "NN", "$,", "PPER", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "dat heet so v\u00e4l, ick kann't betahlen,", "tokens": ["dat", "heet", "so", "v\u00e4l", ",", "ick", "kann't", "be\u00b7tah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "ADJD", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "un dornah kreigt nich Hahn nich Hohn.", "tokens": ["un", "dor\u00b7nah", "kreigt", "nich", "Hahn", "nich", "Hohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "ADV", "VVFIN", "PTKNEG", "NE", "PTKNEG", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Un wenn ick s\u00e4d, ick k\u00fcnn't ok laten,", "tokens": ["Un", "wenn", "ick", "s\u00e4d", ",", "ick", "k\u00fcnn't", "ok", "la\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "NE", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "na, Herr Propo\u00df, je, na, dat heet:", "tokens": ["na", ",", "Herr", "Pro\u00b7po\u00df", ",", "je", ",", "na", ",", "dat", "heet", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "NN", "$,", "ADV", "$,", "ITJ", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Twee Pott, denn b\u00fcn'ck so vull nich gaten,", "tokens": ["Twee", "Pott", ",", "denn", "b\u00fcn'ck", "so", "vull", "nich", "ga\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "KON", "VVIMP", "ADV", "ADJD", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "dat ick nich noch twee P\u00e4gel let!\u00ab", "tokens": ["dat", "ick", "nich", "noch", "twee", "P\u00e4\u00b7gel", "let", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}