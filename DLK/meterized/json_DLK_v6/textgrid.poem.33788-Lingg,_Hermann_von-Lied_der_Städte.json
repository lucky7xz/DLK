{"textgrid.poem.33788": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Lied der St\u00e4dte", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr B\u00fcrger, auf von nah und fern,", "tokens": ["Ihr", "B\u00fcr\u00b7ger", ",", "auf", "von", "nah", "und", "fern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "APPR", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schwingt gleich den M\u00e4nnern von Luzern", "tokens": ["Schwingt", "gleich", "den", "M\u00e4n\u00b7nern", "von", "Lu\u00b7zern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Den Morgenstern,", "tokens": ["Den", "Mor\u00b7gens\u00b7tern", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.4": {"text": "La\u00dft wallen die Paniere,", "tokens": ["La\u00dft", "wal\u00b7len", "die", "Pa\u00b7nie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "La\u00dft fallen die Visiere,", "tokens": ["La\u00dft", "fal\u00b7len", "die", "Vi\u00b7sie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.6": {"text": "Auf gegen die Herrn!", "tokens": ["Auf", "ge\u00b7gen", "die", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Zum Galgen und aufs Hochgericht,", "tokens": ["Zum", "Gal\u00b7gen", "und", "aufs", "Hoch\u00b7ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer unsers Kaisers Frieden bricht!", "tokens": ["Wer", "un\u00b7sers", "Kai\u00b7sers", "Frie\u00b7den", "bricht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir ruhen nicht,", "tokens": ["Wir", "ru\u00b7hen", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Als bis dem letzten Ritter", "tokens": ["Als", "bis", "dem", "letz\u00b7ten", "Rit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sein Wappenschild in Splitter,", "tokens": ["Sein", "Wap\u00b7pen\u00b7schild", "in", "Split\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bis jede Kette bricht.", "tokens": ["Bis", "je\u00b7de", "Ket\u00b7te", "bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Seht hin, wo jener Turm gebaut,", "tokens": ["Seht", "hin", ",", "wo", "je\u00b7ner", "Turm", "ge\u00b7baut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PWAV", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo jene finstre Mauer graut,", "tokens": ["Wo", "je\u00b7ne", "finst\u00b7re", "Mau\u00b7er", "graut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort klagte laut,", "tokens": ["Dort", "klag\u00b7te", "laut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Dort rang die wunden H\u00e4nde", "tokens": ["Dort", "rang", "die", "wun\u00b7den", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Um taube Kerkerw\u00e4nde", "tokens": ["Um", "tau\u00b7be", "Ker\u00b7ker\u00b7w\u00e4n\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des freien B\u00fcrgers Braut.", "tokens": ["Des", "frei\u00b7en", "B\u00fcr\u00b7gers", "Braut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dort lag dein Vater, lag dein Ahn,", "tokens": ["Dort", "lag", "dein", "Va\u00b7ter", ",", "lag", "dein", "Ahn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Hab und Gut ging jene Bahn,", "tokens": ["Dein", "Hab", "und", "Gut", "ging", "je\u00b7ne", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ADJD", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der rote Hahn", "tokens": ["Der", "ro\u00b7te", "Hahn"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Bedrohte selbst dein Erbe!", "tokens": ["Be\u00b7droh\u00b7te", "selbst", "dein", "Er\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sein ganzes Haus verderbe,", "tokens": ["Sein", "gan\u00b7zes", "Haus", "ver\u00b7der\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der uns dies angetan.", "tokens": ["Der", "uns", "dies", "an\u00b7ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PDS", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was Felseneck, was Hohenrain,", "tokens": ["Was", "Fel\u00b7sen\u00b7eck", ",", "was", "Ho\u00b7hen\u00b7rain", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "PRELS", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was Geierhorst und Drachenstein!", "tokens": ["Was", "Gei\u00b7er\u00b7horst", "und", "Dra\u00b7chen\u00b7stein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schlagt drein, schlagt drein!", "tokens": ["Schlagt", "drein", ",", "schlagt", "drein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Schlagt Zugbr\u00fcck ein und Pfosten,", "tokens": ["Schlagt", "Zug\u00b7br\u00fcck", "ein", "und", "Pfos\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKVZ", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Sporen m\u00fcssen rosten,", "tokens": ["Die", "Spo\u00b7ren", "m\u00fcs\u00b7sen", "ros\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und frei die St\u00e4dte sein.", "tokens": ["Und", "frei", "die", "St\u00e4d\u00b7te", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Zerst\u00f6rt das Raubnest bis zum Stumpf", "tokens": ["Zer\u00b7st\u00f6rt", "das", "Raub\u00b7nest", "bis", "zum", "Stumpf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und rufet eher nicht Triumph,", "tokens": ["Und", "ru\u00b7fet", "e\u00b7her", "nicht", "Tri\u00b7umph", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als bis vom Rumpf", "tokens": ["Als", "bis", "vom", "Rumpf"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Die R\u00e4ubersch\u00e4del fallen,", "tokens": ["Die", "R\u00e4u\u00b7ber\u00b7sch\u00e4\u00b7del", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bis in den \u00f6den Hallen", "tokens": ["Bis", "in", "den", "\u00f6\u00b7den", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von ihrem Blut ein Sumpf.", "tokens": ["Von", "ih\u00b7rem", "Blut", "ein", "Sumpf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}