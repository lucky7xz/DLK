{"textgrid.poem.45975": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[draus bei Schleswig vor der Pforte]", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Draus bei Schleswig vor der Pforte", "tokens": ["Draus", "bei", "Schles\u00b7wig", "vor", "der", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "NE", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohnen armer Leute viel,", "tokens": ["Woh\u00b7nen", "ar\u00b7mer", "Leu\u00b7te", "viel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach des Feindes wilder Horde", "tokens": ["Ach", "des", "Fein\u00b7des", "wil\u00b7der", "Hor\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werden sie das erste Ziel.", "tokens": ["Wer\u00b7den", "sie", "das", "ers\u00b7te", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Waffenstillstand ist gek\u00fcndet", "tokens": ["Waf\u00b7fen\u00b7still\u00b7stand", "ist", "ge\u00b7k\u00fcn\u00b7det"], "token_info": ["word", "word", "word"], "pos": ["NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "D\u00e4nen ziehen ab zur Nacht,", "tokens": ["D\u00e4\u00b7nen", "zie\u00b7hen", "ab", "zur", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Russen, Schweden stark verb\u00fcndet,", "tokens": ["Rus\u00b7sen", ",", "Schwe\u00b7den", "stark", "ver\u00b7b\u00fcn\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Brechen her mit wilder Macht.", "tokens": ["Bre\u00b7chen", "her", "mit", "wil\u00b7der", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APZR", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Draus bei Schleswig steht vor allen", "tokens": ["Draus", "bei", "Schles\u00b7wig", "steht", "vor", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "NE", "VVFIN", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weit ein H\u00e4uslein ausgesetzt.", "tokens": ["Weit", "ein", "H\u00e4us\u00b7lein", "aus\u00b7ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Draus bei Schleswig in der H\u00fctte", "tokens": ["Draus", "bei", "Schles\u00b7wig", "in", "der", "H\u00fct\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "NE", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Singt ein frommes M\u00fctterlein,", "tokens": ["Singt", "ein", "from\u00b7mes", "M\u00fct\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herr, in deinen Scho\u00df ich sch\u00fctte", "tokens": ["Herr", ",", "in", "dei\u00b7nen", "Scho\u00df", "ich", "sch\u00fct\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle meine Angst und Pein.", "tokens": ["Al\u00b7le", "mei\u00b7ne", "Angst", "und", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch ihr Enkel ohn' Vertrauen,", "tokens": ["Doch", "ihr", "En\u00b7kel", "ohn'", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zwanzigj\u00e4hrig neuster Zeit,", "tokens": ["Zwan\u00b7zig\u00b7j\u00e4h\u00b7rig", "neus\u00b7ter", "Zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hat den Br\u00e4utigam zu schauen", "tokens": ["Hat", "den", "Br\u00e4u\u00b7ti\u00b7gam", "zu", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seine Lampe nicht bereit.", "tokens": ["Sei\u00b7ne", "Lam\u00b7pe", "nicht", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Draus bei Schleswig in der H\u00fctte", "tokens": ["Draus", "bei", "Schles\u00b7wig", "in", "der", "H\u00fct\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "NE", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Singt ein frommes M\u00fctterlein.", "tokens": ["Singt", "ein", "from\u00b7mes", "M\u00fct\u00b7ter\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Eine Mauer um uns baue", "tokens": ["Ei\u00b7ne", "Mau\u00b7er", "um", "uns", "bau\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Singt das fromme M\u00fctterlein,", "tokens": ["Singt", "das", "from\u00b7me", "M\u00fct\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df dem Feinde vor uns graue", "tokens": ["Da\u00df", "dem", "Fein\u00b7de", "vor", "uns", "grau\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00fcll' in deine Burg uns ein.", "tokens": ["H\u00fcll'", "in", "dei\u00b7ne", "Burg", "uns", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mutter, spricht der Weltgesinnte,", "tokens": ["Mut\u00b7ter", ",", "spricht", "der", "Welt\u00b7ge\u00b7sinn\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine Mauer uns ums Haus", "tokens": ["Ei\u00b7ne", "Mau\u00b7er", "uns", "ums", "Haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Kriegt unm\u00f6glich so geschwinde", "tokens": ["Kriegt", "un\u00b7m\u00f6g\u00b7lich", "so", "ge\u00b7schwin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Euer lieber Gott heraus.", "tokens": ["Eu\u00b7er", "lie\u00b7ber", "Gott", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Eine Mauer um uns baue:", "tokens": ["Ei\u00b7ne", "Mau\u00b7er", "um", "uns", "bau\u00b7e", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Singt das fromme M\u00fctterlein.", "tokens": ["Singt", "das", "from\u00b7me", "M\u00fct\u00b7ter\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Enkel fest ist mein Vertrauen,", "tokens": ["En\u00b7kel", "fest", "ist", "mein", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn's dem lieben Gott gef\u00e4llt,", "tokens": ["Wenn's", "dem", "lie\u00b7ben", "Gott", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kann er uns die Mauer bauen,", "tokens": ["Kann", "er", "uns", "die", "Mau\u00b7er", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was er will ist wohl bestellt.", "tokens": ["Was", "er", "will", "ist", "wohl", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Trommeln rommdidomm rings prasseln", "tokens": ["Trom\u00b7meln", "romm\u00b7di\u00b7domm", "rings", "pras\u00b7seln"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die Trompeten schmettern drein,", "tokens": ["Die", "Trom\u00b7pe\u00b7ten", "schmet\u00b7tern", "drein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Rosse wiehern, Wagen rasseln,", "tokens": ["Ros\u00b7se", "wie\u00b7hern", ",", "Wa\u00b7gen", "ras\u00b7seln", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ach nun bricht der Feind herein,", "tokens": ["Ach", "nun", "bricht", "der", "Feind", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Eine Mauer um uns baue", "tokens": ["Ei\u00b7ne", "Mau\u00b7er", "um", "uns", "bau\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Singt das fromme M\u00fctterlein.", "tokens": ["Singt", "das", "from\u00b7me", "M\u00fct\u00b7ter\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Rings in alle H\u00fctten brechen", "tokens": ["Rings", "in", "al\u00b7le", "H\u00fct\u00b7ten", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwed' und Russe mit Geschrei,", "tokens": ["Schwed'", "und", "Rus\u00b7se", "mit", "Ge\u00b7schrei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4rmen, fluchen, dr\u00e4ngen, zechen.", "tokens": ["L\u00e4r\u00b7men", ",", "flu\u00b7chen", ",", "dr\u00e4n\u00b7gen", ",", "ze\u00b7chen", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch dies Haus ziehn sie vorbei.", "tokens": ["Doch", "dies", "Haus", "ziehn", "sie", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der Enkel spricht in Sorgen", "tokens": ["Und", "der", "En\u00b7kel", "spricht", "in", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Mutter, uns verr\u00e4t das Lied.", "tokens": ["Mut\u00b7ter", ",", "uns", "ver\u00b7r\u00e4t", "das", "Lied", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber