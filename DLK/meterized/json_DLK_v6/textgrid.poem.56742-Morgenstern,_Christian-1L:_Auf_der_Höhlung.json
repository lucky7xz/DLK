{"textgrid.poem.56742": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf der H\u00f6hlung", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf der H\u00f6hlung", "tokens": ["Auf", "der", "H\u00f6h\u00b7lung"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "eines erstorbenen Kraters", "tokens": ["ei\u00b7nes", "ers\u00b7tor\u00b7be\u00b7nen", "Kra\u00b7ters"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "blies heute Pan,", "tokens": ["blies", "heu\u00b7te", "Pan", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "wie Schusterjungen", "tokens": ["wie", "Schus\u00b7ter\u00b7jun\u00b7gen"], "token_info": ["word", "word"], "pos": ["PWAV", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "auf Schl\u00fcsseln pfeifen.", "tokens": ["auf", "Schl\u00fcs\u00b7seln", "pfei\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Er pfiff \u00bbdie Welt\u00ab aus,", "tokens": ["Er", "pfiff", "\u00bb", "die", "Welt", "\u00ab", "aus", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ART", "NN", "$(", "PTKVZ", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "dies sonderbare,", "tokens": ["dies", "son\u00b7der\u00b7ba\u00b7re", ","], "token_info": ["word", "word", "punct"], "pos": ["PDS", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "zweideutige St\u00fcck", "tokens": ["zwei\u00b7deu\u00b7ti\u00b7ge", "St\u00fcck"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "eines Anonymus,", "tokens": ["ei\u00b7nes", "An\u00b7o\u00b7ny\u00b7mus", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "das Tag f\u00fcr Tag", "tokens": ["das", "Tag", "f\u00fcr", "Tag"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "uns vorgespielt wird", "tokens": ["uns", "vor\u00b7ge\u00b7spielt", "wird"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVPP", "VAFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "und niemals endet.", "tokens": ["und", "nie\u00b7mals", "en\u00b7det", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Oh pfeife doch minder,", "tokens": ["Oh", "pfei\u00b7fe", "doch", "min\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "teuerer Waldgott!", "tokens": ["teu\u00b7e\u00b7rer", "Wald\u00b7gott", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Halt Einkehr, Pan!", "tokens": ["Halt", "Ein\u00b7kehr", ",", "Pan", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Wer hie\u00df Dich denn", "tokens": ["Wer", "hie\u00df", "Dich", "denn"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "unter Menschen gehen? ...", "tokens": ["un\u00b7ter", "Men\u00b7schen", "ge\u00b7hen", "?", "..."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}