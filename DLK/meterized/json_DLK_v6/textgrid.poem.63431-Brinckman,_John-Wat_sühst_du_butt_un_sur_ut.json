{"textgrid.poem.63431": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "Wat s\u00fchst du butt un sur ut", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wat s\u00fchst du butt un sur ut,", "tokens": ["Wat", "s\u00fchst", "du", "butt", "un", "sur", "ut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "FM", "FM", "FM", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.2": {"text": "as wenn di wat verdr\u00fctt?", "tokens": ["as", "wenn", "di", "wat", "ver\u00b7dr\u00fctt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "NE", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du s\u00fchst jo as 'n Schur ut,", "tokens": ["Du", "s\u00fchst", "jo", "as", "'n", "Schur", "ut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "dat up S\u0153bnbr\u00f6der g\u00fctt,", "tokens": ["dat", "up", "S\u0153b\u00b7nbr\u00f6\u00b7der", "g\u00fctt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+----+", "measure": "dactylic.init"}, "line.5": {"text": "dat s\u0153ben W\u00e4ken anh\u00f6llt", "tokens": ["dat", "s\u0153\u00b7ben", "W\u00e4\u00b7ken", "an\u00b7h\u00f6llt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "un dat nich ihrer geiht,", "tokens": ["un", "dat", "nich", "ih\u00b7rer", "geiht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "PTKNEG", "PPOSAT", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "bet dat de Wind de Roggensch\u00f6dd", "tokens": ["bet", "dat", "de", "Wind", "de", "Rog\u00b7gen\u00b7sch\u00f6dd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NE", "NN", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "furt vun de Stoppel weigt.", "tokens": ["furt", "vun", "de", "Stop\u00b7pel", "weigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sonn blage Ogen hest du jo,", "tokens": ["Sonn", "bla\u00b7ge", "O\u00b7gen", "hest", "du", "jo", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "PPER", "NE", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "sonn schier' un witte T\u00e4hn \u2013", "tokens": ["sonn", "schier'", "un", "wit\u00b7te", "T\u00e4hn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "FM", "FM", "FM", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "wenn du eens lut uplachen deist,", "tokens": ["wenn", "du", "e\u00b7ens", "lut", "up\u00b7la\u00b7chen", "de\u00b7ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "FM", "FM", "FM", "FM", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "dat lett di gor to sch\u00f6n!", "tokens": ["dat", "lett", "di", "gor", "to", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sonn Schelmenkulen kriggst du,", "tokens": ["Sonn", "Schel\u00b7men\u00b7ku\u00b7len", "kriggst", "du", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "as haddst \u00fcns all to Narrn,", "tokens": ["as", "haddst", "\u00fcns", "all", "to", "Narrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "as ob girn k\u00fcssen m\u00fcchst du \u2013", "tokens": ["as", "ob", "girn", "k\u00fcs\u00b7sen", "m\u00fcchst", "du", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "NN", "VVINF", "VMFIN", "PPER", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.8": {"text": "na, dor kann Rat to warrn!", "tokens": ["na", ",", "dor", "kann", "Rat", "to", "warrn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}