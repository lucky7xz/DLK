{"textgrid.poem.36455": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "5. Ein Hengst und eine Wespe", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine kleine Wespe stach", "tokens": ["Ei\u00b7ne", "klei\u00b7ne", "Wes\u00b7pe", "stach"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Einen Hengst. Er schlug nach ihr;", "tokens": ["Ei\u00b7nen", "Hengst", ".", "Er", "schlug", "nach", "ihr", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die kleine Wespe sprach:", "tokens": ["Und", "die", "klei\u00b7ne", "Wes\u00b7pe", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hengstchen, schlag doch nicht nach mir!", "tokens": ["Hengst\u00b7chen", ",", "schlag", "doch", "nicht", "nach", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "PTKNEG", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sieh', ich sitz' an sicherm Orte,", "tokens": ["Sieh'", ",", "ich", "sitz'", "an", "si\u00b7cherm", "Or\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Hengstchen, sieh! du triffst mich nicht!", "tokens": ["Hengst\u00b7chen", ",", "sieh", "!", "du", "triffst", "mich", "nicht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hengstchen giebt ihr gute Worte;", "tokens": ["Hengst\u00b7chen", "giebt", "ihr", "gu\u00b7te", "Wor\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die kleine Wespe spricht:", "tokens": ["Und", "die", "klei\u00b7ne", "Wes\u00b7pe", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh', nun stech' ich dich nicht mehr!", "tokens": ["Sieh'", ",", "nun", "stech'", "ich", "dich", "nicht", "mehr", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "PRF", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}