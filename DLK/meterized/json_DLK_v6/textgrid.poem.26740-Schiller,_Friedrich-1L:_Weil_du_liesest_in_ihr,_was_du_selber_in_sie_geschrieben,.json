{"textgrid.poem.26740": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weil du liesest in ihr, was du selber in sie geschrieben,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil du liesest in ihr, was du selber in sie geschrieben,", "tokens": ["Weil", "du", "lie\u00b7sest", "in", "ihr", ",", "was", "du", "sel\u00b7ber", "in", "sie", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PPER", "$,", "PWS", "PPER", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "--+--+--+--+-+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Weil du in Gruppen f\u00fcrs Aug ihre Erscheinungen reihst,", "tokens": ["Weil", "du", "in", "Grup\u00b7pen", "f\u00fcrs", "Aug", "ih\u00b7re", "Er\u00b7schei\u00b7nun\u00b7gen", "reihst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Deine Schn\u00fcre gezogen auf ihrem unendlichen Felde,", "tokens": ["Dei\u00b7ne", "Schn\u00fc\u00b7re", "ge\u00b7zo\u00b7gen", "auf", "ih\u00b7rem", "un\u00b7end\u00b7li\u00b7chen", "Fel\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "W\u00e4hnst du, es fasse dein Geist ahnend die gro\u00dfe Natur.", "tokens": ["W\u00e4hnst", "du", ",", "es", "fas\u00b7se", "dein", "Geist", "ah\u00b7nend", "die", "gro\u00b7\u00dfe", "Na\u00b7tur", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "So beschreibt mit Figuren der Astronome den Himmel,", "tokens": ["So", "be\u00b7schreibt", "mit", "Fi\u00b7gu\u00b7ren", "der", "Ast\u00b7ro\u00b7no\u00b7me", "den", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Da\u00df in dem ewigen Raum leichter sich finde der Blick,", "tokens": ["Da\u00df", "in", "dem", "e\u00b7wi\u00b7gen", "Raum", "leich\u00b7ter", "sich", "fin\u00b7de", "der", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "ADJD", "PRF", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Kn\u00fcpft entlegene Sonnen, durch Siriusfernen geschieden,", "tokens": ["Kn\u00fcpft", "ent\u00b7le\u00b7ge\u00b7ne", "Son\u00b7nen", ",", "durch", "Si\u00b7rius\u00b7fer\u00b7nen", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Aneinander im Schwan und in den H\u00f6rnern des Stiers.", "tokens": ["An\u00b7ein\u00b7an\u00b7der", "im", "Schwan", "und", "in", "den", "H\u00f6r\u00b7nern", "des", "Stiers", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "KON", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Aber versteht er darum der Sph\u00e4ren mystische T\u00e4nze,", "tokens": ["A\u00b7ber", "ver\u00b7steht", "er", "da\u00b7rum", "der", "Sph\u00e4\u00b7ren", "mys\u00b7ti\u00b7sche", "T\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PAV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Weil ihm das Sternengew\u00f6lb sein Planiglobium zeigt?", "tokens": ["Weil", "ihm", "das", "Ster\u00b7nen\u00b7ge\u00b7w\u00f6lb", "sein", "Pla\u00b7nig\u00b7lo\u00b7bi\u00b7um", "zeigt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}}}}}