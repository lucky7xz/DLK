{"textgrid.poem.67090": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Hymne an die Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1792, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Froh der s\u00fc\u00dfen Augenweide", "tokens": ["Froh", "der", "s\u00fc\u00b7\u00dfen", "Au\u00b7gen\u00b7wei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wallen wir auf gr\u00fcner Flur;", "tokens": ["Wal\u00b7len", "wir", "auf", "gr\u00fc\u00b7ner", "Flur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unser Priestertum ist Freude,", "tokens": ["Un\u00b7ser", "Pries\u00b7ter\u00b7tum", "ist", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unser Tempel die Natur; \u2013", "tokens": ["Un\u00b7ser", "Tem\u00b7pel", "die", "Na\u00b7tur", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Heute soll kein Auge tr\u00fcbe,", "tokens": ["Heu\u00b7te", "soll", "kein", "Au\u00b7ge", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sorge nicht hienieden sein!", "tokens": ["Sor\u00b7ge", "nicht", "hien\u00b7ie\u00b7den", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Jedes Wesen soll der Liebe,", "tokens": ["Je\u00b7des", "We\u00b7sen", "soll", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Frei und froh, wie wir, sich freun!", "tokens": ["Frei", "und", "froh", ",", "wie", "wir", ",", "sich", "freun", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "PWAV", "PPER", "$,", "PRF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "H\u00f6hnt im Stolze, Schwestern, Br\u00fcder!", "tokens": ["H\u00f6hnt", "im", "Stol\u00b7ze", ",", "Schwes\u00b7tern", ",", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6hnt der scheuen Knechte Tand!", "tokens": ["H\u00f6hnt", "der", "scheu\u00b7en", "Knech\u00b7te", "Tand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jubelt k\u00fchn das Lied der Lieder,", "tokens": ["Ju\u00b7belt", "k\u00fchn", "das", "Lied", "der", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Festgeschlungen Hand in Hand!", "tokens": ["Fest\u00b7ge\u00b7schlun\u00b7gen", "Hand", "in", "Hand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Steigt hinauf am Rebenh\u00fcgel,", "tokens": ["Steigt", "hin\u00b7auf", "am", "Re\u00b7ben\u00b7h\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Blickt hinab ins weite Tal!", "tokens": ["Blickt", "hin\u00b7ab", "ins", "wei\u00b7te", "Tal", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "\u00dcberall der Liebe Fl\u00fcgel,", "tokens": ["\u00dc\u00b7be\u00b7rall", "der", "Lie\u00b7be", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hold und herrlich \u00fcberall!", "tokens": ["Hold", "und", "herr\u00b7lich", "\u00fc\u00b7be\u00b7rall", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Liebe bringt zu jungen Rosen", "tokens": ["Lie\u00b7be", "bringt", "zu", "jun\u00b7gen", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Morgentau von hoher Luft,", "tokens": ["Mor\u00b7gen\u00b7tau", "von", "ho\u00b7her", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lehrt die warmen L\u00fcfte kosen", "tokens": ["Lehrt", "die", "war\u00b7men", "L\u00fcf\u00b7te", "ko\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Maienblume Duft;", "tokens": ["In", "der", "Mai\u00b7en\u00b7blu\u00b7me", "Duft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Um die Orione leitet", "tokens": ["Um", "die", "O\u00b7rio\u00b7ne", "lei\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie die treuen Erden her,", "tokens": ["Sie", "die", "treu\u00b7en", "Er\u00b7den", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Folgsam ihrem Winke, gleitet", "tokens": ["Folg\u00b7sam", "ih\u00b7rem", "Win\u00b7ke", ",", "glei\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NE", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Jeder Strom ins weite Meer;", "tokens": ["Je\u00b7der", "Strom", "ins", "wei\u00b7te", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "An die wilden Berge reihet", "tokens": ["An", "die", "wil\u00b7den", "Ber\u00b7ge", "rei\u00b7het"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie die sanften T\u00e4ler an,", "tokens": ["Sie", "die", "sanf\u00b7ten", "T\u00e4\u00b7ler", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die entbrannte Sonn erfreuet", "tokens": ["Die", "ent\u00b7brann\u00b7te", "Sonn", "er\u00b7freu\u00b7et"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie im stillen Ozean;", "tokens": ["Sie", "im", "stil\u00b7len", "O\u00b7ze\u00b7an", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Siehe! mit der Erde gattet", "tokens": ["Sie\u00b7he", "!", "mit", "der", "Er\u00b7de", "gat\u00b7tet"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$.", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich des Himmels heilge Lust,", "tokens": ["Sich", "des", "Him\u00b7mels", "heil\u00b7ge", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Von den Wettern \u00fcberschattet", "tokens": ["Von", "den", "Wet\u00b7tern", "\u00fc\u00b7bersc\u00b7hat\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bebt entz\u00fcckt der Mutter Brust.", "tokens": ["Bebt", "ent\u00b7z\u00fcckt", "der", "Mut\u00b7ter", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Liebe wallt durch Ozeane,", "tokens": ["Lie\u00b7be", "wallt", "durch", "O\u00b7zea\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "H\u00f6hnt der d\u00fcrren W\u00fcste Sand,", "tokens": ["H\u00f6hnt", "der", "d\u00fcr\u00b7ren", "W\u00fcs\u00b7te", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Blutet an der Siegesfahne", "tokens": ["Blu\u00b7tet", "an", "der", "Sie\u00b7ges\u00b7fah\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jauchzend f\u00fcr das Vaterland;", "tokens": ["Jauch\u00b7zend", "f\u00fcr", "das", "Va\u00b7ter\u00b7land", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebe tr\u00fcmmert Felsen nieder,", "tokens": ["Lie\u00b7be", "tr\u00fcm\u00b7mert", "Fel\u00b7sen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zaubert Paradiese hin \u2013", "tokens": ["Zau\u00b7bert", "Pa\u00b7ra\u00b7die\u00b7se", "hin", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "L\u00e4chelnd kehrt die Unschuld wieder,", "tokens": ["L\u00e4\u00b7chelnd", "kehrt", "die", "Un\u00b7schuld", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "G\u00f6ttlichere Lenze bl\u00fchn.", "tokens": ["G\u00f6tt\u00b7li\u00b7che\u00b7re", "Len\u00b7ze", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "M\u00e4chtig durch die Liebe, winden", "tokens": ["M\u00e4ch\u00b7tig", "durch", "die", "Lie\u00b7be", ",", "win\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der Fessel wir uns los,", "tokens": ["Von", "der", "Fes\u00b7sel", "wir", "uns", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die trunknen Geister schwinden", "tokens": ["Und", "die", "trunk\u00b7nen", "Geis\u00b7ter", "schwin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu den Sternen, frei und gro\u00df!", "tokens": ["Zu", "den", "Ster\u00b7nen", ",", "frei", "und", "gro\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unter Schwur und Ku\u00df vergessen", "tokens": ["Un\u00b7ter", "Schwur", "und", "Ku\u00df", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wir die tr\u00e4ge Flut der Zeit,", "tokens": ["Wir", "die", "tr\u00e4\u00b7ge", "Flut", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die Seele naht vermessen", "tokens": ["Und", "die", "See\u00b7le", "naht", "ver\u00b7mes\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Deiner Lust, Unendlichkeit!", "tokens": ["Dei\u00b7ner", "Lust", ",", "Un\u00b7end\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}