{"textgrid.poem.65628": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Griechen an die Freunde ihres Alterthums", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie haben viel geschrieben, gesungen und gesagt,", "tokens": ["Sie", "ha\u00b7ben", "viel", "ge\u00b7schrie\u00b7ben", ",", "ge\u00b7sun\u00b7gen", "und", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Gepriesen und bewundert, beneidet und beklagt.", "tokens": ["Ge\u00b7prie\u00b7sen", "und", "be\u00b7wun\u00b7dert", ",", "be\u00b7nei\u00b7det", "und", "be\u00b7klagt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$,", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Namen unsrer V\u00e4ter, sie sind von sch\u00f6nem Klang,", "tokens": ["Die", "Na\u00b7men", "uns\u00b7rer", "V\u00e4\u00b7ter", ",", "sie", "sind", "von", "sch\u00f6\u00b7nem", "Klang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sie passen allen V\u00f6lkern in ihren Lobgesang;", "tokens": ["Sie", "pas\u00b7sen", "al\u00b7len", "V\u00f6l\u00b7kern", "in", "ih\u00b7ren", "Lob\u00b7ge\u00b7sang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Und wer ergl\u00fchen wollte f\u00fcr Freiheit, Ehr' und Ruhm,", "tokens": ["Und", "wer", "er\u00b7gl\u00fc\u00b7hen", "woll\u00b7te", "f\u00fcr", "Frei\u00b7heit", ",", "Ehr'", "und", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVINF", "VMFIN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Der holte sich das Feuer aus unserm Alterthum,", "tokens": ["Der", "hol\u00b7te", "sich", "das", "Feu\u00b7er", "aus", "un\u00b7serm", "Al\u00b7ter\u00b7thum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Das Feuer, welches schlummernd in Aschenhaufen ruht,", "tokens": ["Das", "Feu\u00b7er", ",", "wel\u00b7ches", "schlum\u00b7mernd", "in", "A\u00b7schen\u00b7hau\u00b7fen", "ruht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Die einst getrunken haben hellenisch Heldenblut.", "tokens": ["Die", "einst", "ge\u00b7trun\u00b7ken", "ha\u00b7ben", "hel\u00b7le\u00b7nisch", "Hel\u00b7den\u00b7blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "VAFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Was hat euch nun, ihr V\u00f6lker, so scheu und bang' gemacht?", "tokens": ["Was", "hat", "euch", "nun", ",", "ihr", "V\u00f6l\u00b7ker", ",", "so", "scheu", "und", "bang'", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Der Geist, den ihr beschworen, er steigt aus tiefer Nacht", "tokens": ["Der", "Geist", ",", "den", "ihr", "be\u00b7schwo\u00b7ren", ",", "er", "steigt", "aus", "tie\u00b7fer", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Empor in alter Gr\u00f6\u00dfe, und beut euch seine Hand \u2013", "tokens": ["Em\u00b7por", "in", "al\u00b7ter", "Gr\u00f6\u00b7\u00dfe", ",", "und", "beut", "euch", "sei\u00b7ne", "Hand", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,", "KON", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Erkennt ihr es nicht wieder, das freie Griechenland?", "tokens": ["Er\u00b7kennt", "ihr", "es", "nicht", "wie\u00b7der", ",", "das", "frei\u00b7e", "Grie\u00b7chen\u00b7land", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "ADV", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Die Funken in der Asche, in der ihr oft gew\u00fchlt,", "tokens": ["Die", "Fun\u00b7ken", "in", "der", "A\u00b7sche", ",", "in", "der", "ihr", "oft", "ge\u00b7w\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Die Funken, deren Gluthen ihr oft in euch gef\u00fchlt,", "tokens": ["Die", "Fun\u00b7ken", ",", "de\u00b7ren", "Glu\u00b7then", "ihr", "oft", "in", "euch", "ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "PPER", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Sie schlagen lustig lodernd zu hohen Flammen aus \u2013", "tokens": ["Sie", "schla\u00b7gen", "lus\u00b7tig", "lo\u00b7dernd", "zu", "ho\u00b7hen", "Flam\u00b7men", "aus", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "VVPP", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Kleinm\u00fcthige, ihr seht es \u2013 und euch erfa\u00dft ein Graus!", "tokens": ["Klein\u00b7m\u00fct\u00b7hi\u00b7ge", ",", "ihr", "seht", "es", "\u2013", "und", "euch", "er\u00b7fa\u00dft", "ein", "Graus", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "PPER", "VVFIN", "PPER", "$(", "KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "--+-+---+-+-+", "measure": "anapaest.init"}, "line.17": {"text": "O weh, so habt ihr, Freunde, mit Namen nur gespielt!", "tokens": ["O", "weh", ",", "so", "habt", "ihr", ",", "Freun\u00b7de", ",", "mit", "Na\u00b7men", "nur", "ge\u00b7spielt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "$,", "NN", "$,", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Habt in die leeren L\u00fcfte mit stolzem Pfeil gezielt!", "tokens": ["Habt", "in", "die", "lee\u00b7ren", "L\u00fcf\u00b7te", "mit", "stol\u00b7zem", "Pfeil", "ge\u00b7zielt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Gepriesen und bewundert, beneidet und beklagt.", "tokens": ["Ge\u00b7prie\u00b7sen", "und", "be\u00b7wun\u00b7dert", ",", "be\u00b7nei\u00b7det", "und", "be\u00b7klagt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$,", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Was schw\u00e4rmt ihr in den Fernen der grauen Heldenzeit?", "tokens": ["Was", "schw\u00e4rmt", "ihr", "in", "den", "Fer\u00b7nen", "der", "grau\u00b7en", "Hel\u00b7den\u00b7zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Kehrt heim, ihr Hochentz\u00fcckten! \u2013 der Weg ist gar zu weit.", "tokens": ["Kehrt", "heim", ",", "ihr", "Ho\u00b7chent\u00b7z\u00fcck\u00b7ten", "!", "\u2013", "der", "Weg", "ist", "gar", "zu", "weit", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPOSAT", "NN", "$.", "$(", "ART", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Das Alt' ist neu geworden, die Fern' ist euch so nah,", "tokens": ["Das", "Alt'", "ist", "neu", "ge\u00b7wor\u00b7den", ",", "die", "Fern'", "ist", "euch", "so", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VAPP", "$,", "ART", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Was ihr ertr\u00e4umt so lange, leibhaftig steht es da,", "tokens": ["Was", "ihr", "er\u00b7tr\u00e4umt", "so", "lan\u00b7ge", ",", "leib\u00b7haf\u00b7tig", "steht", "es", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "ADV", "ADV", "$,", "ADJD", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Es klopft an eure Pforte \u2013 ihr schlie\u00dft ihm euer Haus \u2013", "tokens": ["Es", "klopft", "an", "eu\u00b7re", "Pfor\u00b7te", "\u2013", "ihr", "schlie\u00dft", "ihm", "eu\u00b7er", "Haus", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$(", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "Sieht es denn gar so anders, als ihr es tr\u00e4umtet, aus?", "tokens": ["Sieht", "es", "denn", "gar", "so", "an\u00b7ders", ",", "als", "ihr", "es", "tr\u00e4um\u00b7tet", ",", "aus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV", "ADV", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}