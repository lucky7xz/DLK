{"textgrid.poem.65403": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du hast zum Trinker mich gemacht,", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast zum Trinker mich gemacht,", "tokens": ["Du", "hast", "zum", "Trin\u00b7ker", "mich", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du sch\u00f6ne Kellnerin!", "tokens": ["Du", "sch\u00f6\u00b7ne", "Kell\u00b7ne\u00b7rin", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ei, ei, wer h\u00e4tte das gedacht,", "tokens": ["Ei", ",", "ei", ",", "wer", "h\u00e4t\u00b7te", "das", "ge\u00b7dacht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$,", "PWS", "VAFIN", "PDS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da ich so jung noch bin?", "tokens": ["Da", "ich", "so", "jung", "noch", "bin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "ADV", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und klag' ich an den s\u00fc\u00dfen Wein,", "tokens": ["Und", "klag'", "ich", "an", "den", "s\u00fc\u00b7\u00dfen", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den sie in's Glas mir gie\u00dft? \u2013", "tokens": ["Den", "sie", "in's", "Glas", "mir", "gie\u00dft", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So klag' ich an den Vater Rhein,", "tokens": ["So", "klag'", "ich", "an", "den", "Va\u00b7ter", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei dem die Rebe sprie\u00dft.", "tokens": ["Bei", "dem", "die", "Re\u00b7be", "sprie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So klag' ich an den Sonnenstrahl,", "tokens": ["So", "klag'", "ich", "an", "den", "Son\u00b7nen\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Thau, Regen, Luft und Wind,", "tokens": ["Thau", ",", "Re\u00b7gen", ",", "Luft", "und", "Wind", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die doch auf Erden allzumal", "tokens": ["Die", "doch", "auf", "Er\u00b7den", "all\u00b7zu\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Himmels Gaben sind.", "tokens": ["Des", "Him\u00b7mels", "Ga\u00b7ben", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und klag' ich an ihr Schelmgesicht,", "tokens": ["Und", "klag'", "ich", "an", "ihr", "Schelm\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr blaues Augenpaar,", "tokens": ["Ihr", "blau\u00b7es", "Au\u00b7gen\u00b7paar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr M\u00fcndchen, das auch schweigend spricht,", "tokens": ["Ihr", "M\u00fcnd\u00b7chen", ",", "das", "auch", "schwei\u00b7gend", "spricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr goldnes Flechtenhaar? \u2013", "tokens": ["Ihr", "gold\u00b7nes", "Flech\u00b7ten\u00b7haar", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie hat ja ihren sch\u00f6nen Leib", "tokens": ["Sie", "hat", "ja", "ih\u00b7ren", "sch\u00f6\u00b7nen", "Leib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich selber nicht gemacht,", "tokens": ["Sich", "sel\u00b7ber", "nicht", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und in dem Grabe liegt das Weib,", "tokens": ["Und", "in", "dem", "Gra\u00b7be", "liegt", "das", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das sie zur Welt gebracht.", "tokens": ["Das", "sie", "zur", "Welt", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wer stellt die Todten vor Gericht", "tokens": ["Wer", "stellt", "die", "Tod\u00b7ten", "vor", "Ge\u00b7richt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und st\u00f6rt des Grabes Ruh'?", "tokens": ["Und", "st\u00f6rt", "des", "Gra\u00b7bes", "Ruh'", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kind, nimm es dir zu Herzen nicht,", "tokens": ["Kind", ",", "nimm", "es", "dir", "zu", "Her\u00b7zen", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "PPER", "APPR", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schenk' nur immer zu!", "tokens": ["Und", "schenk'", "nur", "im\u00b7mer", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}