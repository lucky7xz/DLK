{"textgrid.poem.42217": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Auf dem Faulbett", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf mein Faulbett hingestreckt", "tokens": ["Auf", "mein", "Faul\u00b7bett", "hin\u00b7ge\u00b7streckt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcberdenk ich so meine Tage,", "tokens": ["\u00dc\u00b7ber\u00b7denk", "ich", "so", "mei\u00b7ne", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Forschend, was wohl dahinter steckt,", "tokens": ["For\u00b7schend", ",", "was", "wohl", "da\u00b7hin\u00b7ter", "steckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRELS", "ADV", "PAV", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Da\u00df ich nur immer klage.", "tokens": ["Da\u00df", "ich", "nur", "im\u00b7mer", "kla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich habe zu essen, ich habe Tabak,", "tokens": ["Ich", "ha\u00b7be", "zu", "es\u00b7sen", ",", "ich", "ha\u00b7be", "Ta\u00b7bak", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKZU", "VVINF", "$,", "PPER", "VAFIN", "NN", "$,"], "meter": "-+-----+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ich lebe in jeder Sph\u00e4re,", "tokens": ["Ich", "le\u00b7be", "in", "je\u00b7der", "Sph\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich liebe je nach meinem Geschmack", "tokens": ["Ich", "lie\u00b7be", "je", "nach", "mei\u00b7nem", "Ge\u00b7schmack"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Blaustrumpf oder Het\u00e4re.", "tokens": ["Blau\u00b7strumpf", "o\u00b7der", "He\u00b7t\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Die sexuelle Psychopathie,", "tokens": ["Die", "se\u00b7xu\u00b7el\u00b7le", "Psy\u00b7cho\u00b7pa\u00b7thie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Ich habe sie l\u00e4ngst \u00fcberwunden \u2013", "tokens": ["Ich", "ha\u00b7be", "sie", "l\u00e4ngst", "\u00fc\u00b7berw\u00b7un\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und dennoch, ich verge\u00df es nie,", "tokens": ["Und", "den\u00b7noch", ",", "ich", "ver\u00b7ge\u00df", "es", "nie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es waren doch sch\u00f6ne Stunden.", "tokens": ["Es", "wa\u00b7ren", "doch", "sch\u00f6\u00b7ne", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}