{"textgrid.poem.64205": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Vorgesang", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein liebes V\u00f6glein kommt zu Leide,", "tokens": ["Kein", "lie\u00b7bes", "V\u00f6\u00b7glein", "kommt", "zu", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das mir in Garn und Schlaghaus geht:", "tokens": ["Das", "mir", "in", "Garn", "und", "Schlag\u00b7haus", "geht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Winter, wann durch Wald und Heide", "tokens": ["Im", "Win\u00b7ter", ",", "wann", "durch", "Wald", "und", "Hei\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "APPR", "NN", "KON", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Eiswind und der Hunger weht,", "tokens": ["Der", "Eis\u00b7wind", "und", "der", "Hun\u00b7ger", "weht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da trifft in meiner Halle Weide,", "tokens": ["Da", "trifft", "in", "mei\u00b7ner", "Hal\u00b7le", "Wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was zierlich Schopf und Fittich dreht:", "tokens": ["Was", "zier\u00b7lich", "Schopf", "und", "Fit\u00b7tich", "dreht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Frei, sonder K\u00e4fig, h\u00fcpfen sie", "tokens": ["Frei", ",", "son\u00b7der", "K\u00e4\u00b7fig", ",", "h\u00fcp\u00b7fen", "sie"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "KON", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf Harfe mir, auf Buch und Knie.", "tokens": ["Auf", "Har\u00b7fe", "mir", ",", "auf", "Buch", "und", "Knie", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dann sitz' ich, deckend Bein mit Beine,", "tokens": ["Dann", "sitz'", "ich", ",", "de\u00b7ckend", "Bein", "mit", "Bei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Kinn geneigt zur Hand geschmiegt,", "tokens": ["Das", "Kinn", "ge\u00b7neigt", "zur", "Hand", "ge\u00b7schmiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bei mattem Wintersonnwendscheine", "tokens": ["Bei", "mat\u00b7tem", "Win\u00b7ter\u00b7sonn\u00b7wend\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch H\u00e4nflingsang in Lenz gewiegt,", "tokens": ["Durch", "H\u00e4nf\u00b7lings\u00b7ang", "in", "Lenz", "ge\u00b7wiegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und bis zum Jordan, fern vom Maine,", "tokens": ["Und", "bis", "zum", "Jor\u00b7dan", ",", "fern", "vom", "Mai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NE", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gedenken fr\u00fchrer Zeit mir fliegt,", "tokens": ["Ge\u00b7den\u00b7ken", "fr\u00fch\u00b7rer", "Zeit", "mir", "fliegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gedenken, wie ich rang und stritt", "tokens": ["Ge\u00b7den\u00b7ken", ",", "wie", "ich", "rang", "und", "stritt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und wie ich minnte, sang und litt. \u2013", "tokens": ["Und", "wie", "ich", "minn\u00b7te", ",", "sang", "und", "litt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch, wann der Fr\u00fchling kaum vom weiten", "tokens": ["Doch", ",", "wann", "der", "Fr\u00fch\u00b7ling", "kaum", "vom", "wei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "ADV", "APPRART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den scheuen Gru\u00df der Halde beut,", "tokens": ["Den", "scheu\u00b7en", "Gru\u00df", "der", "Hal\u00b7de", "beut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wann in dem roten, eisbefreiten", "tokens": ["Wann", "in", "dem", "ro\u00b7ten", ",", "eis\u00b7be\u00b7frei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geknosp der Saft sich schwellend neut,", "tokens": ["Ge\u00b7knosp", "der", "Saft", "sich", "schwel\u00b7lend", "neut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wann sch\u00fcchtern um die D\u00e4mmerzeiten", "tokens": ["Wann", "sch\u00fcch\u00b7tern", "um", "die", "D\u00e4m\u00b7mer\u00b7zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zuerst die Amsel lockt \u2013 wie heut' \u2013:", "tokens": ["Zu\u00b7erst", "die", "Am\u00b7sel", "lockt", "\u2013", "wie", "heut'", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$(", "KOKOM", "ADV", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dann schlie\u00df' ich auf die Winterfeste", "tokens": ["Dann", "schlie\u00df'", "ich", "auf", "die", "Win\u00b7ter\u00b7fes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und hui! entschwirren meine G\u00e4ste. \u2013", "tokens": ["Und", "hui", "!", "ent\u00b7schwir\u00b7ren", "mei\u00b7ne", "G\u00e4s\u00b7te", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NE", "$.", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und Undank ist nicht V\u00f6glein Weise!", "tokens": ["Und", "Un\u00b7dank", "ist", "nicht", "V\u00f6\u00b7glein", "Wei\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PTKNEG", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie kennt mich gut, die luft'ge Schar:", "tokens": ["Sie", "kennt", "mich", "gut", ",", "die", "luft'\u00b7ge", "Schar", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zieh' ich im Mai auf gr\u00fcne Reise,", "tokens": ["Zieh'", "ich", "im", "Mai", "auf", "gr\u00fc\u00b7ne", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Werd' ich geleitet wunderbar.", "tokens": ["Werd'", "ich", "ge\u00b7lei\u00b7tet", "wun\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das singt und flattert laut und leise", "tokens": ["Das", "singt", "und", "flat\u00b7tert", "laut", "und", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu H\u00e4upten dicht mir um das Haar", "tokens": ["Zu", "H\u00e4up\u00b7ten", "dicht", "mir", "um", "das", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und gr\u00fc\u00dft: \u00bbHerr Wirt der Winterrast, \u2013", "tokens": ["Und", "gr\u00fc\u00dft", ":", "\u00bb", "Herr", "Wirt", "der", "Win\u00b7ter\u00b7rast", ",", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "NN", "NN", "ART", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Im Walde bist du ", "tokens": ["Im", "Wal\u00b7de", "bist", "du"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Und nun hebt's an. In \u00c4therreine", "tokens": ["Und", "nun", "hebt's", "an", ".", "In", "\u00c4\u00b7ther\u00b7rei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PTKVZ", "$.", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Trilliert der Lerchen Morgenchor,", "tokens": ["Tril\u00b7liert", "der", "Ler\u00b7chen", "Mor\u00b7gen\u00b7chor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Schwarzk\u00f6pflein singt im Busch, das feine,", "tokens": ["Schwarz\u00b7k\u00f6\u00b7pflein", "singt", "im", "Busch", ",", "das", "fei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Herr Fink schl\u00e4gt schmetternd mir ins Ohr,", "tokens": ["Herr", "Fink", "schl\u00e4gt", "schmet\u00b7ternd", "mir", "ins", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "ADJD", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bachstelzlein wippt auf feuchtem Steine", "tokens": ["Bach\u00b7stel\u00b7zlein", "wippt", "auf", "feuch\u00b7tem", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Und aus dem Eichstumpf lugt hervor,", "tokens": ["Und", "aus", "dem", "Eichs\u00b7tumpf", "lugt", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit silbert\u00f6nigem Gepiep,", "tokens": ["Mit", "sil\u00b7ber\u00b7t\u00f6\u00b7ni\u00b7gem", "Ge\u00b7piep", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zaunk\u00f6niglein, der kleine Dieb.", "tokens": ["Zaun\u00b7k\u00f6\u00b7nig\u00b7lein", ",", "der", "klei\u00b7ne", "Dieb", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Ja, rings im Buchhag schwankt kein Reislein,", "tokens": ["Ja", ",", "rings", "im", "Buch\u00b7hag", "schwankt", "kein", "Reis\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "APPRART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von dem kein: Waldwillkomm! mir hallt,", "tokens": ["Von", "dem", "kein", ":", "Wald\u00b7will\u00b7komm", "!", "mir", "hallt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "$.", "NE", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Klopfen rasten Specht und Meislein,", "tokens": ["Im", "Klop\u00b7fen", "ras\u00b7ten", "Specht", "und", "Meis\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Pirol fl\u00f6tet, da\u00df es schallt,", "tokens": ["Der", "Pi\u00b7rol", "fl\u00f6\u00b7tet", ",", "da\u00df", "es", "schallt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im niedern Weidicht schreit das Zeislein:", "tokens": ["Im", "nie\u00b7dern", "Wei\u00b7dicht", "schreit", "das", "Zeis\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbherr Walther kam zum gr\u00fcnen Wald\u00ab,", "tokens": ["\u00bb", "herr", "Walt\u00b7her", "kam", "zum", "gr\u00fc\u00b7nen", "Wald", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NN", "NE", "VVFIN", "APPRART", "ADJA", "NN", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nur Nachtigall setzt sich zu ruhn:", "tokens": ["Nur", "Nach\u00b7ti\u00b7gall", "setzt", "sich", "zu", "ruhn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "\u00bbdu kamst und singst: \u2013 so schweig' ich nun.\u00ab", "tokens": ["\u00bb", "du", "kamst", "und", "singst", ":", "\u2013", "so", "schweig'", "ich", "nun", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "KON", "VVFIN", "$.", "$(", "ADV", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}