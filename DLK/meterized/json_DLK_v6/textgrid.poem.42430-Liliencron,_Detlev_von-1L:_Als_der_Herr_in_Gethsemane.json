{"textgrid.poem.42430": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als der Herr in Gethsemane", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als der Herr in Gethsemane", "tokens": ["Als", "der", "Herr", "in", "Geth\u00b7se\u00b7ma\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf Knieen lag im schwersten Weh,", "tokens": ["Auf", "Kni\u00b7e\u00b7en", "lag", "im", "schwers\u00b7ten", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Als er sich hob, nach den J\u00fcngern zu schauen,", "tokens": ["Als", "er", "sich", "hob", ",", "nach", "den", "J\u00fcn\u00b7gern", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Lie\u00df er die Thr\u00e4nen niedertauen:", "tokens": ["Lie\u00df", "er", "die", "Thr\u00e4\u00b7nen", "nie\u00b7der\u00b7tau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er fand sie schlafend, und mit den Genossen", "tokens": ["Er", "fand", "sie", "schla\u00b7fend", ",", "und", "mit", "den", "Ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Hatte selbst Petrus die Augen geschlossen.", "tokens": ["Hat\u00b7te", "selbst", "Pet\u00b7rus", "die", "Au\u00b7gen", "ge\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "FM", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.7": {"text": "Zum zweiten Mal sucht er die Seinen dann,", "tokens": ["Zum", "zwei\u00b7ten", "Mal", "sucht", "er", "die", "Sei\u00b7nen", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "ART", "PPOSS", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die liegen noch immer in Traumes Bann.", "tokens": ["Die", "lie\u00b7gen", "noch", "im\u00b7mer", "in", "Trau\u00b7mes", "Bann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und zum Dritten, allein im Schmerz,", "tokens": ["Und", "zum", "Drit\u00b7ten", ",", "al\u00b7lein", "im", "Schmerz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Zeigt er Gott das k\u00e4mpfende Herz.", "tokens": ["Zeigt", "er", "Gott", "das", "k\u00e4mp\u00b7fen\u00b7de", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Die heilige Stirn wird ihm feucht und na\u00df,", "tokens": ["Die", "hei\u00b7li\u00b7ge", "Stirn", "wird", "ihm", "feucht", "und", "na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "\u00bbmein Vater, ist es m\u00f6glich, da\u00df ...\u00ab", "tokens": ["\u00bb", "mein", "Va\u00b7ter", ",", "ist", "es", "m\u00f6g\u00b7lich", ",", "da\u00df", "...", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und durch ein Gartenmauerloch", "tokens": ["Und", "durch", "ein", "Gar\u00b7ten\u00b7mau\u00b7er\u00b7loch"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Schl\u00fcpft ein zottig H\u00fcndchen und kroch", "tokens": ["Schl\u00fcpft", "ein", "zot\u00b7tig", "H\u00fcnd\u00b7chen", "und", "kroch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJD", "NN", "KON", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.15": {"text": "Dem Heiland zu F\u00fc\u00dfen und schmiegt sich ihm an,", "tokens": ["Dem", "Hei\u00b7land", "zu", "F\u00fc\u00b7\u00dfen", "und", "schmiegt", "sich", "ihm", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "VVFIN", "PRF", "PPER", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.16": {"text": "Als ob es ihm helfen will und kann.", "tokens": ["Als", "ob", "es", "ihm", "hel\u00b7fen", "will", "und", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PPER", "VVINF", "VMFIN", "KON", "VMFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Und der Herr hat mild l\u00e4chelnd den Trost gesp\u00fcrt,", "tokens": ["Und", "der", "Herr", "hat", "mild", "l\u00e4\u00b7chelnd", "den", "Trost", "ge\u00b7sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "--+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Und er nimmt's und dr\u00e4ngt's an die Brust ger\u00fchrt", "tokens": ["Und", "er", "nimmt's", "und", "dr\u00e4ngt's", "an", "die", "Brust", "ge\u00b7r\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN", "VVPP"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Und mu\u00df es mit seiner Liebe umfassen,", "tokens": ["Und", "mu\u00df", "es", "mit", "sei\u00b7ner", "Lie\u00b7be", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Die Menschen hatten ihn verlassen.", "tokens": ["Die", "Men\u00b7schen", "hat\u00b7ten", "ihn", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}