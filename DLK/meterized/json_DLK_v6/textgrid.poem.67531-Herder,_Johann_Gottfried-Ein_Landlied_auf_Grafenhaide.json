{"textgrid.poem.67531": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Ein Landlied auf Grafenhaide", "genre": "verse", "period": "N.A.", "pub_year": 1766, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nimm mich, nimm mich, G\u00f6ttin, sanfte Freude,", "tokens": ["Nimm", "mich", ",", "nimm", "mich", ",", "G\u00f6t\u00b7tin", ",", "sanf\u00b7te", "Freu\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "VVIMP", "PPER", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ganz in Deinen Schoo\u00df!", "tokens": ["Ganz", "in", "Dei\u00b7nen", "Schoo\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Hier im Sitz der Lust, in Grafenhaide,", "tokens": ["Hier", "im", "Sitz", "der", "Lust", ",", "in", "Gra\u00b7fen\u00b7hai\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wohnst Du kummerlos,", "tokens": ["Wohnst", "Du", "kum\u00b7mer\u00b7los", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Hauchst in jedem Zuge", "tokens": ["Hauchst", "in", "je\u00b7dem", "Zu\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Sanfte Ruhe ein;", "tokens": ["Sanf\u00b7te", "Ru\u00b7he", "ein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Drum im stillen Taubenfluge", "tokens": ["Drum", "im", "stil\u00b7len", "Tau\u00b7ben\u00b7flu\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Will ich mit entz\u00fcckter Seele Dir ein Landlied weihn.", "tokens": ["Will", "ich", "mit", "ent\u00b7z\u00fcck\u00b7ter", "See\u00b7le", "Dir", "ein", "Land\u00b7lied", "weihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.9": {"text": "Alles lacht um mich; wohin ich sehe,", "tokens": ["Al\u00b7les", "lacht", "um", "mich", ";", "wo\u00b7hin", "ich", "se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPER", "$.", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Prangt mit mildem Geiz", "tokens": ["Prangt", "mit", "mil\u00b7dem", "Geiz"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Neu Vergn\u00fcgen, und wo ich nur gehe,", "tokens": ["Neu", "Ver\u00b7gn\u00fc\u00b7gen", ",", "und", "wo", "ich", "nur", "ge\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "KON", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Lacht ein neuer Reiz.", "tokens": ["Lacht", "ein", "neu\u00b7er", "Reiz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "Seht die Aehren wallen!", "tokens": ["Seht", "die", "A\u00b7eh\u00b7ren", "wal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Hin zum runden Hain!", "tokens": ["Hin", "zum", "run\u00b7den", "Hain", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.15": {"text": "Wo Naturconcerte schallen,", "tokens": ["Wo", "Na\u00b7tur\u00b7con\u00b7cer\u00b7te", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Und die Wipfel und die Zephyrs rauschen Lust darein.", "tokens": ["Und", "die", "Wip\u00b7fel", "und", "die", "Ze\u00b7phyrs", "rau\u00b7schen", "Lust", "da\u00b7rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KON", "ART", "NN", "ADJA", "NN", "PAV", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.2": {"line.1": {"text": "Rings umkr\u00e4nzt von See und Wald und Auen,", "tokens": ["Rings", "um\u00b7kr\u00e4nzt", "von", "See", "und", "Wald", "und", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Irrt umher mein Blick,", "tokens": ["Irrt", "um\u00b7her", "mein", "Blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Immer fremde; niemals satt zu schauen,", "tokens": ["Im\u00b7mer", "frem\u00b7de", ";", "nie\u00b7mals", "satt", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "$.", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Find' ich immer neues Gl\u00fcck.", "tokens": ["Find'", "ich", "im\u00b7mer", "neu\u00b7es", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O Natur! Du gl\u00e4nzest", "tokens": ["O", "Na\u00b7tur", "!", "Du", "gl\u00e4n\u00b7zest"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$.", "PPER", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Unersch\u00f6pflich reich;", "tokens": ["Un\u00b7er\u00b7sch\u00f6pf\u00b7lich", "reich", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und ein Ort, den Du bekr\u00e4nzest,", "tokens": ["Und", "ein", "Ort", ",", "den", "Du", "be\u00b7kr\u00e4n\u00b7zest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lacht der Kunst und des Gepr\u00e4nges und ist Eden gleich.", "tokens": ["Lacht", "der", "Kunst", "und", "des", "Ge\u00b7pr\u00e4n\u00b7ges", "und", "ist", "E\u00b7den", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ART", "NN", "KON", "VAFIN", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.3": {"line.1": {"text": "Zwar hier tanzt auf Rosen keine Phryne", "tokens": ["Zwar", "hier", "tanzt", "auf", "Ro\u00b7sen", "kei\u00b7ne", "Phry\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "NN", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Lauten Scherz mir vor,", "tokens": ["Lau\u00b7ten", "Scherz", "mir", "vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Keine Silberfl\u00f6te lockt ins Gr\u00fcne", "tokens": ["Kei\u00b7ne", "Sil\u00b7ber\u00b7fl\u00f6\u00b7te", "lockt", "ins", "Gr\u00fc\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "In ein Nymphenchor;", "tokens": ["In", "ein", "Nym\u00b7phen\u00b7chor", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Doch die stille Freude", "tokens": ["Doch", "die", "stil\u00b7le", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Flieht ins heitre Herz,", "tokens": ["Flieht", "ins", "heit\u00b7re", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und im muntern Hirtenkleide", "tokens": ["Und", "im", "mun\u00b7tern", "Hir\u00b7ten\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lacht sie nur auf heitern Stirnen, weit vom wilden Scherz.", "tokens": ["Lacht", "sie", "nur", "auf", "hei\u00b7tern", "Stir\u00b7nen", ",", "weit", "vom", "wil\u00b7den", "Scherz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.4": {"line.1": {"text": "Prangt, Ihr Dichter, mit ertr\u00e4umten Gr\u00fcnden,", "tokens": ["Prangt", ",", "Ihr", "Dich\u00b7ter", ",", "mit", "er\u00b7tr\u00e4um\u00b7ten", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo die Wollust thront!", "tokens": ["Wo", "die", "Wol\u00b7lust", "thront", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Euer Tempe, sagt, wo ist's zu finden,", "tokens": ["Eu\u00b7er", "Tem\u00b7pe", ",", "sagt", ",", "wo", "ist's", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "$,", "PWAV", "PIS", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Als im Dichtermond?", "tokens": ["Als", "im", "Dich\u00b7ter\u00b7mond", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Zwar ist Grafenhaide", "tokens": ["Zwar", "ist", "Gra\u00b7fen\u00b7hai\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Keine G\u00f6tterflur;", "tokens": ["Kei\u00b7ne", "G\u00f6t\u00b7ter\u00b7flur", ";"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Doch auf dieser Unschuldsweide", "tokens": ["Doch", "auf", "die\u00b7ser", "Un\u00b7schulds\u00b7wei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lacht in Augen und auf Stirnen nichts als Du, Natur.", "tokens": ["Lacht", "in", "Au\u00b7gen", "und", "auf", "Stir\u00b7nen", "nichts", "als", "Du", ",", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "APPR", "NN", "PIS", "KOKOM", "PPER", "$,", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.5": {"line.1": {"text": "Wenn im Abendroth der Himmel schwimmet,", "tokens": ["Wenn", "im", "A\u00b7ben\u00b7droth", "der", "Him\u00b7mel", "schwim\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "W\u00e4hl' ich Dich, o See!", "tokens": ["W\u00e4hl'", "ich", "Dich", ",", "o", "See", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "FM", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn der Silberthau auf Wiesen glimmet,", "tokens": ["Wenn", "der", "Sil\u00b7bert\u00b7hau", "auf", "Wie\u00b7sen", "glim\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "W\u00e4hl' ich Dich, Allee!", "tokens": ["W\u00e4hl'", "ich", "Dich", ",", "Al\u00b7lee", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wenn die Sonne steiget,", "tokens": ["Wenn", "die", "Son\u00b7ne", "stei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Suche ich den Wald;", "tokens": ["Su\u00b7che", "ich", "den", "Wald", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und wenn sich der Abend neiget,", "tokens": ["Und", "wenn", "sich", "der", "A\u00b7bend", "nei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "O, so bist Du, Freundschaftsh\u00fctte, mir ein Aufenthalt!", "tokens": ["O", ",", "so", "bist", "Du", ",", "Freund\u00b7schafts\u00b7h\u00fct\u00b7te", ",", "mir", "ein", "Auf\u00b7ent\u00b7halt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VAFIN", "PPER", "$,", "NN", "$,", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.6": {"line.1": {"text": "Hier als J\u00fcngling Rosenkr\u00e4nze winden,", "tokens": ["Hier", "als", "J\u00fcng\u00b7ling", "Ro\u00b7sen\u00b7kr\u00e4n\u00b7ze", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ist ein K\u00f6nigreich;", "tokens": ["Ist", "ein", "K\u00f6\u00b7nig\u00b7reich", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Hier sein Leben neu verj\u00fcngt empfinden,", "tokens": ["Hier", "sein", "Le\u00b7ben", "neu", "ver\u00b7j\u00fcngt", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJD", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sagt, was ist dem gleich?", "tokens": ["Sagt", ",", "was", "ist", "dem", "gleich", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "VAFIN", "ART", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Hier, wo sich Vergn\u00fcgen", "tokens": ["Hier", ",", "wo", "sich", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "PRF", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Nicht mit Silber zahlt,", "tokens": ["Nicht", "mit", "Sil\u00b7ber", "zahlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und wo sich mit sanften Z\u00fcgen", "tokens": ["Und", "wo", "sich", "mit", "sanf\u00b7ten", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PRF", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Auf dem Antlitz der Bewohner treue Freundschaft malt.", "tokens": ["Auf", "dem", "Ant\u00b7litz", "der", "Be\u00b7woh\u00b7ner", "treu\u00b7e", "Freund\u00b7schaft", "malt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.7": {"line.1": {"text": "Dies schrieb ein Fremdling, der auf Grafenhaide", "tokens": ["Dies", "schrieb", "ein", "Fremd\u00b7ling", ",", "der", "auf", "Gra\u00b7fen\u00b7hai\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Ersten Livlands Landesfreude", "tokens": ["Zum", "Ers\u00b7ten", "Liv\u00b7lands", "Lan\u00b7des\u00b7freu\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Im Cirkel lieber Freunde fand.", "tokens": ["Im", "Cir\u00b7kel", "lie\u00b7ber", "Freun\u00b7de", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}