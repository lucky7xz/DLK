{"dta.poem.3112": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "157.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weltweisheit ist ein Wort, hat weder Sinn noch Kraft;", "tokens": ["Welt\u00b7weis\u00b7heit", "ist", "ein", "Wort", ",", "hat", "we\u00b7der", "Sinn", "noch", "Kraft", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "VAFIN", "KON", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Weisheit h\u00f6chster Hort ist Gotteswissenschaft.", "tokens": ["Der", "Weis\u00b7heit", "h\u00f6chs\u00b7ter", "Hort", "ist", "Got\u00b7tes\u00b7wis\u00b7sen\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Weltweisheit aber soll, damit sie Sinn erh\u00e4lt,", "tokens": ["Welt\u00b7weis\u00b7heit", "a\u00b7ber", "soll", ",", "da\u00b7mit", "sie", "Sinn", "er\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VMFIN", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Weisheit Gottes nur im Spiegel schaun der Welt.", "tokens": ["Die", "Weis\u00b7heit", "Got\u00b7tes", "nur", "im", "Spie\u00b7gel", "schaun", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iamb