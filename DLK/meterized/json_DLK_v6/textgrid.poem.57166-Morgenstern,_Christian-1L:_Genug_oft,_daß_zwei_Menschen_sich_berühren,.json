{"textgrid.poem.57166": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Genug oft, da\u00df zwei Menschen sich ber\u00fchren,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Genug oft, da\u00df zwei Menschen sich ber\u00fchren,", "tokens": ["Ge\u00b7nug", "oft", ",", "da\u00df", "zwei", "Men\u00b7schen", "sich", "be\u00b7r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "CARD", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u2013 nicht leiblich, geistig nur \u2013 da\u00df sie sich", "tokens": ["\u2013", "nicht", "leib\u00b7lich", ",", "geis\u00b7tig", "nur", "\u2013", "da\u00df", "sie", "sich"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PTKNEG", "ADJD", "$,", "ADJD", "ADV", "$(", "KOUS", "PPER", "PRF"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "da\u00df sie sich einmal gegen\u00fcberstehn \u2013", "tokens": ["da\u00df", "sie", "sich", "ein\u00b7mal", "ge\u00b7gen\u00b7\u00fc\u00b7bers\u00b7tehn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "um sich danach vielleicht auf immer zu verlieren.", "tokens": ["um", "sich", "da\u00b7nach", "viel\u00b7leicht", "auf", "im\u00b7mer", "zu", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "PAV", "ADV", "APPR", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Genug oft, da\u00df ein L\u00e4cheln Zweier Seelen", "tokens": ["Ge\u00b7nug", "oft", ",", "da\u00df", "ein", "L\u00e4\u00b7cheln", "Zwei\u00b7er", "See\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "ART", "NN", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "verm\u00e4hlt \u2013 oh nicht verm\u00e4hlt! nur dies: sie f\u00fchrt,", "tokens": ["ver\u00b7m\u00e4hlt", "\u2013", "oh", "nicht", "ver\u00b7m\u00e4hlt", "!", "nur", "dies", ":", "sie", "f\u00fchrt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$(", "FM", "PTKNEG", "VVPP", "$.", "ADV", "PDS", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "so vor einander schweigend und ersch\u00fcttert,", "tokens": ["so", "vor", "ein\u00b7an\u00b7der", "schwei\u00b7gend", "und", "er\u00b7sch\u00fct\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "da\u00df ihnen alle Wort' und W\u00fcnsche fehlen,", "tokens": ["da\u00df", "ih\u00b7nen", "al\u00b7le", "Wort'", "und", "W\u00fcn\u00b7sche", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "und jede, unaussprechlich anger\u00fchrt,", "tokens": ["und", "je\u00b7de", ",", "un\u00b7aus\u00b7sprech\u00b7lich", "an\u00b7ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "nur tief vom Zittern der verwandten zittert.", "tokens": ["nur", "tief", "vom", "Zit\u00b7tern", "der", "ver\u00b7wand\u00b7ten", "zit\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}