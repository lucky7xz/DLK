{"dta.poem.12649": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf Chlorindens unruhige N\u00e4chte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist und bleibt der schlaf ein zucker dieser welt,", "tokens": ["Es", "ist", "und", "bleibt", "der", "schlaf", "ein", "zu\u00b7cker", "die\u00b7ser", "welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "VVFIN", "ART", "NN", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An dem jedwedre brust sich mu\u00df und will vergn\u00fcgen,", "tokens": ["An", "dem", "jed\u00b7wed\u00b7re", "brust", "sich", "mu\u00df", "und", "will", "ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "VVFIN", "PRF", "VMFIN", "KON", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kein mensch lebt, welchem nicht derselbe wohlgef\u00e4llt,", "tokens": ["Kein", "mensch", "lebt", ",", "wel\u00b7chem", "nicht", "der\u00b7sel\u00b7be", "wohl\u00b7ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "PRELS", "PTKNEG", "PDAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und der bey nachte sich l\u00e4st dessen macht besiegen.", "tokens": ["Und", "der", "bey", "nach\u00b7te", "sich", "l\u00e4st", "des\u00b7sen", "macht", "be\u00b7sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "VVFIN", "PRF", "VVFIN", "PDS", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jedoch Chlorindens augen klagen,", "tokens": ["Je\u00b7doch", "Chlo\u00b7rin\u00b7dens", "au\u00b7gen", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df sie sich m\u00fcssen stets mit unruh plagen,", "tokens": ["Da\u00df", "sie", "sich", "m\u00fcs\u00b7sen", "stets", "mit", "un\u00b7ruh", "pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VMFIN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die unruh mattete sie g\u00e4ntzlich ab,", "tokens": ["Die", "un\u00b7ruh", "mat\u00b7te\u00b7te", "sie", "g\u00e4ntz\u00b7lich", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und wo die zeit nicht diesen unfall schl\u00fcsse,", "tokens": ["Und", "wo", "die", "zeit", "nicht", "die\u00b7sen", "un\u00b7fall", "schl\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PTKNEG", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "So tr\u00fcgen sie die abgematten f\u00fcsse", "tokens": ["So", "tr\u00fc\u00b7gen", "sie", "die", "ab\u00b7ge\u00b7mat\u00b7ten", "f\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Noch vor der zeit in das bestimmte grab.", "tokens": ["Noch", "vor", "der", "zeit", "in", "das", "be\u00b7stimm\u00b7te", "grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Doch la\u00df, Chlorinde! mich bey so gestalten sachen", "tokens": ["Doch", "la\u00df", ",", "Chlo\u00b7rin\u00b7de", "!", "mich", "bey", "so", "ge\u00b7stal\u00b7ten", "sa\u00b7chen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "$,", "NN", "$.", "PPER", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Die prophezeyungen von andern f\u00e4llen machen,", "tokens": ["Die", "pro\u00b7phe\u00b7ze\u00b7yun\u00b7gen", "von", "an\u00b7dern", "f\u00e4l\u00b7len", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIS", "VVINF", "VVINF", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Die unruh plaget dich, weil du viel hertzen plagst,", "tokens": ["Die", "un\u00b7ruh", "pla\u00b7get", "dich", ",", "weil", "du", "viel", "hert\u00b7zen", "plagst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und ihren geist durch viel gedancken nagst.", "tokens": ["Und", "ih\u00b7ren", "geist", "durch", "viel", "ge\u00b7dan\u00b7cken", "nagst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Du f\u00fchrest die in banden", "tokens": ["Du", "f\u00fch\u00b7rest", "die", "in", "ban\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Und l\u00e4st ihr hoffnungs-schiff fast niemals landen,", "tokens": ["Und", "l\u00e4st", "ihr", "hoff\u00b7nungs\u00b7schiff", "fast", "nie\u00b7mals", "lan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Drum bringt dir itzund dein geschicke", "tokens": ["Drum", "bringt", "dir", "it\u00b7zund", "dein", "ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Davor cin h\u00e4uffgen ungel\u00fccke:", "tokens": ["Da\u00b7vor", "cin", "h\u00e4uff\u00b7gen", "un\u00b7ge\u00b7l\u00fc\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Ach da\u00df du doch nicht eher k\u00f6ntest ruhn,", "tokens": ["Ach", "da\u00df", "du", "doch", "nicht", "e\u00b7her", "k\u00f6n\u00b7test", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPER", "ADV", "PTKNEG", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Bi\u00df du mir wolst, was ich verlange, thun", "tokens": ["Bi\u00df", "du", "mir", "wolst", ",", "was", "ich", "ver\u00b7lan\u00b7ge", ",", "thun"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Und mich zur zahl, die so du liebest, zehlen,", "tokens": ["Und", "mich", "zur", "zahl", ",", "die", "so", "du", "lie\u00b7best", ",", "zeh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Was gilts? es w\u00fcrden dich die n\u00e4chte nicht mehr qu\u00e4len.", "tokens": ["Was", "gilts", "?", "es", "w\u00fcr\u00b7den", "dich", "die", "n\u00e4ch\u00b7te", "nicht", "mehr", "qu\u00e4\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$.", "PPER", "VAFIN", "PPER", "ART", "ADJA", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Berathe dich, was hier zu schl\u00fcssen,", "tokens": ["Be\u00b7ra\u00b7the", "dich", ",", "was", "hier", "zu", "schl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Denn eines wirst du doch ergreiffen m\u00fcssen,", "tokens": ["Denn", "ei\u00b7nes", "wirst", "du", "doch", "er\u00b7greif\u00b7fen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PPER", "ADV", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Erwehle dir davor entweder mehr die nacht,", "tokens": ["Er\u00b7weh\u00b7le", "dir", "da\u00b7vor", "ent\u00b7we\u00b7der", "mehr", "die", "nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die dich mit ihrer noth fast gar zu leiche macht;", "tokens": ["Die", "dich", "mit", "ih\u00b7rer", "noth", "fast", "gar", "zu", "lei\u00b7che", "macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "ADV", "ADV", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wo nicht? so la\u00df mich deine liebe sp\u00fcren,", "tokens": ["Wo", "nicht", "?", "so", "la\u00df", "mich", "dei\u00b7ne", "lie\u00b7be", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$.", "ADV", "VVIMP", "PPER", "PPOSAT", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Denn wird sich alles di\u00df verliehren,", "tokens": ["Denn", "wird", "sich", "al\u00b7les", "di\u00df", "ver\u00b7lieh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PRF", "PIS", "PDS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Was vor den ha\u00df verdient,", "tokens": ["Was", "vor", "den", "ha\u00df", "ver\u00b7dient", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.30": {"text": "Ich bin der baum, auf dem dein wohlseyn gr\u00fcnt.", "tokens": ["Ich", "bin", "der", "baum", ",", "auf", "dem", "dein", "wohl\u00b7seyn", "gr\u00fcnt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "APPR", "ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Alsdenn wirst du vergn\u00e4get ruhn,", "tokens": ["Als\u00b7denn", "wirst", "du", "ver\u00b7gn\u00e4\u00b7get", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Die tr\u00e4ume werden dich erfreuen,", "tokens": ["Die", "tr\u00e4u\u00b7me", "wer\u00b7den", "dich", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Die tr\u00e4ume, die uns sanffte thun,", "tokens": ["Die", "tr\u00e4u\u00b7me", ",", "die", "uns", "sanff\u00b7te", "thun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Wenn sie was artiges in unsre seele streuen.", "tokens": ["Wenn", "sie", "was", "ar\u00b7ti\u00b7ges", "in", "uns\u00b7re", "see\u00b7le", "streu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "ADJA", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die nacht bricht an,", "tokens": ["Die", "nacht", "bricht", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.36": {"text": "Das bette wartet schon auf die Chlorinde,", "tokens": ["Das", "bet\u00b7te", "war\u00b7tet", "schon", "auf", "die", "Chlo\u00b7rin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.37": {"text": "Doch ehe du betritst des schlafes bahn,", "tokens": ["Doch", "e\u00b7he", "du", "be\u00b7tritst", "des", "schla\u00b7fes", "bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "So mache vor, da\u00df da dein geist auch ruhe finde.", "tokens": ["So", "ma\u00b7che", "vor", ",", "da\u00df", "da", "dein", "geist", "auch", "ru\u00b7he", "fin\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "KOUS", "ADV", "PPOSAT", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Sonst wird dir diese nacht noch zehnmal \u00e4rger seyn,", "tokens": ["Sonst", "wird", "dir", "die\u00b7se", "nacht", "noch", "zehn\u00b7mal", "\u00e4r\u00b7ger", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PDAT", "NN", "ADV", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Dieweil mein hertze will zum schlaf um rache schreyn.", "tokens": ["Die\u00b7weil", "mein", "hert\u00b7ze", "will", "zum", "schlaf", "um", "ra\u00b7che", "schreyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VMFIN", "APPRART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}