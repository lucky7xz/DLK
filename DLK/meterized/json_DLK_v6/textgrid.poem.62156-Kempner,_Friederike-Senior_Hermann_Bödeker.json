{"textgrid.poem.62156": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Senior Hermann B\u00f6deker", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Goethes Anblick \u00fcberrascht,", "tokens": ["Von", "Goe\u00b7thes", "An\u00b7blick", "\u00fc\u00b7berr\u00b7ascht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach Worten einst ein Kaiser hascht,", "tokens": ["Nach", "Wor\u00b7ten", "einst", "ein", "Kai\u00b7ser", "hascht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er sah ihn lange forschend an,", "tokens": ["Er", "sah", "ihn", "lan\u00b7ge", "for\u00b7schend", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und rief dann aus: \u00bbDas ist ein Mann!\u00ab", "tokens": ["Und", "rief", "dann", "aus", ":", "\u00bb", "Das", "ist", "ein", "Mann", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "$.", "$(", "PDS", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein wahrer Mensch \u2013 ja, ja, ganz recht", "tokens": ["Ein", "wah\u00b7rer", "Mensch", "\u2013", "ja", ",", "ja", ",", "ganz", "recht"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "PTKANT", "$,", "PTKANT", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Menschen Typus ganz und echt,", "tokens": ["Des", "Men\u00b7schen", "Ty\u00b7pus", "ganz", "und", "echt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Tr\u00e4gt an der Stirn ein geistig Mal", "tokens": ["Tr\u00e4gt", "an", "der", "Stirn", "ein", "geis\u00b7tig", "Mal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "ADJD", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Von seinem innern Ideal:", "tokens": ["Von", "sei\u00b7nem", "in\u00b7nern", "I\u00b7deal", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wenn Wahrheit k\u00fcndend die Gestalt", "tokens": ["Wenn", "Wahr\u00b7heit", "k\u00fcn\u00b7dend", "die", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Sch\u00f6ne zeigt mit Allgewalt, \u2013", "tokens": ["Das", "Sch\u00f6\u00b7ne", "zeigt", "mit", "All\u00b7ge\u00b7walt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und tatverk\u00fcndend vor uns tritt", "tokens": ["Und", "tat\u00b7ver\u00b7k\u00fcn\u00b7dend", "vor", "uns", "tritt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit menschlich sch\u00f6nem Heldenschritt.", "tokens": ["Mit", "menschlich", "sch\u00f6\u00b7nem", "Hel\u00b7den\u00b7schritt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dann sind wir tief und froh bewegt,", "tokens": ["Dann", "sind", "wir", "tief", "und", "froh", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und unser Herz nur Segen hegt:", "tokens": ["Und", "un\u00b7ser", "Herz", "nur", "Se\u00b7gen", "hegt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn selten ist der Anblick nur", "tokens": ["Denn", "sel\u00b7ten", "ist", "der", "An\u00b7blick", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In uns'rer kleinlichen Natur.", "tokens": ["In", "un\u00b7s'\u00b7rer", "klein\u00b7li\u00b7chen", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Man r\u00fchmt als gro\u00dfe Seltenheit", "tokens": ["Man", "r\u00fchmt", "als", "gro\u00b7\u00dfe", "Sel\u00b7ten\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das G\u00f6tterfeuer Menschlichkeit, \u2013", "tokens": ["Das", "G\u00f6t\u00b7ter\u00b7feu\u00b7er", "Menschlich\u00b7keit", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "(es reiht Geschlecht sich an Geschlecht,", "tokens": ["(", "es", "reiht", "Ge\u00b7schlecht", "sich", "an", "Ge\u00b7schlecht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "NN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Selbsts\u00fcchtig, kleinlich, ungerecht!)", "tokens": ["Selbst\u00b7s\u00fcch\u00b7tig", ",", "klein\u00b7lich", ",", "un\u00b7ge\u00b7recht", "!", ")"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ihr Funken hat fast ausgespr\u00fcht,", "tokens": ["Ihr", "Fun\u00b7ken", "hat", "fast", "aus\u00b7ge\u00b7spr\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er lodert nicht, und nicht er gl\u00fcht \u2013", "tokens": ["Er", "lo\u00b7dert", "nicht", ",", "und", "nicht", "er", "gl\u00fcht", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KON", "PTKNEG", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als Irrlicht nur noch auf dem Plan,", "tokens": ["Als", "Irr\u00b7licht", "nur", "noch", "auf", "dem", "Plan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Stirbt er im Sumpf \u2013 im dunklen Wahn. \u2013", "tokens": ["Stirbt", "er", "im", "Sumpf", "\u2013", "im", "dunk\u00b7len", "Wahn", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$(", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie anders ist's bei Dir: hinauf", "tokens": ["Wie", "an\u00b7ders", "ist's", "bei", "Dir", ":", "hin\u00b7auf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ADV", "VAFIN", "APPR", "PPER", "$.", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Himmel schl\u00e4gt die Flamme auf,", "tokens": ["Zum", "Him\u00b7mel", "schl\u00e4gt", "die", "Flam\u00b7me", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als Leuchte spendend rings ihr Licht,", "tokens": ["Als", "Leuch\u00b7te", "spen\u00b7dend", "rings", "ihr", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Name, er verl\u00f6schet nicht!", "tokens": ["Dein", "Na\u00b7me", ",", "er", "ver\u00b7l\u00f6\u00b7schet", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Es sitzt der Dichter zu Gericht,", "tokens": ["Es", "sitzt", "der", "Dich\u00b7ter", "zu", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Urteil schreibt er im Gedicht,", "tokens": ["Sein", "Ur\u00b7teil", "schreibt", "er", "im", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wer dem Ideale gleicht,", "tokens": ["Und", "wer", "dem", "I\u00b7dea\u00b7le", "gleicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Begeistert er die Palme reicht.", "tokens": ["Be\u00b7geis\u00b7tert", "er", "die", "Pal\u00b7me", "reicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}