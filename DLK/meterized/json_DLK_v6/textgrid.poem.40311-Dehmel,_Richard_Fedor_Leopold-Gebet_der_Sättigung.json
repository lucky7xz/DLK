{"textgrid.poem.40311": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Gebet der S\u00e4ttigung", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun verging der Stern der Fr\u00fche,", "tokens": ["Nun", "ver\u00b7ging", "der", "Stern", "der", "Fr\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "meine Augenlider brennen;", "tokens": ["mei\u00b7ne", "Au\u00b7gen\u00b7li\u00b7der", "bren\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und die Sonne kann mit M\u00fche", "tokens": ["und", "die", "Son\u00b7ne", "kann", "mit", "M\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VMFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "die gefrornen Nebel trennen.", "tokens": ["die", "ge\u00b7fror\u00b7nen", "Ne\u00b7bel", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mich verdrie\u00dft mein n\u00e4chtlich Br\u00fcten;", "tokens": ["Mich", "ver\u00b7drie\u00dft", "mein", "n\u00e4cht\u00b7lich", "Br\u00fc\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "dr\u00fcben an den H\u00e4userw\u00e4nden", "tokens": ["dr\u00fc\u00b7ben", "an", "den", "H\u00e4u\u00b7ser\u00b7w\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "sprie\u00dfen diamantne Bl\u00fcten.", "tokens": ["sprie\u00b7\u00dfen", "di\u00b7a\u00b7mant\u00b7ne", "Bl\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Meine Pr\u00fcfung kann nun enden! \u2013", "tokens": ["Mei\u00b7ne", "Pr\u00fc\u00b7fung", "kann", "nun", "en\u00b7den", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dieser Keller: dumpfer Zwinger!", "tokens": ["Die\u00b7ser", "Kel\u00b7ler", ":", "dum\u00b7pfer", "Zwin\u00b7ger", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf die dunstbelaufnen Scheiben", "tokens": ["Auf", "die", "dunst\u00b7be\u00b7lauf\u00b7nen", "Schei\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "will ich breit mit steifem Finger", "tokens": ["will", "ich", "breit", "mit", "stei\u00b7fem", "Fin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Venus Rediviva schreiben!", "tokens": ["Ve\u00b7nus", "Re\u00b7di\u00b7vi\u00b7va", "schrei\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Denn ich wei\u00df, du bist Astarte,", "tokens": ["Denn", "ich", "wei\u00df", ",", "du", "bist", "As\u00b7tar\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "deren wir in Ketten spotten,", "tokens": ["de\u00b7ren", "wir", "in", "Ket\u00b7ten", "spot\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "du von Anbeginn, du harte", "tokens": ["du", "von", "An\u00b7be\u00b7ginn", ",", "du", "har\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "APPR", "NN", "$,", "PPER", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "G\u00f6ttin, die nicht auszurotten.", "tokens": ["G\u00f6t\u00b7tin", ",", "die", "nicht", "aus\u00b7zu\u00b7rot\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PTKNEG", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aber Ich war weich wie gl\u00fchend Eisen;", "tokens": ["A\u00b7ber", "Ich", "war", "weich", "wie", "gl\u00fc\u00b7hend", "Ei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "KOKOM", "ADJD", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "darum sollst du mich in Wasser tauchen,", "tokens": ["da\u00b7rum", "sollst", "du", "mich", "in", "Was\u00b7ser", "tau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "bis mein Wille l\u00e4\u00dft sein siedendes Kreisen", "tokens": ["bis", "mein", "Wil\u00b7le", "l\u00e4\u00dft", "sein", "sie\u00b7den\u00b7des", "Krei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "und der Stahl wird, den wir brauchen.", "tokens": ["und", "der", "Stahl", "wird", ",", "den", "wir", "brau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Nicht mehr will ich meine Brunst kasteien,", "tokens": ["Nicht", "mehr", "will", "ich", "mei\u00b7ne", "Brunst", "kas\u00b7tei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "die dann mit berauschter Durstgeberde", "tokens": ["die", "dann", "mit", "be\u00b7rauschter", "Durst\u00b7ge\u00b7ber\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "w\u00fcnscht, da\u00df unsre L\u00fcste fruchtbar seien", "tokens": ["w\u00fcnscht", ",", "da\u00df", "uns\u00b7re", "L\u00fcs\u00b7te", "frucht\u00b7bar", "sei\u00b7en"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPOSAT", "NN", "ADJD", "VAFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "und ein Wurm zur G\u00f6ttin werde.", "tokens": ["und", "ein", "Wurm", "zur", "G\u00f6t\u00b7tin", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Nach der Nacht der blinden S\u00fcchte", "tokens": ["Nach", "der", "Nacht", "der", "blin\u00b7den", "S\u00fcch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "seh ich nun mit klaren blo\u00dfen", "tokens": ["seh", "ich", "nun", "mit", "kla\u00b7ren", "blo\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Augen meine Willensfr\u00fcchte;", "tokens": ["Au\u00b7gen", "mei\u00b7ne", "Wil\u00b7lens\u00b7fr\u00fcch\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "denn ich bin wie jene gro\u00dfen", "tokens": ["denn", "ich", "bin", "wie", "je\u00b7ne", "gro\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "KOKOM", "PDAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Tagraubv\u00f6gel, die zum Fliegen", "tokens": ["Ta\u00b7graub\u00b7v\u00f6\u00b7gel", ",", "die", "zum", "Flie\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "sich nur schwer vom Boden heben,", "tokens": ["sich", "nur", "schwer", "vom", "Bo\u00b7den", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "aber, wenn sie aufgestiegen,", "tokens": ["a\u00b7ber", ",", "wenn", "sie", "auf\u00b7ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "frei und leicht und sicher schweben.", "tokens": ["frei", "und", "leicht", "und", "si\u00b7cher", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Glitzernd winkt mein Horst \u2013 Du Eine,", "tokens": ["Glit\u00b7zernd", "winkt", "mein", "Horst", "\u2013", "Du", "Ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$(", "PPER", "ART", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "die ich liebe: Ja und Amen:", "tokens": ["die", "ich", "lie\u00b7be", ":", "Ja", "und", "A\u00b7men", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "VVFIN", "$.", "PTKANT", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "heute komm ich! heut soll meine", "tokens": ["heu\u00b7te", "komm", "ich", "!", "heut", "soll", "mei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ADV", "VMFIN", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Klarheit Deinen Schoo\u00df besamen!", "tokens": ["Klar\u00b7heit", "Dei\u00b7nen", "Schoo\u00df", "be\u00b7sa\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Schon err\u00f6tet dort der Giebel;", "tokens": ["Schon", "er\u00b7r\u00f6\u00b7tet", "dort", "der", "Gie\u00b7bel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sonne, mach ein bischen schneller!", "tokens": ["Son\u00b7ne", ",", "mach", "ein", "bi\u00b7schen", "schnel\u00b7ler", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbschuster \u2013 bring mir meine Stiebel,", "tokens": ["\u00bb", "schus\u00b7ter", "\u2013", "bring", "mir", "mei\u00b7ne", "Stie\u00b7bel", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$(", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "heut verlass'ich deinen Keller!\u00ab", "tokens": ["heut", "ver\u00b7lass'\u00b7ich", "dei\u00b7nen", "Kel\u00b7ler", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}