{"dta.poem.12900": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine in ihrem busen verwelcken-  \n de rose.  \n Aus dem welschen des   Alessandro Gatti.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du ausbund sch\u00f6ner rosen!", "tokens": ["Du", "aus\u00b7bund", "sch\u00f6\u00b7ner", "ro\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kan es wol m\u00f6glich seyn, da\u00df du auf Daphnens brust", "tokens": ["Kan", "es", "wol", "m\u00f6g\u00b7lich", "seyn", ",", "da\u00df", "du", "auf", "Daph\u00b7nens", "brust"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$,", "KOUS", "PPER", "APPR", "NE", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der wohllust sanfften sitz ber\u00fchrest,", "tokens": ["Der", "wohl\u00b7lust", "sanff\u00b7ten", "sitz", "be\u00b7r\u00fch\u00b7rest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und dennoch safft und krafft verliehrest?", "tokens": ["Und", "den\u00b7noch", "safft", "und", "krafft", "ver\u00b7lieh\u00b7rest", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ach k\u00e4m\u2019 es Daphnen an, mir also liebzukosen!", "tokens": ["Ach", "k\u00e4m'", "es", "Daph\u00b7nen", "an", ",", "mir", "al\u00b7so", "lieb\u00b7zu\u00b7ko\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "NN", "PTKVZ", "$,", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So f\u00e4nd\u2019 ich gantz gewi\u00df, wo du verschmachten must,", "tokens": ["So", "f\u00e4nd'", "ich", "gantz", "ge\u00b7wi\u00df", ",", "wo", "du", "ver\u00b7schmach\u00b7ten", "must", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$,", "PWAV", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den himmel meines lebens;", "tokens": ["Den", "him\u00b7mel", "mei\u00b7nes", "le\u00b7bens", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Allein ich seuffze, w\u00fcnsch\u2019 und hoffe nur vergebens.", "tokens": ["Al\u00b7lein", "ich", "seuff\u00b7ze", ",", "w\u00fcn\u00b7sch'", "und", "hof\u00b7fe", "nur", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Denn sie, als feindin meiner ruh,", "tokens": ["Denn", "sie", ",", "als", "fein\u00b7din", "mei\u00b7ner", "ruh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "KOUS", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Denckt, allem ansehn nach, mir nur die h\u00f6lle zu.", "tokens": ["Denckt", ",", "al\u00b7lem", "an\u00b7sehn", "nach", ",", "mir", "nur", "die", "h\u00f6l\u00b7le", "zu", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PIS", "VVFIN", "PTKVZ", "$,", "PPER", "ADV", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}