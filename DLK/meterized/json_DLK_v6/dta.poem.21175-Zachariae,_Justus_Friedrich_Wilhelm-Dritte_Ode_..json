{"dta.poem.21175": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Dritte Ode .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht verzweifelungsvoll, oder des s\u00fc\u00dfesten Gl\u00fccks", "tokens": ["Nicht", "ver\u00b7zwei\u00b7fe\u00b7lungs\u00b7voll", ",", "o\u00b7der", "des", "s\u00fc\u00b7\u00dfes\u00b7ten", "Gl\u00fccks"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "$,", "KON", "ART", "ADJA", "NN"], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ungewi\u00df, klaget mein z\u00e4rtliches Herz;", "tokens": ["Un\u00b7ge\u00b7wi\u00df", ",", "kla\u00b7get", "mein", "z\u00e4rt\u00b7li\u00b7ches", "Herz", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Nein, ich werde geliebt, und nun, da sie mich liebt,", "tokens": ["Nein", ",", "ich", "wer\u00b7de", "ge\u00b7liebt", ",", "und", "nun", ",", "da", "sie", "mich", "liebt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "VVPP", "$,", "KON", "ADV", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Bin ich doch dreymal ungl\u00fccklicher noch!", "tokens": ["Bin", "ich", "doch", "drey\u00b7mal", "un\u00b7gl\u00fcck\u00b7li\u00b7cher", "noch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "ADV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Daphne, liebe mich nicht! Ueber uns h\u00e4nget voll Nacht", "tokens": ["Daph\u00b7ne", ",", "lie\u00b7be", "mich", "nicht", "!", "Ue\u00b7ber", "uns", "h\u00e4n\u00b7get", "voll", "Nacht"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPER", "PTKNEG", "$.", "APPR", "PPER", "VVFIN", "ADJD", "NN"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Schrecklich ein eiserner Himmel herab.", "tokens": ["Schreck\u00b7lich", "ein", "ei\u00b7ser\u00b7ner", "Him\u00b7mel", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Nicht ein g\u00fctiger Stral schimmert uns hinter der Nacht,", "tokens": ["Nicht", "ein", "g\u00fc\u00b7ti\u00b7ger", "Stral", "schim\u00b7mert", "uns", "hin\u00b7ter", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Furcht und Entsetzen schwebt rund um uns her.", "tokens": ["Furcht", "und", "Ent\u00b7set\u00b7zen", "schwebt", "rund", "um", "uns", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+--+-+---+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "O parteyisches Gl\u00fcck, warum l\u00e4chelst du nie", "tokens": ["O", "par\u00b7tey\u00b7isc\u00b7hes", "Gl\u00fcck", ",", "wa\u00b7rum", "l\u00e4\u00b7chelst", "du", "nie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PWAV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Liebender Unschuld und standhafter Treu?", "tokens": ["Lie\u00b7ben\u00b7der", "Un\u00b7schuld", "und", "stand\u00b7haf\u00b7ter", "Treu", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Ists der Z\u00e4rtlichkeit Loos, immer vom t\u00f6dtlichen Gram,", "tokens": ["Ists", "der", "Z\u00e4rt\u00b7lich\u00b7keit", "Loos", ",", "im\u00b7mer", "vom", "t\u00f6dt\u00b7li\u00b7chen", "Gram", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NE", "$,", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Langsam gequ\u00e4let, das Opfer zu seyn?", "tokens": ["Lang\u00b7sam", "ge\u00b7qu\u00e4\u00b7let", ",", "das", "Op\u00b7fer", "zu", "seyn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Jetzo, da du mich liebst, Daphne, fa\u00dft mich mein", "tokens": ["Jet\u00b7zo", ",", "da", "du", "mich", "liebst", ",", "Daph\u00b7ne", ",", "fa\u00dft", "mich", "mein"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,", "NN", "$,", "VVFIN", "PPER", "PPOSAT"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Un\u00fcberwindlich, wie sprech ich ihn aus!", "tokens": ["Un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7lich", ",", "wie", "sprech", "ich", "ihn", "aus", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ach! du liebest nur den, welchen ein pl\u00f6tzlicher Sturm", "tokens": ["Ach", "!", "du", "lie\u00b7best", "nur", "den", ",", "wel\u00b7chen", "ein", "pl\u00f6tz\u00b7li\u00b7cher", "Sturm"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "ADV", "ART", "$,", "PWAT", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Auf den betr\u00fcgrischen Wellen ergrif;", "tokens": ["Auf", "den", "be\u00b7tr\u00fcg\u00b7ri\u00b7schen", "Wel\u00b7len", "er\u00b7grif", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Grausam schmi\u00df ihn der Sturm von dem zaubrischen", "tokens": ["Grau\u00b7sam", "schmi\u00df", "ihn", "der", "Sturm", "von", "dem", "zaub\u00b7ri\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "ADJA"], "meter": "+-+--+--+--", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "An den verw\u00fcsteten Felsen hinan;", "tokens": ["An", "den", "ver\u00b7w\u00fcs\u00b7te\u00b7ten", "Fel\u00b7sen", "hi\u00b7nan", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "---+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Jhn ergreift sein Geschick, ach! und der eiserne Arm", "tokens": ["Jhn", "er\u00b7greift", "sein", "Ge\u00b7schick", ",", "ach", "!", "und", "der", "ei\u00b7ser\u00b7ne", "Arm"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "ITJ", "$.", "KON", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Schmiedet ihn fest an den blutigen Fels.", "tokens": ["Schmie\u00b7det", "ihn", "fest", "an", "den", "blu\u00b7ti\u00b7gen", "Fels", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}