{"textgrid.poem.38242": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Ein Heller und ein Pfenning", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott gr\u00fc\u00df euch all ihr Herren,", "tokens": ["Gott", "gr\u00fc\u00df", "euch", "all", "ihr", "Her\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und die darinnen sind;", "tokens": ["Und", "die", "da\u00b7rin\u00b7nen", "sind", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gott tr\u00f6st' die betr\u00fcbten Seelen,", "tokens": ["Gott", "tr\u00f6st'", "die", "be\u00b7tr\u00fcb\u00b7ten", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die in dem Fegfeuer sind.", "tokens": ["Die", "in", "dem", "Feg\u00b7feu\u00b7er", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "Wir sind daher gegangen,", "tokens": ["Wir", "sind", "da\u00b7her", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir sind daher gesandt;", "tokens": ["Wir", "sind", "da\u00b7her", "ge\u00b7sandt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir bettlen f\u00fcr die Krone,", "tokens": ["Wir", "bett\u00b7len", "f\u00fcr", "die", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die noch steht in Himmelshand.", "tokens": ["Die", "noch", "steht", "in", "Him\u00b7mels\u00b7hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "F\u00fcr einen Schleier heischen wir,", "tokens": ["F\u00fcr", "ei\u00b7nen", "Schlei\u00b7er", "hei\u00b7schen", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und eine sch\u00f6ne Kron;", "tokens": ["Und", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Kron", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu Sponheim in der Kirche", "tokens": ["Zu", "Spon\u00b7heim", "in", "der", "Kir\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Maria soll sie han.", "tokens": ["Die", "Ma\u00b7ria", "soll", "sie", "han", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VMFIN", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Maria K\u00f6niginn,", "tokens": ["Ma\u00b7ria", "K\u00f6\u00b7ni\u00b7ginn", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Sie ist eine reine Magd;", "tokens": ["Sie", "ist", "ei\u00b7ne", "rei\u00b7ne", "Magd", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie kann gar treulich bitten,", "tokens": ["Sie", "kann", "gar", "treu\u00b7lich", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr unsre Missethat.", "tokens": ["F\u00fcr", "uns\u00b7re", "Mis\u00b7se\u00b7that", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ein Heller und ein Pfenning,", "tokens": ["Ein", "Hel\u00b7ler", "und", "ein", "Pfen\u00b7ning", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das ist ein kleiner Werth;", "tokens": ["Das", "ist", "ein", "klei\u00b7ner", "Werth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Maria K\u00f6niginne,", "tokens": ["Ma\u00b7ria", "K\u00f6\u00b7ni\u00b7gin\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ist aller Ehren werth!", "tokens": ["Ist", "al\u00b7ler", "Eh\u00b7ren", "werth", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wir danken f\u00fcr die Gaben,", "tokens": ["Wir", "dan\u00b7ken", "f\u00fcr", "die", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die ihr uns habt gethan;", "tokens": ["Die", "ihr", "uns", "habt", "ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gott wirds an euren Seelen", "tokens": ["Gott", "wirds", "an", "eu\u00b7ren", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Euch zum Guten lassen stahn.", "tokens": ["Euch", "zum", "Gu\u00b7ten", "las\u00b7sen", "stahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}