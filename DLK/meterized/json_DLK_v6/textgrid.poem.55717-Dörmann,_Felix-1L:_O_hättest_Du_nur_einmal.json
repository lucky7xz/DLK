{"textgrid.poem.55717": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: O h\u00e4ttest Du nur einmal", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O h\u00e4ttest Du nur einmal", "tokens": ["O", "h\u00e4t\u00b7test", "Du", "nur", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "ADV", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die brennende Stirn mir gek\u00fchlt,", "tokens": ["Die", "bren\u00b7nen\u00b7de", "Stirn", "mir", "ge\u00b7k\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Entflohen w\u00e4ren die Geister,", "tokens": ["Ent\u00b7flo\u00b7hen", "w\u00e4\u00b7ren", "die", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die meine Seele zerw\u00fchlt.", "tokens": ["Die", "mei\u00b7ne", "See\u00b7le", "zer\u00b7w\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Du aber hast mit Grausen", "tokens": ["Du", "a\u00b7ber", "hast", "mit", "Grau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von mir Dich abgewandt,", "tokens": ["Von", "mir", "Dich", "ab\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und weigerst dem Irrenden, Kranken", "tokens": ["Und", "wei\u00b7gerst", "dem", "Ir\u00b7ren\u00b7den", ",", "Kran\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "F\u00fcr immer die rettende Hand.", "tokens": ["F\u00fcr", "im\u00b7mer", "die", "ret\u00b7ten\u00b7de", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Zerstoben ist und zerronnen", "tokens": ["Zer\u00b7sto\u00b7ben", "ist", "und", "zer\u00b7ron\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "KON", "VVINF"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Der Traum vom reinen Gl\u00fcck,", "tokens": ["Der", "Traum", "vom", "rei\u00b7nen", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wieder bin ich verfallen", "tokens": ["Und", "wie\u00b7der", "bin", "ich", "ver\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dem alten, w\u00fcsten Geschick.", "tokens": ["Dem", "al\u00b7ten", ",", "w\u00fcs\u00b7ten", "Ge\u00b7schick", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Wohlan, Ihr D\u00e4monen der Sinne,", "tokens": ["Wo\u00b7hlan", ",", "Ihr", "D\u00e4\u00b7mo\u00b7nen", "der", "Sin\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Steigt auf, hebt Euch empor,", "tokens": ["Steigt", "auf", ",", "hebt", "Euch", "em\u00b7por", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich will mein Bewu\u00dftsein ert\u00f6ten,", "tokens": ["Ich", "will", "mein", "Be\u00b7wu\u00df\u00b7tsein", "er\u00b7t\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Vergessen, was ich verlor.", "tokens": ["Ver\u00b7ges\u00b7sen", ",", "was", "ich", "ver\u00b7lor", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}