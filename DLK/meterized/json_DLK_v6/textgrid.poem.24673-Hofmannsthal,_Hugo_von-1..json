{"textgrid.poem.24673": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Bl\u00fcten schlafen am Baume", "tokens": ["Die", "Bl\u00fc\u00b7ten", "schla\u00b7fen", "am", "Bau\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In schw\u00fcler, fl\u00fcsternder Nacht,", "tokens": ["In", "schw\u00fc\u00b7ler", ",", "fl\u00fcs\u00b7tern\u00b7der", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sie trinken in duftigem Traume", "tokens": ["Sie", "trin\u00b7ken", "in", "duf\u00b7ti\u00b7gem", "Trau\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die flimmernde, feuchte Pracht.", "tokens": ["Die", "flim\u00b7mern\u00b7de", ",", "feuch\u00b7te", "Pracht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sie trinken den lauen Regen,", "tokens": ["Sie", "trin\u00b7ken", "den", "lau\u00b7en", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Den glitzernden Mondenschein,", "tokens": ["Den", "glit\u00b7zern\u00b7den", "Mon\u00b7den\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Sie zittern dem Licht entgegen,", "tokens": ["Sie", "zit\u00b7tern", "dem", "Licht", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Sie saugen es taumelnd ein:", "tokens": ["Sie", "sau\u00b7gen", "es", "tau\u00b7melnd", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Sie sprengen die schweigende H\u00fclle", "tokens": ["Sie", "spren\u00b7gen", "die", "schwei\u00b7gen\u00b7de", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "Und gleiten berauscht durch die Luft", "tokens": ["Und", "glei\u00b7ten", "be\u00b7rauscht", "durch", "die", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.11": {"text": "Und sterben an der F\u00fclle", "tokens": ["Und", "ster\u00b7ben", "an", "der", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Von Glut und Glanz und Duft.", "tokens": ["Von", "Glut", "und", "Glanz", "und", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das war die Nacht der Tr\u00e4ume,", "tokens": ["Das", "war", "die", "Nacht", "der", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Liebe schw\u00fcl g\u00e4rende Nacht,", "tokens": ["Der", "Lie\u00b7be", "schw\u00fcl", "g\u00e4\u00b7ren\u00b7de", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da sind mit den Knospen der B\u00e4ume", "tokens": ["Da", "sind", "mit", "den", "Knos\u00b7pen", "der", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Auch meine Lieder erwacht.", "tokens": ["Auch", "mei\u00b7ne", "Lie\u00b7der", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Sie sprengten die schweigende H\u00fclle", "tokens": ["Sie", "spreng\u00b7ten", "die", "schwei\u00b7gen\u00b7de", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Und glitten berauscht durch die Luft", "tokens": ["Und", "glit\u00b7ten", "be\u00b7rauscht", "durch", "die", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Und starben an der F\u00fclle", "tokens": ["Und", "star\u00b7ben", "an", "der", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Von Glut und Glanz und Duft.", "tokens": ["Von", "Glut", "und", "Glanz", "und", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}