{"dta.poem.13252": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Kayserinn verbleibt nicht sonder Artigkeiten,", "tokens": ["Die", "Kay\u00b7se\u00b7rinn", "ver\u00b7bleibt", "nicht", "son\u00b7der", "Ar\u00b7tig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie wendet das Gesicht mit Lust auf beyde Seiten;", "tokens": ["Sie", "wen\u00b7det", "das", "Ge\u00b7sicht", "mit", "Lust", "auf", "bey\u00b7de", "Sei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Freude bricht empor; das Wunder mehret sich;", "tokens": ["Die", "Freu\u00b7de", "bricht", "em\u00b7por", ";", "das", "Wun\u00b7der", "meh\u00b7ret", "sich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Fast keine Tugend ist, die nicht auch innerlich", "tokens": ["Fast", "kei\u00b7ne", "Tu\u00b7gend", "ist", ",", "die", "nicht", "auch", "in\u00b7ner\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "$,", "PRELS", "PTKNEG", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "275Ein stilles Was empfindt. Sie fragen mit Begierde:", "tokens": ["stil\u00b7les", "Was", "emp\u00b7findt", ".", "Sie", "fra\u00b7gen", "mit", "Be\u00b7gier\u00b7de", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PWS", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Warum? woher der Flug? was endlich folgen w\u00fcrde?", "tokens": ["Wa\u00b7rum", "?", "wo\u00b7her", "der", "Flug", "?", "was", "end\u00b7lich", "fol\u00b7gen", "w\u00fcr\u00b7de", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PWAV", "ART", "NN", "$.", "PWS", "ADV", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Man sieht einander an. Trost, Vorwiz, Freud und Lust", "tokens": ["Man", "sieht", "ein\u00b7an\u00b7der", "an", ".", "Trost", ",", "Vor\u00b7wiz", ",", "Freud", "und", "Lust"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PRF", "PTKVZ", "$.", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Augen, weist es klar, da\u00df alles unbewu\u00dft.", "tokens": ["Der", "Au\u00b7gen", ",", "weist", "es", "klar", ",", "da\u00df", "al\u00b7les", "un\u00b7be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der unverhoffte Fall bringt vielerley Gedancken,", "tokens": ["Der", "un\u00b7ver\u00b7hoff\u00b7te", "Fall", "bringt", "vie\u00b7ler\u00b7ley", "Ge\u00b7dan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "280Und macht den ganzen Saal in dem Errathen wancken.", "tokens": ["macht", "den", "gan\u00b7zen", "Saal", "in", "dem", "Er\u00b7ra\u00b7then", "wan\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}