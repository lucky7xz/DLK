{"textgrid.poem.59441": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Vom heiligen Eremiten Wilhelm", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Jerusalem die Warten", "tokens": ["Von", "Je\u00b7ru\u00b7sa\u00b7lem", "die", "War\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Lagen schon in rotem Duft,", "tokens": ["La\u00b7gen", "schon", "in", "ro\u00b7tem", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stand der Patriarch im Garten,", "tokens": ["Stand", "der", "Pat\u00b7ri\u00b7arch", "im", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Glockenklang ging durch die Luft.", "tokens": ["Glo\u00b7cken\u00b7klang", "ging", "durch", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kommt ein Pilger da gezogen,", "tokens": ["Kommt", "ein", "Pil\u00b7ger", "da", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tritt zu ihm im Abendrot,", "tokens": ["Tritt", "zu", "ihm", "im", "A\u00b7ben\u00b7drot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bleich, von strupp'gem Haar umflogen,", "tokens": ["Bleich", ",", "von", "strupp'\u00b7gem", "Haar", "um\u00b7flo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bettelt um ein St\u00fccklein Brot.", "tokens": ["Bet\u00b7telt", "um", "ein", "St\u00fcck\u00b7lein", "Brot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbkommst aus Frankreich, frommer Pilger,", "tokens": ["\u00bb", "kommst", "aus", "Fran\u00b7kreich", ",", "from\u00b7mer", "Pil\u00b7ger", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6r der Heimat Laut so gern!", "tokens": ["H\u00f6r", "der", "Hei\u00b7mat", "Laut", "so", "gern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kennst du dort den Grafen Wilhelm,", "tokens": ["Kennst", "du", "dort", "den", "Gra\u00b7fen", "Wil\u00b7helm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meinen vor'gen Landesherrn?\u00ab", "tokens": ["Mei\u00b7nen", "vor'\u00b7gen", "Lan\u00b7des\u00b7herrn", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbkenn ihn wohl, er hat geschrieben", "tokens": ["\u00bb", "kenn", "ihn", "wohl", ",", "er", "hat", "ge\u00b7schrie\u00b7ben"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Feur'ge Schrift mit blut'ger Hand,", "tokens": ["Feur'\u00b7ge", "Schrift", "mit", "blut'\u00b7ger", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat aus Frankreich dich vertrieben,", "tokens": ["Hat", "aus", "Fran\u00b7kreich", "dich", "ver\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NE", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dein Kloster liegt verbrannt.\u00ab", "tokens": ["Und", "dein", "Klos\u00b7ter", "liegt", "ver\u00b7brannt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbgott im Himmel, sollt dich kennen,", "tokens": ["\u00bb", "gott", "im", "Him\u00b7mel", ",", "sollt", "dich", "ken\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "APPRART", "NN", "$,", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie du so den Blick gewandt,", "tokens": ["Wie", "du", "so", "den", "Blick", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bist Graf Wilhelm der Ardennen \u2013\u00ab", "tokens": ["Bist", "Graf", "Wil\u00b7helm", "der", "Ar\u00b7den\u00b7nen", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "NE", "NE", "ART", "NN", "$(", "$("], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "\u00bbalso ward ich sonst genannt.\u00ab", "tokens": ["\u00bb", "al\u00b7so", "ward", "ich", "sonst", "ge\u00b7nannt", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbo mein lieber Herr, am Grabe", "tokens": ["\u00bb", "o", "mein", "lie\u00b7ber", "Herr", ",", "am", "Gra\u00b7be"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "FM", "PPOSAT", "ADJA", "NN", "$,", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stehen beid als S\u00fcnder wir \u2013", "tokens": ["Ste\u00b7hen", "beid", "als", "S\u00fcn\u00b7der", "wir", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KOKOM", "NN", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Haus und Garten, was ich habe,", "tokens": ["Haus", "und", "Gar\u00b7ten", ",", "was", "ich", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PWS", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nehmt es hin und rastet hier!\u00ab", "tokens": ["Nehmt", "es", "hin", "und", "ras\u00b7tet", "hier", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbbet f\u00fcr mich, ich darf nicht rasten,", "tokens": ["\u00bb", "bet", "f\u00fcr", "mich", ",", "ich", "darf", "nicht", "ras\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "PPER", "$,", "PPER", "VMFIN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn ohn Rasten geht die Zeit,", "tokens": ["Denn", "ohn", "Ras\u00b7ten", "geht", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hart mit Gei\u00dfeln, Wachen, Fasten", "tokens": ["Hart", "mit", "Gei\u00b7\u00dfeln", ",", "Wa\u00b7chen", ",", "Fas\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NE", "APPR", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieg ich mit der H\u00f6ll in Streit.", "tokens": ["Lieg", "ich", "mit", "der", "H\u00f6ll", "in", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Kron und Land lie\u00df ich den Erben,", "tokens": ["Kron", "und", "Land", "lie\u00df", "ich", "den", "Er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df mit st\u00fcrmender Gewalt", "tokens": ["Mu\u00df", "mit", "st\u00fcr\u00b7men\u00b7der", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mir ein andres Reich erwerben.\u00ab \u2013", "tokens": ["Mir", "ein", "and\u00b7res", "Reich", "er\u00b7wer\u00b7ben", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVINF", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und so schritt er fort zum Wald.", "tokens": ["Und", "so", "schritt", "er", "fort", "zum", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKVZ", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}