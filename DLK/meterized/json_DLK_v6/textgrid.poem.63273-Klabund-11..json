{"textgrid.poem.63273": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eiche,", "tokens": ["Ei\u00b7che", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Du fassest Wurzeln", "tokens": ["Du", "fas\u00b7sest", "Wur\u00b7zeln"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und stehst.", "tokens": ["Und", "stehst", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Uns aber treibt", "tokens": ["Uns", "a\u00b7ber", "treibt"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ein Unruh", "tokens": ["Ein", "Un\u00b7ruh"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Und Verlangen", "tokens": ["Und", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Von hier nach dort.", "tokens": ["Von", "hier", "nach", "dort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Mir ruft die H\u00f6he,", "tokens": ["Mir", "ruft", "die", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Mir ruft die Tiefe,", "tokens": ["Mir", "ruft", "die", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Der Engel der Mitte", "tokens": ["Der", "En\u00b7gel", "der", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Begnadet mich nicht.", "tokens": ["Be\u00b7gna\u00b7det", "mich", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Zerrissen, zerrissen,", "tokens": ["Zer\u00b7ris\u00b7sen", ",", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ich fasse am Ende", "tokens": ["Ich", "fas\u00b7se", "am", "En\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die knochigen H\u00e4nde", "tokens": ["Die", "kno\u00b7chi\u00b7gen", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Des fraulichen Tods.", "tokens": ["Des", "frau\u00b7li\u00b7chen", "Tods", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Aus meinem Grabe", "tokens": ["Aus", "mei\u00b7nem", "Gra\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die S\u00e4fte sie steigen", "tokens": ["Die", "S\u00e4f\u00b7te", "sie", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "In deine Wurzeln,", "tokens": ["In", "dei\u00b7ne", "Wur\u00b7zeln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Best\u00e4ndige Eiche.", "tokens": ["Be\u00b7st\u00e4n\u00b7di\u00b7ge", "Ei\u00b7che", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "So finde ich Ruhe", "tokens": ["So", "fin\u00b7de", "ich", "Ru\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und St\u00e4rke", "tokens": ["Und", "St\u00e4r\u00b7ke"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "In dir.", "tokens": ["In", "dir", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "PPER", "$."], "meter": "-+", "measure": "iambic.single"}}}}}