{"textgrid.poem.61769": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Der Spielmann", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie sagen, im Freien einst lag er zu Nacht,", "tokens": ["Sie", "sa\u00b7gen", ",", "im", "Frei\u00b7en", "einst", "lag", "er", "zu", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPRART", "NN", "ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da haben ihm Feien die Fiedel gebracht,", "tokens": ["Da", "ha\u00b7ben", "ihm", "Fei\u00b7en", "die", "Fie\u00b7del", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Da hat auf den Klippen bei Monduntergang", "tokens": ["Da", "hat", "auf", "den", "Klip\u00b7pen", "bei", "Mond\u00b7un\u00b7ter\u00b7gang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Der Nix ihm die Lippen gel\u00f6st zum Gesang.", "tokens": ["Der", "Nix", "ihm", "die", "Lip\u00b7pen", "ge\u00b7l\u00f6st", "zum", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "PPER", "ART", "NN", "VVPP", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Nun geigt er und singt er, nun singt er und geigt,", "tokens": ["Nun", "geigt", "er", "und", "singt", "er", ",", "nun", "singt", "er", "und", "geigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "KON", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Herzen bezwingt er, sobald er sich zeigt;", "tokens": ["Die", "Her\u00b7zen", "be\u00b7zwingt", "er", ",", "so\u00b7bald", "er", "sich", "zeigt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Im Dorf an der Linde, im F\u00fcrstenpalast,", "tokens": ["Im", "Dorf", "an", "der", "Lin\u00b7de", ",", "im", "F\u00fcrs\u00b7ten\u00b7pa\u00b7last", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NE", "$,", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wie dr\u00e4ngt sich geschwinde der Schwarm um den Gast!", "tokens": ["Wie", "dr\u00e4ngt", "sich", "ge\u00b7schwin\u00b7de", "der", "Schwarm", "um", "den", "Gast", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ADJA", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Schon hebt er den Bogen, schon weckt er den Schall,", "tokens": ["Schon", "hebt", "er", "den", "Bo\u00b7gen", ",", "schon", "weckt", "er", "den", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da str\u00f6mt es wie Wogen aus klarem Kristall;", "tokens": ["Da", "str\u00f6mt", "es", "wie", "Wo\u00b7gen", "aus", "kla\u00b7rem", "Kris\u00b7tall", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wie schwellen die reinen so stark und so weich!", "tokens": ["Wie", "schwel\u00b7len", "die", "rei\u00b7nen", "so", "stark", "und", "so", "weich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wer's h\u00f6rt, der mu\u00df weinen und jauchzen zugleich.", "tokens": ["Wer's", "h\u00f6rt", ",", "der", "mu\u00df", "wei\u00b7nen", "und", "jauch\u00b7zen", "zu\u00b7gleich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ART", "VMFIN", "VVINF", "KON", "VVFIN", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Was l\u00e4chelt vor Wonne der Greis dort und schw\u00e4rmt?", "tokens": ["Was", "l\u00e4\u00b7chelt", "vor", "Won\u00b7ne", "der", "Greis", "dort", "und", "schw\u00e4rmt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "NN", "ART", "NN", "ADV", "KON", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Er tr\u00e4umt, da\u00df die Sonne der Jugend ihn w\u00e4rmt.", "tokens": ["Er", "tr\u00e4umt", ",", "da\u00df", "die", "Son\u00b7ne", "der", "Ju\u00b7gend", "ihn", "w\u00e4rmt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Was blickt in die Runde der Kriegsmann so k\u00fchn?", "tokens": ["Was", "blickt", "in", "die", "Run\u00b7de", "der", "Kriegs\u00b7mann", "so", "k\u00fchn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Vom Siegsfeld die Wunde beginnt ihm zu gl\u00fchn.", "tokens": ["Vom", "Siegs\u00b7feld", "die", "Wun\u00b7de", "be\u00b7ginnt", "ihm", "zu", "gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Was staunen befangen die Knaben im Kreis?", "tokens": ["Was", "stau\u00b7nen", "be\u00b7fan\u00b7gen", "die", "Kna\u00b7ben", "im", "Kreis", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Was brennt auf den Wangen der M\u00e4dchen so hei\u00df?", "tokens": ["Was", "brennt", "auf", "den", "Wan\u00b7gen", "der", "M\u00e4d\u00b7chen", "so", "hei\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Im bangenden Sinne die Lust und die Qual,", "tokens": ["Im", "ban\u00b7gen\u00b7den", "Sin\u00b7ne", "die", "Lust", "und", "die", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Den Zauber der Minne verstehn sie zumal.", "tokens": ["Den", "Zau\u00b7ber", "der", "Min\u00b7ne", "ver\u00b7stehn", "sie", "zu\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Dem Weidmann erklingt es wie gr\u00fc\u00dfendes Horn,", "tokens": ["Dem", "Weid\u00b7mann", "er\u00b7klingt", "es", "wie", "gr\u00fc\u00b7\u00dfen\u00b7des", "Horn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Den Schnitter umsingt es wie Wachteln im Korn,", "tokens": ["Den", "Schnit\u00b7ter", "um\u00b7singt", "es", "wie", "Wach\u00b7teln", "im", "Korn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KOKOM", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Den Schiffer am Lande bef\u00e4llt's wie ein Weh,", "tokens": ["Den", "Schif\u00b7fer", "am", "Lan\u00b7de", "be\u00b7f\u00e4llt's", "wie", "ein", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Er h\u00f6rt das Gebrande der rollenden See.", "tokens": ["Er", "h\u00f6rt", "das", "Ge\u00b7bran\u00b7de", "der", "rol\u00b7len\u00b7den", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Und wo sich im Kreise verblutet ein Herz,", "tokens": ["Und", "wo", "sich", "im", "Krei\u00b7se", "ver\u00b7blu\u00b7tet", "ein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PRF", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da k\u00fchlt ihm die Weise den brennenden Schmerz;", "tokens": ["Da", "k\u00fchlt", "ihm", "die", "Wei\u00b7se", "den", "bren\u00b7nen\u00b7den", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Aufatmet's betroffen, als tr\u00e4ufelte mild", "tokens": ["Auf\u00b7at\u00b7met's", "be\u00b7trof\u00b7fen", ",", "als", "tr\u00e4u\u00b7fel\u00b7te", "mild"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVPP", "$,", "KOUS", "ADJA", "NN"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Balsamisches Hoffen vom Sternengefild.", "tokens": ["Bal\u00b7sa\u00b7mi\u00b7sches", "Hof\u00b7fen", "vom", "Ster\u00b7nen\u00b7ge\u00b7fild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Wie Adlersgefieder jetzt schwingt sich der Schall,", "tokens": ["Wie", "Ad\u00b7lers\u00b7ge\u00b7fie\u00b7der", "jetzt", "schwingt", "sich", "der", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Jetzt s\u00e4uselt er nieder wie Tropfen im Fall;", "tokens": ["Jetzt", "s\u00e4u\u00b7selt", "er", "nie\u00b7der", "wie", "Trop\u00b7fen", "im", "Fall", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KOKOM", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "So wandeln die Boten des J\u00fcngsten Gerichts,", "tokens": ["So", "wan\u00b7deln", "die", "Bo\u00b7ten", "des", "J\u00fcng\u00b7sten", "Ge\u00b7richts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "So gr\u00fc\u00dfen die Toten vom Orte des Lichts.", "tokens": ["So", "gr\u00fc\u00b7\u00dfen", "die", "To\u00b7ten", "vom", "Or\u00b7te", "des", "Lichts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.9": {"line.1": {"text": "Nun sterben die Kl\u00e4nge, nun schweigen sie ganz \u2013", "tokens": ["Nun", "ster\u00b7ben", "die", "Kl\u00e4n\u00b7ge", ",", "nun", "schwei\u00b7gen", "sie", "ganz", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da jubelt die Menge, da bringt sie den Kranz;", "tokens": ["Da", "ju\u00b7belt", "die", "Men\u00b7ge", ",", "da", "bringt", "sie", "den", "Kranz", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Doch stolz sich verneigend, als dr\u00fcck' ihn der Lohn,", "tokens": ["Doch", "stolz", "sich", "ver\u00b7nei\u00b7gend", ",", "als", "dr\u00fcck", "ihn", "der", "Lohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "VVPP", "$,", "KOUS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ins Dunkel ist schweigend der Spielmann entflohn.", "tokens": ["Ins", "Dun\u00b7kel", "ist", "schwei\u00b7gend", "der", "Spiel\u00b7mann", "ent\u00b7flohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.10": {"line.1": {"text": "Beim Glanze der Sterne, von Winden umrauscht,", "tokens": ["Beim", "Glan\u00b7ze", "der", "Ster\u00b7ne", ",", "von", "Win\u00b7den", "um\u00b7rauscht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Schon wandert er ferne, wo niemand ihm lauscht;", "tokens": ["Schon", "wan\u00b7dert", "er", "fer\u00b7ne", ",", "wo", "nie\u00b7mand", "ihm", "lauscht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PIS", "PPER", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Da geigt er in Tr\u00e4nen sich selbst noch ein St\u00fcck:", "tokens": ["Da", "geigt", "er", "in", "Tr\u00e4\u00b7nen", "sich", "selbst", "noch", "ein", "St\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "PRF", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Verlorenes Sehnen, begrabenes Gl\u00fcck.", "tokens": ["Ver\u00b7lo\u00b7re\u00b7nes", "Seh\u00b7nen", ",", "be\u00b7gra\u00b7be\u00b7nes", "Gl\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}