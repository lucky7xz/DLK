{"textgrid.poem.37440": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "\u00c4rgerlich", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus der M\u00fchle schaut der M\u00fcller,", "tokens": ["Aus", "der", "M\u00fch\u00b7le", "schaut", "der", "M\u00fcl\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der so gerne mahlen will.", "tokens": ["Der", "so", "ger\u00b7ne", "mah\u00b7len", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stiller wird der Wind und stiller,", "tokens": ["Stil\u00b7ler", "wird", "der", "Wind", "und", "stil\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die M\u00fchle stehet still.", "tokens": ["Und", "die", "M\u00fch\u00b7le", "ste\u00b7het", "still", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "So geht's immer, wie ich finde,", "tokens": ["So", "geht's", "im\u00b7mer", ",", "wie", "ich", "fin\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rief der M\u00fcller voller Zorn.", "tokens": ["Rief", "der", "M\u00fcl\u00b7ler", "vol\u00b7ler", "Zorn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat man Korn, so fehlt's am Winde,", "tokens": ["Hat", "man", "Korn", ",", "so", "fehlt's", "am", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "NN", "$,", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat man Wind, so fehlt das Korn.", "tokens": ["Hat", "man", "Wind", ",", "so", "fehlt", "das", "Korn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}