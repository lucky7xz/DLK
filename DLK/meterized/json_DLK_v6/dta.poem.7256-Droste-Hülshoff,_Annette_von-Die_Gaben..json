{"dta.poem.7256": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Die Gaben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Nie fand, so oft auch scherzend ward gefragt,", "tokens": ["Nie", "fand", ",", "so", "oft", "auch", "scher\u00b7zend", "ward", "ge\u00b7fragt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "ADV", "ADV", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich einen Mann, vom Grafen bis zum Schneider,", "tokens": ["Ich", "ei\u00b7nen", "Mann", ",", "vom", "Gra\u00b7fen", "bis", "zum", "Schnei\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "APPRART", "NN", "APPR", "APPRART", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der so bescheiden oder so betagt,", "tokens": ["Der", "so", "be\u00b7schei\u00b7den", "o\u00b7der", "so", "be\u00b7tagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So h\u00fclflos, keinen so Gescheiten leider,", "tokens": ["So", "h\u00fcl\u00b7flos", ",", "kei\u00b7nen", "so", "Ge\u00b7schei\u00b7ten", "lei\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PIAT", "ADV", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der nicht gemeint, des Herrscherthumes B\u00fcrde", "tokens": ["Der", "nicht", "ge\u00b7meint", ",", "des", "Herr\u00b7scher\u00b7thu\u00b7mes", "B\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PTKNEG", "VVPP", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sey seinen Schultern grad das rechte Maa\u00df.", "tokens": ["Sey", "sei\u00b7nen", "Schul\u00b7tern", "grad", "das", "rech\u00b7te", "Maa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "War Einer zweifelnd je an seiner W\u00fcrde,", "tokens": ["War", "Ei\u00b7ner", "zwei\u00b7felnd", "je", "an", "sei\u00b7ner", "W\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So sch\u00e4tzt er seine Kr\u00e4fte desto ba\u00df,", "tokens": ["So", "sch\u00e4tzt", "er", "sei\u00b7ne", "Kr\u00e4f\u00b7te", "des\u00b7to", "ba\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der hoffte auf der Rede Zauberbann;", "tokens": ["Der", "hoff\u00b7te", "auf", "der", "Re\u00b7de", "Zau\u00b7ber\u00b7bann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schlau aus dem Winkel wollte Jener zielen,", "tokens": ["Schlau", "aus", "dem", "Win\u00b7kel", "woll\u00b7te", "Je\u00b7ner", "zie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VMFIN", "PDAT", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Kurz, da\u00df er wisse wie und auch den Mann,", "tokens": ["Kurz", ",", "da\u00df", "er", "wis\u00b7se", "wie", "und", "auch", "den", "Mann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "PPER", "VVFIN", "KOKOM", "KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Lie\u00df Jeder deutlich durch die Blume spielen.", "tokens": ["Lie\u00df", "Je\u00b7der", "deut\u00b7lich", "durch", "die", "Blu\u00b7me", "spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ihr Thoren! glaubt ihr denn da\u00df Gott im Zorne", "tokens": ["Ihr", "Tho\u00b7ren", "!", "glaubt", "ihr", "denn", "da\u00df", "Gott", "im", "Zor\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPER", "ADV", "KOUS", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Gro\u00dfen schuf, ungleich der Menschenschaar,", "tokens": ["Die", "Gro\u00b7\u00dfen", "schuf", ",", "un\u00b7gleich", "der", "Men\u00b7schen\u00b7schaar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hinstreckt wie weiland Nebukadnezar?", "tokens": ["Hins\u00b7treckt", "wie", "wei\u00b7land", "Ne\u00b7bu\u00b7kad\u00b7ne\u00b7zar", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Da\u00df, weil zuweilen unter Zotten schl\u00e4gt", "tokens": ["Da\u00df", ",", "weil", "zu\u00b7wei\u00b7len", "un\u00b7ter", "Zot\u00b7ten", "schl\u00e4gt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "ADV", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Herz wo gro\u00dfe Elemente schlafen,", "tokens": ["Ein", "Herz", "wo", "gro\u00b7\u00dfe", "E\u00b7le\u00b7men\u00b7te", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "De\u00dfhalb wer eine feine Wolle tr\u00e4gt", "tokens": ["De\u00df\u00b7halb", "wer", "ei\u00b7ne", "fei\u00b7ne", "Wol\u00b7le", "tr\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PWS", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Unfehlbar z\u00e4hlt zu den Merinoschafen?", "tokens": ["Un\u00b7fehl\u00b7bar", "z\u00e4hlt", "zu", "den", "Me\u00b7ri\u00b7no\u00b7scha\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "Da\u00df langes Schauen zweifellos erblinde,", "tokens": ["Da\u00df", "lan\u00b7ges", "Schau\u00b7en", "zwei\u00b7fel\u00b7los", "er\u00b7blin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wer den F\u00e4den rastlos nachgesp\u00fcrt,", "tokens": ["Und", "wer", "den", "F\u00e4\u00b7den", "rast\u00b7los", "nach\u00b7ge\u00b7sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df dieser, gleich dem \u00fcberreizten Kinde,", "tokens": ["Da\u00df", "die\u00b7ser", ",", "gleich", "dem", "\u00fc\u00b7berr\u00b7eiz\u00b7ten", "Kin\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So d\u00fcmmer wird je l\u00e4nger er studirt?", "tokens": ["So", "d\u00fcm\u00b7mer", "wird", "je", "l\u00e4n\u00b7ger", "er", "stu\u00b7dirt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ADV", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wer zweifelt, da\u00df ein Herz wie's Throne schm\u00fcckt", "tokens": ["Wer", "zwei\u00b7felt", ",", "da\u00df", "ein", "Herz", "wie's", "Thro\u00b7ne", "schm\u00fcckt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gar oft am Acker fr\u00f6hnt und Forstgehege,", "tokens": ["Gar", "oft", "am", "A\u00b7cker", "fr\u00f6hnt", "und", "Forst\u00b7ge\u00b7he\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df manche Scheitel sich zur Furche b\u00fcckt,", "tokens": ["Da\u00df", "man\u00b7che", "Schei\u00b7tel", "sich", "zur", "Fur\u00b7che", "b\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hochwerth da\u00df eine Krone drauf man lege?", "tokens": ["Hoch\u00b7werth", "da\u00df", "ei\u00b7ne", "Kro\u00b7ne", "drauf", "man", "le\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "ART", "NN", "PAV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.8": {"line.1": {"text": "Doch ihr des Lebens abgehetzte Alten,", "tokens": ["Doch", "ihr", "des", "Le\u00b7bens", "ab\u00b7ge\u00b7hetz\u00b7te", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr innerliche Greise, seyd es nicht.", "tokens": ["Ihr", "in\u00b7ner\u00b7li\u00b7che", "Grei\u00b7se", ",", "seyd", "es", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bewahr' der Himmel uns vor eurem Walten,", "tokens": ["Be\u00b7wahr'", "der", "Him\u00b7mel", "uns", "vor", "eu\u00b7rem", "Wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor dem im Sumpfe angebrannten Licht!", "tokens": ["Vor", "dem", "im", "Sump\u00b7fe", "an\u00b7ge\u00b7brann\u00b7ten", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Ihr w\u00fcrdet mahnen an des Fr\u00f6hners Sohn,", "tokens": ["Ihr", "w\u00fcr\u00b7det", "mah\u00b7nen", "an", "des", "Fr\u00f6h\u00b7ners", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der, woll' ihm Gott ein K\u00f6nigreich verschreiben,", "tokens": ["Der", ",", "woll'", "ihm", "Gott", "ein", "K\u00f6\u00b7nig\u00b7reich", "ver\u00b7schrei\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VMFIN", "PPER", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr's Leben w\u00fcste keinen bessern Lohn,", "tokens": ["F\u00fcr's", "Le\u00b7ben", "w\u00fcs\u00b7te", "kei\u00b7nen", "bes\u00b7sern", "Lohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als seine Schweine dann zu Ro\u00df zu treiben.", "tokens": ["Als", "sei\u00b7ne", "Schwei\u00b7ne", "dann", "zu", "Ro\u00df", "zu", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}