{"textgrid.poem.40046": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Thrax, dem die Jahre schon das Haar mit Schnee bestreuen,", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Thrax, dem die Jahre schon das Haar mit Schnee bestreuen,", "tokens": ["Thrax", ",", "dem", "die", "Jah\u00b7re", "schon", "das", "Haar", "mit", "Schnee", "be\u00b7streu\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "ADV", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Baut H\u00e4user, leget G\u00e4rten an;", "tokens": ["Baut", "H\u00e4u\u00b7ser", ",", "le\u00b7get", "G\u00e4r\u00b7ten", "an", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kauft, was er nur erdencken kann;", "tokens": ["Kauft", ",", "was", "er", "nur", "er\u00b7den\u00b7cken", "kann", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kauft Haus-Raht, Spiegel, Schildereyen.", "tokens": ["Kauft", "Haus\u00b7Raht", ",", "Spie\u00b7gel", ",", "Schil\u00b7de\u00b7re\u00b7yen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Was soll die\u00df alles ihm f\u00fcr Freude geben,", "tokens": ["Was", "soll", "die\u00df", "al\u00b7les", "ihm", "f\u00fcr", "Freu\u00b7de", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PDS", "PIS", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da er bereits mit einem Fuss' im Grabe?", "tokens": ["Da", "er", "be\u00b7reits", "mit", "ei\u00b7nem", "Fuss'", "im", "Gra\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nichts, als da\u00df, beym Verlust von seinem Leben,", "tokens": ["Nichts", ",", "als", "da\u00df", ",", "beym", "Ver\u00b7lust", "von", "sei\u00b7nem", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOKOM", "KOUS", "$,", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Er desto mehr nur zu verliehren habe.", "tokens": ["Er", "des\u00b7to", "mehr", "nur", "zu", "ver\u00b7lieh\u00b7ren", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}