{"textgrid.poem.39801": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Zur Todesfeier der verewigten K\u00f6nigin Katharina von W\u00fcrttemberg", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Herzen Zug, die gern zusammentrauern,", "tokens": ["Der", "Her\u00b7zen", "Zug", ",", "die", "gern", "zu\u00b7sam\u00b7men\u00b7trau\u00b7ern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hat euch zu diesem ernsten Fest verb\u00fcndet;", "tokens": ["Hat", "euch", "zu", "die\u00b7sem", "erns\u00b7ten", "Fest", "ver\u00b7b\u00fcn\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PDAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr kamt, des Redners Wort in diesen Mauern", "tokens": ["Ihr", "kamt", ",", "des", "Red\u00b7ners", "Wort", "in", "die\u00b7sen", "Mau\u00b7ern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu h\u00f6ren, das die Sehnsucht neu entz\u00fcndet.", "tokens": ["Zu", "h\u00f6\u00b7ren", ",", "das", "die", "Sehn\u00b7sucht", "neu", "ent\u00b7z\u00fcn\u00b7det", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PRELS", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dann hat Musik mit ihren heil'gen Schauern", "tokens": ["Dann", "hat", "Mu\u00b7sik", "mit", "ih\u00b7ren", "heil'\u00b7gen", "Schau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Euch euer eigenes Gef\u00fchl verk\u00fcndet.", "tokens": ["Euch", "eu\u00b7er", "ei\u00b7ge\u00b7nes", "Ge\u00b7f\u00fchl", "ver\u00b7k\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Die Dichtkunst schlie\u00dft sich an der T\u00f6ne Klagen:", "tokens": ["Die", "Dicht\u00b7kunst", "schlie\u00dft", "sich", "an", "der", "T\u00f6\u00b7ne", "Kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Was ist ihr Amt, was soll die Muse sagen?", "tokens": ["Was", "ist", "ihr", "Amt", ",", "was", "soll", "die", "Mu\u00b7se", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "NN", "$,", "PWS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ihr himmlisch Vorrecht ist, in ew'ges Leben", "tokens": ["Ihr", "himm\u00b7lisch", "Vor\u00b7recht", "ist", ",", "in", "ew'\u00b7ges", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJD", "NN", "VAFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Sch\u00f6ne, das der Welt entflieht, zu fassen:", "tokens": ["Das", "Sch\u00f6\u00b7ne", ",", "das", "der", "Welt", "ent\u00b7flieht", ",", "zu", "fas\u00b7sen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Nachempfindung soll sie Dauer geben,", "tokens": ["Der", "Na\u00b7ch\u00b7emp\u00b7fin\u00b7dung", "soll", "sie", "Dau\u00b7er", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Die uns vom Guten bleibt, das uns verlassen;", "tokens": ["Die", "uns", "vom", "Gu\u00b7ten", "bleibt", ",", "das", "uns", "ver\u00b7las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie soll sein Bild aus goldnen Stralen weben,", "tokens": ["Sie", "soll", "sein", "Bild", "aus", "gold\u00b7nen", "Stra\u00b7len", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und vor die Augen halten es, die nassen;", "tokens": ["Und", "vor", "die", "Au\u00b7gen", "hal\u00b7ten", "es", ",", "die", "nas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sie soll die F\u00fcrstin, \u00fcber der wir weinen,", "tokens": ["Sie", "soll", "die", "F\u00fcrs\u00b7tin", ",", "\u00fc\u00b7ber", "der", "wir", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Im Glanz des Lebens lassen uns erscheinen.", "tokens": ["Im", "Glanz", "des", "Le\u00b7bens", "las\u00b7sen", "uns", "er\u00b7schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Drum weg aus diesen florumh\u00fcllten W\u00e4nden,", "tokens": ["Drum", "weg", "aus", "die\u00b7sen", "flo\u00b7rum\u00b7h\u00fcll\u00b7ten", "W\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Muse f\u00fchret uns in jene Zeit,", "tokens": ["Die", "Mu\u00b7se", "f\u00fch\u00b7ret", "uns", "in", "je\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo Kunde kam, da\u00df an des Nordens Enden", "tokens": ["Wo", "Kun\u00b7de", "kam", ",", "da\u00df", "an", "des", "Nor\u00b7dens", "En\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "$,", "KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Volk der Flamme seine Stadt geweiht,", "tokens": ["Ein", "Volk", "der", "Flam\u00b7me", "sei\u00b7ne", "Stadt", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wo sich begann des Himmels Zorn zu wenden,", "tokens": ["Wo", "sich", "be\u00b7gann", "des", "Him\u00b7mels", "Zorn", "zu", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "VVFIN", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und wo sein Winter uns zuerst befreit.", "tokens": ["Und", "wo", "sein", "Win\u00b7ter", "uns", "zu\u00b7erst", "be\u00b7freit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Seht ihr ", "tokens": ["Seht", "ihr"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "++", "measure": "spondeus"}, "line.8": {"text": "In jener Freiheit Morgenr\u00f6te wallen?", "tokens": ["In", "je\u00b7ner", "Frei\u00b7heit", "Mor\u00b7gen\u00b7r\u00f6\u00b7te", "wal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nicht m\u00fc\u00dfig schl\u00e4gt ihr Herz nur bei den K\u00e4mpfen,", "tokens": ["Nicht", "m\u00fc\u00b7\u00dfig", "schl\u00e4gt", "ihr", "Herz", "nur", "bei", "den", "K\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie sammelt selbst, sie wappnet eine Schar,", "tokens": ["Sie", "sam\u00b7melt", "selbst", ",", "sie", "wapp\u00b7net", "ei\u00b7ne", "Schar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dem Schlachthorn folget ", "tokens": ["Dem", "Schlach\u00b7thorn", "fol\u00b7get"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und steht an Frankreichs Grenze mit dem Zar.", "tokens": ["Und", "steht", "an", "Fran\u00b7kreichs", "Gren\u00b7ze", "mit", "dem", "Zar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Jetzt unterliegt der zarte Leib den Kr\u00e4mpfen,", "tokens": ["Jetzt", "un\u00b7ter\u00b7liegt", "der", "zar\u00b7te", "Leib", "den", "Kr\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ihm droht vom m\u00e4cht'gen Geiste die Gefahr:", "tokens": ["Ihm", "droht", "vom", "m\u00e4cht'\u00b7gen", "Geis\u00b7te", "die", "Ge\u00b7fahr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da kam der Bote von Paris geeilet.", "tokens": ["Da", "kam", "der", "Bo\u00b7te", "von", "Pa\u00b7ris", "ge\u00b7ei\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und sie stand auf und wandelte geheilet.", "tokens": ["Und", "sie", "stand", "auf", "und", "wan\u00b7del\u00b7te", "ge\u00b7hei\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Den Heldenmut nach W\u00fcrdigkeit zu lohnen", "tokens": ["Den", "Hel\u00b7den\u00b7mut", "nach", "W\u00fcr\u00b7dig\u00b7keit", "zu", "loh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erschien die Liebe von des Himmels H\u00f6h'n,", "tokens": ["Er\u00b7schien", "die", "Lie\u00b7be", "von", "des", "Him\u00b7mels", "H\u00f6h'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bis sie sich einen Sieger ausersehn;", "tokens": ["Bis", "sie", "sich", "ei\u00b7nen", "Sie\u00b7ger", "au\u00b7ser\u00b7sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den K\u00f6nigssohn, dem frische Lorberkronen", "tokens": ["Den", "K\u00f6\u00b7nigs\u00b7sohn", ",", "dem", "fri\u00b7sche", "Lor\u00b7ber\u00b7kro\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Um seine jugendlichen Schl\u00e4fe wehn.", "tokens": ["Um", "sei\u00b7ne", "ju\u00b7gend\u00b7li\u00b7chen", "Schl\u00e4\u00b7fe", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Freiheitsk\u00e4mpfer hat die Braut gefunden,", "tokens": ["Der", "Frei\u00b7heits\u00b7k\u00e4mp\u00b7fer", "hat", "die", "Braut", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der heut'ge Tag ist's, ach! der ", "tokens": ["Der", "heut'\u00b7ge", "Tag", "ist's", ",", "ach", "!", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "ITJ", "$.", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "So kam die Fernbewunderte gezogen,", "tokens": ["So", "kam", "die", "Fern\u00b7be\u00b7wun\u00b7der\u00b7te", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und lie\u00df sich leuchtend sehen in dem Land;", "tokens": ["Und", "lie\u00df", "sich", "leuch\u00b7tend", "se\u00b7hen", "in", "dem", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "VVPP", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Entgegen str\u00f6mten ", "tokens": ["Ent\u00b7ge\u00b7gen", "str\u00f6m\u00b7ten"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Es deutete manch ausgestreckte Hand", "tokens": ["Es", "deu\u00b7te\u00b7te", "manch", "aus\u00b7ge\u00b7streck\u00b7te", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Dem Nordschein zu, der an des Himmels Bogen,", "tokens": ["Dem", "Nord\u00b7schein", "zu", ",", "der", "an", "des", "Him\u00b7mels", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PRELS", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch als ein Segen bringend Zeichen, stand.", "tokens": ["Doch", "als", "ein", "Se\u00b7gen", "brin\u00b7gend", "Zei\u00b7chen", ",", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "O dieses wundersch\u00f6ne Licht aus Norden,", "tokens": ["O", "die\u00b7ses", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "Licht", "aus", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Warum ist's nicht zur vollen Sonne worden?", "tokens": ["Wa\u00b7rum", "ist's", "nicht", "zur", "vol\u00b7len", "Son\u00b7ne", "wor\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PTKNEG", "APPRART", "ADJA", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Die F\u00fcrstin kam, in ", "tokens": ["Die", "F\u00fcrs\u00b7tin", "kam", ",", "in"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Nicht blos das Feuer, das den V\u00f6lkerkrieg", "tokens": ["Nicht", "blos", "das", "Feu\u00b7er", ",", "das", "den", "V\u00f6l\u00b7ker\u00b7krieg"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "w