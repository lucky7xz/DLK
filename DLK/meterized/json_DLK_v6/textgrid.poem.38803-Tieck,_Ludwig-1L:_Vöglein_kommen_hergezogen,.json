{"textgrid.poem.38803": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: V\u00f6glein kommen hergezogen,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "V\u00f6glein kommen hergezogen,", "tokens": ["V\u00f6\u00b7glein", "kom\u00b7men", "her\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Setzen sich auf d\u00fcrre Aeste: \u2013", "tokens": ["Set\u00b7zen", "sich", "auf", "d\u00fcr\u00b7re", "A\u00b7es\u00b7te", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "\u00bbweit, ach weit sind wir geflogen,", "tokens": ["\u00bb", "weit", ",", "ach", "weit", "sind", "wir", "ge\u00b7flo\u00b7gen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "ADV", "ADJD", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Angelockt vom Fr\u00fchlingsweste.\u00ab", "tokens": ["An\u00b7ge\u00b7lockt", "vom", "Fr\u00fch\u00b7lings\u00b7wes\u00b7te", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVPP", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Also klagen sie, die Kleinen:", "tokens": ["Al\u00b7so", "kla\u00b7gen", "sie", ",", "die", "Klei\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbschmetterlinge schw\u00e4rmen schon,", "tokens": ["\u00bb", "schmet\u00b7ter\u00b7lin\u00b7ge", "schw\u00e4r\u00b7men", "schon", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bienen sumsen ihren Ton,", "tokens": ["Bie\u00b7nen", "sum\u00b7sen", "ih\u00b7ren", "Ton", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Suchen Honig, finden keinen.", "tokens": ["Su\u00b7chen", "Ho\u00b7nig", ",", "fin\u00b7den", "kei\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PIAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Fr\u00fchling! Fr\u00fchling! komm hervor!", "tokens": ["Fr\u00fch\u00b7ling", "!", "Fr\u00fch\u00b7ling", "!", "komm", "her\u00b7vor", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "$.", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6re doch auf unsre Lieder,", "tokens": ["H\u00f6\u00b7re", "doch", "auf", "uns\u00b7re", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gieb uns unsre Bl\u00e4tter wieder,", "tokens": ["Gieb", "uns", "uns\u00b7re", "Bl\u00e4t\u00b7ter", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Horch, wir singen dir in's Ohr.", "tokens": ["Horch", ",", "wir", "sin\u00b7gen", "dir", "in's", "Ohr", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "K\u00f6mmt noch nicht das gr\u00fcne Laub?", "tokens": ["K\u00f6mmt", "noch", "nicht", "das", "gr\u00fc\u00b7ne", "Laub", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df die kleinen Bl\u00e4tter spielen,", "tokens": ["La\u00df", "die", "klei\u00b7nen", "Bl\u00e4t\u00b7ter", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sie warme Sonne f\u00fchlen,", "tokens": ["Da\u00df", "sie", "war\u00b7me", "Son\u00b7ne", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keines wird dem Frost zum Raub;\u00ab \u2013", "tokens": ["Kei\u00b7nes", "wird", "dem", "Frost", "zum", "Raub", ";", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "APPRART", "NN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbwas singt so lieblich leise?", "tokens": ["\u00bb", "was", "singt", "so", "lieb\u00b7lich", "lei\u00b7se", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Spricht drauf die Fr\u00fchlingswelt,", "tokens": ["Spricht", "drauf", "die", "Fr\u00fch\u00b7lings\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ist die alte Weise,", "tokens": ["Es", "ist", "die", "al\u00b7te", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie kommen von der Reise,", "tokens": ["Sie", "kom\u00b7men", "von", "der", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Keine Furcht mich r\u00fcckw\u00e4rts h\u00e4lt.\u00ab", "tokens": ["Kei\u00b7ne", "Furcht", "mich", "r\u00fcck\u00b7w\u00e4rts", "h\u00e4lt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "PPER", "ADV", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Auf thun sich gr\u00fcne Aeugelein,", "tokens": ["Auf", "thun", "sich", "gr\u00fc\u00b7ne", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PRF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Knospen sich erschlie\u00dfen", "tokens": ["Die", "Knos\u00b7pen", "sich", "er\u00b7schlie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die V\u00f6gelein zu gr\u00fc\u00dfen,", "tokens": ["Die", "V\u00f6\u00b7ge\u00b7lein", "zu", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu kosten den Sonnenschein.", "tokens": ["Zu", "kos\u00b7ten", "den", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Durch alle B\u00e4ume geht der Waldgeist", "tokens": ["Durch", "al\u00b7le", "B\u00e4u\u00b7me", "geht", "der", "Wald\u00b7geist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und sumst: Auf Kinder! Der Fr\u00fchling ist da;", "tokens": ["Und", "sumst", ":", "Auf", "Kin\u00b7der", "!", "Der", "Fr\u00fch\u00b7ling", "ist", "da", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "APPR", "NN", "$.", "ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Storch, Schwalbe, die ich schon oftmals sah,", "tokens": ["Storch", ",", "Schwal\u00b7be", ",", "die", "ich", "schon", "oft\u00b7mals", "sah", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auch Lerch' und Grasem\u00fcck' ist hergerei\u00dft.", "tokens": ["Auch", "Ler\u00b7ch'", "und", "Gra\u00b7se\u00b7m\u00fcck", "ist", "her\u00b7ge\u00b7rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Streckt ihnen die gr\u00fcnen Arm entgegen,", "tokens": ["Streckt", "ih\u00b7nen", "die", "gr\u00fc\u00b7nen", "Arm", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "La\u00dft sie wohnen wie immer, im schattigen Zelt,", "tokens": ["La\u00dft", "sie", "woh\u00b7nen", "wie", "im\u00b7mer", ",", "im", "schat\u00b7ti\u00b7gen", "Zelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "KOKOM", "ADV", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Da\u00df sie von Zweig zu Zweig sich regen,", "tokens": ["Da\u00df", "sie", "von", "Zweig", "zu", "Zweig", "sich", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "APPR", "NN", "PRF", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und jubeln und singen in frischer Welt.", "tokens": ["Und", "ju\u00b7beln", "und", "sin\u00b7gen", "in", "fri\u00b7scher", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.9": {"line.1": {"text": "Nun regt sich's und w\u00fchlt in allen Zweigen,", "tokens": ["Nun", "regt", "sich's", "und", "w\u00fchlt", "in", "al\u00b7len", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "KON", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Alle Quellen mit neuem Leben spielen,", "tokens": ["Al\u00b7le", "Quel\u00b7len", "mit", "neu\u00b7em", "Le\u00b7ben", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "In den Aesten Lust und Kraft und W\u00fchlen,", "tokens": ["In", "den", "A\u00b7es\u00b7ten", "Lust", "und", "Kraft", "und", "W\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Jeder Baum will sich vor dem andern zeigen.", "tokens": ["Je\u00b7der", "Baum", "will", "sich", "vor", "dem", "an\u00b7dern", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PRF", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.10": {"line.1": {"text": "Nun rauscht's und alle stehn in gr\u00fcner Pracht,", "tokens": ["Nun", "rauscht's", "und", "al\u00b7le", "stehn", "in", "gr\u00fc\u00b7ner", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "PIS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Abendwolken \u00fcber W\u00e4lder ziehn,", "tokens": ["Die", "Ab\u00b7end\u00b7wol\u00b7ken", "\u00fc\u00b7ber", "W\u00e4l\u00b7der", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und sch\u00f6ner durch die Wipfel gl\u00fchn,", "tokens": ["Und", "sch\u00f6\u00b7ner", "durch", "die", "Wip\u00b7fel", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der gr\u00fcne Hain von goldnem Feuer angefacht.", "tokens": ["Der", "gr\u00fc\u00b7ne", "Hain", "von", "gold\u00b7nem", "Feu\u00b7er", "an\u00b7ge\u00b7facht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Gebiert das Thal die Blumen an das Licht,", "tokens": ["Ge\u00b7biert", "das", "Thal", "die", "Blu\u00b7men", "an", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die die holde Liebe der Welt verk\u00fcnden,", "tokens": ["Die", "die", "hol\u00b7de", "Lie\u00b7be", "der", "Welt", "ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es l\u00e4chelt und winkt in stillen Gr\u00fcnden", "tokens": ["Es", "l\u00e4\u00b7chelt", "und", "winkt", "in", "stil\u00b7len", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Des sanften Veilchens Angesicht,", "tokens": ["Des", "sanf\u00b7ten", "Veil\u00b7chens", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das sinnige Vergi\u00dfmeinnicht.", "tokens": ["Das", "sin\u00b7ni\u00b7ge", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Sie sind die Winke, die s\u00fc\u00dfen Blicke,", "tokens": ["Sie", "sind", "die", "Win\u00b7ke", ",", "die", "s\u00fc\u00b7\u00dfen", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die dem Geliebten das M\u00e4dchen reicht,", "tokens": ["Die", "dem", "Ge\u00b7lieb\u00b7ten", "das", "M\u00e4d\u00b7chen", "reicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Vorboten vom zuk\u00fcnft'gen Gl\u00fccke,", "tokens": ["Vor\u00b7bo\u00b7ten", "vom", "zu\u00b7k\u00fcnft'\u00b7gen", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Auge, das schmachtend entgegen neigt.", "tokens": ["Ein", "Au\u00b7ge", ",", "das", "schmach\u00b7tend", "ent\u00b7ge\u00b7gen", "neigt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "PTKVZ", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.13": {"line.1": {"text": "Sie b\u00fccken sich mit schalkhaftem Sinn", "tokens": ["Sie", "b\u00fc\u00b7cken", "sich", "mit", "schalk\u00b7haf\u00b7tem", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und gr\u00fc\u00dfen, wer vor\u00fcbergeht,", "tokens": ["Und", "gr\u00fc\u00b7\u00dfen", ",", "wer", "vor\u00b7\