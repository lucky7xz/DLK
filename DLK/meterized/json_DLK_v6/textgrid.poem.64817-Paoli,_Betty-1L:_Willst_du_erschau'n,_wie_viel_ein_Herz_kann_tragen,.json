{"textgrid.poem.64817": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Willst du erschau'n, wie viel ein Herz kann tragen,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du erschau'n, wie viel ein Herz kann tragen,", "tokens": ["Willst", "du", "er\u00b7schau'n", ",", "wie", "viel", "ein", "Herz", "kann", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "PWAV", "ADV", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O blick' in mein's!", "tokens": ["O", "blick'", "in", "mein's", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "So reich an Wunden, vom Geschick geschlagen,", "tokens": ["So", "reich", "an", "Wun\u00b7den", ",", "vom", "Ge\u00b7schick", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "War wohl noch kein's.", "tokens": ["War", "wohl", "noch", "kein'", "s."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["VAFIN", "ADV", "ADV", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Doch mitten in den w\u00fcthendsten Orkanen", "tokens": ["Doch", "mit\u00b7ten", "in", "den", "w\u00fct\u00b7hends\u00b7ten", "Or\u00b7ka\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Erhob ich mich,", "tokens": ["Er\u00b7hob", "ich", "mich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und schritt dahin auf meinen fernen Bahnen \u2013", "tokens": ["Und", "schritt", "da\u00b7hin", "auf", "mei\u00b7nen", "fer\u00b7nen", "Bah\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie stark war ich!", "tokens": ["Wie", "stark", "war", "ich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wie ward mir doch nun so mit einem Male", "tokens": ["Wie", "ward", "mir", "doch", "nun", "so", "mit", "ei\u00b7nem", "Ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Kraft geraubt?", "tokens": ["Die", "Kraft", "ge\u00b7raubt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Es trotzte muthig dem Gewitterstrahle", "tokens": ["Es", "trotz\u00b7te", "mut\u00b7hig", "dem", "Ge\u00b7wit\u00b7ter\u00b7strah\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein stolzes Haupt,", "tokens": ["Mein", "stol\u00b7zes", "Haupt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Doch als du zu mir sprachst mit leisem Gr\u00fc\u00dfen:", "tokens": ["Doch", "als", "du", "zu", "mir", "sprachst", "mit", "lei\u00b7sem", "Gr\u00fc\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "\u00bbich liebe dich!\u00ab", "tokens": ["\u00bb", "ich", "lie\u00b7be", "dich", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Da sank ich still und weinend dir zu F\u00fc\u00dfen \u2013", "tokens": ["Da", "sank", "ich", "still", "und", "wei\u00b7nend", "dir", "zu", "F\u00fc\u00b7\u00dfen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "PPER", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie schwach bin ich!", "tokens": ["Wie", "schwach", "bin", "ich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}