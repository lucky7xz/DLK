{"textgrid.poem.61797": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Spielmanns Heimkehr", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun sch\u00fcre die Glut mir empor auf dem Herd,", "tokens": ["Nun", "sch\u00fc\u00b7re", "die", "Glut", "mir", "em\u00b7por", "auf", "dem", "Herd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Denn dahin ist die sonnige Zeit;", "tokens": ["Denn", "da\u00b7hin", "ist", "die", "son\u00b7ni\u00b7ge", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der Sturm saust \u00fcber die Halde,", "tokens": ["Der", "Sturm", "saust", "\u00fc\u00b7ber", "die", "Hal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und es fallen die Bl\u00e4tter im Walde \u2013", "tokens": ["Und", "es", "fal\u00b7len", "die", "Bl\u00e4t\u00b7ter", "im", "Wal\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$("], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "O du Jugend, wie liegst du so weit!", "tokens": ["O", "du", "Ju\u00b7gend", ",", "wie", "liegst", "du", "so", "weit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$,", "PWAV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-++--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Einst zog ich hinaus in die klingende Welt,", "tokens": ["Einst", "zog", "ich", "hin\u00b7aus", "in", "die", "klin\u00b7gen\u00b7de", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APZR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da standen die Rosen in Blust.", "tokens": ["Da", "stan\u00b7den", "die", "Ro\u00b7sen", "in", "Blust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Von der Nachtigall lernt' ich das Reisen,", "tokens": ["Von", "der", "Nach\u00b7ti\u00b7gall", "lernt'", "ich", "das", "Rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und ich habe die schmelzendsten Weisen", "tokens": ["Und", "ich", "ha\u00b7be", "die", "schmel\u00b7zends\u00b7ten", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.5": {"text": "Und die feurigsten Lieder gewu\u00dft.", "tokens": ["Und", "die", "feu\u00b7rigs\u00b7ten", "Lie\u00b7der", "ge\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "\u00bbgott gr\u00fc\u00df' euch im Gr\u00fcnen, Gott gr\u00fc\u00df' euch im Schlo\u00df!", "tokens": ["\u00bb", "gott", "gr\u00fc\u00df'", "euch", "im", "Gr\u00fc\u00b7nen", ",", "Gott", "gr\u00fc\u00df'", "euch", "im", "Schlo\u00df", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "APPRART", "NN", "$,", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Wer kredenzt mir den funkelnden Wein?", "tokens": ["Wer", "kre\u00b7denzt", "mir", "den", "fun\u00b7keln\u00b7den", "Wein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Gott gr\u00fc\u00df' euch im d\u00e4mmernden St\u00e4dtchen!", "tokens": ["Gott", "gr\u00fc\u00df'", "euch", "im", "d\u00e4m\u00b7mern\u00b7den", "St\u00e4dt\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und ich spiel' euch zum Reigen, ihr M\u00e4dchen,", "tokens": ["Und", "ich", "spiel'", "euch", "zum", "Rei\u00b7gen", ",", "ihr", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Und die Sch\u00f6nste soll K\u00f6nigin sein!", "tokens": ["Und", "die", "Sch\u00f6ns\u00b7te", "soll", "K\u00f6\u00b7ni\u00b7gin", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "NN", "VAINF", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Gott gr\u00fc\u00df' euch, ihr eisernen Reiter! Wohin", "tokens": ["Gott", "gr\u00fc\u00df'", "euch", ",", "ihr", "ei\u00b7ser\u00b7nen", "Rei\u00b7ter", "!", "Wo\u00b7hin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$.", "PWAV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Bei des Fr\u00fchlichts blutigem Rot!", "tokens": ["Bei", "des", "Fr\u00fch\u00b7lichts", "blu\u00b7ti\u00b7gem", "Rot", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "In das Feld, in die Schlacht, in das Wetter?", "tokens": ["In", "das", "Feld", ",", "in", "die", "Schlacht", ",", "in", "das", "Wet\u00b7ter", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "O so la\u00dft zum Trompetengeschmetter", "tokens": ["O", "so", "la\u00dft", "zum", "Trom\u00b7pe\u00b7ten\u00b7ge\u00b7schmet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "APPRART", "NN"], "meter": "+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Mich euch singen von Sieg und von Tod!", "tokens": ["Mich", "euch", "sin\u00b7gen", "von", "Sieg", "und", "von", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Und ihr Pfleger des Geistes mit sinnender Stirn,", "tokens": ["Und", "ihr", "Pfle\u00b7ger", "des", "Geis\u00b7tes", "mit", "sin\u00b7nen\u00b7der", "Stirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Gott gr\u00fc\u00df' euch, und reicht mir die Hand!", "tokens": ["Gott", "gr\u00fc\u00df'", "euch", ",", "und", "reicht", "mir", "die", "Hand", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Von der Sch\u00f6pfung geheiligtem Ringe,", "tokens": ["Von", "der", "Sch\u00f6p\u00b7fung", "ge\u00b7hei\u00b7lig\u00b7tem", "Rin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Von dem Wandel der irdischen Dinge", "tokens": ["Von", "dem", "Wan\u00b7del", "der", "ir\u00b7di\u00b7schen", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Hab' ich manches geschaut und erkannt.\u00ab", "tokens": ["Hab'", "ich", "man\u00b7ches", "ge\u00b7schaut", "und", "er\u00b7kannt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "PIS", "VVPP", "KON", "VVPP", "$.", "$("], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Und ich wanderte fern, wo das Haupt des Olymps", "tokens": ["Und", "ich", "wan\u00b7der\u00b7te", "fern", ",", "wo", "das", "Haupt", "des", "O\u00b7lymps"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "$,", "PWAV", "ART", "NN", "ART", "NN"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Goldschwingig der Adler umzieht,", "tokens": ["Gold\u00b7schwin\u00b7gig", "der", "Ad\u00b7ler", "um\u00b7zieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und ich trank aus dem Rhein, aus dem gr\u00fcnen,", "tokens": ["Und", "ich", "trank", "aus", "dem", "Rhein", ",", "aus", "dem", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NE", "$,", "APPR", "ART", "ADJA", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Und ich sa\u00df auf den Gr\u00e4bern der H\u00fcnen,", "tokens": ["Und", "ich", "sa\u00df", "auf", "den", "Gr\u00e4\u00b7bern", "der", "H\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Und ich sang an den Gletschern mein Lied.", "tokens": ["Und", "ich", "sang", "an", "den", "Glet\u00b7schern", "mein", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.7": {"line.1": {"text": "Doch die Jahre vergingen wie Spreu vor dem Wind,", "tokens": ["Doch", "die", "Jah\u00b7re", "ver\u00b7gin\u00b7gen", "wie", "Spreu", "vor", "dem", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KOKOM", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "M\u00fcd bin ich nach Hause gekehrt;", "tokens": ["M\u00fcd", "bin", "ich", "nach", "Hau\u00b7se", "ge\u00b7kehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ach, die einst sich gefreut mit dem Knaben,", "tokens": ["Ach", ",", "die", "einst", "sich", "ge\u00b7freut", "mit", "dem", "Kna\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PRELS", "ADV", "PRF", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Sind zerstreut, sind dahin, sind begraben,", "tokens": ["Sind", "zer\u00b7streut", ",", "sind", "da\u00b7hin", ",", "sind", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "VAFIN", "PAV", "$,", "VAFIN", "VVPP", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Und ein ander Geschlecht sitzt am Herd.", "tokens": ["Und", "ein", "an\u00b7der", "Ge\u00b7schlecht", "sitzt", "am", "Herd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.8": {"line.1": {"text": "Ich wende die Augen um und um;", "tokens": ["Ich", "wen\u00b7de", "die", "Au\u00b7gen", "um", "und", "um", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "KON", "KOUI", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wer ist, der den Alten noch kennt?", "tokens": ["Wer", "ist", ",", "der", "den", "Al\u00b7ten", "noch", "kennt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da dunkelt's am himmlischen Bogen,", "tokens": ["Da", "dun\u00b7kelt's", "am", "himm\u00b7li\u00b7schen", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und es kommen die Sterne gezogen,", "tokens": ["Und", "es", "kom\u00b7men", "die", "Ster\u00b7ne", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Und die Sterne sind treu bis ans End'.", "tokens": ["Und", "die", "Ster\u00b7ne", "sind", "treu", "bis", "ans", "End'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "APPR", "APPRART", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}}}}