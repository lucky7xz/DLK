{"textgrid.poem.41455": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Fresser", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein ber\u00fchmter Held im Fressen,", "tokens": ["Ein", "be\u00b7r\u00fchm\u00b7ter", "Held", "im", "Fres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den das Schlemmen aufgeschwellt,", "tokens": ["Den", "das", "Schlem\u00b7men", "auf\u00b7ge\u00b7schwellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hatt' einmal zum Abendessen", "tokens": ["Hatt'", "ein\u00b7mal", "zum", "A\u00b7ben\u00b7des\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sich den gr\u00f6\u00dften St\u00f6r bestellt.", "tokens": ["Sich", "den", "gr\u00f6\u00df\u00b7ten", "St\u00f6r", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieser ward sehr bald bezwungen:", "tokens": ["Die\u00b7ser", "ward", "sehr", "bald", "be\u00b7zwun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur das Kopfst\u00fcck blieb ihm nach,", "tokens": ["Nur", "das", "Kopf\u00b7st\u00fcck", "blieb", "ihm", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Das er noch nicht halb verschlungen,", "tokens": ["Das", "er", "noch", "nicht", "halb", "ver\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Als vom Krampf sein Magen brach.", "tokens": ["Als", "vom", "Krampf", "sein", "Ma\u00b7gen", "brach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jeder Garkoch wird betr\u00fcbet:", "tokens": ["Je\u00b7der", "Gar\u00b7koch", "wird", "be\u00b7tr\u00fc\u00b7bet", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mancher holt den Arzt herbei,", "tokens": ["Man\u00b7cher", "holt", "den", "Arzt", "her\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der sogleich die Antwort gibet,", "tokens": ["Der", "sog\u00b7leich", "die", "Ant\u00b7wort", "gi\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df der Bruch unheilbar sei.", "tokens": ["Da\u00df", "der", "Bruch", "un\u00b7heil\u00b7bar", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle Hoffnung ist verschwunden;", "tokens": ["Al\u00b7le", "Hoff\u00b7nung", "ist", "ver\u00b7schwun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Man verk\u00fcndigt ihm den Schlu\u00df,", "tokens": ["Man", "ver\u00b7k\u00fcn\u00b7digt", "ihm", "den", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df er in sehr wenig Stunden", "tokens": ["Da\u00df", "er", "in", "sehr", "we\u00b7nig", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unvermeidlich sterben mu\u00df.", "tokens": ["Un\u00b7ver\u00b7meid\u00b7lich", "ster\u00b7ben", "mu\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Soll die Zunge schon erkalten,", "tokens": ["Soll", "die", "Zun\u00b7ge", "schon", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die so vieles nicht geschmeckt?", "tokens": ["Die", "so", "vie\u00b7les", "nicht", "ge\u00b7schmeckt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIS", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freunde, bin ich zu erhalten?", "tokens": ["Freun\u00b7de", ",", "bin", "ich", "zu", "er\u00b7hal\u00b7ten", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder werd' ich nur erschreckt?", "tokens": ["O\u00b7der", "werd'", "ich", "nur", "er\u00b7schreckt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch, ist euer Wort untr\u00fcglich,", "tokens": ["Doch", ",", "ist", "eu\u00b7er", "Wort", "un\u00b7tr\u00fcg\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Steht des Arztes Ausspruch fest:", "tokens": ["Steht", "des", "Arz\u00b7tes", "Aus\u00b7spruch", "fest", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach so reicht mir unverz\u00fcglich", "tokens": ["Ach", "so", "reicht", "mir", "un\u00b7ver\u00b7z\u00fcg\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Meines Fischchens Ueberrest.", "tokens": ["Mei\u00b7nes", "Fischchens", "Ue\u00b7ber\u00b7rest", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}