{"textgrid.poem.64895": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich kann, was ich mu\u00df! o seltnes Geschick!", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kann, was ich mu\u00df! o seltnes Geschick!", "tokens": ["Ich", "kann", ",", "was", "ich", "mu\u00df", "!", "o", "selt\u00b7nes", "Ge\u00b7schick", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PWS", "PPER", "VMFIN", "$.", "FM", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich will, was ich mu\u00df \u2013 \u2013 o doppeltes Gl\u00fcck.", "tokens": ["Ich", "will", ",", "was", "ich", "mu\u00df", "\u2013", "\u2013", "o", "dop\u00b7pel\u00b7tes", "Gl\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PWS", "PPER", "VMFIN", "$(", "$(", "FM", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Mein Herz ist an St\u00e4rke dem Felsen gleich,", "tokens": ["Mein", "Herz", "ist", "an", "St\u00e4r\u00b7ke", "dem", "Fel\u00b7sen", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mein Herz ist, wie Blumen, sanft und weich.", "tokens": ["Mein", "Herz", "ist", ",", "wie", "Blu\u00b7men", ",", "sanft", "und", "weich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "PWAV", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Mein Wesen gleicht Glocken von strengem Metall:", "tokens": ["Mein", "We\u00b7sen", "gleicht", "Glo\u00b7cken", "von", "stren\u00b7gem", "Me\u00b7tall", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Schlag kr\u00e4ftig d'ran, gibt es auch kr\u00e4ftigen Schall.", "tokens": ["Schlag", "kr\u00e4f\u00b7tig", "d'\u00b7ran", ",", "gibt", "es", "auch", "kr\u00e4f\u00b7ti\u00b7gen", "Schall", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "$,", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Mein Geist st\u00fcrmt auf eiligem Wolkenro\u00df hin;", "tokens": ["Mein", "Geist", "st\u00fcrmt", "auf", "ei\u00b7li\u00b7gem", "Wol\u00b7ken\u00b7ro\u00df", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+-++", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mein Geist spielt mit Kindern mit kindlichem Sinn.", "tokens": ["Mein", "Geist", "spielt", "mit", "Kin\u00b7dern", "mit", "kind\u00b7li\u00b7chem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Ich wei\u00df, was ich will! und weil ich es wei\u00df,", "tokens": ["Ich", "wei\u00df", ",", "was", "ich", "will", "!", "und", "weil", "ich", "es", "wei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$.", "KON", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Drum bann' ich's zu mir in den magischen Kreis.", "tokens": ["Drum", "bann'", "ich's", "zu", "mir", "in", "den", "ma\u00b7gi\u00b7schen", "Kreis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "APPR", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Ich wei\u00df, was ich will! das ist ja die Kraft,", "tokens": ["Ich", "wei\u00df", ",", "was", "ich", "will", "!", "das", "ist", "ja", "die", "Kraft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$.", "PDS", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die sich aus dem Chaos ein Weltall entrafft.", "tokens": ["Die", "sich", "aus", "dem", "Chaos", "ein", "Wel\u00b7tall", "en\u00b7trafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-++-+", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Ich wei\u00df, was ich will! und wenn ich's erreich',", "tokens": ["Ich", "wei\u00df", ",", "was", "ich", "will", "!", "und", "wenn", "ich's", "er\u00b7reich'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$.", "KON", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dann gelten der Tod und das Leben mir gleich.", "tokens": ["Dann", "gel\u00b7ten", "der", "Tod", "und", "das", "Le\u00b7ben", "mir", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "ART", "NN", "PPER", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}