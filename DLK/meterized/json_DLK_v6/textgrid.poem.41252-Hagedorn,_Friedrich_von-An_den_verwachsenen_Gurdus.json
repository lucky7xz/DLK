{"textgrid.poem.41252": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An den verwachsenen Gurdus", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du gleichest dem Aesop; doch dein Verstand ist klein.", "tokens": ["Du", "glei\u00b7chest", "dem", "A\u00b7e\u00b7sop", ";", "doch", "dein", "Ver\u00b7stand", "ist", "klein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "ADV", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der Kern der Bucklichen r\u00e4umt dir gewi\u00df nicht ein,", "tokens": ["Der", "Kern", "der", "Buck\u00b7li\u00b7chen", "r\u00e4umt", "dir", "ge\u00b7wi\u00df", "nicht", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So dumm als h\u00f6ckericht, und dennoch stolz zu sein.", "tokens": ["So", "dumm", "als", "h\u00f6\u00b7cke\u00b7richt", ",", "und", "den\u00b7noch", "stolz", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "VVFIN", "$,", "KON", "ADV", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}