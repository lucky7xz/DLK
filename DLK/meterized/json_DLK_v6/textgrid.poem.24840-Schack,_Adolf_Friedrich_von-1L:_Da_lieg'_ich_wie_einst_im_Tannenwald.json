{"textgrid.poem.24840": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da lieg' ich wie einst im Tannenwald", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da lieg' ich wie einst im Tannenwald", "tokens": ["Da", "lieg'", "ich", "wie", "einst", "im", "Tan\u00b7nen\u00b7wald"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "ADV", "APPRART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf dem Lager von Moos und Bl\u00e4ttern;", "tokens": ["Auf", "dem", "La\u00b7ger", "von", "Moos", "und", "Bl\u00e4t\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Der Wipfel mir \u00fcberm Haupte schallt", "tokens": ["Der", "Wip\u00b7fel", "mir", "\u00fc\u00b7berm", "Haup\u00b7te", "schallt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von des Eichhorns mutigem Klettern.", "tokens": ["Von", "des", "Eich\u00b7horns", "mu\u00b7ti\u00b7gem", "Klet\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "In den Winden, wie sie von Ort zu Ort", "tokens": ["In", "den", "Win\u00b7den", ",", "wie", "sie", "von", "Ort", "zu", "Ort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "PPER", "APPR", "NN", "APPR", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Den Schatten der Aeste jagen,", "tokens": ["Den", "Schat\u00b7ten", "der", "A\u00b7es\u00b7te", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "T\u00f6nt mir im fl\u00fcsternden Laub manch Wort,", "tokens": ["T\u00f6nt", "mir", "im", "fl\u00fcs\u00b7tern\u00b7den", "Laub", "manch", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "PIAT", "NN", "$,"], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wie ein Ruf aus verschollenen Tagen.", "tokens": ["Wie", "ein", "Ruf", "aus", "ver\u00b7schol\u00b7le\u00b7nen", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Und ich f\u00fchl' in der Seele tief, o tief,", "tokens": ["Und", "ich", "f\u00fchl'", "in", "der", "See\u00b7le", "tief", ",", "o", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "ADJD", "$,", "FM", "ADJD", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ein Atemholen, ein Regen,", "tokens": ["Ein", "A\u00b7tem\u00b7ho\u00b7len", ",", "ein", "Re\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als wollte die Jugend, die l\u00e4ngst entschlief,", "tokens": ["Als", "woll\u00b7te", "die", "Ju\u00b7gend", ",", "die", "l\u00e4ngst", "ent\u00b7schlief", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Erwachend die Wimpern bewegen.", "tokens": ["Er\u00b7wa\u00b7chend", "die", "Wim\u00b7pern", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Sie richten sich auf, sie steigen empor,", "tokens": ["Sie", "rich\u00b7ten", "sich", "auf", ",", "sie", "stei\u00b7gen", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Geister, lange begraben,", "tokens": ["Die", "Geis\u00b7ter", ",", "lan\u00b7ge", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und raunen mir s\u00fc\u00dfe Laute ins Ohr;", "tokens": ["Und", "rau\u00b7nen", "mir", "s\u00fc\u00b7\u00dfe", "Lau\u00b7te", "ins", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie wollen mich wieder haben.", "tokens": ["Sie", "wol\u00b7len", "mich", "wie\u00b7der", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VAINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Fort! fort! Ihr findet den Alten nicht mehr,", "tokens": ["Fort", "!", "fort", "!", "Ihr", "fin\u00b7det", "den", "Al\u00b7ten", "nicht", "mehr", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PTKVZ", "$.", "PPER", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der einst hier lag in den Tannen!", "tokens": ["Der", "einst", "hier", "lag", "in", "den", "Tan\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein Windsto\u00df braust durch die Wipfel daher", "tokens": ["Ein", "Wind\u00b7sto\u00df", "braust", "durch", "die", "Wip\u00b7fel", "da\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "PAV"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und tr\u00e4gt die Stimmen von dannen.", "tokens": ["Und", "tr\u00e4gt", "die", "Stim\u00b7men", "von", "dan\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}