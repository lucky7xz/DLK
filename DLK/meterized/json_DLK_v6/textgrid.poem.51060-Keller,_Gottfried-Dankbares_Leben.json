{"textgrid.poem.51060": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Dankbares Leben", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sch\u00f6n, wie sch\u00f6n ist dieses kurze Leben,", "tokens": ["Wie", "sch\u00f6n", ",", "wie", "sch\u00f6n", "ist", "die\u00b7ses", "kur\u00b7ze", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "VAFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn es er\u00f6ffnet alle seine Quellen!", "tokens": ["Wenn", "es", "er\u00b7\u00f6ff\u00b7net", "al\u00b7le", "sei\u00b7ne", "Quel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Tage gleichen klaren Silberwellen,", "tokens": ["Die", "Ta\u00b7ge", "glei\u00b7chen", "kla\u00b7ren", "Sil\u00b7ber\u00b7wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die sich mit Macht zu \u00fcberholen streben.", "tokens": ["Die", "sich", "mit", "Macht", "zu", "\u00fc\u00b7berh\u00b7o\u00b7len", "stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was gestern freudig mocht das Herz erheben,", "tokens": ["Was", "ge\u00b7stern", "freu\u00b7dig", "mocht", "das", "Herz", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir m\u00fcssen's l\u00e4chelnd heute r\u00fcckw\u00e4rts stellen;", "tokens": ["Wir", "m\u00fcs\u00b7sen's", "l\u00e4\u00b7chelnd", "heu\u00b7te", "r\u00fcck\u00b7w\u00e4rts", "stel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn die Erfahrungen des Geistes schwellen,", "tokens": ["Wenn", "die", "Er\u00b7fah\u00b7run\u00b7gen", "des", "Geis\u00b7tes", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Erlebnisse gleich Blumen sie durchweben.", "tokens": ["Er\u00b7leb\u00b7nis\u00b7se", "gleich", "Blu\u00b7men", "sie", "durch\u00b7we\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "PPER", "VVINF", "$."], "meter": "-+---+-+-+-", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "So mag man breiter stets den Strom erschauen,", "tokens": ["So", "mag", "man", "brei\u00b7ter", "stets", "den", "Strom", "er\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADJD", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auch tiefer m\u00e4hlich sehn den Grund wir winken", "tokens": ["Auch", "tie\u00b7fer", "m\u00e4h\u00b7lich", "sehn", "den", "Grund", "wir", "win\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADJD", "VVFIN", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und lernen t\u00e4glich mehr der Flut vertrauen.", "tokens": ["Und", "ler\u00b7nen", "t\u00e4g\u00b7lich", "mehr", "der", "Flut", "ver\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nun zierliche Geschirre, sie zu trinken,", "tokens": ["Nun", "zier\u00b7li\u00b7che", "Ge\u00b7schir\u00b7re", ",", "sie", "zu", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Leiht, G\u00f6tter! uns, und Marmor, um zu bauen", "tokens": ["Leiht", ",", "G\u00f6t\u00b7ter", "!", "uns", ",", "und", "Mar\u00b7mor", ",", "um", "zu", "bau\u00b7en"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$.", "PPER", "$,", "KON", "NN", "$,", "KOUI", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den festen Damm zur Rechten und zur Linken!", "tokens": ["Den", "fes\u00b7ten", "Damm", "zur", "Rech\u00b7ten", "und", "zur", "Lin\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}