{"textgrid.poem.26509": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da\u00df fr\u00fch sich euer Sinn gew\u00f6hne", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df fr\u00fch sich euer Sinn gew\u00f6hne", "tokens": ["Da\u00df", "fr\u00fch", "sich", "eu\u00b7er", "Sinn", "ge\u00b7w\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "PRF", "PPOSAT", "NN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An ernsten Wortes ernsten Klang,", "tokens": ["An", "erns\u00b7ten", "Wor\u00b7tes", "erns\u00b7ten", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vernehmt jetzt meines Liedes T\u00f6ne", "tokens": ["Ver\u00b7nehmt", "jetzt", "mei\u00b7nes", "Lie\u00b7des", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In eu'rer Jugend frohem Drang.", "tokens": ["In", "eu'\u00b7rer", "Ju\u00b7gend", "fro\u00b7hem", "Drang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie m\u00f6gen euch auf allen Wegen", "tokens": ["Sie", "m\u00f6\u00b7gen", "euch", "auf", "al\u00b7len", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nachklingen leise im Gem\u00fcth,", "tokens": ["Nach\u00b7klin\u00b7gen", "lei\u00b7se", "im", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein tief empfund'ner Dichtersegen,", "tokens": ["Ein", "tief", "emp\u00b7fun\u00b7d'\u00b7ner", "Dich\u00b7ter\u00b7se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "D'raus euch des Daseins Gl\u00fcck erbl\u00fcht.", "tokens": ["D'raus", "euch", "des", "Da\u00b7seins", "Gl\u00fcck", "er\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Noch ist ein jeder Tag euch helle,", "tokens": ["Noch", "ist", "ein", "je\u00b7der", "Tag", "euch", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "PIAT", "NN", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und s\u00fc\u00dfer Schlummer jede Nacht;", "tokens": ["Und", "s\u00fc\u00b7\u00dfer", "Schlum\u00b7mer", "je\u00b7de", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch steht ihr an der Zukunft Schwelle", "tokens": ["Doch", "steht", "ihr", "an", "der", "Zu\u00b7kunft", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und m\u00e4lig schon ziemt euch Bedacht;", "tokens": ["Und", "m\u00e4\u00b7lig", "schon", "ziemt", "euch", "Be\u00b7dacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Ob ihr auch noch in kind'schen Spielen", "tokens": ["Ob", "ihr", "auch", "noch", "in", "kin\u00b7d'\u00b7schen", "Spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Auf bunten Wiesenplanen tollt \u2013:", "tokens": ["Auf", "bun\u00b7ten", "Wie\u00b7sen\u00b7pla\u00b7nen", "tollt", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es naht die Zeit mit ernsten Zielen,", "tokens": ["Es", "naht", "die", "Zeit", "mit", "erns\u00b7ten", "Zie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "F\u00fcr die ihr leben, wirken sollt.", "tokens": ["F\u00fcr", "die", "ihr", "le\u00b7ben", ",", "wir\u00b7ken", "sollt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVINF", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Zeit, wo eu'rem \u00bbHeut'\u00ab ein \u00bbMorgen\u00ab", "tokens": ["Die", "Zeit", ",", "wo", "eu'\u00b7rem", "\u00bb", "Heut'", "\u00ab", "ein", "\u00bb", "Mor\u00b7gen", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPOSAT", "$(", "ADV", "$(", "ART", "$(", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit neuer Forderung sich reiht,", "tokens": ["Mit", "neu\u00b7er", "For\u00b7de\u00b7rung", "sich", "reiht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und langsam nur, bei steten Sorgen,", "tokens": ["Und", "lang\u00b7sam", "nur", ",", "bei", "ste\u00b7ten", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein schwierig Tagewerk gedeiht.", "tokens": ["Ein", "schwie\u00b7rig", "Ta\u00b7ge\u00b7werk", "ge\u00b7deiht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann aber mu\u00df sich erst bew\u00e4hren", "tokens": ["Dann", "a\u00b7ber", "mu\u00df", "sich", "erst", "be\u00b7w\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "PRF", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die tiefste Kraft, die in euch wohnt,", "tokens": ["Die", "tiefs\u00b7te", "Kraft", ",", "die", "in", "euch", "wohnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und ob der Eltern gold'ne Lehren,", "tokens": ["Und", "ob", "der", "El\u00b7tern", "gold'\u00b7ne", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des Meisters Eifer sich gelohnt.", "tokens": ["Des", "Meis\u00b7ters", "Ei\u00b7fer", "sich", "ge\u00b7lohnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch wie auch rastlos eu're M\u00fche,", "tokens": ["Doch", "wie", "auch", "rast\u00b7los", "eu'\u00b7re", "M\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gen\u00fcg' euch die erf\u00fcllte Pflicht:", "tokens": ["Ge\u00b7n\u00fcg'", "euch", "die", "er\u00b7f\u00fcll\u00b7te", "Pflicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vergeltungskr\u00e4nze, rasche, fr\u00fche,", "tokens": ["Ver\u00b7gel\u00b7tungs\u00b7kr\u00e4n\u00b7ze", ",", "ra\u00b7sche", ",", "fr\u00fc\u00b7he", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr euer Thun erwartet nicht.", "tokens": ["F\u00fcr", "eu\u00b7er", "Thun", "er\u00b7war\u00b7tet", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und seht ihr ringsum Freuden sprie\u00dfen,", "tokens": ["Und", "seht", "ihr", "ring\u00b7sum", "Freu\u00b7den", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Davon euch keine je verg\u00f6nnt:", "tokens": ["Da\u00b7von", "euch", "kei\u00b7ne", "je", "ver\u00b7g\u00f6nnt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PIAT", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So zeigt, da\u00df ihr nicht blo\u00df genie\u00dfen \u2013", "tokens": ["So", "zeigt", ",", "da\u00df", "ihr", "nicht", "blo\u00df", "ge\u00b7nie\u00b7\u00dfen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df ihr auch froh entbehren k\u00f6nnt.", "tokens": ["Da\u00df", "ihr", "auch", "froh", "ent\u00b7beh\u00b7ren", "k\u00f6nnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Mit gier'gem Wunsch streckt der Gemeine", "tokens": ["Mit", "gier'\u00b7gem", "Wunsch", "streckt", "der", "Ge\u00b7mei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nach Allem, was da lockt, die Hand;", "tokens": ["Nach", "Al\u00b7lem", ",", "was", "da", "lockt", ",", "die", "Hand", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "ADV", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erhab'nen Sinns begehrt der Reine", "tokens": ["Er\u00b7hab'\u00b7nen", "Sinns", "be\u00b7gehrt", "der", "Rei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVPP", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur was er seiner w\u00fcrdig fand.", "tokens": ["Nur", "was", "er", "sei\u00b7ner", "w\u00fcr\u00b7dig", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PPER", "PPOSAT", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zufrieden seid, wenn euch im Alter", "tokens": ["Zu\u00b7frie\u00b7den", "seid", ",", "wenn", "euch", "im", "Al\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dieselbe Blume noch begl\u00fcckt \u2013", "tokens": ["Die\u00b7sel\u00b7be", "Blu\u00b7me", "noch", "be\u00b7gl\u00fcckt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Derselbe hold beschwingte Falter,", "tokens": ["Der\u00b7sel\u00b7be", "hold", "be\u00b7schwing\u00b7te", "Fal\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der in der Kindheit euch entz\u00fcckt.", "tokens": ["Der", "in", "der", "Kind\u00b7heit", "euch", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So wachs't heran mit stillem Ringen,", "tokens": ["So", "wachs't", "he\u00b7ran", "mit", "stil\u00b7lem", "Rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein edles, lauteres Geschlecht,", "tokens": ["Ein", "ed\u00b7les", ",", "lau\u00b7te\u00b7res", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch stark genug, das Schwert zu schwingen,", "tokens": ["Noch", "stark", "ge\u00b7nug", ",", "das", "Schwert", "zu", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gilt es den Kampf f\u00fcr Licht und Recht;", "tokens": ["Gilt", "es", "den", "Kampf", "f\u00fcr", "Licht", "und", "Recht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Doch auch so mild schon, nichts zu hassen,", "tokens": ["Doch", "auch", "so", "mild", "schon", ",", "nichts", "zu", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "ADV", "$,", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als was da hemmt des Geistes Flug \u2013", "tokens": ["Als", "was", "da", "hemmt", "des", "Geis\u00b7tes", "Flug", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWS", "ADV", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und, gern verzeihend, zu umfassen", "tokens": ["Und", ",", "gern", "ver\u00b7zei\u00b7hend", ",", "zu", "um\u00b7fas\u00b7sen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "ADV", "VVPP", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den Feind selbst, der euch Wunden schlug.", "tokens": ["Den", "Feind", "selbst", ",", "der", "euch", "Wun\u00b7den", "schlug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Dann aber seid ihr nah und n\u00e4her", "tokens": ["Dann", "a\u00b7ber", "seid", "ihr", "nah", "und", "n\u00e4\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Gipfel schon des sch\u00f6nsten Ruhms,", "tokens": ["Dem", "Gip\u00b7fel", "schon", "des", "sch\u00f6ns\u00b7ten", "Ruhms", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf eu'ren Stirnen sieht der Seher", "tokens": ["Auf", "eu'\u00b7ren", "Stir\u00b7nen", "sieht", "der", "Se\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Vollglanz \u00e4chten Menschenthums.", "tokens": ["Den", "Voll\u00b7glanz", "\u00e4ch\u00b7ten", "Men\u00b7schen\u00b7thums", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und also ruht \u2013 ob euch mit Scherzen", "tokens": ["Und", "al\u00b7so", "ruht", "\u2013", "ob", "euch", "mit", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "$(", "KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Stunde noch umfangen h\u00e4lt \u2013:", "tokens": ["Die", "Stun\u00b7de", "noch", "um\u00b7fan\u00b7gen", "h\u00e4lt", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "VVFIN", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In eu'ren jugendlichen Herzen", "tokens": ["In", "eu'\u00b7ren", "ju\u00b7gend\u00b7li\u00b7chen", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Zukunft und das Heil der Welt!", "tokens": ["Die", "Zu\u00b7kunft", "und", "das", "Heil", "der", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}