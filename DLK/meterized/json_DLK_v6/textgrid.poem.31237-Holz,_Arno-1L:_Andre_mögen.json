{"textgrid.poem.31237": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Andre m\u00f6gen ", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Andre m\u00f6gen ", "tokens": ["And\u00b7re", "m\u00f6\u00b7gen"], "token_info": ["word", "word"], "pos": ["PIS", "VMFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "und ihr G\u00f6ldt in Wein verkehren/", "tokens": ["und", "ihr", "G\u00f6ldt", "in", "Wein", "ver\u00b7keh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "itzt b\u00fcn ich ein andrer Mann", "tokens": ["itzt", "b\u00fcn", "ich", "ein", "an\u00b7drer", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "und ich dencke nicht mehr dran.", "tokens": ["und", "ich", "den\u00b7cke", "nicht", "mehr", "dran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fehder/ Dinte und Bappihr", "tokens": ["Feh\u00b7der", "/", "Din\u00b7te", "und", "Bap\u00b7pihr"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "NN", "KON", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "acht ich \u00fcber jede Zihr/", "tokens": ["acht", "ich", "\u00fc\u00b7ber", "je\u00b7de", "Zihr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "PPER", "APPR", "PIAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "nur noch Eins erhizzt mein Bluht/", "tokens": ["nur", "noch", "Eins", "er\u00b7hizzt", "mein", "Bluht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "der gel\u00e4hrte Doctor-Hut!", "tokens": ["der", "ge\u00b7l\u00e4hr\u00b7te", "Doc\u00b7tor\u00b7Hut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Morgens bey dem sch\u00f6nsten Wettre/", "tokens": ["Mor\u00b7gens", "bey", "dem", "sch\u00f6ns\u00b7ten", "Wett\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "kaum/ da\u00df ich dem Bett entklettre/", "tokens": ["kaum", "/", "da\u00df", "ich", "dem", "Bett", "ent\u00b7klett\u00b7re", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "lauff ich gleich und ", "tokens": ["lauff", "ich", "gleich", "und"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "KON"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "dem verehrten ", "tokens": ["dem", "ver\u00b7ehr\u00b7ten"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "fegt schon ihren B\u00fccher-Saal/", "tokens": ["fegt", "schon", "ih\u00b7ren", "B\u00fc\u00b7cher\u00b7Saal", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "drin so macht mich durchau\u00df froh", "tokens": ["drin", "so", "macht", "mich", "durch\u00b7au\u00df", "froh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "der belihbte ", "tokens": ["der", "be\u00b7lihb\u00b7te"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Wider\u00fcmb nach Hau\u00df gekommen/", "tokens": ["Wi\u00b7de\u00b7r\u00fcmb", "nach", "Hau\u00df", "ge\u00b7kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wird ", "tokens": ["wird"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "der dem ", "tokens": ["der", "dem"], "token_info": ["word", "word"], "pos": ["ART", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "wie ein Kokkel nach gekreht.", "tokens": ["wie", "ein", "Kok\u00b7kel", "nach", "ge\u00b7kreht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nein/ wie mich dih\u00df bloh\u00df erbarmt/", "tokens": ["Nein", "/", "wie", "mich", "dih\u00df", "bloh\u00df", "er\u00b7barmt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PWAV", "PPER", "PDS", "ADV", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "da\u00df ihn schon der Sand \u00fcmb armt!", "tokens": ["da\u00df", "ihn", "schon", "der", "Sand", "\u00fcmb", "armt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wa\u00df er auffs Bappihr gesezzt/", "tokens": ["Wa\u00df", "er", "auffs", "Bap\u00b7pihr", "ge\u00b7sezzt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "steht in Marmol ein geezzt!", "tokens": ["steht", "in", "Mar\u00b7mol", "ein", "geezzt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "ART", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "sind mir keines-falls zu mager/", "tokens": ["sind", "mir", "kei\u00b7nes\u00b7falls", "zu", "ma\u00b7ger", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "ihr bes\u00fcsster Honig-Seim", "tokens": ["ihr", "be\u00b7s\u00fcs\u00b7ster", "Ho\u00b7nig\u00b7Seim"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "h\u00e4lt mich f\u00e4st wie Vogel-Leim.", "tokens": ["h\u00e4lt", "mich", "f\u00e4st", "wie", "Vo\u00b7gel\u00b7Leim", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "KOKOM", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auff dem Murr-Kopff ", "tokens": ["Auff", "dem", "Murr\u00b7Kopff"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "folgt der weise ", "tokens": ["folgt", "der", "wei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "dihsem b\u00fcn ich gantz verm\u00e4nckt/", "tokens": ["dih\u00b7sem", "b\u00fcn", "ich", "gantz", "ver\u00b7m\u00e4nckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "weil er mir bloh\u00df K\u00fc\u00dfgens sch\u00e4nckt!", "tokens": ["weil", "er", "mir", "bloh\u00df", "K\u00fc\u00df\u00b7gens", "sch\u00e4nckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "zeigt schon ihre Silber-Stirne/", "tokens": ["zeigt", "schon", "ih\u00b7re", "Sil\u00b7ber\u00b7Stir\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "immer noch so lig ich fromb", "tokens": ["im\u00b7mer", "noch", "so", "lig", "ich", "fromb"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ADJD", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00fcber meinem ", "tokens": ["\u00fc\u00b7ber", "mei\u00b7nem"], "token_info": ["word", "word"], "pos": ["APPR", "PPOSAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "schafft mir nicht mehr Zeit-Verdreib/", "tokens": ["schafft", "mir", "nicht", "mehr", "Zeit\u00b7Ver\u00b7dreib", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "PIAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "mir gen\u00fcgt al\u00df ", "tokens": ["mir", "ge\u00b7n\u00fcgt", "al\u00df"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "ab und zu ein Dr\u00f6pffgen Thee.", "tokens": ["ab", "und", "zu", "ein", "Dr\u00f6pff\u00b7gen", "Thee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "sind for mich das sch\u00f6nste Trio/", "tokens": ["sind", "for", "mich", "das", "sch\u00f6ns\u00b7te", "Trio", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "sonst so k\u00fcnt es mir ergehn/", "tokens": ["sonst", "so", "k\u00fcnt", "es", "mir", "er\u00b7gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "wie's dem ", "tokens": ["wie's", "dem"], "token_info": ["word", "word"], "pos": ["VVFIN", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Dihsen fra\u00df mit Haut und Hahr", "tokens": ["Dih\u00b7sen", "fra\u00df", "mit", "Haut", "und", "Hahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "die entm\u00e4ntschte ", "tokens": ["die", "ent\u00b7m\u00e4ntschte"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Dr\u00fcmb so la\u00df mich itzt in Ruh/", "tokens": ["Dr\u00fcmb", "so", "la\u00df", "mich", "itzt", "in", "Ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVIMP", "PPER", "ADV", "APPR", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "du verfluchtes Fl\u00f6tgen du!", "tokens": ["du", "ver\u00b7fluch\u00b7tes", "Fl\u00f6t\u00b7gen", "du", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}