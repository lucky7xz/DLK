{"dta.poem.2421": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "A jove principium.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Hoer armer Sterblicher! wenn du was an-wilst-", "tokens": ["Hoer", "ar\u00b7mer", "Sterb\u00b7li\u00b7cher", "!", "wenn", "du", "was", "an\u00b7wilst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$.", "KOUS", "PPER", "PWS", "TRUNC"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "fangen/", "tokens": ["fan\u00b7gen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "So sieh auf anders nichts als nur auf Gott", "tokens": ["So", "sieh", "auf", "an\u00b7ders", "nichts", "als", "nur", "auf", "Gott"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADV", "PIS", "KOKOM", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "allein", "tokens": ["al\u00b7lein"], "token_info": ["word"], "pos": ["ADV"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "La\u00df ihn in allem Thun dein rechter Anfang", "tokens": ["La\u00df", "ihn", "in", "al\u00b7lem", "Thun", "dein", "rech\u00b7ter", "An\u00b7fang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "PIS", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "seyn/", "tokens": ["seyn", "/"], "token_info": ["word", "punct"], "pos": ["VAINF", "$("], "meter": "+", "measure": "single.up"}, "line.7": {"text": "So wird dein gutes Werk/ ein gutes End erlangen.", "tokens": ["So", "wird", "dein", "gu\u00b7tes", "Werk", "/", "ein", "gu\u00b7tes", "End", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}