{"textgrid.poem.41326": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen L\u00e4chler", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seht, wie ein seichter Flu\u00df, der voller Wirbel l\u00e4uft,", "tokens": ["Seht", ",", "wie", "ein", "seich\u00b7ter", "Flu\u00df", ",", "der", "vol\u00b7ler", "Wir\u00b7bel", "l\u00e4uft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Je minder tief er ist, die kleinen Kreise h\u00e4uft!", "tokens": ["Je", "min\u00b7der", "tief", "er", "ist", ",", "die", "klei\u00b7nen", "Krei\u00b7se", "h\u00e4uft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "PPER", "VAFIN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des seichten Glycons Bild, des L\u00e4chlers ohne Geist,", "tokens": ["Des", "seich\u00b7ten", "Gly\u00b7cons", "Bild", ",", "des", "L\u00e4ch\u00b7lers", "oh\u00b7ne", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der stets die Backen dehnt, stets ihre Gr\u00fcbchen weist.", "tokens": ["Der", "stets", "die", "Ba\u00b7cken", "dehnt", ",", "stets", "ih\u00b7re", "Gr\u00fcb\u00b7chen", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$,", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}