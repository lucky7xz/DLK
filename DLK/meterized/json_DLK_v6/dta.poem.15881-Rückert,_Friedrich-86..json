{"dta.poem.15881": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "86.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O sch\u00f6ne Zeit, wo sch\u00f6n noch war interessant,", "tokens": ["O", "sch\u00f6\u00b7ne", "Zeit", ",", "wo", "sch\u00f6n", "noch", "war", "in\u00b7ter\u00b7es\u00b7sant", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PWAV", "ADJD", "ADV", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nichts intressanteres als Sch\u00f6nes war bekannt!", "tokens": ["Nichts", "in\u00b7tres\u00b7san\u00b7te\u00b7res", "als", "Sch\u00f6\u00b7nes", "war", "be\u00b7kannt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PIS", "KOKOM", "NE", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nun sind die beiden, die sonst waren eins in Frieden,", "tokens": ["Nun", "sind", "die", "bei\u00b7den", ",", "die", "sonst", "wa\u00b7ren", "eins", "in", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "PIAT", "$,", "PRELS", "ADV", "VAFIN", "PIS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sch\u00f6n und Interessant, in Feindschaft und geschieden.", "tokens": ["Sch\u00f6n", "und", "In\u00b7ter\u00b7es\u00b7sant", ",", "in", "Feind\u00b7schaft", "und", "ge\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,", "APPR", "NN", "KON", "VVPP", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Nicht mehr interessant ist nun das sch\u00f6n genannte,", "tokens": ["Nicht", "mehr", "in\u00b7ter\u00b7es\u00b7sant", "ist", "nun", "das", "sch\u00f6n", "ge\u00b7nann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "VAFIN", "ADV", "ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und noch viel minder sch\u00f6n ist das Interessante.", "tokens": ["Und", "noch", "viel", "min\u00b7der", "sch\u00f6n", "ist", "das", "In\u00b7ter\u00b7es\u00b7san\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Was ist interessant? Was der Moment gebiert,", "tokens": ["Was", "ist", "in\u00b7ter\u00b7es\u00b7sant", "?", "Was", "der", "Mo\u00b7ment", "ge\u00b7biert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJD", "$.", "PWS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Was seine Geltung mit dem Augenblick verliert.", "tokens": ["Was", "sei\u00b7ne", "Gel\u00b7tung", "mit", "dem", "Au\u00b7gen\u00b7blick", "ver\u00b7liert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Interessant, was heut, sch\u00f6n ist, was ewig freut;", "tokens": ["In\u00b7ter\u00b7es\u00b7sant", ",", "was", "heut", ",", "sch\u00f6n", "ist", ",", "was", "e\u00b7wig", "freut", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "$,", "ADJD", "VAFIN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Ew'ge tritt zur\u00fcck, wo Heutiges gebeut.", "tokens": ["Das", "Ew'\u00b7ge", "tritt", "zu\u00b7r\u00fcck", ",", "wo", "Heu\u00b7ti\u00b7ges", "ge\u00b7beut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PTKVZ", "$,", "PWAV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}