{"textgrid.poem.55272": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Wie du mir, so ich dir", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mann mit zugekn\u00f6pften Taschen,", "tokens": ["Mann", "mit", "zu\u00b7ge\u00b7kn\u00f6pf\u00b7ten", "Ta\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir tut niemand was zulieb:", "tokens": ["Dir", "tut", "nie\u00b7mand", "was", "zu\u00b7lieb", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PWS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hand wird nur von Hand gewaschen;", "tokens": ["Hand", "wird", "nur", "von", "Hand", "ge\u00b7wa\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn du nehmen willst, so gib!", "tokens": ["Wenn", "du", "neh\u00b7men", "willst", ",", "so", "gib", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$,", "ADV", "VVIMP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}