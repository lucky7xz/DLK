{"textgrid.poem.28437": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "52.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es trachten ihrer viel uns m\u00f6rdlich umzubringen,", "tokens": ["Es", "trach\u00b7ten", "ih\u00b7rer", "viel", "uns", "m\u00f6rd\u00b7lich", "um\u00b7zu\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADV", "PPER", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df wir nicht ihrem Thun und ihres Sinnes Dingen", "tokens": ["Da\u00df", "wir", "nicht", "ih\u00b7rem", "Thun", "und", "ih\u00b7res", "Sin\u00b7nes", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verpflichtet sind wie sie. O, dulde dich! das Ziel", "tokens": ["Ver\u00b7pflich\u00b7tet", "sind", "wie", "sie", ".", "O", ",", "dul\u00b7de", "dich", "!", "das", "Ziel"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVPP", "VAFIN", "KOKOM", "PPER", "$.", "NE", "$,", "VVFIN", "PPER", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist nahe; Gott wird doch wol machen, was er wil.", "tokens": ["Ist", "na\u00b7he", ";", "Gott", "wird", "doch", "wol", "ma\u00b7chen", ",", "was", "er", "wil", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$.", "NN", "VAFIN", "ADV", "ADV", "VVINF", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}