{"dta.poem.13829": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Soldatenlied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wie seltsame Kl\u00e4nge schwingen", "tokens": ["Wie", "selt\u00b7sa\u00b7me", "Kl\u00e4n\u00b7ge", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sich dort von der Waldesh\u00f6h'!", "tokens": ["Sich", "dort", "von", "der", "Wal\u00b7des\u00b7h\u00f6h'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ja, H\u00f6rner sind es, die singen", "tokens": ["Ja", ",", "H\u00f6r\u00b7ner", "sind", "es", ",", "die", "sin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "NN", "VAFIN", "PPER", "$,", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wie rasend vor Lust und Weh.", "tokens": ["Wie", "ra\u00b7send", "vor", "Lust", "und", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}