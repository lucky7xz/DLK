{"textgrid.poem.25314": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: D\u00e4mmerl\u00e4uten sch\u00fcttet in den veilchenblauen Abend", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "D\u00e4mmerl\u00e4uten sch\u00fcttet in den veilchenblauen Abend", "tokens": ["D\u00e4m\u00b7mer\u00b7l\u00e4u\u00b7ten", "sch\u00fct\u00b7tet", "in", "den", "veil\u00b7chen\u00b7blau\u00b7en", "A\u00b7bend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "wei\u00dfe Bl\u00fctenflocken. Kleine Flocken", "tokens": ["wei\u00b7\u00dfe", "Bl\u00fc\u00b7ten\u00b7flo\u00b7cken", ".", "Klei\u00b7ne", "Flo\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "blank wie Muschelperlen rieseln tanzen", "tokens": ["blank", "wie", "Mu\u00b7schel\u00b7per\u00b7len", "rie\u00b7seln", "tan\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "NN", "VVINF", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "schw\u00e4rmen weich wie d\u00fcnne blasse Daunen", "tokens": ["schw\u00e4r\u00b7men", "weich", "wie", "d\u00fcn\u00b7ne", "blas\u00b7se", "Dau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KOKOM", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "wirbelnd w\u00f6lkend. Schwere Bl\u00fctenb\u00e4ume", "tokens": ["wir\u00b7belnd", "w\u00f6l\u00b7kend", ".", "Schwe\u00b7re", "Bl\u00fc\u00b7ten\u00b7b\u00e4u\u00b7me"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVPP", "$.", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "streuen goldne Garben. Wilde G\u00e4rten", "tokens": ["streu\u00b7en", "gold\u00b7ne", "Gar\u00b7ben", ".", "Wil\u00b7de", "G\u00e4r\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "ADJA", "NN", "$.", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "tragen mich in blaue Wundern\u00e4chte", "tokens": ["tra\u00b7gen", "mich", "in", "blau\u00b7e", "Wun\u00b7der\u00b7n\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "gro\u00dfe wilde G\u00e4rten. Tiefe Beete", "tokens": ["gro\u00b7\u00dfe", "wil\u00b7de", "G\u00e4r\u00b7ten", ".", "Tie\u00b7fe", "Bee\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "ADJA", "NN", "$.", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "schwanken brennend auf wie Traumgew\u00e4sser", "tokens": ["schwan\u00b7ken", "bren\u00b7nend", "auf", "wie", "Traum\u00b7ge\u00b7w\u00e4s\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PTKVZ", "KOKOM", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "still und spiegelnd. Silberk\u00e4hne heben", "tokens": ["still", "und", "spie\u00b7gelnd", ".", "Sil\u00b7ber\u00b7k\u00e4h\u00b7ne", "he\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "$.", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "mich von braunen Uferwiesen", "tokens": ["mich", "von", "brau\u00b7nen", "U\u00b7fer\u00b7wie\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "in das Leuchten. \u00dcber Scharlachfluten", "tokens": ["in", "das", "Leuch\u00b7ten", ".", "\u00dc\u00b7ber", "Schar\u00b7lach\u00b7flu\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "dunklen Mohns der rot in Flammens\u00e4ulen", "tokens": ["dunk\u00b7len", "Mohns", "der", "rot", "in", "Flam\u00b7men\u00b7s\u00e4u\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "ADJD", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "z\u00fcngelt treibt der Nachen. Bleiche Lilien", "tokens": ["z\u00fcn\u00b7gelt", "treibt", "der", "Na\u00b7chen", ".", "Blei\u00b7che", "Li\u00b7li\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "VVFIN", "ART", "NN", "$.", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.15": {"text": "tropfen schillernd dr\u00fcberhin wie Wellen.", "tokens": ["trop\u00b7fen", "schil\u00b7lernd", "dr\u00fc\u00b7berh\u00b7in", "wie", "Wel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "KOKOM", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "D\u00fcfte aus kristallnen N\u00e4chten tauchend", "tokens": ["D\u00fcf\u00b7te", "aus", "kris\u00b7tall\u00b7nen", "N\u00e4ch\u00b7ten", "tau\u00b7chend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "CARD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "schlingen wirr und h\u00e4ngen sich ins Haar", "tokens": ["schlin\u00b7gen", "wirr", "und", "h\u00e4n\u00b7gen", "sich", "ins", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "VVFIN", "PRF", "APPRART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.18": {"text": "und sie locken ... leise leise ...", "tokens": ["und", "sie", "lo\u00b7cken", "...", "lei\u00b7se", "lei\u00b7se", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "$(", "ADJD", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "und die gr\u00fcnen klaren Tiefen flimmern ...", "tokens": ["und", "die", "gr\u00fc\u00b7nen", "kla\u00b7ren", "Tie\u00b7fen", "flim\u00b7mern", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Purpurstrahlen schie\u00dfen ... leise sink ich ...", "tokens": ["Pur\u00b7pur\u00b7strah\u00b7len", "schie\u00b7\u00dfen", "...", "lei\u00b7se", "sink", "ich", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "ADJD", "VVFIN", "PPER", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "s\u00fc\u00df umf\u00e4ngt mich m\u00fcder Laut von Geigen ...", "tokens": ["s\u00fc\u00df", "um\u00b7f\u00e4ngt", "mich", "m\u00fc\u00b7der", "Laut", "von", "Gei\u00b7gen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADJD", "APPR", "APPR", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "schwingt sinkt gleitende Pal\u00e4ste", "tokens": ["schwingt", "sinkt", "glei\u00b7ten\u00b7de", "Pa\u00b7l\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "funkeln fern. Licht st\u00fcrzt", "tokens": ["fun\u00b7keln", "fern", ".", "Licht", "st\u00fcrzt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$.", "NN", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.24": {"text": "\u00fcber mich. Weit gr\u00fcn", "tokens": ["\u00fc\u00b7ber", "mich", ".", "Weit", "gr\u00fcn"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "$.", "NN", "ADJD"], "meter": "+--++", "measure": "adoneus"}, "line.25": {"text": "schwebt ein Gl\u00e4nzen ...", "tokens": ["schwebt", "ein", "Gl\u00e4n\u00b7zen", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}}}}}