{"textgrid.poem.35069": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Doppelsieg", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denk nicht an dich, wenn dir ein Weh", "tokens": ["Denk", "nicht", "an", "dich", ",", "wenn", "dir", "ein", "Weh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKNEG", "APPR", "PPER", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von irgend Jemand widerf\u00e4hrt;", "tokens": ["Von", "ir\u00b7gend", "Je\u00b7mand", "wi\u00b7der\u00b7f\u00e4hrt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denk nur an ihn allein, und geh", "tokens": ["Denk", "nur", "an", "ihn", "al\u00b7lein", ",", "und", "geh"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "ADV", "APPR", "PPER", "ADV", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dorthin, wo dich's Gebet verkl\u00e4rt.", "tokens": ["Dor\u00b7thin", ",", "wo", "dich's", "Ge\u00b7bet", "ver\u00b7kl\u00e4rt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dann f\u00fchlst du wohl, da\u00df im Verzeihn", "tokens": ["Dann", "f\u00fchlst", "du", "wohl", ",", "da\u00df", "im", "Ver\u00b7zeihn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein zweifach gro\u00dfer Segen liegt:", "tokens": ["Ein", "zwei\u00b7fach", "gro\u00b7\u00dfer", "Se\u00b7gen", "liegt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du hast nicht \u00fcber dich allein,", "tokens": ["Du", "hast", "nicht", "\u00fc\u00b7ber", "dich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du hast auch \u00fcber ihn gesiegt.", "tokens": ["Du", "hast", "auch", "\u00fc\u00b7ber", "ihn", "ge\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}