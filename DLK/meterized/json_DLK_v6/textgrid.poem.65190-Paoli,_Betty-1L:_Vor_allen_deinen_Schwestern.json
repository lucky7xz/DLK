{"textgrid.poem.65190": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vor allen deinen Schwestern", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor allen deinen Schwestern", "tokens": ["Vor", "al\u00b7len", "dei\u00b7nen", "Schwes\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gepriesen seist du mir", "tokens": ["Ge\u00b7prie\u00b7sen", "seist", "du", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du, die so heut wie gestern", "tokens": ["Du", ",", "die", "so", "heut", "wie", "ge\u00b7stern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ADV", "ADV", "KOKOM", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Des Gartens bl\u00fch'nde Zier.", "tokens": ["Des", "Gar\u00b7tens", "bl\u00fch'n\u00b7de", "Zier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die, wenn die andern lange", "tokens": ["Die", ",", "wenn", "die", "an\u00b7dern", "lan\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "ART", "ADJA", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den letzten Duft verstreut,", "tokens": ["Den", "letz\u00b7ten", "Duft", "ver\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In freud'gem Lebensdrange", "tokens": ["In", "freu\u00b7d'\u00b7gem", "Le\u00b7bens\u00b7dran\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sich immerfort erneut!", "tokens": ["Sich", "im\u00b7mer\u00b7fort", "er\u00b7neut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "La\u00df sie nur prunkend stehen", "tokens": ["La\u00df", "sie", "nur", "prun\u00b7kend", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und hauchen w\u00fcrz'gen Brand!", "tokens": ["Und", "hau\u00b7chen", "w\u00fcrz'\u00b7gen", "Brand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie bl\u00fchen und verwehen,", "tokens": ["Sie", "bl\u00fc\u00b7hen", "und", "ver\u00b7we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du aber hast Bestand.", "tokens": ["Du", "a\u00b7ber", "hast", "Be\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du rankst an welken Hagen", "tokens": ["Du", "rankst", "an", "wel\u00b7ken", "Ha\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PWAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und zauberst unserm Blick", "tokens": ["Und", "zau\u00b7berst", "un\u00b7serm", "Blick"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch in des Herbstes Tagen", "tokens": ["Noch", "in", "des", "Herbs\u00b7tes", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Rosenmond zur\u00fcck. \u2013", "tokens": ["Den", "Ro\u00b7sen\u00b7mond", "zu\u00b7r\u00fcck", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Mir spiegelt sich in jenen", "tokens": ["Mir", "spie\u00b7gelt", "sich", "in", "je\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PDAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Gl\u00fcck, das lockend glei\u00dft", "tokens": ["Das", "Gl\u00fcck", ",", "das", "lo\u00b7ckend", "glei\u00dft"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und, wenn wir's unser w\u00e4hnen,", "tokens": ["Und", ",", "wenn", "wir's", "un\u00b7ser", "w\u00e4h\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PIS", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich unserm Arm entrei\u00dft;", "tokens": ["Sich", "un\u00b7serm", "Arm", "ent\u00b7rei\u00dft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "In dir der stete Segen,", "tokens": ["In", "dir", "der", "ste\u00b7te", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den mild ein guter Geist", "tokens": ["Den", "mild", "ein", "gu\u00b7ter", "Geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf unsern Erdenwegen", "tokens": ["Auf", "un\u00b7sern", "Er\u00b7den\u00b7we\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Uns still begleiten hei\u00dft.", "tokens": ["Uns", "still", "be\u00b7glei\u00b7ten", "hei\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Hold tritt er uns entgegen,", "tokens": ["Hold", "tritt", "er", "uns", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn bang die Seele ringt,", "tokens": ["Wenn", "bang", "die", "See\u00b7le", "ringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der unscheinbare Segen,", "tokens": ["Der", "un\u00b7schein\u00b7ba\u00b7re", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den jeder Tag uns bringt!", "tokens": ["Den", "je\u00b7der", "Tag", "uns", "bringt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}