{"textgrid.poem.35484": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Sohn der Zeit", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon floh die Mitternacht. \u2013 Noch aber pocht's", "tokens": ["Schon", "floh", "die", "Mit\u00b7ter\u00b7nacht", ".", "\u2013", "Noch", "a\u00b7ber", "pocht's"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$(", "ADV", "ADV", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durch meine Brust wie ein titanisch Wollen,", "tokens": ["Durch", "mei\u00b7ne", "Brust", "wie", "ein", "ti\u00b7ta\u00b7nisch", "Wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das nach der ", "tokens": ["Das", "nach", "der"], "token_info": ["word", "word", "word"], "pos": ["PDS", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Das vor der Tat wie vor der H\u00f6lle schaudert ...", "tokens": ["Das", "vor", "der", "Tat", "wie", "vor", "der", "H\u00f6l\u00b7le", "schau\u00b7dert", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "KOKOM", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich f\u00fchle meine Schultern atlasstark,", "tokens": ["Ich", "f\u00fch\u00b7le", "mei\u00b7ne", "Schul\u00b7tern", "at\u00b7las\u00b7stark", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es blitzt mein Blick \u2013 es bl\u00fcht mein junges Mark \u2013", "tokens": ["Es", "blitzt", "mein", "Blick", "\u2013", "es", "bl\u00fcht", "mein", "jun\u00b7ges", "Mark", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie ein Gigant m\u00f6cht' aus den Angeln heben", "tokens": ["Wie", "ein", "Gi\u00b7gant", "m\u00f6cht'", "aus", "den", "An\u00b7geln", "he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich diese morsche, pestzerfre\u00dfne Welt", "tokens": ["Ich", "die\u00b7se", "mor\u00b7sche", ",", "pest\u00b7zer\u00b7fre\u00df\u00b7ne", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "PDAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und einer ", "tokens": ["Und", "ei\u00b7ner"], "token_info": ["word", "word"], "pos": ["KON", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Drin sich das ", "tokens": ["Drin", "sich", "das"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PRF", "ART"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Nun lastet's auf mir wie Myriaden S\u00fcnden,", "tokens": ["Nun", "las\u00b7tet's", "auf", "mir", "wie", "My\u00b7ria\u00b7den", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "KOKOM", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die mich zerschn\u00fcren und zerstampfen,", "tokens": ["Die", "mich", "zer\u00b7schn\u00fc\u00b7ren", "und", "zer\u00b7stamp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Wahnsinnsfratzen mir das Hirn umdampfen,", "tokens": ["Wie", "Wahn\u00b7sinns\u00b7frat\u00b7zen", "mir", "das", "Hirn", "um\u00b7damp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie Hurenatem meine Seele schinden ...", "tokens": ["Wie", "Hu\u00b7re\u00b7na\u00b7tem", "mei\u00b7ne", "See\u00b7le", "schin\u00b7den", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So such' ich, schw\u00e4rmend in des Weltalls Weiten,", "tokens": ["So", "such'", "ich", ",", "schw\u00e4r\u00b7mend", "in", "des", "Welt\u00b7alls", "Wei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vergangenheit und k\u00fcnft'ge Ewigkeiten", "tokens": ["Ver\u00b7gan\u00b7gen\u00b7heit", "und", "k\u00fcnft'\u00b7ge", "E\u00b7wig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durchf\u00fchlend auf sekundenkurzer Spur,", "tokens": ["Durch\u00b7f\u00fch\u00b7lend", "auf", "se\u00b7kun\u00b7den\u00b7kur\u00b7zer", "Spur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vor mir zu fliehn in g\u00f6ttergro\u00dfem Wagen,", "tokens": ["Vor", "mir", "zu", "fliehn", "in", "g\u00f6t\u00b7ter\u00b7gro\u00b7\u00dfem", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In m\u00e4nnlich starkem, freiem Weltentsagen \u2013 \u2013 \u2013", "tokens": ["In", "m\u00e4nn\u00b7lich", "star\u00b7kem", ",", "frei\u00b7em", "Wel\u00b7ten\u00b7tsa\u00b7gen", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ADJD", "ADJA", "$,", "ADJA", "NN", "$(", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und nie entrinnst du dir, o Kreatur! ...", "tokens": ["Und", "nie", "ent\u00b7rinnst", "du", "dir", ",", "o", "Kre\u00b7a\u00b7tur", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "$,", "FM", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Es ekelt mich der Menschheit W\u00fcrmerbrut \u2013", "tokens": ["Es", "e\u00b7kelt", "mich", "der", "Menschheit", "W\u00fcr\u00b7mer\u00b7brut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nun w\u00e4chst mein Herz in der Begeistrung Glut \u2013", "tokens": ["Nun", "w\u00e4chst", "mein", "Herz", "in", "der", "Be\u00b7geis\u00b7trung", "Glut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie gleitet hin wie kraftlos fl\u00fccht'ge Wellen,", "tokens": ["Sie", "glei\u00b7tet", "hin", "wie", "kraft\u00b7los", "fl\u00fccht'\u00b7ge", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KOKOM", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die aufw\u00e4rts steigen, wieder zu zerst\u00e4uben \u2013", "tokens": ["Die", "auf\u00b7w\u00e4rts", "stei\u00b7gen", ",", "wie\u00b7der", "zu", "zer\u00b7st\u00e4u\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "$,", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sich einen Augenblick", "tokens": ["Sich", "ei\u00b7nen", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word"], "pos": ["PRF", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In namenlosem Gl\u00fcck", "tokens": ["In", "na\u00b7men\u00b7lo\u00b7sem", "Gl\u00fcck"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Als Weltenspiegel zu bet\u00e4uben ...", "tokens": ["Als", "Wel\u00b7ten\u00b7spie\u00b7gel", "zu", "be\u00b7t\u00e4u\u00b7ben", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich mu\u00df die Wunden f\u00fchlen, die ihr Leib", "tokens": ["Ich", "mu\u00df", "die", "Wun\u00b7den", "f\u00fch\u00b7len", ",", "die", "ihr", "Leib"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie ein zersch\u00e4ndet, ehrenloses Weib", "tokens": ["Wie", "ein", "zer\u00b7sch\u00e4n\u00b7det", ",", "eh\u00b7ren\u00b7lo\u00b7ses", "Weib"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "VVPP", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit sich herumschleppt, prunkend bald \u2013", "tokens": ["Mit", "sich", "her\u00b7um\u00b7schleppt", ",", "prun\u00b7kend", "bald", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVPP", "$,", "ADJD", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bald schamgekr\u00fcmmt \u2013 \u2013 ich mu\u00df \u2013 ich mu\u00df", "tokens": ["Bald", "scham\u00b7ge\u00b7kr\u00fcmmt", "\u2013", "\u2013", "ich", "mu\u00df", "\u2013", "ich", "mu\u00df"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "$(", "$(", "PPER", "VMFIN", "$(", "PPER", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie f\u00fchlen, wie der ", "tokens": ["Sie", "f\u00fch\u00b7len", ",", "wie", "der"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Dem nichts den Feuerfra\u00df der Schmerzen k\u00fchlt,", "tokens": ["Dem", "nichts", "den", "Feu\u00b7er\u00b7fra\u00df", "der", "Schmer\u00b7zen", "k\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Den die D\u00e4monen kreuz'gen mit dem Ku\u00df ...", "tokens": ["Den", "die", "D\u00e4\u00b7mo\u00b7nen", "kreuz'\u00b7gen", "mit", "dem", "Ku\u00df", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}, "line.9": {"text": "Wie Christus, Dante, Michelangelo,", "tokens": ["Wie", "Chris\u00b7tus", ",", "Dan\u00b7te", ",", "Mi\u00b7che\u00b7lan\u00b7ge\u00b7lo", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "NE", "$,", "NE", "$,", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Die Riesen, unbegreiflich vor mir ragen,", "tokens": ["Die", "Rie\u00b7sen", ",", "un\u00b7be\u00b7greif\u00b7lich", "vor", "mir", "ra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ragt auch in diesen dunklen Zweiflertagen", "tokens": ["Ragt", "auch", "in", "die\u00b7sen", "dunk\u00b7len", "Zweif\u00b7ler\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Vor mir der Zeit tiefinnerstes Geheimnis ...", "tokens": ["Vor", "mir", "der", "Zeit", "tie\u00b7fin\u00b7ners\u00b7tes", "Ge\u00b7heim\u00b7nis", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ich kann es ", "tokens": ["Ich", "kann", "es"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Sein Wesen sp\u00fcr' ich durch die Seele zittern,", "tokens": ["Sein", "We\u00b7sen", "sp\u00fcr'", "ich", "durch", "die", "See\u00b7le", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch find' ich nicht die L\u00f6sung, die es tilgt ...", "tokens": ["Doch", "find'", "ich", "nicht", "die", "L\u00f6\u00b7sung", ",", "die", "es", "tilgt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es sch\u00fcrt in mir \u2013 sein Atem s\u00e4t Verderben,", "tokens": ["Es", "sch\u00fcrt", "in", "mir", "\u2013", "sein", "A\u00b7tem", "s\u00e4t", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$(", "PPOSAT", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Brandung schreit \u2013 und St\u00fcrme, sie ersch\u00fcttern,", "tokens": ["Die", "Bran\u00b7dung", "schreit", "\u2013", "und", "St\u00fcr\u00b7me", ",", "sie", "er\u00b7sch\u00fct\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "KON", "NN", "$,", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Entwurzeln mich \u2013 doch ob die Brust auch br\u00fcllt,", "tokens": ["Ent\u00b7wur\u00b7zeln", "mich", "\u2013", "doch", "ob", "die", "Brust", "auch", "br\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "ADV", "KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nach ", "tokens": ["Nach"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Den sie gefangen hinter Eisengittern:", "tokens": ["Den", "sie", "ge\u00b7fan\u00b7gen", "hin\u00b7ter", "Ei\u00b7sen\u00b7git\u00b7tern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der Zeit Geschw\u00fcre kann ich nicht verwinden \u2013", "tokens": ["Der", "Zeit", "Ge\u00b7schw\u00fc\u00b7re", "kann", "ich", "nicht", "ver\u00b7win\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Es lasten auf mir ihre harten S\u00fcnden \u2013", "tokens": ["Es", "las\u00b7ten", "auf", "mir", "ih\u00b7re", "har\u00b7ten", "S\u00fcn\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ich bin der Sohn der Zeit \u2013 doch ach! ihr G\u00f6tter!", "tokens": ["Ich", "bin", "der", "Sohn", "der", "Zeit", "\u2013", "doch", "ach", "!", "ihr", "G\u00f6t\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$(", "ADV", "ADV", "$.", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Ich bin ihr ", "tokens": ["Ich", "bin", "ihr"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}}}}}