{"textgrid.poem.46930": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr, die der Himmel hat bestellt, als Lichter", "tokens": ["Ihr", ",", "die", "der", "Him\u00b7mel", "hat", "be\u00b7stellt", ",", "als", "Lich\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "VAFIN", "VVPP", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu leuchten denen, die im Finstern klimmen,", "tokens": ["Zu", "leuch\u00b7ten", "de\u00b7nen", ",", "die", "im", "Fins\u00b7tern", "klim\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PDS", "$,", "PRELS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie habt ihr also euer Amt zum schlimmen", "tokens": ["Wie", "habt", "ihr", "al\u00b7so", "eu\u00b7er", "Amt", "zum", "schlim\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mi\u00dfbraucht, ihr Lehrer, Denker, Forscher, Dichter!", "tokens": ["Mi\u00df\u00b7braucht", ",", "ihr", "Leh\u00b7rer", ",", "Den\u00b7ker", ",", "For\u00b7scher", ",", "Dich\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Den Schlaf der Tr\u00e4gheit, aller Kraft Vernichter,", "tokens": ["Den", "Schlaf", "der", "Tr\u00e4g\u00b7heit", ",", "al\u00b7ler", "Kraft", "Ver\u00b7nich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Drin aufgel\u00f6st ihr euer Volk seht schwimmen,", "tokens": ["Drin", "auf\u00b7ge\u00b7l\u00f6st", "ihr", "eu\u00b7er", "Volk", "seht", "schwim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Statt es zu wecken draus mit euren Stimmen,", "tokens": ["Statt", "es", "zu", "we\u00b7cken", "draus", "mit", "eu\u00b7ren", "Stim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKZU", "VVINF", "PAV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wiegt ihr's noch mehr in eitle Traumgesichter.", "tokens": ["Wiegt", "ih\u00b7r's", "noch", "mehr", "in", "eit\u00b7le", "Traum\u00b7ge\u00b7sich\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.9": {"text": "Eins ist uns not! Wach sein zum Kampfgewitter.", "tokens": ["Eins", "ist", "uns", "not", "!", "Wach", "sein", "zum", "Kampf\u00b7ge\u00b7wit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "NN", "$.", "NN", "PPOSAT", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wollt ihr nicht mehren selbst der K\u00e4mpfer Summe,", "tokens": ["Wollt", "ihr", "nicht", "meh\u00b7ren", "selbst", "der", "K\u00e4mp\u00b7fer", "Sum\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Schmelzt sie nur nicht durchs Klimpern eurer Zither.", "tokens": ["Schmelzt", "sie", "nur", "nicht", "durchs", "Klim\u00b7pern", "eu\u00b7rer", "Zi\u00b7ther", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "H\u00f6rt wohl ein Gott eu'r loses Wortgesumme?", "tokens": ["H\u00f6rt", "wohl", "ein", "Gott", "eu'r", "lo\u00b7ses", "Wort\u00b7ge\u00b7sum\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Er h\u00f6r's, da\u00df er die Lei'r euch schlag' in Splitter,", "tokens": ["Er", "h\u00f6r's", ",", "da\u00df", "er", "die", "Lei'r", "euch", "schlag'", "in", "Split\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und euch schlag' auf den Mund, da\u00df er verstumme.", "tokens": ["Und", "euch", "schlag'", "auf", "den", "Mund", ",", "da\u00df", "er", "ver\u00b7stum\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}