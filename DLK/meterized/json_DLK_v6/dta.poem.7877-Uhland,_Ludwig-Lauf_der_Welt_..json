{"dta.poem.7877": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Lauf der Welt .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "An jedem Abend geh\u2019 ich aus,", "tokens": ["An", "je\u00b7dem", "A\u00b7bend", "geh'", "ich", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hinauf den Wiesensteg.", "tokens": ["Hin\u00b7auf", "den", "Wie\u00b7sens\u00b7teg", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie schaut aus ihrem Gartenhaus,", "tokens": ["Sie", "schaut", "aus", "ih\u00b7rem", "Gar\u00b7ten\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es stehet hart am Weg.", "tokens": ["Es", "ste\u00b7het", "hart", "am", "Weg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wir haben uns noch nie bestellt,", "tokens": ["Wir", "ha\u00b7ben", "uns", "noch", "nie", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es ist nur so der Lauf der Welt.", "tokens": ["Es", "ist", "nur", "so", "der", "Lauf", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df nicht, wie es so geschah,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wie", "es", "so", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seit lange k\u00fcss\u2019 ich sie.", "tokens": ["Seit", "lan\u00b7ge", "k\u00fcss'", "ich", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich bitte nicht, sie sagt nicht: ja!", "tokens": ["Ich", "bit\u00b7te", "nicht", ",", "sie", "sagt", "nicht", ":", "ja", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "$,", "PPER", "VVFIN", "PTKNEG", "$.", "PTKANT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch sagt sie: nein! auch nie.", "tokens": ["Doch", "sagt", "sie", ":", "nein", "!", "auch", "nie", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "PTKANT", "$.", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn Lippe gern auf Lippe ruht,", "tokens": ["Wenn", "Lip\u00b7pe", "gern", "auf", "Lip\u00b7pe", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wir hindern\u2019s nicht, uns d\u00fcnkt es gut.", "tokens": ["Wir", "hin\u00b7dern's", "nicht", ",", "uns", "d\u00fcnkt", "es", "gut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das L\u00fcftchen mit der Rose spielt,", "tokens": ["Das", "L\u00fcft\u00b7chen", "mit", "der", "Ro\u00b7se", "spielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es fragt nicht: hast mich lieb?", "tokens": ["Es", "fragt", "nicht", ":", "hast", "mich", "lieb", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$.", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das R\u00f6schen sich am Thaue k\u00fchlt,", "tokens": ["Das", "R\u00f6\u00b7schen", "sich", "am", "Thau\u00b7e", "k\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es sagt nicht lange: gib!", "tokens": ["Es", "sagt", "nicht", "lan\u00b7ge", ":", "gib", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$.", "VVIMP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich liebe sie, sie liebet mich,", "tokens": ["Ich", "lie\u00b7be", "sie", ",", "sie", "lie\u00b7bet", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch Keines sagt: ich liebe dich!", "tokens": ["Doch", "Kei\u00b7nes", "sagt", ":", "ich", "lie\u00b7be", "dich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}