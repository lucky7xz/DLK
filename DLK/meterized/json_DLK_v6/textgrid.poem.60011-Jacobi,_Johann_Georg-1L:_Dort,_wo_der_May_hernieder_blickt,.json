{"textgrid.poem.60011": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dort, wo der May hernieder blickt,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort, wo der May hernieder blickt,", "tokens": ["Dort", ",", "wo", "der", "May", "her\u00b7nie\u00b7der", "blickt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In jenem Thale, das, entz\u00fcckt,", "tokens": ["In", "je\u00b7nem", "Tha\u00b7le", ",", "das", ",", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PDS", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich mit den ersten Blumen schm\u00fcckt,", "tokens": ["Sich", "mit", "den", "ers\u00b7ten", "Blu\u00b7men", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sah' ich ein Chor von Liebesg\u00f6ttern", "tokens": ["Sah'", "ich", "ein", "Chor", "von", "Lie\u00b7bes\u00b7g\u00f6t\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vertieft in den Geschichten bl\u00e4ttern,", "tokens": ["Ver\u00b7tieft", "in", "den", "Ge\u00b7schich\u00b7ten", "bl\u00e4t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die eine Muse Dir erz\u00e4hlt:", "tokens": ["Die", "ei\u00b7ne", "Mu\u00b7se", "Dir", "er\u00b7z\u00e4hlt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie Grazien den Amor wiegten,", "tokens": ["Wie", "Gra\u00b7zi\u00b7en", "den", "A\u00b7mor", "wieg\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und ihn durch manches Spiel vergn\u00fcgten;", "tokens": ["Und", "ihn", "durch", "man\u00b7ches", "Spiel", "ver\u00b7gn\u00fcg\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wie er mit Psychen sich verm\u00e4hlt;", "tokens": ["Wie", "er", "mit", "Psy\u00b7chen", "sich", "ver\u00b7m\u00e4hlt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wie seine goldnen Waffen kriegten", "tokens": ["Wie", "sei\u00b7ne", "gold\u00b7nen", "Waf\u00b7fen", "krieg\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und \u00fcber alle G\u00f6tter siegten;", "tokens": ["Und", "\u00fc\u00b7ber", "al\u00b7le", "G\u00f6t\u00b7ter", "sieg\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Wie, bey der Leyer Harmonie,", "tokens": ["Wie", ",", "bey", "der", "Le\u00b7yer", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Den st\u00e4rksten L\u00f6wen er bezwungen,", "tokens": ["Den", "st\u00e4rks\u00b7ten", "L\u00f6\u00b7wen", "er", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und dann die Keule sich errungen,", "tokens": ["Und", "dann", "die", "Keu\u00b7le", "sich", "er\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Die bis zum Tartarus gedrungen,", "tokens": ["Die", "bis", "zum", "Tar\u00b7ta\u00b7rus", "ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.16": {"text": "Die\u00df, Freund! die\u00df alles lasen sie.", "tokens": ["Die\u00df", ",", "Freund", "!", "die\u00df", "al\u00b7les", "la\u00b7sen", "sie", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "NN", "$.", "PDS", "PIS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Da sprach ein Amor zu dem andern:", "tokens": ["Da", "sprach", "ein", "A\u00b7mor", "zu", "dem", "an\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Uns will, der dieses schrieb, entfliehn;", "tokens": ["Uns", "will", ",", "der", "die\u00b7ses", "schrieb", ",", "ent\u00b7fliehn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PRELS", "PDS", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Des Pluto Reich will er durchwandern;", "tokens": ["Des", "Plu\u00b7to", "Reich", "will", "er", "durch\u00b7wan\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Allein wir selbst begleiten ihn.", "tokens": ["Al\u00b7lein", "wir", "selbst", "be\u00b7glei\u00b7ten", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Ixions Rad mu\u00df er nicht h\u00f6ren,", "tokens": ["I\u00b7xions", "Rad", "mu\u00df", "er", "nicht", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Ihn darf der Zerberus nicht st\u00f6ren,", "tokens": ["Ihn", "darf", "der", "Zer\u00b7be\u00b7rus", "nicht", "st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Ihm rauschet nicht der H\u00f6llenflu\u00df;", "tokens": ["Ihm", "rau\u00b7schet", "nicht", "der", "H\u00f6l\u00b7len\u00b7flu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Nicht sehen wird ihn Tantalus,", "tokens": ["Nicht", "se\u00b7hen", "wird", "ihn", "Tan\u00b7ta\u00b7lus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VAFIN", "PPER", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Und nicht der m\u00fcde Sisyphus.", "tokens": ["Und", "nicht", "der", "m\u00fc\u00b7de", "Si\u00b7syp\u00b7hus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Nur auf besonnten stillen H\u00f6hen,", "tokens": ["Nur", "auf", "be\u00b7sonn\u00b7ten", "stil\u00b7len", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Soll er mit Liebesg\u00f6ttern gehen.", "tokens": ["Soll", "er", "mit", "Lie\u00b7bes\u00b7g\u00f6t\u00b7tern", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.28": {"text": "Im bl\u00fchenden Elysium", "tokens": ["Im", "bl\u00fc\u00b7hen\u00b7den", "E\u00b7ly\u00b7si\u00b7um"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Versammeln wir um ihn herum", "tokens": ["Ver\u00b7sam\u00b7meln", "wir", "um", "ihn", "he\u00b7rum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Corinnen, Lesbien, Helenen,", "tokens": ["Co\u00b7rin\u00b7nen", ",", "Les\u00b7bi\u00b7en", ",", "He\u00b7le\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.31": {"text": "Mit allen einst gepriesnen Sch\u00f6nen.", "tokens": ["Mit", "al\u00b7len", "einst", "ge\u00b7pri\u00b7es\u00b7nen", "Sch\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "Umarmen soll ihn einst Tibull;", "tokens": ["Um\u00b7ar\u00b7men", "soll", "ihn", "einst", "Ti\u00b7bull", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Ein neues Lied singt ihm Catull,", "tokens": ["Ein", "neu\u00b7es", "Lied", "singt", "ihm", "Ca\u00b7tull", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "NE", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.34": {"text": "Der dort in seines M\u00e4dchens Hand", "tokens": ["Der", "dort", "in", "sei\u00b7nes", "M\u00e4d\u00b7chens", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Den muntern Sperling wieder fand.", "tokens": ["Den", "mun\u00b7tern", "Sper\u00b7ling", "wie\u00b7der", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Es sollen artige Satyren", "tokens": ["Es", "sol\u00b7len", "ar\u00b7ti\u00b7ge", "Sa\u00b7ty\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Den besten Wein zur H\u00f6lle f\u00fchren,", "tokens": ["Den", "bes\u00b7ten", "Wein", "zur", "H\u00f6l\u00b7le", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Und da, wo sonst kein Becher winkt,", "tokens": ["Und", "da", ",", "wo", "sonst", "kein", "Be\u00b7cher", "winkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Wo man nur Lethens Wasser trinkt,", "tokens": ["Wo", "man", "nur", "Le\u00b7thens", "Was\u00b7ser", "trinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Soll er, wir wollen ihn belauschen,", "tokens": ["Soll", "er", ",", "wir", "wol\u00b7len", "ihn", "be\u00b7lau\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Sich mit Anakreon berauschen.", "tokens": ["Sich", "mit", "A\u00b7nak\u00b7re\u00b7on", "be\u00b7rau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NE", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}