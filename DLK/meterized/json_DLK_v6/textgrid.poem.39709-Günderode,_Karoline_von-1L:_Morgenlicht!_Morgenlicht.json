{"textgrid.poem.39709": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Morgenlicht! Morgenlicht", "genre": "verse", "period": "N.A.", "pub_year": 1804, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Morgenlicht! Morgenlicht", "tokens": ["Mor\u00b7gen\u00b7licht", "!", "Mor\u00b7gen\u00b7licht"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$.", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Scheint mir hell ins Gesicht!", "tokens": ["Scheint", "mir", "hell", "ins", "Ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wenn ich Tag kommen seh,", "tokens": ["Wenn", "ich", "Tag", "kom\u00b7men", "seh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "wird mir leid und weh;", "tokens": ["wird", "mir", "leid", "und", "weh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KON", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Denn im Grabe liegt", "tokens": ["Denn", "im", "Gra\u00b7be", "liegt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Ein jung M\u00e4gdelein;", "tokens": ["Ein", "jung", "M\u00e4g\u00b7del\u00b7ein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Des Fr\u00fchroths Schein", "tokens": ["Des", "Fr\u00fch\u00b7roths", "Schein"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Sieht traurig hinein", "tokens": ["Sieht", "trau\u00b7rig", "hin\u00b7ein"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADJD", "PTKVZ"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "In das enge K\u00e4mmerlein.", "tokens": ["In", "das", "en\u00b7ge", "K\u00e4m\u00b7mer\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "M\u00f6gt wekken das Jungfr\u00e4ulein,", "tokens": ["M\u00f6gt", "wek\u00b7ken", "das", "Jung\u00b7fr\u00e4u\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Das kann vom Schlaf nicht erstehn,", "tokens": ["Das", "kann", "vom", "Schlaf", "nicht", "er\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPRART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.12": {"text": "Morgenlicht nicht sehn;", "tokens": ["Mor\u00b7gen\u00b7licht", "nicht", "sehn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "Drum wenn ich Fr\u00fchroth kommen seh,", "tokens": ["Drum", "wenn", "ich", "Fr\u00fch\u00b7roth", "kom\u00b7men", "seh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "PPER", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wird mir leid und weh.", "tokens": ["Wird", "mir", "leid", "und", "weh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KON", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}