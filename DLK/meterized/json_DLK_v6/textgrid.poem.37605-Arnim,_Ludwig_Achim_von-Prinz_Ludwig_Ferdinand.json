{"textgrid.poem.37605": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Prinz Ludwig Ferdinand", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Nixe von der Saale,", "tokens": ["O", "Ni\u00b7xe", "von", "der", "Saa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Was r\u00f6thet dein Gewand,", "tokens": ["Was", "r\u00f6\u00b7thet", "dein", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hast du beim Burschenmahle", "tokens": ["Hast", "du", "beim", "Bur\u00b7schen\u00b7mah\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Becher umgewandt?", "tokens": ["Den", "Be\u00b7cher", "um\u00b7ge\u00b7wandt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbder Becher ist geleeret,", "tokens": ["\u00bb", "der", "Be\u00b7cher", "ist", "ge\u00b7lee\u00b7ret", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Geist ist schon dahin,", "tokens": ["Der", "Geist", "ist", "schon", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich laufe ganz zerst\u00f6ret,", "tokens": ["Ich", "lau\u00b7fe", "ganz", "zer\u00b7st\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weil ich nicht bei mir bin.\u00ab", "tokens": ["Weil", "ich", "nicht", "bei", "mir", "bin", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "PPER", "VAFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nun wei\u00df ich was geschehen,", "tokens": ["Nun", "wei\u00df", "ich", "was", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Warum sie floh daher,", "tokens": ["Wa\u00b7rum", "sie", "floh", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht wagte umzusehen,", "tokens": ["Nicht", "wag\u00b7te", "um\u00b7zu\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich st\u00fcrzte in das Meer.", "tokens": ["Sich", "st\u00fcrz\u00b7te", "in", "das", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Warum die Blumen erbleichen", "tokens": ["Wa\u00b7rum", "die", "Blu\u00b7men", "er\u00b7blei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Auf ihres Pfades Spur,", "tokens": ["Auf", "ih\u00b7res", "Pfa\u00b7des", "Spur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Unschuldig Blut kann weichen", "tokens": ["Un\u00b7schul\u00b7dig", "Blut", "kann", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "NN", "VMFIN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Der schuld'gen Rache nur.", "tokens": ["Der", "schuld'\u00b7gen", "Ra\u00b7che", "nur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er ist bei ihr get\u00f6dtet", "tokens": ["Er", "ist", "bei", "ihr", "ge\u00b7t\u00f6d\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bei ihr, sie st\u00fcrzt ins Meer.", "tokens": ["Bei", "ihr", ",", "sie", "st\u00fcrzt", "ins", "Meer", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Held Ocean err\u00f6thet", "tokens": ["Held", "O\u00b7cean", "er\u00b7r\u00f6t\u00b7het"], "token_info": ["word", "word", "word"], "pos": ["NN", "NE", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und seufzet auf so schwer.", "tokens": ["Und", "seuf\u00b7zet", "auf", "so", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Also der Feinde Flotte", "tokens": ["Al\u00b7so", "der", "Fein\u00b7de", "Flot\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Wirft aus am fernen Strand!", "tokens": ["Wirft", "aus", "am", "fer\u00b7nen", "Strand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbsei frei beim Wellengotte,", "tokens": ["\u00bb", "sei", "frei", "beim", "Wel\u00b7len\u00b7got\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da Sklaverei im Land!\u00ab", "tokens": ["Da", "Skla\u00b7ve\u00b7rei", "im", "Land", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wer's mit dem Blut bew\u00e4hret,", "tokens": ["Wer's", "mit", "dem", "Blut", "be\u00b7w\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Der ist ein Prinz vom Blut.", "tokens": ["Der", "ist", "ein", "Prinz", "vom", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Prinz Ludwig war bescheeret", "tokens": ["Prinz", "Lud\u00b7wig", "war", "be\u00b7schee\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NE", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und wie ein Schutzgott gut.", "tokens": ["Und", "wie", "ein", "Schutz\u00b7gott", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Auf seinem leichten Schimmel,", "tokens": ["Auf", "sei\u00b7nem", "leich\u00b7ten", "Schim\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf seiner Tigerdeck,", "tokens": ["Auf", "sei\u00b7ner", "Ti\u00b7ger\u00b7deck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da kam er wie vom Himmel", "tokens": ["Da", "kam", "er", "wie", "vom", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So sicher und so keck.", "tokens": ["So", "si\u00b7cher", "und", "so", "keck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Er war bei allen Dingen", "tokens": ["Er", "war", "bei", "al\u00b7len", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit seiner ganzen Seel,", "tokens": ["Mit", "sei\u00b7ner", "gan\u00b7zen", "Seel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mocht es ihm wohl gelingen,", "tokens": ["Mocht", "es", "ihm", "wohl", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Mocht es ihm schlagen fehl.", "tokens": ["Mocht", "es", "ihm", "schla\u00b7gen", "fehl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.10": {"line.1": {"text": "Er war bei allen Dingen", "tokens": ["Er", "war", "bei", "al\u00b7len", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In allem gleich geschickt.", "tokens": ["In", "al\u00b7lem", "gleich", "ge\u00b7schickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Feinden konnt er ringen,", "tokens": ["Mit", "Fein\u00b7den", "konnt", "er", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch Saitenklang begl\u00fcckt.", "tokens": ["Durch", "Sai\u00b7ten\u00b7klang", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Die Meister ihn umstanden", "tokens": ["Die", "Meis\u00b7ter", "ihn", "um\u00b7stan\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Demuth bei dem Spiel,", "tokens": ["In", "De\u00b7muth", "bei", "dem", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und seine Worte fanden", "tokens": ["Und", "sei\u00b7ne", "Wor\u00b7te", "fan\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie spitz'ge Pfeil ihr Ziel.", "tokens": ["Wie", "spitz'\u00b7ge", "Pfeil", "ihr", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Mit seinem hohen Leibe,", "tokens": ["Mit", "sei\u00b7nem", "ho\u00b7hen", "Lei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit seinem Stern und Gl\u00fcck", "tokens": ["Mit", "sei\u00b7nem", "Stern", "und", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Stellt er sich hin als Scheibe,", "tokens": ["Stellt", "er", "sich", "hin", "als", "Schei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "KOUS", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Ob heilig unser Geschick? \u2013", "tokens": ["Ob", "hei\u00b7lig", "un\u00b7ser", "Ge\u00b7schick", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADJD", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.13": {"line.1": {"text": "Nur f\u00fcr sich selbst kann stehen", "tokens": ["Nur", "f\u00fcr", "sich", "selbst", "kann", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PRF", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der edle, k\u00fchne Mann,", "tokens": ["Der", "ed\u00b7le", ",", "k\u00fch\u00b7ne", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob er die Welt gesehen,", "tokens": ["Ob", "er", "die", "Welt", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das pr\u00fcft er, wo er kann.", "tokens": ["Das", "pr\u00fcft", "er", ",", "wo", "er", "kann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Nicht in den Weg zu treten", "tokens": ["Nicht", "in", "den", "Weg", "zu", "tre\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der gro\u00dfen Weltenseel,", "tokens": ["Der", "gro\u00b7\u00dfen", "Wel\u00b7ten\u00b7seel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Darum will er nicht beten,", "tokens": ["Da\u00b7rum", "will", "er", "nicht", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Er bleibet ohne Fehl.", "tokens": ["Er", "blei\u00b7bet", "oh\u00b7ne", "Fehl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Denn was zum Heil begonnen,", "tokens": ["Denn", "was", "zum", "Heil", "be\u00b7gon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das f\u00e4ngt sich an mit Gl\u00fcck,", "tokens": ["Das", "f\u00e4ngt", "sich", "an", "mit", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Opferthiere sonnen", "tokens": ["Die", "Op\u00b7fer\u00b7thie\u00b7re", "son\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich in der Mordaxt Blick.", "tokens": ["Sich", "in", "der", "Mor\u00b7daxt", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Er l\u00e4\u00dft die Locken schneiden", "tokens": ["Er", "l\u00e4\u00dft", "die", "Lo\u00b7cken", "schnei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fcr die Geliebte sein,", "tokens": ["F\u00fcr", "die", "Ge\u00b7lieb\u00b7te", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Damit wenn er sollt scheiden,", "tokens": ["Da\u00b7mit", "wenn", "er", "sollt", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr blieb von ihm ein Schein.", "tokens": ["Ihr", "blieb", "von", "ihm", "ein", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "\u00bbumlockt vom Lorbeerkranze", "tokens": ["\u00bb", "um\u00b7lockt", "vom", "Lor\u00b7beer\u00b7kran\u00b7ze"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Komm ich zur\u00fcck zu dir,", "tokens": ["Komm", "ich", "zu\u00b7r\u00fcck", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sonst seh ich in dem Lenze", "tokens": ["Sonst", "seh", "ich", "in", "dem", "Len\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht mehr die gr\u00fcne Zier!\u00ab", "tokens": ["Nicht", "mehr", "die", "gr\u00fc\u00b7ne", "Zier", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "ADV", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Bet\u00e4ubt der Trommel Schallen!", "tokens": ["Be\u00b7t\u00e4ubt", "der", "Trom\u00b7mel", "Schal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was du voraus gesagt,", "tokens": ["Was", "du", "vo\u00b7raus", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Guten werden fallen,", "tokens": ["Die", "Gu\u00b7ten", "wer\u00b7den", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr Fall bleibt unbeklagt!", "tokens": ["Ihr", "Fall", "bleibt", "un\u00b7be\u00b7klagt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Ihr Fall wird nicht mehr n\u00fctzen,", "tokens": ["Ihr", "Fall", "wird", "nicht", "mehr", "n\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie fallen nach der Zeit,", "tokens": ["Sie", "fal\u00b7len", "nach", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Thoren werden sitzen", "tokens": ["Die", "Tho\u00b7ren", "wer\u00b7den", "sit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit klugem Spruch bereit.", "tokens": ["Mit", "klu\u00b7gem", "Spruch", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "\u00bbes sei! Doch nicht erblicken", "tokens": ["\u00bb", "es", "sei", "!", "Doch", "nicht", "er\u00b7bli\u00b7cken"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "$.", "KON", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Will ich die Schmach der Welt,", "tokens": ["Will", "ich", "die", "Schmach", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So nimm mein letzt Entz\u00fccken,", "tokens": ["So", "nimm", "mein", "letzt", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPOSAT", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich sieg, ich fall als Held!\u00ab", "tokens": ["Ich", "sieg", ",", "ich", "fall", "als", "Held", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "NN", "KOUS", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}