{"textgrid.poem.45230": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[will dich der Reichstag nicht erkennen]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Will dich der Reichstag nicht erkennen,", "tokens": ["Will", "dich", "der", "Reichs\u00b7tag", "nicht", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sei nicht erz\u00fcrnt ob solchen Streichs,", "tokens": ["Sei", "nicht", "er\u00b7z\u00fcrnt", "ob", "sol\u00b7chen", "Streichs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "VVFIN", "KOUS", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Reichstag ist ein Tag des Reichs,", "tokens": ["Der", "Reichs\u00b7tag", "ist", "ein", "Tag", "des", "Reichs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch die Jahrhunderte desselben Reiches,", "tokens": ["Doch", "die", "Jahr\u00b7hun\u00b7der\u00b7te", "des\u00b7sel\u00b7ben", "Rei\u00b7ches", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie werden Sch\u00fctzer dich und Retter nennen,", "tokens": ["Sie", "wer\u00b7den", "Sch\u00fct\u00b7zer", "dich", "und", "Ret\u00b7ter", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "PPER", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und, wer verst\u00e4ndig, tut schon jetzt ein Gleiches.", "tokens": ["Und", ",", "wer", "ver\u00b7st\u00e4n\u00b7dig", ",", "tut", "schon", "jetzt", "ein", "Glei\u00b7ches", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWS", "ADJD", "$,", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}