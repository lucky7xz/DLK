{"dta.poem.15793": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "53.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zwei Dichter wei\u00df ich, die zur h\u00f6chsten H\u00f6he flogen,", "tokens": ["Zwei", "Dich\u00b7ter", "wei\u00df", "ich", ",", "die", "zur", "h\u00f6chs\u00b7ten", "H\u00f6\u00b7he", "flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "$,", "PRELS", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bald Nachahmung bald Bewundrung nach sich zogen.", "tokens": ["Und", "bald", "Nac\u00b7hah\u00b7mung", "bald", "Be\u00b7wund\u00b7rung", "nach", "sich", "zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "ADV", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch zog der eine meist nach sich die gr\u00f6\u00dfre Schaar,", "tokens": ["Doch", "zog", "der", "ei\u00b7ne", "meist", "nach", "sich", "die", "gr\u00f6\u00df\u00b7re", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ART", "ADV", "APPR", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Inde\u00df des andern die gew\u00e4hlte kleinre war.", "tokens": ["In\u00b7de\u00df", "des", "an\u00b7dern", "die", "ge\u00b7w\u00e4hl\u00b7te", "klein\u00b7re", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "ART", "ADJA", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein hohes Ideal dem einen schwebte vor,", "tokens": ["Ein", "ho\u00b7hes", "I\u00b7deal", "dem", "ei\u00b7nen", "schweb\u00b7te", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "PIS", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zu dem er unverwandt sein Antlitz hielt empor,", "tokens": ["Zu", "dem", "er", "un\u00b7ver\u00b7wandt", "sein", "Ant\u00b7litz", "hielt", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und seinen Flug; doch nie konnt' es der Flug erreichen;", "tokens": ["Und", "sei\u00b7nen", "Flug", ";", "doch", "nie", "konnt'", "es", "der", "Flug", "er\u00b7rei\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$.", "ADV", "ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Je h\u00f6her er sich hob, je h\u00f6her mu\u00dft' es weichen.", "tokens": ["Je", "h\u00f6\u00b7her", "er", "sich", "hob", ",", "je", "h\u00f6\u00b7her", "mu\u00dft'", "es", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PRF", "VVFIN", "$,", "ADV", "ADJD", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Vom Ideale selbst der andre flog gehoben;", "tokens": ["Vom", "I\u00b7dea\u00b7le", "selbst", "der", "and\u00b7re", "flog", "ge\u00b7ho\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "PIS", "VVFIN", "VVPP", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Er war stets wo es war, nie unten er, es oben.", "tokens": ["Er", "war", "stets", "wo", "es", "war", ",", "nie", "un\u00b7ten", "er", ",", "es", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PWAV", "PPER", "VAFIN", "$,", "ADV", "ADV", "PPER", "$,", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Kein Aeu\u00dferliches wars, wonach er ringend strebte,", "tokens": ["Kein", "A\u00b7e\u00b7u\u00b7\u00dfer\u00b7li\u00b7ches", "wars", ",", "wo\u00b7nach", "er", "rin\u00b7gend", "streb\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Es war sein Innres selbst, das was er war und lebte.", "tokens": ["Es", "war", "sein", "Inn\u00b7res", "selbst", ",", "das", "was", "er", "war", "und", "leb\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADV", "$,", "PRELS", "PWS", "PPER", "VAFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Dem ringe nach! Es kann mit rechter Kraftanwendung", "tokens": ["Dem", "rin\u00b7ge", "nach", "!", "Es", "kann", "mit", "rech\u00b7ter", "Kraf\u00b7tan\u00b7wen\u00b7dung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PTKVZ", "$.", "PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Mensch auf jeder Stuf' erreichen die Vollendung.", "tokens": ["Der", "Mensch", "auf", "je\u00b7der", "Stuf'", "er\u00b7rei\u00b7chen", "die", "Vol\u00b7len\u00b7dung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}}}}}