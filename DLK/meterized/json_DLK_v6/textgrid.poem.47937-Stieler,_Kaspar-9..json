{"textgrid.poem.47937": {"metadata": {"author": {"name": "Stieler, Kaspar", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1669, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nas' an dir ist Spannen-lang/", "tokens": ["Die", "Nas'", "an", "dir", "ist", "Span\u00b7nen\u00b7lang", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VAFIN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "das Maul steht als ein Tohr-weg offen/", "tokens": ["das", "Maul", "steht", "als", "ein", "Tohr\u00b7weg", "of\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "die Z\u00e4ne sind zwey Daumen breit/", "tokens": ["die", "Z\u00e4\u00b7ne", "sind", "zwey", "Dau\u00b7men", "breit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "CARD", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "der Wangen Schwarz' ist Qwittengeel beloffen.", "tokens": ["der", "Wan\u00b7gen", "Schwa\u00b7rz'", "ist", "Qwit\u00b7ten\u00b7geel", "be\u00b7lof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Der Augen Glanz sicht wie die teure Zeit:", "tokens": ["Der", "Au\u00b7gen", "Glanz", "sicht", "wie", "die", "teu\u00b7re", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "doch bistu stolz und h\u00e4ltst dich trefflich sch\u00f6n/", "tokens": ["doch", "bis\u00b7tu", "stolz", "und", "h\u00e4ltst", "dich", "treff\u00b7lich", "sch\u00f6n", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "KON", "VVFIN", "PRF", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "das macht: ein Mahler hat die Venus abgerissen", "tokens": ["das", "macht", ":", "ein", "Mah\u00b7ler", "hat", "die", "Ve\u00b7nus", "ab\u00b7ge\u00b7ris\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$.", "ART", "NN", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und Mopsa oben an geschrieben/", "tokens": ["und", "Mop\u00b7sa", "o\u00b7ben", "an", "ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADV", "APZR", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Das Bild hastu vor deines angesehn", "tokens": ["Das", "Bild", "has\u00b7tu", "vor", "dei\u00b7nes", "an\u00b7ge\u00b7sehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "und meinst es m\u00fc\u00df' in dich sich jederman verlieben.", "tokens": ["und", "meinst", "es", "m\u00fc\u00df'", "in", "dich", "sich", "je\u00b7der\u00b7man", "ver\u00b7lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VMFIN", "APPR", "PPER", "PRF", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}