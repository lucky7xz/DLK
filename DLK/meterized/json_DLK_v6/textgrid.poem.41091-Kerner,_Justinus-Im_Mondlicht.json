{"textgrid.poem.41091": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Im Mondlicht", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Mond, dem Tau und Quelle", "tokens": ["Du", "Mond", ",", "dem", "Tau", "und", "Quel\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Innern ganz gebricht,", "tokens": ["Im", "In\u00b7nern", "ganz", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vertrockneter Geselle!", "tokens": ["Ver\u00b7trock\u00b7ne\u00b7ter", "Ge\u00b7sel\u00b7le", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gern weckst du Menschentr\u00e4nen,", "tokens": ["Gern", "weckst", "du", "Men\u00b7schen\u00b7tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Saugst sie mit deinem Licht.", "tokens": ["Saugst", "sie", "mit", "dei\u00b7nem", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Von mir kannst du nichts saugen:", "tokens": ["Von", "mir", "kannst", "du", "nichts", "sau\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie du, vertrocknet, alt,", "tokens": ["Wie", "du", ",", "ver\u00b7trock\u00b7net", ",", "alt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "VVFIN", "$,", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist Quelle meiner Augen,", "tokens": ["Ist", "Quel\u00b7le", "mei\u00b7ner", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist meines Herzens Bronnen,", "tokens": ["Ist", "mei\u00b7nes", "Her\u00b7zens", "Bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So alles tot und kalt.", "tokens": ["So", "al\u00b7les", "tot", "und", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Saug' nur mit deinem Schimmer", "tokens": ["Saug'", "nur", "mit", "dei\u00b7nem", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Tau, der dir gebricht,", "tokens": ["Den", "Tau", ",", "der", "dir", "ge\u00b7bricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus andern Augen immer,", "tokens": ["Aus", "an\u00b7dern", "Au\u00b7gen", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mond, du Vampir der Tr\u00e4nen,", "tokens": ["Mond", ",", "du", "Vam\u00b7pir", "der", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Die meinen weinen nicht.", "tokens": ["Die", "mei\u00b7nen", "wei\u00b7nen", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}