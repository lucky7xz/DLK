{"dta.poem.2283": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Amor im Garten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne sank nach Westen,", "tokens": ["Die", "Son\u00b7ne", "sank", "nach", "Wes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und machte noch im Sinken", "tokens": ["Und", "mach\u00b7te", "noch", "im", "Sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die letzte Abendr\u00f6te;", "tokens": ["Die", "letz\u00b7te", "A\u00b7bend\u00b7r\u00f6\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als mich ein k\u00fchler Zefir", "tokens": ["Als", "mich", "ein", "k\u00fch\u00b7ler", "Ze\u00b7fir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.5": {"text": "Aus meinem Zimmer lokkte.", "tokens": ["Aus", "mei\u00b7nem", "Zim\u00b7mer", "lokk\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich folgt ihm in das Gr\u00fcne,", "tokens": ["Ich", "folgt", "ihm", "in", "das", "Gr\u00fc\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wo tausend Rosen bl\u00fchten,", "tokens": ["Wo", "tau\u00b7send", "Ro\u00b7sen", "bl\u00fch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Um die er gauklend scherzte.", "tokens": ["Um", "die", "er", "gauk\u00b7lend", "scherz\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Der B\u00fcsche kleine S\u00e4nger", "tokens": ["Der", "B\u00fc\u00b7sche", "klei\u00b7ne", "S\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Erg\u00f6tzten mich im Stillen,", "tokens": ["Er\u00b7g\u00f6tz\u00b7ten", "mich", "im", "Stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Und meine Augenlieder", "tokens": ["Und", "mei\u00b7ne", "Au\u00b7gen\u00b7lie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Befiel ein s\u00fcsser Schlummer.", "tokens": ["Be\u00b7fiel", "ein", "s\u00fcs\u00b7ser", "Schlum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Ich tr\u00e4umte von der Liebe,", "tokens": ["Ich", "tr\u00e4um\u00b7te", "von", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Ich tr\u00e4umte von Dorinden,", "tokens": ["Ich", "tr\u00e4um\u00b7te", "von", "Do\u00b7rin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Von vielen andern Sch\u00f6nen,", "tokens": ["Von", "vie\u00b7len", "an\u00b7dern", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Und von der lieben Venus.", "tokens": ["Und", "von", "der", "lie\u00b7ben", "Ve\u00b7nus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.17": {"text": "Ich k\u00fc\u00dfte sie im Traume,", "tokens": ["Ich", "k\u00fc\u00df\u00b7te", "sie", "im", "Trau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Ich sa\u00df auf ihrem Schosse,", "tokens": ["Ich", "sa\u00df", "auf", "ih\u00b7rem", "Schos\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Und sagt ihr von Dorinden.", "tokens": ["Und", "sagt", "ihr", "von", "Do\u00b7rin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Sie hielt mich in den Armen,", "tokens": ["Sie", "hielt", "mich", "in", "den", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Und sprach: Sie soll dich lieben.", "tokens": ["Und", "sprach", ":", "Sie", "soll", "dich", "lie\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Schnell ward ich wieder munter.", "tokens": ["Schnell", "ward", "ich", "wie\u00b7der", "mun\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Ich sah mich um, und lauschte;", "tokens": ["Ich", "sah", "mich", "um", ",", "und", "lauschte", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Denn unter frischen Rosen", "tokens": ["Denn", "un\u00b7ter", "fri\u00b7schen", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Fand ich mich ganz begraben.", "tokens": ["Fand", "ich", "mich", "ganz", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.26": {"text": "Ich sprang von meinem Lager,", "tokens": ["Ich", "sprang", "von", "mei\u00b7nem", "La\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Den losen Gast zu suchen,", "tokens": ["Den", "lo\u00b7sen", "Gast", "zu", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Der mich so sch\u00f6n bedekket;", "tokens": ["Der", "mich", "so", "sch\u00f6n", "be\u00b7dek\u00b7ket", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Allein im schnellen Springen", "tokens": ["Al\u00b7lein", "im", "schnel\u00b7len", "Sprin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Empfand ich pl\u00f6tzlich Schmerzen.", "tokens": ["Emp\u00b7fand", "ich", "pl\u00f6tz\u00b7lich", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Ein kleines Kind mit Fl\u00fcgeln,", "tokens": ["Ein", "klei\u00b7nes", "Kind", "mit", "Fl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Das ich noch nie gesehen,", "tokens": ["Das", "ich", "noch", "nie", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Sa\u00df l\u00e4chelnd hinterm Busche,", "tokens": ["Sa\u00df", "l\u00e4\u00b7chelnd", "hin\u00b7term", "Bu\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Und sprach: Dis kan mein Bogen,", "tokens": ["Und", "sprach", ":", "Dis", "kan", "mein", "Bo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PDS", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.35": {"text": "Und wies mir mit dem Bogen", "tokens": ["Und", "wies", "mir", "mit", "dem", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Dorinden in der Laube.", "tokens": ["Do\u00b7rin\u00b7den", "in", "der", "Lau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "Ich wei\u00df nicht, welche Wunde!", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wel\u00b7che", "Wun\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Sie schmerzt, und that doch sanfte,", "tokens": ["Sie", "schmerzt", ",", "und", "that", "doch", "sanf\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Und, als ich nur die Sch\u00f6ne", "tokens": ["Und", ",", "als", "ich", "nur", "die", "Sch\u00f6\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "Drauf in der Laub\u2019 erblikkte,", "tokens": ["Drauf", "in", "der", "Laub'", "er\u00b7blikk\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.41": {"text": "Verschwanden alle Schmerzen;", "tokens": ["Ver\u00b7schwan\u00b7den", "al\u00b7le", "Schmer\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.42": {"text": "Denn sie war gar zu freundlich.", "tokens": ["Denn", "sie", "war", "gar", "zu", "freund\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}