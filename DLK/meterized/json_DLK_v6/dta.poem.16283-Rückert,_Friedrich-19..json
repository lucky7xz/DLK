{"dta.poem.16283": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn wir dich gr\u00fc\u00dfen, f\u00fchlst du dich vom Dank beschwert,", "tokens": ["Wenn", "wir", "dich", "gr\u00fc\u00b7\u00dfen", ",", "f\u00fchlst", "du", "dich", "vom", "Dank", "be\u00b7schwert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "$,", "VVFIN", "PPER", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gr\u00fc\u00dfen wir dich nicht, so bist du ungeehrt.", "tokens": ["Und", "gr\u00fc\u00b7\u00dfen", "wir", "dich", "nicht", ",", "so", "bist", "du", "un\u00b7geehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "PTKNEG", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So sage denn wie man es dir zu Danke macht,", "tokens": ["So", "sa\u00b7ge", "denn", "wie", "man", "es", "dir", "zu", "Dan\u00b7ke", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "PWAV", "PIS", "PPER", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn dich von uns verdrie\u00dft die Unacht wie die Acht?", "tokens": ["Wenn", "dich", "von", "uns", "ver\u00b7drie\u00dft", "die", "U\u00b7nacht", "wie", "die", "Acht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "p