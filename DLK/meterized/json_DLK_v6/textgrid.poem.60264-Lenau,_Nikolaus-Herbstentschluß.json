{"textgrid.poem.60264": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Herbstentschlu\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tr\u00fcbe Wolken, Herbstesluft,", "tokens": ["Tr\u00fc\u00b7be", "Wol\u00b7ken", ",", "Herbs\u00b7tes\u00b7luft", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Einsam wandl ich meine Stra\u00dfen,", "tokens": ["Ein\u00b7sam", "wandl", "ich", "mei\u00b7ne", "Stra\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Welkes Laub, kein Vogel ruft \u2013", "tokens": ["Wel\u00b7kes", "Laub", ",", "kein", "Vo\u00b7gel", "ruft", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "$,", "PIAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, wie stille! wie verlassen!", "tokens": ["Ach", ",", "wie", "stil\u00b7le", "!", "wie", "ver\u00b7las\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "$.", "PWAV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Todesk\u00fchl der Winter naht;", "tokens": ["To\u00b7des\u00b7k\u00fchl", "der", "Win\u00b7ter", "naht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo sind, W\u00e4lder, eure Wonnen?", "tokens": ["Wo", "sind", ",", "W\u00e4l\u00b7der", ",", "eu\u00b7re", "Won\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fluren, eurer vollen Saat", "tokens": ["Flu\u00b7ren", ",", "eu\u00b7rer", "vol\u00b7len", "Saat"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Goldne Wellen sind verronnen!", "tokens": ["Gold\u00b7ne", "Wel\u00b7len", "sind", "ver\u00b7ron\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Es ist worden k\u00fchl und sp\u00e4t,", "tokens": ["Es", "ist", "wor\u00b7den", "k\u00fchl", "und", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VAPP", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nebel auf der Wiese weidet,", "tokens": ["Ne\u00b7bel", "auf", "der", "Wie\u00b7se", "wei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch die \u00f6den Haine weht", "tokens": ["Durch", "die", "\u00f6\u00b7den", "Hai\u00b7ne", "weht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Heimweh; \u2013 alles flieht und scheidet.", "tokens": ["Heim\u00b7weh", ";", "\u2013", "al\u00b7les", "flieht", "und", "schei\u00b7det", "."], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Herz, vernimmst du diesen Klang", "tokens": ["Herz", ",", "ver\u00b7nimmst", "du", "die\u00b7sen", "Klang"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "von den felsentst\u00fcrzten B\u00e4chen?", "tokens": ["von", "den", "fel\u00b7sent\u00b7st\u00fcrz\u00b7ten", "B\u00e4\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zeit gewesen w\u00e4r es lang,", "tokens": ["Zeit", "ge\u00b7we\u00b7sen", "w\u00e4r", "es", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAPP", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df wir ernsthaft uns bespr\u00e4chen!", "tokens": ["Da\u00df", "wir", "ernst\u00b7haft", "uns", "be\u00b7spr\u00e4\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Herz, du hast dir selber oft", "tokens": ["Herz", ",", "du", "hast", "dir", "sel\u00b7ber", "oft"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wehgetan und hast es andern,", "tokens": ["Weh\u00b7ge\u00b7tan", "und", "hast", "es", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VAFIN", "PPER", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil du hast geliebt, gehofft;", "tokens": ["Weil", "du", "hast", "ge\u00b7liebt", ",", "ge\u00b7hofft", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "VVPP", "$,", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun ists aus, wir m\u00fcssen wandern!", "tokens": ["Nun", "ists", "aus", ",", "wir", "m\u00fcs\u00b7sen", "wan\u00b7dern", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKVZ", "$,", "PPER", "VMFIN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Auf die Reise will ich fest", "tokens": ["Auf", "die", "Rei\u00b7se", "will", "ich", "fest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein dich schlie\u00dfen und verwahren,", "tokens": ["Ein", "dich", "schlie\u00b7\u00dfen", "und", "ver\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Drau\u00dfen mag ein linder West", "tokens": ["Drau\u00b7\u00dfen", "mag", "ein", "lin\u00b7der", "West"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder Sturm vor\u00fcberfahren;", "tokens": ["O\u00b7der", "Sturm", "vor\u00b7\u00fc\u00b7berf\u00b7ah\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Da\u00df wir unsern letzten Gang", "tokens": ["Da\u00df", "wir", "un\u00b7sern", "letz\u00b7ten", "Gang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schweigsam wandeln und alleine,", "tokens": ["Schweig\u00b7sam", "wan\u00b7deln", "und", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "KON", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df auf unsern Grabeshang", "tokens": ["Da\u00df", "auf", "un\u00b7sern", "Gra\u00b7bes\u00b7hang"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Niemand als der Regen weine!", "tokens": ["Nie\u00b7mand", "als", "der", "Re\u00b7gen", "wei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}