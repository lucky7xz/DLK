{"dta.poem.16140": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "194.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein alter Weiser lehrt, da\u00df Tugend vielerlei,", "tokens": ["Ein", "al\u00b7ter", "Wei\u00b7ser", "lehrt", ",", "da\u00df", "Tu\u00b7gend", "vie\u00b7ler\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "KOUS", "NN", "PIAT", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch stets ein Mittleres von zweien Aeu\u00dfern sei;", "tokens": ["Doch", "stets", "ein", "Mitt\u00b7le\u00b7res", "von", "zwei\u00b7en", "A\u00b7e\u00b7u\u00b7\u00dfern", "sei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "CARD", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Im Wesen selber eins, doch von verschiednen Namen,", "tokens": ["Im", "We\u00b7sen", "sel\u00b7ber", "eins", ",", "doch", "von", "ver\u00b7schied\u00b7nen", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PIS", "$,", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie viele Sch\u00f6\u00dflinge aus einer Wurzel kamen.", "tokens": ["Wie", "vie\u00b7le", "Sch\u00f6\u00df\u00b7lin\u00b7ge", "aus", "ei\u00b7ner", "Wur\u00b7zel", "ka\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Gerechtigkeit, entfernt von Zu- und Gegenneigung,", "tokens": ["Ge\u00b7rech\u00b7tig\u00b7keit", ",", "ent\u00b7fernt", "von", "Zu", "und", "Ge\u00b7gen\u00b7nei\u00b7gung", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "APPR", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Vorlieb' und Mislieb', Abgunst und Gunstbezeigung.", "tokens": ["Von", "Vor\u00b7lieb'", "und", "Mis\u00b7lieb'", ",", "Ab\u00b7gunst", "und", "Gunst\u00b7be\u00b7zei\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Leutseligkeit, entfernt von Schmeichelei und Trutz,", "tokens": ["Leut\u00b7se\u00b7lig\u00b7keit", ",", "ent\u00b7fernt", "von", "Schmei\u00b7che\u00b7lei", "und", "Trutz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie Wohlanst\u00e4ndigkeit von Flitterpracht und Schmutz.", "tokens": ["Wie", "Wohl\u00b7an\u00b7st\u00e4n\u00b7dig\u00b7keit", "von", "Flit\u00b7ter\u00b7pracht", "und", "Schmutz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Mannhaftigkeit, entfernt von Trotzigkeit und Zagnis,", "tokens": ["Mann\u00b7haf\u00b7tig\u00b7keit", ",", "ent\u00b7fernt", "von", "Trot\u00b7zig\u00b7keit", "und", "Zag\u00b7nis", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Tapferkeit, von Furcht und \u00fcberm\u00fcth'gem Wagnis.", "tokens": ["Und", "Tap\u00b7fer\u00b7keit", ",", "von", "Furcht", "und", "\u00fc\u00b7ber\u00b7m\u00fcth'\u00b7gem", "Wag\u00b7nis", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Freigebigkeit, gleichfern von Geiz und von Verschwendung;", "tokens": ["Frei\u00b7ge\u00b7big\u00b7keit", ",", "gleich\u00b7fern", "von", "Geiz", "und", "von", "Ver\u00b7schwen\u00b7dung", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Besonnenheit, so fern von Arglist als Verblendung.", "tokens": ["Be\u00b7son\u00b7nen\u00b7heit", ",", "so", "fern", "von", "Arg\u00b7list", "als", "Ver\u00b7blen\u00b7dung", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "APPR", "NN", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Der Glaube, gleich entfernt von Un- und Ueberglauben,", "tokens": ["Der", "Glau\u00b7be", ",", "gleich", "ent\u00b7fernt", "von", "Un", "und", "Ue\u00b7berg\u00b7lau\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "APPR", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der nichts dir dringet auf, und nichts sich l\u00e4sset rauben.", "tokens": ["Der", "nichts", "dir", "drin\u00b7get", "auf", ",", "und", "nichts", "sich", "l\u00e4s\u00b7set", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PPER", "VVFIN", "PTKVZ", "$,", "KON", "PIS", "PRF", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Die N\u00fcchternheit, entfernt von Schlemmerei und Fasten;", "tokens": ["Die", "N\u00fcch\u00b7tern\u00b7heit", ",", "ent\u00b7fernt", "von", "Schlem\u00b7me\u00b7rei", "und", "Fas\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die R\u00fchrigkeit, entfernt von Uebereil' und Rasten.", "tokens": ["Die", "R\u00fch\u00b7rig\u00b7keit", ",", "ent\u00b7fernt", "von", "Ue\u00b7be\u00b7reil'", "und", "Ras\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Demuth, gleichweit von Stolz und Niedertr\u00e4chtigkeit,", "tokens": ["De\u00b7muth", ",", "gleich\u00b7weit", "von", "Stolz", "und", "Nie\u00b7der\u00b7tr\u00e4ch\u00b7tig\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie Leibeswohlgestalt von Fett und Schm\u00e4chtigkeit.", "tokens": ["Wie", "Lei\u00b7bes\u00b7wohl\u00b7ge\u00b7stalt", "von", "Fett", "und", "Schm\u00e4ch\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Das Mittelma\u00df ist gut dem Alter wie der Jugend,", "tokens": ["Das", "Mit\u00b7tel\u00b7ma\u00df", "ist", "gut", "dem", "Al\u00b7ter", "wie", "der", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur Mittelm\u00e4\u00dfigkeit allein ist keine Tugend.", "tokens": ["Nur", "Mit\u00b7tel\u00b7m\u00e4\u00b7\u00dfig\u00b7keit", "al\u00b7lein", "ist", "kei\u00b7ne", "Tu\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADV", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Im Mittelma\u00df vereint sich zweier Aeu\u00dfern Kraft,", "tokens": ["Im", "Mit\u00b7tel\u00b7ma\u00df", "ver\u00b7eint", "sich", "zwei\u00b7er", "A\u00b7e\u00b7u\u00b7\u00dfern", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Doch Mittelm\u00e4\u00dfigkeit ist beider untheilhaft.", "tokens": ["Doch", "Mit\u00b7tel\u00b7m\u00e4\u00b7\u00dfig\u00b7keit", "ist", "bei\u00b7der", "un\u00b7theil\u00b7haft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}