{"textgrid.poem.62581": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Geh hin! mein Heyland Jesu Christ!", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geh hin! mein Heyland Jesu Christ!", "tokens": ["Geh", "hin", "!", "mein", "Hey\u00b7land", "Je\u00b7su", "Christ", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "PPOSAT", "NN", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geh hin nach Salems Mauren,", "tokens": ["Geh", "hin", "nach", "Sa\u00b7lems", "Mau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo deine Feinde jetzt mit List", "tokens": ["Wo", "dei\u00b7ne", "Fein\u00b7de", "jetzt", "mit", "List"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Bosheit auf dich lauren.", "tokens": ["Und", "Bos\u00b7heit", "auf", "dich", "lau\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Geh hin! dein Kampf-Platz ist dir nah,", "tokens": ["Geh", "hin", "!", "dein", "Kampf\u00b7Platz", "ist", "dir", "nah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Die Leidens-Stunde ist nun da,", "tokens": ["Die", "Lei\u00b7dens\u00b7Stun\u00b7de", "ist", "nun", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Damit auf dieser Erde", "tokens": ["Da\u00b7mit", "auf", "die\u00b7ser", "Er\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des Vaters Zorn von Ewigkeit", "tokens": ["Des", "Va\u00b7ters", "Zorn", "von", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Zu unsrer Seelen Trost und Freud,", "tokens": ["Zu", "uns\u00b7rer", "See\u00b7len", "Trost", "und", "Freud", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Durch dich vollendet werde.", "tokens": ["Durch", "dich", "voll\u00b7en\u00b7det", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Geh hin! und schwitze warmes Blut,", "tokens": ["Geh", "hin", "!", "und", "schwit\u00b7ze", "war\u00b7mes", "Blut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "KON", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df dich die Angst entstellen!", "tokens": ["La\u00df", "dich", "die", "Angst", "ent\u00b7stel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Empfinde Gottes Zorn und Glut,", "tokens": ["Emp\u00b7fin\u00b7de", "Got\u00b7tes", "Zorn", "und", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und alle Macht der H\u00f6llen.", "tokens": ["Und", "al\u00b7le", "Macht", "der", "H\u00f6l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Herr! dieses Leiden tr\u00f6stet mich,", "tokens": ["Herr", "!", "die\u00b7ses", "Lei\u00b7den", "tr\u00f6s\u00b7tet", "mich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PDAT", "NN", "VVFIN", "PPER", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Wenn mich die H\u00f6lle m\u00e4chtiglich", "tokens": ["Wenn", "mich", "die", "H\u00f6l\u00b7le", "m\u00e4ch\u00b7tig\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Will peinigen und plagen.", "tokens": ["Will", "pei\u00b7ni\u00b7gen", "und", "pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ja, dieser Kampf hilft mir in Noth,", "tokens": ["Ja", ",", "die\u00b7ser", "Kampf", "hilft", "mir", "in", "Noth", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDAT", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "In Pein und Quaal, ja selbst im Tod", "tokens": ["In", "Pein", "und", "Qua\u00b7al", ",", "ja", "selbst", "im", "Tod"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "All meine Angst ertragen.", "tokens": ["All", "mei\u00b7ne", "Angst", "er\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Geh hin, mein liebster Seelen-Freund!", "tokens": ["Geh", "hin", ",", "mein", "liebs\u00b7ter", "See\u00b7len\u00b7Freund", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und la\u00df dich gerne binden,", "tokens": ["Und", "la\u00df", "dich", "ger\u00b7ne", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So wird an mir der arge Feind,", "tokens": ["So", "wird", "an", "mir", "der", "ar\u00b7ge", "Feind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht leichtlich Herrschaft finden.", "tokens": ["Nicht", "leicht\u00b7lich", "Herr\u00b7schaft", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Herr! deine Bande dr\u00fccken dich,", "tokens": ["Herr", "!", "dei\u00b7ne", "Ban\u00b7de", "dr\u00fc\u00b7cken", "dich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Sie sind mein Schmuck, und k\u00f6nnen mich", "tokens": ["Sie", "sind", "mein", "Schmuck", ",", "und", "k\u00f6n\u00b7nen", "mich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "KON", "VMFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von Sattans Strick und Ketten;", "tokens": ["Von", "Sat\u00b7tans", "Strick", "und", "Ket\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Von Mosis Fluch und Finsterni\u00df,", "tokens": ["Von", "Mo\u00b7sis", "Fluch", "und", "Fins\u00b7ter\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die\u00df wei\u00df und glaub ich ganz gewi\u00df,", "tokens": ["Die\u00df", "wei\u00df", "und", "glaub", "ich", "ganz", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und von der H\u00f6ll erretten.", "tokens": ["Und", "von", "der", "H\u00f6ll", "er\u00b7ret\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Geh hin! mein Heil! und stelle dich", "tokens": ["Geh", "hin", "!", "mein", "Heil", "!", "und", "stel\u00b7le", "dich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$.", "PPOSAT", "NN", "$.", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den J\u00fcden und den Heyden.", "tokens": ["Den", "J\u00fc\u00b7den", "und", "den", "Hey\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Herr dein Verh\u00f6r das tr\u00f6stet mich", "tokens": ["Herr", "dein", "Ver\u00b7h\u00f6r", "das", "tr\u00f6s\u00b7tet", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "PDS", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In meinem S\u00fcnden-Leiden.", "tokens": ["In", "mei\u00b7nem", "S\u00fcn\u00b7den\u00b7Lei\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "H\u00e4llt Tod und H\u00f6ll, und Sattan mir", "tokens": ["H\u00e4llt", "Tod", "und", "H\u00f6ll", ",", "und", "Sat\u00b7tan", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN", "$,", "KON", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gesetz und Straf und S\u00fcnde f\u00fcr,", "tokens": ["Ge\u00b7setz", "und", "Straf", "und", "S\u00fcn\u00b7de", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So sch\u00fctzt mich dein Verklagen.", "tokens": ["So", "sch\u00fctzt", "mich", "dein", "Ver\u00b7kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die\u00df wiederstehet dem Gericht", "tokens": ["Die\u00df", "wie\u00b7ders\u00b7te\u00b7het", "dem", "Ge\u00b7richt"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Und Gottes Zorn, und l\u00e4\u00dft mich nicht", "tokens": ["Und", "Got\u00b7tes", "Zorn", ",", "und", "l\u00e4\u00dft", "mich", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "NN", "$,", "KON", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "In meiner Angst verzagen.", "tokens": ["In", "mei\u00b7ner", "Angst", "ver\u00b7za\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Geh hin! mein treuer Herr und Gott!", "tokens": ["Geh", "hin", "!", "mein", "treu\u00b7er", "Herr", "und", "Gott", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "PPOSAT", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und la\u00df dich da verspotten.", "tokens": ["Und", "la\u00df", "dich", "da", "ver\u00b7spot\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hier will man sich zu deiner Noth", "tokens": ["Hier", "will", "man", "sich", "zu", "dei\u00b7ner", "Noth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Macht zusammen rotten.", "tokens": ["Mit", "Macht", "zu\u00b7sam\u00b7men", "rot\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Schmach, Hohn und Speichel warten dein,", "tokens": ["Schmach", ",", "Hohn", "und", "Spei\u00b7chel", "war\u00b7ten", "dein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Soll dieses deine Ehre seyn?", "tokens": ["Soll", "die\u00b7ses", "dei\u00b7ne", "Eh\u00b7re", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O K\u00f6nig aller Ehren!", "tokens": ["O", "K\u00f6\u00b7nig", "al\u00b7ler", "Eh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Doch steh es aus: denn deine Schmach,", "tokens": ["Doch", "steh", "es", "aus", ":", "denn", "dei\u00b7ne", "Schmach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$.", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Dein Hohn zieht mir die Ehre nach.", "tokens": ["Dein", "Hohn", "zieht", "mir", "die", "Eh\u00b7re", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Dort werd ichs sehn und h\u00f6ren.", "tokens": ["Dort", "werd", "ichs", "sehn", "und", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Geh hin! mein Gott! la\u00df deinen Leib", "tokens": ["Geh", "hin", "!", "mein", "Gott", "!", "la\u00df", "dei\u00b7nen", "Leib"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$.", "PPOSAT", "NN", "$.", "VVIMP", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Schl\u00e4g und Striemen f\u00e4rben.", "tokens": ["Mit", "Schl\u00e4g", "und", "Strie\u00b7men", "f\u00e4r\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Durch dieses l\u00e4\u00dft du Mann und Weib,", "tokens": ["Durch", "die\u00b7ses", "l\u00e4\u00dft", "du", "Mann", "und", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In keinem Kreuze sterben.", "tokens": ["In", "kei\u00b7nem", "Kreu\u00b7ze", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Herr! deine blutge Dornen-Kron", "tokens": ["Herr", "!", "dei\u00b7ne", "blut\u00b7ge", "Dor\u00b7nen\u00b7Kron"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erwirbt mir der Gerechten Lohn,", "tokens": ["Er\u00b7wirbt", "mir", "der", "Ge\u00b7rech\u00b7ten", "Lohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dein Rohr die Sieges-Palmen.", "tokens": ["Dein", "Rohr", "die", "Sie\u00b7ges\u00b7Pal\u00b7men", "."], "token_info": ["word", "word", "wor