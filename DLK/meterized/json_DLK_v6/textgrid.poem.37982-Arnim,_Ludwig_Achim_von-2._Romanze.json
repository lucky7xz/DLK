{"textgrid.poem.37982": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "2. Romanze", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als er auf Stauffenberg nun kam,", "tokens": ["Als", "er", "auf", "Stauf\u00b7fen\u00b7berg", "nun", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schnell sprang da ab der edle Mann,", "tokens": ["Schnell", "sprang", "da", "ab", "der", "ed\u00b7le", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein jeder wollt ihn sehen, h\u00f6ren,", "tokens": ["Ein", "je\u00b7der", "wollt", "ihn", "se\u00b7hen", ",", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "PPER", "VVINF", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein jeder wollt ihn h\u00f6her ehren,", "tokens": ["Ein", "je\u00b7der", "wollt", "ihn", "h\u00f6\u00b7her", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Von seinen Dienern gro\u00dfe Eil,", "tokens": ["Von", "sei\u00b7nen", "Die\u00b7nern", "gro\u00b7\u00dfe", "Eil", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von Fraun und M\u00e4dchen gro\u00df Kurzweil.", "tokens": ["Von", "Fraun", "und", "M\u00e4d\u00b7chen", "gro\u00df", "Kurz\u00b7weil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zu Bette trachtet nur der Herr,", "tokens": ["Zu", "Bet\u00b7te", "trach\u00b7tet", "nur", "der", "Herr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach seiner Frau verlangt er sehr,", "tokens": ["Nach", "sei\u00b7ner", "Frau", "ver\u00b7langt", "er", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Viel herrlich Rauchwerk ward gemacht,", "tokens": ["Viel", "herr\u00b7lich", "Rauch\u00b7werk", "ward", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Bett verh\u00e4ngt mit gro\u00dfer Pracht,", "tokens": ["Das", "Bett", "ver\u00b7h\u00e4ngt", "mit", "gro\u00b7\u00dfer", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Dienern bald erlauben th\u00e4t,", "tokens": ["Den", "Die\u00b7nern", "bald", "er\u00b7lau\u00b7ben", "th\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df sie sich legten all zu Bett.", "tokens": ["Da\u00df", "sie", "sich", "leg\u00b7ten", "all", "zu", "Bett", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "PIAT", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er zog sich ab, sezt sich aufs Bett,", "tokens": ["Er", "zog", "sich", "ab", ",", "sezt", "sich", "aufs", "Bett", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$,", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und zu sich selber also redt:", "tokens": ["Und", "zu", "sich", "sel\u00b7ber", "al\u00b7so", "redt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRF", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbo h\u00e4tt ich sie im Arm allein,", "tokens": ["\u00bb", "o", "h\u00e4tt", "ich", "sie", "im", "Arm", "al\u00b7lein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "VAFIN", "PPER", "PPER", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die heut ich fand auf hohem Stein!\u00ab", "tokens": ["Die", "heut", "ich", "fand", "auf", "ho\u00b7hem", "Stein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als er die Worte kaum noch sprach,", "tokens": ["Als", "er", "die", "Wor\u00b7te", "kaum", "noch", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Sch\u00f6ne er mit Augen sah.", "tokens": ["Die", "Sch\u00f6\u00b7ne", "er", "mit", "Au\u00b7gen", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Viel froher Minne sie begehn,", "tokens": ["Viel", "fro\u00b7her", "Min\u00b7ne", "sie", "be\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie mochten einander ins Herze sehn,", "tokens": ["Sie", "moch\u00b7ten", "ein\u00b7an\u00b7der", "ins", "Her\u00b7ze", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wenn einer th\u00e4t dem nachgedenken,", "tokens": ["Wenn", "ei\u00b7ner", "th\u00e4t", "dem", "nach\u00b7ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "ART", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So m\u00f6chte ihn wohl die Sehnsucht kr\u00e4nken.", "tokens": ["So", "m\u00f6ch\u00b7te", "ihn", "wohl", "die", "Sehn\u00b7sucht", "kr\u00e4n\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Als er erwachte, glaubt ers kaum,", "tokens": ["Als", "er", "er\u00b7wach\u00b7te", ",", "glaubt", "ers", "kaum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er fand den Ring, sonst wars ein Traum.", "tokens": ["Er", "fand", "den", "Ring", ",", "sonst", "wars", "ein", "Traum", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}