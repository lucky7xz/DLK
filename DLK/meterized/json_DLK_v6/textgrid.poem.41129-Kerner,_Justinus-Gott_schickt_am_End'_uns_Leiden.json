{"textgrid.poem.41129": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Gott schickt am End' uns Leiden", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott schickt am End' uns Leiden,", "tokens": ["Gott", "schickt", "am", "End'", "uns", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf da\u00df uns diese Welt,", "tokens": ["Auf", "da\u00df", "uns", "die\u00b7se", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn wir nun von ihr scheiden,", "tokens": ["Wenn", "wir", "nun", "von", "ihr", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht mehr so m\u00e4chtig h\u00e4lt.", "tokens": ["Nicht", "mehr", "so", "m\u00e4ch\u00b7tig", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Mutter legt den Br\u00fcsten", "tokens": ["Die", "Mut\u00b7ter", "legt", "den", "Br\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am End' was Bittres bei,", "tokens": ["Am", "End'", "was", "Bitt\u00b7res", "bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PWS", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf da\u00df des Kinds Gel\u00fcsten", "tokens": ["Auf", "da\u00df", "des", "Kinds", "Ge\u00b7l\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht mehr so m\u00e4chtig sei.", "tokens": ["Nicht", "mehr", "so", "m\u00e4ch\u00b7tig", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Pflanze wird der Bl\u00e4tter", "tokens": ["Die", "Pflan\u00b7ze", "wird", "der", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Bl\u00fcten erst beraubt,", "tokens": ["Und", "Bl\u00fc\u00b7ten", "erst", "be\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bevor in Herbsteswetter", "tokens": ["Be\u00b7vor", "in", "Herbs\u00b7tes\u00b7wet\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie senkt ihr m\u00fcdes Haupt.", "tokens": ["Sie", "senkt", "ihr", "m\u00fc\u00b7des", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Was willst du dich beschweren,", "tokens": ["Was", "willst", "du", "dich", "be\u00b7schwe\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df welk die Freuden sind?", "tokens": ["Da\u00df", "welk", "die", "Freu\u00b7den", "sind", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du darfst nicht mehr begehren,", "tokens": ["Du", "darfst", "nicht", "mehr", "be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Herz! als Kraut und Kind.", "tokens": ["Mein", "Herz", "!", "als", "Kraut", "und", "Kind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "KOUS", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}