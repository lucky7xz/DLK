{"textgrid.poem.64826": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: O k\u00f6nnte ich dich von mir werfen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O k\u00f6nnte ich dich von mir werfen", "tokens": ["O", "k\u00f6nn\u00b7te", "ich", "dich", "von", "mir", "wer\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "PRF", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du Fluch des Sangs, der auf mir ruht!", "tokens": ["Du", "Fluch", "des", "Sangs", ",", "der", "auf", "mir", "ruht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du Todesstahl, der, sich zu sch\u00e4rfen,", "tokens": ["Du", "To\u00b7des\u00b7stahl", ",", "der", ",", "sich", "zu", "sch\u00e4r\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "$,", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ergl\u00fcht in meines Herzens Blut,", "tokens": ["Er\u00b7gl\u00fcht", "in", "mei\u00b7nes", "Her\u00b7zens", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wieder dann in meinen Thr\u00e4nen", "tokens": ["Und", "wie\u00b7der", "dann", "in", "mei\u00b7nen", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die K\u00fchlung suchet, die ihm noth!", "tokens": ["Die", "K\u00fch\u00b7lung", "su\u00b7chet", ",", "die", "ihm", "noth", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zerst\u00f6rt hat mich Dein glei\u00dfend H\u00f6hnen;", "tokens": ["Zer\u00b7st\u00f6rt", "hat", "mich", "Dein", "glei\u00b7\u00dfend", "H\u00f6h\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich lebe nicht und bin nicht todt!", "tokens": ["Ich", "le\u00b7be", "nicht", "und", "bin", "nicht", "todt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "KON", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich lebe nicht! denn auf der Erde", "tokens": ["Ich", "le\u00b7be", "nicht", "!", "denn", "auf", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$.", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wall' ich umher, ein fremder Gast,", "tokens": ["Wall'", "ich", "um\u00b7her", ",", "ein", "frem\u00b7der", "Gast", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bin heimathlich an keinem Herde,", "tokens": ["Bin", "hei\u00b7math\u00b7lich", "an", "kei\u00b7nem", "Her\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bin nicht geliebt und nicht geha\u00dft,", "tokens": ["Bin", "nicht", "ge\u00b7liebt", "und", "nicht", "ge\u00b7ha\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "VVPP", "KON", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hab keinen Antheil an den Gaben,", "tokens": ["Hab", "kei\u00b7nen", "An\u00b7theil", "an", "den", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Gott den Menschen mild verleiht;", "tokens": ["Die", "Gott", "den", "Men\u00b7schen", "mild", "ver\u00b7leiht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was sie erquickt kann mich nicht laben \u2013", "tokens": ["Was", "sie", "er\u00b7quickt", "kann", "mich", "nicht", "la\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich bin nicht aus der Zeitlichkeit!", "tokens": ["Ich", "bin", "nicht", "aus", "der", "Zeit\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich bin nicht todt! denn tief im Herzen", "tokens": ["Ich", "bin", "nicht", "todt", "!", "denn", "tief", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$.", "ADV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Regt sich der Wunsch noch gl\u00fchend hei\u00df,", "tokens": ["Regt", "sich", "der", "Wunsch", "noch", "gl\u00fc\u00b7hend", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADV", "ADJD", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Nach sel'ger Freude Fr\u00fchlingsscherzen,", "tokens": ["Nach", "sel'\u00b7ger", "Freu\u00b7de", "Fr\u00fch\u00b7lings\u00b7scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach jungen Gl\u00fcckes frischem Reis!", "tokens": ["Nach", "jun\u00b7gen", "Gl\u00fc\u00b7ckes", "fri\u00b7schem", "Reis", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Noch dr\u00e4nget sich mir auf die Frage:", "tokens": ["Noch", "dr\u00e4n\u00b7get", "sich", "mir", "auf", "die", "Fra\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ob ich allein dem Schmerz geweiht?", "tokens": ["Ob", "ich", "al\u00b7lein", "dem", "Schmerz", "ge\u00b7weiht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich weine, hoffe und verzage \u2013", "tokens": ["Ich", "wei\u00b7ne", ",", "hof\u00b7fe", "und", "ver\u00b7za\u00b7ge", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich bin nicht aus der Ewigkeit!", "tokens": ["Ich", "bin", "nicht", "aus", "der", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und dieses martervolle Schwanken,", "tokens": ["Und", "die\u00b7ses", "mar\u00b7ter\u00b7vol\u00b7le", "Schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die\u00df Fremdsein an jedwedem Ort,", "tokens": ["Die\u00df", "Fremd\u00b7sein", "an", "jed\u00b7we\u00b7dem", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die\u00df Himmelst\u00fcrmen der Gedanken,", "tokens": ["Die\u00df", "Him\u00b7mel\u00b7st\u00fcr\u00b7men", "der", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die\u00df Sehnen nach dem Grabesport,", "tokens": ["Die\u00df", "Seh\u00b7nen", "nach", "dem", "Gra\u00b7be\u00b7sport", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die\u00df Heimweh, das des Daseins Bl\u00fcthe", "tokens": ["Die\u00df", "Heim\u00b7weh", ",", "das", "des", "Da\u00b7seins", "Bl\u00fc\u00b7the"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "NN", "$,", "PRELS", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In ihrem ersten Keime brach \u2013", "tokens": ["In", "ih\u00b7rem", "ers\u00b7ten", "Kei\u00b7me", "brach", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im einst so freudigen Gem\u00fcthe", "tokens": ["Im", "einst", "so", "freu\u00b7di\u00b7gen", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Riefst du es, Lied, allein nur wach!", "tokens": ["Riefst", "du", "es", ",", "Lied", ",", "al\u00b7lein", "nur", "wach", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "NN", "$,", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du lehrtest mich das Jenseits ahnen", "tokens": ["Du", "lehr\u00b7test", "mich", "das", "Jen\u00b7seits", "ah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und seiner Wonnen G\u00f6ttlichkeit!", "tokens": ["Und", "sei\u00b7ner", "Won\u00b7nen", "G\u00f6tt\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich schwebte hin auf Sternenbahnen,", "tokens": ["Ich", "schweb\u00b7te", "hin", "auf", "Ster\u00b7nen\u00b7bah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich trank vom Quell der Seligkeit,", "tokens": ["Ich", "trank", "vom", "Quell", "der", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und als, ach, wie mit Blitzesschnelle,", "tokens": ["Und", "als", ",", "ach", ",", "wie", "mit", "Blit\u00b7zes\u00b7schnel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "ITJ", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Das Traumbild dann zerstoben war,", "tokens": ["Das", "Traum\u00b7bild", "dann", "zer\u00b7sto\u00b7ben", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Stand an des Paradieses Schwelle,", "tokens": ["Stand", "an", "des", "Pa\u00b7ra\u00b7die\u00b7ses", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich aller Erdenhoffnung bar!", "tokens": ["Ich", "al\u00b7ler", "Er\u00b7den\u00b7hoff\u00b7nung", "bar", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Denn nichts kann mir das Leben bieten,", "tokens": ["Denn", "nichts", "kann", "mir", "das", "Le\u00b7ben", "bie\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was jenen Wonnen k\u00e4me nah,", "tokens": ["Was", "je\u00b7nen", "Won\u00b7nen", "k\u00e4\u00b7me", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die oft ich in des Lieds Gebieten", "tokens": ["Die", "oft", "ich", "in", "des", "Lieds", "Ge\u00b7bie\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So himmlisch hold erbl\u00fchet sah;", "tokens": ["So", "himm\u00b7lisch", "hold", "er\u00b7bl\u00fc\u00b7het", "sah", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch nach den lastenden Gesetzen", "tokens": ["Doch", "nach", "den", "las\u00b7ten\u00b7den", "Ge\u00b7set\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Uns fesselnd an die Erdenbahn,", "tokens": ["Uns", "fes\u00b7selnd", "an", "die", "Er\u00b7den\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Will sich in Gl\u00fcck die Brust noch letzen,", "tokens": ["Will", "sich", "in", "Gl\u00fcck", "die", "Brust", "noch", "let\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "NN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die's f\u00fchlen nicht, noch missen kann!", "tokens": ["Die's", "f\u00fch\u00b7len", "nicht", ",", "noch", "mis\u00b7sen", "kann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "$,", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "So leb' ich fort, ein t\u00e4glich Sterben", "tokens": ["So", "leb'", "ich", "fort", ",", "ein", "t\u00e4g\u00b7lich", "Ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Schwanken zwischen Dort und Hier,", "tokens": ["Im", "Schwan\u00b7ken", "zwi\u00b7schen", "Dort", "und", "Hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein unerm\u00fcdlich Qualerwerben,", "tokens": ["Ein", "un\u00b7er\u00b7m\u00fcd\u00b7lich", "Qua\u00b7ler\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Traumessein im Tagsgewirr,", "tokens": ["Ein", "Trau\u00b7mes\u00b7sein", "im", "Tags\u00b7ge\u00b7wirr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein einsam Sein in d\u00fcstrer Mahnung", "tokens": ["Ein", "ein\u00b7sam", "Sein", "in", "d\u00fcst\u00b7rer", "Mah\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "PPOSAT", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Inmitten dieser lauten Welt,", "tokens": ["In\u00b7mit\u00b7ten", "die\u00b7ser", "lau\u00b7ten", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In tiefer Nacht, die nur die Ahnung", "tokens": ["In", "tie\u00b7fer", "Nacht", ",", "die", "nur", "die", "Ah\u00b7nung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als Stern jetzt, jetzt als Blitz erhellt!", "tokens": ["Als", "Stern", "jetzt", ",", "jetzt", "als", "Blitz", "er\u00b7hellt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "$,", "ADV", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}