{"textgrid.poem.41972": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Graunvoll saust, durch den gellenden Forst hintobend, der Sturmwind,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Graunvoll saust, durch den gellenden Forst hintobend, der Sturmwind,", "tokens": ["Graun\u00b7voll", "saust", ",", "durch", "den", "gel\u00b7len\u00b7den", "Forst", "hin\u00b7to\u00b7bend", ",", "der", "Sturm\u00b7wind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "VVPP", "$,", "ART", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Laut an Fenster und Dach schmettert des Regens Gewalt;", "tokens": ["Laut", "an", "Fens\u00b7ter", "und", "Dach", "schmet\u00b7tert", "des", "Re\u00b7gens", "Ge\u00b7walt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NN", "KON", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Sieh, wie die Fichte sich tief hinbeugt, wie sie k\u00e4mpfend emporstrebt,", "tokens": ["Sieh", ",", "wie", "die", "Fich\u00b7te", "sich", "tief", "hin\u00b7beugt", ",", "wie", "sie", "k\u00e4mp\u00b7fend", "em\u00b7por\u00b7strebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "PRF", "ADJD", "VVPP", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Horch, wie herab von den H\u00f6hn wild das Gew\u00e4sser sich st\u00fcrzt!", "tokens": ["Horch", ",", "wie", "her\u00b7ab", "von", "den", "H\u00f6hn", "wild", "das", "Ge\u00b7w\u00e4s\u00b7ser", "sich", "st\u00fcrzt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADV", "APPR", "ART", "NN", "ADJD", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Ueber den Harzwald w\u00e4lzt, wie ein finsterer Geist, sich der Wolke", "tokens": ["Ue\u00b7ber", "den", "Harz\u00b7wald", "w\u00e4lzt", ",", "wie", "ein", "fins\u00b7te\u00b7rer", "Geist", ",", "sich", "der", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "$,", "PRF", "ART", "NN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "K\u00e4mpfendes Spiel, und es wogt rings die befl\u00fcgelte Nacht.", "tokens": ["K\u00e4mp\u00b7fen\u00b7des", "Spiel", ",", "und", "es", "wogt", "rings", "die", "be\u00b7fl\u00fc\u00b7gel\u00b7te", "Nacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.7": {"text": "Kalt ist's drau\u00dfen und dunkel im Hain: doch es flammt mir im Zimmer", "tokens": ["Kalt", "ist's", "drau\u00b7\u00dfen", "und", "dun\u00b7kel", "im", "Hain", ":", "doch", "es", "flammt", "mir", "im", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "KON", "ADJD", "APPRART", "NN", "$.", "KON", "PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Fr\u00f6hlich die Gluth, und das Herz leuchtet im heiteren Glanz.", "tokens": ["Fr\u00f6h\u00b7lich", "die", "Gluth", ",", "und", "das", "Herz", "leuch\u00b7tet", "im", "hei\u00b7te\u00b7ren", "Glanz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "KON", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Lieder der Schlacht heult drau\u00dfen der Sturm: doch dem friedlichen Dichter", "tokens": ["Lie\u00b7der", "der", "Schlacht", "heult", "drau\u00b7\u00dfen", "der", "Sturm", ":", "doch", "dem", "fried\u00b7li\u00b7chen", "Dich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "$.", "ADV", "ART", "ADJA", "NN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Sendet des Liebchens Blick Lieder der Lieb' und der Lust.", "tokens": ["Sen\u00b7det", "des", "Lieb\u00b7chens", "Blick", "Lie\u00b7der", "der", "Lieb'", "und", "der", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+--+-++--+--+", "measure": "iambic.hexa.invert"}, "line.11": {"text": "W\u00fcrzt sie selbst doch das Mahl mir im stillen Gemach, und bekr\u00e4nzt sie", "tokens": ["W\u00fcrzt", "sie", "selbst", "doch", "das", "Mahl", "mir", "im", "stil\u00b7len", "Ge\u00b7mach", ",", "und", "be\u00b7kr\u00e4nzt", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "PPER", "APPRART", "ADJA", "NN", "$,", "KON", "VVFIN", "PPER"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Selbst doch freundlich mit Wein, wenn ich ihr winke, den Kelch,", "tokens": ["Selbst", "doch", "freund\u00b7lich", "mit", "Wein", ",", "wenn", "ich", "ihr", "win\u00b7ke", ",", "den", "Kelch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPR", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Nippt mit dem rosigen Mund, vorkostend den Trank, und err\u00f6thend", "tokens": ["Nippt", "mit", "dem", "ro\u00b7si\u00b7gen", "Mund", ",", "vor\u00b7kos\u00b7tend", "den", "Trank", ",", "und", "er\u00b7r\u00f6\u00b7thend"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,", "VVPP", "ART", "NN", "$,", "KON", "VVPP"], "meter": "+--+--+-+--+--++", "measure": "dactylic.di.plus"}, "line.14": {"text": "Beut sie der Wang' und des Weins doppelte Gluthen mir dar.", "tokens": ["Beut", "sie", "der", "Wang'", "und", "des", "Weins", "dop\u00b7pel\u00b7te", "Glu\u00b7then", "mir", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "M\u00e4hrchen erz\u00e4hlen wir uns, graunhafte Gedichte der Vorzeit,", "tokens": ["M\u00e4hr\u00b7chen", "er\u00b7z\u00e4h\u00b7len", "wir", "uns", ",", "graun\u00b7haf\u00b7te", "Ge\u00b7dich\u00b7te", "der", "Vor\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "$,", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+-+-++--+--+-", "measure": "iambic.septa.invert"}, "line.16": {"text": "W\u00e4hnen die Nixe zu sehn, welche den Knaben geraubt,", "tokens": ["W\u00e4h\u00b7nen", "die", "Ni\u00b7xe", "zu", "sehn", ",", "wel\u00b7che", "den", "Kna\u00b7ben", "ge\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKZU", "VVINF", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.17": {"text": "H\u00f6ren, wie laut die verzauberte Jagd in's gellende Horn st\u00f6\u00dft,", "tokens": ["H\u00f6\u00b7ren", ",", "wie", "laut", "die", "ver\u00b7zau\u00b7ber\u00b7te", "Jagd", "in's", "gel\u00b7len\u00b7de", "Horn", "st\u00f6\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ADJD", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.18": {"text": "Und wie die Elf' im Hain singend den Schleier sich webt.", "tokens": ["Und", "wie", "die", "El\u00b7f'", "im", "Hain", "sin\u00b7gend", "den", "Schlei\u00b7er", "sich", "webt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPRART", "NN", "ADJD", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "S\u00fc\u00df ist's, wenn aufd\u00e4mmernd die Furcht wie ein Nebelgebild' uns", "tokens": ["S\u00fc\u00df", "ist's", ",", "wenn", "auf\u00b7d\u00e4m\u00b7mernd", "die", "Furcht", "wie", "ein", "Ne\u00b7bel\u00b7ge\u00b7bild'", "uns"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "$,", "KOUS", "VVPP", "ART", "NN", "KOKOM", "ART", "NN", "PPER"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.20": {"text": "Schweigend um Augen und Herz schleicht aus der Tiefe der Brust!", "tokens": ["Schwei\u00b7gend", "um", "Au\u00b7gen", "und", "Herz", "schleicht", "aus", "der", "Tie\u00b7fe", "der", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.21": {"text": "N\u00e4her r\u00fccken wir dann auf dem traulichen Sitz, und die Hand sucht", "tokens": ["N\u00e4\u00b7her", "r\u00fc\u00b7cken", "wir", "dann", "auf", "dem", "trau\u00b7li\u00b7chen", "Sitz", ",", "und", "die", "Hand", "sucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$,", "KON", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.22": {"text": "Schmeichelnd die Hand, und es klopft bebend am Herzen das Herz.", "tokens": ["Schmei\u00b7chelnd", "die", "Hand", ",", "und", "es", "klopft", "be\u00b7bend", "am", "Her\u00b7zen", "das", "Herz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "ADJD", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.23": {"text": "Leise beruhiget bald mein kosendes Wort die Verzagte,", "tokens": ["Lei\u00b7se", "be\u00b7ru\u00b7hi\u00b7get", "bald", "mein", "ko\u00b7sen\u00b7des", "Wort", "die", "Ver\u00b7zag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+---+-+-+--+--+-", "measure": "trochaic.hexa.relaxed"}, "line.24": {"text": "W\u00e4hrend im Busen mir selbst heimliches Grausen noch weilt;", "tokens": ["W\u00e4h\u00b7rend", "im", "Bu\u00b7sen", "mir", "selbst", "heim\u00b7li\u00b7ches", "Grau\u00b7sen", "noch", "weilt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PPER", "ADV", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.25": {"text": "Ach, dann hebt sie das Auge so klar, und mein sehnender Geist sinkt", "tokens": ["Ach", ",", "dann", "hebt", "sie", "das", "Au\u00b7ge", "so", "klar", ",", "und", "mein", "seh\u00b7nen\u00b7der", "Geist", "sinkt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "ADJD", "$,", "KON", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "--+--+--+--+--++", "measure": "anapaest.tetra.plus"}, "line.26": {"text": "Still in des seligen Blicks heilige Tiefen hinab.", "tokens": ["Still", "in", "des", "se\u00b7li\u00b7gen", "Blicks", "hei\u00b7li\u00b7ge", "Tie\u00b7fen", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.27": {"text": "Kinder scheinen wir dann: doch es braust aufwachend der J\u00fcngling", "tokens": ["Kin\u00b7der", "schei\u00b7nen", "wir", "dann", ":", "doch", "es", "braust", "auf\u00b7wa\u00b7chend", "der", "J\u00fcng\u00b7ling"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$.", "KON", "PPER", "VVFIN", "VVPP", "ART", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.28": {"text": "St\u00fcrmischer oft, und es wehrt ernster das sittige Weib.", "tokens": ["St\u00fcr\u00b7mi\u00b7scher", "oft", ",", "und", "es", "wehrt", "erns\u00b7ter", "das", "sit\u00b7ti\u00b7ge", "Weib", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KON", "PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.29": {"text": "Blume des \u00f6den Gebirgs, wie hat die Natur in den Kelch dir", "tokens": ["Blu\u00b7me", "des", "\u00f6\u00b7den", "Ge\u00b7birgs", ",", "wie", "hat", "die", "Na\u00b7tur", "in", "den", "Kelch", "dir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "PWAV", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "PPER"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.30": {"text": "Jeglichen Reiz, den die Kunst nimmer ertheilte, gelegt!", "tokens": ["Jeg\u00b7li\u00b7chen", "Reiz", ",", "den", "die", "Kunst", "nim\u00b7mer", "er\u00b7theil\u00b7te", ",", "ge\u00b7legt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ART", "NN", "ADV", "VVFIN", "$,", "VVPP", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.31": {"text": "Du nur lehrtest zuerst mich die reinere Lust, wenn der Sehnsucht", "tokens": ["Du", "nur", "lehr\u00b7test", "zu\u00b7erst", "mich", "die", "rei\u00b7ne\u00b7re", "Lust", ",", "wenn", "der", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "PPER", "ART", "ADJA", "NN", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-+--+--++", "measure": "trochaic.octa.plus.relaxed"}, "line.32": {"text": "Brennender Hauch in der Zucht freundlichem Thaue sich k\u00fchlt.", "tokens": ["Bren\u00b7nen\u00b7der", "Hauch", "in", "der", "Zucht", "freund\u00b7li\u00b7chem", "Thau\u00b7e", "sich", "k\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.33": {"text": "Sch\u00fcchterner werd' ich und friedlicher stets, je freyer dein Blick mir", "tokens": ["Sch\u00fcch\u00b7ter\u00b7ner", "werd'", "ich", "und", "fried\u00b7li\u00b7cher", "stets", ",", "je", "frey\u00b7er", "dein", "Blick", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "KON", "ADJD", "ADV", "$,", "ADV", "ADJD", "PPOSAT", "NN", "PPER"], "meter": "+--+--+-+-++--+-", "measure": "dactylic.di.plus"}, "line.34": {"text": "L\u00e4chelt, je mehr dein Herz gl\u00e4ubig dem meinen vertraut.", "tokens": ["L\u00e4\u00b7chelt", ",", "je", "mehr", "dein", "Herz", "gl\u00e4u\u00b7big", "dem", "mei\u00b7nen", "ver\u00b7traut", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "ADV", "PPOSAT", "NN", "ADJD", "ART", "PPOSAT", "ADJD", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.35": {"text": "Walte nur fort, schwarzwogende Nacht! Hier gl\u00e4nzt mir der Sonne", "tokens": ["Wal\u00b7te", "nur", "fort", ",", "schwarz\u00b7wo\u00b7gen\u00b7de", "Nacht", "!", "Hier", "gl\u00e4nzt", "mir", "der", "Son\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PTKVZ", "$,", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+--++-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.36": {"text": "Heiterster Strahl, hier w\u00e4rmt fr\u00f6hlich die Flamme der Lust;", "tokens": ["Hei\u00b7ters\u00b7ter", "Strahl", ",", "hier", "w\u00e