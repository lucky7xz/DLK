{"dta.poem.12072": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "92.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des einen freu' ich mich, wenn r\u00fcckwerts geht der Blick", "tokens": ["Des", "ei\u00b7nen", "freu'", "ich", "mich", ",", "wenn", "r\u00fcck\u00b7werts", "geht", "der", "Blick"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "PPER", "PRF", "$,", "KOUS", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf meines Lebens buntverworrenes Geschick,", "tokens": ["Auf", "mei\u00b7nes", "Le\u00b7bens", "bunt\u00b7ver\u00b7wor\u00b7re\u00b7nes", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wo der Zusammenhang der Pfade zu entgehn", "tokens": ["Wo", "der", "Zu\u00b7sam\u00b7men\u00b7hang", "der", "Pfa\u00b7de", "zu", "ent\u00b7gehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Aug' und alles scheint in irrem Kreis zu drehn;", "tokens": ["Dem", "Aug'", "und", "al\u00b7les", "scheint", "in", "ir\u00b7rem", "Kreis", "zu", "drehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PIS", "VVFIN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Des einen freu' ich mich, da\u00df doch, statt zu ermatten,", "tokens": ["Des", "ei\u00b7nen", "freu'", "ich", "mich", ",", "da\u00df", "doch", ",", "statt", "zu", "er\u00b7mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPER", "PRF", "$,", "KOUS", "ADV", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Reise leichter stets, je weiter, gieng vonstatten;", "tokens": ["Die", "Rei\u00b7se", "leich\u00b7ter", "stets", ",", "je", "wei\u00b7ter", ",", "gieng", "von\u00b7stat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADV", "$,", "ADV", "PTKVZ", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Als sie die Federkraft, die schwindende, der Glieder", "tokens": ["Als", "sie", "die", "Fe\u00b7der\u00b7kraft", ",", "die", "schwin\u00b7den\u00b7de", ",", "der", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "ART", "ADJA", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ersetzt durch tragendes unsichtbares Gefieder;", "tokens": ["Er\u00b7setzt", "durch", "tra\u00b7gen\u00b7des", "un\u00b7sicht\u00b7ba\u00b7res", "Ge\u00b7fie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Soda\u00df auf seiner Bahn der Geist m\u00fchloser strebt,", "tokens": ["So\u00b7da\u00df", "auf", "sei\u00b7ner", "Bahn", "der", "Geist", "m\u00fch\u00b7lo\u00b7ser", "strebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der, wo er unten sonst gerungen, oben schwebt.", "tokens": ["Der", ",", "wo", "er", "un\u00b7ten", "sonst", "ge\u00b7run\u00b7gen", ",", "o\u00b7ben", "schwebt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "PPER", "ADV", "ADV", "VVPP", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wenn nun sich ein Gedank' aus jener Zeit erfrischt", "tokens": ["Wenn", "nun", "sich", "ein", "Ge\u00b7dank'", "aus", "je\u00b7ner", "Zeit", "er\u00b7frischt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PRF", "ART", "NN", "APPR", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In neuer Form, ist ihm was eignes beigemischt:", "tokens": ["In", "neu\u00b7er", "Form", ",", "ist", "ihm", "was", "eig\u00b7nes", "bei\u00b7ge\u00b7mischt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VAFIN", "PPER", "PIS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Das jugendliche Roth der Wangen hat er nicht,", "tokens": ["Das", "ju\u00b7gend\u00b7li\u00b7che", "Roth", "der", "Wan\u00b7gen", "hat", "er", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch daf\u00fcr einen Stral auf seinem Angesicht.", "tokens": ["Doch", "da\u00b7f\u00fcr", "ei\u00b7nen", "Stral", "auf", "sei\u00b7nem", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ich k\u00f6nnte, wollt' ich abgethanes neuverrichten,", "tokens": ["Ich", "k\u00f6nn\u00b7te", ",", "wollt'", "ich", "ab\u00b7ge\u00b7tha\u00b7nes", "neu\u00b7ver\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "VMFIN", "PPER", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "All mein Gedichtetes in h\u00f6hern Stil umdichten.", "tokens": ["All", "mein", "Ge\u00b7dich\u00b7te\u00b7tes", "in", "h\u00f6\u00b7hern", "Stil", "um\u00b7dich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}