{"textgrid.poem.41730": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auch Du, auch Du rufst uns zur Stelle,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch Du, auch Du rufst uns zur Stelle,", "tokens": ["Auch", "Du", ",", "auch", "Du", "rufst", "uns", "zur", "Stel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "ADV", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Du rufst uns auf den Schlachtenplan,", "tokens": ["Du", "rufst", "uns", "auf", "den", "Schlach\u00b7ten\u00b7plan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hinaus, hinaus aus enger Zelle,", "tokens": ["Hin\u00b7aus", ",", "hin\u00b7aus", "aus", "en\u00b7ger", "Zel\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wir Frauen solln des Hauses Schwelle", "tokens": ["Wir", "Frau\u00b7en", "solln", "des", "Hau\u00b7ses", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "VMFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ernst \u00fcberschreitend Euch uns nahn.", "tokens": ["Ernst", "\u00fc\u00b7bersc\u00b7hrei\u00b7tend", "Euch", "uns", "nahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "PPER", "PPER", "ADJA", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und jubelnd streck' ich Dir entgegen", "tokens": ["Und", "ju\u00b7belnd", "streck'", "ich", "Dir", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPER", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Schwesterhand \u2013 o habe Dank!", "tokens": ["Die", "Schwes\u00b7ter\u00b7hand", "\u2013", "o", "ha\u00b7be", "Dank", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "FM", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du hast nicht Hohn, nein, Du hast Segen,", "tokens": ["Du", "hast", "nicht", "Hohn", ",", "nein", ",", "Du", "hast", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "$,", "PTKANT", "$,", "PPER", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn wir auch uns're Leier regen", "tokens": ["Wenn", "wir", "auch", "un\u00b7s'\u00b7re", "Lei\u00b7er", "re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "ADJA"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Zum Freiheitskampfe ohne Wank.", "tokens": ["Zum", "Frei\u00b7heits\u00b7kamp\u00b7fe", "oh\u00b7ne", "Wank", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es braust das Lied in h\u00f6hren Ch\u00f6ren", "tokens": ["Es", "braust", "das", "Lied", "in", "h\u00f6h\u00b7ren", "Ch\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo man der eignen Kraft vertraut!", "tokens": ["Wo", "man", "der", "eig\u00b7nen", "Kraft", "ver\u00b7traut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie dr\u00e4ngt michs, meinem Volk' zu schw\u00f6ren!", "tokens": ["Wie", "dr\u00e4ngt", "michs", ",", "mei\u00b7nem", "Vol\u00b7k'", "zu", "schw\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und Gott der Herr wird mich erh\u00f6ren,", "tokens": ["Und", "Gott", "der", "Herr", "wird", "mich", "er\u00b7h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der in die Menschenherzen schaut.", "tokens": ["Der", "in", "die", "Men\u00b7schen\u00b7her\u00b7zen", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein Lenzsturm braust durch alle Lande \u2013", "tokens": ["Ein", "Lenz\u00b7sturm", "braust", "durch", "al\u00b7le", "Lan\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Odem ist's der neuen Zeit;", "tokens": ["Der", "O\u00b7dem", "ist's", "der", "neu\u00b7en", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Str\u00f6me brechen ihre Bande,", "tokens": ["Die", "Str\u00f6\u00b7me", "bre\u00b7chen", "ih\u00b7re", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es schmilzt das Eis im Sonnenbrande", "tokens": ["Es", "schmilzt", "das", "Eis", "im", "Son\u00b7nen\u00b7bran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Frei singt die Lerche und gefeit.", "tokens": ["Frei", "singt", "die", "Ler\u00b7che", "und", "ge\u00b7feit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "KON", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Und nun \u2013 bei all dem frischen Leben", "tokens": ["Und", "nun", "\u2013", "bei", "all", "dem", "fri\u00b7schen", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$(", "APPR", "PIAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie tief w\u00e4r da verdammt das Weib,", "tokens": ["Wie", "tief", "w\u00e4r", "da", "ver\u00b7dammt", "das", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "D\u00fcrft' es nicht mit den M\u00e4nnern streben", "tokens": ["D\u00fcrft'", "es", "nicht", "mit", "den", "M\u00e4n\u00b7nern", "stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "VVFIN"], "meter": "+----+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Und nichts dem eignen Volke geben", "tokens": ["Und", "nichts", "dem", "eig\u00b7nen", "Vol\u00b7ke", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Als sein Gekos, als seinen Leib!", "tokens": ["Als", "sein", "Ge\u00b7kos", ",", "als", "sei\u00b7nen", "Leib", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbhabt Ihr Gesang, so schlagt die Leier!\u00ab", "tokens": ["\u00bb", "habt", "Ihr", "Ge\u00b7sang", ",", "so", "schlagt", "die", "Lei\u00b7er", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ruft uns Dein warmes Dichterherz \u2013", "tokens": ["Ruft", "uns", "Dein", "war\u00b7mes", "Dich\u00b7ter\u00b7herz", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So t\u00f6nt des Vaterlandes Feier", "tokens": ["So", "t\u00f6nt", "des", "Va\u00b7ter\u00b7lan\u00b7des", "Fei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In meinen S\u00e4ngen frei und freier", "tokens": ["In", "mei\u00b7nen", "S\u00e4n\u00b7gen", "frei", "und", "frei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und flammt begeistert himmelw\u00e4rts!", "tokens": ["Und", "flammt", "be\u00b7geis\u00b7tert", "him\u00b7mel\u00b7w\u00e4rts", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "ADV", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.7": {"line.1": {"text": "Und fr\u00f6hlich h\u00f6rte ich Dein Mahnen", "tokens": ["Und", "fr\u00f6h\u00b7lich", "h\u00f6r\u00b7te", "ich", "Dein", "Mah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und dr\u00fccke Deine Bruderhand.", "tokens": ["Und", "dr\u00fc\u00b7cke", "Dei\u00b7ne", "Bru\u00b7der\u00b7hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der neuen Zeiten neuem Ahnen,", "tokens": ["Der", "neu\u00b7en", "Zei\u00b7ten", "neu\u00b7em", "Ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Freiheit einen Weg zu bahnen", "tokens": ["Der", "Frei\u00b7heit", "ei\u00b7nen", "Weg", "zu", "bah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dazu ward uns das Lied gesandt.", "tokens": ["Da\u00b7zu", "ward", "uns", "das", "Lied", "ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und ob die Sp\u00f6tter mich verh\u00f6hnen", "tokens": ["Und", "ob", "die", "Sp\u00f6t\u00b7ter", "mich", "ver\u00b7h\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Nicht Myrt' noch Lorbeer wird mich kr\u00f6nen,", "tokens": ["Nicht", "Myrt'", "noch", "Lor\u00b7beer", "wird", "mich", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADV", "NE", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch Freiheit wird die Leier t\u00f6nen,", "tokens": ["Doch", "Frei\u00b7heit", "wird", "die", "Lei\u00b7er", "t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So lang mein Herz noch schl\u00e4gt und gl\u00fct.", "tokens": ["So", "lang", "mein", "Herz", "noch", "schl\u00e4gt", "und", "gl\u00fct", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}