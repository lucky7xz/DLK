{"textgrid.poem.53399": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[ein Denckmahl dir zu schreiben]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Denckmahl dir zu schreiben,", "tokens": ["Ein", "Denck\u00b7mahl", "dir", "zu", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Herr Kr\u00f6l, ist meine Pflicht,", "tokens": ["Herr", "Kr\u00f6l", ",", "ist", "mei\u00b7ne", "Pflicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich mu\u00df es lassen bleiben,", "tokens": ["Ich", "mu\u00df", "es", "las\u00b7sen", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Kranckheit l\u00e4sst mich nicht:", "tokens": ["Die", "Kran\u00b7ck\u00b7heit", "l\u00e4sst", "mich", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Bey dieser Hitze singen,", "tokens": ["Bey", "die\u00b7ser", "Hit\u00b7ze", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vnd seiner m\u00e4chtig seyn,", "tokens": ["Vnd", "sei\u00b7ner", "m\u00e4ch\u00b7tig", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ist Tag aus Nacht erzwingen", "tokens": ["Ist", "Tag", "aus", "Nacht", "er\u00b7zwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vnd Wasser aus dem Stein.", "tokens": ["Vnd", "Was\u00b7ser", "aus", "dem", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Chim\u00e4ren Vngehewer,", "tokens": ["Chi\u00b7m\u00e4\u00b7ren", "Vn\u00b7ge\u00b7he\u00b7wer", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Augien Stall und Wust,", "tokens": ["Au\u00b7gi\u00b7en", "Stall", "und", "Wust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Vnd, Sterope, dein Fewer", "tokens": ["Vnd", ",", "Ste\u00b7ro\u00b7pe", ",", "dein", "Fe\u00b7wer"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "$,", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wohnt itzt auff meiner Brust,", "tokens": ["Wohnt", "itzt", "auff", "mei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fcr Hitze meiner Lungen", "tokens": ["F\u00fcr", "Hit\u00b7ze", "mei\u00b7ner", "Lun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ist gar kein Speichel schier", "tokens": ["Ist", "gar", "kein", "Spei\u00b7chel", "schier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Auff meiner schwachen Zungen", "tokens": ["Auff", "mei\u00b7ner", "schwa\u00b7chen", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vnd alles glut bey mir.", "tokens": ["Vnd", "al\u00b7les", "glut", "bey", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Kein Hirsch k\u00f6mpt so geronnen", "tokens": ["Kein", "Hirsch", "k\u00f6mpt", "so", "ge\u00b7ron\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vnd lescht den Schlangen-stich", "tokens": ["Vnd", "lescht", "den", "Schlan\u00b7gen\u00b7stich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit einem k\u00fchlen Bronnen,", "tokens": ["Mit", "ei\u00b7nem", "k\u00fch\u00b7len", "Bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch Tantal sehnet sich", "tokens": ["Noch", "Tan\u00b7tal", "seh\u00b7net", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So \u00e4ngstig nach den Quellen,", "tokens": ["So", "\u00e4ngs\u00b7tig", "nach", "den", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Als ich, ob eine Kunst", "tokens": ["Als", "ich", ",", "ob", "ei\u00b7ne", "Kunst"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mir endlich m\u00f6chte f\u00e4llen", "tokens": ["Mir", "end\u00b7lich", "m\u00f6ch\u00b7te", "f\u00e4l\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den grossen Durst. Umbsonst.", "tokens": ["Den", "gros\u00b7sen", "Durst", ".", "Um\u00b7bsonst", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Artzt ist selbst dawider", "tokens": ["Der", "Artzt", "ist", "selbst", "da\u00b7wi\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "PAV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Setz' ich die Feder an,", "tokens": ["Setz'", "ich", "die", "Fe\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Spricht: Dieses schw\u00e4cht die Glieder,", "tokens": ["Spricht", ":", "Die\u00b7ses", "schw\u00e4cht", "die", "Glie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PDS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "La\u00df schreiben wer da kan.", "tokens": ["La\u00df", "schrei\u00b7ben", "wer", "da", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PWS", "ADV", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Lass' ich es nach? mit nichten,", "tokens": ["Lass'", "ich", "es", "nach", "?", "mit", "nich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "PTKVZ", "$.", "APPR", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Liebe treibt mein Spiel,", "tokens": ["Die", "Lie\u00b7be", "treibt", "mein", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich mu\u00df dir etwas tichten,", "tokens": ["Ich", "mu\u00df", "dir", "et\u00b7was", "tich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Es klinge wie es wil.", "tokens": ["Es", "klin\u00b7ge", "wie", "es", "wil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Doch wie sol das nicht klingen", "tokens": ["Doch", "wie", "sol", "das", "nicht", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VMFIN", "PDS", "PTKNEG", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Was wahre Treu erdacht,", "tokens": ["Was", "wah\u00b7re", "Treu", "er\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd Freundschafft lehret singen?", "tokens": ["Vnd", "Freund\u00b7schafft", "leh\u00b7ret", "sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein sch\u00f6ner Music-Pracht", "tokens": ["Kein", "sch\u00f6\u00b7ner", "Mu\u00b7sic\u00b7Pracht"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kan also Gott gewinnen", "tokens": ["Kan", "al\u00b7so", "Gott", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Als wenn ohn falschen Neid", "tokens": ["Als", "wenn", "ohn", "fal\u00b7schen", "Neid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein Band von trewen Sinnen", "tokens": ["Ein", "Band", "von", "tre\u00b7wen", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Macht' ein' Einhelligheit.", "tokens": ["Macht'", "ein'", "Ein\u00b7hel\u00b7lig\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Offt sind wir zu dir kommen", "tokens": ["Offt", "sind", "wir", "zu", "dir", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der selig' Albert, ich.", "tokens": ["Der", "se\u00b7lig'", "Al\u00b7bert", ",", "ich", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "NE", "$,", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Dein Gart hat auffgenommen", "tokens": ["Dein", "Gart", "hat", "auff\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vns beyde br\u00fcderlich,", "tokens": ["Vns", "bey\u00b7de", "br\u00fc\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PIS", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie liessest du da blicken", "tokens": ["Wie", "lies\u00b7sest", "du", "da", "bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "(wo ist die s\u00fcsse Ruh?)", "tokens": ["(", "wo", "ist", "die", "s\u00fcs\u00b7se", "Ruh", "?", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "VAFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In tausent Liebes-St\u00fccken", "tokens": ["In", "tau\u00b7sent", "Lie\u00b7bes\u00b7St\u00fc\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dich und dein Hertz dazu.", "tokens": ["Dich", "und", "dein", "Hertz", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "In dem da auff uns hielten", "tokens": ["In", "dem", "da", "auff", "uns", "hiel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Lufft, B\u00e4ume, V\u00f6gelein", "tokens": ["Lufft", ",", "B\u00e4u\u00b7me", ",", "V\u00f6\u00b7ge\u00b7lein"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd wir zusammen spielten,", "tokens": ["Vnd", "wir", "zu\u00b7sam\u00b7men", "spiel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So sangest du darein,", "tokens": ["So", "san\u00b7gest", "du", "da\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da ward kein Wort verlohren", "tokens": ["Da", "ward", "kein", "Wort", "ver\u00b7loh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So \u00fcppigheit gesucht,", "tokens": ["So", "\u00fcp\u00b7pig\u00b7heit", "ge\u00b7sucht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da hat man nicht geschworen,", "tokens": ["Da", "hat", "man", "nicht", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da hat man nicht geflucht.", "tokens": ["Da", "hat", "man", "nicht", "ge\u00b7flucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Es ward da nicht gefehdet,", "tokens": ["Es", "ward", "da", "nicht", "ge\u00b7feh\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kein Mensch ward da zerlegt", "tokens": ["Kein", "Mensch", "ward", "da", "zer\u00b7legt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hinterr\u00fccks beredet", "tokens": ["Und", "hin\u00b7ter\u00b7r\u00fccks", "be\u00b7re\u00b7det"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie zu geschehen pflegt.", "tokens": ["Wie", "zu", "ge\u00b7sche\u00b7hen", "pflegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Das Gras des H\u00f6chsten G\u00fcte,", "tokens": ["Das", "Gras", "des", "H\u00f6chs\u00b7ten", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Erden reicher Leib,", "tokens": ["Der", "Er\u00b7den", "rei\u00b7cher", "Leib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die B\u00e4um' und ihre Bl\u00fcte", "tokens": ["Die", "B\u00e4um'", "und", "ih\u00b7re", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "War unser Zeit-vertreib.", "tokens": ["War", "un\u00b7ser", "Zeit\u00b7ver\u00b7treib", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Nun ist dies Band zerst\u00fccket,", "tokens": ["Nun", "ist", "dies", "Band", "zer\u0