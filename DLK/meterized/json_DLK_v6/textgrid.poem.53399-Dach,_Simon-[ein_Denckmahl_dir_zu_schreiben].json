nct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wie aber ist es kommen,", "tokens": ["Wie", "a\u00b7ber", "ist", "es", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df du durch diesen Fall", "tokens": ["Da\u00df", "du", "durch", "die\u00b7sen", "Fall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vns bist hinweg genommen?", "tokens": ["Vns", "bist", "hin\u00b7weg", "ge\u00b7nom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Sinn forscht \u00fcberall.", "tokens": ["Mein", "Sinn", "forscht", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "That es dein b\u00f6ses Leben?", "tokens": ["That", "es", "dein", "b\u00f6\u00b7ses", "Le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Kein Kind kan fr\u00f6mmer seyn.", "tokens": ["Kein", "Kind", "kan", "fr\u00f6m\u00b7mer", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Warst du dem Soff ergeben", "tokens": ["Warst", "du", "dem", "Soff", "er\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vnd andern Lastern? nein.", "tokens": ["Vnd", "an\u00b7dern", "Las\u00b7tern", "?", "nein", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$.", "PTKANT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Wenn hat man nicht gesehen", "tokens": ["Wenn", "hat", "man", "nicht", "ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PIS", "PTKNEG", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dich in der Frommen Raht", "tokens": ["Dich", "in", "der", "From\u00b7men", "Raht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gott umb Erbarmung flehen", "tokens": ["Gott", "umb", "Er\u00b7bar\u00b7mung", "fle\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In unsrer Missethat?", "tokens": ["In", "uns\u00b7rer", "Mis\u00b7se\u00b7that", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O wohnte bey uns allen", "tokens": ["O", "wohn\u00b7te", "bey", "uns", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPER", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nur deine Fr\u00f6mmigheit,", "tokens": ["Nur", "dei\u00b7ne", "Fr\u00f6m\u00b7mi\u00b7gheit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er h\u00e4tt' uns nicht befallen", "tokens": ["Er", "h\u00e4tt'", "uns", "nicht", "be\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Jammer dieser Zeit,", "tokens": ["Der", "Jam\u00b7mer", "die\u00b7ser", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Die\u00df grausam Vngewitter", "tokens": ["Die\u00df", "grau\u00b7sam", "Vn\u00b7ge\u00b7wit\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["PDS", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fcr welches letzten Noht", "tokens": ["F\u00fcr", "wel\u00b7ches", "letz\u00b7ten", "Noht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wie ein Laub erzitter'", "tokens": ["Ich", "wie", "ein", "Laub", "er\u00b7zit\u00b7ter'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "KOKOM", "ART", "NN", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Vnd bleibe nur nicht tod.", "tokens": ["Vnd", "blei\u00b7be", "nur", "nicht", "tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es sind der Satzung H\u00e4nde", "tokens": ["Es", "sind", "der", "Sat\u00b7zung", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die machen es mit dir,", "tokens": ["Die", "ma\u00b7chen", "es", "mit", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Du lieber Freund, ein Ende,", "tokens": ["Du", "lie\u00b7ber", "Freund", ",", "ein", "En\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und holen dich von hier", "tokens": ["Und", "ho\u00b7len", "dich", "von", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "In deine stille Kammer,", "tokens": ["In", "dei\u00b7ne", "stil\u00b7le", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In deine stoltze Ruh.", "tokens": ["In", "dei\u00b7ne", "stolt\u00b7ze", "Ruh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du hast f\u00fcr allem Jammer", "tokens": ["Du", "hast", "f\u00fcr", "al\u00b7lem", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PIS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da Aug- und Ohren zu,", "tokens": ["Da", "Aug", "und", "Oh\u00b7ren", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "TRUNC", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was etwa heut und morgen", "tokens": ["Was", "et\u00b7wa", "heut", "und", "mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADV", "KON", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vns hie begegnen sol,", "tokens": ["Vns", "hie", "be\u00b7geg\u00b7nen", "sol", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da-l\u00e4sst du uns-f\u00fcr sorgen,", "tokens": ["Da\u00b7l\u00e4sst", "du", "un\u00b7sf\u00fcr", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Du aber schl\u00e4ffest wol.", "tokens": ["Du", "a\u00b7ber", "schl\u00e4f\u00b7fest", "wol", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Vnd hast du nicht empfunden", "tokens": ["Vnd", "hast", "du", "nicht", "emp\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was Lust die Eh gewehrt,", "tokens": ["Was", "Lust", "die", "Eh", "ge\u00b7wehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So waren auch nicht Wunden", "tokens": ["So", "wa\u00b7ren", "auch", "nicht", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PTKNEG", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die deinen Tod beschwert.", "tokens": ["Die", "dei\u00b7nen", "Tod", "be\u00b7schwert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kein schlechtes ists verscheiden", "tokens": ["Kein", "schlech\u00b7tes", "ists", "ver\u00b7schei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und Weib und Kind allein,", "tokens": ["Und", "Weib", "und", "Kind", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wer weis zu welchem Leiden,", "tokens": ["Wer", "weis", "zu", "wel\u00b7chem", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKVZ", "APPR", "PWAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Hie lassen \u00fcbrig seyn.", "tokens": ["Hie", "las\u00b7sen", "\u00fcb\u00b7rig", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Leb wol in solchen Wonnen", "tokens": ["Leb", "wol", "in", "sol\u00b7chen", "Won\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In solchem hellen Pracht,", "tokens": ["In", "sol\u00b7chem", "hel\u00b7len", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr dem der Glantz der Sonnen", "tokens": ["F\u00fcr", "dem", "der", "Glantz", "der", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist eine finstre Nacht!", "tokens": ["Ist", "ei\u00b7ne", "finst\u00b7re", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dein Grab m\u00fcss' ewig gr\u00fcnen,", "tokens": ["Dein", "Grab", "m\u00fcss'", "e\u00b7wig", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Nymfen m\u00fcssen dir", "tokens": ["Die", "Nym\u00b7fen", "m\u00fcs\u00b7sen", "dir"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Asch' und Gebein bedienen", "tokens": ["Asch'", "und", "Ge\u00b7bein", "be\u00b7die\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KON", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Mit allzeit-stiller Zier:", "tokens": ["Mit", "all\u00b7zeit\u00b7stil\u00b7ler", "Zier", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Ich w\u00e4re schier gewesen,", "tokens": ["Ich", "w\u00e4\u00b7re", "schier", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Durst nimmt aber Ruh,", "tokens": ["Mein", "Durst", "nimmt", "a\u00b7ber", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich scheine zu genesen,", "tokens": ["Ich", "schei\u00b7ne", "zu", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Gott weis allein wozu:", "tokens": ["Gott", "weis", "al\u00b7lein", "wo\u00b7zu", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihm hab ich Danck zu sagen,", "tokens": ["Ihm", "hab", "ich", "Danck", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Er l\u00e4ngert mir den Lauff:", "tokens": ["Er", "l\u00e4n\u00b7gert", "mir", "den", "Lauff", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch kompt zu gr\u00f6ssern Plagen", "tokens": ["Doch", "kompt", "zu", "gr\u00f6s\u00b7sern", "Pla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ihm mancher wieder auff.", "tokens": ["Ihm", "man\u00b7cher", "wie\u00b7der", "auff", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}