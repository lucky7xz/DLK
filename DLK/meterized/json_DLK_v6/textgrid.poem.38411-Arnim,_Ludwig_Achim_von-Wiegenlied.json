{"textgrid.poem.38411": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Wiegenlied", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eio popeio, was rasselt im Stroh,", "tokens": ["Ei\u00b7o", "po\u00b7pei\u00b7o", ",", "was", "ras\u00b7selt", "im", "Stroh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "$,", "PWS", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Die G\u00e4nslein gehn barfus,", "tokens": ["Die", "G\u00e4ns\u00b7lein", "gehn", "bar\u00b7fus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und haben keine Schuh,", "tokens": ["Und", "ha\u00b7ben", "kei\u00b7ne", "Schuh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Schuster hats Leder,", "tokens": ["Der", "Schus\u00b7ter", "hats", "Le\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kein Leisten dazu,", "tokens": ["Kein", "Leis\u00b7ten", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PAV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Kann er den G\u00e4nslein", "tokens": ["Kann", "er", "den", "G\u00e4ns\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Auch machen kein Schuh.", "tokens": ["Auch", "ma\u00b7chen", "kein", "Schuh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Eio popeio, schlags Kikelchen todt,", "tokens": ["Ei\u00b7o", "po\u00b7pei\u00b7o", ",", "schlags", "Ki\u00b7kel\u00b7chen", "todt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Legt mir keine Eier,", "tokens": ["Legt", "mir", "kei\u00b7ne", "Ei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und fri\u00dft mir mein Brod,", "tokens": ["Und", "fri\u00dft", "mir", "mein", "Brod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Rupfen wir ihm dann", "tokens": ["Rup\u00b7fen", "wir", "ihm", "dann"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Die Federchen aus,", "tokens": ["Die", "Fe\u00b7der\u00b7chen", "aus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Machen dem Kindlein", "tokens": ["Ma\u00b7chen", "dem", "Kin\u00b7dlein"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Ein Bettlein daraus.", "tokens": ["Ein", "Bet\u00b7tlein", "da\u00b7raus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Eio popeio, das ist eine Noth,", "tokens": ["Ei\u00b7o", "po\u00b7pei\u00b7o", ",", "das", "ist", "ei\u00b7ne", "Noth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "$,", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wer schenkt mir ein Heller,", "tokens": ["Wer", "schenkt", "mir", "ein", "Hel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Zu Zucker und Brod?", "tokens": ["Zu", "Zu\u00b7cker", "und", "Brod", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Verkauf ich mein Bettlein,", "tokens": ["Ver\u00b7kauf", "ich", "mein", "Bet\u00b7tlein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und leg mich aufs Stroh,", "tokens": ["Und", "leg", "mich", "aufs", "Stroh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Sticht mich keine Feder,", "tokens": ["Sticht", "mich", "kei\u00b7ne", "Fe\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und bei\u00dft mich kein Floh", "tokens": ["Und", "bei\u00dft", "mich", "kein", "Floh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Eio popeio.", "tokens": ["Ei\u00b7o", "po\u00b7pei\u00b7o", "."], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}