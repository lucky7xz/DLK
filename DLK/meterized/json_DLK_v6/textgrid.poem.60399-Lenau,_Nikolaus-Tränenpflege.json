{"textgrid.poem.60399": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Tr\u00e4nenpflege", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, Freundin, ich habe dich gest\u00f6rt", "tokens": ["Ach", ",", "Freun\u00b7din", ",", "ich", "ha\u00b7be", "dich", "ge\u00b7st\u00f6rt"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "NN", "$,", "PPER", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "In deinem verborgnen Weinen;", "tokens": ["In", "dei\u00b7nem", "ver\u00b7borg\u00b7nen", "Wei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nun hast du zu weinen aufgeh\u00f6rt,", "tokens": ["Nun", "hast", "du", "zu", "wei\u00b7nen", "auf\u00b7ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKZU", "VVINF", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und ruhig willst du scheinen.", "tokens": ["Und", "ru\u00b7hig", "willst", "du", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn deine Z\u00fcge verh\u00fcllend auch", "tokens": ["Wenn", "dei\u00b7ne", "Z\u00fc\u00b7ge", "ver\u00b7h\u00fcl\u00b7lend", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vor deinen Schmerz sich reihen", "tokens": ["Vor", "dei\u00b7nen", "Schmerz", "sich", "rei\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ihn nicht nennt der Lippen Hauch,", "tokens": ["Und", "ihn", "nicht", "nennt", "der", "Lip\u00b7pen", "Hauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich h\u00f6r ihn im Herzen schreien.", "tokens": ["Ich", "h\u00f6r", "ihn", "im", "Her\u00b7zen", "schrei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Pfleg deinen Schmerz mit Tr\u00e4nen lind,", "tokens": ["Pfleg", "dei\u00b7nen", "Schmerz", "mit", "Tr\u00e4\u00b7nen", "lind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als eine weinende Aja,", "tokens": ["Als", "ei\u00b7ne", "wei\u00b7nen\u00b7de", "A\u00b7ja", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NE", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Einschl\u00e4fre ihn, als wie ihr Kind", "tokens": ["Ein\u00b7schl\u00e4f\u00b7re", "ihn", ",", "als", "wie", "ihr", "Kind"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "KOKOM", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Mutter im Himalaya.", "tokens": ["Die", "Mut\u00b7ter", "im", "Hi\u00b7ma\u00b7la\u00b7ya", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Sie legt das Kind im Schattengestein", "tokens": ["Sie", "legt", "das", "Kind", "im", "Schat\u00b7ten\u00b7ge\u00b7stein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Dem Tropfbach unter, vertrauend;", "tokens": ["Dem", "Tropf\u00b7bach", "un\u00b7ter", ",", "ver\u00b7trau\u00b7end", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "APPR", "$,", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die leisen Tropfen schl\u00e4fern es ein,", "tokens": ["Die", "lei\u00b7sen", "Trop\u00b7fen", "schl\u00e4\u00b7fern", "es", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ihm auf die Wangen tauend.", "tokens": ["Ihm", "auf", "die", "Wan\u00b7gen", "tau\u00b7end", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}