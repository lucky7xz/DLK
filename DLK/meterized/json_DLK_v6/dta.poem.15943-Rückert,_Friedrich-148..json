{"dta.poem.15943": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "148.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Im Fr\u00fchling f\u00fchl' ich mich verbunden mit der Welt,", "tokens": ["Im", "Fr\u00fch\u00b7ling", "f\u00fchl'", "ich", "mich", "ver\u00b7bun\u00b7den", "mit", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PRF", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo die Natur mir selbst den Spiegel Gottes h\u00e4lt.", "tokens": ["Wo", "die", "Na\u00b7tur", "mir", "selbst", "den", "Spie\u00b7gel", "Got\u00b7tes", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Im Winter aber mu\u00df von Zeit zu Zeit mir sagen", "tokens": ["Im", "Win\u00b7ter", "a\u00b7ber", "mu\u00df", "von", "Zeit", "zu", "Zeit", "mir", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VMFIN", "APPR", "NN", "APPR", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Blick, ein Wort, ein Gru\u00df, von Herzen, die mir schlagen.", "tokens": ["Ein", "Blick", ",", "ein", "Wort", ",", "ein", "Gru\u00df", ",", "von", "Her\u00b7zen", ",", "die", "mir", "schla\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "APPR", "NN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Im Lenz war jedes Laub von Freundes Hand ein Blatt;", "tokens": ["Im", "Lenz", "war", "je\u00b7des", "Laub", "von", "Freun\u00b7des", "Hand", "ein", "Blatt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PIAT", "NN", "APPR", "NN", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jetzt sagt mir nur sein Brief, was er zu sagen hat.", "tokens": ["Jetzt", "sagt", "mir", "nur", "sein", "Brief", ",", "was", "er", "zu", "sa\u00b7gen", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,", "PWS", "PPER", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}