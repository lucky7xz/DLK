{"dta.poem.9179": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "ViII.  \n An Gritgen.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Gunst-geneigtes tugend-bild/", "tokens": ["Gunst\u00b7ge\u00b7neig\u00b7tes", "tu\u00b7gen\u00b7dbild", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruh und friede meines hertzens.", "tokens": ["Ruh", "und", "frie\u00b7de", "mei\u00b7nes", "hert\u00b7zens", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ietzund da sich alles schmertzens/", "tokens": ["Ie\u00b7tzund", "da", "sich", "al\u00b7les", "schmert\u00b7zens", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PRF", "PIS", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trost und ruh in dich verh\u00fcllt/", "tokens": ["Trost", "und", "ruh", "in", "dich", "ver\u00b7h\u00fcllt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PPER", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gib mir doch den abschieds-ku\u00df/", "tokens": ["Gib", "mir", "doch", "den", "ab\u00b7schieds\u00b7ku\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Eh' dich mein betr\u00fcbter gru\u00df/", "tokens": ["Eh'", "dich", "mein", "be\u00b7tr\u00fcb\u00b7ter", "gru\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Noch einmahl erinnern mu\u00df.", "tokens": ["Noch", "ein\u00b7mahl", "e\u00b7rin\u00b7nern", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "2. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Rein und lieblich anzuschauen/", "tokens": ["Rein", "und", "lieb\u00b7lich", "an\u00b7zu\u00b7schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVIZU", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Itzt da ich von diesen auen", "tokens": ["Itzt", "da", "ich", "von", "die\u00b7sen", "au\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Traurig abschied nehmen will:", "tokens": ["Trau\u00b7rig", "ab\u00b7schied", "neh\u00b7men", "will", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gl\u00e4ntze nur zu mir herein/", "tokens": ["Gl\u00e4nt\u00b7ze", "nur", "zu", "mir", "her\u00b7ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPER", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Eben als wann dieser schein/", "tokens": ["E\u00b7ben", "als", "wann", "die\u00b7ser", "schein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PWAV", "PDAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nun mein leit-stern solte seyn.", "tokens": ["Nun", "mein", "leit\u00b7stern", "sol\u00b7te", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "VVINF", "VMFIN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "3. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Recht und niedlich lieb zu kosen/", "tokens": ["Recht", "und", "nied\u00b7lich", "lieb", "zu", "ko\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "ADJD", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In der zier/ da nechst den rosen/", "tokens": ["In", "der", "zier", "/", "da", "nechst", "den", "ro\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$(", "ADV", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tulpen und narcissen sind/", "tokens": ["Tul\u00b7pen", "und", "nar\u00b7cis\u00b7sen", "sind", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gib den mund da mir der safft", "tokens": ["Gib", "den", "mund", "da", "mir", "der", "safft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "KOUS", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Einer honig-s\u00fcssen krafft/", "tokens": ["Ei\u00b7ner", "ho\u00b7nig\u00b7s\u00fcs\u00b7sen", "krafft", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nicht geringen trost verschafft.", "tokens": ["Nicht", "ge\u00b7rin\u00b7gen", "trost", "ver\u00b7schafft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "4. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Ruhm und sch\u00f6nen danck zu wissen/", "tokens": ["Ruhm", "und", "sch\u00f6\u00b7nen", "danck", "zu", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem hertzen seyn beflissen.", "tokens": ["In", "dem", "hert\u00b7zen", "seyn", "be\u00b7flis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tugend deine pracht und zier", "tokens": ["Tu\u00b7gend", "dei\u00b7ne", "pracht", "und", "zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gl\u00e4ntzt an ihren stralen voll", "tokens": ["Gl\u00e4ntzt", "an", "ih\u00b7ren", "stra\u00b7len", "voll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Eben da ich reisen soll/", "tokens": ["E\u00b7ben", "da", "ich", "rei\u00b7sen", "soll", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nun ich scheide/ lebe wohl.", "tokens": ["Nun", "ich", "schei\u00b7de", "/", "le\u00b7be", "wohl", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$(", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "5. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Reich an seegen/ reich an gl\u00fccke.", "tokens": ["Reich", "an", "see\u00b7gen", "/", "reich", "an", "gl\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "VVINF", "$(", "ADJD", "APPR", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Itzt da ich zur reise schicke/", "tokens": ["Itzt", "da", "ich", "zur", "rei\u00b7se", "schi\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Thu mir noch die courtesie/", "tokens": ["Thu", "mir", "noch", "die", "cour\u00b7te\u00b7sie", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "G\u00fcldnes kind/ und lasse du/", "tokens": ["G\u00fcld\u00b7nes", "kind", "/", "und", "las\u00b7se", "du", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "KON", "VVFIN", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Endlich mir die letzte ruh/", "tokens": ["End\u00b7lich", "mir", "die", "letz\u00b7te", "ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur auff deinen lippen zu.", "tokens": ["Nur", "auff", "dei\u00b7nen", "lip\u00b7pen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}