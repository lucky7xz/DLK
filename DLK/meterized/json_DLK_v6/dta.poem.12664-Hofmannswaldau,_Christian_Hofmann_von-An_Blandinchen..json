{"dta.poem.12664": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An Blandinchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Blandinchen! reiner schwan,", "tokens": ["Blan\u00b7din\u00b7chen", "!", "rei\u00b7ner", "schwan", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der nichts, als sromm seyn, kan!", "tokens": ["Der", "nichts", ",", "als", "sromm", "seyn", ",", "kan", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIS", "$,", "KOUS", "ADJD", "VAINF", "$,", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein heller tugend-schall,", "tokens": ["Dein", "hel\u00b7ler", "tu\u00b7gen\u00b7dschall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du sch\u00f6ne nachtigall!", "tokens": ["Du", "sch\u00f6\u00b7ne", "nach\u00b7ti\u00b7gall", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Klingt besser, als citrinchen,", "tokens": ["Klingt", "bes\u00b7ser", ",", "als", "cit\u00b7rin\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Blandinchen!", "tokens": ["Blan\u00b7din\u00b7chen", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Die lerche mag ich nicht,", "tokens": ["Die", "ler\u00b7che", "mag", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Auch sonsten kein gericht,", "tokens": ["Auch", "sons\u00b7ten", "kein", "ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenns gleich das beste w\u00e4r", "tokens": ["Wenns", "gleich", "das", "bes\u00b7te", "w\u00e4r"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Aus Palaestina her,", "tokens": ["Aus", "Pa\u00b7laes\u00b7ti\u00b7na", "her", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Als dich, du haselh\u00fcnchen!", "tokens": ["Als", "dich", ",", "du", "ha\u00b7sel\u00b7h\u00fcn\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Blandinchen!", "tokens": ["Blan\u00b7din\u00b7chen", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Blandinchen! gold und geld,", "tokens": ["Blan\u00b7din\u00b7chen", "!", "gold", "und", "geld", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die sch\u00e4tze dieser welt", "tokens": ["Die", "sch\u00e4t\u00b7ze", "die\u00b7ser", "welt"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sind nur spreu gegen dir:", "tokens": ["Sind", "nur", "spreu", "ge\u00b7gen", "dir", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "APPR", "PPER", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Du bleibest meine zier,", "tokens": ["Du", "blei\u00b7best", "mei\u00b7ne", "zier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und meiner sch\u00e4tze tr\u00fcnchen,", "tokens": ["Und", "mei\u00b7ner", "sch\u00e4t\u00b7ze", "tr\u00fcn\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Blandinchen!", "tokens": ["Blan\u00b7din\u00b7chen", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Wenn deiner lippen k\u00fc\u00df,", "tokens": ["Wenn", "dei\u00b7ner", "lip\u00b7pen", "k\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ich dann und wann gen\u00fc\u00df,", "tokens": ["Ich", "dann", "und", "wann", "ge\u00b7n\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KON", "PWAV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So fliest ein zucker-thau", "tokens": ["So", "fliest", "ein", "zu\u00b7cke\u00b7r\u00b7thau"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Aus deines hertzens-au,", "tokens": ["Aus", "dei\u00b7nes", "hert\u00b7zens\u00b7au", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du honigmachend bienchen!", "tokens": ["Du", "ho\u00b7nig\u00b7ma\u00b7chend", "bien\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Blandinchen!", "tokens": ["Blan\u00b7din\u00b7chen", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "Blandinchen! wenn dein licht", "tokens": ["Blan\u00b7din\u00b7chen", "!", "wenn", "dein", "licht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mir scheinet ins gesicht,", "tokens": ["Mir", "schei\u00b7net", "ins", "ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So sp\u00fcr ich, da\u00df die krafft", "tokens": ["So", "sp\u00fcr", "ich", ",", "da\u00df", "die", "krafft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mir starcke regung schafft", "tokens": ["Mir", "star\u00b7cke", "re\u00b7gung", "schafft"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gar bi\u00df ins lincke zinchen,", "tokens": ["Gar", "bi\u00df", "ins", "lin\u00b7cke", "zin\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Blandinchen!", "tokens": ["Blan\u00b7din\u00b7chen", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.6": {"line.1": {"text": "Blandinchen! vor die lust,", "tokens": ["Blan\u00b7din\u00b7chen", "!", "vor", "die", "lust", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So uns allein bewust,", "tokens": ["So", "uns", "al\u00b7lein", "be\u00b7wust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nimm dieses h\u00e4ubgen an,", "tokens": ["Nimm", "die\u00b7ses", "h\u00e4ub\u00b7gen", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das schenckt dir dein galan", "tokens": ["Das", "schenckt", "dir", "dein", "ga\u00b7lan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vor deines krantzes sch\u00fcnchen,", "tokens": ["Vor", "dei\u00b7nes", "krant\u00b7zes", "sch\u00fcn\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Blandinchen!", "tokens": ["Blan\u00b7din\u00b7chen", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}