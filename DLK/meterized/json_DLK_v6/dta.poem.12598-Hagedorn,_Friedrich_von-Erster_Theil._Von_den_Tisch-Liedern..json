{"dta.poem.12598": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Erster Theil.  \n Von den Tisch-Liedern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Seht, wie Zevs durch Regen-G\u00fcsse", "tokens": ["Seht", ",", "wie", "Zevs", "durch", "Re\u00b7gen\u00b7G\u00fcs\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "NE", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Felder \u00fcberschwemmt;", "tokens": ["Fel\u00b7der", "\u00fc\u00b7bersc\u00b7hwemmt", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Seht, der Lauf der schnellsten Fl\u00fcsse", "tokens": ["Seht", ",", "der", "Lauf", "der", "schnells\u00b7ten", "Fl\u00fcs\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird durch Eis gehemmt;", "tokens": ["Wird", "durch", "Eis", "ge\u00b7hemmt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "V\u00f6llig unterthan.", "tokens": ["V\u00f6l\u00b7lig", "un\u00b7ter\u00b7than", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Fort! den Frost nicht zu empfinden,", "tokens": ["Fort", "!", "den", "Frost", "nicht", "zu", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Z\u00fcndet Feuer an!", "tokens": ["Z\u00fcn\u00b7det", "Feu\u00b7er", "an", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Doch man mu\u00df, nach meinem D\u00fcnken,", "tokens": ["Doch", "man", "mu\u00df", ",", "nach", "mei\u00b7nem", "D\u00fcn\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jtzt auch lustig seyn.", "tokens": ["Jtzt", "auch", "lus\u00b7tig", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Gebt uns reichlich Wein zu trinken;", "tokens": ["Gebt", "uns", "reich\u00b7lich", "Wein", "zu", "trin\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber guten Wein.", "tokens": ["A\u00b7ber", "gu\u00b7ten", "Wein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Der, ihr wi\u00dft schon, den ich meyne,", "tokens": ["Der", ",", "ihr", "wi\u00dft", "schon", ",", "den", "ich", "mey\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PPER", "VVFIN", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Honig-Farbe zeigt,", "tokens": ["Ho\u00b7nig\u00b7Fa\u00b7rbe", "zeigt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und nicht, wie die andern Weine,", "tokens": ["Und", "nicht", ",", "wie", "die", "an\u00b7dern", "Wei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gleich zu Kopfe steigt.", "tokens": ["Gleich", "zu", "Kop\u00b7fe", "steigt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}