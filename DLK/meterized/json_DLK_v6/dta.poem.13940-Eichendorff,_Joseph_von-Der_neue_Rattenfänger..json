{"dta.poem.13940": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der neue Rattenf\u00e4nger.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Was! wir gering? Ihr vornehm, reich?", "tokens": ["Was", "!", "wir", "ge\u00b7ring", "?", "Ihr", "vor\u00b7nehm", ",", "reich", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "$.", "PPER", "ADJD", "$.", "PPER", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Planirend schwirrt die Scheere,", "tokens": ["Pla\u00b7ni\u00b7rend", "schwirrt", "die", "Schee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Seid Lumps' wie wir, so sind wir gleich,", "tokens": ["Seid", "Lumps'", "wie", "wir", ",", "so", "sind", "wir", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "NN", "KOKOM", "PPER", "$,", "ADV", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00fcbsch breit wird die Misere!", "tokens": ["H\u00fcbsch", "breit", "wird", "die", "Mi\u00b7se\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}}}}}