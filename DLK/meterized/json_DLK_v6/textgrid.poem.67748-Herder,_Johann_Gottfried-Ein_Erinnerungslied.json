{"textgrid.poem.67748": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Ein Erinnerungslied", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nimm mich, nimm mich, Muse froher Tugend,", "tokens": ["Nimm", "mich", ",", "nimm", "mich", ",", "Mu\u00b7se", "fro\u00b7her", "Tu\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "VVIMP", "PPER", "$,", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mich noch einmal hin ins Eden meiner Jugend,", "tokens": ["Mich", "noch", "ein\u00b7mal", "hin", "ins", "E\u00b7den", "mei\u00b7ner", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Das als Knospe mich dem Fr\u00fchlinge entwand,", "tokens": ["Das", "als", "Knos\u00b7pe", "mich", "dem", "Fr\u00fch\u00b7lin\u00b7ge", "ent\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KOUS", "NN", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.4": {"text": "Wo ich, ganz mit Grazie begossen,", "tokens": ["Wo", "ich", ",", "ganz", "mit", "Gra\u00b7zie", "be\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Meiner Freude erste Bl\u00fcth' genossen,", "tokens": ["Mei\u00b7ner", "Freu\u00b7de", "ers\u00b7te", "Bl\u00fcth'", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Jeder Bl\u00fcthe Morgenduft empfand!", "tokens": ["Je\u00b7der", "Bl\u00fc\u00b7the", "Mor\u00b7gen\u00b7duft", "emp\u00b7fand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Nimm von mir zwei Jahr-Olympiaden,", "tokens": ["Nimm", "von", "mir", "zwei", "Jahr\u00b7Olym\u00b7pi\u00b7a\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df mich in der Wunderquelle baden,", "tokens": ["La\u00df", "mich", "in", "der", "Wun\u00b7der\u00b7quel\u00b7le", "ba\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die den Greis zum J\u00fcngling neu verj\u00fcngt,", "tokens": ["Die", "den", "Greis", "zum", "J\u00fcng\u00b7ling", "neu", "ver\u00b7j\u00fcngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Die ins graue Haar ihm Rosen bindet,", "tokens": ["Die", "ins", "grau\u00b7e", "Haar", "ihm", "Ro\u00b7sen", "bin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "NN", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Um den d\u00fcrren Stab ihm Weinlaub windet,", "tokens": ["Um", "den", "d\u00fcr\u00b7ren", "Stab", "ihm", "Wein\u00b7laub", "win\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "In die Adern neuen Fr\u00fchling singt;", "tokens": ["In", "die", "A\u00b7dern", "neu\u00b7en", "Fr\u00fch\u00b7ling", "singt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Wo ich einst mit langen Othemz\u00fcgen", "tokens": ["Wo", "ich", "einst", "mit", "lan\u00b7gen", "O\u00b7them\u00b7z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Heitern Aether, immer neu Vergn\u00fcgen,", "tokens": ["Hei\u00b7tern", "A\u00b7e\u00b7ther", ",", "im\u00b7mer", "neu", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "ADJD", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Mit der Muttermilch die Weisheit sog,", "tokens": ["Mit", "der", "Mut\u00b7ter\u00b7milch", "die", "Weis\u00b7heit", "sog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Mit dem Auge \u00fcberall sah junge Reize bl\u00fchen,", "tokens": ["Mit", "dem", "Au\u00b7ge", "\u00fc\u00b7be\u00b7rall", "sah", "jun\u00b7ge", "Rei\u00b7ze", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "Mit dem Ohre \u00fcberall Gef\u00fchl und Harmonieen", "tokens": ["Mit", "dem", "Oh\u00b7re", "\u00fc\u00b7be\u00b7rall", "Ge\u00b7f\u00fchl", "und", "Har\u00b7mo\u00b7ni\u00b7e\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "NN", "KON", "NN"], "meter": "+-+-+-+-+-+----", "measure": "unknown.measure.hexa"}, "line.6": {"text": "Und der Freude Erstling in mich zog.", "tokens": ["Und", "der", "Freu\u00b7de", "Erst\u00b7ling", "in", "mich", "zog", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Drum, o G\u00f6ttin meiner Jugendtr\u00e4ume,", "tokens": ["Drum", ",", "o", "G\u00f6t\u00b7tin", "mei\u00b7ner", "Ju\u00b7gend\u00b7tr\u00e4u\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "FM", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Komm und zeige mir die anmuthsreichen B\u00e4ume,", "tokens": ["Komm", "und", "zei\u00b7ge", "mir", "die", "an\u00b7muths\u00b7rei\u00b7chen", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Die mit mir zur Krone aufgebl\u00fcht,", "tokens": ["Die", "mit", "mir", "zur", "Kro\u00b7ne", "auf\u00b7ge\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wo mein erstes Liedeslallen ist gelungen,", "tokens": ["Wo", "mein", "ers\u00b7tes", "Lie\u00b7des\u00b7lal\u00b7len", "ist", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Wo Du mir ein st\u00e4rker Lied gesungen,", "tokens": ["Wo", "Du", "mir", "ein", "st\u00e4r\u00b7ker", "Lied", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Das noch jetzt durch meine Adern gl\u00fcht!", "tokens": ["Das", "noch", "jetzt", "durch", "mei\u00b7ne", "A\u00b7dern", "gl\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Zeige mir den Stamm, in den die Namen", "tokens": ["Zei\u00b7ge", "mir", "den", "Stamm", ",", "in", "den", "die", "Na\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "$,", "APPR", "ART", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deiner S\u00f6hne, Kleist's und Lessing's, kamen,", "tokens": ["Dei\u00b7ner", "S\u00f6h\u00b7ne", ",", "Kleist's", "und", "Les\u00b7sing's", ",", "ka\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NE", "KON", "NE", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Denen ich so oft, ach! nachgelallt,", "tokens": ["De\u00b7nen", "ich", "so", "oft", ",", "ach", "!", "nach\u00b7ge\u00b7lallt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADV", "$,", "ITJ", "$.", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Oft benetzt mit hei\u00dfen Jugendz\u00e4hren!", "tokens": ["Oft", "be\u00b7netzt", "mit", "hei\u00b7\u00dfen", "Ju\u00b7gend\u00b7z\u00e4h\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "O, wenn alle volle Zweige w\u00e4ren,", "tokens": ["O", ",", "wenn", "al\u00b7le", "vol\u00b7le", "Zwei\u00b7ge", "w\u00e4\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PIAT", "ADJA", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Bl\u00fchte schon ein kleiner Lorbeerwald!", "tokens": ["Bl\u00fch\u00b7te", "schon", "ein", "klei\u00b7ner", "Lor\u00b7beer\u00b7wald", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Hier will ich mir meine Leyer h\u00f6hlen,", "tokens": ["Hier", "will", "ich", "mir", "mei\u00b7ne", "Le\u00b7yer", "h\u00f6h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du sollst sie mit Zauberton beseelen,", "tokens": ["Du", "sollst", "sie", "mit", "Zau\u00b7ber\u00b7ton", "be\u00b7see\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Amorn in sie schlie\u00dfen, der sie d\u00e4mpft.", "tokens": ["A\u00b7morn", "in", "sie", "schlie\u00b7\u00dfen", ",", "der", "sie", "d\u00e4mpft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "VVINF", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Gieb Dein goldnes Haar zu goldnen Saiten,", "tokens": ["Gieb", "Dein", "gold\u00b7nes", "Haar", "zu", "gold\u00b7nen", "Sai\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Deine Stimme, jeden Ton zu leiten,", "tokens": ["Dei\u00b7ne", "Stim\u00b7me", ",", "je\u00b7den", "Ton", "zu", "lei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und Dein Lied, das mit ihr k\u00e4mpft!", "tokens": ["Und", "Dein", "Lied", ",", "das", "mit", "ihr", "k\u00e4mpft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Dann will ich zwar nicht auf Sonnenpfeilen", "tokens": ["Dann", "will", "ich", "zwar", "nicht", "auf", "Son\u00b7nen\u00b7pfei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Adlergleich den h\u00f6chsten Aether theilen,", "tokens": ["Ad\u00b7lerg\u00b7leich", "den", "h\u00f6chs\u00b7ten", "A\u00b7e\u00b7ther", "thei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Um mein Land zum Pindus zu erh\u00f6hn;", "tokens": ["Um", "mein", "Land", "zum", "Pin\u00b7dus", "zu", "er\u00b7h\u00f6hn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Aber sanft mit stillen Taubenfl\u00fcgeln", "tokens": ["A\u00b7ber", "sanft", "mit", "stil\u00b7len", "Tau\u00b7ben\u00b7fl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Will ich von der Morgenr\u00f6the H\u00fcgeln", "tokens": ["Will", "ich", "von", "der", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "H\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Meine Jugend wiedersehn.", "tokens": ["Mei\u00b7ne", "Ju\u00b7gend", "wie\u00b7der\u00b7sehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Meinen Kindheitsengel will ich singen,", "tokens": ["Mei\u00b7nen", "Kind\u00b7heits\u00b7en\u00b7gel", "will", "ich", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der mein junges Leben oft aus Schlingen,", "tokens": ["Der", "mein", "jun\u00b7ges", "Le\u00b7ben", "oft", "aus", "Schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Aus Gefahren mich mit sanften Banden ri\u00df;", "tokens": ["Aus", "Ge\u00b7fah\u00b7ren", "mich", "mit", "sanf\u00b7ten", "Ban\u00b7den", "ri\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Der dem zarten Geist der Weisheit Namen", "tokens": ["Der", "dem", "zar\u00b7ten", "Geist", "der", "Weis\u00b7heit", "Na\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Einschrieb, der ins weiche Herz den Samen", "tokens": ["Ein\u00b7schrieb", ",", "der", "ins", "wei\u00b7che", "Herz", "den", "Sa\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Jeder Tugend pflanzen lie\u00df.", "tokens": ["Je\u00b7der", "Tu\u00b7gend", "pflan\u00b7zen", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Singen will ich, die mit Mutterschmerzen", "tokens": ["Sin\u00b7gen", "will", "ich", ",", "die", "mit", "Mut\u00b7ter\u00b7schmer\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "$,", "PRELS", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mir das beste Blut aus ihrem Herzen", "tokens": ["Mir", "das", "bes\u00b7te", "Blut", "aus", "ih\u00b7rem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Schenkte, die mich in die Welt gebar,", "tokens": ["Schenk\u00b7te", ",", "die", "mich", "in", "die", "Welt", "ge\u00b7bar", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PRF", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Mich mit Milch und Schlaf und Nektark\u00fcssen tr\u00e4nkte,", "tokens": ["Mich", "mit", "Milch", "und", "Schlaf", "und", "Nek\u00b7tar\u00b7k\u00fcs\u00b7sen", "tr\u00e4nk\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Meinen Fu\u00df und Herz und Zunge lenkte", "tokens": ["Mei\u00b7nen", "Fu\u00df", "und", "Herz", "und", "Zun\u00b7ge", "lenk\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und mein zweiter Sch\u00f6pfer war.", "tokens": ["Und", "mein", "zwei\u00b7ter", "Sch\u00f6p\u00b7fer", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Mutter, die mich mit der Weisheit Tropfen n\u00e4hrte,", "tokens": ["Mut\u00b7ter", ",", "die", "mich", "mit", "der", "Weis\u00b7heit", "Trop\u00b7fen", "n\u00e4hr\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Die mich beten, f\u00fchlen, denken lehrte,", "tokens": ["Die", "mich", "be\u00b7ten", ",", "f\u00fch\u00b7len", ",", "den\u00b7ken", "lehr\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "VVFIN", "$,", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "O, gesegnet seist Du, Mutterschoo\u00df!", "tokens": ["O", ",", "ge\u00b7seg\u00b7net", "seist", "Du", ",", "Mut\u00b7ter\u00b7schoo\u00df", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVPP", "VAFIN", "PPER", "$,", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wo ich fr\u00fch als Mensch und Christ schon f\u00fchlte,", "tokens": ["Wo", "ich", "fr\u00fch", "als", "Mensch", "und", "Christ", "schon", "f\u00fchl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "KOKOM", "NN", "KON", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Gott mit Thr\u00e4nen lallte, \u2013 und sie k\u00fchlte", "tokens": ["Gott", "mit", "Thr\u00e4\u00b7nen", "lall\u00b7te", ",", "\u2013", "und", "sie", "k\u00fchl\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VVFIN", "$,", "$(", "KON", "PPER", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Jede Thr\u00e4n', die ich vergo\u00df.", "tokens": ["Je\u00b7de", "Thr\u00e4n'", ",", "die", "ich", "ver\u00b7go\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Thr\u00e4nend will ich ihre H\u00e4nde k\u00fcssen,", "tokens": ["Thr\u00e4\u00b7nend", "will", "ich", "ih\u00b7re", "H\u00e4n\u00b7de", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Thr\u00e4nend meines Vaters Grab umschlie\u00dfen,", "tokens": ["Thr\u00e4\u00b7nend", "mei\u00b7nes", "Va\u00b7ters", "Grab", "um\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wie ein Pilgrim, ringsum Rosen streun.", "tokens": ["Wie", "ein", "Pil\u00b7grim", ",", "ring\u00b7sum", "Ro\u00b7sen", "streun", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ADV", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ruhe sanft, mein Vater, Freund, Erhalter,", "tokens": ["Ru\u00b7he", "sanft", ",", "mein", "Va\u00b7ter", ",", "Freund", ",", "Er\u00b7hal\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PPOSAT", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Patriot f\u00fcr zweene Menschenalter,", "tokens": ["Pat\u00b7ri\u00b7ot", "f\u00fcr", "zwee\u00b7ne", "Men\u00b7schen\u00b7al\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Dieses sei Dein Leichenstein!", "tokens": ["Die\u00b7ses", "sei", "Dein", "Lei\u00b7chen\u00b7stein", "!"], "token_info": ["word", "word", "word", "word", "pu