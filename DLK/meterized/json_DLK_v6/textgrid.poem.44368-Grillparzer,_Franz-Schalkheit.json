{"textgrid.poem.44368": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Schalkheit", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "L\u00e4chelst du mir durch die Zweige,", "tokens": ["L\u00e4\u00b7chelst", "du", "mir", "durch", "die", "Zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Muse, lieblich anzuschauen?", "tokens": ["Mu\u00b7se", ",", "lieb\u00b7lich", "an\u00b7zu\u00b7schau\u00b7en", "?"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und verweigerst doch zu kommen!", "tokens": ["Und", "ver\u00b7wei\u00b7gerst", "doch", "zu", "kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ganz herbei komm, oder fort ganz!", "tokens": ["Ganz", "her\u00b7bei", "komm", ",", "o\u00b7der", "fort", "ganz", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$,", "KON", "PTKVZ", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn, ich geb es zu erw\u00e4gen,", "tokens": ["Denn", ",", "ich", "geb", "es", "zu", "er\u00b7w\u00e4\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Obs auch recht und billig schien,", "tokens": ["Obs", "auch", "recht", "und", "bil\u00b7lig", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Erst mit Blicken aufzuregen,", "tokens": ["Erst", "mit", "Bli\u00b7cken", "auf\u00b7zu\u00b7re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dann den Aufgeregten fliehn!", "tokens": ["Dann", "den", "Auf\u00b7ge\u00b7reg\u00b7ten", "fliehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}