{"textgrid.poem.53902": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Alfred Kerr", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nu schicken alle B\u00e4cker Kuchen", "tokens": ["Nu", "schi\u00b7cken", "al\u00b7le", "B\u00e4\u00b7cker", "Ku\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "nach Ihrem Haus in'n Jrunewald.", "tokens": ["nach", "Ih\u00b7rem", "Haus", "in'n", "Jru\u00b7ne\u00b7wald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schohspieler kommen Sie besuchen", "tokens": ["Schoh\u00b7spie\u00b7ler", "kom\u00b7men", "Sie", "be\u00b7su\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "von wegen hmzig Jahre alt.", "tokens": ["von", "we\u00b7gen", "hm\u00b7zig", "Jah\u00b7re", "alt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "CARD", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich schieb mir leise mit se ran,", "tokens": ["Ich", "schieb", "mir", "lei\u00b7se", "mit", "se", "ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "mit Orska und Herrn Sudermann,", "tokens": ["mit", "O\u00b7rska", "und", "Herrn", "Su\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "NE", "$,"], "meter": "--+-++-+", "measure": "anapaest.init"}, "line.7": {"text": "und ruf aus der pariser Ferne:", "tokens": ["und", "ruf", "aus", "der", "pa\u00b7ri\u00b7ser", "Fer\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbick kann mir nich helfen \u2013 ick hab Ihnen jerne \u2013!\u00ab", "tokens": ["\u00bb", "ick", "kann", "mir", "nich", "hel\u00b7fen", "\u2013", "ick", "hab", "Ih\u00b7nen", "jer\u00b7ne", "\u2013", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$(", "PPER", "VAFIN", "PPER", "ADV", "$(", "$.", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie haben, als wir angefangen,", "tokens": ["Sie", "ha\u00b7ben", ",", "als", "wir", "an\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "uns doch das Laufen beigebracht.", "tokens": ["uns", "doch", "das", "Lau\u00b7fen", "bei\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ick bin nich imma mitjejangen \u2013", "tokens": ["Ick", "bin", "nich", "im\u00b7ma", "mit\u00b7je\u00b7jan\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "VVINF", "$("], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "zum Beispiel: bei die Russenschlacht . . .", "tokens": ["zum", "Bei\u00b7spiel", ":", "bei", "die", "Rus\u00b7sen\u00b7schlacht", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPRART", "NN", "$.", "APPR", "ART", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch einen, der die Sprache packt,", "tokens": ["Doch", "ei\u00b7nen", ",", "der", "die", "Spra\u00b7che", "packt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und nie Bolljong \u2013 und stets Extrakt \u2013", "tokens": ["und", "nie", "Boll\u00b7jong", "\u2013", "und", "stets", "Ex\u00b7trakt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NE", "$(", "KON", "ADV", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "des such dir man mit die Lanterne:", "tokens": ["des", "such", "dir", "man", "mit", "die", "Lan\u00b7ter\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Ick kann mir nich helfen \u2013 ich hab Ihnen jerne.", "tokens": ["Ick", "kann", "mir", "nich", "hel\u00b7fen", "\u2013", "ich", "hab", "Ih\u00b7nen", "jer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$(", "PPER", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Musik! Musik vor allen Dingen!", "tokens": ["Mu\u00b7sik", "!", "Mu\u00b7sik", "vor", "al\u00b7len", "Din\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie heben eine schmale Hand", "tokens": ["Sie", "he\u00b7ben", "ei\u00b7ne", "schma\u00b7le", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und lassen \u00c4therwellen klingen", "tokens": ["und", "las\u00b7sen", "\u00c4t\u00b7her\u00b7wel\u00b7len", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und spielen den Dichter an die Wand.", "tokens": ["und", "spie\u00b7len", "den", "Dich\u00b7ter", "an", "die", "Wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Sie zogen manchem stramm die Hosen", "tokens": ["Sie", "zo\u00b7gen", "man\u00b7chem", "stramm", "die", "Ho\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "bei uns und in den U.S.A.,", "tokens": ["bei", "uns", "und", "in", "den", "U.", "S.", "A.", ","], "token_info": ["word", "word", "word", "word", "word", "abbreviation", "abbreviation", "abbreviation", "punct"], "pos": ["APPR", "PPER", "KON", "APPR", "ART", "ADJA", "NN", "APPRART", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "und trugen ein Florett in Rosen;", "tokens": ["und", "tru\u00b7gen", "ein", "Flo\u00b7rett", "in", "Ro\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "es pfeift Ihr Nein, es ehrt Ihr Ja.", "tokens": ["es", "pfeift", "Ihr", "Nein", ",", "es", "ehrt", "Ihr", "Ja", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sie haben unsere Tr\u00e4nen geweint,", "tokens": ["Sie", "ha\u00b7ben", "un\u00b7se\u00b7re", "Tr\u00e4\u00b7nen", "ge\u00b7weint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Ihr Lachen hat uns alle vereint.", "tokens": ["Ihr", "La\u00b7chen", "hat", "uns", "al\u00b7le", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PIS", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Sie sehen die Erde", "tokens": ["Sie", "se\u00b7hen", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "und die Sterne \u2013:", "tokens": ["und", "die", "Ster\u00b7ne", "\u2013", ":"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "$(", "$."], "meter": "--+-", "measure": "anapaest.init"}, "line.13": {"text": "Ick kann mir nich helfen \u2013 ick hab Ihnen jerne.", "tokens": ["Ick", "kann", "mir", "nich", "hel\u00b7fen", "\u2013", "ick", "hab", "Ih\u00b7nen", "jer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$(", "PPER", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}