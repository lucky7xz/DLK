{"dta.poem.15807": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du fragest, was du sollst, was nicht, in Verse bringen?", "tokens": ["Du", "fra\u00b7gest", ",", "was", "du", "sollst", ",", "was", "nicht", ",", "in", "Ver\u00b7se", "brin\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$,", "PRELS", "PTKNEG", "$,", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was dir in Prosa nicht zu fassen will gelingen.", "tokens": ["Was", "dir", "in", "Pro\u00b7sa", "nicht", "zu", "fas\u00b7sen", "will", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "PTKNEG", "PTKZU", "VVINF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Verloren ist die Kunst, in Versen vorzutragen,", "tokens": ["Ver\u00b7lo\u00b7ren", "ist", "die", "Kunst", ",", "in", "Ver\u00b7sen", "vor\u00b7zu\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "APPR", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was du gef\u00e4lliger in Prosa k\u00f6nntest sagen.", "tokens": ["Was", "du", "ge\u00b7f\u00e4l\u00b7li\u00b7ger", "in", "Pro\u00b7sa", "k\u00f6nn\u00b7test", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJ