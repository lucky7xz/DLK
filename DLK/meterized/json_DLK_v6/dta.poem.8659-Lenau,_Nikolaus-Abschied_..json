{"dta.poem.8659": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Abschied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Sey mir zum leztenmal gegr\u00fc\u00dft,", "tokens": ["Sey", "mir", "zum", "lez\u00b7ten\u00b7mal", "ge\u00b7gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Vaterland, das feige dumm,", "tokens": ["Mein", "Va\u00b7ter\u00b7land", ",", "das", "fei\u00b7ge", "dumm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Ferse dem Despoten k\u00fc\u00dft,", "tokens": ["Die", "Fer\u00b7se", "dem", "Des\u00b7po\u00b7ten", "k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und seinem Wink gehorchet stumm.", "tokens": ["Und", "sei\u00b7nem", "Wink", "ge\u00b7hor\u00b7chet", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wohl schlief das Kind in deinem Arm,", "tokens": ["Wohl", "schlief", "das", "Kind", "in", "dei\u00b7nem", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du gabst, was Knaben freuen kann,", "tokens": ["Du", "gabst", ",", "was", "Kna\u00b7ben", "freu\u00b7en", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der J\u00fcngling fand ein Liebchen warm;", "tokens": ["Der", "J\u00fcng\u00b7ling", "fand", "ein", "Lieb\u00b7chen", "warm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch keine Freiheit fand der Mann.", "tokens": ["Doch", "kei\u00b7ne", "Frei\u00b7heit", "fand", "der", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Im Hochland streckt der J\u00e4ger sich", "tokens": ["Im", "Hoch\u00b7land", "streckt", "der", "J\u00e4\u00b7ger", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Boden schnell, wenn Wildesschaar", "tokens": ["Zu", "Bo\u00b7den", "schnell", ",", "wenn", "Wil\u00b7des\u00b7schaar"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "ADJD", "$,", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Heran sich st\u00fcrzet f\u00fcrchterlich,", "tokens": ["He\u00b7ran", "sich", "st\u00fcr\u00b7zet", "f\u00fcrch\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann schnaubt vor\u00fcber die Gefahr:", "tokens": ["Dann", "schnaubt", "vor\u00b7\u00fc\u00b7ber", "die", "Ge\u00b7fahr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mein Vaterland, so sinkst du hin,", "tokens": ["Mein", "Va\u00b7ter\u00b7land", ",", "so", "sinkst", "du", "hin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Rauscht deines Herrschers Tritt heran,", "tokens": ["Rauscht", "dei\u00b7nes", "Herr\u00b7schers", "Tritt", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und l\u00e4ssest ihn vor\u00fcberzieh'n,", "tokens": ["Und", "l\u00e4s\u00b7sest", "ihn", "vor\u00b7\u00fcber\u00b7zieh'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und h\u00e4ltst den bangen Athem an. \u2014", "tokens": ["Und", "h\u00e4ltst", "den", "ban\u00b7gen", "A\u00b7them", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Fleug, Schiff, wie Wolken durch die Luft,", "tokens": ["Fleug", ",", "Schiff", ",", "wie", "Wol\u00b7ken", "durch", "die", "Luft", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PWAV", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hin, wo die G\u00f6tterflamme brennt!", "tokens": ["Hin", ",", "wo", "die", "G\u00f6t\u00b7ter\u00b7flam\u00b7me", "brennt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Sp\u00fcl' mir hinweg, o Meer, die Kluft,", "tokens": ["Sp\u00fcl'", "mir", "hin\u00b7weg", ",", "o", "Meer", ",", "die", "Kluft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "FM", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die von der Freiheit noch mich trennt!", "tokens": ["Die", "von", "der", "Frei\u00b7heit", "noch", "mich", "trennt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du neue Welt, du freie Welt,", "tokens": ["Du", "neu\u00b7e", "Welt", ",", "du", "frei\u00b7e", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An deren bl\u00fcthenreichem Strand", "tokens": ["An", "de\u00b7ren", "bl\u00fc\u00b7then\u00b7rei\u00b7chem", "Strand"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Fluth der Tyrannei zerschellt,", "tokens": ["Die", "Fluth", "der", "Ty\u00b7ran\u00b7nei", "zer\u00b7schellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich gr\u00fc\u00dfe dich, mein Vaterland!", "tokens": ["Ich", "gr\u00fc\u00b7\u00dfe", "dich", ",", "mein", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}