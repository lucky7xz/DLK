{"dta.poem.10761": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "I.  \n Sonnet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.85", "sv:0.14"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Evch ruff ich erstlich an jhr draymahl drey G\u00f6ttinnen/", "tokens": ["Evch", "ruff", "ich", "erst\u00b7lich", "an", "jhr", "dray\u00b7mahl", "drey", "G\u00f6t\u00b7tin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "ADV", "CARD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Macht aufs/ da\u00df ewer Brunn/ vnd Fl\u00fcgelspferde Flu\u00df", "tokens": ["Macht", "aufs", "/", "da\u00df", "e\u00b7wer", "Brunn", "/", "vnd", "Fl\u00fc\u00b7gel\u00b7spfer\u00b7de", "Flu\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPRART", "$(", "KOUS", "PPOSAT", "NN", "$(", "KON", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Abfliessen her zu mir in mein gem\u00fcthe mus.", "tokens": ["Ab\u00b7flies\u00b7sen", "her", "zu", "mir", "in", "mein", "ge\u00b7m\u00fc\u00b7the", "mus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APZR", "APPR", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Regieret meinen Geist/ regieret meine Sinnen/", "tokens": ["Re\u00b7gie\u00b7ret", "mei\u00b7nen", "Geist", "/", "re\u00b7gie\u00b7ret", "mei\u00b7ne", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df ich wie ich gern wolt vnd solt je mehr vnd mehr/", "tokens": ["Da\u00df", "ich", "wie", "ich", "gern", "wolt", "vnd", "solt", "je", "mehr", "vnd", "mehr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PWAV", "PPER", "ADV", "VMFIN", "KON", "VMFIN", "ADV", "ADV", "KON", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Erhebe weit vnd breit de\u00df Frawenzimmers Ehr.", "tokens": ["Er\u00b7he\u00b7be", "weit", "vnd", "breit", "de\u00df", "Fra\u00b7wen\u00b7zim\u00b7mers", "Ehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wolan so strecke dich/ o Feder wohlbekandt/", "tokens": ["Wo\u00b7lan", "so", "stre\u00b7cke", "dich", "/", "o", "Fe\u00b7der", "wohl\u00b7be\u00b7kandt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$(", "FM", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vnd mache dich an den so hochgeehrten standt/", "tokens": ["Vnd", "ma\u00b7che", "dich", "an", "den", "so", "hoch\u00b7geehr\u00b7ten", "standt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "ADV", "ADJA", "VVFIN", "$("], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Damit jhr Lob vnd Ruhm/ so weit m\u00f6g vmbher fliegen/", "tokens": ["Da\u00b7mit", "jhr", "Lob", "vnd", "Ruhm", "/", "so", "weit", "m\u00f6g", "vm\u00b7bher", "flie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "KON", "NN", "$(", "ADV", "ADJD", "ADJD", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So weit die Sonne laufft durchs grosse Himmelshau\u00df/", "tokens": ["So", "weit", "die", "Son\u00b7ne", "laufft", "durchs", "gros\u00b7se", "Him\u00b7mels\u00b7hau\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Beweise/ da\u00df sie mit volkommenheit durchau\u00df/", "tokens": ["Be\u00b7wei\u00b7se", "/", "da\u00df", "sie", "mit", "vol\u00b7kom\u00b7men\u00b7heit", "durch\u00b7au\u00df", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPER", "APPR", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Zwar wider den gebrauch/ den M\u00e4nnern selbst obliegen.", "tokens": ["Zwar", "wi\u00b7der", "den", "ge\u00b7brauch", "/", "den", "M\u00e4n\u00b7nern", "selbst", "ob\u00b7lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$(", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}