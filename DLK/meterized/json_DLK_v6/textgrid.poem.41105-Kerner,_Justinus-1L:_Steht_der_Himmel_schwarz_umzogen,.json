{"textgrid.poem.41105": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Steht der Himmel schwarz umzogen,", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Steht der Himmel schwarz umzogen,", "tokens": ["Steht", "der", "Him\u00b7mel", "schwarz", "um\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df man ihn mu\u00df traurend w\u00e4hnen,", "tokens": ["Da\u00df", "man", "ihn", "mu\u00df", "trau\u00b7rend", "w\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wandeln sich zum farb'gen Bogen", "tokens": ["Wan\u00b7deln", "sich", "zum", "fa\u00b7rb'\u00b7gen", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPRART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Schnell oft alle seine Tr\u00e4nen.", "tokens": ["Schnell", "oft", "al\u00b7le", "sei\u00b7ne", "Tr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mitten unter Donnerwettern", "tokens": ["Mit\u00b7ten", "un\u00b7ter", "Don\u00b7ner\u00b7wet\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rt' ich schon aus voller Kehle", "tokens": ["H\u00f6rt'", "ich", "schon", "aus", "vol\u00b7ler", "Keh\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Freudig eine Lerche schmettern,", "tokens": ["Freu\u00b7dig", "ei\u00b7ne", "Ler\u00b7che", "schmet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fl\u00f6ten eine Philomele.", "tokens": ["Fl\u00f6\u00b7ten", "ei\u00b7ne", "Phi\u00b7lo\u00b7me\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wird dir Erd' und Himmel tr\u00fcbe,", "tokens": ["Wird", "dir", "Erd'", "und", "Him\u00b7mel", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Beugt dich Gram und Alter nieder,", "tokens": ["Beugt", "dich", "Gram", "und", "Al\u00b7ter", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df nicht Jugend, la\u00df nicht Liebe,", "tokens": ["La\u00df", "nicht", "Ju\u00b7gend", ",", "la\u00df", "nicht", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "NN", "$,", "VVIMP", "PTKNEG", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df nicht den Gesang der Lieder!", "tokens": ["La\u00df", "nicht", "den", "Ge\u00b7sang", "der", "Lie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Treu, im immerwarmen Busen", "tokens": ["Treu", ",", "im", "im\u00b7mer\u00b7war\u00b7men", "Bu\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine innre Farbenquelle,", "tokens": ["Ei\u00b7ne", "inn\u00b7re", "Far\u00b7ben\u00b7quel\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wahr' den Born, mit dem die Musen", "tokens": ["Wahr'", "den", "Born", ",", "mit", "dem", "die", "Mu\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NE", "$,", "APPR", "PRELS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir ersetzen Tageshelle.", "tokens": ["Dir", "er\u00b7set\u00b7zen", "Ta\u00b7ges\u00b7hel\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}