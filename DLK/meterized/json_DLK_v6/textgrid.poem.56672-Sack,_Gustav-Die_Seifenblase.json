{"textgrid.poem.56672": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Die Seifenblase", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sie mit ihren D\u00fcnsten sich umgeben!", "tokens": ["Wie", "sie", "mit", "ih\u00b7ren", "D\u00fcns\u00b7ten", "sich", "um\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie sie, den Finger an der Himmelsnase,", "tokens": ["Wie", "sie", ",", "den", "Fin\u00b7ger", "an", "der", "Him\u00b7mels\u00b7na\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "weislich erforschen ihre eignen Gase,", "tokens": ["weis\u00b7lich", "er\u00b7for\u00b7schen", "ih\u00b7re", "eig\u00b7nen", "Ga\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "wie diese Kl\u00f6tze an der Erde kleben!", "tokens": ["wie", "die\u00b7se", "Kl\u00f6t\u00b7ze", "an", "der", "Er\u00b7de", "kle\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch immer hohler, weiter wird mein Leben,", "tokens": ["Doch", "im\u00b7mer", "hoh\u00b7ler", ",", "wei\u00b7ter", "wird", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "es wird noch, gl\u00e4nzend wie die Seifenblase,", "tokens": ["es", "wird", "noch", ",", "gl\u00e4n\u00b7zend", "wie", "die", "Sei\u00b7fen\u00b7bla\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "in schwereloser, wiegender Ekstase", "tokens": ["in", "schwe\u00b7re\u00b7lo\u00b7ser", ",", "wie\u00b7gen\u00b7der", "Eks\u00b7ta\u00b7se"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "mit allen Winden in die Weite schweben;", "tokens": ["mit", "al\u00b7len", "Win\u00b7den", "in", "die", "Wei\u00b7te", "schwe\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "und wenn auf ihren d\u00fcnnen Kugelschalen", "tokens": ["und", "wenn", "auf", "ih\u00b7ren", "d\u00fcn\u00b7nen", "Ku\u00b7gel\u00b7scha\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "sich eure schweren Nebelgr\u00fcnde bunt", "tokens": ["sich", "eu\u00b7re", "schwe\u00b7ren", "Ne\u00b7bel\u00b7gr\u00fcn\u00b7de", "bunt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "PPOSAT", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "verzerrt zu einem sch\u00f6nen Spuk abmalen,", "tokens": ["ver\u00b7zerrt", "zu", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Spuk", "ab\u00b7ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "wird sie in ihrer h\u00f6chsten Pracht der Schlund", "tokens": ["wird", "sie", "in", "ih\u00b7rer", "h\u00f6chs\u00b7ten", "Pracht", "der", "Schlund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "des Frostes und der Einsamkeit verzehren,", "tokens": ["des", "Fros\u00b7tes", "und", "der", "Ein\u00b7sam\u00b7keit", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "nichts wird in eure Tiefen wiederkehren.", "tokens": ["nichts", "wird", "in", "eu\u00b7re", "Tie\u00b7fen", "wie\u00b7der\u00b7keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}