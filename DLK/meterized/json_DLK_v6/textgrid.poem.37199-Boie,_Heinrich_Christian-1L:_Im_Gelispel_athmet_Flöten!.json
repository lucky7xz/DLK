{"textgrid.poem.37199": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Gelispel athmet Fl\u00f6ten!", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Gelispel athmet Fl\u00f6ten!", "tokens": ["Im", "Ge\u00b7lis\u00b7pel", "ath\u00b7met", "Fl\u00f6\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leis entschlummert sinkt das Haupt", "tokens": ["Leis", "ent\u00b7schlum\u00b7mert", "sinkt", "das", "Haupt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meiner Freundin, das zu r\u00f6then", "tokens": ["Mei\u00b7ner", "Freun\u00b7din", ",", "das", "zu", "r\u00f6\u00b7then"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich ein s\u00fc\u00dfer Traum erlaubt,", "tokens": ["Sich", "ein", "s\u00fc\u00b7\u00dfer", "Traum", "er\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und von Maienk\u00fchl' umf\u00e4chelt", "tokens": ["Und", "von", "Mai\u00b7en\u00b7k\u00fchl'", "um\u00b7f\u00e4\u00b7chelt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Liebe hauchet, Liebe l\u00e4chelt.", "tokens": ["Lie\u00b7be", "hau\u00b7chet", ",", "Lie\u00b7be", "l\u00e4\u00b7chelt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Blumen sind dem prallen Moose,", "tokens": ["Blu\u00b7men", "sind", "dem", "pral\u00b7len", "Moo\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das sie wieget, eingestickt;", "tokens": ["Das", "sie", "wie\u00b7get", ",", "ein\u00b7ge\u00b7stickt", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ueber ihr h\u00e4ngt eine Rose,", "tokens": ["Ue\u00b7ber", "ihr", "h\u00e4ngt", "ei\u00b7ne", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Die versch\u00e4mt am Stocke nickt;", "tokens": ["Die", "ver\u00b7sch\u00e4mt", "am", "Sto\u00b7cke", "nickt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und den Balsam rings ergie\u00dfen", "tokens": ["Und", "den", "Bal\u00b7sam", "rings", "er\u00b7gie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "L\u00fcfte, die sie sanft umschlie\u00dfen.", "tokens": ["L\u00fcf\u00b7te", ",", "die", "sie", "sanft", "um\u00b7schlie\u00b7\u00dfen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihr gelagert gegen\u00fcber", "tokens": ["Ihr", "ge\u00b7la\u00b7gert", "ge\u00b7gen\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVPP", "APPR"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wagt mein Odem keinen Zug.", "tokens": ["Wagt", "mein", "O\u00b7dem", "kei\u00b7nen", "Zug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kalt und gl\u00fchend als im Fieber", "tokens": ["Kalt", "und", "gl\u00fc\u00b7hend", "als", "im", "Fie\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "KOKOM", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hemm ich meines Seufzers Flug.", "tokens": ["Hemm", "ich", "mei\u00b7nes", "Seuf\u00b7zers", "Flug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn der Traum, der sie umschwebet,", "tokens": ["Wenn", "der", "Traum", ",", "der", "sie", "um\u00b7schwe\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur kein fremdes Bild belebet!", "tokens": ["Nur", "kein", "frem\u00b7des", "Bild", "be\u00b7le\u00b7bet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}