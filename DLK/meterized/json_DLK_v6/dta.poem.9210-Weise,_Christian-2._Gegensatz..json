{"dta.poem.9210": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "2.  Gegensatz.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Das wasser wird berg an spatziren/", "tokens": ["Das", "was\u00b7ser", "wird", "berg", "an", "spat\u00b7zi\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "NE", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die erde wird im monden gehn/", "tokens": ["Die", "er\u00b7de", "wird", "im", "mon\u00b7den", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die sonne wird am himmel frieren/", "tokens": ["Die", "son\u00b7ne", "wird", "am", "him\u00b7mel", "frie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die berge werden niedrig stehn/", "tokens": ["Die", "ber\u00b7ge", "wer\u00b7den", "nied\u00b7rig", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die tische werden untern b\u00e4ncken/", "tokens": ["Die", "ti\u00b7sche", "wer\u00b7den", "un\u00b7tern", "b\u00e4n\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die pf\u00fctzen \u00fcbern weiden hencken.", "tokens": ["Die", "pf\u00fct\u00b7zen", "\u00fc\u00b7bern", "wei\u00b7den", "hen\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Man wird das weinfa\u00df auf dem dache/", "tokens": ["Man", "wird", "das", "wein\u00b7fa\u00df", "auf", "dem", "da\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PDS", "VVFIN", "APPR", "ART", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das storchs-nest in dem keller baun/", "tokens": ["Das", "storchs\u00b7nest", "in", "dem", "kel\u00b7ler", "baun", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man wird ", "tokens": ["Man", "wird"], "token_info": ["word", "word"], "pos": ["PIS", "VAFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Citronen auf der fichte schaun/", "tokens": ["Cit\u00b7ro\u00b7nen", "auf", "der", "fich\u00b7te", "schaun", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Man wird mit alten scheiden handeln/", "tokens": ["Man", "wird", "mit", "al\u00b7ten", "schei\u00b7den", "han\u00b7deln", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "ADJA", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und sie vor in kaneel verwandeln.", "tokens": ["Und", "sie", "vor", "in", "ka\u00b7ne\u00b7el", "ver\u00b7wan\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Man wird das maul mit peche schmincken/", "tokens": ["Man", "wird", "das", "maul", "mit", "pe\u00b7che", "schmin\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "APPR", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Man wird speck auf die merthe streun/", "tokens": ["Man", "wird", "speck", "auf", "die", "mer\u00b7the", "streun", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADJD", "APPR", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man wird aus leehren kannen trincken/", "tokens": ["Man", "wird", "aus", "le\u00b7eh\u00b7ren", "kan\u00b7nen", "trin\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "VVINF", "VMFIN", "VVINF", "$("], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Bey faulen eyern lustig seyn/", "tokens": ["Bey", "fau\u00b7len", "ey\u00b7ern", "lus\u00b7tig", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Man wird das bier mit ellen messen/", "tokens": ["Man", "wird", "das", "bier", "mit", "el\u00b7len", "mes\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und heckerling vor spargen fressen.", "tokens": ["Und", "he\u00b7cker\u00b7ling", "vor", "spar\u00b7gen", "fres\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der wird sich auf ein pflugrad sehnen", "tokens": ["Der", "wird", "sich", "auf", "ein", "pflu\u00b7grad", "seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PRF", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der gerne weiche pretzeln frist/", "tokens": ["Der", "ger\u00b7ne", "wei\u00b7che", "pret\u00b7zeln", "frist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein ander gar nach hobelsp\u00e4nen", "tokens": ["Ein", "an\u00b7der", "gar", "nach", "ho\u00b7bel\u00b7sp\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der dem salate g\u00fcnstig ist/", "tokens": ["Der", "dem", "sa\u00b7la\u00b7te", "g\u00fcns\u00b7tig", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "VVFIN", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wann ich mein liebgen werde kennen/", "tokens": ["Wann", "ich", "mein", "lieb\u00b7gen", "wer\u00b7de", "ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "VVINF", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df sie mich wird ihr liebgen nennen.", "tokens": ["Da\u00df", "sie", "mich", "wird", "ihr", "lieb\u00b7gen", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}