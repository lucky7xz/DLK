{"textgrid.poem.62079": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[es wankt der Boden unter unsren F\u00fc\u00dfen]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wankt der Boden unter unsren F\u00fc\u00dfen,", "tokens": ["Es", "wankt", "der", "Bo\u00b7den", "un\u00b7ter", "un\u00b7sren", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des letzten Morgenrotes heilige Parole,", "tokens": ["Des", "letz\u00b7ten", "Mor\u00b7gen\u00b7ro\u00b7tes", "hei\u00b7li\u00b7ge", "Pa\u00b7ro\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gesegnet sch\u00f6n und anerkannt von Pol zu Pole;", "tokens": ["Ge\u00b7seg\u00b7net", "sch\u00f6n", "und", "an\u00b7er\u00b7kannt", "von", "Pol", "zu", "Po\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Menschlichkeit ist aus und Tr\u00e4nen flie\u00dfen.", "tokens": ["Die", "Menschlich\u00b7keit", "ist", "aus", "und", "Tr\u00e4\u00b7nen", "flie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "KON", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Es zieht die Nacht hinauf, die Schwerter blitzen,", "tokens": ["Es", "zieht", "die", "Nacht", "hin\u00b7auf", ",", "die", "Schwer\u00b7ter", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Irrlicht spr\u00fcht, kein einzig klares Sternlein gl\u00fcht,", "tokens": ["Das", "Irr\u00b7licht", "spr\u00fcht", ",", "kein", "ein\u00b7zig", "kla\u00b7res", "Stern\u00b7lein", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PIAT", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das zarte Bl\u00fcmlein unter Rosseshuf verbl\u00fcht, \u2013", "tokens": ["Das", "zar\u00b7te", "Bl\u00fcm\u00b7lein", "un\u00b7ter", "Ros\u00b7ses\u00b7huf", "ver\u00b7bl\u00fcht", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVPP", "$,", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Pulse gl\u00fch'n, die Leidenschaften sich erhitzen.", "tokens": ["Die", "Pul\u00b7se", "gl\u00fch'n", ",", "die", "Lei\u00b7den\u00b7schaf\u00b7ten", "sich", "er\u00b7hit\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was wird aus dieser sp\u00e4ten Nacht entstehen?", "tokens": ["Was", "wird", "aus", "die\u00b7ser", "sp\u00e4\u00b7ten", "Nacht", "ent\u00b7ste\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PDAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Sch\u00f6nste, was man ehrt, es wird zum Raube,", "tokens": ["Das", "Sch\u00f6ns\u00b7te", ",", "was", "man", "ehrt", ",", "es", "wird", "zum", "Rau\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,", "PPER", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Lieb' und Duldung liegen tief im Staube,", "tokens": ["Und", "Lieb'", "und", "Dul\u00b7dung", "lie\u00b7gen", "tief", "im", "Stau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was bleibt von allen Erdeng\u00fctern da noch stehen?", "tokens": ["Was", "bleibt", "von", "al\u00b7len", "Er\u00b7den\u00b7g\u00fc\u00b7tern", "da", "noch", "ste\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PIAT", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}