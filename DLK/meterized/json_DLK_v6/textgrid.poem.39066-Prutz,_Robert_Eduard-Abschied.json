{"textgrid.poem.39066": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und mu\u00df es denn und mu\u00df es sein,", "tokens": ["Und", "mu\u00df", "es", "denn", "und", "mu\u00df", "es", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "KON", "VMFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und m\u00fcssen wir uns trennen \u2013", "tokens": ["und", "m\u00fcs\u00b7sen", "wir", "uns", "tren\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wohlan, so schenkt noch einmal ein", "tokens": ["Wo\u00b7hlan", ",", "so", "schenkt", "noch", "ein\u00b7mal", "ein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VVFIN", "ADV", "ADV", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und la\u00dft noch einmal zu dem Wein", "tokens": ["und", "la\u00dft", "noch", "ein\u00b7mal", "zu", "dem", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "die Herzen lodernd brennen!", "tokens": ["die", "Her\u00b7zen", "lo\u00b7dernd", "bren\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du gehst, o Freund, nicht tr\u00e4nenlos \u2013", "tokens": ["Du", "gehst", ",", "o", "Freund", ",", "nicht", "tr\u00e4\u00b7nen\u00b7los", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "FM", "NN", "$,", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O la\u00df sie, la\u00df sie rinnen!", "tokens": ["O", "la\u00df", "sie", ",", "la\u00df", "sie", "rin\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn ach, von deiner Mutter Scho\u00df,", "tokens": ["Denn", "ach", ",", "von", "dei\u00b7ner", "Mut\u00b7ter", "Scho\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "du rei\u00dft vom Vaterland dich los,", "tokens": ["du", "rei\u00dft", "vom", "Va\u00b7ter\u00b7land", "dich", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "ein neues zu gewinnen!", "tokens": ["ein", "neu\u00b7es", "zu", "ge\u00b7win\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Von fremder K\u00fcste, stolz und frei,", "tokens": ["Von", "frem\u00b7der", "K\u00fcs\u00b7te", ",", "stolz", "und", "frei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "die W\u00e4lder h\u00f6rst du rauschen;", "tokens": ["die", "W\u00e4l\u00b7der", "h\u00f6rst", "du", "rau\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "willst gegen seidne Sklaverei,", "tokens": ["willst", "ge\u00b7gen", "seid\u00b7ne", "Skla\u00b7ve\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "willst gegen bunte Liverei", "tokens": ["willst", "ge\u00b7gen", "bun\u00b7te", "Li\u00b7ve\u00b7rei"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ADJA", "NN"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.5": {"text": "die nackte Freiheit tauschen.", "tokens": ["die", "nack\u00b7te", "Frei\u00b7heit", "tau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du bist es satt, ein Knecht zu sein", "tokens": ["Du", "bist", "es", "satt", ",", "ein", "Knecht", "zu", "sein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,", "ART", "NN", "PTKZU", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und frei dich nur zu tr\u00e4umen,", "tokens": ["und", "frei", "dich", "nur", "zu", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "du bist es satt, mit Heuchelein,", "tokens": ["du", "bist", "es", "satt", ",", "mit", "Heu\u00b7che\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "mit goldner Worte Flitterschein", "tokens": ["mit", "gold\u00b7ner", "Wor\u00b7te", "Flit\u00b7ter\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "die Kette zu ums\u00e4umen.", "tokens": ["die", "Ket\u00b7te", "zu", "um\u00b7s\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du bist des eigenen Volkes satt,", "tokens": ["Du", "bist", "des", "ei\u00b7ge\u00b7nen", "Vol\u00b7kes", "satt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "der schmachgewohnten Seelen:", "tokens": ["der", "schmach\u00b7ge\u00b7wohn\u00b7ten", "See\u00b7len", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "des Volkes, das, zum Handeln matt,", "tokens": ["des", "Vol\u00b7kes", ",", "das", ",", "zum", "Han\u00b7deln", "matt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "$,", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gelehrte und nur Dichter hat,", "tokens": ["Ge\u00b7lehr\u00b7te", "und", "nur", "Dich\u00b7ter", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und dem die M\u00e4nner fehlen!", "tokens": ["und", "dem", "die", "M\u00e4n\u00b7ner", "feh\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Du wirst nicht gl\u00fccklich werden, nein,", "tokens": ["Du", "wirst", "nicht", "gl\u00fcck\u00b7lich", "wer\u00b7den", ",", "nein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "VAINF", "$,", "PTKANT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "auch nicht im freien Lande.", "tokens": ["auch", "nicht", "im", "frei\u00b7en", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch willst du lieber elend sein,", "tokens": ["Doch", "willst", "du", "lie\u00b7ber", "e\u00b7lend", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "im fremden Land, stumm und allein,", "tokens": ["im", "frem\u00b7den", "Land", ",", "stumm", "und", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "als Knecht im Vaterlande.", "tokens": ["als", "Knecht", "im", "Va\u00b7ter\u00b7lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O d\u00fcrften wir in deinem Lauf,", "tokens": ["O", "d\u00fcrf\u00b7ten", "wir", "in", "dei\u00b7nem", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "o d\u00fcrften wir dich halten!", "tokens": ["o", "d\u00fcrf\u00b7ten", "wir", "dich", "hal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und d\u00fcrften sagen: schau, hinauf!", "tokens": ["Und", "d\u00fcrf\u00b7ten", "sa\u00b7gen", ":", "schau", ",", "hin\u00b7auf", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VMFIN", "VVINF", "$.", "PTKVZ", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da steigt die Sonne schon herauf,", "tokens": ["Da", "steigt", "die", "Son\u00b7ne", "schon", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "der Tag will sich entfalten!", "tokens": ["der", "Tag", "will", "sich", "ent\u00b7fal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Umsonst! Noch s\u00e4umt das holde Licht,", "tokens": ["Um\u00b7sonst", "!", "Noch", "s\u00e4umt", "das", "hol\u00b7de", "Licht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "noch sind die Herzen bleiern,", "tokens": ["noch", "sind", "die", "Her\u00b7zen", "blei\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "noch r\u00fchren sich die Schl\u00e4fer nicht,", "tokens": ["noch", "r\u00fch\u00b7ren", "sich", "die", "Schl\u00e4\u00b7fer", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "noch ist das H\u00f6chste ein Gedicht,", "tokens": ["noch", "ist", "das", "H\u00f6chs\u00b7te", "ein", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "das die Poeten feiern!", "tokens": ["das", "die", "Po\u00b7et\u00b7en", "fei\u00b7ern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und doch, ihr Br\u00fcder, schenket ein!", "tokens": ["Und", "doch", ",", "ihr", "Br\u00fc\u00b7der", ",", "schen\u00b7ket", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch mu\u00df ein Morgen tagen,", "tokens": ["Doch", "mu\u00df", "ein", "Mor\u00b7gen", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "da bricht die Freiheit stolz herein,", "tokens": ["da", "bricht", "die", "Frei\u00b7heit", "stolz", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "da wird bei Ja, da wird bei Nein,", "tokens": ["da", "wird", "bei", "Ja", ",", "da", "wird", "bei", "Nein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PTKANT", "$,", "ADV", "VAFIN", "APPR", "PTKANT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "da wird das Joch zerschlagen!", "tokens": ["da", "wird", "das", "Joch", "zer\u00b7schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ein Tag, wie die Trompete klingt,", "tokens": ["Ein", "Tag", ",", "wie", "die", "Trom\u00b7pe\u00b7te", "klingt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "die M\u00e4nner anzuwerben!", "tokens": ["die", "M\u00e4n\u00b7ner", "an\u00b7zu\u00b7wer\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es kommt ein Tag, der, sturmbeschwingt,", "tokens": ["Es", "kommt", "ein", "Tag", ",", "der", ",", "sturm\u00b7be\u00b7schwingt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "zur\u00fcck in unsern Arm dich bringt,", "tokens": ["zu\u00b7r\u00fcck", "in", "un\u00b7sern", "Arm", "dich", "bringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "zu siegen und zu sterben!", "tokens": ["zu", "sie\u00b7gen", "und", "zu", "ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}