{"textgrid.poem.35815": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vater! Vater! willst Du mich nicht h\u00f6ren?", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vater! Vater! willst Du mich nicht h\u00f6ren?", "tokens": ["Va\u00b7ter", "!", "Va\u00b7ter", "!", "willst", "Du", "mich", "nicht", "h\u00f6\u00b7ren", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Soll mein schmerzlich Ringen ewig w\u00e4hren?", "tokens": ["Soll", "mein", "schmerz\u00b7lich", "Rin\u00b7gen", "e\u00b7wig", "w\u00e4h\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJD", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "La\u00df mich nicht in der Versuchung sterben;", "tokens": ["La\u00df", "mich", "nicht", "in", "der", "Ver\u00b7su\u00b7chung", "ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Siegen la\u00df mich und Dein Heil erwerben.", "tokens": ["Sie\u00b7gen", "la\u00df", "mich", "und", "Dein", "Heil", "er\u00b7wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVIMP", "PPER", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "O, mein Jesus! willst Du mich nicht kennen?", "tokens": ["O", ",", "mein", "Je\u00b7sus", "!", "willst", "Du", "mich", "nicht", "ken\u00b7nen", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NE", "$.", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Soll dies H\u00f6llenfeuer mich verbrennen?", "tokens": ["Soll", "dies", "H\u00f6l\u00b7len\u00b7feu\u00b7er", "mich", "ver\u00b7bren\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Jesus! Jesus! la\u00df mich nicht versinken;", "tokens": ["Je\u00b7sus", "!", "Je\u00b7sus", "!", "la\u00df", "mich", "nicht", "ver\u00b7sin\u00b7ken", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "$.", "VVIMP", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Durft' ich doch von Deinem Blute trinken.", "tokens": ["Durft'", "ich", "doch", "von", "Dei\u00b7nem", "Blu\u00b7te", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Heil'ger Geist! o, woll'st mich nicht verdammen,", "tokens": ["Heil'\u00b7ger", "Geist", "!", "o", ",", "woll'st", "mich", "nicht", "ver\u00b7dam\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "FM", "$,", "PWAV", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mich nicht werfen in die ew'gen Flammen!", "tokens": ["Mich", "nicht", "wer\u00b7fen", "in", "die", "ew'\u00b7gen", "Flam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Hast mir einst doch einen Blick gegeben;", "tokens": ["Hast", "mir", "einst", "doch", "ei\u00b7nen", "Blick", "ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "H\u00e4tt' ich sonst dem Herrn geweiht mein Leben?", "tokens": ["H\u00e4tt'", "ich", "sonst", "dem", "Herrn", "ge\u00b7weiht", "mein", "Le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Geist! erleuchte mir die rechten Stege!", "tokens": ["Geist", "!", "er\u00b7leuch\u00b7te", "mir", "die", "rech\u00b7ten", "Ste\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Leite mich, mein Fu\u00df ist irr' und tr\u00e4ge!", "tokens": ["Lei\u00b7te", "mich", ",", "mein", "Fu\u00df", "ist", "irr'", "und", "tr\u00e4\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ich verzage, komm, o s\u00fc\u00dfer Friede,", "tokens": ["Ich", "ver\u00b7za\u00b7ge", ",", "komm", ",", "o", "s\u00fc\u00b7\u00dfer", "Frie\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "FM", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Salbe mich, mein Haupt ist schlafesm\u00fcde.", "tokens": ["Sal\u00b7be", "mich", ",", "mein", "Haupt", "ist", "schla\u00b7fes\u00b7m\u00fc\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Vater! ach, Du wirst mich doch nicht richten?", "tokens": ["Va\u00b7ter", "!", "ach", ",", "Du", "wirst", "mich", "doch", "nicht", "rich\u00b7ten", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "XY", "$,", "PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Was Dein Hauch beseelte nicht vernichten? \u2013", "tokens": ["Was", "Dein", "Hauch", "be\u00b7seel\u00b7te", "nicht", "ver\u00b7nich\u00b7ten", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "PTKNEG", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Vater! es gescheh' Dein ew'ger Wille!", "tokens": ["Va\u00b7ter", "!", "es", "ge\u00b7scheh'", "Dein", "ew'\u00b7ger", "Wil\u00b7le", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Gieb, da\u00df ich mein Herz in Demuth stille.", "tokens": ["Gieb", ",", "da\u00df", "ich", "mein", "Herz", "in", "De\u00b7muth", "stil\u00b7le", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Milder Hirt! o, woll'st den Satan z\u00e4hmen;", "tokens": ["Mil\u00b7der", "Hirt", "!", "o", ",", "woll'st", "den", "Sa\u00b7tan", "z\u00e4h\u00b7men", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "FM", "$,", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dein ge\u00e4ngstet Lamm zur H\u00fcrde nehmen;", "tokens": ["Dein", "ge\u00b7\u00e4ngs\u00b7tet", "Lamm", "zur", "H\u00fcr\u00b7de", "neh\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVPP", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Tritt zum Vater hin mit Deinen Wunden,", "tokens": ["Tritt", "zum", "Va\u00b7ter", "hin", "mit", "Dei\u00b7nen", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sprich: \u00bbDie hab' Ich auch f\u00fcr sie empfunden.\u00ab", "tokens": ["Sprich", ":", "\u00bb", "Die", "hab'", "Ich", "auch", "f\u00fcr", "sie", "emp\u00b7fun\u00b7den", ".", "\u00ab"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "$.", "$(", "PDS", "VAFIN", "PPER", "ADV", "APPR", "PPER", "VVPP", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Ich bin blind; Dein Geist mu\u00df mich erleuchten.", "tokens": ["Ich", "bin", "blind", ";", "Dein", "Geist", "mu\u00df", "mich", "er\u00b7leuch\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$.", "PPOSAT", "NN", "VMFIN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ich bin d\u00fcrr; Dein Blut mu\u00df mich befeuchten.", "tokens": ["Ich", "bin", "d\u00fcrr", ";", "Dein", "Blut", "mu\u00df", "mich", "be\u00b7feuch\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$.", "PPOSAT", "NN", "VMFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ich bin todt; Dein Odem mu\u00df mir Leben,", "tokens": ["Ich", "bin", "todt", ";", "Dein", "O\u00b7dem", "mu\u00df", "mir", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$.", "PPOSAT", "NN", "VMFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Geist und Liebe, Kraft und Glauben geben.", "tokens": ["Geist", "und", "Lie\u00b7be", ",", "Kraft", "und", "Glau\u00b7ben", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}