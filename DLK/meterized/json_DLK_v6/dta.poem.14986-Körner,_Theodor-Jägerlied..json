{"dta.poem.14986": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "J\u00e4gerlied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1814", "urn": "urn:nbn:de:kobv:b4-200905193258", "language": ["de:0.99"], "booktitle": "K\u00f6rner, Theodor: Leyer und Schwerdt. Berlin, 1814."}, "poem": {"stanza.1": {"line.1": {"text": "Die B\u00fcchse von der Wand!", "tokens": ["Die", "B\u00fcch\u00b7se", "von", "der", "Wand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Muthige bek\u00e4mpft die Welt!", "tokens": ["Der", "Mut\u00b7hi\u00b7ge", "be\u00b7k\u00e4mpft", "die", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Frisch auf den Feind! Frisch in das Feld!", "tokens": ["Frisch", "auf", "den", "Feind", "!", "Frisch", "in", "das", "Feld", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$.", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcrs deutsche Vaterland!", "tokens": ["F\u00fcrs", "deut\u00b7sche", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Aus Westen, Norden, S\u00fcd und Ost", "tokens": ["Aus", "Wes\u00b7ten", ",", "Nor\u00b7den", ",", "S\u00fcd", "und", "Ost"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Treibt uns der Rache Strahl.", "tokens": ["Treibt", "uns", "der", "Ra\u00b7che", "Strahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom Oderflusse, Weser, Main,", "tokens": ["Vom", "O\u00b7der\u00b7flus\u00b7se", ",", "We\u00b7ser", ",", "Main", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "NN", "$,", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Elbstrom und vom Vater Rhein", "tokens": ["Vom", "E\u00b7lbstrom", "und", "vom", "Va\u00b7ter", "Rhein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "NE"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und aus dem Donauthal.", "tokens": ["Und", "aus", "dem", "Do\u00b7naut\u00b7hal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch Br\u00fcder sind wir allzusammen,", "tokens": ["Doch", "Br\u00fc\u00b7der", "sind", "wir", "all\u00b7zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und das schwellt unsern Muth.", "tokens": ["Und", "das", "schwellt", "un\u00b7sern", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Uns kn\u00fcpft der Sprache heilig Band,", "tokens": ["Uns", "kn\u00fcpft", "der", "Spra\u00b7che", "hei\u00b7lig", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns kn\u00fcpft ", "tokens": ["Uns", "kn\u00fcpft"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+", "measure": "iambic.single"}}, "stanza.4": {"line.1": {"text": "Nicht zum Erobern zogen wir", "tokens": ["Nicht", "zum", "Er\u00b7o\u00b7bern", "zo\u00b7gen", "wir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPRART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom v\u00e4terlichen Heerd,", "tokens": ["Vom", "v\u00e4\u00b7ter\u00b7li\u00b7chen", "Heerd", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die sch\u00e4ndlichste Tyrannenmacht", "tokens": ["Die", "sch\u00e4nd\u00b7lichs\u00b7te", "Ty\u00b7ran\u00b7nen\u00b7macht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bek\u00e4mpfen wir in freud'ger Schlacht,", "tokens": ["Be\u00b7k\u00e4mp\u00b7fen", "wir", "in", "freu\u00b7d'\u00b7ger", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Das ist des Blutes werth.", "tokens": ["Das", "ist", "des", "Blu\u00b7tes", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ihr aber, die uns treu geliebt,", "tokens": ["Ihr", "a\u00b7ber", ",", "die", "uns", "treu", "ge\u00b7liebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Herr sey euer Schild,", "tokens": ["Der", "Herr", "sey", "eu\u00b7er", "Schild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bezahlen wir's mit unserm Blut,", "tokens": ["Be\u00b7zah\u00b7len", "wir's", "mit", "un\u00b7serm", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn Freiheit ist das h\u00f6chste Gut,", "tokens": ["Denn", "Frei\u00b7heit", "ist", "das", "h\u00f6chs\u00b7te", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ob's tausend Leben gilt.", "tokens": ["Ob's", "tau\u00b7send", "Le\u00b7ben", "gilt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Drum muntre J\u00e4ger, frei und flink,", "tokens": ["Drum", "mun\u00b7tre", "J\u00e4\u00b7ger", ",", "frei", "und", "flink", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie auch das Liebchen weint,", "tokens": ["Wie", "auch", "das", "Lieb\u00b7chen", "weint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gott hilft uns im gerechten Krieg!", "tokens": ["Gott", "hilft", "uns", "im", "ge\u00b7rech\u00b7ten", "Krieg", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Frisch in den Kampf! \u2014 Tod oder Sieg!", "tokens": ["Frisch", "in", "den", "Kampf", "!", "Tod", "o\u00b7der", "Sieg", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$.", "$(", "NN", "KON", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Frisch, Br\u00fcder, auf den Feind!", "tokens": ["Frisch", ",", "Br\u00fc\u00b7der", ",", "auf", "den", "Feind", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}