{"textgrid.poem.55218": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Neologen", "genre": "verse", "period": "N.A.", "pub_year": 1808, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich begegnet einem jungen Mann,", "tokens": ["Ich", "be\u00b7geg\u00b7net", "ei\u00b7nem", "jun\u00b7gen", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ich fragt ihn um sein Gewerbe;", "tokens": ["Ich", "fragt", "ihn", "um", "sein", "Ge\u00b7wer\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er sagt': \u00bbIch sorge, wie ich kann,", "tokens": ["Er", "sagt'", ":", "\u00bb", "Ich", "sor\u00b7ge", ",", "wie", "ich", "kann", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da\u00df ich mir, eh ich sterbe,", "tokens": ["Da\u00df", "ich", "mir", ",", "eh", "ich", "ster\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Bauerg\u00fctchen erwerbe.\u00ab", "tokens": ["Ein", "Bau\u00b7er\u00b7g\u00fct\u00b7chen", "er\u00b7wer\u00b7be", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich sagte: \u00bbDas ist sehr wohl gedacht\u00ab;", "tokens": ["Ich", "sag\u00b7te", ":", "\u00bb", "Das", "ist", "sehr", "wohl", "ge\u00b7dacht", "\u00ab", ";"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PDS", "VAFIN", "ADV", "ADV", "VVPP", "$(", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und w\u00fcnschte, er h\u00e4tt es so weit gebracht.", "tokens": ["Und", "w\u00fcnschte", ",", "er", "h\u00e4tt", "es", "so", "weit", "ge\u00b7bracht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Da h\u00f6rt ich, er habe vom lieben Papa", "tokens": ["Da", "h\u00f6rt", "ich", ",", "er", "ha\u00b7be", "vom", "lie\u00b7ben", "Pa\u00b7pa"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und ebenso von der Frau Mama", "tokens": ["Und", "e\u00b7ben\u00b7so", "von", "der", "Frau", "Ma\u00b7ma"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Die allersch\u00f6nsten Ritterg\u00fcter.", "tokens": ["Die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Rit\u00b7ter\u00b7g\u00fc\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das nenn ich doch originale Gem\u00fcter.", "tokens": ["Das", "nenn", "ich", "doch", "o\u00b7rig\u00b7i\u00b7na\u00b7le", "Ge\u00b7m\u00fc\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}