{"textgrid.poem.48137": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Santa Madonna di Capri", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein grauer Nebel flattert durch die Luft,", "tokens": ["Ein", "grau\u00b7er", "Ne\u00b7bel", "flat\u00b7tert", "durch", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "auf Schattenarmen tr\u00e4gt er Myrtenduft,", "tokens": ["auf", "Schat\u00b7ten\u00b7ar\u00b7men", "tr\u00e4gt", "er", "Myr\u00b7ten\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und allen Wohlgeruch der Inselau", "tokens": ["und", "al\u00b7len", "Wohl\u00b7ge\u00b7ruch", "der", "In\u00b7se\u00b7lau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "streut er zu F\u00fc\u00dfen unsrer lieben Frau", "tokens": ["streut", "er", "zu", "F\u00fc\u00b7\u00dfen", "uns\u00b7rer", "lie\u00b7ben", "Frau"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Santa Madonna di Capri!", "tokens": ["San\u00b7ta", "Ma\u00b7don\u00b7na", "di", "Cap\u00b7ri", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Scheu durch den Nebel schleicht sich, schlangengleich,", "tokens": ["Scheu", "durch", "den", "Ne\u00b7bel", "schleicht", "sich", ",", "schlan\u00b7gen\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "PRF", "$,", "ADJD", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "ein Wallfahrtszug. Dem Bilde, s\u00fc\u00df und bleich,", "tokens": ["ein", "Wall\u00b7fahrts\u00b7zug", ".", "Dem", "Bil\u00b7de", ",", "s\u00fc\u00df", "und", "bleich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "vom Licht umkr\u00e4nzt, von jedem Makel frei,", "tokens": ["vom", "Licht", "um\u00b7kr\u00e4nzt", ",", "von", "je\u00b7dem", "Ma\u00b7kel", "frei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "dem Bild dort oben t\u00f6nt die Litanei:", "tokens": ["dem", "Bild", "dort", "o\u00b7ben", "t\u00f6nt", "die", "Li\u00b7ta\u00b7nei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Santa Maria di Capri!", "tokens": ["San\u00b7ta", "Ma\u00b7ria", "di", "Cap\u00b7ri", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dem Stern der Meere! Und ein Priester hebt", "tokens": ["Dem", "Stern", "der", "Mee\u00b7re", "!", "Und", "ein", "Pries\u00b7ter", "hebt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$.", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "die blasse Hand. Auf allen Lippen bebt", "tokens": ["die", "blas\u00b7se", "Hand", ".", "Auf", "al\u00b7len", "Lip\u00b7pen", "bebt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "APPR", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "ein frommes Lied. Die Stirnen neigen sich . . .", "tokens": ["ein", "from\u00b7mes", "Lied", ".", "Die", "Stir\u00b7nen", "nei\u00b7gen", "sich", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "NN", "VVFIN", "PRF", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "ich fahr vor\u00fcber und gr\u00fc\u00dfe dich,", "tokens": ["ich", "fahr", "vor\u00b7\u00fc\u00b7ber", "und", "gr\u00fc\u00b7\u00dfe", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Santa Maria di Capri!", "tokens": ["San\u00b7ta", "Ma\u00b7ria", "di", "Cap\u00b7ri", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du heilige Frau, du Weltentragende,", "tokens": ["Du", "hei\u00b7li\u00b7ge", "Frau", ",", "du", "Wel\u00b7ten\u00b7tra\u00b7gen\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+--+-+-+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Erl\u00f6sende und Nieversagende,", "tokens": ["Er\u00b7l\u00f6\u00b7sen\u00b7de", "und", "Nie\u00b7ver\u00b7sa\u00b7gen\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Tausende beten \u2013, und dich kennt allein", "tokens": ["Tau\u00b7sen\u00b7de", "be\u00b7ten", "\u2013", ",", "und", "dich", "kennt", "al\u00b7lein"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "$(", "$,", "KON", "PPER", "VVFIN", "ADV"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "mein Herz, die Rose und der Sonnenschein,", "tokens": ["mein", "Herz", ",", "die", "Ro\u00b7se", "und", "der", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Santa Maria di Capri!", "tokens": ["San\u00b7ta", "Ma\u00b7ria", "di", "Cap\u00b7ri", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}