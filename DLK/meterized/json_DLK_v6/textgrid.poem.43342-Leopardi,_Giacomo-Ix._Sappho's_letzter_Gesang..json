{"textgrid.poem.43342": {"metadata": {"author": {"name": "Leopardi, Giacomo", "birth": "N.A.", "death": "N.A."}, "title": "Ix. Sappho's letzter Gesang.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du sanfte Nacht und du, versch\u00e4mter Strahl", "tokens": ["Du", "sanf\u00b7te", "Nacht", "und", "du", ",", "ver\u00b7sch\u00e4m\u00b7ter", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJA", "NN", "KON", "PPER", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des sp\u00e4ten Monds, und du dort \u00fcberm Felsen", "tokens": ["Des", "sp\u00e4\u00b7ten", "Monds", ",", "und", "du", "dort", "\u00fc\u00b7berm", "Fel\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aufgl\u00e4nzend aus des Waldes stummen Wipfeln,", "tokens": ["Auf\u00b7gl\u00e4n\u00b7zend", "aus", "des", "Wal\u00b7des", "stum\u00b7men", "Wip\u00b7feln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du Tagesbote, die ihr meinen Augen,", "tokens": ["Du", "Ta\u00b7ges\u00b7bo\u00b7te", ",", "die", "ihr", "mei\u00b7nen", "Au\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Eh' ich das Schicksal kannt' und die Erinnys,", "tokens": ["Eh'", "ich", "das", "Schick\u00b7sal", "kannt'", "und", "die", "E\u00b7rin\u00b7nys", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VMFIN", "KON", "ART", "NE", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "So lieb und hold erschient: nun tr\u00f6stet nimmer", "tokens": ["So", "lieb", "und", "hold", "er\u00b7schient", ":", "nun", "tr\u00f6s\u00b7tet", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVPP", "$.", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ein wonnig Schauspiel mein verzweifelnd Herz!", "tokens": ["Ein", "won\u00b7nig", "Schau\u00b7spiel", "mein", "ver\u00b7zwei\u00b7felnd", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PPOSAT", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nur dann belebt mich langentw\u00f6hnte Freude,", "tokens": ["Nur", "dann", "be\u00b7lebt", "mich", "lan\u00b7gent\u00b7w\u00f6hn\u00b7te", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wenn durch den Aether schwimmend und die Fluren,", "tokens": ["Wenn", "durch", "den", "A\u00b7e\u00b7ther", "schwim\u00b7mend", "und", "die", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJD", "KON", "ART", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Die bang erzittern, sich der Strom des S\u00fcdwinds", "tokens": ["Die", "bang", "er\u00b7zit\u00b7tern", ",", "sich", "der", "Strom", "des", "S\u00fcd\u00b7winds"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVINF", "$,", "PRF", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Mit Wogen Staubes w\u00e4lzt, und wenn der Wagen,", "tokens": ["Mit", "Wo\u00b7gen", "Stau\u00b7bes", "w\u00e4lzt", ",", "und", "wenn", "der", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "$,", "KON", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Zeus' schwerer Wagen \u00fcber unsern H\u00e4uptern", "tokens": ["Zeus'", "schwe\u00b7rer", "Wa\u00b7gen", "\u00fc\u00b7ber", "un\u00b7sern", "H\u00e4up\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Hindonnernd durch die finstern L\u00fcfte f\u00e4hrt.", "tokens": ["Hin\u00b7don\u00b7nernd", "durch", "die", "fins\u00b7tern", "L\u00fcf\u00b7te", "f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Durch Klippen nur und tiefe Kl\u00fcfte m\u00f6cht' ich", "tokens": ["Durch", "Klip\u00b7pen", "nur", "und", "tie\u00b7fe", "Kl\u00fcf\u00b7te", "m\u00f6cht'", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "KON", "ADJA", "NN", "VMFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "In Wetterwolken wandeln; mich erg\u00f6tzt", "tokens": ["In", "Wet\u00b7ter\u00b7wol\u00b7ken", "wan\u00b7deln", ";", "mich", "er\u00b7g\u00f6tzt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "$.", "PPER", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Erschreckter Heerden Flucht, das dumpfe Brausen", "tokens": ["Er\u00b7schreck\u00b7ter", "Heer\u00b7den", "Flucht", ",", "das", "dump\u00b7fe", "Brau\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Der hochgeschwellten Flut", "tokens": ["Der", "hoch\u00b7ge\u00b7schwell\u00b7ten", "Flut"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Am schwanken Ufer und der Wellen Wuth.", "tokens": ["Am", "schwan\u00b7ken", "U\u00b7fer", "und", "der", "Wel\u00b7len", "Wuth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6n ist dein Kleid, erhabner Himmel; sch\u00f6n", "tokens": ["Sch\u00f6n", "ist", "dein", "Kleid", ",", "er\u00b7hab\u00b7ner", "Him\u00b7mel", ";", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$,", "ADJA", "NN", "$.", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bist du, thaufrische Erde. Ach, von aller", "tokens": ["Bist", "du", ",", "thauf\u00b7ri\u00b7sche", "Er\u00b7de", ".", "Ach", ",", "von", "al\u00b7ler"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "ADJA", "NN", "$.", "ITJ", "$,", "APPR", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Endlosen Sch\u00f6ne nicht den kleinsten Theil", "tokens": ["End\u00b7lo\u00b7sen", "Sch\u00f6\u00b7ne", "nicht", "den", "kleins\u00b7ten", "Theil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "PTKNEG", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Verliehn die G\u00f6tter und das t\u00fcckische Schicksal", "tokens": ["Ver\u00b7liehn", "die", "G\u00f6t\u00b7ter", "und", "das", "t\u00fc\u00b7cki\u00b7sche", "Schick\u00b7sal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-++-+-", "measure": "unknown.measure.hexa"}, "line.5": {"text": "Der armen Sappho. Ein verachteter", "tokens": ["Der", "ar\u00b7men", "Sap\u00b7pho", ".", "Ein", "ver\u00b7ach\u00b7te\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NE", "$.", "ART", "ADJA"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und l\u00e4st'ger Gast in deinem stolzen Reiche,", "tokens": ["Und", "l\u00e4st'\u00b7ger", "Gast", "in", "dei\u00b7nem", "stol\u00b7zen", "Rei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Natur, hebt die verschm\u00e4hte Liebende", "tokens": ["Na\u00b7tur", ",", "hebt", "die", "ver\u00b7schm\u00e4h\u00b7te", "Lie\u00b7ben\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Umsonst zu deinen Reizen Herz und Augen", "tokens": ["Um\u00b7sonst", "zu", "dei\u00b7nen", "Rei\u00b7zen", "Herz", "und", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Um H\u00fclfe flehend auf. Mir lacht nicht mehr", "tokens": ["Um", "H\u00fcl\u00b7fe", "fle\u00b7hend", "auf", ".", "Mir", "lacht", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "NN", "VVPP", "PTKVZ", "$.", "PPER", "VVFIN", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der sonnige Strand, der morgendliche Glanz", "tokens": ["Der", "son\u00b7ni\u00b7ge", "Strand", ",", "der", "mor\u00b7gend\u00b7li\u00b7che", "Glanz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Am Himmelsthor; mich gr\u00fc\u00dft nicht der Gesang", "tokens": ["Am", "Him\u00b7mel\u00b7sthor", ";", "mich", "gr\u00fc\u00dft", "nicht", "der", "Ge\u00b7sang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$.", "PPER", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Der buntgefiederten V\u00f6gel, nicht das Rauschen", "tokens": ["Der", "bunt\u00b7ge\u00b7fie\u00b7der\u00b7ten", "V\u00f6\u00b7gel", ",", "nicht", "das", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PTKNEG", "ART", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Der Buchenwipfel; und wo unterm Schatten", "tokens": ["Der", "Bu\u00b7chen\u00b7wip\u00b7fel", ";", "und", "wo", "un\u00b7term", "Schat\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "KON", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Der H\u00e4ngeweiden seinen reinen Schoo\u00df", "tokens": ["Der", "H\u00e4n\u00b7ge\u00b7wei\u00b7den", "sei\u00b7nen", "rei\u00b7nen", "Schoo\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Der klare Bach erschlie\u00dft, entzieht er meinem", "tokens": ["Der", "kla\u00b7re", "Bach", "er\u00b7schlie\u00dft", ",", "ent\u00b7zieht", "er", "mei\u00b7nem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "PPER", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Unsichern Fu\u00dfe die geschmeid'gen Wellen,", "tokens": ["Un\u00b7si\u00b7chern", "Fu\u00b7\u00dfe", "die", "ge\u00b7schmei\u00b7d'\u00b7gen", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.17": {"text": "Als w\u00e4r' ich ihm verha\u00dft,", "tokens": ["Als", "w\u00e4r'", "ich", "ihm", "ver\u00b7ha\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Und flieht am bl\u00fch'nden Ufer hin in Hast.", "tokens": ["Und", "flieht", "am", "bl\u00fch'n\u00b7den", "U\u00b7fer", "hin", "in", "Hast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "ADV", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Welch ein Vergehn, welch arge Missethat", "tokens": ["Welch", "ein", "Ver\u00b7gehn", ",", "welch", "ar\u00b7ge", "Mis\u00b7se\u00b7that"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ART", "NN", "$,", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hat mich befleckt vor der Geburt, da\u00df mich", "tokens": ["Hat", "mich", "be\u00b7fleckt", "vor", "der", "Ge\u00b7burt", ",", "da\u00df", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Himmel und das Gl\u00fcck so finster ansehn?", "tokens": ["Der", "Him\u00b7mel", "und", "das", "Gl\u00fcck", "so", "fins\u00b7ter", "an\u00b7sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Was frevelt' ich als Kind schon, wo das Leben", "tokens": ["Was", "fre\u00b7velt'", "ich", "als", "Kind", "schon", ",", "wo", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "KOUS", "NN", "ADV", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Noch Nichts von S\u00fcnde wei\u00df, da\u00df so beraubt", "tokens": ["Noch", "Nichts", "von", "S\u00fcn\u00b7de", "wei\u00df", ",", "da\u00df", "so", "be\u00b7raubt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PIS", "APPR", "NN", "VVFIN", "$,", "KOUS", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Jugend, so entbl\u00e4ttert durch die Spindel", "tokens": ["Der", "Ju\u00b7gend", ",", "so", "ent\u00b7bl\u00e4t\u00b7tert", "durch", "die", "Spin\u00b7del"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der unerbittlichen Parze, meine Bl\u00fcte", "tokens": ["Der", "un\u00b7er\u00b7bitt\u00b7li\u00b7chen", "Par\u00b7ze", ",", "mei\u00b7ne", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Verdorren mu\u00df? Ach, unbedachte Worte", "tokens": ["Ver\u00b7dor\u00b7ren", "mu\u00df", "?", "Ach", ",", "un\u00b7be\u00b7dach\u00b7te", "Wor\u00b7te"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "VMFIN", "$.", "ITJ", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Spricht deine Lippe! Unsre Loose lenkt", "tokens": ["Spricht", "dei\u00b7ne", "Lip\u00b7pe", "!", "Uns\u00b7re", "Loo\u00b7se", "lenkt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Geheimer Schicksalsschlu\u00df. Geheim ist Alles,", "tokens": ["Ge\u00b7hei\u00b7mer", "Schick\u00b7sals\u00b7schlu\u00df", ".", "Ge\u00b7heim", "ist", "Al\u00b7les", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NN", "VAFIN", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Nur unser Schmerz nicht. Ausgesetzte Kinder,", "tokens": ["Nur", "un\u00b7ser", "Schmerz", "nicht", ".", "Aus\u00b7ge\u00b7setz\u00b7te", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKNEG", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Zum Weinen nur geboren; das Warum", "tokens": ["Zum", "Wei\u00b7nen", "nur", "ge\u00b7bo\u00b7ren", ";", "das", "Wa\u00b7rum"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VVPP", "$.", "ART", "NN"], "meter": "-+-+