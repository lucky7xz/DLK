{"textgrid.poem.30490": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Buch k\u00fcmmet her vom Bug und Bogen von dem b\u00fcgen,", "tokens": ["Buch", "k\u00fcm\u00b7met", "her", "vom", "Bug", "und", "Bo\u00b7gen", "von", "dem", "b\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPRART", "NN", "KON", "NN", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann sie man in ein Buch zusammen pflegt zu f\u00fcgen.", "tokens": ["Wann", "sie", "man", "in", "ein", "Buch", "zu\u00b7sam\u00b7men", "pflegt", "zu", "f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIS", "APPR", "ART", "NN", "ADV", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bey Klugen, du mein Buch, thu willig einen Bug", "tokens": ["Bey", "Klu\u00b7gen", ",", "du", "mein", "Buch", ",", "thu", "wil\u00b7lig", "ei\u00b7nen", "Bug"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PPER", "PPOSAT", "NN", "$,", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und bitte sie um Gunst f\u00fcr das, was nicht hat Fug;", "tokens": ["Und", "bit\u00b7te", "sie", "um", "Gunst", "f\u00fcr", "das", ",", "was", "nicht", "hat", "Fug", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "APPR", "PDS", "$,", "PRELS", "PTKNEG", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "F\u00fcr denen beug dich nicht, die von den stoltzen Winden", "tokens": ["F\u00fcr", "de\u00b7nen", "beug", "dich", "nicht", ",", "die", "von", "den", "stolt\u00b7zen", "Win\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDS", "VVFIN", "PPER", "PTKNEG", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der groben Dunckeley sich strotz- und trotzig finden.", "tokens": ["Der", "gro\u00b7ben", "Dun\u00b7cke\u00b7ley", "sich", "strotz", "und", "trot\u00b7zig", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "TRUNC", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}