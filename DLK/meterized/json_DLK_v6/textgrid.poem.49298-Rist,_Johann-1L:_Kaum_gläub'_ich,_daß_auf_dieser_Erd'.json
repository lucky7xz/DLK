{"textgrid.poem.49298": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kaum gl\u00e4ub' ich, da\u00df auf dieser Erd'", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kaum gl\u00e4ub' ich, da\u00df auf dieser Erd'", "tokens": ["Kaum", "gl\u00e4ub'", "ich", ",", "da\u00df", "auf", "die\u00b7ser", "Erd'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein h\u00f6her Lob gegeben werd'", "tokens": ["Ein", "h\u00f6\u00b7her", "Lob", "ge\u00b7ge\u00b7ben", "werd'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An allem Ort' und Enden,", "tokens": ["An", "al\u00b7lem", "Ort'", "und", "En\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als denen, die mit Hand und Mund", "tokens": ["Als", "de\u00b7nen", ",", "die", "mit", "Hand", "und", "Mund"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDS", "$,", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Himmels Gaben machen kund,", "tokens": ["Des", "Him\u00b7mels", "Ga\u00b7ben", "ma\u00b7chen", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ja Lehr' und Tugend senden", "tokens": ["Ja", "Lehr'", "und", "Tu\u00b7gend", "sen\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "In manches Herz, das dieser Zeit", "tokens": ["In", "man\u00b7ches", "Herz", ",", "das", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sich sondert von der Eitelkeit.", "tokens": ["Sich", "son\u00b7dert", "von", "der", "Ei\u00b7tel\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Poeten mein' ich, werter Freund,", "tokens": ["Po\u00b7et\u00b7en", "mein'", "ich", ",", "wer\u00b7ter", "Freund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Poeten, welchen niemand feind", "tokens": ["Po\u00b7et\u00b7en", ",", "wel\u00b7chen", "nie\u00b7mand", "feind"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWAT", "PIS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Als Leute, die nichts wi\u00dfen;", "tokens": ["Als", "Leu\u00b7te", ",", "die", "nichts", "wi\u00b7\u00dfen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Die nur sind Schlaven dieser Welt,", "tokens": ["Die", "nur", "sind", "Schla\u00b7ven", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ja Tag und Nacht das blo\u00dfe Geld", "tokens": ["Ja", "Tag", "und", "Nacht", "das", "blo\u00b7\u00dfe", "Geld"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "NN", "KON", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Zu samlen sind gefli\u00dfen.", "tokens": ["Zu", "sam\u00b7len", "sind", "ge\u00b7fli\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Bei solchem Stank und Lasterschaum", "tokens": ["Bei", "sol\u00b7chem", "Stank", "und", "Las\u00b7ter\u00b7schaum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Hat selten ein Poete Raum.", "tokens": ["Hat", "sel\u00b7ten", "ein", "Poe\u00b7te", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Ein edler Geist, der h\u00f6her zielt,", "tokens": ["Ein", "ed\u00b7ler", "Geist", ",", "der", "h\u00f6\u00b7her", "zielt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Ein Geist, der Feur und Himmel f\u00fchlt,", "tokens": ["Ein", "Geist", ",", "der", "Feur", "und", "Him\u00b7mel", "f\u00fchlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Ist inniglich gewogen", "tokens": ["Ist", "in\u00b7nig\u00b7lich", "ge\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Der hochgelahrten Tichter Schar,", "tokens": ["Der", "hoch\u00b7ge\u00b7lahr\u00b7ten", "Tich\u00b7ter", "Schar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Von welchen nimmermehr f\u00fcrwahr", "tokens": ["Von", "wel\u00b7chen", "nim\u00b7mer\u00b7mehr", "f\u00fcr\u00b7wahr"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Ein Frommer wird betrogen;", "tokens": ["Ein", "From\u00b7mer", "wird", "be\u00b7tro\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Da samlet sich zu ieder Frist", "tokens": ["Da", "sam\u00b7let", "sich", "zu", "ie\u00b7der", "Frist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Was hungrig nach der Weisheit ist.", "tokens": ["Was", "hung\u00b7rig", "nach", "der", "Weis\u00b7heit", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Wenn lobet Gott ein reiner Mund,", "tokens": ["Wenn", "lo\u00b7bet", "Gott", "ein", "rei\u00b7ner", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Wer ehret ihn aus Herzengrund?", "tokens": ["Wer", "eh\u00b7ret", "ihn", "aus", "Her\u00b7zen\u00b7grund", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Ich mein', es thun Poeten.", "tokens": ["Ich", "mein'", ",", "es", "thun", "Po\u00b7et\u00b7en", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVINF", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Wer r\u00fchmet Gottes Wunderthat,", "tokens": ["Wer", "r\u00fch\u00b7met", "Got\u00b7tes", "Wun\u00b7der\u00b7that", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Im Fall er ihn erl\u00f6set hat", "tokens": ["Im", "Fall", "er", "ihn", "er\u00b7l\u00f6\u00b7set", "hat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPER", "PPER", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Aus gro\u00dfer Angst und N\u00f6ten?", "tokens": ["Aus", "gro\u00b7\u00dfer", "Angst", "und", "N\u00f6\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Wer singet Gott ein Liedelein?", "tokens": ["Wer", "sin\u00b7get", "Gott", "ein", "Lie\u00b7de\u00b7lein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Ich sage, da\u00df es Tichter sein.", "tokens": ["Ich", "sa\u00b7ge", ",", "da\u00df", "es", "Tich\u00b7ter", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Wer w\u00fcste von den Helden doch", "tokens": ["Wer", "w\u00fcs\u00b7te", "von", "den", "Hel\u00b7den", "doch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Ein einzigs Wort zu sagen noch,", "tokens": ["Ein", "ein\u00b7zigs", "Wort", "zu", "sa\u00b7gen", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Welch' Ilium bezwungen,", "tokens": ["Welch'", "I\u00b7lium", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.36": {"text": "Wenn der Poeten Haubt und Licht,", "tokens": ["Wenn", "der", "Po\u00b7et\u00b7en", "Haubt", "und", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Homerus, ihre Thaten nicht", "tokens": ["Ho\u00b7me\u00b7rus", ",", "ih\u00b7re", "Tha\u00b7ten", "nicht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "NN", "PTKNEG"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.38": {"text": "Der Nachwelt vorgesungen?", "tokens": ["Der", "Nach\u00b7welt", "vor\u00b7ge\u00b7sun\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Ein hochbegabter Tichter schreibt", "tokens": ["Ein", "hoch\u00b7be\u00b7gab\u00b7ter", "Tich\u00b7ter", "schreibt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Ein Werk, das nach dem Tode bleibt.", "tokens": ["Ein", "Werk", ",", "das", "nach", "dem", "To\u00b7de", "bleibt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Poeten k\u00f6nnen Herz und Sinn", "tokens": ["Po\u00b7et\u00b7en", "k\u00f6n\u00b7nen", "Herz", "und", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Durch ihre Kunst zum Trauren hin,", "tokens": ["Durch", "ih\u00b7re", "Kunst", "zum", "Trau\u00b7ren", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Wenn sie nur wollen, bringen;", "tokens": ["Wenn", "sie", "nur", "wol\u00b7len", ",", "brin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.44": {"text": "Sie k\u00f6nnen wiedrum schweres Leid", "tokens": ["Sie", "k\u00f6n\u00b7nen", "wie\u00b7drum", "schwe\u00b7res", "Leid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Verkehren bald in lauter Freud'", "tokens": ["Ver\u00b7keh\u00b7ren", "bald", "in", "lau\u00b7ter", "Freud'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Und solches durch ihr Singen.", "tokens": ["Und", "sol\u00b7ches", "durch", "ihr", "Sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.47": {"text": "Was Menschen Augen je gesehn,", "tokens": ["Was", "Men\u00b7schen", "Au\u00b7gen", "je", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Mu\u00df ihnen schnell zu Dienste stehn.", "tokens": ["Mu\u00df", "ih\u00b7nen", "schnell", "zu", "Diens\u00b7te", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Dafern nur ein Poete wil,", "tokens": ["Da\u00b7fern", "nur", "ein", "Poe\u00b7te", "wil", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ART", "NN", "VMFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.50": {"text": "So steht der Himmel nimmer stil,", "tokens": ["So", "steht", "der", "Him\u00b7mel", "nim\u00b7mer", "stil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Die Sterne m\u00fcssen tanzen;", "tokens": ["Die", "Ster\u00b7ne", "m\u00fcs\u00b7sen", "tan\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.52": {"text": "Es springen auch die Stein' herf\u00fcr,", "tokens": ["Es", "sprin\u00b7gen", "auch", "die", "Stein'", "her\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Da h\u00fcpfen W\u00e4lder, Berg' und Thier',", "tokens": ["Da", "h\u00fcp\u00b7fen", "W\u00e4l\u00b7der", ",", "Ber\u00b7g'", "und", "Thier'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.54": {"text": "Es zittern W\u00e4ll' und Schanzen;", "tokens": ["Es", "zit\u00b7tern", "W\u00e4ll'", "und", "Schan\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.55": {"text": "Ja, was die schwarze Nacht bedeckt,", "tokens": ["Ja", ",", "was", "die", "schwar\u00b7ze", "Nacht", "be\u00b7deckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.56": {"text": "Wird durch Poeten aufgeweckt.", "tokens": ["Wird", "durch", "Po\u00b7et\u00b7en", "auf\u00b7ge\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.57": {"text": "Herr Klaius, tretet doch herbei,", "tokens": ["Herr", "Klai\u00b7us", ",", "tre\u00b7tet", "doch", "her\u00b7bei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.58": {"text": "Durchleset die\u00df und saget frei,", "tokens": ["Durch\u00b7le\u00b7set", "die\u00df", "und", "sa\u00b7get", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.59": {"text": "Ob ich die Wahrheit 