{"textgrid.poem.67738": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u2013 In heil'gen Bildern \u00e4tztest Du ihr ein:", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u2013 In heil'gen Bildern \u00e4tztest Du ihr ein:", "tokens": ["\u2013", "In", "heil'\u00b7gen", "Bil\u00b7dern", "\u00e4tz\u00b7test", "Du", "ihr", "ein", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ADJA", "NN", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbich war und bin und werde sein.\u00ab", "tokens": ["\u00bb", "ich", "war", "und", "bin", "und", "wer\u00b7de", "sein", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "KON", "VAFIN", "KON", "VAFIN", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verkn\u00fcpft Euch, Reiche, es zu durchsto\u00dfen!", "tokens": ["Ver\u00b7kn\u00fcpft", "Euch", ",", "Rei\u00b7che", ",", "es", "zu", "durch\u00b7sto\u00b7\u00dfen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NE", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Denn nachzubaun \u2013 vergebens!", "tokens": ["Denn", "nach\u00b7zu\u00b7baun", "\u2013", "ver\u00b7ge\u00b7bens", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "VVINF", "$(", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich halt's, mein Werk; und seine Todten,", "tokens": ["Ich", "halt's", ",", "mein", "Werk", ";", "und", "sei\u00b7ne", "Tod\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "$.", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie athmen mir zu Fu\u00df, und jeder Schatten k\u00e4mpft.", "tokens": ["Sie", "ath\u00b7men", "mir", "zu", "Fu\u00df", ",", "und", "je\u00b7der", "Schat\u00b7ten", "k\u00e4mpft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$,", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die W\u00fcrmer weg. Ich salbte sie \u2013 \u2020\u2020", "tokens": ["Die", "W\u00fcr\u00b7mer", "weg", ".", "Ich", "salb\u00b7te", "sie", "\u2013", "\u2020\u2020"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "$(", "XY"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "\u2020\u2020\u2020 ihn zu den Griechen; Musen hinter ihm,", "tokens": ["\u2020\u2020\u2020", "ihn", "zu", "den", "Grie\u00b7chen", ";", "Mu\u00b7sen", "hin\u00b7ter", "ihm", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "APPR", "ART", "NN", "$.", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.9": {"text": "Die Griechenland f\u00fcr Occident umformten.", "tokens": ["Die", "Grie\u00b7chen\u00b7land", "f\u00fcr", "Oc\u00b7ci\u00b7dent", "um\u00b7form\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Was seh' ich in dem Musentempel?", "tokens": ["Was", "seh'", "ich", "in", "dem", "Mu\u00b7sen\u00b7tem\u00b7pel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die griech'schen, R\u00f6mischen und die Homere", "tokens": ["Die", "griech'\u00b7schen", ",", "R\u00f6\u00b7mi\u00b7schen", "und", "die", "Ho\u00b7me\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "NN", "KON", "ART", "NN"], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Und Pindars, derer Barden r\u00e4uchern ihr Lorbeer,", "tokens": ["Und", "Pin\u00b7dars", ",", "de\u00b7rer", "Bar\u00b7den", "r\u00e4u\u00b7chern", "ihr", "Lor\u00b7beer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PDS", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+---+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Der Zeit \u2013 mich weckt ihr Lied!", "tokens": ["Der", "Zeit", "\u2013", "mich", "weckt", "ihr", "Lied", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Ich sing' sie an des Tempels Schwelle.", "tokens": ["Ich", "sing'", "sie", "an", "des", "Tem\u00b7pels", "Schwel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Du, die die Musen durch die S\u00fcmpf' Aegyptens,", "tokens": ["Du", ",", "die", "die", "Mu\u00b7sen", "durch", "die", "S\u00fcmpf'", "A\u00b7e\u00b7gyp\u00b7tens", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "APPR", "ART", "NN", "NE", "$,"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.16": {"text": "Der R\u00f6mer M\u00e4rkte und der Barden W\u00e4lder", "tokens": ["Der", "R\u00f6\u00b7mer", "M\u00e4rk\u00b7te", "und", "der", "Bar\u00b7den", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Zu unsrer W\u00fcste voll b\u00f6ot'scher Luft", "tokens": ["Zu", "uns\u00b7rer", "W\u00fcs\u00b7te", "voll", "b\u00f6\u00b7ot'\u00b7scher", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Hinbrachtest \u2013 uns mit Nachtdampf erst bedeckest,", "tokens": ["Hin\u00b7brach\u00b7test", "\u2013", "uns", "mit", "Nacht\u00b7dampf", "erst", "be\u00b7de\u00b7ckest", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PPER", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.19": {"text": "Dann durch ein Licht uns weckest und erquickst \u2013", "tokens": ["Dann", "durch", "ein", "Licht", "uns", "we\u00b7ckest", "und", "er\u00b7quickst", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPER", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Ach, gleich verfliegt's! \u2013 was brauchen wir noch von Dir?", "tokens": ["Ach", ",", "gleich", "ver\u00b7fliegt's", "!", "\u2013", "was", "brau\u00b7chen", "wir", "noch", "von", "Dir", "?"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "$.", "$(", "PWS", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Und was die Nachwelt? Freue Dich, Nachwelt! wir schlafen,", "tokens": ["Und", "was", "die", "Nach\u00b7welt", "?", "Freu\u00b7e", "Dich", ",", "Nach\u00b7welt", "!", "wir", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "$.", "VVFIN", "PPER", "$,", "NN", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wir t\u00e4ndeln, und Kolossen bleiben Dir.", "tokens": ["Wir", "t\u00e4n\u00b7deln", ",", "und", "Ko\u00b7los\u00b7sen", "blei\u00b7ben", "Dir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Dort seh' ich sie, die neidenswerthe Nachwelt,", "tokens": ["Dort", "seh'", "ich", "sie", ",", "die", "nei\u00b7dens\u00b7wert\u00b7he", "Nach\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.24": {"text": "Den Pfad (wir sahen ihn, doch gingen wir ihn nicht)", "tokens": ["Den", "Pfad", "(", "wir", "sa\u00b7hen", "ihn", ",", "doch", "gin\u00b7gen", "wir", "ihn", "nicht", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PPER", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Durch Blitz und Nacht zum Ziel aufklimmen.", "tokens": ["Durch", "Blitz", "und", "Nacht", "zum", "Ziel", "auf\u00b7klim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Ein Hieron weckte Pindarn!", "tokens": ["Ein", "Hie\u00b7ron", "weck\u00b7te", "Pin\u00b7darn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.27": {"text": "Zum Spott liest sie dann unsre Gottscheds-Oden.", "tokens": ["Zum", "Spott", "liest", "sie", "dann", "uns\u00b7re", "Gott\u00b7sche\u00b7ds\u00b7O\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Wir irren, handeln, zanken; sie wird schweigen,", "tokens": ["Wir", "ir\u00b7ren", ",", "han\u00b7deln", ",", "zan\u00b7ken", ";", "sie", "wird", "schwei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVINF", "$,", "VVINF", "$.", "PPER", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Sich h\u00fcten \u2013 denken und erfinden.", "tokens": ["Sich", "h\u00fc\u00b7ten", "\u2013", "den\u00b7ken", "und", "er\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "$(", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "O Nachwelt, sieh, doch nenn nicht unsre Fehler", "tokens": ["O", "Nach\u00b7welt", ",", "sieh", ",", "doch", "nenn", "nicht", "uns\u00b7re", "Feh\u00b7ler"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VVFIN", "$,", "ADV", "ADV", "PTKNEG", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Und lege Deinen Kranz der Zeit zu F\u00fc\u00dfen!", "tokens": ["Und", "le\u00b7ge", "Dei\u00b7nen", "Kranz", "der", "Zeit", "zu", "F\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Ein Zufall \u2013 goldne Zeit \u2013 und unsre Grille", "tokens": ["Ein", "Zu\u00b7fall", "\u2013", "gold\u00b7ne", "Zeit", "\u2013", "und", "uns\u00b7re", "Gril\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ADJA", "NN", "$(", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Lehrt Dich, und den Newtons sind wir Kepplers,", "tokens": ["Lehrt", "Dich", ",", "und", "den", "New\u00b7tons", "sind", "wir", "Kepp\u00b7lers", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "ART", "NN", "VAFIN", "PPER", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.34": {"text": "Wie uns Saturn, so siehst Du uns vielleicht,", "tokens": ["Wie", "uns", "Sa\u00b7turn", ",", "so", "siehst", "Du", "uns", "viel\u00b7leicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "$,", "ADV", "VVFIN", "PPER", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Wie wir die Vorwelt sehn. War Vorwelt besser,", "tokens": ["Wie", "wir", "die", "Vor\u00b7welt", "sehn", ".", "War", "Vor\u00b7welt", "bes\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVINF", "$.", "VAFIN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Wir schlecht? Du jetzt grausam, Zeit?", "tokens": ["Wir", "schlecht", "?", "Du", "jetzt", "grau\u00b7sam", ",", "Zeit", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "ADV", "ADJD", "$,", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.37": {"text": "Schweig, Nachwelt! und sprich, Ewigkeit!", "tokens": ["Schweig", ",", "Nach\u00b7welt", "!", "und", "sprich", ",", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$.", "KON", "ADJD", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Wenn, Zeit, einst nach zertr\u00fcmmertem All", "tokens": ["Wenn", ",", "Zeit", ",", "einst", "nach", "zer\u00b7tr\u00fcm\u00b7mer\u00b7tem", "All"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "NN", "$,", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.39": {"text": "Du Deiner Brust tief Deinen Liebling eingr\u00e4bst,", "tokens": ["Du", "Dei\u00b7ner", "Brust", "tief", "Dei\u00b7nen", "Lieb\u00b7ling", "ein\u00b7gr\u00e4bst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADJD", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.40": {"text": "Dann mit den Ph\u00f6nixschwingen Dir ein Feuer fachest,", "tokens": ["Dann", "mit", "den", "Ph\u00f6\u00b7nix\u00b7schwin\u00b7gen", "Dir", "ein", "Feu\u00b7er", "fa\u00b7chest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "So brenne, der Ewigkeit Nacht un\u00fcbergl\u00e4nzbar zu leuchten,", "tokens": ["So", "bren\u00b7ne", ",", "der", "E\u00b7wig\u00b7keit", "Nacht", "un\u00b7\u00fc\u00b7ber\u00b7gl\u00e4nz\u00b7bar", "zu", "leuch\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ART", "NN", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-+-+--+-", "measure": "amphibrach.tri.plus"}, "line.42": {"text": "Auch Dein Name, ", "tokens": ["Auch", "Dein", "Na\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}}}}}