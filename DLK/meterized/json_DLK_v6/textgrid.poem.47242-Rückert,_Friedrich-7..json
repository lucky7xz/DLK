{"textgrid.poem.47242": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einen klassischen Dichter in den H\u00e4nden,", "tokens": ["Ei\u00b7nen", "klas\u00b7si\u00b7schen", "Dich\u00b7ter", "in", "den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Den romantischen Fr\u00fchlingshain durchirrend,", "tokens": ["Den", "ro\u00b7man\u00b7ti\u00b7schen", "Fr\u00fch\u00b7lings\u00b7hain", "durch\u00b7ir\u00b7rend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Konnt' ich lesend und wandelnd nicht vereinen", "tokens": ["Konnt'", "ich", "le\u00b7send", "und", "wan\u00b7delnd", "nicht", "ver\u00b7ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADJD", "KON", "ADJD", "PTKNEG", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Jene Klassicit\u00e4t und die Romantik.", "tokens": ["Je\u00b7ne", "Klas\u00b7si\u00b7ci\u00b7t\u00e4t", "und", "die", "Ro\u00b7man\u00b7tik", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Wenn ich blickt' in das Buch, erschien mir's farblos", "tokens": ["Wenn", "ich", "blickt'", "in", "das", "Buch", ",", "er\u00b7schien", "mir's", "farb\u00b7los"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "NE", "NE"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Vor dem schwellenden Knospendrang des Lebens;", "tokens": ["Vor", "dem", "schwel\u00b7len\u00b7den", "Knos\u00b7pen\u00b7drang", "des", "Le\u00b7bens", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Wenn ich schaut' in den gr\u00fcnen Wald, erschien er", "tokens": ["Wenn", "ich", "schaut'", "in", "den", "gr\u00fc\u00b7nen", "Wald", ",", "er\u00b7schien", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "PPER"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Wirrvoll gegen die wohlgebauten Strophen,", "tokens": ["Wirr\u00b7voll", "ge\u00b7gen", "die", "wohl\u00b7ge\u00b7bau\u00b7ten", "Stro\u00b7phen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Schlecht geordnet die Schatten und die Lichter.", "tokens": ["Schlecht", "ge\u00b7ord\u00b7net", "die", "Schat\u00b7ten", "und", "die", "Lich\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "So mi\u00dffiel mir das eine durch das andre,", "tokens": ["So", "mi\u00df\u00b7fiel", "mir", "das", "ei\u00b7ne", "durch", "das", "and\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "ART", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Wechselnd richtete Buch und Welt zu Grund' sich.", "tokens": ["Wech\u00b7selnd", "rich\u00b7te\u00b7te", "Buch", "und", "Welt", "zu", "Grund'", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "KON", "NN", "APPR", "NN", "PRF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "M\u00fcde setzt' ich mich endlich auf den Stein hin,", "tokens": ["M\u00fc\u00b7de", "setzt'", "ich", "mich", "end\u00b7lich", "auf", "den", "Stein", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Wo zum Quellengeriesel Schatten rauschten,", "tokens": ["Wo", "zum", "Quel\u00b7len\u00b7ge\u00b7rie\u00b7sel", "Schat\u00b7ten", "rauschten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.14": {"text": "Weiter lesend und blickend aus dazwischen.", "tokens": ["Wei\u00b7ter", "le\u00b7send", "und", "bli\u00b7ckend", "aus", "da\u00b7zwi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "APPR", "NE", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "Immer lauschender blickt' ich, immer stiller", "tokens": ["Im\u00b7mer", "lau\u00b7schen\u00b7der", "blickt'", "ich", ",", "im\u00b7mer", "stil\u00b7ler"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "$,", "ADV", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Las ich, immer versenkter und versunkner;", "tokens": ["Las", "ich", ",", "im\u00b7mer", "ver\u00b7senk\u00b7ter", "und", "ver\u00b7sun\u00b7kner", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Ob ich las, ob ich blickte, wu\u00dft' ich selbst nicht.", "tokens": ["Ob", "ich", "las", ",", "ob", "ich", "blick\u00b7te", ",", "wu\u00dft'", "ich", "selbst", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.18": {"text": "Immer lieblicher, leiser flossen, rannen,", "tokens": ["Im\u00b7mer", "lieb\u00b7li\u00b7cher", ",", "lei\u00b7ser", "flos\u00b7sen", ",", "ran\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.19": {"text": "Immer inniger, tiefer schmolzen, schwammen", "tokens": ["Im\u00b7mer", "in\u00b7ni\u00b7ger", ",", "tie\u00b7fer", "schmol\u00b7zen", ",", "schwam\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "ADJD", "$,", "ADJD", "VVINF", "$,", "VVFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.20": {"text": "Ineinander der Fr\u00fchling und der Dichter.", "tokens": ["In\u00b7ein\u00b7an\u00b7der", "der", "Fr\u00fch\u00b7ling", "und", "der", "Dich\u00b7ter."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.21": {"text": "Verse rieseln in Wellen, und symmetrisch", "tokens": ["Ver\u00b7se", "rie\u00b7seln", "in", "Wel\u00b7len", ",", "und", "sym\u00b7met\u00b7risch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "NN", "$,", "KON", "ADJD"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.22": {"text": "Bauen bl\u00fchende Lauben sich zu Stanzen.", "tokens": ["Bau\u00b7en", "bl\u00fc\u00b7hen\u00b7de", "Lau\u00b7ben", "sich", "zu", "Stan\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PRF", "APPR", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.23": {"text": "Staunend f\u00fchl' ich von einem Geist mich wiegen,", "tokens": ["Stau\u00b7nend", "f\u00fchl'", "ich", "von", "ei\u00b7nem", "Geist", "mich", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.24": {"text": "Der des Lebens und Todes Widerspruch l\u00f6st \u2013", "tokens": ["Der", "des", "Le\u00b7bens", "und", "To\u00b7des", "Wi\u00b7der\u00b7spruch", "l\u00f6st", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "KON", "NN", "NN", "VVFIN", "$("], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.25": {"text": "Von dem Geiste des Schlummers und des Traumes,", "tokens": ["Von", "dem", "Geis\u00b7te", "des", "Schlum\u00b7mers", "und", "des", "Trau\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.26": {"text": "Die im Schatten mich \u00fcberschlichen hatten,", "tokens": ["Die", "im", "Schat\u00b7ten", "mich", "\u00fc\u00b7berschli\u00b7chen", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PPER", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.27": {"text": "Ohne da\u00df ich es merkte, bis, vom Odem", "tokens": ["Oh\u00b7ne", "da\u00df", "ich", "es", "merk\u00b7te", ",", "bis", ",", "vom", "O\u00b7dem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "PPER", "VVFIN", "$,", "KOUS", "$,", "APPRART", "NN"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.28": {"text": "Einer st\u00fcrmischen Luft entk\u00fc\u00dft, ein Baumblatt", "tokens": ["Ei\u00b7ner", "st\u00fcr\u00b7mi\u00b7schen", "Luft", "ent\u00b7k\u00fc\u00dft", ",", "ein", "Baum\u00b7blatt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.29": {"text": "Auf das Buch, und das Buch mir aus der Hand fiel.", "tokens": ["Auf", "das", "Buch", ",", "und", "das", "Buch", "mir", "aus", "der", "Hand", "fiel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KON", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.30": {"text": "Schlaf, Verm\u00e4hler des Himmels mit der Erde!", "tokens": ["Schlaf", ",", "Ver\u00b7m\u00e4h\u00b7ler", "des", "Him\u00b7mels", "mit", "der", "Er\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.31": {"text": "Traum, Vermittler des Diesseits mit dem Jenseits!", "tokens": ["Traum", ",", "Ver\u00b7mitt\u00b7ler", "des", "Dies\u00b7seits", "mit", "dem", "Jen\u00b7seits", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.32": {"text": "Allvereinende, stets vereinte Br\u00fcder!", "tokens": ["All\u00b7ver\u00b7ei\u00b7nen\u00b7de", ",", "stets", "ver\u00b7ein\u00b7te", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADV", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.33": {"text": "Kommt noch \u00f6fter auf meinen Fr\u00fchlingsg\u00e4ngen", "tokens": ["Kommt", "noch", "\u00f6f\u00b7ter", "auf", "mei\u00b7nen", "Fr\u00fch\u00b7lings\u00b7g\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.34": {"text": "Mir entgegen und helfet mir studieren!", "tokens": ["Mir", "ent\u00b7ge\u00b7gen", "und", "hel\u00b7fet", "mir", "stu\u00b7die\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "KON", "VVFIN", "PPER", "VVINF", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.35": {"text": "Kein Ausleger vermag doch auszulegen", "tokens": ["Kein", "Aus\u00b7le\u00b7ger", "ver\u00b7mag", "doch", "aus\u00b7zu\u00b7le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Seinen Dichter, wie ihr, aus der Natur, und", "tokens": ["Sei\u00b7nen", "Dich\u00b7ter", ",", "wie", "ihr", ",", "aus", "der", "Na\u00b7tur", ",", "und"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "PPER", "$,", "APPR", "ART", "NN", "$,", "KON"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.37": {"text": "Einzulegen die Sch\u00f6pfung in den Dichter.", "tokens": ["Ein\u00b7zu\u00b7le\u00b7gen", "die", "Sch\u00f6p\u00b7fung", "in", "den", "Dich\u00b7ter."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-++-+-+-", "measure": "unknown.measure.penta"}}}}}