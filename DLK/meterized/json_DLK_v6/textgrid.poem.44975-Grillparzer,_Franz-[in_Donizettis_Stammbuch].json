{"textgrid.poem.44975": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[in Donizettis Stammbuch]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich schreibe dir, und du verstehst mich nicht.", "tokens": ["Ich", "schrei\u00b7be", "dir", ",", "und", "du", "ver\u00b7stehst", "mich", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was du geschrieben, hab ich wohl verstanden.", "tokens": ["Was", "du", "ge\u00b7schrie\u00b7ben", ",", "hab", "ich", "wohl", "ver\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Kopf versteht nur, was die Zunge spricht,", "tokens": ["Der", "Kopf", "ver\u00b7steht", "nur", ",", "was", "die", "Zun\u00b7ge", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Herzen sprechen gleich in allen \u2013 Landen.", "tokens": ["Die", "Her\u00b7zen", "spre\u00b7chen", "gleich", "in", "al\u00b7len", "\u2013", "Lan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PIAT", "$(", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}