{"textgrid.poem.55305": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Wanderlied", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von dem Berge zu den H\u00fcgeln,", "tokens": ["Von", "dem", "Ber\u00b7ge", "zu", "den", "H\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Niederab das Tal entlang,", "tokens": ["Nie\u00b7der\u00b7ab", "das", "Tal", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da erklingt es wie von Fl\u00fcgeln,", "tokens": ["Da", "er\u00b7klingt", "es", "wie", "von", "Fl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da bewegt sich's wie Gesang;", "tokens": ["Da", "be\u00b7wegt", "sich's", "wie", "Ge\u00b7sang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "KOKOM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und dem unbedingten Triebe", "tokens": ["Und", "dem", "un\u00b7be\u00b7ding\u00b7ten", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Folget Freude, folget Rat;", "tokens": ["Fol\u00b7get", "Freu\u00b7de", ",", "fol\u00b7get", "Rat", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und dein Streben, sei's in Liebe,", "tokens": ["Und", "dein", "Stre\u00b7ben", ",", "sei's", "in", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "VAFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und dein Leben sei die Tat!", "tokens": ["Und", "dein", "Le\u00b7ben", "sei", "die", "Tat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn die Bande sind zerrissen,", "tokens": ["Denn", "die", "Ban\u00b7de", "sind", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das Vertrauen ist verletzt;", "tokens": ["Das", "Ver\u00b7trau\u00b7en", "ist", "ver\u00b7letzt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kann ich sagen, kann ich wissen,", "tokens": ["Kann", "ich", "sa\u00b7gen", ",", "kann", "ich", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welchem Zufall ausgesetzt", "tokens": ["Wel\u00b7chem", "Zu\u00b7fall", "aus\u00b7ge\u00b7setzt"], "token_info": ["word", "word", "word"], "pos": ["PWAT", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich nun scheiden, ich nun wandern,", "tokens": ["Ich", "nun", "schei\u00b7den", ",", "ich", "nun", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$,", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie die Witwe, trauervoll,", "tokens": ["Wie", "die", "Wit\u00b7we", ",", "trau\u00b7er\u00b7voll", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Statt dem einen, mit dem andern", "tokens": ["Statt", "dem", "ei\u00b7nen", ",", "mit", "dem", "an\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "ART", "$,", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Fort und fort mich wenden soll!", "tokens": ["Fort", "und", "fort", "mich", "wen\u00b7den", "soll", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PTKVZ", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Bleibe nicht am Boden heften,", "tokens": ["Blei\u00b7be", "nicht", "am", "Bo\u00b7den", "hef\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Frisch gewagt und frisch hinaus!", "tokens": ["Frisch", "ge\u00b7wagt", "und", "frisch", "hin\u00b7aus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "KON", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kopf und Arm mit heitern Kr\u00e4ften,", "tokens": ["Kopf", "und", "Arm", "mit", "hei\u00b7tern", "Kr\u00e4f\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcberall sind sie zu Haus;", "tokens": ["\u00dc\u00b7be\u00b7rall", "sind", "sie", "zu", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo wir uns der Sonne freuen,", "tokens": ["Wo", "wir", "uns", "der", "Son\u00b7ne", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sind wir jede Sorge los;", "tokens": ["Sind", "wir", "je\u00b7de", "Sor\u00b7ge", "los", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df wir uns in ihr zerstreuen,", "tokens": ["Da\u00df", "wir", "uns", "in", "ihr", "zer\u00b7streu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Darum ist die Welt so gro\u00df.", "tokens": ["Da\u00b7rum", "ist", "die", "Welt", "so", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}}}}