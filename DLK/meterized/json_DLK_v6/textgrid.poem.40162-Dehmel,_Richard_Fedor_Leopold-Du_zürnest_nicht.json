{"textgrid.poem.40162": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Du z\u00fcrnest nicht", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O z\u00fcrne nicht, wenn mein Begehren", "tokens": ["O", "z\u00fcr\u00b7ne", "nicht", ",", "wenn", "mein", "Be\u00b7geh\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKNEG", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "aus seinen Tiefen lodernd bricht!", "tokens": ["aus", "sei\u00b7nen", "Tie\u00b7fen", "lo\u00b7dernd", "bricht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "soll es sich selber nicht verzehren,", "tokens": ["soll", "es", "sich", "sel\u00b7ber", "nicht", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "mu\u00df zuckend es hinaus ans Licht.", "tokens": ["mu\u00df", "zu\u00b7ckend", "es", "hin\u00b7aus", "ans", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVPP", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "und wenn empor die Flamme bricht,", "tokens": ["und", "wenn", "em\u00b7por", "die", "Flam\u00b7me", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "dich meine Liebe j\u00e4h umflutet,", "tokens": ["dich", "mei\u00b7ne", "Lie\u00b7be", "j\u00e4h", "um\u00b7flu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "dann bebst du, \u2013 \u2013 doch du z\u00fcrnest nicht.", "tokens": ["dann", "bebst", "du", ",", "\u2013", "\u2013", "doch", "du", "z\u00fcr\u00b7nest", "nicht", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "$(", "$(", "KON", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}