{"textgrid.poem.33617": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Weinlese", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alt und jung und gro\u00df und klein", "tokens": ["Alt", "und", "jung", "und", "gro\u00df", "und", "klein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "KON", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommt zu Fu\u00df und kommt zu Wagen,", "tokens": ["Kommt", "zu", "Fu\u00df", "und", "kommt", "zu", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles eilt, dabei zu sein,", "tokens": ["Al\u00b7les", "eilt", ",", "da\u00b7bei", "zu", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PAV", "PTKZU", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ein Jeder will was tragen.", "tokens": ["Und", "ein", "Je\u00b7der", "will", "was", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIS", "VMFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Volle K\u00f6rbe schleppt man her", "tokens": ["Vol\u00b7le", "K\u00f6r\u00b7be", "schleppt", "man", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PIS", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor des Torkels braune Stufen,", "tokens": ["Vor", "des", "Tor\u00b7kels", "brau\u00b7ne", "Stu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Trauben werden goldig schwer", "tokens": ["Trau\u00b7ben", "wer\u00b7den", "gol\u00b7dig", "schwer"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADJD", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eingestampft in hohe Kufen.", "tokens": ["Ein\u00b7ge\u00b7stampft", "in", "ho\u00b7he", "Ku\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Gro\u00df und klein und jung und alt", "tokens": ["Gro\u00df", "und", "klein", "und", "jung", "und", "alt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "KON", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wei\u00df die Freude kaum zu z\u00fcgeln,", "tokens": ["Wei\u00df", "die", "Freu\u00b7de", "kaum", "zu", "z\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles jubelt, da\u00df es schallt,", "tokens": ["Al\u00b7les", "ju\u00b7belt", ",", "da\u00df", "es", "schallt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Widerhallt an allen H\u00fcgeln!", "tokens": ["Wi\u00b7der\u00b7hallt", "an", "al\u00b7len", "H\u00fc\u00b7geln", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mir auch mit den Augen lacht", "tokens": ["Mir", "auch", "mit", "den", "Au\u00b7gen", "lacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Heute von den M\u00e4dchen eines,", "tokens": ["Heu\u00b7te", "von", "den", "M\u00e4d\u00b7chen", "ei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil ich immer gro\u00df gedacht", "tokens": ["Weil", "ich", "im\u00b7mer", "gro\u00df", "ge\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Von dem edlen Gott des Weines.", "tokens": ["Von", "dem", "ed\u00b7len", "Gott", "des", "Wei\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Weil ich sein Getreuer war,", "tokens": ["Weil", "ich", "sein", "Ge\u00b7treu\u00b7er", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der ihm freudig stets gehuldigt,", "tokens": ["Der", "ihm", "freu\u00b7dig", "stets", "ge\u00b7hul\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil mich seine Nymphenschar", "tokens": ["Weil", "mich", "sei\u00b7ne", "Nym\u00b7phen\u00b7schar"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nie der L\u00e4ssigkeit beschuldigt.", "tokens": ["Nie", "der", "L\u00e4s\u00b7sig\u00b7keit", "be\u00b7schul\u00b7digt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Lach, du Sch\u00f6ne, lach nur zu,", "tokens": ["Lach", ",", "du", "Sch\u00f6\u00b7ne", ",", "lach", "nur", "zu", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "NN", "$,", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwing dich auf den Wagen munter,", "tokens": ["Schwing", "dich", "auf", "den", "Wa\u00b7gen", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00fcrze dich und streife Schuh'", "tokens": ["Sch\u00fcr\u00b7ze", "dich", "und", "strei\u00b7fe", "Schuh'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Str\u00fcmpfchen auch herunter!", "tokens": ["Und", "die", "Str\u00fcmpf\u00b7chen", "auch", "her\u00b7un\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Auf den Thyrsusstab gest\u00fctzt", "tokens": ["Auf", "den", "Thyr\u00b7sus\u00b7stab", "ge\u00b7st\u00fctzt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaut' ich dich heut Nacht im Traume,", "tokens": ["Schaut'", "ich", "dich", "heut", "Nacht", "im", "Trau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis ans Knie hinauf bespritzt", "tokens": ["Bis", "ans", "Knie", "hin\u00b7auf", "be\u00b7spritzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPRART", "NN", "ADV", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Von dem s\u00fc\u00dfen roten Schaume.", "tokens": ["Von", "dem", "s\u00fc\u00b7\u00dfen", "ro\u00b7ten", "Schau\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Einen guten Jahrgang mu\u00df", "tokens": ["Ei\u00b7nen", "gu\u00b7ten", "Jahr\u00b7gang", "mu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Diese Traubenlese bringen,", "tokens": ["Die\u00b7se", "Trau\u00b7ben\u00b7le\u00b7se", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den Wein, wie mich dein Ku\u00df,", "tokens": ["Und", "den", "Wein", ",", "wie", "mich", "dein", "Ku\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird des Feuers Kraft durchdringen!", "tokens": ["Wird", "des", "Feu\u00b7ers", "Kraft", "durch\u00b7drin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}