{"textgrid.poem.55947": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Tanagra", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein wenig gebrannter Erde,", "tokens": ["Ein", "we\u00b7nig", "ge\u00b7brann\u00b7ter", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "wie von gro\u00dfer Sonne gebrannt.", "tokens": ["wie", "von", "gro\u00b7\u00dfer", "Son\u00b7ne", "ge\u00b7brannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Als w\u00e4re die Geb\u00e4rde", "tokens": ["Als", "w\u00e4\u00b7re", "die", "Ge\u00b7b\u00e4r\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "einer M\u00e4dchenhand", "tokens": ["ei\u00b7ner", "M\u00e4d\u00b7chen\u00b7hand"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "auf einmal nicht mehr vergangen;", "tokens": ["auf", "ein\u00b7mal", "nicht", "mehr", "ver\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PTKNEG", "ADV", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "ohne nach etwas zu langen,", "tokens": ["oh\u00b7ne", "nach", "et\u00b7was", "zu", "lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "PIS", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "zu keinem Dinge hin", "tokens": ["zu", "kei\u00b7nem", "Din\u00b7ge", "hin"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "aus ihrem Gef\u00fchle f\u00fchrend,", "tokens": ["aus", "ih\u00b7rem", "Ge\u00b7f\u00fch\u00b7le", "f\u00fch\u00b7rend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "nur an sich selber r\u00fchrend", "tokens": ["nur", "an", "sich", "sel\u00b7ber", "r\u00fch\u00b7rend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PRF", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "wie eine Hand ans Kinn.", "tokens": ["wie", "ei\u00b7ne", "Hand", "ans", "Kinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wir heben und wir drehen", "tokens": ["Wir", "he\u00b7ben", "und", "wir", "dre\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "eine und eine Figur;", "tokens": ["ei\u00b7ne", "und", "ei\u00b7ne", "Fi\u00b7gur", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "KON", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "wir k\u00f6nnen fast verstehen", "tokens": ["wir", "k\u00f6n\u00b7nen", "fast", "ver\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "weshalb sie nicht vergehen, \u2013", "tokens": ["we\u00b7shalb", "sie", "nicht", "ver\u00b7ge\u00b7hen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "VVINF", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "aber wir sollen nur", "tokens": ["a\u00b7ber", "wir", "sol\u00b7len", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ADV"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "tiefer und wunderbarer", "tokens": ["tie\u00b7fer", "und", "wun\u00b7der\u00b7ba\u00b7rer"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "ADJA"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "h\u00e4ngen an dem was war", "tokens": ["h\u00e4n\u00b7gen", "an", "dem", "was", "war"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "PWS", "VAFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "und l\u00e4cheln: ein wenig klarer", "tokens": ["und", "l\u00e4\u00b7cheln", ":", "ein", "we\u00b7nig", "kla\u00b7rer"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "ART", "PIAT", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "vielleicht als vor einem Jahr.", "tokens": ["viel\u00b7leicht", "als", "vor", "ei\u00b7nem", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}