{"textgrid.poem.55666": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "[oh! da\u00df der Sinnen doch so viele sind!]", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oh! da\u00df der Sinnen doch so viele sind!", "tokens": ["Oh", "!", "da\u00df", "der", "Sin\u00b7nen", "doch", "so", "vie\u00b7le", "sind", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "ART", "NN", "ADV", "ADV", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verwirrung bringen sie ins Gl\u00fcck herein.", "tokens": ["Ver\u00b7wir\u00b7rung", "brin\u00b7gen", "sie", "ins", "Gl\u00fcck", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn ich dich sehe, w\u00fcnsch ich taub zu sein,", "tokens": ["Wenn", "ich", "dich", "se\u00b7he", ",", "w\u00fcnsch", "ich", "taub", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wenn ich dich h\u00f6re, blind.", "tokens": ["Wenn", "ich", "dich", "h\u00f6\u00b7re", ",", "blind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}