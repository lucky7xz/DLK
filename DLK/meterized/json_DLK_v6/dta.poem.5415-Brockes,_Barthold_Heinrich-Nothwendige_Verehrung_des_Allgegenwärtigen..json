{"dta.poem.5415": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Nothwendige Verehrung des  \n Allgegenw\u00e4rtigen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Z\u00f6g' ein grosser F\u00fcrst, verkleidet, unbekannt im Land'", "tokens": ["Z\u00f6g'", "ein", "gros\u00b7ser", "F\u00fcrst", ",", "ver\u00b7klei\u00b7det", ",", "un\u00b7be\u00b7kannt", "im", "Land'"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "VVPP", "$,", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "umher,", "tokens": ["um\u00b7her", ","], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Und ein Unterthan vermeinte ihn zu kennen; sollt er nicht", "tokens": ["Und", "ein", "Un\u00b7ter\u00b7than", "ver\u00b7mein\u00b7te", "ihn", "zu", "ken\u00b7nen", ";", "sollt", "er", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$.", "VMFIN", "PPER", "PTKNEG"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Bey Verwundrungs-vollen Mienen und mit fr\u00f6lichem", "tokens": ["Bey", "Ver\u00b7wun\u00b7drungs\u00b7vol\u00b7len", "Mie\u00b7nen", "und", "mit", "fr\u00f6\u00b7li\u00b7chem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "APPR", "ADJA"], "meter": "+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.5": {"text": "Gesicht", "tokens": ["Ge\u00b7sicht"], "token_info": ["word"], "pos": ["NN"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Etwas Gutes gerne sprechen, zu des Landes F\u00fcrsten Ehr?", "tokens": ["Et\u00b7was", "Gu\u00b7tes", "ger\u00b7ne", "spre\u00b7chen", ",", "zu", "des", "Lan\u00b7des", "F\u00fcrs\u00b7ten", "Ehr", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVINF", "$,", "APPR", "ART", "NN", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Und wir wissen, da\u00df der Sch\u00f6pfer, durch den so viel", "tokens": ["Und", "wir", "wis\u00b7sen", ",", "da\u00df", "der", "Sch\u00f6p\u00b7fer", ",", "durch", "den", "so", "viel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVINF", "$,", "KOUS", "ART", "NN", "$,", "APPR", "ART", "ADV", "ADV"], "meter": "+-+-+-+-++-+", "measure": "unknown.measure.septa"}, "line.8": {"text": "Guts geschicht,", "tokens": ["Guts", "ge\u00b7schicht", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Jmmer bey uns gegenw\u00e4rtig. La\u00dft uns denn doch auch", "tokens": ["Jm\u00b7mer", "bey", "uns", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", ".", "La\u00dft", "uns", "denn", "doch", "auch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "ADJD", "$.", "VVIMP", "PPER", "ADV", "ADV", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.10": {"text": "nicht schweigen;", "tokens": ["nicht", "schwei\u00b7gen", ";"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Sondern \u00fcber seine Wunder, ihm zum Ruhm, uns froh", "tokens": ["Son\u00b7dern", "\u00fc\u00b7ber", "sei\u00b7ne", "Wun\u00b7der", ",", "ihm", "zum", "Ruhm", ",", "uns", "froh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "$,", "PPER", "APPRART", "NN", "$,", "PPER", "ADJD"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.12": {"text": "bezeigen!", "tokens": ["be\u00b7zei\u00b7gen", "!"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}