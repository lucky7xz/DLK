{"textgrid.poem.24936": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Drei Hexen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nachts auf dem rauchenden Schutte Lyons, matt von den Schreckensthaten,", "tokens": ["Nachts", "auf", "dem", "rau\u00b7chen\u00b7den", "Schut\u00b7te", "Ly\u00b7ons", ",", "matt", "von", "den", "Schre\u00b7ckenst\u00b7ha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "NE", "$,", "KOUI", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Die sie im Fron des Konvents vollbracht, lag ein Haufe Soldaten;", "tokens": ["Die", "sie", "im", "Fron", "des", "Kon\u00b7vents", "voll\u00b7bracht", ",", "lag", "ein", "Hau\u00b7fe", "Sol\u00b7da\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "ART", "NN", "VVPP", "$,", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "M\u00fcde ruhte das fressende Schwert, stumm die W\u00fcrgerkanone;", "tokens": ["M\u00fc\u00b7de", "ruh\u00b7te", "das", "fres\u00b7sen\u00b7de", "Schwert", ",", "stumm", "die", "W\u00fcr\u00b7ger\u00b7ka\u00b7no\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,", "ADJD", "ART", "NN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Blutgeschwollen mit wirbelnder Flut rauschte vor\u00fcber der Rhone.", "tokens": ["Blut\u00b7ge\u00b7schwol\u00b7len", "mit", "wir\u00b7beln\u00b7der", "Flut", "rauschte", "vor\u00b7\u00fc\u00b7ber", "der", "Rho\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+---+-+--+--+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Allen die eisernen Sinne tief hielt der Schlummer gebunden;", "tokens": ["Al\u00b7len", "die", "ei\u00b7ser\u00b7nen", "Sin\u00b7ne", "tief", "hielt", "der", "Schlum\u00b7mer", "ge\u00b7bun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "ADJD", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Einer, ein junger Krieger, allein hatte nicht Schlaf gefunden;", "tokens": ["Ei\u00b7ner", ",", "ein", "jun\u00b7ger", "Krie\u00b7ger", ",", "al\u00b7lein", "hat\u00b7te", "nicht", "Schlaf", "ge\u00b7fun\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "ART", "ADJA", "NN", "$,", "ADV", "VAFIN", "PTKNEG", "NN", "VVPP", "$."], "meter": "+--+-+-+-+--+-+-", "measure": "iambic.septa.invert"}, "line.3": {"text": "Fort und fort, wie Schatten und Licht \u00fcber die sch\u00e4umenden Fluten,", "tokens": ["Fort", "und", "fort", ",", "wie", "Schat\u00b7ten", "und", "Licht", "\u00fc\u00b7ber", "die", "sch\u00e4u\u00b7men\u00b7den", "Flu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PTKVZ", "$,", "PWAV", "NN", "KON", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--++--+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Zogen Gedanken ihm \u00fcber die Stirn, welche nicht wichen noch ruhten.", "tokens": ["Zo\u00b7gen", "Ge\u00b7dan\u00b7ken", "ihm", "\u00fc\u00b7ber", "die", "Stirn", ",", "wel\u00b7che", "nicht", "wi\u00b7chen", "noch", "ruh\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "PTKNEG", "VVFIN", "ADV", "VVFIN", "$."], "meter": "+--+--+--+---+--+-", "measure": "dactylic.tetra.plus"}}, "stanza.3": {"line.1": {"text": "Da zu H\u00e4upten ihm unversehns standen drei Weibergestalten,", "tokens": ["Da", "zu", "H\u00e4up\u00b7ten", "ihm", "un\u00b7ver\u00b7sehns", "stan\u00b7den", "drei", "Wei\u00b7ber\u00b7ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PPER", "ADJD", "VVFIN", "CARD", "NN", "$,"], "meter": "--+--+-+--++--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Bleich und blutlos das Angesicht, tief umdunkelt von Falten;", "tokens": ["Bleich", "und", "blut\u00b7los", "das", "An\u00b7ge\u00b7sicht", ",", "tief", "um\u00b7dun\u00b7kelt", "von", "Fal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "ART", "NN", "$,", "ADJD", "VVPP", "APPR", "NN", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.3": {"text": "Hagere H\u00e4nde streckten sie aus, ihre Stimme scholl heiser,", "tokens": ["Ha\u00b7ge\u00b7re", "H\u00e4n\u00b7de", "streck\u00b7ten", "sie", "aus", ",", "ih\u00b7re", "Stim\u00b7me", "scholl", "hei\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "NN", "ADJD", "ADJA", "$,"], "meter": "+--+-+-+-+-+--+-", "measure": "iambic.septa.invert"}, "line.4": {"text": "Und im Chore riefen die drei: \u00bbHeil dir, k\u00fcnftiger Kaiser.", "tokens": ["Und", "im", "Cho\u00b7re", "rie\u00b7fen", "die", "drei", ":", "\u00bb", "Heil", "dir", ",", "k\u00fcnf\u00b7ti\u00b7ger", "Kai\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "CARD", "$.", "$(", "NN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "--+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Der du in Glut des Kampfes den Sinn fr\u00fch zu Stahl dir geschmiedet,", "tokens": ["Der", "du", "in", "Glut", "des", "Kamp\u00b7fes", "den", "Sinn", "fr\u00fch", "zu", "Stahl", "dir", "ge\u00b7schmie\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "ART", "NN", "ART", "NN", "ADJD", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und ihn geh\u00e4rtet in Str\u00f6men des Bluts, das auf dem Schlachtfeld siedet,", "tokens": ["Und", "ihn", "ge\u00b7h\u00e4r\u00b7tet", "in", "Str\u00f6\u00b7men", "des", "Bluts", ",", "das", "auf", "dem", "Schlacht\u00b7feld", "sie\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "APPR", "NN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Dem die Seele kein Mitleid taut, die zu Eise gefroren,", "tokens": ["Dem", "die", "See\u00b7le", "kein", "Mit\u00b7leid", "taut", ",", "die", "zu", "Ei\u00b7se", "ge\u00b7fro\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "PIAT", "NN", "VVFIN", "$,", "PRELS", "APPR", "NN", "VVPP", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Zu gewaltigem Werk dich hat unser Meister erkoren!", "tokens": ["Zu", "ge\u00b7wal\u00b7ti\u00b7gem", "Werk", "dich", "hat", "un\u00b7ser", "Meis\u00b7ter", "er\u00b7ko\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}}, "stanza.5": {"line.1": {"text": "H\u00f6rst du des Kriegsvolks w\u00fcstes Geschrei? Sengen und Rauben und Morden", "tokens": ["H\u00f6rst", "du", "des", "Kriegs\u00b7volks", "w\u00fcs\u00b7tes", "Ge\u00b7schrei", "?", "Sen\u00b7gen", "und", "Rau\u00b7ben", "und", "Mor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$.", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+--+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Ist den W\u00fctenden wie die Luft, drin sie atmen, geworden.", "tokens": ["Ist", "den", "W\u00fc\u00b7ten\u00b7den", "wie", "die", "Luft", ",", "drin", "sie", "at\u00b7men", ",", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KOKOM", "ART", "NN", "$,", "ADV", "PPER", "VVINF", "$,", "VAPP", "$."], "meter": "--+-++-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "B\u00e4ndige du sie mit Stachel und Zaum, f\u00fchre gekoppelt die Meute,", "tokens": ["B\u00e4n\u00b7di\u00b7ge", "du", "sie", "mit", "Sta\u00b7chel", "und", "Zaum", ",", "f\u00fch\u00b7re", "ge\u00b7kop\u00b7pelt", "die", "Meu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "APPR", "NN", "KON", "NN", "$,", "VVFIN", "VVPP", "ART", "NN", "$,"], "meter": "+--+--+--+---+--+-", "measure": "dactylic.tetra.plus"}, "line.4": {"text": "Und die V\u00f6lker und Reiche der Welt wirst du erjagen als Beute!", "tokens": ["Und", "die", "V\u00f6l\u00b7ker", "und", "Rei\u00b7che", "der", "Welt", "wirst", "du", "er\u00b7ja\u00b7gen", "als", "Beu\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KON", "NE", "ART", "NN", "VAFIN", "PPER", "VVINF", "KOKOM", "NN", "$."], "meter": "--+--+--+-+-+--+-", "measure": "anapaest.tri.plus"}}, "stanza.6": {"line.1": {"text": "Wer mag leichterer Fang dir sein, F\u00fcrsten oder Nationen?", "tokens": ["Wer", "mag", "leich\u00b7te\u00b7rer", "Fang", "dir", "sein", ",", "F\u00fcrs\u00b7ten", "o\u00b7der", "Na\u00b7ti\u00b7o\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADJA", "NN", "PPER", "VAINF", "$,", "NN", "KON", "NN", "$."], "meter": "--+--+--+-+-+-+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Locke die einen mit glei\u00dfendem Wort; k\u00f6dre die andern mit Kronen;", "tokens": ["Lo\u00b7cke", "die", "ei\u00b7nen", "mit", "glei\u00b7\u00dfen\u00b7dem", "Wort", ";", "k\u00f6d\u00b7re", "die", "an\u00b7dern", "mit", "Kro\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ART", "APPR", "ADJA", "NN", "$.", "VVFIN", "ART", "ADJA", "APPR", "NN", "$."], "meter": "+--+--+--++--+--+-", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Auf die Beth\u00f6rten dann brich ein, wie der Wolf auf die Herden,", "tokens": ["Auf", "die", "Be\u00b7th\u00f6r\u00b7ten", "dann", "brich", "ein", ",", "wie", "der", "Wolf", "auf", "die", "Her\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADJD", "PTKVZ", "$,", "PWAV", "ART", "NE", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Und nicht einer wird dir entgehn \u2013 du sollst herrschen auf Erden!", "tokens": ["Und", "nicht", "ei\u00b7ner", "wird", "dir", "ent\u00b7gehn", "\u2013", "du", "sollst", "herr\u00b7schen", "auf", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PIS", "VAFIN", "PPER", "VVINF", "$(", "PPER", "VMFIN", "VVINF", "APPR", "NN", "$."], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Menschen, th\u00f6richt tolles Geschlecht, das wir zum Spiel uns erlasen!", "tokens": ["Men\u00b7schen", ",", "th\u00f6\u00b7richt", "tol\u00b7les", "Ge\u00b7schlecht", ",", "das", "wir", "zum", "Spiel", "uns", "er\u00b7la\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "ADJA", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+--+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Sie entstehn und vergehen in nichts, wie auf dem Schaume die Blasen;", "tokens": ["Sie", "ent\u00b7stehn", "und", "ver\u00b7ge\u00b7hen", "in", "nichts", ",", "wie", "auf", "dem", "Schau\u00b7me", "die", "Bla\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "PIS", "$,", "PWAV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+--+-+-+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Schminke nur ist das Lebensrot, um den Tod zu verbergen;", "tokens": ["Schmin\u00b7ke", "nur", "ist", "das", "Le\u00b7bens\u00b7rot", ",", "um", "den", "Tod", "zu", "ver\u00b7ber\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VAFIN", "ART", "NN", "$,", "KOUI", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Zu entlarven die L\u00fcge, zieh aus mit dem Heere der Schergen!", "tokens": ["Zu", "ent\u00b7lar\u00b7ven", "die", "L\u00fc\u00b7ge", ",", "zieh", "aus", "mit", "dem", "Hee\u00b7re", "der", "Scher\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "NN", "$,", "VVFIN", "APPR", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+-+-+-+--+-", "measure": "anapaest.di.plus"}}, "stanza.8": {"line.1": {"text": "Dringst du bei brennender St\u00e4dte Schein durch das Grauen der Schlachten", "tokens": ["Dringst", "du", "bei", "bren\u00b7nen\u00b7der", "St\u00e4d\u00b7te", "Schein", "durch", "das", "Grau\u00b7en", "der", "Schlach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Vor, bis wo die Nebel des Pols ewig die Welt umnachten,", "tokens": ["Vor", ",", "bis", "wo", "die", "Ne\u00b7bel", "des", "Pols", "e\u00b7wig", "die", "Welt", "um\u00b7nach\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "KON", "PWAV", "ART", "NN", "ART", "NN", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ueber Bergen Erschlagener dort und zertr\u00fcmmerten Reichen", "tokens": ["Ue\u00b7ber", "Ber\u00b7gen", "Er\u00b7schla\u00b7ge\u00b7ner", "dort", "und", "zer\u00b7tr\u00fcm\u00b7mer\u00b7ten", "Rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "ADV", "KON", "ADJA", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Magst du in Stolz erheben dein Haupt; wer kann dir sich vergleichen?", "tokens": ["Magst", "du", "in", "Stolz", "er\u00b7he\u00b7ben", "dein", "Haupt", ";", "wer", "kann", "dir", "sich", "ver\u00b7glei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVFIN", "PPOSAT", "NN", "$.", "PWS", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+--+-+--+-+-+-+-", "measure": "iambic.septa.invert"}}, "stanza.9": {"line.1": {"text": "Und in Freude zum erstenmal wird die Seele dir schmelzen,", "tokens": ["Und", "in", "Freu\u00b7de", "zum", "ers\u00b7ten\u00b7mal", "wird", "die", "See\u00b7le", "dir", "schmel\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "APPRART", "ADV", "VAFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Und Gedanken, wie keiner gedacht, wirst du im Geiste w\u00e4lzen:", "tokens": ["Und", "Ge\u00b7dan\u00b7ken", ",", "wie", "kei\u00b7ner", "ge\u00b7dacht", ",", "wirst", "du", "im", "Geis\u00b7te", "w\u00e4l\u00b7zen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PWAV", "PIS", "VVPP", "$,", "VAFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "--+--+--+-+-+-+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Gro\u00df wie der eine, der drunten ist, unser Meister, zu werden!\u00ab", "tokens": ["Gro\u00df", "wie", "der", "ei\u00b7ne", ",", "der", "drun\