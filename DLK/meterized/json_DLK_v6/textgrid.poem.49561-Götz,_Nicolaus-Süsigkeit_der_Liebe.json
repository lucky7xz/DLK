{"textgrid.poem.49561": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "S\u00fcsigkeit der Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die G\u00f6tter thaten, uns zu necken,", "tokens": ["Die", "G\u00f6t\u00b7ter", "tha\u00b7ten", ",", "uns", "zu", "ne\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schmertz, Sorgen, Krankheit, Mangel, Schwermuth", "tokens": ["Schmertz", ",", "Sor\u00b7gen", ",", "Krank\u00b7heit", ",", "Man\u00b7gel", ",", "Schwer\u00b7muth"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und alle Gattungen von \u00dcbel", "tokens": ["Und", "al\u00b7le", "Gat\u00b7tun\u00b7gen", "von", "\u00dc\u00b7bel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "APPR", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Vorzeiten in Pandorens B\u00fcchse;", "tokens": ["Vor\u00b7zei\u00b7ten", "in", "Pan\u00b7do\u00b7rens", "B\u00fcch\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "NE", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Allein Cythere, unsre Freundin,", "tokens": ["Al\u00b7lein", "Cy\u00b7the\u00b7re", ",", "uns\u00b7re", "Freun\u00b7din", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "That ihren Sohn dazu: derselbe", "tokens": ["That", "ih\u00b7ren", "Sohn", "da\u00b7zu", ":", "der\u00b7sel\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$.", "PDAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Vers\u00fcst uns nun die \u00dcbel alle.", "tokens": ["Ver\u00b7s\u00fcst", "uns", "nun", "die", "\u00dc\u00b7bel", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PIAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}