{"textgrid.poem.62333": {"metadata": {"author": {"name": "Gessner, Salomon", "birth": "N.A.", "death": "N.A."}, "title": "An den Wasserfall", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist das der Ort, wo sonst Entzyken", "tokens": ["Ist", "das", "der", "Ort", ",", "wo", "sonst", "Ent\u00b7zy\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PDS", "ART", "NN", "$,", "PWAV", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im sanften Schatten auf mich kam?", "tokens": ["Im", "sanf\u00b7ten", "Schat\u00b7ten", "auf", "mich", "kam", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bist du es, Fels! wo aus den Str\u00e6uchen", "tokens": ["Bist", "du", "es", ",", "Fels", "!", "wo", "aus", "den", "Str\u00e6u\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "$,", "NE", "$.", "PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Quelle hoch herunterstyrzt?", "tokens": ["Die", "Quel\u00b7le", "hoch", "her\u00b7un\u00b7ter\u00b7styrzt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da wo sonst deine klare Quelle", "tokens": ["Da", "wo", "sonst", "dei\u00b7ne", "kla\u00b7re", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Schaum und Moos herab sich styrzt,", "tokens": ["Auf", "Schaum", "und", "Moos", "her\u00b7ab", "sich", "styrzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da blinkt von Eis izt eine S\u00e6ule", "tokens": ["Da", "blinkt", "von", "Eis", "izt", "ei\u00b7ne", "S\u00e6u\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vom unterh\u0153lten Fels herab.", "tokens": ["Vom", "un\u00b7ter\u00b7h\u0153l\u00b7ten", "Fels", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie \u0153d, wie nakt sind die Gestr\u00e6uche,", "tokens": ["Wie", "\u0153d", ",", "wie", "nakt", "sind", "die", "Ge\u00b7str\u00e6u\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo sonst im dunkeln Laub-Gew\u0153lb", "tokens": ["Wo", "sonst", "im", "dun\u00b7keln", "Laub\u00b7Ge\u00b7w\u0153lb"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Zephir mit den Blythen spielten,", "tokens": ["Die", "Ze\u00b7phir", "mit", "den", "Bly\u00b7then", "spiel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mit dem sanft-bewegten Laub,", "tokens": ["Und", "mit", "dem", "sanft\u00b7be\u00b7weg\u00b7ten", "Laub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da\u00df schnell-verschwundne Sonnen-Stralen", "tokens": ["Da\u00df", "schnell\u00b7ver\u00b7schwund\u00b7ne", "Son\u00b7nen\u00b7S\u00b7tra\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Wellen, Schaum und weichem Moos,", "tokens": ["Auf", "Wel\u00b7len", ",", "Schaum", "und", "wei\u00b7chem", "Moos", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Lichter durch den Schatten blizten,", "tokens": ["Wie", "Lich\u00b7ter", "durch", "den", "Schat\u00b7ten", "bliz\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie \u0153d, wie nakt h\u00e6ngt ihr herab!", "tokens": ["Wie", "\u0153d", ",", "wie", "nakt", "h\u00e6ngt", "ihr", "her\u00b7ab", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch bald, bald k\u0153mmt der Fryhling wieder,", "tokens": ["Doch", "bald", ",", "bald", "k\u0153mmt", "der", "Fryh\u00b7ling", "wie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00e6ngt yber dich ein frisch Gew\u0153lb,", "tokens": ["H\u00e6ngt", "yber", "dich", "ein", "frisch", "Ge\u00b7w\u0153lb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und \u0153fnet die verschlo\u00dfne Quelle,", "tokens": ["Und", "\u0153f\u00b7net", "die", "ver\u00b7schlo\u00df\u00b7ne", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df Kyhlung mit den Wellen flie\u00dft.", "tokens": ["Da\u00df", "Kyh\u00b7lung", "mit", "den", "Wel\u00b7len", "flie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O dann nihm mich in deine Schatten,", "tokens": ["O", "dann", "nihm", "mich", "in", "dei\u00b7ne", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo keine bange Sorg mich findt,", "tokens": ["Wo", "kei\u00b7ne", "ban\u00b7ge", "Sorg", "mich", "findt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du Wasser-Fall und du Gebysche,", "tokens": ["Du", "Was\u00b7ser\u00b7Fall", "und", "du", "Ge\u00b7by\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du Lager von dem weichsten Moos!", "tokens": ["Du", "La\u00b7ger", "von", "dem", "weichs\u00b7ten", "Moos", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Dann k\u0153mmt vom Thal und von den Hygeln,", "tokens": ["Dann", "k\u0153mmt", "vom", "Thal", "und", "von", "den", "Hy\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom dunkeln Wald und von der Flur,", "tokens": ["Vom", "dun\u00b7keln", "Wald", "und", "von", "der", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir k\u0153mmt von jeder Fryhlings-Blume", "tokens": ["Mir", "k\u0153mmt", "von", "je\u00b7der", "Fry\u00b7hlings\u00b7Blu\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein froh Entzyken in die Brust.", "tokens": ["Ein", "froh", "Ent\u00b7zy\u00b7ken", "in", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und, k\u0153nnt' ich einen Fyrst beneiden,", "tokens": ["Und", ",", "k\u0153nnt'", "ich", "ei\u00b7nen", "Fyrst", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn neben mir im kalten Bach", "tokens": ["Wenn", "ne\u00b7ben", "mir", "im", "kal\u00b7ten", "Bach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "APPRART", "ADJA", "NN"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Die Wellen mit der Flasche spielen,", "tokens": ["Die", "Wel\u00b7len", "mit", "der", "Fla\u00b7sche", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von altem Wein hoch aufgefyllt,", "tokens": ["Von", "al\u00b7tem", "Wein", "hoch", "auf\u00b7ge\u00b7fyllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und wenn in deinem kyhlen Schatten", "tokens": ["Und", "wenn", "in", "dei\u00b7nem", "kyh\u00b7len", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir oft ein frohes Lied gelingt,", "tokens": ["Mir", "oft", "ein", "fro\u00b7hes", "Lied", "ge\u00b7lingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das noch mit Unschuld-voller Freude", "tokens": ["Das", "noch", "mit", "Un\u00b7schuld\u00b7vol\u00b7ler", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des sp\u00e6ten Enkels Brust erfyllt?", "tokens": ["Des", "sp\u00e6ten", "En\u00b7kels", "Brust", "er\u00b7fyllt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}