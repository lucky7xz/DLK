{"textgrid.poem.24903": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommt, Perin und Dschinnen!", "tokens": ["Kommt", ",", "Pe\u00b7rin", "und", "Dschin\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "KON", "NN", "$."], "meter": "--+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Auf dem Mauerkranz", "tokens": ["Auf", "dem", "Mau\u00b7er\u00b7kranz"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Der Alhambrazinnen", "tokens": ["Der", "Al\u00b7hamb\u00b7ra\u00b7zin\u00b7nen"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Liegt der Mondenglanz;", "tokens": ["Liegt", "der", "Mon\u00b7den\u00b7glanz", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Unter Palmen\u00e4sten", "tokens": ["Un\u00b7ter", "Pal\u00b7me\u00b7n\u00e4s\u00b7ten"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Schlinget hier im Westen,", "tokens": ["Schlin\u00b7get", "hier", "im", "Wes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Wie bei Bagdads Festen,", "tokens": ["Wie", "bei", "Bag\u00b7dads", "Fes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Euren Reihentanz!", "tokens": ["Eu\u00b7ren", "Rei\u00b7hen\u00b7tanz", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Schwingt euch auf den spitzen", "tokens": ["Schwingt", "euch", "auf", "den", "spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Turm des Boabdil!", "tokens": ["Turm", "des", "Boab\u00b7dil", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Seht die H\u00f6fe blitzen", "tokens": ["Seht", "die", "H\u00f6\u00b7fe", "blit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "In der Wellen Spiel!", "tokens": ["In", "der", "Wel\u00b7len", "Spiel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "--+-+", "measure": "anapaest.init"}, "line.5": {"text": "Ins Bassin gesunken,", "tokens": ["Ins", "Bas\u00b7sin", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Tanzen goldne Funken,", "tokens": ["Tan\u00b7zen", "gold\u00b7ne", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und vom Lichte trunken", "tokens": ["Und", "vom", "Lich\u00b7te", "trun\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Schimmert der Jenil.", "tokens": ["Schim\u00b7mert", "der", "Je\u00b7nil", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wie ein Glanz von Osten", "tokens": ["Wie", "ein", "Glanz", "von", "Os\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Quillt es um den Stein,", "tokens": ["Quillt", "es", "um", "den", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und die Jaspispfosten", "tokens": ["Und", "die", "Jas\u00b7pis\u00b7pfos\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Mit den Marmorleu'n,", "tokens": ["Mit", "den", "Mar\u00b7mor\u00b7leu'n", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Drum sich bunt in Ringen", "tokens": ["Drum", "sich", "bunt", "in", "Rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "PRF", "ADJD", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Zauberspr\u00fcche schlingen,", "tokens": ["Zau\u00b7ber\u00b7spr\u00fc\u00b7che", "schlin\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Strahlen und erklingen", "tokens": ["Strah\u00b7len", "und", "er\u00b7klin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "In dem Widerschein.", "tokens": ["In", "dem", "Wi\u00b7der\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "H\u00f6rt ihr der Drommeten", "tokens": ["H\u00f6rt", "ihr", "der", "Drom\u00b7me\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und der Zinken Schall?", "tokens": ["Und", "der", "Zin\u00b7ken", "Schall", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$."], "meter": "--+-+", "measure": "anapaest.init"}, "line.3": {"text": "An den sternbes\u00e4ten", "tokens": ["An", "den", "stern\u00b7be\u00b7s\u00e4\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Decken \u00fcberall,", "tokens": ["De\u00b7cken", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Aus den S\u00e4uleng\u00e4ngen,", "tokens": ["Aus", "den", "S\u00e4u\u00b7len\u00b7g\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wo in Laubgeh\u00e4ngen", "tokens": ["Wo", "in", "Laub\u00b7ge\u00b7h\u00e4n\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Sich die Bl\u00fcten dr\u00e4ngen,", "tokens": ["Sich", "die", "Bl\u00fc\u00b7ten", "dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "T\u00f6nt der Wiederhall.", "tokens": ["T\u00f6nt", "der", "Wie\u00b7der\u00b7hall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "In dem Schwestersaale", "tokens": ["In", "dem", "Schwes\u00b7ter\u00b7saa\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Schallt es wie Gesang;", "tokens": ["Schallt", "es", "wie", "Ge\u00b7sang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Aus der Weihrauchschale,", "tokens": ["Aus", "der", "Weih\u00b7rauch\u00b7scha\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Der er sich entschwang,", "tokens": ["Der", "er", "sich", "ent\u00b7schwang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wallt der Duft des Ambra", "tokens": ["Wallt", "der", "Duft", "des", "A\u00b7mbra"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Hin durch die Alhambra,", "tokens": ["Hin", "durch", "die", "Al\u00b7hamb\u00b7ra", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Und zur muntern Zambra", "tokens": ["Und", "zur", "mun\u00b7tern", "Zamb\u00b7ra"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Ruft der Schellenklang.", "tokens": ["Ruft", "der", "Schel\u00b7len\u00b7klang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Welch ein bunter Flimmer!", "tokens": ["Welch", "ein", "bun\u00b7ter", "Flim\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Nah und n\u00e4her tritt's!", "tokens": ["Nah", "und", "n\u00e4\u00b7her", "tritt's", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Seidner Kleider Schimmer,", "tokens": ["Seid\u00b7ner", "Klei\u00b7der", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Blanker Waffen Blitz!", "tokens": ["Blan\u00b7ker", "Waf\u00b7fen", "Blitz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Die vom Schlaf Erwachten", "tokens": ["Die", "vom", "Schlaf", "Er\u00b7wach\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Nahn in reichen Trachten,", "tokens": ["Nahn", "in", "rei\u00b7chen", "Trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Strahlend von Smaragden,", "tokens": ["Strah\u00b7lend", "von", "Sma\u00b7rag\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,"], "meter": "+--++-", "measure": "dactylic.init"}, "line.8": {"text": "Ihrem alten Sitz.", "tokens": ["Ih\u00b7rem", "al\u00b7ten", "Sitz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Agas mit der Fahne", "tokens": ["A\u00b7gas", "mit", "der", "Fah\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN"], "meter": "+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Gehn dem Zuge vor,", "tokens": ["Gehn", "dem", "Zu\u00b7ge", "vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Krumme Ataghane", "tokens": ["Krum\u00b7me", "A\u00b7tag\u00b7ha\u00b7ne"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Schwingt ein jeder Mohr;", "tokens": ["Schwingt", "ein", "je\u00b7der", "Mohr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PIAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Ritter, nicht zu z\u00e4hlen,", "tokens": ["Rit\u00b7ter", ",", "nicht", "zu", "z\u00e4h\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Zegris und Gomelen,", "tokens": ["Ze\u00b7gris", "und", "Go\u00b7me\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Fluten zu den S\u00e4len", "tokens": ["Flu\u00b7ten", "zu", "den", "S\u00e4\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Durch das Richterthor.", "tokens": ["Durch", "das", "Rich\u00b7ter\u00b7thor", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Tartschen tr\u00e4gt ein jeder,", "tokens": ["Tart\u00b7schen", "tr\u00e4gt", "ein", "je\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "PIS", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Blitzend wie Demant,", "tokens": ["Blit\u00b7zend", "wie", "De\u00b7mant", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und die Reiherfeder", "tokens": ["Und", "die", "Rei\u00b7her\u00b7fe\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "An des Turbans Rand;", "tokens": ["An", "des", "Tur\u00b7bans", "Rand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Allen die Gew\u00e4nder", "tokens": ["Al\u00b7len", "die", "Ge\u00b7w\u00e4n\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["NE", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Schm\u00fccken bunte B\u00e4nder,", "tokens": ["Schm\u00fc\u00b7cken", "bun\u00b7te", "B\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Teure Liebespf\u00e4nder", "tokens": ["Teu\u00b7re", "Lie\u00b7be\u00b7spf\u00e4n\u00b7der"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Von der Sch\u00f6nen Hand.", "tokens": ["Von", "der", "Sch\u00f6\u00b7nen", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Sehet, was im Liede", "tokens": ["Se\u00b7het", ",", "was", "im", "Lie\u00b7de"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Euer Liebling war:", "tokens": ["Eu\u00b7er", "Lieb\u00b7ling", "war", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Gazul und Zaide,", "tokens": ["Ga\u00b7zul", "und", "Zai\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Das gepries'ne Paar!", "tokens": ["Das", "ge\u00b7pries'\u00b7ne", "Paar", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Sie an seiner Rechten!", "tokens": ["Sie", "an", "sei\u00b7ner", "Rech\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Schwarz gleich dunklen N\u00e4chten", "tokens": ["Schwarz", "gleich", "dunk\u00b7len", "N\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Mit gel\u00f6sten Flechten", "tokens": ["Mit", "ge\u00b7l\u00f6s\u00b7ten", "Flech\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Wallt herab ihr Haar.", "tokens": ["Wallt", "her\u00b7ab", "ihr", "Haar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Wilder nun und bunter,", "tokens": ["Wil\u00b7der", "nun", "und", "bun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "KON", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Rauschenden Gewands,", "tokens": ["Rau\u00b7schen\u00b7den", "Ge\u00b7wands", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Aufw\u00e4rts und hinunter", "tokens": ["Auf\u00b7w\u00e4rts", "und", "hin\u00b7un\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Schlingen sie den Tanz \u2013", "tokens": ["Schlin\u00b7gen", "sie", "den", "Tanz", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Doch die Stunden rinnen", "tokens": ["Doch", "die", "Stun\u00b7den", "rin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ohne Rast von hinnen;", "tokens": ["Oh\u00b7ne", "Rast", "von", "hin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Um des Schlosses Zinnen", "tokens": ["Um", "des", "Schlos\u00b7ses", "Zin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Zuckt ein roter Glanz.", "tokens": ["Zuckt", "ein", "ro\u00b7ter", "Glanz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "Wehe dir, Granada,", "tokens": ["We\u00b7he", "dir", ",", "Gra\u00b7na\u00b7da", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$,", "NE", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Deine Pracht zerf\u00e4llt,", "tokens": ["Dei\u00b7ne", "Pracht", "zer\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wie sich die Nevada", "tokens": ["Wie", "sich", "die", "Ne\u00b7va\u00b7da"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Morgendlich erhellt!", "tokens": ["Mor\u00b7gend\u00b7lich", "er\u00b7hellt", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Gleich den Nebelrauchen", "tokens": ["Gleich", "den", "Ne\u00b7bel\u00b7rau\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "In des Ostens Hauchen", "tokens": ["In", "des", "Os\u00b7tens", "Hau\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Mu\u00dft du untertauchen,", "tokens": ["Mu\u00dft", "du", "un\u00b7ter\u00b7tau\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Sch\u00f6ne Zauberwelt!", "tokens": ["Sch\u00f6\u00b7ne", "Zau\u00b7ber\u00b7welt", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}