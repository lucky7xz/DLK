{"textgrid.poem.56753": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Irrlicht, schwebt ich heut im Traume", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Irrlicht, schwebt ich heut im Traume", "tokens": ["Ein", "Irr\u00b7licht", ",", "schwebt", "ich", "heut", "im", "Trau\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "auf einem weiten, d\u00fcstren Sumpfe,", "tokens": ["auf", "ei\u00b7nem", "wei\u00b7ten", ",", "d\u00fcst\u00b7ren", "Sump\u00b7fe", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "und um mich der Gespielen Reigen", "tokens": ["und", "um", "mich", "der", "Ge\u00b7spie\u00b7len", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PRF", "ART", "NN", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "in wunderlich geschlungnen Kr\u00e4nzen.", "tokens": ["in", "wun\u00b7der\u00b7lich", "ge\u00b7schlung\u00b7nen", "Kr\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wir sangen traurig-s\u00fc\u00dfe Lieder", "tokens": ["Wir", "san\u00b7gen", "trau\u00b7rig\u00b7s\u00fc\u00b7\u00dfe", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "mit leisen, feinen Geisterstimmen,", "tokens": ["mit", "lei\u00b7sen", ",", "fei\u00b7nen", "Geis\u00b7ters\u00b7tim\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "viel feiner als die lauten Grillen,", "tokens": ["viel", "fei\u00b7ner", "als", "die", "lau\u00b7ten", "Gril\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "die fern im Korn eint\u00f6nig sangen.", "tokens": ["die", "fern", "im", "Korn", "ein\u00b7t\u00f6\u00b7nig", "san\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wir sangen, wie das harte Schicksal", "tokens": ["Wir", "san\u00b7gen", ",", "wie", "das", "har\u00b7te", "Schick\u00b7sal"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "uns wehre, da\u00df wir Menschen w\u00fcrden:", "tokens": ["uns", "weh\u00b7re", ",", "da\u00df", "wir", "Men\u00b7schen", "w\u00fcr\u00b7den", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "So oft schon waren wir erschienen,", "tokens": ["So", "oft", "schon", "wa\u00b7ren", "wir", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "wo sich zwei Liebende vereinten,", "tokens": ["wo", "sich", "zwei", "Lie\u00b7ben\u00b7de", "ver\u00b7ein\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.13": {"text": "doch immer, ach, war schon ein andres", "tokens": ["doch", "im\u00b7mer", ",", "ach", ",", "war", "schon", "ein", "and\u00b7res"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "ITJ", "$,", "VAFIN", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Irr-Seelchen uns zuvorgekommen,", "tokens": ["Irr\u00b7Seel\u00b7chen", "uns", "zu\u00b7vor\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "und seufzend hatten wir von neuem", "tokens": ["und", "seuf\u00b7zend", "hat\u00b7ten", "wir", "von", "neu\u00b7em"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "VAFIN", "PPER", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "zur\u00fcck gemu\u00dft zum dunklen Sumpfe.", "tokens": ["zu\u00b7r\u00fcck", "ge\u00b7mu\u00dft", "zum", "dunk\u00b7len", "Sump\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "So sangen wir von unsern Leiden \u2013", "tokens": ["So", "san\u00b7gen", "wir", "von", "un\u00b7sern", "Lei\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "als uns mit einem Mal Entsetzen", "tokens": ["als", "uns", "mit", "ei\u00b7nem", "Mal", "Ent\u00b7set\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "in wirren L\u00e4ufen huschen machte.", "tokens": ["in", "wir\u00b7ren", "L\u00e4u\u00b7fen", "hu\u00b7schen", "mach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Ein Mensch entsprang dem nahen Walde", "tokens": ["Ein", "Mensch", "ent\u00b7sprang", "dem", "na\u00b7hen", "Wal\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "und lief verzweifelten Gebarens", "tokens": ["und", "lief", "ver\u00b7zwei\u00b7fel\u00b7ten", "Ge\u00b7ba\u00b7rens"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.22": {"text": "gerade auf uns zu \u2013: Der Boden", "tokens": ["ge\u00b7ra\u00b7de", "auf", "uns", "zu", "\u2013", ":", "Der", "Bo\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ADV", "APPR", "PPER", "PTKZU", "$(", "$.", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "schlug schwankend, eine schwere Woge,", "tokens": ["schlug", "schwan\u00b7kend", ",", "ei\u00b7ne", "schwe\u00b7re", "Wo\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "dem Armen \u00fcberm Haupt zusammen.", "tokens": ["dem", "Ar\u00b7men", "\u00fc\u00b7berm", "Haupt", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Verstummt zu zitterndem Gefl\u00fcster", "tokens": ["Ver\u00b7stummt", "zu", "zit\u00b7tern\u00b7dem", "Ge\u00b7fl\u00fcs\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "umschwirrten wir die grause Stelle ...", "tokens": ["um\u00b7schwirr\u00b7ten", "wir", "die", "grau\u00b7se", "Stel\u00b7le", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Bald aber sangen wir von neuem", "tokens": ["Bald", "a\u00b7ber", "san\u00b7gen", "wir", "von", "neu\u00b7em"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "die alten traurig-s\u00fc\u00dfen Lieder.", "tokens": ["die", "al\u00b7ten", "trau\u00b7rig\u00b7s\u00fc\u00b7\u00dfen", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}