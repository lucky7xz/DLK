{"dta.poem.3314": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Galathee.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Fleuch, Galathee! den Stolz verlebter Sch\u00f6nen!", "tokens": ["Fleuch", ",", "Ga\u00b7la\u00b7thee", "!", "den", "Stolz", "ver\u00b7leb\u00b7ter", "Sch\u00f6\u00b7nen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schilt auf die Liebe nicht.", "tokens": ["Schilt", "auf", "die", "Lie\u00b7be", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du wirst sie nur mit falschen Lippen h\u00f6hnen:", "tokens": ["Du", "wirst", "sie", "nur", "mit", "fal\u00b7schen", "Lip\u00b7pen", "h\u00f6h\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein Auge widerspricht.", "tokens": ["Dein", "Au\u00b7ge", "wi\u00b7der\u00b7spricht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es m\u00fcsse dich die s\u00fcsse Leyer lehren,", "tokens": ["Es", "m\u00fcs\u00b7se", "dich", "die", "s\u00fcs\u00b7se", "Le\u00b7yer", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die \u00fcberredend klingt,", "tokens": ["Die", "\u00fc\u00b7berr\u00b7e\u00b7dend", "klingt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und, wie man glaubt, trotz heuchlerischem Wehren,", "tokens": ["Und", ",", "wie", "man", "glaubt", ",", "trotz", "heuch\u00b7le\u00b7ri\u00b7schem", "Weh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PIS", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Von manchem spr\u00f6den Mund oft manchen Ku\u00df erzwingt.", "tokens": ["Von", "man\u00b7chem", "spr\u00f6\u00b7den", "Mund", "oft", "man\u00b7chen", "Ku\u00df", "er\u00b7zwingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Liebesgott schlief unter Myrthenb\u00fcschen,", "tokens": ["Der", "Lie\u00b7bes\u00b7gott", "schlief", "un\u00b7ter", "Myr\u00b7then\u00b7b\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Bluhmen hingestreckt;", "tokens": ["In", "Bluh\u00b7men", "hin\u00b7ge\u00b7streckt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und lie\u00df im Schlaf durch Nymphen sich erwischen,", "tokens": ["Und", "lie\u00df", "im", "Schlaf", "durch", "Nym\u00b7phen", "sich", "er\u00b7wi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "APPR", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die er so oft erschreckt.", "tokens": ["Die", "er", "so", "oft", "er\u00b7schreckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nur eingedenk, wie Amor sie geplaget,", "tokens": ["Nur", "ein\u00b7ge\u00b7denk", ",", "wie", "A\u00b7mor", "sie", "ge\u00b7pla\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "NE", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nicht, wie er sie entz\u00fcckt,", "tokens": ["Nicht", ",", "wie", "er", "sie", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PWAV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ver\u00fcbten sie, was niemand noch gewaget:", "tokens": ["Ver\u00b7\u00fcb\u00b7ten", "sie", ",", "was", "nie\u00b7mand", "noch", "ge\u00b7wa\u00b7get", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sie fesselten den Gott, der G\u00f6tter selbst bestrickt.", "tokens": ["Sie", "fes\u00b7sel\u00b7ten", "den", "Gott", ",", "der", "G\u00f6t\u00b7ter", "selbst", "be\u00b7strickt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Der schlaue Gott sah, als er schnell erwachte,", "tokens": ["Der", "schlau\u00b7e", "Gott", "sah", ",", "als", "er", "schnell", "er\u00b7wach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Den ihm gespielten Streich.", "tokens": ["Den", "ihm", "ge\u00b7spiel\u00b7ten", "Streich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O loses Volk! sprach dieser Schalk und lachte;", "tokens": ["O", "lo\u00b7ses", "Volk", "!", "sprach", "die\u00b7ser", "Schalk", "und", "lach\u00b7te", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "VVFIN", "PDAT", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie listig r\u00e4cht ihr euch!", "tokens": ["Wie", "lis\u00b7tig", "r\u00e4cht", "ihr", "euch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich l\u00e4ugne nicht, was ich an euch begangen:", "tokens": ["Ich", "l\u00e4ug\u00b7ne", "nicht", ",", "was", "ich", "an", "euch", "be\u00b7gan\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich macht\u2019 euch tausend Pein.", "tokens": ["Ich", "macht'", "euch", "tau\u00b7send", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "CARD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bes\u00e4nftigt euch! nun habt ihr mich gefangen:", "tokens": ["Be\u00b7s\u00e4nf\u00b7tigt", "euch", "!", "nun", "habt", "ihr", "mich", "ge\u00b7fan\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "ADV", "VAFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Jhr werdet ungequ\u00e4lt und ungek\u00fcsset seyn.", "tokens": ["Ihr", "wer\u00b7det", "un\u00b7ge\u00b7qu\u00e4lt", "und", "un\u00b7ge\u00b7k\u00fcs\u00b7set", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und ungek\u00fcfst? welch grausamer Gedanke!", "tokens": ["Und", "un\u00b7ge\u00b7k\u00fcfst", "?", "welch", "grau\u00b7sa\u00b7mer", "Ge\u00b7dan\u00b7ke", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "PWAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Man dachte reifer nach,", "tokens": ["Man", "dach\u00b7te", "rei\u00b7fer", "nach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sah besch\u00e4mt, wie dem verwegnen Zanke", "tokens": ["Und", "sah", "be\u00b7sch\u00e4mt", ",", "wie", "dem", "ver\u00b7weg\u00b7nen", "Zan\u00b7ke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Herze widersprach.", "tokens": ["Das", "Her\u00b7ze", "wi\u00b7der\u00b7sprach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie thaten \u2012\u2012 was? was alle M\u00e4dchen th\u00e4ten!", "tokens": ["Sie", "tha\u00b7ten", "\u2012\u2012", "was", "?", "was", "al\u00b7le", "M\u00e4d\u00b7chen", "th\u00e4\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWS", "$.", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Sie banden Amorn los,", "tokens": ["Sie", "ban\u00b7den", "A\u00b7morn", "los", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und Amor flog, da sie um Gnade flehten,", "tokens": ["Und", "A\u00b7mor", "flog", ",", "da", "sie", "um", "Gna\u00b7de", "fleh\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Von ihnen lachend weg in seiner Mutter Schoos.", "tokens": ["Von", "ih\u00b7nen", "la\u00b7chend", "weg", "in", "sei\u00b7ner", "Mut\u00b7ter", "Schoos", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}