{"dta.poem.20456": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "An Sr. Churfl. Durchl. zu Sachsen/  \n als sie bey antretung dero regierung den 24. Julii  \n an. 1694. zu Leipzig die erb-huldigung  \n annahmen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der himmel werde klar/ und du/ gew\u00f6lckte lufft/", "tokens": ["Der", "him\u00b7mel", "wer\u00b7de", "klar", "/", "und", "du", "/", "ge\u00b7w\u00f6lck\u00b7te", "lufft", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$(", "KON", "PPER", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df dein beliebtes licht mit gold-gemengten strahlen/", "tokens": ["La\u00df", "dein", "be\u00b7lieb\u00b7tes", "licht", "mit", "gold\u00b7ge\u00b7meng\u00b7ten", "strah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den treu-gesinnten krey\u00df um diesen ort bemahlen.", "tokens": ["Den", "treu\u00b7ge\u00b7sinn\u00b7ten", "krey\u00df", "um", "die\u00b7sen", "ort", "be\u00b7mah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch was vor antwort schallt? und welche stimme rufft:", "tokens": ["Doch", "was", "vor", "ant\u00b7wort", "schallt", "?", "und", "wel\u00b7che", "stim\u00b7me", "rufft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "VVFIN", "$.", "KON", "PWAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die sonne scheint besch\u00e4mt auff euch begl\u00fcckte grentzen/", "tokens": ["Die", "son\u00b7ne", "scheint", "be\u00b7sch\u00e4mt", "auff", "euch", "be\u00b7gl\u00fcck\u00b7te", "grent\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "PPER", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Itzt will ein h\u00f6her licht in euren mauren gl\u00e4ntzen.", "tokens": ["Itzt", "will", "ein", "h\u00f6\u00b7her", "licht", "in", "eu\u00b7ren", "mau\u00b7ren", "gl\u00e4nt\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Erlauchter G\u00f6tter-Sohn/ ach ja! du hast die bahn", "tokens": ["Er\u00b7lauch\u00b7ter", "G\u00f6t\u00b7ter\u00b7Sohn", "/", "ach", "ja", "!", "du", "hast", "die", "bahn"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$(", "ITJ", "ITJ", "$.", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im frohen horizont der Linden-stadt genommen.", "tokens": ["Im", "fro\u00b7hen", "ho\u00b7ri\u00b7zont", "der", "Lin\u00b7den\u00b7stadt", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Musen heissen dich viel tausendmahl willkommen/", "tokens": ["Die", "Mu\u00b7sen", "heis\u00b7sen", "dich", "viel", "tau\u00b7send\u00b7mahl", "will\u00b7kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn ihnen bricht anitzt des gl\u00fcckes morgen an.", "tokens": ["Denn", "ih\u00b7nen", "bricht", "a\u00b7nitzt", "des", "gl\u00fc\u00b7ckes", "mor\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "ADJA", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie friedlich und vermehrt wird diese schaar sich m\u00fchen/", "tokens": ["Wie", "fried\u00b7lich", "und", "ver\u00b7mehrt", "wird", "die\u00b7se", "schaar", "sich", "m\u00fc\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "VAFIN", "PDS", "VVFIN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "O Friederich August/ in deiner gunst zu bl\u00fchen.", "tokens": ["O", "Frie\u00b7de\u00b7rich", "Au\u00b7gust", "/", "in", "dei\u00b7ner", "gunst", "zu", "bl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$(", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wir s\u00f6hne Schlesiens/ die itzt in dieser stadt", "tokens": ["Wir", "s\u00f6h\u00b7ne", "Schle\u00b7si\u00b7ens", "/", "die", "itzt", "in", "die\u00b7ser", "stadt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$(", "ART", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der wei\u00dfheit edle kost gantz ungest\u00f6hrt geniessen/", "tokens": ["Der", "wei\u00df\u00b7heit", "ed\u00b7le", "kost", "gantz", "un\u00b7ge\u00b7st\u00f6hrt", "ge\u00b7nies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Crscheinen hier geb\u00fcckt/ und legen zu den f\u00fcssen/", "tokens": ["Cr\u00b7schei\u00b7nen", "hier", "ge\u00b7b\u00fcckt", "/", "und", "le\u00b7gen", "zu", "den", "f\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "$(", "KON", "VVFIN", "APPR", "ART", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was treu und danckbarkeit uns vorgeschrieben hat.", "tokens": ["Was", "treu", "und", "dan\u00b7ck\u00b7bar\u00b7keit", "uns", "vor\u00b7ge\u00b7schrie\u00b7ben", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Erlaube/ da\u00df di\u00df blat/ du beyspiel gr\u00f6ster Helden/", "tokens": ["Er\u00b7lau\u00b7be", "/", "da\u00df", "di\u00df", "blat", "/", "du", "bey\u00b7spiel", "gr\u00f6s\u00b7ter", "Hel\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KOUS", "PDS", "VVFIN", "$(", "PPER", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dieweil dein ruhm zu hoch/ darff dein gel\u00fccke melden.", "tokens": ["Die\u00b7weil", "dein", "ruhm", "zu", "hoch", "/", "darff", "dein", "ge\u00b7l\u00fc\u00b7cke", "mel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKA", "ADJD", "$(", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der D\u00e4nen Majest\u00e4t/ und dritter Friederich/", "tokens": ["Der", "D\u00e4\u00b7nen", "Ma\u00b7jes\u00b7t\u00e4t", "/", "und", "drit\u00b7ter", "Frie\u00b7de\u00b7rich", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der durch sein k\u00f6nigs-blut auch deinen purpur zieret/", "tokens": ["Der", "durch", "sein", "k\u00f6\u00b7nigs\u00b7blut", "auch", "dei\u00b7nen", "pur\u00b7pur", "zie\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "ADV", "PPOSAT", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hat unter seiner zahl am gl\u00fccklichsten regieret:", "tokens": ["Hat", "un\u00b7ter", "sei\u00b7ner", "zahl", "am", "gl\u00fcck\u00b7lichs\u00b7ten", "re\u00b7gie\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "In seinem sterbens-jahr erblickte Sachsen dich.", "tokens": ["In", "sei\u00b7nem", "ster\u00b7bens\u00b7jahr", "er\u00b7blick\u00b7te", "Sach\u00b7sen", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "NE", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dem himmel/ grosser F\u00fcrst/ gefiel hiemit zu deuten:", "tokens": ["Dem", "him\u00b7mel", "/", "gros\u00b7ser", "F\u00fcrst", "/", "ge\u00b7fiel", "hie\u00b7mit", "zu", "deu\u00b7ten", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADJA", "NN", "$(", "VVFIN", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er wolle sein gel\u00fcck auff dich/ als enckel/ leiten.", "tokens": ["Er", "wol\u00b7le", "sein", "ge\u00b7l\u00fcck", "auff", "dich", "/", "als", "en\u00b7ckel", "/", "lei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "APPR", "PPER", "$(", "KOUS", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wohlan! es wird schon wahr. Du kommst auff einen thron/", "tokens": ["Wo\u00b7hlan", "!", "es", "wird", "schon", "wahr", ".", "Du", "kommst", "auff", "ei\u00b7nen", "thron", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VAFIN", "ADV", "ADJD", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Worauff du nicht zuvor in hoffnung bist gestiegen.", "tokens": ["Wo\u00b7rauff", "du", "nicht", "zu\u00b7vor", "in", "hoff\u00b7nung", "bist", "ge\u00b7stie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "ADV", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nicht nur dein unterthan benennt dich sein vergn\u00fcgen;", "tokens": ["Nicht", "nur", "dein", "un\u00b7ter\u00b7than", "be\u00b7nennt", "dich", "sein", "ver\u00b7gn\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PPOSAT", "ADJD", "VVFIN", "PPER", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gantz Deutschland sieht an dir auch seinen schutz-gott an.", "tokens": ["Gantz", "Deutschland", "sieht", "an", "dir", "auch", "sei\u00b7nen", "schutz\u00b7gott", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "APPR", "PPER", "ADV", "PPOSAT", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Drum steht dir aller hertz wie thor\u2019 und pforten offen/", "tokens": ["Drum", "steht", "dir", "al\u00b7ler", "hertz", "wie", "thor'", "und", "pfor\u00b7ten", "of\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PIAT", "NN", "KOKOM", "ADJD", "KON", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Weil sie mit dir zugleich des gl\u00fcckes einzug hoffen.", "tokens": ["Weil", "sie", "mit", "dir", "zu\u00b7gleich", "des", "gl\u00fc\u00b7ckes", "ein\u00b7zug", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Dein hoher name prangt von lauter fried und ruh/", "tokens": ["Dein", "ho\u00b7her", "na\u00b7me", "prangt", "von", "lau\u00b7ter", "fried", "und", "ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "PIAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch deine tapfferkeit zeigt schwerdter/ blut und leichen.", "tokens": ["Doch", "dei\u00b7ne", "tapf\u00b7fer\u00b7keit", "zeigt", "schwerd\u00b7ter", "/", "blut", "und", "lei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD", "$(", "NN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das erste wird dein schutz den unterthanen reichen/", "tokens": ["Das", "ers\u00b7te", "wird", "dein", "schutz", "den", "un\u00b7ter\u00b7tha\u00b7nen", "rei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PPOSAT", "NN", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die andre w\u00fcrckung plitzt nur auff die feinde zu.", "tokens": ["Die", "and\u00b7re", "w\u00fcr\u00b7ckung", "plitzt", "nur", "auff", "die", "fein\u00b7de", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du wirst mit Simsons-krafft auch ihre helden zwingen/", "tokens": ["Du", "wirst", "mit", "Sim\u00b7sons\u00b7krafft", "auch", "ih\u00b7re", "hel\u00b7den", "zwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und friedens-honig noch aus ihrem aase bringen.", "tokens": ["Und", "frie\u00b7dens\u00b7ho\u00b7nig", "noch", "aus", "ih\u00b7rem", "aa\u00b7se", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Uns d\u00fcncket/ da\u00df die kunst den abri\u00df schon gethan/", "tokens": ["Uns", "d\u00fcn\u00b7cket", "/", "da\u00df", "die", "kunst", "den", "ab\u00b7ri\u00df", "schon", "ge\u00b7than", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "ART", "NN", "ART", "VVFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie dir die nach-welt wird ein solches denckmahl bauen:", "tokens": ["Wie", "dir", "die", "nach\u00b7welt", "wird", "ein", "sol\u00b7ches", "denck\u00b7mahl", "bau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VAFIN", "ART", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Geharnischt l\u00e4st sie dich auff einem wagen schauen/", "tokens": ["Ge\u00b7har\u00b7nischt", "l\u00e4st", "sie", "dich", "auff", "ei\u00b7nem", "wa\u00b7gen", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "APPR", "ART", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die ehre spannt davor gez\u00e4hmte l\u00f6wen an.", "tokens": ["Die", "eh\u00b7re", "spannt", "da\u00b7vor", "ge\u00b7z\u00e4hm\u00b7te", "l\u00f6\u00b7wen", "an", "."], "token_info": ["word", "word", "word", "word",