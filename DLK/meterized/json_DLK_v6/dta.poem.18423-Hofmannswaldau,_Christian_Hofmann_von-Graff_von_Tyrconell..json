{"dta.poem.18423": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Graff von Tyrconell.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein unverhoffter tod hat mich erw\u00fcndscht entrissen/", "tokens": ["Ein", "un\u00b7ver\u00b7hoff\u00b7ter", "tod", "hat", "mich", "er\u00b7w\u00fcnd\u00b7scht", "ent\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Bevor ich noch den sieg der feinde sehen m\u00fcssen;", "tokens": ["Be\u00b7vor", "ich", "noch", "den", "sieg", "der", "fein\u00b7de", "se\u00b7hen", "m\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "ART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch weil das laster selbst die straffe mit sich tr\u00e4gt/", "tokens": ["Doch", "weil", "das", "las\u00b7ter", "selbst", "die", "straf\u00b7fe", "mit", "sich", "tr\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "ADV", "ART", "VVFIN", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So hat mich bleiches gifft und unmuth hingelegt.", "tokens": ["So", "hat", "mich", "blei\u00b7ches", "gifft", "und", "un\u00b7muth", "hin\u00b7ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}