{"textgrid.poem.53403": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie richtest du, mein GOTT, mich zu?", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie richtest du, mein GOTT, mich zu?", "tokens": ["Wie", "rich\u00b7test", "du", ",", "mein", "GoTT", ",", "mich", "zu", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Zorn auff mich ist ausgelassen,", "tokens": ["Dein", "Zorn", "auff", "mich", "ist", "aus\u00b7ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Druckt und verfolget mich ohn massen,", "tokens": ["Druckt", "und", "ver\u00b7fol\u00b7get", "mich", "ohn", "mas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ich habe Tag und Nacht nicht Rhu,", "tokens": ["Ich", "ha\u00b7be", "Tag", "und", "Nacht", "nicht", "Rhu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "PTKNEG", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich schrey und fleh, ach la\u00df mein Behten", "tokens": ["Ich", "schrey", "und", "fleh", ",", "ach", "la\u00df", "mein", "Beh\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "KON", "VVFIN", "$,", "XY", "VVIMP", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In deine Vater-Ohren treten.", "tokens": ["In", "dei\u00b7ne", "Va\u00b7ter\u00b7Oh\u00b7ren", "tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein arme Seel' ist Jammersvoll", "tokens": ["Mein", "ar\u00b7me", "Seel'", "ist", "Jam\u00b7mers\u00b7voll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Leben nahe bey der Hellen.", "tokens": ["Mein", "Le\u00b7ben", "na\u00b7he", "bey", "der", "Hel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Man suchet mich gleich dem zu f\u00e4llen", "tokens": ["Man", "su\u00b7chet", "mich", "gleich", "dem", "zu", "f\u00e4l\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "ART", "PTKZU", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der nirgends Rettung haben sol,", "tokens": ["Der", "nir\u00b7gends", "Ret\u00b7tung", "ha\u00b7ben", "sol", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bin als die kein Licht mehr schawen", "tokens": ["Ich", "bin", "als", "die", "kein", "Licht", "mehr", "scha\u00b7wen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM", "ART", "PIAT", "NN", "ADV", "VVINF"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Vnd die Verwesung m\u00fcssen bawen.", "tokens": ["Vnd", "die", "Ver\u00b7we\u00b7sung", "m\u00fcs\u00b7sen", "ba\u00b7wen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Du hast von mir dich weg gewand,", "tokens": ["Du", "hast", "von", "mir", "dich", "weg", "ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich lieg' als die so man erschlagen,", "tokens": ["Ich", "lieg'", "als", "die", "so", "man", "er\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "ADV", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mu\u00df Finsterni\u00df und Schrecken tragen", "tokens": ["Mu\u00df", "Fins\u00b7ter\u00b7ni\u00df", "und", "Schre\u00b7cken", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gesondert seyn von deiner Hand.", "tokens": ["Ge\u00b7son\u00b7dert", "seyn", "von", "dei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein Grim und Eiffer hat mein Leben", "tokens": ["Dein", "Grim", "und", "Eif\u00b7fer", "hat", "mein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit deinen Fluthen gantz umbgeben.", "tokens": ["Mit", "dei\u00b7nen", "Flut\u00b7hen", "gantz", "umb\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du l\u00e4ssest mich zum Grewel stehn", "tokens": ["Du", "l\u00e4s\u00b7sest", "mich", "zum", "Gre\u00b7wel", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Freundschafft die mir abgegangen.", "tokens": ["Der", "Freund\u00b7schafft", "die", "mir", "ab\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich lieg' im Stock und hart gefangen,", "tokens": ["Ich", "lieg'", "im", "Stock", "und", "hart", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd wei\u00df nicht Mittel zu entgehn,", "tokens": ["Vnd", "wei\u00df", "nicht", "Mit\u00b7tel", "zu", "ent\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Jammer hat mich so zerrissen", "tokens": ["Der", "Jam\u00b7mer", "hat", "mich", "so", "zer\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich Gestalt und Art mu\u00df missen.", "tokens": ["Da\u00df", "ich", "Ge\u00b7stalt", "und", "Art", "mu\u00df", "mis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich ruffe t\u00e4glich, Herr, zu dir,", "tokens": ["Ich", "ruf\u00b7fe", "t\u00e4g\u00b7lich", ",", "Herr", ",", "zu", "dir", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "NN", "$,", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir breit' ich aus in dem Elende", "tokens": ["Dir", "breit'", "ich", "aus", "in", "dem", "E\u00b7len\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPR", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ohn abla\u00df meine magern H\u00e4nde.", "tokens": ["Ohn", "ab\u00b7la\u00df", "mei\u00b7ne", "ma\u00b7gern", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Grab sieht deiner Wunder Zier,", "tokens": ["Kein", "Grab", "sieht", "dei\u00b7ner", "Wun\u00b7der", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wennstehen auff die schon verdorben?", "tokens": ["Wenn\u00b7ste\u00b7hen", "auff", "die", "schon", "ver\u00b7dor\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn dancken dir, die l\u00e4ngst Verstorben?", "tokens": ["Wenn", "dan\u00b7cken", "dir", ",", "die", "l\u00e4ngst", "Ver\u00b7stor\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "$,", "PRELS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich aber schrey, o GOTT, dich an", "tokens": ["Ich", "a\u00b7ber", "schrey", ",", "o", "GoTT", ",", "dich", "an"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "ADJD", "$,", "FM", "NE", "$,", "PRF", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus meines Hertzens tieffen H\u00f6le.", "tokens": ["Aus", "mei\u00b7nes", "Hert\u00b7zens", "tief\u00b7fen", "H\u00f6\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Warumb verwirffst du meine Seele,", "tokens": ["Wa\u00b7rumb", "ver\u00b7wirffst", "du", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hast dein Gesicht von mir gethan?", "tokens": ["Hast", "dein", "Ge\u00b7sicht", "von", "mir", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcr Elend hab' ich abgenommen", "tokens": ["F\u00fcr", "E\u00b7lend", "hab'", "ich", "ab\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd bin von allen Kr\u00e4fften kommen.", "tokens": ["Vnd", "bin", "von", "al\u00b7len", "Kr\u00e4ff\u00b7ten", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Denn ich zu gar verlassen bin.", "tokens": ["Denn", "ich", "zu", "gar", "ver\u00b7las\u00b7sen", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Schrecken so ich mu\u00df ertragen", "tokens": ["Dein", "Schre\u00b7cken", "so", "ich", "mu\u00df", "er\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zwingt mich bey nahe zu verzagen,", "tokens": ["Zwingt", "mich", "bey", "na\u00b7he", "zu", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Dein Grimm erdruckt und bringt mich hin,", "tokens": ["Dein", "Grimm", "er\u00b7druckt", "und", "bringt", "mich", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "VVPP", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein Schrecken \u00e4ngstigt mich ohn massen,", "tokens": ["Dein", "Schre\u00b7cken", "\u00e4ngs\u00b7tigt", "mich", "ohn", "mas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich auch nicht kan Athem fassen.", "tokens": ["Da\u00df", "ich", "auch", "nicht", "kan", "A\u00b7them", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Sie dringen t\u00e4glich zu mir ein,", "tokens": ["Sie", "drin\u00b7gen", "t\u00e4g\u00b7lich", "zu", "mir", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Wasser werd ich stets umbgeben,", "tokens": ["Wie", "Was\u00b7ser", "werd", "ich", "stets", "umb\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Man trachtet mir nach meinem Leben,", "tokens": ["Man", "trach\u00b7tet", "mir", "nach", "mei\u00b7nem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Freunden mu\u00df ich frembde seyn,", "tokens": ["Den", "Freun\u00b7den", "mu\u00df", "ich", "fremb\u00b7de", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Elend macht mich ohn Verwandten,", "tokens": ["Mein", "E\u00b7lend", "macht", "mich", "ohn", "Ver\u00b7wand\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich bin ein Schewsal den Bekandten.", "tokens": ["Ich", "bin", "ein", "Schew\u00b7sal", "den", "Be\u00b7kand\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}