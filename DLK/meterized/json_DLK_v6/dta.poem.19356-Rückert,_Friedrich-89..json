{"dta.poem.19356": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "89.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den Menschen wenn der Mensch im Menschen stets erkennte,", "tokens": ["Den", "Men\u00b7schen", "wenn", "der", "Mensch", "im", "Men\u00b7schen", "stets", "er\u00b7kenn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "ART", "NN", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So manche Schranke nicht von Menschen Menschen trennte;", "tokens": ["So", "man\u00b7che", "Schran\u00b7ke", "nicht", "von", "Men\u00b7schen", "Men\u00b7schen", "trenn\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PTKNEG", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es w\u00fcrde weniger Mensch gegen Menschen stehn,", "tokens": ["Es", "w\u00fcr\u00b7de", "we\u00b7ni\u00b7ger", "Mensch", "ge\u00b7gen", "Men\u00b7schen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Es w\u00fcrde sich kein Mensch am Menschlichen vergehn.", "tokens": ["Es", "w\u00fcr\u00b7de", "sich", "kein", "Mensch", "am", "Menschli\u00b7chen", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "PIAT", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "Was w\u00fcthet hoch vom Thron herab ein W\u00fctherich?", "tokens": ["Was", "w\u00fct\u00b7het", "hoch", "vom", "Thron", "her\u00b7ab", "ein", "W\u00fct\u00b7he\u00b7rich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADJD", "APPRART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er sieht die Menschen tief gleich Thieren unter sich.", "tokens": ["Er", "sieht", "die", "Men\u00b7schen", "tief", "gleich", "Thie\u00b7ren", "un\u00b7ter", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "ADV", "NN", "APPR", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was gilt dem Muselman f\u00fcr einen Hund der Christ?", "tokens": ["Was", "gilt", "dem", "Mu\u00b7sel\u00b7man", "f\u00fcr", "ei\u00b7nen", "Hund", "der", "Christ", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er sieht es ihm nicht an, da\u00df er sein Bruder ist.", "tokens": ["Er", "sieht", "es", "ihm", "nicht", "an", ",", "da\u00df", "er", "sein", "Bru\u00b7der", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "PTKNEG", "PTKVZ", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Was macht den Wei\u00dfen hart dem Schwarzen gegen\u00fcber?", "tokens": ["Was", "macht", "den", "Wei\u00b7\u00dfen", "hart", "dem", "Schwar\u00b7zen", "ge\u00b7gen\u00b7\u00fc\u00b7ber", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Menschheit Z\u00fcge sind auf dessen Antlitz tr\u00fcber.", "tokens": ["Der", "Menschheit", "Z\u00fc\u00b7ge", "sind", "auf", "des\u00b7sen", "Ant\u00b7litz", "tr\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "APPR", "PRELAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.6": {"line.1": {"text": "Der Arme, Niedre, ha\u00dft den H\u00f6heren, den Reichen,", "tokens": ["Der", "Ar\u00b7me", ",", "Nied\u00b7re", ",", "ha\u00dft", "den", "H\u00f6\u00b7he\u00b7ren", ",", "den", "Rei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PIS", "$,", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil er so wenig selbst sich f\u00fchlt als dessen gleichen.", "tokens": ["Weil", "er", "so", "we\u00b7nig", "selbst", "sich", "f\u00fchlt", "als", "des\u00b7sen", "glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "PRF", "VVFIN", "KOKOM", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Und wer sich jedes Rechts von andern sieht beraubt,", "tokens": ["Und", "wer", "sich", "je\u00b7des", "Rechts", "von", "an\u00b7dern", "sieht", "be\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "PIAT", "NN", "APPR", "PIS", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "H\u00e4lt jedes Unrecht auch sich gegen sie erlaubt.", "tokens": ["H\u00e4lt", "je\u00b7des", "Un\u00b7recht", "auch", "sich", "ge\u00b7gen", "sie", "er\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADV", "PRF", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ihr Menschenw\u00e4chter, drum, wenn ihr wollt ruhig schlafen,", "tokens": ["Ihr", "Men\u00b7schen\u00b7w\u00e4ch\u00b7ter", ",", "drum", ",", "wenn", "ihr", "wollt", "ru\u00b7hig", "schla\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PAV", "$,", "KOUS", "PPER", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Abhelfen m\u00fc\u00dft ihr dem, was ihr nur wollt bestrafen.", "tokens": ["Ab\u00b7hel\u00b7fen", "m\u00fc\u00dft", "ihr", "dem", ",", "was", "ihr", "nur", "wollt", "be\u00b7stra\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ART", "$,", "PWS", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Macht, da\u00df ein Mensch sich k\u00f6nn' und m\u00fc\u00df' als Menschen f\u00fchlen,", "tokens": ["Macht", ",", "da\u00df", "ein", "Mensch", "sich", "k\u00f6nn'", "und", "m\u00fc\u00df'", "als", "Men\u00b7schen", "f\u00fch\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "PRF", "VMFIN", "KON", "VMFIN", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wird er nicht den Grund der Menschheit unterw\u00fchlen.", "tokens": ["So", "wird", "er", "nicht", "den", "Grund", "der", "Menschheit", "un\u00b7ter\u00b7w\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}