{"dta.poem.14217": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcck .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wie jauchzt meine Seele", "tokens": ["Wie", "jauchzt", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und singet in sich!", "tokens": ["Und", "sin\u00b7get", "in", "sich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PRF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Kaum, da\u00df ich's verhehle", "tokens": ["Kaum", ",", "da\u00df", "ich's", "ver\u00b7heh\u00b7le"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PIS", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "So gl\u00fccklich bin ich.", "tokens": ["So", "gl\u00fcck\u00b7lich", "bin", "ich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}