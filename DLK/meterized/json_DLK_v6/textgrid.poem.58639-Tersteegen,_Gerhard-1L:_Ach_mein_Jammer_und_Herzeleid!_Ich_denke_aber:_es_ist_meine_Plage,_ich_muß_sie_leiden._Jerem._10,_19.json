{"textgrid.poem.58639": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach mein Jammer und Herzeleid! Ich denke aber: es ist meine Plage, ich mu\u00df sie leiden. Jerem. 10, 19", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach mein Jammer und Herzeleid! Ich denke aber: es ist meine Plage, ich mu\u00df sie leiden. Jerem. 10, 19", "tokens": ["Ach", "mein", "Jam\u00b7mer", "und", "Her\u00b7ze\u00b7leid", "!", "Ich", "den\u00b7ke", "a\u00b7ber", ":", "es", "ist", "mei\u00b7ne", "Pla\u00b7ge", ",", "ich", "mu\u00df", "sie", "lei\u00b7den", ".", "Je\u00b7rem", ".", "10", ",", "19"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "number", "punct", "number"], "pos": ["ITJ", "PPOSAT", "NN", "KON", "NN", "$.", "PPER", "VVFIN", "ADV", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$.", "NE", "$.", "CARD", "$,", "CARD"], "meter": "+-+--+-+-+-+---+-+--+-+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "O S\u00fcnde, du betr\u00fcglich' Ding, du bringst nur bittre Plagen,", "tokens": ["O", "S\u00fcn\u00b7de", ",", "du", "be\u00b7tr\u00fcg\u00b7lich'", "Ding", ",", "du", "bringst", "nur", "bitt\u00b7re", "Pla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "ADJA", "NN", "$,", "PPER", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Drum mu\u00df ich nun das \u00bbWehe mir!\u00ab, da ich dich f\u00fchle, klagen;", "tokens": ["Drum", "mu\u00df", "ich", "nun", "das", "\u00bb", "We\u00b7he", "mir", "!", "\u00ab", ",", "da", "ich", "dich", "f\u00fch\u00b7le", ",", "kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "ART", "$(", "NN", "PPER", "$.", "$(", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "In M\u00f6rderh\u00e4nde fiel ich ein, da\u00df ich im Blut mich seh,", "tokens": ["In", "M\u00f6r\u00b7der\u00b7h\u00e4n\u00b7de", "fiel", "ich", "ein", ",", "da\u00df", "ich", "im", "Blut", "mich", "seh", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Ich bin geschlagen krank und matt und schreie nur: O weh!", "tokens": ["Ich", "bin", "ge\u00b7schla\u00b7gen", "krank", "und", "matt", "und", "schrei\u00b7e", "nur", ":", "O", "weh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ADJD", "KON", "ADJD", "KON", "VVFIN", "ADV", "$.", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Getrost, o Seele, die du f\u00fchlst die S\u00fcndennot im Herzen,", "tokens": ["Ge\u00b7trost", ",", "o", "See\u00b7le", ",", "die", "du", "f\u00fchlst", "die", "S\u00fcn\u00b7den\u00b7not", "im", "Her\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "PRELS", "PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Denk, Gottes Weisheit t\u00f6tet so die S\u00fcnd' durch S\u00fcndenschmerzen;", "tokens": ["Denk", ",", "Got\u00b7tes", "Weis\u00b7heit", "t\u00f6\u00b7tet", "so", "die", "S\u00fcnd'", "durch", "S\u00fcn\u00b7den\u00b7schmer\u00b7zen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "NN", "VVFIN", "ADV", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Trag deine Plage sanft und still, du bist ja selber schuld,", "tokens": ["Trag", "dei\u00b7ne", "Pla\u00b7ge", "sanft", "und", "still", ",", "du", "bist", "ja", "sel\u00b7ber", "schuld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Und harre nur auf Gottes Hilf' in Demut und Geduld!", "tokens": ["Und", "har\u00b7re", "nur", "auf", "Got\u00b7tes", "Hil\u00b7f'", "in", "De\u00b7mut", "und", "Ge\u00b7duld", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "NE", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+--+---+", "measure": "iambic.hexa.relaxed"}}}}}