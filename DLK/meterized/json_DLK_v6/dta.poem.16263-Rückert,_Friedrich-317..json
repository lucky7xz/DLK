{"dta.poem.16263": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "317.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sonst hat ein hoher Wahn, ein Glaube mich gehoben:", "tokens": ["Sonst", "hat", "ein", "ho\u00b7her", "Wahn", ",", "ein", "Glau\u00b7be", "mich", "ge\u00b7ho\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich m\u00fc\u00dfe leben, weil ich viel noch m\u00fc\u00df' erproben;", "tokens": ["Ich", "m\u00fc\u00b7\u00dfe", "le\u00b7ben", ",", "weil", "ich", "viel", "noch", "m\u00fc\u00df'", "er\u00b7pro\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "KOUS", "PPER", "ADV", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich m\u00fc\u00dfe leben, weil ich viel noch m\u00fc\u00dfe schaffen;", "tokens": ["Ich", "m\u00fc\u00b7\u00dfe", "le\u00b7ben", ",", "weil", "ich", "viel", "noch", "m\u00fc\u00b7\u00dfe", "schaf\u00b7fen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "KOUS", "PPER", "ADV", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun will der hohe Wahn, der Glaube, mir erschlaffen.", "tokens": ["Nun", "will", "der", "ho\u00b7he", "Wahn", ",", "der", "Glau\u00b7be", ",", "mir", "er\u00b7schlaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich f\u00fchle, da\u00df geprobt, geschaffen ist genug;", "tokens": ["Ich", "f\u00fch\u00b7le", ",", "da\u00df", "ge\u00b7probt", ",", "ge\u00b7schaf\u00b7fen", "ist", "ge\u00b7nug", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "VVPP", "$,", "VVPP", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und unterbleiben kann, was \u00fcbrig ist, mit Fug.", "tokens": ["Und", "un\u00b7ter\u00b7blei\u00b7ben", "kann", ",", "was", "\u00fcb\u00b7rig", "ist", ",", "mit", "Fug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}