{"textgrid.poem.54759": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "73. Auf die ungl\u00fcckselige Heyraht D.R.Z.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Hurenj\u00e4ger nimmt den keuschen Sinn zur Eh/", "tokens": ["Der", "Hu\u00b7ren\u00b7j\u00e4\u00b7ger", "nimmt", "den", "keu\u00b7schen", "Sinn", "zur", "Eh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Pengel ", "tokens": ["Der", "Pen\u00b7gel"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Die Grobheit den Verstand. O ungepartes Par", "tokens": ["Die", "Grob\u00b7heit", "den", "Ver\u00b7stand", ".", "O", "un\u00b7ge\u00b7par\u00b7tes", "Par"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$.", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Di\u00df Brautbett ", "tokens": ["Di\u00df", "Braut\u00b7bett"], "token_info": ["word", "word"], "pos": ["PDS", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Du wilst ein Hochzeit-Lid. Ach nein! das dint dir nicht", "tokens": ["Du", "wilst", "ein", "Hoch\u00b7zeit\u00b7Lid", ".", "Ach", "nein", "!", "das", "dint", "dir", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "$.", "NN", "PTKANT", "$.", "PDS", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Erw\u00fcrgten schreibt man nur ein kl\u00e4glich Leich-Gedicht.", "tokens": ["Er\u00b7w\u00fcrg\u00b7ten", "schreibt", "man", "nur", "ein", "kl\u00e4g\u00b7lich", "Leich\u00b7Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Hitze brennet mich schon dreyzehn Wochen lang/", "tokens": ["Die", "Hit\u00b7ze", "bren\u00b7net", "mich", "schon", "drey\u00b7zehn", "Wo\u00b7chen", "lang", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "CARD", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch ist mir mehr vor dein als meinem Grabe bang'.", "tokens": ["Doch", "ist", "mir", "mehr", "vor", "dein", "als", "mei\u00b7nem", "Gra\u00b7be", "bang'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "KOUS", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}