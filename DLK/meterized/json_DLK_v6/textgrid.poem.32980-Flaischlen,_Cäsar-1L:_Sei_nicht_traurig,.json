{"textgrid.poem.32980": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sei nicht traurig,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei nicht traurig,", "tokens": ["Sei", "nicht", "trau\u00b7rig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "sei nicht traurig ...", "tokens": ["sei", "nicht", "trau\u00b7rig", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "es ist heute nur", "tokens": ["es", "ist", "heu\u00b7te", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV"], "meter": "--+-+", "measure": "anapaest.init"}, "line.4": {"text": "so tr\u00fcbe,", "tokens": ["so", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJA", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "es ist heute nur", "tokens": ["es", "ist", "heu\u00b7te", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV"], "meter": "--+-+", "measure": "anapaest.init"}, "line.6": {"text": "so schwer!", "tokens": ["so", "schwer", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Morgen lacht die Sonne wieder,", "tokens": ["Mor\u00b7gen", "lacht", "die", "Son\u00b7ne", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "leuchten Rosen, wei\u00df und rot,", "tokens": ["leuch\u00b7ten", "Ro\u00b7sen", ",", "wei\u00df", "und", "rot", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und mit lauter Lerchenliedern", "tokens": ["und", "mit", "lau\u00b7ter", "Ler\u00b7chen\u00b7lie\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "jubelt's in den hellen Morgen,", "tokens": ["ju\u00b7belt's", "in", "den", "hel\u00b7len", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "jubelt's in den blauen Himmel", "tokens": ["ju\u00b7belt's", "in", "den", "blau\u00b7en", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "siegreich \u00fcber Leid und Not ...", "tokens": ["sieg\u00b7reich", "\u00fc\u00b7ber", "Leid", "und", "Not", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Quillt und schwillt mit jungen Kr\u00e4ften,", "tokens": ["Quillt", "und", "schwillt", "mit", "jun\u00b7gen", "Kr\u00e4f\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "quillt und schwillt mit junger Lust", "tokens": ["quillt", "und", "schwillt", "mit", "jun\u00b7ger", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "lebenswarm dir in die Brust;", "tokens": ["le\u00b7bens\u00b7warm", "dir", "in", "die", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "weckt und wappnet deine Seele", "tokens": ["weckt", "und", "wapp\u00b7net", "dei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "glaubensfroh zu neuer Wehr ...", "tokens": ["glau\u00b7bens\u00b7froh", "zu", "neu\u00b7er", "Wehr", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sei nicht zag drum,", "tokens": ["Sei", "nicht", "zag", "drum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "PAV", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "sei nicht traurig ...", "tokens": ["sei", "nicht", "trau\u00b7rig", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "es ist heute nur", "tokens": ["es", "ist", "heu\u00b7te", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV"], "meter": "--+-+", "measure": "anapaest.init"}, "line.4": {"text": "so tr\u00fcbe,", "tokens": ["so", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJA", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "es ist heute nur", "tokens": ["es", "ist", "heu\u00b7te", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV"], "meter": "--+-+", "measure": "anapaest.init"}, "line.6": {"text": "so schwer!", "tokens": ["so", "schwer", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}}}}}