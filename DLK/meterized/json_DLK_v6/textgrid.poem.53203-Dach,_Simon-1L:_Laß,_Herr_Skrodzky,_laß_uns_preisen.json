{"textgrid.poem.53203": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: La\u00df, Herr Skrodzky, la\u00df uns preisen", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df, Herr Skrodzky, la\u00df uns preisen", "tokens": ["La\u00df", ",", "Herr", "Skrodz\u00b7ky", ",", "la\u00df", "uns", "prei\u00b7sen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "$,", "NN", "NE", "$,", "VVIMP", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diesen Anmuht-reichen Mann,", "tokens": ["Die\u00b7sen", "Anm\u00b7uht\u00b7rei\u00b7chen", "Mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der mit seinen s\u00fc\u00dfen Weisen", "tokens": ["Der", "mit", "sei\u00b7nen", "s\u00fc\u00b7\u00dfen", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohr und Hertz bezaubern kan,", "tokens": ["Ohr", "und", "Hertz", "be\u00b7zau\u00b7bern", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vnd mich g\u00e4ntzlich mir entf\u00fchrt,", "tokens": ["Vnd", "mich", "g\u00e4ntz\u00b7lich", "mir", "ent\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn er nur die Seiten r\u00fchrt.", "tokens": ["Wenn", "er", "nur", "die", "Sei\u00b7ten", "r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hebt er an die sch\u00f6nen Lieder,", "tokens": ["Hebt", "er", "an", "die", "sch\u00f6\u00b7nen", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist mir anders nicht zu muht,", "tokens": ["Ist", "mir", "an\u00b7ders", "nicht", "zu", "muht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "PTKZU", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Al\u00df wenn \u00fcber meine Glieder", "tokens": ["Al\u00df", "wenn", "\u00fc\u00b7ber", "mei\u00b7ne", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Herlieff' eine kalte Fluht,", "tokens": ["Her\u00b7lief\u00b7f'", "ei\u00b7ne", "kal\u00b7te", "Fluht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Daher ich in meinem Sinn", "tokens": ["Da\u00b7her", "ich", "in", "mei\u00b7nem", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Itzt betr\u00fcbt, itzt fr\u00f6hlich bin.", "tokens": ["Itzt", "be\u00b7tr\u00fcbt", ",", "itzt", "fr\u00f6h\u00b7lich", "bin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Also hat auff Hemus H\u00fcgel", "tokens": ["Al\u00b7so", "hat", "auff", "He\u00b7mus", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Orpheus selber nicht gespielt,", "tokens": ["Or\u00b7pheus", "sel\u00b7ber", "nicht", "ge\u00b7spielt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Al\u00df er wie im strengen Z\u00fcgel", "tokens": ["Al\u00df", "er", "wie", "im", "stren\u00b7gen", "Z\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KOKOM", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fl\u00fc\u00dfe, Sturm' und W\u00e4lder hielt,", "tokens": ["Fl\u00fc\u00b7\u00dfe", ",", "Sturm'", "und", "W\u00e4l\u00b7der", "hielt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vnd sein angenehmer Klangk", "tokens": ["Vnd", "sein", "an\u00b7ge\u00b7neh\u00b7mer", "Klangk"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Selbst der H\u00f6llen Reich bezwangk.", "tokens": ["Selbst", "der", "H\u00f6l\u00b7len", "Reich", "be\u00b7zwangk", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ja ich halt' Amphions Leyer", "tokens": ["Ja", "ich", "halt'", "Am\u00b7phi\u00b7ons", "Le\u00b7yer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "PPER", "VVFIN", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Blo\u00df f\u00fcr einen Tichter-Tand,", "tokens": ["Blo\u00df", "f\u00fcr", "ei\u00b7nen", "Tich\u00b7ter\u00b7Tand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sie, Thebe, dein Gem\u00e4uer", "tokens": ["Da\u00df", "sie", ",", "The\u00b7be", ",", "dein", "Ge\u00b7m\u00e4u\u00b7er"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "$,", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat gebracht in seinen Stand,", "tokens": ["Hat", "ge\u00b7bracht", "in", "sei\u00b7nen", "Stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vnd da\u00df willig auch die Stein'", "tokens": ["Vnd", "da\u00df", "wil\u00b7lig", "auch", "die", "Stein'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADJD", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihr gefolget sollen seyn.", "tokens": ["Ihr", "ge\u00b7fol\u00b7get", "sol\u00b7len", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VMFIN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "W\u00e4r' es war, B\u0105czalski w\u00fcrde", "tokens": ["W\u00e4r'", "es", "war", ",", "B\u0105czal\u00b7ski", "w\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "VAFIN", "$,", "NE", "VAFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Durch der Seiten S\u00fc\u00dfigkeit", "tokens": ["Durch", "der", "Sei\u00b7ten", "S\u00fc\u00b7\u00dfig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weg thun dieser Waffen B\u00fcrde", "tokens": ["Weg", "thun", "die\u00b7ser", "Waf\u00b7fen", "B\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "PDAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vnd beg\u00fcten allen Streit,", "tokens": ["Vnd", "be\u00b7g\u00fc\u00b7ten", "al\u00b7len", "Streit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der die Degen gr\u00fcmmig wetzt,", "tokens": ["Der", "die", "De\u00b7gen", "gr\u00fcm\u00b7mig", "wetzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nord und Ost zusammen hetzt.", "tokens": ["Nord", "und", "Ost", "zu\u00b7sam\u00b7men", "hetzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Weil Amphions Spiel bey weiten", "tokens": ["Weil", "Am\u00b7phi\u00b7ons", "Spiel", "bey", "wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "NN", "APPR", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Diesem es nicht gleich gethan,", "tokens": ["Die\u00b7sem", "es", "nicht", "gleich", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "PPER", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Also sind Apollos Seiten,", "tokens": ["Al\u00b7so", "sind", "A\u00b7pol\u00b7los", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Also spielt nach meinem Wahn", "tokens": ["Al\u00b7so", "spielt", "nach", "mei\u00b7nem", "Wahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Clio auff dem Helicon,", "tokens": ["Clio", "auff", "dem", "He\u00b7li\u00b7con", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Wenn sie r\u00fchmet Pelus Sohn.", "tokens": ["Wenn", "sie", "r\u00fch\u00b7met", "Pe\u00b7lus", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Aber ach! Die wilden Waffen", "tokens": ["A\u00b7ber", "ach", "!", "Die", "wil\u00b7den", "Waf\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6ren nicht was Ph\u00f6bus singt,", "tokens": ["H\u00f6\u00b7ren", "nicht", "was", "Ph\u00f6\u00b7bus", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PWS", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd was kan Thalia schaffen,", "tokens": ["Vnd", "was", "kan", "Tha\u00b7lia", "schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn die Heer-Posaun erklingt", "tokens": ["Wenn", "die", "Heer\u00b7Po\u00b7saun", "er\u00b7klingt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vnd der Feld-Carthaunen Brand", "tokens": ["Vnd", "der", "Feld\u00b7Cart\u00b7hau\u00b7nen", "Brand"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "St\u00e4dte kehrt in ebnes Land?", "tokens": ["St\u00e4d\u00b7te", "kehrt", "in", "eb\u00b7nes", "Land", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Unterde\u00dfen lasst uns singen", "tokens": ["Un\u00b7ter\u00b7de\u00b7\u00dfen", "lasst", "uns", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trotz der Trummeln und der Schlacht,", "tokens": ["Trotz", "der", "Trum\u00b7meln", "und", "der", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sie g\u00e4ntzlich sollen zwingen", "tokens": ["Da\u00df", "sie", "g\u00e4ntz\u00b7lich", "sol\u00b7len", "zwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was Euterpen Klangk erdacht!", "tokens": ["Was", "Eu\u00b7ter\u00b7pen", "Klangk", "er\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "NN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Mars, Gesch\u00fctz, und alles f\u00e4llt", "tokens": ["Mars", ",", "Ge\u00b7sch\u00fctz", ",", "und", "al\u00b7les", "f\u00e4llt"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "KON", "PIS", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was Thalia nicht erh\u00e4llt.", "tokens": ["Was", "Tha\u00b7lia", "nicht", "er\u00b7h\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Lasst Euch, Herr B\u0105czalski, h\u00f6ren,", "tokens": ["Lasst", "Euch", ",", "Herr", "B\u0105czal\u00b7ski", ",", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "NE", "$,", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Herr Skrodzki schl\u00e4gt darein", "tokens": ["Mein", "Herr", "Skrodz\u00b7ki", "schl\u00e4gt", "da\u00b7rein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NE", "VVFIN", "PAV"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Vnd wil euren Ruhm vermehren,", "tokens": ["Vnd", "wil", "eu\u00b7ren", "Ruhm", "ver\u00b7meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er nicht sol sterblich seyn,", "tokens": ["Da\u00df", "er", "nicht", "sol", "sterb\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Euer Spiel f\u00fchlt noch die Zeit,", "tokens": ["Eu\u00b7er", "Spiel", "f\u00fchlt", "noch", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Er giebt ihm die Ewigkeit.", "tokens": ["Er", "giebt", "ihm", "die", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Wie der Rosen edle Bl\u00fcte,", "tokens": ["Wie", "der", "Ro\u00b7sen", "ed\u00b7le", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ihr in das Wappen nehmt,", "tokens": ["Die", "ihr", "in", "das", "Wap\u00b7pen", "nehmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Absticht aller Blumen G\u00fcte,", "tokens": ["Ab\u00b7sticht", "al\u00b7ler", "Blu\u00b7men", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie Natur die Kunst besch\u00e4mt,", "tokens": ["Wie", "Na\u00b7tur", "die", "Kunst", "be\u00b7sch\u00e4mt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Also, singt er, Zwingt auch ihr", "tokens": ["Al\u00b7so", ",", "singt", "er", ",", "Zwingt", "auch", "ihr"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "VVFIN", "ADV", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Aller andern K\u00fcnstler Zier.", "tokens": ["Al\u00b7ler", "an\u00b7dern", "K\u00fcnst\u00b7ler", "Zier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}