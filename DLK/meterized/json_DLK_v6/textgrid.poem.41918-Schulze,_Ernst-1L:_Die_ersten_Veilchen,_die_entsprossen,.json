{"textgrid.poem.41918": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die ersten Veilchen, die entsprossen,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die ersten Veilchen, die entsprossen,", "tokens": ["Die", "ers\u00b7ten", "Veil\u00b7chen", ",", "die", "ent\u00b7spros\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du nahmst sie an und danktest still;", "tokens": ["Du", "nahmst", "sie", "an", "und", "dank\u00b7test", "still", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch heut ist deine Th\u00fcr verschlossen,", "tokens": ["Doch", "heut", "ist", "dei\u00b7ne", "Th\u00fcr", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da ich die letzten bringen will.", "tokens": ["Da", "ich", "die", "letz\u00b7ten", "brin\u00b7gen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die ersten wollten kaum entkeimen,", "tokens": ["Die", "ers\u00b7ten", "woll\u00b7ten", "kaum", "ent\u00b7kei\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die letzten wollen schon vergehn;", "tokens": ["Die", "letz\u00b7ten", "wol\u00b7len", "schon", "ver\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So hab' ich auch von meinen Tr\u00e4umen", "tokens": ["So", "hab'", "ich", "auch", "von", "mei\u00b7nen", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die volle Bl\u00fcthe nicht gesehn.", "tokens": ["Die", "vol\u00b7le", "Bl\u00fc\u00b7the", "nicht", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch meine Tr\u00e4ume bl\u00fchn und leben", "tokens": ["Doch", "mei\u00b7ne", "Tr\u00e4u\u00b7me", "bl\u00fchn", "und", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In leisen Liedern noch f\u00fcr dich;", "tokens": ["In", "lei\u00b7sen", "Lie\u00b7dern", "noch", "f\u00fcr", "dich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Veilchen k\u00f6nnen nichts mehr geben,", "tokens": ["Die", "Veil\u00b7chen", "k\u00f6n\u00b7nen", "nichts", "mehr", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn matt ihr zartes Haupt verblich.", "tokens": ["Wenn", "matt", "ihr", "zar\u00b7tes", "Haupt", "ver\u00b7blich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPOSAT", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Jetzt welken sie in kaltem Regen,", "tokens": ["Jetzt", "wel\u00b7ken", "sie", "in", "kal\u00b7tem", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weil ich sie fort ins Dunkel warf;", "tokens": ["Weil", "ich", "sie", "fort", "ins", "Dun\u00b7kel", "warf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKVZ", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht mag ich Sch\u00f6nes sehn und pflegen,", "tokens": ["Nicht", "mag", "ich", "Sch\u00f6\u00b7nes", "sehn", "und", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PPER", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn ich es dir nicht bieten darf.", "tokens": ["Wenn", "ich", "es", "dir", "nicht", "bie\u00b7ten", "darf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}