{"dta.poem.7427": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Das Lied vom Hasse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wohlauf, wohlauf, \u00fcber Berg und Flu\u00df", "tokens": ["Wohl\u00b7auf", ",", "wohl\u00b7auf", ",", "\u00fc\u00b7ber", "Berg", "und", "Flu\u00df"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dem Morgenrot entgegen!", "tokens": ["Dem", "Mor\u00b7gen\u00b7rot", "ent\u00b7ge\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dem treuen Weib den letzten Ku\u00df,", "tokens": ["Dem", "treu\u00b7en", "Weib", "den", "letz\u00b7ten", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dann zum treuen Degen!", "tokens": ["Und", "dann", "zum", "treu\u00b7en", "De\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bis unsre Hand in Asche stiebt,", "tokens": ["Bis", "uns\u00b7re", "Hand", "in", "A\u00b7sche", "stiebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Soll sie vom Schwert nicht lassen;", "tokens": ["Soll", "sie", "vom", "Schwert", "nicht", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wir haben lang genug geliebt,", "tokens": ["Wir", "ha\u00b7ben", "lang", "ge\u00b7nug", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und wollen endlich hassen!", "tokens": ["Und", "wol\u00b7len", "end\u00b7lich", "has\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Liebe kann uns helfen nicht,", "tokens": ["Die", "Lie\u00b7be", "kann", "uns", "hel\u00b7fen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Liebe nicht erretten;", "tokens": ["Die", "Lie\u00b7be", "nicht", "er\u00b7ret\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Halt Du, o Ha\u00df, Dein j\u00fcngst Gericht,", "tokens": ["Halt", "Du", ",", "o", "Ha\u00df", ",", "Dein", "j\u00fcngst", "Ge\u00b7richt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "FM", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Brich Du, o Ha\u00df, die Ketten!", "tokens": ["Brich", "Du", ",", "o", "Ha\u00df", ",", "die", "Ket\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "FM", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und wo es noch Tyrannen gibt,", "tokens": ["Und", "wo", "es", "noch", "Ty\u00b7ran\u00b7nen", "gibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die la\u00dft uns keck erfassen;", "tokens": ["Die", "la\u00dft", "uns", "keck", "er\u00b7fas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wir haben lang genug geliebt,", "tokens": ["Wir", "ha\u00b7ben", "lang", "ge\u00b7nug", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und wollen endlich hassen!", "tokens": ["Und", "wol\u00b7len", "end\u00b7lich", "has\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wer noch ein Herz besitzt, dem soll's", "tokens": ["Wer", "noch", "ein", "Herz", "be\u00b7sitzt", ",", "dem", "soll's"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "ADV", "ART", "NN", "VVPP", "$,", "PRELS", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Hasse nur sich r\u00fchren;", "tokens": ["Im", "Has\u00b7se", "nur", "sich", "r\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "All\u00fcberall ist d\u00fcrres Holz,", "tokens": ["Al\u00b7l\u00fc\u00b7be\u00b7rall", "ist", "d\u00fcr\u00b7res", "Holz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um unsre Glut zu sch\u00fcren.", "tokens": ["Um", "uns\u00b7re", "Glut", "zu", "sch\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die ihr der Freiheit noch verbliebt,", "tokens": ["Die", "ihr", "der", "Frei\u00b7heit", "noch", "ver\u00b7bliebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Singt durch die deutschen Strassen:", "tokens": ["Singt", "durch", "die", "deut\u00b7schen", "Stras\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u201eihr habet lang genug geliebt,", "tokens": ["\u201e", "ihr", "ha\u00b7bet", "lang", "ge\u00b7nug", "ge\u00b7liebt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "O lernet endlich hassen!\u201c", "tokens": ["O", "ler\u00b7net", "end\u00b7lich", "has\u00b7sen", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Bek\u00e4mpfet sie ohn' Unterla\u00df,", "tokens": ["Be\u00b7k\u00e4mp\u00b7fet", "sie", "ohn'", "Un\u00b7ter\u00b7la\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Tyrannei, auf Erden,", "tokens": ["Die", "Ty\u00b7ran\u00b7nei", ",", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Als unsre Liebe, werden.", "tokens": ["Als", "uns\u00b7re", "Lie\u00b7be", ",", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bis unsre Hand in Asche stiebt,", "tokens": ["Bis", "uns\u00b7re", "Hand", "in", "A\u00b7sche", "stiebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Soll sie vom Schwert nicht lassen;", "tokens": ["Soll", "sie", "vom", "Schwert", "nicht", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wir haben lang genug geliebt,", "tokens": ["Wir", "ha\u00b7ben", "lang", "ge\u00b7nug", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und wollen endlich hassen!", "tokens": ["Und", "wol\u00b7len", "end\u00b7lich", "has\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}