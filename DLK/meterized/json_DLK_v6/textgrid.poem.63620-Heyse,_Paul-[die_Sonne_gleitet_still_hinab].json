{"textgrid.poem.63620": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "[die Sonne gleitet still hinab]", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne gleitet still hinab", "tokens": ["Die", "Son\u00b7ne", "glei\u00b7tet", "still", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ins Wellengrab.", "tokens": ["Ins", "Wel\u00b7len\u00b7grab", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ein feiner falber Schleier f\u00e4llt", "tokens": ["Ein", "fei\u00b7ner", "fal\u00b7ber", "Schlei\u00b7er", "f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Rings auf die Welt.", "tokens": ["Rings", "auf", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Am blauen Bergeshorizont", "tokens": ["Am", "blau\u00b7en", "Ber\u00b7ges\u00b7ho\u00b7ri\u00b7zont"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gl\u00fcht auf der Mond.", "tokens": ["Gl\u00fcht", "auf", "der", "Mond", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Es hellt sein d\u00fcsterwildes Licht", "tokens": ["Es", "hellt", "sein", "d\u00fcs\u00b7ter\u00b7wil\u00b7des", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Tr\u00fcbe nicht.", "tokens": ["Die", "Tr\u00fc\u00b7be", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Wir wandeln traurig Hand in Hand", "tokens": ["Wir", "wan\u00b7deln", "trau\u00b7rig", "Hand", "in", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durchs Totenland.", "tokens": ["Durchs", "To\u00b7ten\u00b7land", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Was jedes denkt so weit von Haus,", "tokens": ["Was", "je\u00b7des", "denkt", "so", "weit", "von", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Spricht keines aus.", "tokens": ["Spricht", "kei\u00b7nes", "aus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ein Nachglanz von verlornem Gl\u00fcck", "tokens": ["Ein", "Nach\u00b7glanz", "von", "ver\u00b7lor\u00b7nem", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Blieb uns zur\u00fcck \u2013", "tokens": ["Blieb", "uns", "zu\u00b7r\u00fcck", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Es hellt sein rotverweintes Licht", "tokens": ["Es", "hellt", "sein", "rot\u00b7ver\u00b7wein\u00b7tes", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Tr\u00fcbe nicht!", "tokens": ["Die", "Tr\u00fc\u00b7be", "nicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}