{"textgrid.poem.62505": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Am Neujahrstage", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit welcher v\u00e4terlichen Huld,", "tokens": ["Mit", "wel\u00b7cher", "v\u00e4\u00b7ter\u00b7li\u00b7chen", "Huld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit welcher Liebe und Geduld", "tokens": ["Mit", "wel\u00b7cher", "Lie\u00b7be", "und", "Ge\u00b7duld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hast du im letzten Jahr,", "tokens": ["Hast", "du", "im", "letz\u00b7ten", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mein Sch\u00f6pfer, deinen Knecht bedacht,", "tokens": ["Mein", "Sch\u00f6p\u00b7fer", ",", "dei\u00b7nen", "Knecht", "be\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gen\u00e4hrt, gekleidet und bewacht,", "tokens": ["Ge\u00b7n\u00e4hrt", ",", "ge\u00b7klei\u00b7det", "und", "be\u00b7wacht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In jeglicher Gefahr!", "tokens": ["In", "jeg\u00b7li\u00b7cher", "Ge\u00b7fahr", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sei ferner gn\u00e4dig meiner Noth,", "tokens": ["Sei", "fer\u00b7ner", "gn\u00e4\u00b7dig", "mei\u00b7ner", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schickst du mir den Engel Tod", "tokens": ["Und", "schickst", "du", "mir", "den", "En\u00b7gel", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In diesem Jahreslauf \u2013", "tokens": ["In", "die\u00b7sem", "Jah\u00b7res\u00b7lauf", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie du dem Lebenden gethan,", "tokens": ["Wie", "du", "dem", "Le\u00b7ben\u00b7den", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nimm dann des Sterbenden dich an", "tokens": ["Nimm", "dann", "des", "Ster\u00b7ben\u00b7den", "dich", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ART", "NN", "PPER", "PTKVZ"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Und meine Seele auf! \u2013", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "auf", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}