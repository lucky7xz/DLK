{"textgrid.poem.26788": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Sachm\u00e4nner", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbgeistreich nennt man dies Werk? Wir k\u00f6nnen ja nichts daraus sch\u00f6pfen.\u00ab", "tokens": ["\u00bb", "geist\u00b7reich", "nennt", "man", "dies", "Werk", "?", "Wir", "k\u00f6n\u00b7nen", "ja", "nichts", "da\u00b7raus", "sch\u00f6p\u00b7fen", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PIS", "PDS", "NN", "$.", "PPER", "VMFIN", "ADV", "PIS", "PAV", "VVINF", "$.", "$("], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Toren ihr! W\u00e4r es denn Geist, fing man in Eimern es auf.", "tokens": ["To\u00b7ren", "ihr", "!", "W\u00e4r", "es", "denn", "Geist", ",", "fing", "man", "in", "Ei\u00b7mern", "es", "auf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "VAFIN", "PPER", "ADV", "NN", "$,", "VVFIN", "PIS", "APPR", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Euch ist alles ein Nichts, was man mit Scheffeln nicht misset,", "tokens": ["Euch", "ist", "al\u00b7les", "ein", "Nichts", ",", "was", "man", "mit", "Schef\u00b7feln", "nicht", "mis\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ART", "PIS", "$,", "PRELS", "PIS", "APPR", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+--+-+-+-+++", "measure": "trochaic.octa.plus.relaxed"}, "line.4": {"text": "Was man in B\u00fcndel nicht packt, was man in Speichern nicht h\u00e4uft.", "tokens": ["Was", "man", "in", "B\u00fcn\u00b7del", "nicht", "packt", ",", "was", "man", "in", "Spei\u00b7chern", "nicht", "h\u00e4uft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPR", "NN", "PTKNEG", "VVFIN", "$,", "PRELS", "PIS", "APPR", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}}}}}