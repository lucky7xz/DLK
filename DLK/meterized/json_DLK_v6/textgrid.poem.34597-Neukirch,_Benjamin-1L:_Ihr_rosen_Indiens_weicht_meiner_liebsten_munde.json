{"textgrid.poem.34597": {"metadata": {"author": {"name": "Neukirch, Benjamin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr rosen Indiens/ weicht meiner liebsten munde/", "genre": "verse", "period": "N.A.", "pub_year": 1697, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr rosen Indiens/ weicht meiner liebsten munde/", "tokens": ["Ihr", "ro\u00b7sen", "In\u00b7di\u00b7ens", "/", "weicht", "mei\u00b7ner", "liebs\u00b7ten", "mun\u00b7de", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr balsam-blumen r\u00fchmt mir euren honig nicht/", "tokens": ["Ihr", "bal\u00b7sam\u00b7blu\u00b7men", "r\u00fchmt", "mir", "eu\u00b7ren", "ho\u00b7nig", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Eur glantz und eur geschmack vergeht in einer stunde/", "tokens": ["Eur", "glantz", "und", "eur", "ge\u00b7schmack", "ver\u00b7geht", "in", "ei\u00b7ner", "stun\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihr mund wird aber stets von neuem angericht.", "tokens": ["Ihr", "mund", "wird", "a\u00b7ber", "stets", "von", "neu\u00b7em", "an\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "APPR", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ie mehr ich rosen schau/ ie sch\u00f6nre seh ich bl\u00fchen;", "tokens": ["Ie", "mehr", "ich", "ro\u00b7sen", "schau", "/", "ie", "sch\u00f6n\u00b7re", "seh", "ich", "bl\u00fc\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "VVFIN", "PTKVZ", "$(", "ADV", "VVFIN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ie mehr ich ihn gek\u00fcst/ ie s\u00fcsser schmeckt der safft/", "tokens": ["Ie", "mehr", "ich", "ihn", "ge\u00b7k\u00fcst", "/", "ie", "s\u00fcs\u00b7ser", "schmeckt", "der", "safft", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "PPER", "VVPP", "$(", "ADV", "ADJD", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sein purpur kan mein blut zwar aus den adern ziehen;", "tokens": ["Sein", "pur\u00b7pur", "kan", "mein", "blut", "zwar", "aus", "den", "a\u00b7dern", "zie\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VMFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich aber geb ihm nur durch meine geister krafft:", "tokens": ["Ich", "a\u00b7ber", "geb", "ihm", "nur", "durch", "mei\u00b7ne", "geis\u00b7ter", "krafft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach da\u00df er Sylvia doch eher nicht verd\u00fcrbe/", "tokens": ["Ach", "da\u00df", "er", "Syl\u00b7via", "doch", "e\u00b7her", "nicht", "ver\u00b7d\u00fcr\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPER", "NE", "ADV", "ADV", "PTKNEG", "VVFIN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "Als bi\u00df ich k\u00fcssens satt auff seinen lippen st\u00fcrbe!", "tokens": ["Als", "bi\u00df", "ich", "k\u00fcs\u00b7sens", "satt", "auff", "sei\u00b7nen", "lip\u00b7pen", "st\u00fcr\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}