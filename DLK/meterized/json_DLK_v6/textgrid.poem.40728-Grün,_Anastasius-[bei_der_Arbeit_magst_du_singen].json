{"textgrid.poem.40728": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "[bei der Arbeit magst du singen]", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bei der Arbeit magst du singen,", "tokens": ["Bei", "der", "Ar\u00b7beit", "magst", "du", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das verleiht der Arbeit Schwingen;", "tokens": ["Das", "ver\u00b7leiht", "der", "Ar\u00b7beit", "Schwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Singen doch nie Arbeit sei,", "tokens": ["Sin\u00b7gen", "doch", "nie", "Ar\u00b7beit", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sohlen tr\u00e4gt sie dann von Blei.", "tokens": ["Soh\u00b7len", "tr\u00e4gt", "sie", "dann", "von", "Blei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}