{"dta.poem.23817": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Charlatan.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin auf diesem Plan mit Theriack erschienen/", "tokens": ["Ich", "bin", "auf", "die\u00b7sem", "Plan", "mit", "The\u00b7riack", "er\u00b7schie\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PDAT", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit Balsam und Extract/ ich gebe guten Kauff;", "tokens": ["Mit", "Bal\u00b7sam", "und", "E\u00b7xtract", "/", "ich", "ge\u00b7be", "gu\u00b7ten", "Kauff", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$(", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es komme wie es wil/ h\u00f6rt gleich mein Handel auf/", "tokens": ["Es", "kom\u00b7me", "wie", "es", "wil", "/", "h\u00f6rt", "gleich", "mein", "Han\u00b7del", "auf", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PPER", "VMFIN", "$(", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So kan - - - mir neues Geld verdienen.", "tokens": ["So", "kan", "mir", "neu\u00b7es", "Geld", "ver\u00b7die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$(", "$(", "$(", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich bin auf Schacherey und auf Betrug bedacht/", "tokens": ["Ich", "bin", "auf", "Scha\u00b7che\u00b7rey", "und", "auf", "Be\u00b7trug", "be\u00b7dacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und manchen falschen Stein hab ich schon angebracht.", "tokens": ["Und", "man\u00b7chen", "fal\u00b7schen", "Stein", "hab", "ich", "schon", "an\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Lacht nicht/ ihr die ihr seht zwey Weiber mit mir wan-", "tokens": ["Lacht", "nicht", "/", "ihr", "die", "ihr", "seht", "zwey", "Wei\u00b7ber", "mit", "mir", "wan"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$(", "PPOSAT", "PRELS", "PPER", "VVFIN", "CARD", "NN", "APPR", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wer Lust zu kauffen hat/ kan eine von mir handeln.", "tokens": ["Wer", "Lust", "zu", "kauf\u00b7fen", "hat", "/", "kan", "ei\u00b7ne", "von", "mir", "han\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PTKZU", "VVINF", "VAFIN", "$(", "VMFIN", "ART", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}