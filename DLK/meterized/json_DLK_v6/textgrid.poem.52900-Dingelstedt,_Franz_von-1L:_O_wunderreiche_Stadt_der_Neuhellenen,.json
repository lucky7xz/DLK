{"textgrid.poem.52900": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: O wunderreiche Stadt der Neuhellenen,", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wunderreiche Stadt der Neuhellenen,", "tokens": ["O", "wun\u00b7der\u00b7rei\u00b7che", "Stadt", "der", "Neu\u00b7hel\u00b7le\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Apollo-Antlitz mit Silenos-Finnen,", "tokens": ["A\u00b7pol\u00b7lo\u00b7Ant\u00b7litz", "mit", "Si\u00b7le\u00b7nos\u00b7Fin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Kom\u00f6dienhaus, voll neuen Tr\u00f6dels innen,", "tokens": ["Ko\u00b7m\u00f6\u00b7di\u00b7en\u00b7haus", ",", "voll", "neu\u00b7en", "Tr\u00f6\u00b7dels", "in\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Au\u00dfen bemalt mit hochantiken Szenen!", "tokens": ["Au\u00b7\u00dfen", "be\u00b7malt", "mit", "hoch\u00b7an\u00b7ti\u00b7ken", "Sze\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Pfaffe deklamiert statt Demosthenen,", "tokens": ["Ein", "Pfaf\u00b7fe", "de\u00b7kla\u00b7miert", "statt", "De\u00b7mos\u00b7the\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das Kuchelmensch ersetzt die Charitinnen", "tokens": ["Das", "Ku\u00b7chel\u00b7mensch", "er\u00b7setzt", "die", "Cha\u00b7ri\u00b7tin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und schenkt den ewig durst'gen Pierinnen", "tokens": ["Und", "schenkt", "den", "e\u00b7wig", "dur\u00b7st'\u00b7gen", "Pie\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bock und Salvator, ihre Hippokrenen.", "tokens": ["Bock", "und", "Sal\u00b7va\u00b7tor", ",", "ih\u00b7re", "Hip\u00b7po\u00b7kre\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Stellt doch ans Tor als st\u00e4dtisches Gewappen", "tokens": ["Stellt", "doch", "ans", "Tor", "als", "st\u00e4d\u00b7ti\u00b7sches", "Ge\u00b7wap\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein griechisch G\u00f6tterbild mit kahlem Scheitel,", "tokens": ["Ein", "grie\u00b7chisch", "G\u00f6t\u00b7ter\u00b7bild", "mit", "kah\u00b7lem", "Schei\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Worauf der Inful bunte H\u00e4ngelappen;", "tokens": ["Wo\u00b7rauf", "der", "In\u00b7ful", "bun\u00b7te", "H\u00e4n\u00b7ge\u00b7lap\u00b7pen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Gebt in die Linke ihm ein leeres Seidel,", "tokens": ["Gebt", "in", "die", "Lin\u00b7ke", "ihm", "ein", "lee\u00b7res", "Sei\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihm in die Rechte drei St\u00fcck Kruzifixe: \u2013", "tokens": ["Ihm", "in", "die", "Rech\u00b7te", "drei", "St\u00fcck", "Kru\u00b7zi\u00b7fi\u00b7xe", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "CARD", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "So habt ihr M\u00fcnchen im modernen Wixe!", "tokens": ["So", "habt", "ihr", "M\u00fcn\u00b7chen", "im", "mo\u00b7der\u00b7nen", "Wi\u00b7xe", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--++--+", "measure": "iambic.penta.relaxed"}}}}}