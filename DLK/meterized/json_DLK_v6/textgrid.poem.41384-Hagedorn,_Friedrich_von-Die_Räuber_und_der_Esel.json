{"textgrid.poem.41384": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die R\u00e4uber und der Esel", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zween R\u00e4uber zankten sich", "tokens": ["Zween", "R\u00e4u\u00b7ber", "zank\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Des gestohlnen Esels wegen,", "tokens": ["Des", "ge\u00b7stohl\u00b7nen", "E\u00b7sels", "we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und von Worten kam's zu Schl\u00e4gen,", "tokens": ["Und", "von", "Wor\u00b7ten", "kam's", "zu", "Schl\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "NE", "APPR", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Beide fochten ritterlich.", "tokens": ["Bei\u00b7de", "foch\u00b7ten", "rit\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Als nun jeder in dem Streite", "tokens": ["Als", "nun", "je\u00b7der", "in", "dem", "Strei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PIS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seinen Feind aufs sch\u00e4rfste trieb;", "tokens": ["Sei\u00b7nen", "Feind", "aufs", "sch\u00e4rfs\u00b7te", "trieb", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nahte sich ein kl\u00fcgrer Dieb,", "tokens": ["Nah\u00b7te", "sich", "ein", "kl\u00fcg\u00b7rer", "Dieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und entging mit ihrer Beute.", "tokens": ["Und", "ent\u00b7ging", "mit", "ih\u00b7rer", "Beu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Diesem Esel gleicht ein Staat,", "tokens": ["Die\u00b7sem", "E\u00b7sel", "gleicht", "ein", "Staat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der den R\u00e4ubern der Provinzen,", "tokens": ["Der", "den", "R\u00e4u\u00b7bern", "der", "Pro\u00b7vin\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zween neuverbundnen Prinzen,", "tokens": ["Zween", "neu\u00b7ver\u00b7bund\u00b7nen", "Prin\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zeitig sich ergeben hat.", "tokens": ["Zei\u00b7tig", "sich", "er\u00b7ge\u00b7ben", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Beide zanken sich oft m\u00fcde,", "tokens": ["Bei\u00b7de", "zan\u00b7ken", "sich", "oft", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil die Herrschsucht trotzig ist;", "tokens": ["Weil", "die", "Herrschsucht", "trot\u00b7zig", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch ein Dritter stillt den Zwist,", "tokens": ["Doch", "ein", "Drit\u00b7ter", "stillt", "den", "Zwist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimmt das Land, und machet Friede.", "tokens": ["Nimmt", "das", "Land", ",", "und", "ma\u00b7chet", "Frie\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}