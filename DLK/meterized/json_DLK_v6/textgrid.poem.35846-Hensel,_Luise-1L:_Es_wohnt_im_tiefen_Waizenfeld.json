{"textgrid.poem.35846": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es wohnt im tiefen Waizenfeld", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wohnt im tiefen Waizenfeld", "tokens": ["Es", "wohnt", "im", "tie\u00b7fen", "Wai\u00b7zen\u00b7feld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein V\u00f6glein, treu gesinnt,", "tokens": ["Ein", "V\u00f6\u00b7glein", ",", "treu", "ge\u00b7sinnt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das eine ernste Frage stellt", "tokens": ["Das", "ei\u00b7ne", "erns\u00b7te", "Fra\u00b7ge", "stellt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jedwedem Menschenkind:", "tokens": ["Jed\u00b7we\u00b7dem", "Men\u00b7schen\u00b7kind", ":"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Wenn kaum der junge Morgen graut", "tokens": ["Wenn", "kaum", "der", "jun\u00b7ge", "Mor\u00b7gen", "graut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und frisch die Fr\u00fchluft weht,", "tokens": ["Und", "frisch", "die", "Fr\u00fch\u00b7luft", "weht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weckt dich des V\u00f6gleins muntrer Laut", "tokens": ["Weckt", "dich", "des", "V\u00f6\u00b7gleins", "mun\u00b7trer", "Laut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu Arbeit und Gebet:", "tokens": ["Zu", "Ar\u00b7beit", "und", "Ge\u00b7bet", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Eilst du dahin mit leichtem Fu\u00df", "tokens": ["Eilst", "du", "da\u00b7hin", "mit", "leich\u00b7tem", "Fu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Tanz und Spiel und Scherz,", "tokens": ["Zu", "Tanz", "und", "Spiel", "und", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schl\u00e4gt ernst des V\u00f6gleins Mahnungsgru\u00df", "tokens": ["Schl\u00e4gt", "ernst", "des", "V\u00f6\u00b7gleins", "Mah\u00b7nungs\u00b7gru\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An's weltzerstreute Herz:", "tokens": ["An's", "welt\u00b7zer\u00b7streu\u00b7te", "Herz", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und schleichst du tr\u00fcb' und la\u00df daher,", "tokens": ["Und", "schleichst", "du", "tr\u00fcb'", "und", "la\u00df", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "KON", "VVIMP", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Rufst frevelnd wohl den Tod,", "tokens": ["Rufst", "fre\u00b7velnd", "wohl", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denkst deines lichten Ziels nicht mehr,", "tokens": ["Denkst", "dei\u00b7nes", "lich\u00b7ten", "Ziels", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "T\u00f6nt's hell durch deine Noth:", "tokens": ["T\u00f6nt's", "hell", "durch", "dei\u00b7ne", "Noth", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Verga\u00dfest du, da\u00df Gott dich schuf", "tokens": ["Ver\u00b7ga\u00b7\u00dfest", "du", ",", "da\u00df", "Gott", "dich", "schuf"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur f\u00fcr ein ", "tokens": ["Nur", "f\u00fcr", "ein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Schallt laut des V\u00f6gleins Frageruf", "tokens": ["Schallt", "laut", "des", "V\u00f6\u00b7gleins", "Fra\u00b7ge\u00b7ruf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dir in die Seel' hinein:", "tokens": ["Dir", "in", "die", "Seel'", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O sch\u00fctze Gott dich, V\u00f6glein traut,", "tokens": ["O", "sch\u00fct\u00b7ze", "Gott", "dich", ",", "V\u00f6\u00b7glein", "traut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "PPER", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df deine Brut gedeiht", "tokens": ["Da\u00df", "dei\u00b7ne", "Brut", "ge\u00b7deiht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und oft ihr frommer, frischer Laut", "tokens": ["Und", "oft", "ihr", "from\u00b7mer", ",", "fri\u00b7scher", "Laut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns mahnt zur rechten Zeit:", "tokens": ["Uns", "mahnt", "zur", "rech\u00b7ten", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}