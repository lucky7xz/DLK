{"textgrid.poem.58347": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "385. Gott ist hier, schau, in dir!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott ist dir inniger, als du dir selbst, zugegen,", "tokens": ["Gott", "ist", "dir", "in\u00b7ni\u00b7ger", ",", "als", "du", "dir", "selbst", ",", "zu\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "PPER", "ADV", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Willst du ihm nahen, mu\u00dft du dich nicht viel bewegen;", "tokens": ["Willst", "du", "ihm", "na\u00b7hen", ",", "mu\u00dft", "du", "dich", "nicht", "viel", "be\u00b7we\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADJA", "$,", "VMFIN", "PPER", "PRF", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "La\u00df da, was sinnlich ist, bleib willenlos und still,", "tokens": ["La\u00df", "da", ",", "was", "sinn\u00b7lich", "ist", ",", "bleib", "wil\u00b7len\u00b7los", "und", "still", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PRELS", "ADJD", "VAFIN", "$,", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kehr einw\u00e4rts dein Gemerk, da Gott sich zeigen will!", "tokens": ["Kehr", "ein\u00b7w\u00e4rts", "dein", "Ge\u00b7merk", ",", "da", "Gott", "sich", "zei\u00b7gen", "will", "!"], "token_info": ["word", "word", 