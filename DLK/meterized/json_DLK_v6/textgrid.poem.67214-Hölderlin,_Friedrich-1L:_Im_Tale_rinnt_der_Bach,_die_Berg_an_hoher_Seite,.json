{"textgrid.poem.67214": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Tale rinnt der Bach, die Berg an hoher Seite,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Tale rinnt der Bach, die Berg an hoher Seite,", "tokens": ["Im", "Ta\u00b7le", "rinnt", "der", "Bach", ",", "die", "Berg", "an", "ho\u00b7her", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie gr\u00fcnen weit umher an dieses Tales Breite,", "tokens": ["Sie", "gr\u00fc\u00b7nen", "weit", "um\u00b7her", "an", "die\u00b7ses", "Ta\u00b7les", "Brei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und B\u00e4ume mit dem Laube stehn gebreitet,", "tokens": ["Und", "B\u00e4u\u00b7me", "mit", "dem", "Lau\u00b7be", "stehn", "ge\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "VVINF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df fast verborgen dort der Bach hinunter gleitet.", "tokens": ["Da\u00df", "fast", "ver\u00b7bor\u00b7gen", "dort", "der", "Bach", "hin\u00b7un\u00b7ter", "glei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVPP", "ADV", "ART", "NN", "APPO", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So gl\u00e4nzt darob des sch\u00f6nen Sommers Sonne,", "tokens": ["So", "gl\u00e4nzt", "da\u00b7rob", "des", "sch\u00f6\u00b7nen", "Som\u00b7mers", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PAV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df fast zu eilen scheint des hellen Tages Wonne,", "tokens": ["Da\u00df", "fast", "zu", "ei\u00b7len", "scheint", "des", "hel\u00b7len", "Ta\u00b7ges", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PTKZU", "VVINF", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Abend mit der Frische kommt zu Ende,", "tokens": ["Der", "A\u00b7bend", "mit", "der", "Fri\u00b7sche", "kommt", "zu", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und trachtet, wie er das dem Menschen noch vollende.", "tokens": ["Und", "trach\u00b7tet", ",", "wie", "er", "das", "dem", "Men\u00b7schen", "noch", "voll\u00b7en\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "PDS", "ART", "NN", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}