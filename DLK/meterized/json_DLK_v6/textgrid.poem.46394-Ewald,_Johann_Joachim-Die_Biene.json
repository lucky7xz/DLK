{"textgrid.poem.46394": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Die Biene", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Biene, stichst du Doris Brust,", "tokens": ["O", "Bie\u00b7ne", ",", "stichst", "du", "Do\u00b7ris", "Brust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Sitz des Reizes und der Lust?", "tokens": ["Den", "Sitz", "des", "Rei\u00b7zes", "und", "der", "Lust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wilst du vielleicht die Spr\u00f6de stechen,", "tokens": ["Wilst", "du", "viel\u00b7leicht", "die", "Spr\u00f6\u00b7de", "ste\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um meine Quaal an sie zu r\u00e4chen?", "tokens": ["Um", "mei\u00b7ne", "Qua\u00b7al", "an", "sie", "zu", "r\u00e4\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Doch nein ich seh, du willst durch Doris Hand verderben", "tokens": ["Doch", "nein", "ich", "seh", ",", "du", "willst", "durch", "Do\u00b7ris", "Hand", "ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKANT", "PPER", "VVFIN", "$,", "PPER", "VMFIN", "APPR", "NE", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und gl\u00fccklicher als ich auf ihrem Busen sterben.", "tokens": ["Und", "gl\u00fcck\u00b7li\u00b7cher", "als", "ich", "auf", "ih\u00b7rem", "Bu\u00b7sen", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}