{"textgrid.poem.34323": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "50.", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gute Laune, Lieb und Lachen", "tokens": ["Gu\u00b7te", "Lau\u00b7ne", ",", "Lieb", "und", "La\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll mich hier", "tokens": ["Soll", "mich", "hier"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Unaufh\u00f6rlich gl\u00fccklich machen,", "tokens": ["Un\u00b7auf\u00b7h\u00f6r\u00b7lich", "gl\u00fcck\u00b7lich", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die ganze Welt mit mir.", "tokens": ["Und", "die", "gan\u00b7ze", "Welt", "mit", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf dem Sammt der Rosen wiegen", "tokens": ["Auf", "dem", "Sammt", "der", "Ro\u00b7sen", "wie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich die Weisen nur allein.", "tokens": ["Sich", "die", "Wei\u00b7sen", "nur", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Liebe? ist sie nicht Vergn\u00fcgen?", "tokens": ["Lie\u00b7be", "?", "ist", "sie", "nicht", "Ver\u00b7gn\u00fc\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PPER", "PTKNEG", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nur die Treue macht die Pein.", "tokens": ["Nur", "die", "Treu\u00b7e", "macht", "die", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}