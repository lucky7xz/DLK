{"dta.poem.12107": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Gottesbild am Weg; and\u00e4chtig hin wird treten", "tokens": ["Ein", "Got\u00b7tes\u00b7bild", "am", "Weg", ";", "an\u00b7d\u00e4ch\u00b7tig", "hin", "wird", "tre\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "$.", "ADJD", "ADV", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Wanderer, und eh er weiter wandert, beten.", "tokens": ["Der", "Wan\u00b7de\u00b7rer", ",", "und", "eh", "er", "wei\u00b7ter", "wan\u00b7dert", ",", "be\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "KOUS", "PPER", "ADV", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein zweites Bild, er wird auch Andacht ihm bezeugen!", "tokens": ["Ein", "zwei\u00b7tes", "Bild", ",", "er", "wird", "auch", "An\u00b7dacht", "ihm", "be\u00b7zeu\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VAFIN", "ADV", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein drittes, und er wird villeicht ein Knie noch beugen.", "tokens": ["Ein", "drit\u00b7tes", ",", "und", "er", "wird", "vil\u00b7leicht", "ein", "Knie", "noch", "beu\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "KON", "PPER", "VAFIN", "ADV", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein viertes, und er wird das Haupt noch fl\u00fcchtig b\u00fccken;", "tokens": ["Ein", "vier\u00b7tes", ",", "und", "er", "wird", "das", "Haupt", "noch", "fl\u00fcch\u00b7tig", "b\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "KON", "PPER", "VAFIN", "ART", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein f\u00fcnftes, und er wird den Hut nachl\u00e4ssig r\u00fccken.", "tokens": ["Ein", "f\u00fcnf\u00b7tes", ",", "und", "er", "wird", "den", "Hut", "nach\u00b7l\u00e4s\u00b7sig", "r\u00fc\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "KON", "PPER", "VAFIN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und wenn ihr immer mehr und immer wieder kehrt,", "tokens": ["Und", "wenn", "ihr", "im\u00b7mer", "mehr", "und", "im\u00b7mer", "wie\u00b7der", "kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "KON", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geht er zuletzt vorbei, und l\u00e4\u00dft euch ungeehrt.", "tokens": ["Geht", "er", "zu\u00b7letzt", "vor\u00b7bei", ",", "und", "l\u00e4\u00dft", "euch", "un\u00b7geehrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}