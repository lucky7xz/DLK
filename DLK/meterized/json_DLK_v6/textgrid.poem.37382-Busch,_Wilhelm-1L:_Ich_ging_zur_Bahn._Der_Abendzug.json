{"textgrid.poem.37382": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich ging zur Bahn. Der Abendzug", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ging zur Bahn. Der Abendzug", "tokens": ["Ich", "ging", "zur", "Bahn", ".", "Der", "A\u00b7bend\u00b7zug"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kam erst um halber zehn.", "tokens": ["Kam", "erst", "um", "hal\u00b7ber", "zehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "APPO", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer zeitig geht, der handelt klug,", "tokens": ["Wer", "zei\u00b7tig", "geht", ",", "der", "han\u00b7delt", "klug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "$,", "PRELS", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er kann gem\u00fctlich gehn.", "tokens": ["Er", "kann", "ge\u00b7m\u00fct\u00b7lich", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Fr\u00fchling war so warm und mild,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "war", "so", "warm", "und", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich ging wie neubelebt,", "tokens": ["Ich", "ging", "wie", "neu\u00b7be\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zumal ein wertes Frauenbild", "tokens": ["Zu\u00b7mal", "ein", "wer\u00b7tes", "Frau\u00b7en\u00b7bild"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir vor der Seele schwebt.", "tokens": ["Mir", "vor", "der", "See\u00b7le", "schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da\u00df ich sie heut noch sehen soll,", "tokens": ["Da\u00df", "ich", "sie", "heut", "noch", "se\u00b7hen", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df sie gewi\u00df noch wach,", "tokens": ["Da\u00df", "sie", "ge\u00b7wi\u00df", "noch", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Davon ist mir das Herz so voll,", "tokens": ["Da\u00b7von", "ist", "mir", "das", "Herz", "so", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich steh und denke nach.", "tokens": ["Ich", "steh", "und", "den\u00b7ke", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ein H\u00e4slein, das vor\u00fcberstiebt,", "tokens": ["Ein", "H\u00e4s\u00b7lein", ",", "das", "vor\u00b7\u00fc\u00b7bers\u00b7tiebt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ermahnt ich: La\u00df dir Zeit,", "tokens": ["Er\u00b7mahnt", "ich", ":", "La\u00df", "dir", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "VVIMP", "PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein guter Mensch, der gl\u00fccklich liebt,", "tokens": ["Ein", "gu\u00b7ter", "Mensch", ",", "der", "gl\u00fcck\u00b7lich", "liebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tut keinem was zu leid.", "tokens": ["Tut", "kei\u00b7nem", "was", "zu", "leid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "PIS", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Von ferne aus dem Wiesenteich", "tokens": ["Von", "fer\u00b7ne", "aus", "dem", "Wie\u00b7sen\u00b7teich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erklang der Fr\u00f6sche Chor,", "tokens": ["Er\u00b7klang", "der", "Fr\u00f6\u00b7sche", "Chor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und \u00fcberm Walde stieg zugleich", "tokens": ["Und", "\u00fc\u00b7berm", "Wal\u00b7de", "stieg", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der goldne Mond empor.", "tokens": ["Der", "gold\u00b7ne", "Mond", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da bist du ja, ich gr\u00fc\u00dfe dich,", "tokens": ["Da", "bist", "du", "ja", ",", "ich", "gr\u00fc\u00b7\u00dfe", "dich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du traulicher Kumpan.", "tokens": ["Du", "trau\u00b7li\u00b7cher", "Kum\u00b7pan", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Bed\u00e4chtig wandelst du wie ich", "tokens": ["Be\u00b7d\u00e4ch\u00b7tig", "wan\u00b7delst", "du", "wie", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PWAV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dahin auf deiner Bahn.", "tokens": ["Da\u00b7hin", "auf", "dei\u00b7ner", "Bahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Dies lenkte meinen Denkersinn", "tokens": ["Dies", "lenk\u00b7te", "mei\u00b7nen", "Den\u00b7ker\u00b7sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf den Gesch\u00e4ftsverlauf;", "tokens": ["Auf", "den", "Ge\u00b7sch\u00e4fts\u00b7ver\u00b7lauf", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich \u00fcberschlug mir den Gewinn.", "tokens": ["Ich", "\u00fc\u00b7bersc\u00b7hlug", "mir", "den", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das hielt mich etwas auf.", "tokens": ["Das", "hielt", "mich", "et\u00b7was", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Doch horch, da ist die Nachtigall,", "tokens": ["Doch", "horch", ",", "da", "ist", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie fl\u00f6tet wundersch\u00f6n.", "tokens": ["Sie", "fl\u00f6\u00b7tet", "wun\u00b7der\u00b7sch\u00f6n", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich fl\u00f6te selbst mit sanftem Schall", "tokens": ["Ich", "fl\u00f6\u00b7te", "selbst", "mit", "sanf\u00b7tem", "Schall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bleib ein wenig stehn.", "tokens": ["Und", "bleib", "ein", "we\u00b7nig", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "PIS", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und fl\u00f6tend kam ich zur Station,", "tokens": ["Und", "fl\u00f6\u00b7tend", "kam", "ich", "zur", "Sta\u00b7ti\u00b7on", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie das bei mir Gebrauch.", "tokens": ["Wie", "das", "bei", "mir", "Ge\u00b7brauch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O weh, was ist das f\u00fcr ein Ton?", "tokens": ["O", "weh", ",", "was", "ist", "das", "f\u00fcr", "ein", "Ton", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PWS", "VAFIN", "PDS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Zug, der fl\u00f6tet auch.", "tokens": ["Der", "Zug", ",", "der", "fl\u00f6\u00b7tet", "auch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Dort saust er hin. Ich stand versteint.", "tokens": ["Dort", "saust", "er", "hin", ".", "Ich", "stand", "ver\u00b7steint", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann sah ich nach der Uhr", "tokens": ["Dann", "sah", "ich", "nach", "der", "Uhr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie jeder, der zu sp\u00e4t erscheint.", "tokens": ["Wie", "je\u00b7der", ",", "der", "zu", "sp\u00e4t", "er\u00b7scheint", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "PRELS", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So will es die Natur.", "tokens": ["So", "will", "es", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}