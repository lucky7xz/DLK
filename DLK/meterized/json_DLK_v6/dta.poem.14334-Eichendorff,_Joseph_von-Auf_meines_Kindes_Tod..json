{"dta.poem.14334": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Auf meines Kindes Tod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und mancher mich da aus dem Himmelblau rief,", "tokens": ["Und", "man\u00b7cher", "mich", "da", "aus", "dem", "Him\u00b7mel\u00b7blau", "rief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PRF", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn ich drunten im Garten schlief. \u2014", "tokens": ["Wenn", "ich", "drun\u00b7ten", "im", "Gar\u00b7ten", "schlief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und mitten zwischen den Blumen und Scheinen", "tokens": ["Und", "mit\u00b7ten", "zwi\u00b7schen", "den", "Blu\u00b7men", "und", "Schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "KON", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Steht die sch\u00f6nste von allen Frauen,", "tokens": ["Steht", "die", "sch\u00f6ns\u00b7te", "von", "al\u00b7len", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "APPR", "PIAT", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Ein gl\u00e4nzend Kindlein an ihrer Brust. \u2014", "tokens": ["Ein", "gl\u00e4n\u00b7zend", "Kin\u00b7dlein", "an", "ih\u00b7rer", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Ich kann nicht sprechen und auch nicht weinen,", "tokens": ["Ich", "kann", "nicht", "spre\u00b7chen", "und", "auch", "nicht", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "KON", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Nur singen immer und wieder dann schauen", "tokens": ["Nur", "sin\u00b7gen", "im\u00b7mer", "und", "wie\u00b7der", "dann", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "KON", "ADV", "ADV", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Still vor gro\u00dfer, seeliger Lust.\u201c", "tokens": ["Still", "vor", "gro\u00b7\u00dfer", ",", "see\u00b7li\u00b7ger", "Lust", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "ADJA", "$,", "ADJA", "NN", "$.", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}