{"textgrid.poem.24170": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Kleiner, h\u00fcte dich!", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Worte werfen die Wahrheit nicht um, doch wackelt sie manchmal,", "tokens": ["Wor\u00b7te", "wer\u00b7fen", "die", "Wahr\u00b7heit", "nicht", "um", ",", "doch", "wa\u00b7ckelt", "sie", "manch\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKNEG", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Wenn von rechts und von links Wortegest\u00f6ber krawallt.", "tokens": ["Wenn", "von", "rechts", "und", "von", "links", "Wor\u00b7te\u00b7ge\u00b7st\u00f6\u00b7ber", "kra\u00b7wallt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADV", "KON", "APPR", "ADV", "PAV", "VVFIN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Leider gen\u00fcgt das bereits, denn wackelnde Wahrheiten werden", "tokens": ["Lei\u00b7der", "ge\u00b7n\u00fcgt", "das", "be\u00b7reits", ",", "denn", "wa\u00b7ckeln\u00b7de", "Wahr\u00b7hei\u00b7ten", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PDS", "ADV", "$,", "KON", "ADJA", "NN", "VAINF"], "meter": "+--+--+-+---+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Gerne von hoch und gering eilig als Irrtum gesch\u00e4tzt.", "tokens": ["Ger\u00b7ne", "von", "hoch", "und", "ge\u00b7ring", "ei\u00b7lig", "als", "Irr\u00b7tum", "ge\u00b7sch\u00e4tzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJD", "KON", "ADJD", "ADJD", "KOKOM", "NE", "VVPP", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Steht aber Unsinn recht stramm, breitbeinig und wuchtig im Erdreich,", "tokens": ["Steht", "a\u00b7ber", "Un\u00b7sinn", "recht", "stramm", ",", "breit\u00b7bei\u00b7nig", "und", "wuch\u00b7tig", "im", "Er\u00b7dreich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "ADJD", "VVFIN", "$,", "ADJD", "KON", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.6": {"text": "F\u00fchlt sich die Menge zu ihm wie mit Magneten gelenkt,", "tokens": ["F\u00fchlt", "sich", "die", "Men\u00b7ge", "zu", "ihm", "wie", "mit", "Mag\u00b7ne\u00b7ten", "ge\u00b7lenkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "PPER", "KOKOM", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Klebt wie der Eisenfeilspahn daran fest und f\u00fchlt sich geborgen,", "tokens": ["Klebt", "wie", "der", "Ei\u00b7sen\u00b7feil\u00b7spahn", "da\u00b7ran", "fest", "und", "f\u00fchlt", "sich", "ge\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "PAV", "PTKVZ", "KON", "VVFIN", "PRF", "VVPP", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Denn das Feste allein bietet den Wankenden Schutz.", "tokens": ["Denn", "das", "Fes\u00b7te", "al\u00b7lein", "bie\u00b7tet", "den", "Wan\u00b7ken\u00b7den", "Schutz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Wehe dir, M\u00e4nnchen mit Spaten und Beil, wenns keck dich gel\u00fcstet,", "tokens": ["We\u00b7he", "dir", ",", "M\u00e4nn\u00b7chen", "mit", "Spa\u00b7ten", "und", "Beil", ",", "wenns", "keck", "dich", "ge\u00b7l\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "APPR", "NN", "KON", "NN", "$,", "KOUS", "ADJD", "PPER", "VVPP", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Auszugraben den Stamm, der so gar viele besch\u00fctzt!", "tokens": ["Aus\u00b7zu\u00b7gra\u00b7ben", "den", "Stamm", ",", "der", "so", "gar", "vie\u00b7le", "be\u00b7sch\u00fctzt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "ADV", "PIS", "VVPP", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Bist du nicht stark wie der Sturm, der knorrige Eichen entwurzelt,", "tokens": ["Bist", "du", "nicht", "stark", "wie", "der", "Sturm", ",", "der", "knor\u00b7ri\u00b7ge", "Ei\u00b7chen", "ent\u00b7wur\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "KOKOM", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Nageln sie dich mit Geh\u00f6hn an den geheiligten Stamm.", "tokens": ["Na\u00b7geln", "sie", "dich", "mit", "Ge\u00b7h\u00f6hn", "an", "den", "ge\u00b7hei\u00b7lig\u00b7ten", "Stamm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.13": {"text": "Und es geschieht dir recht, denn der sch\u00e4ndlichste Dilettantismus", "tokens": ["Und", "es", "ge\u00b7schieht", "dir", "recht", ",", "denn", "der", "sch\u00e4nd\u00b7lichs\u00b7te", "Di\u00b7let\u00b7tan\u00b7tis\u00b7mus"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "$,", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.14": {"text": "Ist es, f\u00fcrs Gro\u00dfe ins Feld als ein Pygm\u00e4e zu ziehn.", "tokens": ["Ist", "es", ",", "f\u00fcrs", "Gro\u00b7\u00dfe", "ins", "Feld", "als", "ein", "Pyg\u00b7m\u00e4e", "zu", "ziehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "APPRART", "ADJA", "APPRART", "NN", "KOKOM", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "---+--+--+--+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Ehrfurcht gebietet die Kraft auch als Wahn; sie niederzuk\u00e4mpfen", "tokens": ["Ehr\u00b7furcht", "ge\u00b7bie\u00b7tet", "die", "Kraft", "auch", "als", "Wahn", ";", "sie", "nie\u00b7der\u00b7zu\u00b7k\u00e4mp\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "KOUS", "NN", "$.", "PPER", "VVINF"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.16": {"text": "Ziemt nur wieder der Kraft; Macht geb\u00fchrt nur der Macht.", "tokens": ["Ziemt", "nur", "wie\u00b7der", "der", "Kraft", ";", "Macht", "ge\u00b7b\u00fchrt", "nur", "der", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "$.", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+--++-+--+", "measure": "trochaic.hexa.relaxed"}}}}}