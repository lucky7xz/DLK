{"dta.poem.12903": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der Chloris antwort an den Adon,  \n der ihr seine liebe angetragen, und  \n gegen-liebe verlanget.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist zu viel, Adon! sich also gleich verlieben:", "tokens": ["Es", "ist", "zu", "viel", ",", "A\u00b7don", "!", "sich", "al\u00b7so", "gleich", "ver\u00b7lie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "PIS", "$,", "NE", "$.", "PRF", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ist zu viel, Adon! alsbald halbtodt zu seyn;", "tokens": ["Es", "ist", "zu", "viel", ",", "A\u00b7don", "!", "als\u00b7bald", "halb\u00b7todt", "zu", "seyn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "PIS", "$,", "NE", "$.", "ADV", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch was die feder gleich auf das papier geschrieben,", "tokens": ["Doch", "was", "die", "fe\u00b7der", "gleich", "auf", "das", "pa\u00b7pier", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Das trifft deswegen nicht auch mit dem hertzen ein.", "tokens": ["Das", "trifft", "des\u00b7we\u00b7gen", "nicht", "auch", "mit", "dem", "hert\u00b7zen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PAV", "PTKNEG", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein wort ist doch kein pfeil. Es kostet wenig dinte,", "tokens": ["Ein", "wort", "ist", "doch", "kein", "pfeil", ".", "Es", "kos\u00b7tet", "we\u00b7nig", "din\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PIAT", "NN", "$.", "PPER", "VVFIN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So stirbt man hundert mal auf allen bl\u00e4ttern hin.", "tokens": ["So", "stirbt", "man", "hun\u00b7dert", "mal", "auf", "al\u00b7len", "bl\u00e4t\u00b7tern", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "CARD", "ADV", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Drum halt\u2019 ich deinen brief vor eine blose finte,", "tokens": ["Drum", "halt'", "ich", "dei\u00b7nen", "brief", "vor", "ei\u00b7ne", "blo\u00b7se", "fin\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "VVFIN", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Darauf ich, viel zu bau\u2019n, noch nicht versehen bin.", "tokens": ["Da\u00b7rauf", "ich", ",", "viel", "zu", "bau'n", ",", "noch", "nicht", "ver\u00b7se\u00b7hen", "bin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "$,", "ADV", "PTKZU", "VVINF", "$,", "ADV", "PTKNEG", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du hast nur blosen schertz in bleichen ernst gekleidet,", "tokens": ["Du", "hast", "nur", "blo\u00b7sen", "schertz", "in", "blei\u00b7chen", "ernst", "ge\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "APPR", "ADJA", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich wei\u00df wohl, da\u00df Adon nicht so geschwinde liebt;", "tokens": ["Ich", "wei\u00df", "wohl", ",", "da\u00df", "A\u00b7don", "nicht", "so", "ge\u00b7schwin\u00b7de", "liebt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "NE", "PTKNEG", "ADV", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dieweil sein kluger geist die \u00fcbereilung meidet,", "tokens": ["Die\u00b7weil", "sein", "klu\u00b7ger", "geist", "die", "\u00fc\u00b7be\u00b7rei\u00b7lung", "mei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So ihren dienern nichts, als reu zu lohne giebt.", "tokens": ["So", "ih\u00b7ren", "die\u00b7nern", "nichts", ",", "als", "reu", "zu", "loh\u00b7ne", "giebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PIS", "$,", "KOUS", "ADJD", "PTKZU", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Verzeih\u2019 denn, Werther Freund! da\u00df ich der marter lache,", "tokens": ["Ver\u00b7zeih'", "denn", ",", "Wert\u00b7her", "Freund", "!", "da\u00df", "ich", "der", "mar\u00b7ter", "la\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "ADJA", "NN", "$.", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die deine poesie der h\u00f6llen gleiche macht:", "tokens": ["Die", "dei\u00b7ne", "po\u00b7e\u00b7sie", "der", "h\u00f6l\u00b7len", "glei\u00b7che", "macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ART", "ADJA", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Gereimte klagen sind gar eine leichte sache;", "tokens": ["Ge\u00b7reim\u00b7te", "kla\u00b7gen", "sind", "gar", "ei\u00b7ne", "leich\u00b7te", "sa\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wie bald ist, was nicht ist, in einen vers gebracht?", "tokens": ["Wie", "bald", "ist", ",", "was", "nicht", "ist", ",", "in", "ei\u00b7nen", "vers", "ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "$,", "PRELS", "PTKNEG", "VAFIN", "$,", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Du sprichst zwar: Wo ich dir kein pflaster \u00fcbersende,", "tokens": ["Du", "sprichst", "zwar", ":", "Wo", "ich", "dir", "kein", "pflas\u00b7ter", "\u00fc\u00b7ber\u00b7sen\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "PWAV", "PPER", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So werdest du ein raub des blassen todes seyn;", "tokens": ["So", "wer\u00b7dest", "du", "ein", "raub", "des", "blas\u00b7sen", "to\u00b7des", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Allein es geht mit dir noch lange nicht zum ende.", "tokens": ["Al\u00b7lein", "es", "geht", "mit", "dir", "noch", "lan\u00b7ge", "nicht", "zum", "en\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "APPR", "PPER", "ADV", "ADV", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und welch Galenus giebt gesunden artzney ein?", "tokens": ["Und", "welch", "Ga\u00b7le\u00b7nus", "giebt", "ge\u00b7sun\u00b7den", "artz\u00b7ney", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "VVFIN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wer auf dem tode liegt, wird keine verse dichten.", "tokens": ["Wer", "auf", "dem", "to\u00b7de", "liegt", ",", "wird", "kei\u00b7ne", "ver\u00b7se", "dich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,", "VAFIN", "PIAT", "ADJA", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "(schau, wie dein eigen brief mir dein gem\u00fcth entdeckt).", "tokens": ["(", "schau", ",", "wie", "dein", "ei\u00b7gen", "brief", "mir", "dein", "ge\u00b7m\u00fcth", "ent\u00b7deckt", ")", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$,", "PWAV", "PPOSAT", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "VVPP", "$(", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.23": {"text": "Die seuffzer lassen sich nach keiner regel richten,", "tokens": ["Die", "seuff\u00b7zer", "las\u00b7sen", "sich", "nach", "kei\u00b7ner", "re\u00b7gel", "rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wenn schmertz und todes-angst in marck und adern steckt.", "tokens": ["Wenn", "schmertz", "und", "to\u00b7des\u00b7angst", "in", "marck", "und", "a\u00b7dern", "steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "ADJD", "APPR", "NN", "KON", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Unm\u00f6glich ist es nicht, da\u00df dich ein aug\u2019 entz\u00fcndet,", "tokens": ["Un\u00b7m\u00f6g\u00b7lich", "ist", "es", "nicht", ",", "da\u00df", "dich", "ein", "aug'", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Doch meines klagest du mit h\u00f6chstem unrecht an,", "tokens": ["Doch", "mei\u00b7nes", "kla\u00b7gest", "du", "mit", "h\u00f6chs\u00b7tem", "un\u00b7recht", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Nachdem es keinen strahl in seinem sterne findet,", "tokens": ["Nach\u00b7dem", "es", "kei\u00b7nen", "strahl", "in", "sei\u00b7nem", "ster\u00b7ne", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "APPR", "PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der ein so edles hertz in flammen setzen kan.", "tokens": ["Der", "ein", "so", "ed\u00b7les", "hertz", "in", "flam\u00b7men", "set\u00b7zen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADV", "ADJA", "NN", "APPR", "VVINF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und k\u00f6nnt\u2019 ich mich auch das schon \u00fcberreden lassen;", "tokens": ["Und", "k\u00f6nnt'", "ich", "mich", "auch", "das", "schon", "\u00fc\u00b7berr\u00b7e\u00b7den", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "ADV", "ART", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "So w\u00fcrd\u2019 ich dennoch nicht von allem zweiffel los.", "tokens": ["So", "w\u00fcrd'", "ich", "den\u00b7noch", "nicht", "von", "al\u00b7lem", "zweif\u00b7fel", "los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "APPR", "PIS", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Denn hertzen, die so bald und hefftig funcken fassen,", "tokens": ["Denn", "hert\u00b7zen", ",", "die", "so", "bald", "und", "heff\u00b7tig", "fun\u00b7cken", "fas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "ADV", "ADV", "KON", "ADJD", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Verfallen insgemein in mehr als eine schoos.", "tokens": ["Ver\u00b7fal\u00b7len", "ins\u00b7ge\u00b7mein", "in", "mehr", "als", "ei\u00b7ne", "schoos", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PIAT", "KOKOM", "ART", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}