{"dta.poem.18580": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Cantata  \n Eines verliebten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Unbestand", "tokens": ["Un\u00b7be\u00b7stand"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Ist das Gifft verliebter Seelen.", "tokens": ["Ist", "das", "Gifft", "ver\u00b7lieb\u00b7ter", "See\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tausend Schmertzen/ Angst und Pein", "tokens": ["Tau\u00b7send", "Schmert\u00b7zen", "/", "Angst", "und", "Pein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["CARD", "NN", "$(", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00f6nnen nicht so grausam seyn/", "tokens": ["K\u00f6n\u00b7nen", "nicht", "so", "grau\u00b7sam", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ADV", "ADJD", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als die Falschheit wei\u00df zu qu\u00e4len.", "tokens": ["Als", "die", "Falschheit", "wei\u00df", "zu", "qu\u00e4\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.6": {"text": " unbestand", "tokens": ["un\u00b7be\u00b7stand"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Ist das Gifft verliebter Seelen.", "tokens": ["Ist", "das", "Gifft", "ver\u00b7lieb\u00b7ter", "See\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Allein/", "tokens": ["Al\u00b7lein", "/"], "token_info": ["word", "punct"], "pos": ["ADV", "$("], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Ein edeles Gem\u00fchte/", "tokens": ["Ein", "e\u00b7de\u00b7les", "Ge\u00b7m\u00fch\u00b7te", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und das die Tugend liebt/", "tokens": ["Und", "das", "die", "Tu\u00b7gend", "liebt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Gr\u00fcnt in der Treue Bl\u00fchte/", "tokens": ["Gr\u00fcnt", "in", "der", "Treu\u00b7e", "Bl\u00fch\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "So nochmahls auch die sch\u00f6nsten Fr\u00fcchte giebt.", "tokens": ["So", "noch\u00b7mahls", "auch", "die", "sch\u00f6ns\u00b7ten", "Fr\u00fcch\u00b7te", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Drum will ich lieber tod/ als falsch und untreu seyn.", "tokens": ["Drum", "will", "ich", "lie\u00b7ber", "tod", "/", "als", "falsch", "und", "un\u00b7treu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "NN", "$(", "KOUS", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Falsche Sinnen", "tokens": ["Fal\u00b7sche", "Sin\u00b7nen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Geht von hinnen/", "tokens": ["Geht", "von", "hin\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADV", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Und vermeidet meine Brust/", "tokens": ["Und", "ver\u00b7mei\u00b7det", "mei\u00b7ne", "Brust", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn dem Hertzen/", "tokens": ["Denn", "dem", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Sind die Kertzen", "tokens": ["Sind", "die", "Kert\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Edler Treue nur bewust.", "tokens": ["Ed\u00b7ler", "Treu\u00b7e", "nur", "be\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Engel der mich hat besiegt/", "tokens": ["Der", "En\u00b7gel", "der", "mich", "hat", "be\u00b7siegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist Wunder-sch\u00f6n zu nennen/", "tokens": ["Ist", "Wun\u00b7der\u00b7sch\u00f6n", "zu", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Drum schwer ich auch vergn\u00fcgt/", "tokens": ["Drum", "schwer", "ich", "auch", "ver\u00b7gn\u00fcgt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bi\u00df in die Grufft zu brennen.", "tokens": ["Bi\u00df", "in", "die", "Grufft", "zu", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ja das Meer mit seiner Fluht", "tokens": ["Ja", "das", "Meer", "mit", "sei\u00b7ner", "Fluht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Leschet nicht die starcke Glut", "tokens": ["Le\u00b7schet", "nicht", "die", "star\u00b7cke", "Glut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn weil die Treue mich beseelt/", "tokens": ["Denn", "weil", "die", "Treu\u00b7e", "mich", "be\u00b7seelt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So hat die Liebe/", "tokens": ["So", "hat", "die", "Lie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Durch ihre sch\u00f6nen Triebe/", "tokens": ["Durch", "ih\u00b7re", "sch\u00f6\u00b7nen", "Trie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Uns diese Lust zum Zeitvertreib erwehlt/", "tokens": ["Uns", "die\u00b7se", "Lust", "zum", "Zeit\u00b7ver\u00b7treib", "er\u00b7wehlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "NN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sch\u00f6nster Engel la\u00df dich k\u00fcssen/", "tokens": ["Sch\u00f6ns\u00b7ter", "En\u00b7gel", "la\u00df", "dich", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVIMP", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fcsse mich mein Anderich!", "tokens": ["K\u00fcs\u00b7se", "mich", "mein", "An\u00b7de\u00b7rich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Brich die s\u00fcsse Lust-Narcissen/", "tokens": ["Brich", "die", "s\u00fcs\u00b7se", "Lust\u00b7\u00b7N\u00b7a\u00b7rcis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Liebe mich/ ich liebe dich.", "tokens": ["Lie\u00b7be", "mich", "/", "ich", "lie\u00b7be", "dich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$(", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df uns doch vertraulich schertzen", "tokens": ["La\u00df", "uns", "doch", "ver\u00b7trau\u00b7lich", "schert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Drum sol die Felsen-Art", "tokens": ["Drum", "sol", "die", "Fel\u00b7sen\u00b7Art"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Treue mich bekr\u00f6nen/", "tokens": ["Der", "Treu\u00b7e", "mich", "be\u00b7kr\u00f6\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Schicksal sey so hart/", "tokens": ["Das", "Schick\u00b7sal", "sey", "so", "hart", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So wil ich doch die Zeit/", "tokens": ["So", "wil", "ich", "doch", "die", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ja Gl\u00fcck und Neid/", "tokens": ["Ja", "Gl\u00fcck", "und", "Neid", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "KON", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Best\u00e4ndig verh\u00f6hnen.", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", "ver\u00b7h\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Best\u00e4ndigkeit", "tokens": ["Be\u00b7st\u00e4n\u00b7dig\u00b7keit"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Sol mich noch in den Haven f\u00fchren/", "tokens": ["Sol", "mich", "noch", "in", "den", "Ha\u00b7ven", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wo die rechte g\u00fcldne Zeit/", "tokens": ["Wo", "die", "rech\u00b7te", "g\u00fcld\u00b7ne", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Und mein Paradie\u00df zu sp\u00fchren.", "tokens": ["Und", "mein", "Pa\u00b7ra\u00b7die\u00df", "zu", "sp\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Ich wil durch die Treue siegen/", "tokens": ["Ich", "wil", "durch", "die", "Treu\u00b7e", "sie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "S\u00fcsse Stunde sey bereit/", "tokens": ["S\u00fcs\u00b7se", "Stun\u00b7de", "sey", "be\u00b7reit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Sehnsucht zu vergn\u00fcgen/", "tokens": ["Mei\u00b7ne", "Sehn\u00b7sucht", "zu", "ver\u00b7gn\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Cr\u00f6ne die Best\u00e4ndigkeit.", "tokens": ["Cr\u00f6\u00b7ne", "die", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00fchre mich/ ach ja bey Zeiten/", "tokens": ["F\u00fch\u00b7re", "mich", "/", "ach", "ja", "bey", "Zei\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "ITJ", "ADV", "APPR", "NN", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Hin zu tausend Lieblichkeiten.", "tokens": ["Hin", "zu", "tau\u00b7send", "Lieb\u00b7lich\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "CARD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}