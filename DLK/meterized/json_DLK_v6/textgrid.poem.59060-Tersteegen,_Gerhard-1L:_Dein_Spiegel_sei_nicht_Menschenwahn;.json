{"textgrid.poem.59060": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dein Spiegel sei nicht Menschenwahn;", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Spiegel sei nicht Menschenwahn;", "tokens": ["Dein", "Spie\u00b7gel", "sei", "nicht", "Men\u00b7schen\u00b7wahn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schau Gott in dir, dich in Gott an!", "tokens": ["Schau", "Gott", "in", "dir", ",", "dich", "in", "Gott", "an", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PPER", "$,", "PRF", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du bist der, wie dich Gott erkennet,", "tokens": ["Du", "bist", "der", ",", "wie", "dich", "Gott", "er\u00b7ken\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "$,", "PWAV", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gro\u00df, wenn er dich den Seinen nennet.", "tokens": ["Gro\u00df", ",", "wenn", "er", "dich", "den", "Sei\u00b7nen", "nen\u00b7net", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "PRF", "ART", "PPOSS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}