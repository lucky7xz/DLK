{"textgrid.poem.43656": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wie der Zaunschl\u00fcpfer K\u00f6nig ward", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weithin K\u00f6nig Adler sah,", "tokens": ["Weit\u00b7hin", "K\u00f6\u00b7nig", "Ad\u00b7ler", "sah", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie der Fr\u00fchling fern und nah", "tokens": ["Wie", "der", "Fr\u00fch\u00b7ling", "fern", "und", "nah"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles hatte sch\u00f6n geschm\u00fcckt", "tokens": ["Al\u00b7les", "hat\u00b7te", "sch\u00f6n", "ge\u00b7schm\u00fcckt"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "ADJD", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und erfreuet und begl\u00fcckt.", "tokens": ["Und", "er\u00b7freu\u00b7et", "und", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "K\u00f6nig Adler fr\u00f6hlich war,", "tokens": ["K\u00f6\u00b7nig", "Ad\u00b7ler", "fr\u00f6h\u00b7lich", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Rief: \u00bbGut Heil, du Vogelschaar!", "tokens": ["Rief", ":", "\u00bb", "Gut", "Heil", ",", "du", "Vo\u00b7gel\u00b7schaar", "!"], "token_info": ["word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "ADJD", "NN", "$,", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Morgen soll ein Turnfest sein", "tokens": ["Mor\u00b7gen", "soll", "ein", "Turn\u00b7fest", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ART", "NN", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich lad' euch Alle ein!\u00ab", "tokens": ["Und", "ich", "lad'", "euch", "Al\u00b7le", "ein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PIS", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und da kam die Vogelschaar:", "tokens": ["Und", "da", "kam", "die", "Vo\u00b7gel\u00b7schaar", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Rabe, Drossel, Elster, Staar,", "tokens": ["Ra\u00b7be", ",", "Dros\u00b7sel", ",", "Els\u00b7ter", ",", "Staar", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NE", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meise, H\u00e4her, Wiedewall,", "tokens": ["Mei\u00b7se", ",", "H\u00e4\u00b7her", ",", "Wie\u00b7de\u00b7wall", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kuckuck, Specht und Nachtigall.", "tokens": ["Ku\u00b7ckuck", ",", "Specht", "und", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und man reckt sich, streckt sich, ringt,", "tokens": ["Und", "man", "reckt", "sich", ",", "streckt", "sich", ",", "ringt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PRF", "$,", "VVFIN", "PRF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Klettert, schwingt sich, h\u00fcpft und springt;", "tokens": ["Klet\u00b7tert", ",", "schwingt", "sich", ",", "h\u00fcpft", "und", "springt", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PRF", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Endlich kommt der Dauerlauf \u2013", "tokens": ["End\u00b7lich", "kommt", "der", "Dau\u00b7er\u00b7lauf", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00f6nig Adler spricht darauf:", "tokens": ["K\u00f6\u00b7nig", "Ad\u00b7ler", "spricht", "da\u00b7rauf", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbstellt euch jetzt in Reih' und Glied,", "tokens": ["\u00bb", "stellt", "euch", "jetzt", "in", "Reih'", "und", "Glied", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle, ohne Unterschied!", "tokens": ["Al\u00b7le", ",", "oh\u00b7ne", "Un\u00b7ter\u00b7schied", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUI", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer am h\u00f6chsten fliegen kann,", "tokens": ["Wer", "am", "h\u00f6chs\u00b7ten", "flie\u00b7gen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "ADJA", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der soll K\u00f6nig sein fortan!\u00ab", "tokens": ["Der", "soll", "K\u00f6\u00b7nig", "sein", "for\u00b7tan", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PIAT", "NN", "VAINF", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Turr! da flog die ganze Schaar,", "tokens": ["Turr", "!", "da", "flog", "die", "gan\u00b7ze", "Schaar", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber unterm Fl\u00fcgelpaar", "tokens": ["A\u00b7ber", "un\u00b7term", "Fl\u00fc\u00b7gel\u00b7paar"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nig Adlers sa\u00df versteckt", "tokens": ["K\u00f6\u00b7nig", "Ad\u00b7lers", "sa\u00df", "ver\u00b7steckt"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Herr Zaunschl\u00fcpfer unentdeckt.", "tokens": ["Herr", "Zaun\u00b7schl\u00fcp\u00b7fer", "un\u00b7ent\u00b7deckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Als der Adler endlich doch", "tokens": ["Als", "der", "Ad\u00b7ler", "end\u00b7lich", "doch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Matt ward, flog Zaunschl\u00fcpfer noch,", "tokens": ["Matt", "ward", ",", "flog", "Zaun\u00b7schl\u00fcp\u00b7fer", "noch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "VVFIN", "NN", "ADV", "$,"], "meter": "++-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "H\u00f6her noch ein ganzes St\u00fcck,", "tokens": ["H\u00f6\u00b7her", "noch", "ein", "gan\u00b7zes", "St\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kam als K\u00f6nig dann zur\u00fcck.", "tokens": ["Kam", "als", "K\u00f6\u00b7nig", "dann", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}