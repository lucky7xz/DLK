{"textgrid.poem.63739": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Soll ich nun ganz mich entw\u00f6hnen des sch\u00f6nen Gesichts, das freundlich,", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Soll ich nun ganz mich entw\u00f6hnen des sch\u00f6nen Gesichts, das freundlich,", "tokens": ["Soll", "ich", "nun", "ganz", "mich", "ent\u00b7w\u00f6h\u00b7nen", "des", "sch\u00f6\u00b7nen", "Ge\u00b7sichts", ",", "das", "freund\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "+--+--+--+--+-+-", "measure": "dactylic.tetra.plus"}, "line.2": {"text": "Schwesterlich kam und ging, immer zu tr\u00f6sten bereit?", "tokens": ["Schwes\u00b7ter\u00b7lich", "kam", "und", "ging", ",", "im\u00b7mer", "zu", "tr\u00f6s\u00b7ten", "be\u00b7reit", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "KON", "VVFIN", "$,", "ADV", "PTKZU", "VVINF", "ADJD", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "Ach, wenn widriger Wind in Napoli hemmte die Barke,", "tokens": ["Ach", ",", "wenn", "wid\u00b7ri\u00b7ger", "Wind", "in", "Na\u00b7po\u00b7li", "hemm\u00b7te", "die", "Bar\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "ADJA", "NN", "APPR", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Die von der Liebsten ein Blatt heut mir zu bringen versprach,", "tokens": ["Die", "von", "der", "Liebs\u00b7ten", "ein", "Blatt", "heut", "mir", "zu", "brin\u00b7gen", "ver\u00b7sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "ADV", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "---+--+---+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und ich stand und im Weiten mit ungeduldigen Augen", "tokens": ["Und", "ich", "stand", "und", "im", "Wei\u00b7ten", "mit", "un\u00b7ge\u00b7dul\u00b7di\u00b7gen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KON", "APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Folgte vom offenen Dach jeglichem Segel im Meer,", "tokens": ["Folg\u00b7te", "vom", "of\u00b7fe\u00b7nen", "Dach", "jeg\u00b7li\u00b7chem", "Se\u00b7gel", "im", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.7": {"text": "Dann wohl \u00fcber die Mauer die liebliche Stimme vernahm ich:", "tokens": ["Dann", "wohl", "\u00fc\u00b7ber", "die", "Mau\u00b7er", "die", "lieb\u00b7li\u00b7che", "Stim\u00b7me", "ver\u00b7nahm", "ich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+---+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Seid Ihr traurig? Warum? Seht, es betr\u00fcbt mich sogleich,", "tokens": ["Seid", "Ihr", "trau\u00b7rig", "?", "Wa\u00b7rum", "?", "Seht", ",", "es", "be\u00b7tr\u00fcbt", "mich", "sog\u00b7leich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ADJD", "$.", "PWAV", "$.", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Wenn Ihr finster und stille den Kopf h\u00e4ngt. Lasset uns plaudern!", "tokens": ["Wenn", "Ihr", "fins\u00b7ter", "und", "stil\u00b7le", "den", "Kopf", "h\u00e4ngt", ".", "Las\u00b7set", "uns", "plau\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "KON", "VVFIN", "ART", "NN", "VVFIN", "$.", "VVFIN", "PPER", "VVINF", "$."], "meter": "--+--+--+--+-+-", "measure": "anapaest.tetra.plus"}, "line.10": {"text": "Ist das Leben doch s\u00fc\u00df; seid Ihr doch jung und geliebt. \u2013", "tokens": ["Ist", "das", "Le\u00b7ben", "doch", "s\u00fc\u00df", ";", "seid", "Ihr", "doch", "jung", "und", "ge\u00b7liebt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADJD", "$.", "VAFIN", "PPER", "ADV", "ADJD", "KON", "VVPP", "$.", "$("], "meter": "+-+--++-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Und was hilft mir der Liebe Gewi\u00dfheit? Haben ist alles;", "tokens": ["Und", "was", "hilft", "mir", "der", "Lie\u00b7be", "Ge\u00b7wi\u00df\u00b7heit", "?", "Ha\u00b7ben", "ist", "al\u00b7les", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "ART", "NN", "NN", "$.", "NN", "VAFIN", "PIS", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.12": {"text": "Haben allein ist s\u00fc\u00df; Haben ist Jugend allein. \u2013", "tokens": ["Ha\u00b7ben", "al\u00b7lein", "ist", "s\u00fc\u00df", ";", "Ha\u00b7ben", "ist", "Ju\u00b7gend", "al\u00b7lein", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "VAFIN", "ADJD", "$.", "NN", "VAFIN", "NN", "ADV", "$.", "$("], "meter": "+-----+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "Und sie tr\u00f6stete mich mit dem kindischen Troste: So seid nur", "tokens": ["Und", "sie", "tr\u00f6s\u00b7te\u00b7te", "mich", "mit", "dem", "kin\u00b7di\u00b7schen", "Tros\u00b7te", ":", "So", "seid", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$.", "ADV", "VAFIN", "ADV"], "meter": "--+--++-+--+--+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "Klug und geduldig. Ihr kehrt wieder und habt sie aufs neu'. \u2013", "tokens": ["Klug", "und", "ge\u00b7dul\u00b7dig", ".", "Ihr", "kehrt", "wie\u00b7der", "und", "habt", "sie", "aufs", "neu'", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "KON", "ADJD", "$.", "PPER", "VVFIN", "ADV", "KON", "VAFIN", "PPER", "APPRART", "ADJA", "$.", "$("], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Kann ich's glauben? Und zweifle sogar am Gewissesten, da\u00df ich", "tokens": ["Kann", "ich's", "glau\u00b7ben", "?", "Und", "zweif\u00b7le", "so\u00b7gar", "am", "Ge\u00b7wis\u00b7ses\u00b7ten", ",", "da\u00df", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PIS", "VVINF", "$.", "KON", "VVFIN", "ADV", "APPRART", "NN", "$,", "KOUS", "PPER"], "meter": "+-+--+--+--+---+", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "Einst sie besa\u00df; mir scheint's nun wie ein Trug, wie ein Traum. \u2013", "tokens": ["Einst", "sie", "be\u00b7sa\u00df", ";", "mir", "scheint's", "nun", "wie", "ein", "Trug", ",", "wie", "ein", "Traum", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "KOKOM", "ART", "NN", "$,", "PWAV", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.17": {"text": "Unzufriedner! Und kommt nicht flei\u00dfig ein Briefchen und sagt Euch,", "tokens": ["Un\u00b7zu\u00b7fried\u00b7ner", "!", "Und", "kommt", "nicht", "flei\u00b7\u00dfig", "ein", "Brief\u00b7chen", "und", "sagt", "Euch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "VVFIN", "PTKNEG", "ADJD", "ART", "NN", "KON", "VVFIN", "PPER", "$,"], "meter": "+-+--+-+--+-+++", "measure": "trochaic.octa.plus.relaxed"}, "line.18": {"text": "Da\u00df dies alles sich erst k\u00fcrzlich und wirklich begab? \u2013", "tokens": ["Da\u00df", "dies", "al\u00b7les", "sich", "erst", "k\u00fcrz\u00b7lich", "und", "wirk\u00b7lich", "be\u00b7gab", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PDS", "PIS", "PRF", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$.", "$("], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Sonst wohl kam's, und da glaubt' ich es leicht. Nun aber entbehr' ich's", "tokens": ["Sonst", "wohl", "kam's", ",", "und", "da", "glaubt'", "ich", "es", "leicht", ".", "Nun", "a\u00b7ber", "ent\u00b7behr'", "ich's"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "$,", "KON", "ADV", "VVFIN", "PPER", "PPER", "ADJD", "$.", "ADV", "ADV", "VVFIN", "PIS"], "meter": "--+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Schon seit Tagen, und gleich dr\u00e4ngt sich der Zweifel ans Herz. \u2013", "tokens": ["Schon", "seit", "Ta\u00b7gen", ",", "und", "gleich", "dr\u00e4ngt", "sich", "der", "Zwei\u00b7fel", "ans", "Herz", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "KON", "ADV", "VVFIN", "PRF", "ART", "NN", "APPRART", "NN", "$.", "$("], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.21": {"text": "Ihr habt recht. Schwer ist es, vergangene Dinge zu glauben.", "tokens": ["Ihr", "habt", "recht", ".", "Schwer", "ist", "es", ",", "ver\u00b7gan\u00b7ge\u00b7ne", "Din\u00b7ge", "zu", "glau\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$.", "NN", "VAFIN", "PPER", "$,", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+---+--+--+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Grade die liebsten, sie sehn heut wie die fremdesten aus;", "tokens": ["Gra\u00b7de", "die", "liebs\u00b7ten", ",", "sie", "sehn", "heut", "wie", "die", "frem\u00b7des\u00b7ten", "aus", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "$,", "PPER", "VVFIN", "ADV", "KOKOM", "ART", "ADJA", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.23": {"text": "Jeder erfuhr's, ich auch, und es braucht nicht Meilen dazwischen,", "tokens": ["Je\u00b7der", "er\u00b7fuhr's", ",", "ich", "auch", ",", "und", "es", "braucht", "nicht", "Mei\u00b7len", "da\u00b7zwi\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PPER", "ADV", "$,", "KON", "PPER", "VVFIN", "PTKNEG", "NN", "PAV", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.24": {"text": "Da\u00df unm\u00f6glich erscheint, was wir mit Augen gesehn.", "tokens": ["Da\u00df", "un\u00b7m\u00f6g\u00b7lich", "er\u00b7scheint", ",", "was", "wir", "mit", "Au\u00b7gen", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVFIN", "$,", "PRELS", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+---+-+-+--+", "measure": "iambic.penta.chol"}, "line.25": {"text": "Vor zwei Jahren einmal, im Sommer, ich war kaum sechzehn,", "tokens": ["Vor", "zwei", "Jah\u00b7ren", "ein\u00b7mal", ",", "im", "Som\u00b7mer", ",", "ich", "war", "kaum", "sech\u00b7zehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADV", "$,", "APPRART", "NN", "$,", "PPER", "VAFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+--+-+++-+", "measure": "trochaic.octa.plus.relaxed"}, "line.26": {"text": "Fa\u00dft' ein Fieber mich an, eisig und bang wie der Tod,", "tokens": ["Fa\u00dft'", "ein", "Fie\u00b7ber", "mich", "an", ",", "ei\u00b7sig", "und", "bang", "wie", "der", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,", "ADJD", "KON", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.27": {"text": "Und sie dachten, es sei mein Letztes; ich selber, ich dachte", "tokens": ["Und", "sie", "dach\u00b7ten", ",", "es", "sei", "mein", "Letz\u00b7tes", ";", "ich", "sel\u00b7ber", ",", "ich", "dach\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$.", "PPER", "ADV", "$,", "PPER", "VVFIN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.28": {"text": "Wenig. So lieg' ich am Tag, schon f\u00fcr verloren, im Bett,", "tokens": ["We\u00b7nig", ".", "So", "lieg'", "ich", "am", "Tag", ",", "schon", "f\u00fcr", "ver\u00b7lo\u00b7ren", ",", "im", "Bett", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$,", "ADV", "APPR", "VVPP", "$,", "APPRART", "NN", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.29": {"text": "Gar einsam, denn die Mutter besorgte den Herd, und Pepina", "tokens": ["Gar", "ein\u00b7sam", ",", "denn", "die", "Mut\u00b7ter", "be\u00b7sorg\u00b7te", "den", "Herd", ",", "und", "Pe\u00b7pi\u00b7na"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KON", "ART", "NN", "VVFIN", "ART", "NN", "$,", "KON", "NE"], "meter": "-+-+-+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.30": {"text": "War in Meta. Da kommt einer zur Kammer herein,", "tokens": ["War", "in", "Me\u00b7ta", ".", "Da", "kommt", "ei\u00b7ner", "zur", "Kam\u00b7mer", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NE", "$.", "ADV", "VVFIN", "ART", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.31": {"text": "Setzt sich neben das Bett und fa\u00dft mir die Hand und beschaut mich", "tokens": ["Setzt", "sich", "ne\u00b7ben", "das", "Bett", "und", "fa\u00dft", "mir", "die", "Hand", "und", "be\u00b7schaut", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "KON", "VVFIN", "PPER", "ART", "NN", "KON", "VVFIN", "PPER"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.32": {"text": "Lang. So verworren ich war, dennoch erkannt' ich ihn gleich", "tokens": ["Lang", ".", "So", "ver\u00b7wor\u00b7ren", "ich", "war", ",", "den\u00b7noch", "er\u00b7kannt'", "ich", "ihn", "gleich"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "VVFIN", "PPER", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "PPER", "ADV"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.33": {"text": "Und schwieg still. Um die Welt nicht h\u00e4tt' ich ein W\u00f6rtchen geredet.", "tokens": ["Und", "schwieg", "still", ".", "Um", "die", "Welt", "nicht", "h\u00e4tt'", "ich", "ein", "W\u00f6rt\u00b7chen", "ge\u00b7re\u00b7det", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$.", "KOUI", "ART", "NN", "PTKNEG", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "Doch er sagte: Du bist krank, Mariuccia. Geduld!", "tokens": ["Doch", "er", "sag\u00b7te", ":", "Du", "bist", "krank", ",", "Ma\u00b7riuc\u00b7cia", ".", "Ge\u00b7duld", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "PPER", "VAFIN", "ADJD", "$,", "NE", "$.", "NN", "$."], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.35": {"text": "Du wirst wieder genesen; ich fragte den Arzt; er versprach mir's.", "tokens": ["Du", "wirst", "wie\u00b7der", "ge\u00b7ne\u00b7sen", ";", "ich", "frag\u00b7te", "den", "Arzt", ";", "er", "ver\u00b7sprach", "mir'", "s."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$.", "PPER", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "NE", "NE"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.36": {"text": "Halte das Herz nur still! \u2013 Aber er k\u00fc\u00dfte mich auch", "tokens": ["Hal\u00b7te", "das", "Herz", "nur", "still", "!", "\u2013", "A\u00b7ber", "er", "k\u00fc\u00df\u00b7te", "mich", "auch"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "ADV", "PTKVZ", "$.", "$(", "KON", "PPER", "VVFIN", "PPER", "ADV"], "meter": "+--+-++--+--+", "measure": "iambic.hexa.invert"}, "line.37": {"text": "Und sprach weiter: Du sollst bald lachen und singen wie fr\u00fcher,", "tokens": ["Und", "sprach", "wei\u00b7ter", ":", "Du", "sollst", "bald", "la\u00b7chen", "und", "sin\u00b7gen", "wie", "fr\u00fc\u00b7her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$.", "PPER", "VMFIN", "ADV", "VVINF", "KON", "VVFIN", "KOKOM", "ADJD", "$,"], "meter": "--+--+-+--+-+-+", "measure": "anapaest.di.plus"}, "line.38": {"text": "Und viel besser sogar. \u2013 Sehet, das sagt' er und ging.", "tokens": ["Und", "viel", "bes\u00b7ser", "so\u00b7gar", ".", "\u2013", "Se\u00b7het", ",", "das", "sagt'", "er", "und", "ging", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ADV", "$.", "$(", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "KON", "VVFIN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.39": {"text": "Von Stund an wie ein Wunder geschah mir's, da\u00df mich das Fieber", "tokens": ["Von", "Stund", "an", "wie", "ein", "Wun\u00b7der", "ge\u00b7schah", "mir's", ",", "da\u00df", "mich", "das", "Fie\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PTKVZ", "KOKOM", "ART", "NN", "VVFIN", "NE", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.40": {"text": "Lie\u00df. Schon Tages darauf sa\u00df ich auf offenem Dach.", "tokens": ["Lie\u00df", ".", "Schon", "Ta\u00b7ges", "da\u00b7rauf", "sa\u00df", "ich", "auf", "of\u00b7fe\u00b7nem", "Dach", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ADV", "NN", "PAV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.41": {"text": "Doch mein Lachen und Singen \u2013 wo blieb's? Jetzt, seh' ich denselben,", "tokens": ["Doch", "mein", "La\u00b7chen", "und", "Sin\u00b7gen", "\u2013", "wo", "blie\u00b7b's", "?", "Jetzt", ",", "seh'", "ich", "den\u00b7sel\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN", "$(", "PWAV", "NE", "$.", "ADV", "$,", "VVFIN", "PPER", "PDS", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.42": {"text": "Ist mir's immer, es sei falsch und ich h\u00e4tt' es getr\u00e4umt.", "tokens": ["Ist", "mir's", "im\u00b7mer", ",", "es", "sei", "falsch", "und", "ich", "h\u00e4tt'", "es", "ge\u00b7tr\u00e4umt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADV", "$,", "PPER", "VAFIN", "ADJD", "KON", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.43": {"text": "Mag's denn sein, wie es will \u2013 was ist's auch? Aber da schaut nur", "tokens": ["Mag's", "denn", "sein", ",", "wie", "es", "will", "\u2013", "was", "ist's", "auch", "?", "A\u00b7ber", "da", "schaut", "nur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "KON", "VAINF", "$,", "PWAV", "PPER", "VMFIN", "$(", "PWS", "VAFIN", "ADV", "$.", "KON", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.44": {"text": "\u00dcber das Meer. Kommt dort nicht von Neapel das Boot?", "tokens": ["\u00dc\u00b7ber", "das", "Meer", ".", "Kommt", "dort", "nicht", "von", "Nea\u00b7pel", "das", "Boot", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "VVFIN", "ADV", "PTKNEG", "APPR", "NE", "ART", "NN", "$."], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.45": {"text": "Ja, ich erkenne das Wimpel Luigis. Seid Ihr auch jetzt noch", "tokens": ["Ja", ",", "ich", "er\u00b7ken\u00b7ne", "das", "Wim\u00b7pel", "Lui\u00b7gis", ".", "Seid", "Ihr", "auch", "jetzt", "noch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "ART", "NN", "NE", "$.", "VAIMP", "PPER", "ADV", "ADV", "ADV"], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.46": {"text": "Nicht zufrieden? \u2013 Ich w\u00e4r's, w\u00e4re der Brief mir gewi\u00df,", "tokens": ["Nicht", "zu\u00b7frie\u00b7den", "?", "\u2013", "Ich", "w\u00e4r's", ",", "w\u00e4\u00b7re", "der", "Brief", "mir", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$.", "$(", "PPER", "VAFIN", "$,", "VAFIN", "ART", "NN", "PPER", "ADV", "$,"], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.47": {"text": "Br\u00e4cht' er Erw\u00fcnschtes, und h\u00e4tt' ich ihn schon. Noch tr\u00e4gt ihn die falsche", "tokens": ["Br\u00e4cht'", "er", "Er\u00b7w\u00fcnschtes", ",", "und", "h\u00e4tt'", "ich", "ihn", "schon", ".", "Noch", "tr\u00e4gt", "ihn", "die", "fal\u00b7sche"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "$,", "KON", "VAFIN", "PPER", "PPER", "ADV", "$.", "ADV", "VVFIN", "PPER", "ART", "ADJA"], "meter": "-+-+-+-+-++--+-", "measure": "iambic.septa.relaxed"}, "line.48": {"text": "Welle. Verzeih! mich treibt's selbst an den Hafen hinab.", "tokens": ["Wel\u00b7le", ".", "Ver\u00b7zeih", "!", "mich", "treibt's", "selbst", "an", "den", "Ha\u00b7fen", "hin\u00b7ab", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.49": {"text": "Und sie nickte, die Freundin. Und kam ich zur\u00fcck und erklomm dann", "tokens": ["Und", "sie", "nick\u00b7te", ",", "die", "Freun\u00b7din", ".", "Und", "kam", "ich", "zu\u00b7r\u00fcck", "und", "er\u00b7klomm", "dann"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "ART", "NN", "$.", "KON", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "ADV"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.50": {"text": "Jubelnd das Dach, sie stand droben und wartete mein \u2013", "tokens": ["Ju\u00b7belnd", "das", "Dach", ",", "sie", "stand", "dro\u00b7ben", "und", "war\u00b7te\u00b7te", "mein", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "PPER", "VVFIN", "ADV", "KON", "VVFIN", "PPOSAT", "$("], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.51": {"text": "Aber nun lest mir ein wenig zum Dank! \u2013 Ich tat es, so gut ich", "tokens": ["A\u00b7ber", "nun", "lest", "mir", "ein", "we\u00b7nig", "zum", "Dank", "!", "\u2013", "Ich", "tat", "es", ",", "so", "gut", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "PIS", "APPRART", "NN", "$.", "$(", "PPER", "VVFIN", "PPER", "$,", "ADV", "ADJD", "PPER"], "meter": "+-+-+-+--+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.52": {"text": "Konnte; das sinnige Deutsch setzt' ich in welsche Musik.", "tokens": ["Konn\u00b7te", ";", "das", "sin\u00b7ni\u00b7ge", "Deutsch", "setzt'", "ich", "in", "wel\u00b7sche", "Mu\u00b7sik", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$.", "ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PWAT", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.53": {"text": "Und sie lauscht' und wandte sich dann, schwerm\u00fctig \u2013 Das ist nun", "tokens": ["Und", "sie", "lauscht'", "und", "wand\u00b7te", "sich", "dann", ",", "schwer\u00b7m\u00fc\u00b7tig", "\u2013", "Das", "ist", "nun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "PRF", "ADV", "$,", "ADJD", "$(", "PDS", "VAFIN", "ADV"], "meter": "--+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.54": {"text": "Schon seit Tagen vorbei. Freuden und Schmerzen", "tokens": ["Schon", "seit", "Ta\u00b7gen", "vor\u00b7bei", ".", "Freu\u00b7den", "und", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "PTKVZ", "$.", "NN", "KON", "NN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.55": {"text": "Mu\u00df ich tragen, seitdem ihr liebes Auge zum letzten", "tokens": ["Mu\u00df", "ich", "tra\u00b7gen", ",", "seit\u00b7dem", "ihr", "lie\u00b7bes", "Au\u00b7ge", "zum", "letz\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "APPRART", "ADJA"], "meter": "+-+----+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.56": {"text": "Mal mich gr\u00fc\u00dfte bei Nacht und mir so pl\u00f6tzlich entschwand.", "tokens": ["Mal", "mich", "gr\u00fc\u00df\u00b7te", "bei", "Nacht", "und", "mir", "so", "pl\u00f6tz\u00b7lich", "ent\u00b7schwand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "APPR", "NN", "KON", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.57": {"text": "Jetzt belagert das Dach hartn\u00e4ckig die garstige Hexe", "tokens": ["Jetzt", "be\u00b7la\u00b7gert", "das", "Dach", "hart\u00b7n\u00e4\u00b7ckig", "die", "gars\u00b7ti\u00b7ge", "He\u00b7xe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "ART", "ADJA", "NN"], "meter": "--+--+-+--+-+-+", "measure": "anapaest.di.plus"}, "line.58": {"text": "Angiolina. Sie tut freilich gesch\u00e4ftig genug.", "tokens": ["An\u00b7gi\u00b7o\u00b7li\u00b7na", ".", "Sie", "tut", "frei\u00b7lich", "ge\u00b7sch\u00e4f\u00b7tig", "ge\u00b7nug", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "ADV", "ADJD", "ADV", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.59": {"text": "Bald mu\u00df Fiffi heran, da badet und k\u00e4mmt sie den Armen,", "tokens": ["Bald", "mu\u00df", "Fif\u00b7fi", "he\u00b7ran", ",", "da", "ba\u00b7det", "und", "k\u00e4mmt", "sie", "den", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NE", "PTKVZ", "$,", "ADV", "VVFIN", "KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.60": {"text": "Oder sie nimmt ein Buch, das sie zum Scheine studiert.", "tokens": ["O\u00b7der", "sie", "nimmt", "ein", "Buch", ",", "das", "sie", "zum", "Schei\u00b7ne", "stu\u00b7diert", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.61": {"text": "Ach, und leider verdunkelt Gew\u00f6lk am Tage den Himmel,", "tokens": ["Ach", ",", "und", "lei\u00b7der", "ver\u00b7dun\u00b7kelt", "Ge\u00b7w\u00f6lk", "am", "Ta\u00b7ge", "den", "Him\u00b7mel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "ADV", "VVPP", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.62": {"text": "Kein willkommener Strahl scheucht mir die Neidische fort.", "tokens": ["Kein", "will\u00b7kom\u00b7me\u00b7ner", "Strahl", "scheucht", "mir", "die", "Nei\u00b7di\u00b7sche", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.63": {"text": "Mir ist doppelt die Sonne versteckt. Ich frage Luisa,", "tokens": ["Mir", "ist", "dop\u00b7pelt", "die", "Son\u00b7ne", "ver\u00b7steckt", ".", "Ich", "fra\u00b7ge", "Lu\u00b7i\u00b7sa", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ART", "NN", "VVPP", 