], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.68": {"text": "Staatsgef\u00e4hrlich? Und was, denken sie, f\u00fchrt' ich im Schild?", "tokens": ["Staats\u00b7ge\u00b7f\u00e4hr\u00b7lich", "?", "Und", "was", ",", "den\u00b7ken", "sie", ",", "f\u00fchrt'", "ich", "im", "Schild", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "KON", "PWS", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.69": {"text": "Ich, als merkt' ich es nicht, gleichg\u00fcltig in H\u00e4nden den alten", "tokens": ["Ich", ",", "als", "merkt'", "ich", "es", "nicht", ",", "gleich\u00b7g\u00fcl\u00b7tig", "in", "H\u00e4n\u00b7den", "den", "al\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "KOUS", "VVFIN", "PPER", "PPER", "PTKNEG", "$,", "ADJD", "APPR", "NN", "ART", "ADJA"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.70": {"text": "Vater Homer, vom Altan halt' ich die Sp\u00e4her in Schach.", "tokens": ["Va\u00b7ter", "Ho\u00b7mer", ",", "vom", "Al\u00b7tan", "halt'", "ich", "die", "Sp\u00e4\u00b7her", "in", "Schach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "APPR", "NE", "$."], "meter": "+-+--+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.71": {"text": "Immer das n\u00e4mliche les' ich, und nicht nur, weil ich zerstreut bin:", "tokens": ["Im\u00b7mer", "das", "n\u00e4m\u00b7li\u00b7che", "les'", "ich", ",", "und", "nicht", "nur", ",", "weil", "ich", "zer\u00b7streut", "bin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "