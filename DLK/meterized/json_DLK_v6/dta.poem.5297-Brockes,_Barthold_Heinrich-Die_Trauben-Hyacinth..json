{"dta.poem.5297": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Trauben-Hyacinth.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Angenehmes Fr\u00fchlings-Kindchen,", "tokens": ["An\u00b7ge\u00b7neh\u00b7mes", "Fr\u00fch\u00b7lings\u00b7Kind\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kleines Trauben-Hyacintchen,", "tokens": ["Klei\u00b7nes", "Trau\u00b7ben\u00b7Hya\u00b7cint\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Deiner Farb\u2019 und Bildung Zier", "tokens": ["Dei\u00b7ner", "Fa\u00b7rb'", "und", "Bil\u00b7dung", "Zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN", "NN"], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Zeiget, mit Verwundrung, mir,", "tokens": ["Zei\u00b7get", ",", "mit", "Ver\u00b7wund\u00b7rung", ",", "mir", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NN", "$,", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Von der bildenden Natur", "tokens": ["Von", "der", "bil\u00b7den\u00b7den", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine neue Sch\u00f6nheits-Spur.", "tokens": ["Ei\u00b7ne", "neu\u00b7e", "Sch\u00f6n\u00b7heits\u00b7Spur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "An des Stengels blauer Spitzen", "tokens": ["An", "des", "Sten\u00b7gels", "blau\u00b7er", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sieht man, wenn man billig sieht,", "tokens": ["Sieht", "man", ",", "wenn", "man", "bil\u00b7lig", "sieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Deiner sonderbahren Bl\u00fcht", "tokens": ["Dei\u00b7ner", "son\u00b7der\u00b7bah\u00b7ren", "Bl\u00fcht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Kleine blaue Kugeln sitzen,", "tokens": ["Klei\u00b7ne", "blau\u00b7e", "Ku\u00b7geln", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Dran, so lange sich ihr Blat", "tokens": ["Dran", ",", "so", "lan\u00b7ge", "sich", "ihr", "Blat"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "$,", "ADV", "ADV", "PRF", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Noch nicht aufgeschlossen hat,", "tokens": ["Noch", "nicht", "auf\u00b7ge\u00b7schlos\u00b7sen", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Wie ein Purpur-Stern sie schm\u00fccket,", "tokens": ["Wie", "ein", "Pur\u00b7pur\u00b7S\u00b7tern", "sie", "schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.14": {"text": "Man, nicht sonder Lust, erblicket.", "tokens": ["Man", ",", "nicht", "son\u00b7der", "Lust", ",", "er\u00b7bli\u00b7cket", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "$,", "PTKNEG", "KON", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Aber wie von ungefehr", "tokens": ["A\u00b7ber", "wie", "von", "un\u00b7ge\u00b7fehr"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Meine Blicke hin und her", "tokens": ["Mei\u00b7ne", "Bli\u00b7cke", "hin", "und", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKVZ", "KON", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Auf die ofnen Blumen lieffen,", "tokens": ["Auf", "die", "of\u00b7nen", "Blu\u00b7men", "lief\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Kont ich, in den blauen Tieffen,", "tokens": ["Kont", "ich", ",", "in", "den", "blau\u00b7en", "Tief\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Wie aus Himmel-blauen H\u00f6hen,", "tokens": ["Wie", "aus", "Him\u00b7mel\u00b7blau\u00b7en", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Silber-weisse Sternchen sehen,", "tokens": ["Sil\u00b7ber\u00b7weis\u00b7se", "Stern\u00b7chen", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Die in einer blauen Nacht,", "tokens": ["Die", "in", "ei\u00b7ner", "blau\u00b7en", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "So sie rings bedeckt, im Dunckeln,", "tokens": ["So", "sie", "rings", "be\u00b7deckt", ",", "im", "Dun\u00b7ckeln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VVPP", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Mit dadurch erh\u00f6hter Pracht", "tokens": ["Mit", "da\u00b7durch", "er\u00b7h\u00f6h\u00b7ter", "Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PAV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Noch um desto heller funckeln.", "tokens": ["Noch", "um", "des\u00b7to", "hel\u00b7ler", "fun\u00b7ckeln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Jhr so zierliches Gepr\u00e4nge,", "tokens": ["Ihr", "so", "zier\u00b7li\u00b7ches", "Ge\u00b7pr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Jhre Nettigkeit und Menge,", "tokens": ["Ih\u00b7re", "Net\u00b7tig\u00b7keit", "und", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Die die blauen Tieffen f\u00fcllt,", "tokens": ["Die", "die", "blau\u00b7en", "Tief\u00b7fen", "f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Schiene mir des Himmels Bild,", "tokens": ["Schie\u00b7ne", "mir", "des", "Him\u00b7mels", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}