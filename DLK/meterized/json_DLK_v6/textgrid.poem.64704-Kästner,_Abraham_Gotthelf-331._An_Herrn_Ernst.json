{"textgrid.poem.64704": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "331. An Herrn Ernst", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was Eyern ohne Hahn gebricht,", "tokens": ["Was", "Ey\u00b7ern", "oh\u00b7ne", "Hahn", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wei\u00dft du bisher, begl\u00fcckter Knabe, nicht:", "tokens": ["Wei\u00dft", "du", "bis\u00b7her", ",", "be\u00b7gl\u00fcck\u00b7ter", "Kna\u00b7be", ",", "nicht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ADJA", "NN", "$,", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Drum magst du noch mit Ball und Kreisel spielen;", "tokens": ["Drum", "magst", "du", "noch", "mit", "Ball", "und", "Krei\u00b7sel", "spie\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Lern' es nur eh' verstehn, als f\u00fchlen.", "tokens": ["Lern'", "es", "nur", "eh'", "ver\u00b7stehn", ",", "als", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "VVINF", "$,", "KOUS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}