{"textgrid.poem.59884": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach einem hohen Ziele streben wir,", "tokens": ["Nach", "ei\u00b7nem", "ho\u00b7hen", "Zie\u00b7le", "stre\u00b7ben", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So ich wie du!", "tokens": ["So", "ich", "wie", "du", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PWAV", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Uns in Gefangenschaft begeben wir,", "tokens": ["Uns", "in", "Ge\u00b7fan\u00b7gen\u00b7schaft", "be\u00b7ge\u00b7ben", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So ich wie du!", "tokens": ["So", "ich", "wie", "du", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PWAV", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Getrennt und doch vereint, so leben wir,", "tokens": ["Ge\u00b7trennt", "und", "doch", "ver\u00b7eint", ",", "so", "le\u00b7ben", "wir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADV", "VVPP", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So ich wie du!", "tokens": ["So", "ich", "wie", "du", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PWAV", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Dich fing mein Witz und mich dein sch\u00f6nes Auge,", "tokens": ["Dich", "fing", "mein", "Witz", "und", "mich", "dein", "sch\u00f6\u00b7nes", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und wie zwei Fisch' am Angel schweben wir,", "tokens": ["Und", "wie", "zwei", "Fisch'", "am", "An\u00b7gel", "schwe\u00b7ben", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "CARD", "NN", "APPRART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "So ich wie du!", "tokens": ["So", "ich", "wie", "du", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PWAV", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Und doch den Fischen ungleich \u2013 durch die L\u00fcfte", "tokens": ["Und", "doch", "den", "Fi\u00b7schen", "un\u00b7gleich", "\u2013", "durch", "die", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "ADJD", "$(", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Uns wie ein Adlerpaar erheben wir,", "tokens": ["Uns", "wie", "ein", "Ad\u00b7ler\u00b7paar", "er\u00b7he\u00b7ben", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOKOM", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "So ich wie du!", "tokens": ["So", "ich", "wie", "du", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PWAV", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}