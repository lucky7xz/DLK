{"textgrid.poem.40365": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Der kleine S\u00fcnder", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gestern lief der Peter weg,", "tokens": ["Ge\u00b7stern", "lief", "der", "Pe\u00b7ter", "weg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NE", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "spinnefix verstohlen;", "tokens": ["spin\u00b7ne\u00b7fix", "ver\u00b7stoh\u00b7len", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "setzt sich Mutter den B\u00e4nderhut auf:", "tokens": ["setzt", "sich", "Mut\u00b7ter", "den", "B\u00e4n\u00b7der\u00b7hut", "auf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "wart, ich will dich holen!", "tokens": ["wart", ",", "ich", "will", "dich", "ho\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VMFIN", "PRF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sausepeter,", "tokens": ["Sau\u00b7se\u00b7pe\u00b7ter", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Flausepeter,", "tokens": ["Flau\u00b7se\u00b7pe\u00b7ter", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "kleiner S\u00fcnder, wo bist du?", "tokens": ["klei\u00b7ner", "S\u00fcn\u00b7der", ",", "wo", "bist", "du", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "VAFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hahnematz steht auf der Wiese,", "tokens": ["Hah\u00b7ne\u00b7matz", "steht", "auf", "der", "Wie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\"kiek ins Gr\u00fcne!\" kr\u00e4ht er;", "tokens": ["\"", "kiek", "ins", "Gr\u00fc\u00b7ne", "!", "\"", "kr\u00e4ht", "er", ";"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "NE", "APPRART", "NN", "$.", "$(", "VVFIN", "PPER", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "sag mir, bunter Kickeriki,", "tokens": ["sag", "mir", ",", "bun\u00b7ter", "Ki\u00b7cke\u00b7ri\u00b7ki", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJD", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "wo ist unser Peter?", "tokens": ["wo", "ist", "un\u00b7ser", "Pe\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Bummelpeter,", "tokens": ["Bum\u00b7mel\u00b7pe\u00b7ter", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Schummelpeter,", "tokens": ["Schum\u00b7mel\u00b7pe\u00b7ter", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "kleiner S\u00fcnder, wo bist du?", "tokens": ["klei\u00b7ner", "S\u00fcn\u00b7der", ",", "wo", "bist", "du", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "VAFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie sie sich im Garten umkuckt,", "tokens": ["Wie", "sie", "sich", "im", "Gar\u00b7ten", "um\u00b7kuckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "ist er nicht zu sehen;", "tokens": ["ist", "er", "nicht", "zu", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "bleibt sie neben dem Spargelbeet", "tokens": ["bleibt", "sie", "ne\u00b7ben", "dem", "Spar\u00b7gel\u00b7beet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "unterm Pflaumbaum stehen.", "tokens": ["un\u00b7term", "Pflaum\u00b7baum", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Aber Peter,", "tokens": ["A\u00b7ber", "Pe\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NE", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "nirgends steht er;", "tokens": ["nir\u00b7gends", "steht", "er", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "kleiner S\u00fcnder, wo bist du?", "tokens": ["klei\u00b7ner", "S\u00fcn\u00b7der", ",", "wo", "bist", "du", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "VAFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "H\u00f6rt sie etwas lachen, horch,", "tokens": ["H\u00f6rt", "sie", "et\u00b7was", "la\u00b7chen", ",", "horch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "VVINF", "$,", "VVIMP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "oben aus dem Baume;", "tokens": ["o\u00b7ben", "aus", "dem", "Bau\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "sitzt der Peter seelenvergn\u00fcgt,", "tokens": ["sitzt", "der", "Pe\u00b7ter", "see\u00b7len\u00b7ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "pfl\u00fcckt sich eine Pflaume.", "tokens": ["pfl\u00fcckt", "sich", "ei\u00b7ne", "Pflau\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wirft ein Steinchen,", "tokens": ["Wirft", "ein", "Stein\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "schwenkt die Beinchen,", "tokens": ["schwenkt", "die", "Be\u00b7in\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "wupptich \u2013: Mutter, da ", "tokens": ["wupp\u00b7tich", "\u2013", ":", "Mut\u00b7ter", ",", "da"], "token_info": ["word", "punct", "punct", "word", "punct", "word"], "pos": ["ADJD", "$(", "$.", "NN", "$,", "KOUS"], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}