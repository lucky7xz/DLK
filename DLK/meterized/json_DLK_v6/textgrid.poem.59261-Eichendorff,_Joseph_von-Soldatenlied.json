{"textgrid.poem.59261": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Soldatenlied", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was zieht da f\u00fcr schreckliches Sausen,", "tokens": ["Was", "zieht", "da", "f\u00fcr", "schreck\u00b7li\u00b7ches", "Sau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wie Pfeifen durch Sturmeswehn?", "tokens": ["Wie", "Pfei\u00b7fen", "durch", "Stur\u00b7mes\u00b7wehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das wendet das Herz recht vor Grausen,", "tokens": ["Das", "wen\u00b7det", "das", "Herz", "recht", "vor", "Grau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Als sollte die Welt vergehn.", "tokens": ["Als", "soll\u00b7te", "die", "Welt", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Das Fu\u00dfvolk kommt da geschritten,", "tokens": ["Das", "Fu\u00df\u00b7volk", "kommt", "da", "ge\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Trommeln wirbeln voran,", "tokens": ["Die", "Trom\u00b7meln", "wir\u00b7beln", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Fahne in ihrer Mitten", "tokens": ["Die", "Fah\u00b7ne", "in", "ih\u00b7rer", "Mit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Weht \u00fcber den gr\u00fcnen Plan,", "tokens": ["Weht", "\u00fc\u00b7ber", "den", "gr\u00fc\u00b7nen", "Plan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sie prangt in schneewei\u00dfem Kleide", "tokens": ["Sie", "prangt", "in", "schnee\u00b7wei\u00b7\u00dfem", "Klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Als wie eine milde Braut,", "tokens": ["Als", "wie", "ei\u00b7ne", "mil\u00b7de", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die gibt dem hohe Freude,", "tokens": ["Die", "gibt", "dem", "ho\u00b7he", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wen Gott ihr angetraut.", "tokens": ["Wen", "Gott", "ihr", "an\u00b7ge\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Sie haben sie recht umschlossen,", "tokens": ["Sie", "ha\u00b7ben", "sie", "recht", "um\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Dicht Mann an Mann ger\u00fcckt,", "tokens": ["Dicht", "Mann", "an", "Mann", "ge\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "So ziehen die Kriegsgenossen", "tokens": ["So", "zie\u00b7hen", "die", "Kriegs\u00b7ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Sreng, schweigend und ungeschm\u00fcckt,", "tokens": ["Sreng", ",", "schwei\u00b7gend", "und", "un\u00b7ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "++--+-+", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "Wie Gottes dunkler Wille,", "tokens": ["Wie", "Got\u00b7tes", "dunk\u00b7ler", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Wie ein Gewitter schwer,", "tokens": ["Wie", "ein", "Ge\u00b7wit\u00b7ter", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Da wird es ringsum so stille,", "tokens": ["Da", "wird", "es", "ring\u00b7sum", "so", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Der Tod nur blitzt hin und her.", "tokens": ["Der", "Tod", "nur", "blitzt", "hin", "und", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Wie seltsame Kl\u00e4nge schwingen", "tokens": ["Wie", "selt\u00b7sa\u00b7me", "Kl\u00e4n\u00b7ge", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sich dort von der Waldesh\u00f6h!", "tokens": ["Sich", "dort", "von", "der", "Wal\u00b7des\u00b7h\u00f6h", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ja, H\u00f6rner sind es, die singen", "tokens": ["Ja", ",", "H\u00f6r\u00b7ner", "sind", "es", ",", "die", "sin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "NN", "VAFIN", "PPER", "$,", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wie rasend vor Lust und Weh.", "tokens": ["Wie", "ra\u00b7send", "vor", "Lust", "und", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Die jungen J\u00e4ger sich zeigen", "tokens": ["Die", "jun\u00b7gen", "J\u00e4\u00b7ger", "sich", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PRF", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dort dr\u00fcben im gr\u00fcnen Wald,", "tokens": ["Dort", "dr\u00fc\u00b7ben", "im", "gr\u00fc\u00b7nen", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bald schimmernd zwischen den Zweigen,", "tokens": ["Bald", "schim\u00b7mernd", "zwi\u00b7schen", "den", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Bald lauernd im Hinterhalt.", "tokens": ["Bald", "lau\u00b7ernd", "im", "Hin\u00b7ter\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wohl sinkt da in ewiges Schweigen", "tokens": ["Wohl", "sinkt", "da", "in", "e\u00b7wi\u00b7ges", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Manch schlanke Rittergestalt,", "tokens": ["Manch", "schlan\u00b7ke", "Rit\u00b7ter\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Die anderen \u00fcber ihn steigen,", "tokens": ["Die", "an\u00b7de\u00b7ren", "\u00fc\u00b7ber", "ihn", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Hurra! in dem sch\u00f6nen Wald,", "tokens": ["Hur\u00b7ra", "!", "in", "dem", "sch\u00f6\u00b7nen", "Wald", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Es funkelt das Blau durch die B\u00e4ume \u2013", "tokens": ["Es", "fun\u00b7kelt", "das", "Blau", "durch", "die", "B\u00e4u\u00b7me", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "\u00bbach, Vater, ich komme bald!\u00ab", "tokens": ["\u00bb", "ach", ",", "Va\u00b7ter", ",", "ich", "kom\u00b7me", "bald", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "NN", "$,", "PPER", "VVFIN", "ADV", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Trompeten nur h\u00f6r ich werben", "tokens": ["Trom\u00b7pe\u00b7ten", "nur", "h\u00f6r", "ich", "wer\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "PPER", "VVINF"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "So hell durch die Fr\u00fchlingsluft,", "tokens": ["So", "hell", "durch", "die", "Fr\u00fch\u00b7lings\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zur Hochzeit oder zum Sterben", "tokens": ["Zur", "Hoch\u00b7zeit", "o\u00b7der", "zum", "Ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "So \u00fcberm\u00e4chtig es ruft.", "tokens": ["So", "\u00fc\u00b7ber\u00b7m\u00e4ch\u00b7tig", "es", "ruft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Das sind meine lieben Reiter,", "tokens": ["Das", "sind", "mei\u00b7ne", "lie\u00b7ben", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die rufen hinaus zur Schlacht,", "tokens": ["Die", "ru\u00b7fen", "hin\u00b7aus", "zur", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APZR", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das sind meine lustigen Reiter,", "tokens": ["Das", "sind", "mei\u00b7ne", "lus\u00b7ti\u00b7gen", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nun, Liebchen, gute Nacht!", "tokens": ["Nun", ",", "Lieb\u00b7chen", ",", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie wird es da vorne so heiter,", "tokens": ["Wie", "wird", "es", "da", "vor\u00b7ne", "so", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Wie spr\u00fchet der Morgenwind,", "tokens": ["Wie", "spr\u00fc\u00b7het", "der", "Mor\u00b7gen\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "In den Sieg, in den Tod und weiter,", "tokens": ["In", "den", "Sieg", ",", "in", "den", "Tod", "und", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "KON", "ADV", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Bis da\u00df wir im Himmel sind!", "tokens": ["Bis", "da\u00df", "wir", "im", "Him\u00b7mel", "sind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "APPRART", "NN", "VAFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}}}}