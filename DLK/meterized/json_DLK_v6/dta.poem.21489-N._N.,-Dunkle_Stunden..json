{"dta.poem.21489": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Dunkle Stunden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Novemberwind! Novemberwind! Der Himmel so grau und die W\u00e4lder", "tokens": ["No\u00b7vem\u00b7ber\u00b7wind", "!", "No\u00b7vem\u00b7ber\u00b7wind", "!", "Der", "Him\u00b7mel", "so", "grau", "und", "die", "W\u00e4l\u00b7der"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "NN", "$.", "ART", "NN", "ADV", "ADJD", "KON", "ART", "NN"], "meter": "-+-+-+-+-+--+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "entlaubt,", "tokens": ["ent\u00b7laubt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Und die Luft so kalt, die Luft so schaurig! Stumm lag an meiner", "tokens": ["Und", "die", "Luft", "so", "kalt", ",", "die", "Luft", "so", "schau\u00b7rig", "!", "Stumm", "lag", "an", "mei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "ADJD", "$,", "ART", "NN", "ADV", "ADJD", "$.", "NE", "VVFIN", "APPR", "PPOSAT"], "meter": "--+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Brust dein Haupt.", "tokens": ["Brust", "dein", "Haupt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Dein Haupt, du, deren Namen nie mein Lied, mein Mund niemals bekennt,", "tokens": ["Dein", "Haupt", ",", "du", ",", "de\u00b7ren", "Na\u00b7men", "nie", "mein", "Lied", ",", "mein", "Mund", "nie\u00b7mals", "be\u00b7kennt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "$,", "PRELAT", "NN", "ADV", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-++--+", "measure": "iambic.octa.plus.octa.plus.chol"}, "line.2": {"text": "Obwohl mein Herz doch alle Zeit f\u00fcr dich in Feuern der Liebe brennt.", "tokens": ["Ob\u00b7wohl", "mein", "Herz", "doch", "al\u00b7le", "Zeit", "f\u00fcr", "dich", "in", "Feu\u00b7ern", "der", "Lie\u00b7be", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "PIAT", "NN", "APPR", "PPER", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+--+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Dein Antlitz bla\u00df wie das fahle Licht, wie der scheidenden Sonne kalter Strahl,", "tokens": ["Dein", "Ant\u00b7litz", "bla\u00df", "wie", "das", "fah\u00b7le", "Licht", ",", "wie", "der", "schei\u00b7den\u00b7den", "Son\u00b7ne", "kal\u00b7ter", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-+--+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Und ich h\u00f6rte des Herzens dumpfen Schlag, wie Grabeslaut voll banger Qual.", "tokens": ["Und", "ich", "h\u00f6r\u00b7te", "des", "Her\u00b7zens", "dum\u00b7pfen", "Schlag", ",", "wie", "Gra\u00b7bes\u00b7laut", "voll", "ban\u00b7ger", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,", "PWAV", "ADJD", "ADJD", "ADJA", "NN", "$."], "meter": "--+--+-+-+-+-+-+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Und immer und immer bei Nacht und Tag, und immer und immer in", "tokens": ["Und", "im\u00b7mer", "und", "im\u00b7mer", "bei", "Nacht", "und", "Tag", ",", "und", "im\u00b7mer", "und", "im\u00b7mer", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KON", "ADV", "APPR", "NN", "KON", "NN", "$,", "KON", "ADV", "KON", "ADV", "APPR"], "meter": "-+--+--+-+-+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Lust und in Schmerz", "tokens": ["Lust", "und", "in", "Schmerz"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "APPR", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "T\u00f6nt in mein Ohr deiner Stimme Klang und greift mit Dornen in mein Herz:", "tokens": ["T\u00f6nt", "in", "mein", "Ohr", "dei\u00b7ner", "Stim\u00b7me", "Klang", "und", "greift", "mit", "Dor\u00b7nen", "in", "mein", "Herz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "NN", "KON", "VVFIN", "APPR", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-+-+-+-+", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "\u201eo wende von mir dein Auge ab und k\u00fcsse mich nicht mit dieser Gluth,", "tokens": ["\u201e", "o", "wen\u00b7de", "von", "mir", "dein", "Au\u00b7ge", "ab", "und", "k\u00fcs\u00b7se", "mich", "nicht", "mit", "die\u00b7ser", "Gluth", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "VVFIN", "APPR", "PPER", "PPOSAT", "NN", "PTKVZ", "KON", "VVFIN", "PPER", "PTKNEG", "APPR", "PDAT", "NN", "$,"], "meter": "-+----+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Du wei\u00dft ja nicht, wie bitterweh mir all\u2019 deine hei\u00dfe Liebe thut.", "tokens": ["Du", "wei\u00dft", "ja", "nicht", ",", "wie", "bit\u00b7ter\u00b7weh", "mir", "all'", "dei\u00b7ne", "hei\u00b7\u00dfe", "Lie\u00b7be", "thut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$,", "PWAV", "VVFIN", "PPER", "PIAT", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+----+-+-+", "measure": "unknown.measure.septa"}}, "stanza.6": {"line.1": {"text": "\u201eschaust du mich an, erschauert mir das Herz vor Angst und dunklem Weh,", "tokens": ["\u201e", "schaust", "du", "mich", "an", ",", "er\u00b7schau\u00b7ert", "mir", "das", "Herz", "vor", "Angst", "und", "dunk\u00b7lem", "Weh", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PRF", "PTKVZ", "$,", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Und meine arme Seele zittert, wenn ich in deine Augen seh\u2019.", "tokens": ["Und", "mei\u00b7ne", "ar\u00b7me", "See\u00b7le", "zit\u00b7tert", ",", "wenn", "ich", "in", "dei\u00b7ne", "Au\u00b7gen", "seh'", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.7": {"line.1": {"text": "\u201enein, geh\u2019 hinfort, und wende nicht dein Angesicht zu mir zur\u00fcck,", "tokens": ["\u201e", "nein", ",", "geh'", "hin\u00b7fort", ",", "und", "wen\u00b7de", "nicht", "dein", "An\u00b7ge\u00b7sicht", "zu", "mir", "zu\u00b7r\u00fcck", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "VVFIN", "ADV", "$,", "KON", "VVFIN", "PTKNEG", "PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Ich hab\u2019 auf all\u2019 und ewige Zeit verloren die Liebe, verloren mein Gl\u00fcck.", "tokens": ["Ich", "hab'", "auf", "all'", "und", "e\u00b7wi\u00b7ge", "Zeit", "ver\u00b7lo\u00b7ren", "die", "Lie\u00b7be", ",", "ver\u00b7lo\u00b7ren", "mein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "KON", "ADJA", "NN", "VVPP", "ART", "NN", "$,", "VVPP", "PPOSAT", "NN", "$."], "meter": "--+--+--+-+--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.8": {"line.1": {"text": "\u201ewohl f\u00fchl\u2019 ich hier, wenn\u2019s mich bedr\u00e4ngt, und lieg\u2019 ich ohne Schlaf", "tokens": ["\u201e", "wohl", "f\u00fchl'", "ich", "hier", ",", "wenn's", "mich", "be\u00b7dr\u00e4ngt", ",", "und", "lieg'", "ich", "oh\u00b7ne", "Schlaf"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "KON", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "und Ruh\u2019,", "tokens": ["und", "Ruh'", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Da\u00df ich ohne dich vergehen mu\u00df, denn all\u2019 meine Liebe \u2014 das bist du!", "tokens": ["Da\u00df", "ich", "oh\u00b7ne", "dich", "ver\u00b7ge\u00b7hen", "mu\u00df", ",", "denn", "all'", "mei\u00b7ne", "Lie\u00b7be", "das", "bist", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVINF", "VMFIN", "$,", "KON", "PIS", "PPOSAT", "NN", "$(", "PDS", "VAFIN", "PPER", "$."], "meter": "+-+-+-+--+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.9": {"line.1": {"text": "\u201emeine Arme m\u00f6cht\u2019 ich schlingen wohl und halten dich und k\u00fcssen dich,", "tokens": ["\u201e", "mei\u00b7ne", "Ar\u00b7me", "m\u00f6cht'", "ich", "schlin\u00b7gen", "wohl", "und", "hal\u00b7ten", "dich", "und", "k\u00fcs\u00b7sen", "dich", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VMFIN", "PPER", "VVFIN", "ADV", "KON", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Doch l\u00e4ngst vergang\u2019ne Tage dr\u00e4ngen sich dunkel zwischen dich und mich!", "tokens": ["Doch", "l\u00e4ngst", "ver\u00b7gang'\u00b7ne", "Ta\u00b7ge", "dr\u00e4n\u00b7gen", "sich", "dun\u00b7kel", "zwi\u00b7schen", "dich", "und", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "VVFIN", "PRF", "ADJD", "APPR", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.10": {"line.1": {"text": "\u201evor meiner Seele steigt es auf \u2014 verflossen ist schon Jahr um Jahr,", "tokens": ["\u201e", "vor", "mei\u00b7ner", "See\u00b7le", "steigt", "es", "auf", "ver\u00b7flos\u00b7sen", "ist", "schon", "Jahr", "um", "Jahr", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "APPR", "$(", "VVPP", "VAFIN", "ADV", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Doch hebt sich\u2019s auf vor meinem Geiste so schaurig und so d\u00fcster klar.", "tokens": ["Doch", "hebt", "sich's", "auf", "vor", "mei\u00b7nem", "Geis\u00b7te", "so", "schau\u00b7rig", "und", "so", "d\u00fcs\u00b7ter", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "APPR", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.11": {"line.1": {"text": "\u201emeine erste s\u00fc\u00dfe Jugendzeit, licht wie der Fr\u00fchling im Bl\u00fcthenschein,", "tokens": ["\u201e", "mei\u00b7ne", "ers\u00b7te", "s\u00fc\u00b7\u00dfe", "Ju\u00b7gend\u00b7zeit", ",", "licht", "wie", "der", "Fr\u00fch\u00b7ling", "im", "Bl\u00fc\u00b7then\u00b7schein", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "ADJA", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Und mein erster, mein erster Liebestraum h\u00fcllte mit Zaubern die Seele", "tokens": ["Und", "mein", "ers\u00b7ter", ",", "mein", "ers\u00b7ter", "Lie\u00b7best\u00b7raum", "h\u00fcll\u00b7te", "mit", "Zau\u00b7bern", "die", "See\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "$,", "PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "NN", "ART", "NN"], "meter": "--+--+-+--+--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "mir ein.", "tokens": ["mir", "ein", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.12": {"line.1": {"text": "\u201eo frage mich nicht, wie\u2019s einst geschah, \u2014 o wende dich ab, sieh mich", "tokens": ["\u201e", "o", "fra\u00b7ge", "mich", "nicht", ",", "wie's", "einst", "ge\u00b7schah", ",", "o", "wen\u00b7de", "dich", "ab", ",", "sieh", "mich"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "FM", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "ADV", "VVFIN", "$,", "$(", "FM", "VVFIN", "PPER", "PTKVZ", "$,", "VVIMP", "PPER"], "meter": "-+-+--+-+---+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "nicht an,", "tokens": ["nicht", "an", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "PTKVZ", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Ich kann nicht schauen, wie du weinst, du herzgeliebter theurer Mann.", "tokens": ["Ich", "kann", "nicht", "schau\u00b7en", ",", "wie", "du", "weinst", ",", "du", "herz\u00b7ge\u00b7lieb\u00b7ter", "theu\u00b7rer", "Mann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "PWAV", "PPER", "VVFIN", "$,", "PPER", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.13": {"line.1": {"text": "\u201ewie die Nacht einst kam von Rosenduft berauscht und trunken von", "tokens": ["\u201e", "wie", "die", "Nacht", "einst", "kam", "von", "Ro\u00b7sen\u00b7duft", "be\u00b7rauscht", "und", "trun\u00b7ken", "von"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "ART", "NN", "ADV", "VVFIN", "APPR", "NN", "VVPP", "KON", "ADJD", "APPR"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Mondesglanz,", "tokens": ["Mon\u00b7des\u00b7glanz", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und die Nachtigallen schluchzten s\u00fc\u00df, und die Elfen wiegten sich im Tanz.", "tokens": ["Und", "die", "Nach\u00b7ti\u00b7gal\u00b7len", "schluchz\u00b7ten", "s\u00fc\u00df", ",", "und", "die", "El\u00b7fen", "wieg\u00b7ten", "sich", "im", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$,", "KON", "ART", "NN", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+-+-+--+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.14": {"line.1": {"text": "\u201edie Winde wallten die Stra\u00dfe hinab und fernher zitternd die Geige klang,", "tokens": ["\u201e", "die", "Win\u00b7de", "wall\u00b7ten", "die", "Stra\u00b7\u00dfe", "hin\u00b7ab", "und", "fern\u00b7her", "zit\u00b7ternd", "die", "Gei\u00b7ge", "klang", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "KON", "ADJD", "VVPP", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-+-+--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Und die Wasser rauschten tr\u00e4umend hinab den schattend\u00fcst\u2019ren Waldesgang.", "tokens": ["Und", "die", "Was\u00b7ser", "rauschten", "tr\u00e4u\u00b7mend", "hin\u00b7ab", "den", "schat\u00b7ten\u00b7d\u00fcst'\u00b7ren", "Wal\u00b7des\u00b7gang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "ADV", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-+-+-+-+", "measure": "anapaest.tri.plus"}}, "stanza.15": {"line.1": {"text": "\u201eda lag sein Haupt an meiner Brust, und wildes Sehnen in mir schwoll,", "tokens": ["\u201e", "da", "lag", "sein", "Haupt", "an", "mei\u00b7ner", "Brust", ",", "und", "wil\u00b7des", "Seh\u00b7nen", "in", "mir", "schwoll", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,", "KON", "ADJA", "NN", "APPR", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Und er k\u00fc\u00dfte mich \u2026 und er k\u00fc\u00dfte mich \u2026 und mein Herz ward", "tokens": ["Und", "er", "k\u00fc\u00df\u00b7te", "mich", "\u2026", "und", "er", "k\u00fc\u00df\u00b7te", "mich", "\u2026", "und", "mein", "Herz", "ward"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$(", "KON", "PPER", "VVFIN", "PPER", "$(", "KON", "PPOSAT", "NN", "VAFIN"], "meter": "--+-++-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "weit und mein Herz ward voll.", "tokens": ["weit", "und", "mein", "Herz", "ward", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.16": {"line.1": {"text": "\u201eund vor mir sank die Welt dahin \u2026 Es schwanden in Nebel Zeit und Raum", "tokens": ["\u201e", "und", "vor", "mir", "sank", "die", "Welt", "da\u00b7hin", "\u2026", "Es", "schwan\u00b7den", "in", "Ne\u00b7bel", "Zeit", "und", "Raum"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "APPR", "PPER", "VVFIN", "ART", "NN", "PAV", "$(", "PPER", "VVFIN", "APPR", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Und \u00fcber mich kam\u2019s wie s\u00fc\u00dfer Schlaf, wie ein todesschwerer bittrer Traum.", "tokens": ["Und", "\u00fc\u00b7ber", "mich", "kam's", "wie", "s\u00fc\u00b7\u00dfer", "Schlaf", ",", "wie", "ein", "to\u00b7des\u00b7schwe\u00b7rer", "bit\u00b7trer", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "KOKOM", "ADJA", "NN", "$,", "PWAV", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.17": {"line.1": {"text": "\u201edoch als der Morgen in Osten sich hob, \u2014 o wie grau und schwer", "tokens": ["\u201e", "doch", "als", "der", "Mor\u00b7gen", "in", "Os\u00b7ten", "sich", "hob", ",", "o", "wie", "grau", "und", "schwer"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "KOUS", "ART", "NN", "APPR", "NN", "PRF", "VVFIN", "$,", "$(", "FM", "KOKOM", "ADJD", "KON", "ADJD"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "und wie kalt der Tag,", "tokens": ["und", "wie", "kalt", "der", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und er nahm mein Gl\u00fcck und lie\u00df mir nichts zur\u00fcck als Schande und", "tokens": ["Und", "er", "nahm", "mein", "Gl\u00fcck", "und", "lie\u00df", "mir", "nichts", "zu\u00b7r\u00fcck", "als", "Schan\u00b7de", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "PIS", "PTKVZ", "KOKOM", "NN", "KON"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "bitt\u2019re Schmach.", "tokens": ["bitt'\u00b7re", "Schmach", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.18": {"line.1": {"text": "\u201enein, fluch\u2019 ihm nicht! Schwer fiel die Hand des Himmels auf sein", "tokens": ["\u201e", "nein", ",", "fluch'", "ihm", "nicht", "!", "Schwer", "fiel", "die", "Hand", "des", "Him\u00b7mels", "auf", "sein"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PTKANT", "$,", "VVFIN", "PPER", "PTKNEG", "$.", "NN", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "PPOSAT"], "meter": "-+--++-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "sch\u00f6nes Haupt,", "tokens": ["sch\u00f6\u00b7nes", "Haupt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Seines Herzens Glocke hat ausget\u00f6nt, und sein Gebein ist l\u00e4ngst verstaubt.", "tokens": ["Sei\u00b7nes", "Her\u00b7zens", "Glo\u00b7cke", "hat", "aus\u00b7ge\u00b7t\u00f6nt", ",", "und", "sein", "Ge\u00b7bein", "ist", "l\u00e4ngst", "ver\u00b7staubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAFIN", "VVPP", "$,", "KON", "PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.19": {"line.1": {"text": "\u201eder Wahnsinn fiel in sein Gehirn mit hei\u00dfer und versengender Gluth,", "tokens": ["\u201e", "der", "Wahn\u00b7sinn", "fiel", "in", "sein", "Ge\u00b7hirn", "mit", "hei\u00b7\u00dfer", "und", "ver\u00b7sen\u00b7gen\u00b7der", "Gluth", ","], "token_info": ["punct", "word", "word", "word", "word"