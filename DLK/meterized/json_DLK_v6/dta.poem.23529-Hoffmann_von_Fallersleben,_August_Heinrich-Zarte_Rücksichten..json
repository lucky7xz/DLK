{"dta.poem.23529": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Zarte R\u00fccksichten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir waren es! o Heil, da\u00df ", "tokens": ["Wir", "wa\u00b7ren", "es", "!", "o", "Heil", ",", "da\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "PPER", "$.", "FM", "NN", "$,", "KOUS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die einst erfanden vor vierhundert Jahren", "tokens": ["Die", "einst", "er\u00b7fan\u00b7den", "vor", "vier\u00b7hun\u00b7dert", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dich, Pflegetochter hoher Gnad' und Gunst,", "tokens": ["Dich", ",", "Pfle\u00b7ge\u00b7toch\u00b7ter", "ho\u00b7her", "Gnad'", "und", "Gunst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dich, weltber\u00fchmte edle Druckerkunst!", "tokens": ["Dich", ",", "welt\u00b7be\u00b7r\u00fchm\u00b7te", "ed\u00b7le", "Dru\u00b7cker\u00b7kunst", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PWAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Herbei aus allen deutschen Gau'n in Schaaren!", "tokens": ["Her\u00b7bei", "aus", "al\u00b7len", "deut\u00b7schen", "Gau'n", "in", "Schaa\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kommt, lasst uns unsern Dank Ihm offenbaren,", "tokens": ["Kommt", ",", "lasst", "uns", "un\u00b7sern", "Dank", "Ihm", "of\u00b7fen\u00b7ba\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihm, der das Wort gefreit aus seinem Bann,", "tokens": ["Ihm", ",", "der", "das", "Wort", "ge\u00b7freit", "aus", "sei\u00b7nem", "Bann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df es die ganze Welt erfreuen kann.", "tokens": ["Da\u00df", "es", "die", "gan\u00b7ze", "Welt", "er\u00b7freu\u00b7en", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Von allen Th\u00fcrmen soll es hell erschallen,", "tokens": ["Von", "al\u00b7len", "Th\u00fcr\u00b7men", "soll", "es", "hell", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus allen Feuerschl\u00fcnden wiederhallen!", "tokens": ["Aus", "al\u00b7len", "Feu\u00b7er\u00b7schl\u00fcn\u00b7den", "wie\u00b7der\u00b7hal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dank, Guttenberg, du hast das Wort gefreit,", "tokens": ["Dank", ",", "Gut\u00b7ten\u00b7berg", ",", "du", "hast", "das", "Wort", "ge\u00b7freit", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Frei sei's und bleib's bei uns auch allezeit!", "tokens": ["Frei", "sei's", "und", "bleib's", "bei", "uns", "auch", "al\u00b7le\u00b7zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "NE", "APPR", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch nein! es ist manch allerh\u00f6chster Wille,", "tokens": ["Doch", "nein", "!", "es", "ist", "manch", "al\u00b7ler\u00b7h\u00f6chs\u00b7ter", "Wil\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$.", "PPER", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df wir uns jetzt nur freu'n ganz stille, stille:", "tokens": ["Da\u00df", "wir", "uns", "jetzt", "nur", "freu'n", "ganz", "stil\u00b7le", ",", "stil\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "ADV", "ADV", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Jubelfest von Deutschland nur allein", "tokens": ["Ein", "Ju\u00b7bel\u00b7fest", "von", "Deutschland", "nur", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE", "ADV", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "S\u00e4h' aus, als sollt' es Schadenfreude sein.", "tokens": ["S\u00e4h'", "aus", ",", "als", "sollt'", "es", "Scha\u00b7den\u00b7freu\u00b7de", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "KOUS", "VMFIN", "PPER", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Was w\u00fcrde Holland wohl, was China sagen,", "tokens": ["Was", "w\u00fcr\u00b7de", "Hol\u00b7land", "wohl", ",", "was", "Chi\u00b7na", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NE", "ADV", "$,", "PRELS", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn wir so jubelten in diesen Tagen?", "tokens": ["Wenn", "wir", "so", "ju\u00b7bel\u00b7ten", "in", "die\u00b7sen", "Ta\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Es ist kein sch\u00f6nes, ist kein w\u00fcrdig Fest,", "tokens": ["Es", "ist", "kein", "sch\u00f6\u00b7nes", ",", "ist", "kein", "w\u00fcr\u00b7dig", "Fest", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "$,", "VAFIN", "PIAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wozu sich nicht der Nachbar laden l\u00e4\u00dft.", "tokens": ["Wo\u00b7zu", "sich", "nicht", "der", "Nach\u00b7bar", "la\u00b7den", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PTKNEG", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}