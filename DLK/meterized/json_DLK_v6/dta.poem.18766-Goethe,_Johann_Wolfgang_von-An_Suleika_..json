{"dta.poem.18766": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "An Suleika .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Dir mit Wohlgeruch zu kosen,", "tokens": ["Dir", "mit", "Wohl\u00b7ge\u00b7ruch", "zu", "ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine Freuden zu erh\u00f6hn,", "tokens": ["Dei\u00b7ne", "Freu\u00b7den", "zu", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Knospend m\u00fcssen tausend Rosen", "tokens": ["Knos\u00b7pend", "m\u00fcs\u00b7sen", "tau\u00b7send", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VMFIN", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Erst in Gluten untergehn.", "tokens": ["Erst", "in", "Glu\u00b7ten", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Um ein Fl\u00e4schchen zu besitzen", "tokens": ["Um", "ein", "Fl\u00e4\u00b7schchen", "zu", "be\u00b7sit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das den Ruch auf ewig h\u00e4lt,", "tokens": ["Das", "den", "Ruch", "auf", "e\u00b7wig", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "APPR", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schlank wie deine Fingerspitzen,", "tokens": ["Schlank", "wie", "dei\u00b7ne", "Fin\u00b7ger\u00b7spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da bedarf es einer Welt.", "tokens": ["Da", "be\u00b7darf", "es", "ei\u00b7ner", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Einer Welt von Lebenstrieben,", "tokens": ["Ei\u00b7ner", "Welt", "von", "Le\u00b7ben\u00b7strie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die, in ihrer F\u00fclle Drang,", "tokens": ["Die", ",", "in", "ih\u00b7rer", "F\u00fcl\u00b7le", "Drang", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ahndeten schon Bulbuls Lieben,", "tokens": ["Ahn\u00b7de\u00b7ten", "schon", "Bul\u00b7buls", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NE", "ADJA", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Seeleregenden Gesang.", "tokens": ["See\u00b7le\u00b7re\u00b7gen\u00b7den", "Ge\u00b7sang", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sollte jene Quaal uns qu\u00e4len?", "tokens": ["Soll\u00b7te", "je\u00b7ne", "Qua\u00b7al", "uns", "qu\u00e4\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Da sie unsre Lust vermehrt.", "tokens": ["Da", "sie", "uns\u00b7re", "Lust", "ver\u00b7mehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat nicht Myriaden Seelen", "tokens": ["Hat", "nicht", "My\u00b7ria\u00b7den", "See\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "NN", "NN"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Timurs Herrschaft aufgezehrt!", "tokens": ["Ti\u00b7murs", "Herr\u00b7schaft", "auf\u00b7ge\u00b7zehrt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}