{"textgrid.poem.37540": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Einleitung", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Weise, welcher sitzt und denkt", "tokens": ["Der", "Wei\u00b7se", ",", "wel\u00b7cher", "sitzt", "und", "denkt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und tief sich in sich selbst versenkt,", "tokens": ["Und", "tief", "sich", "in", "sich", "selbst", "ver\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "APPR", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um in der Seele D\u00e4mmerschein", "tokens": ["Um", "in", "der", "See\u00b7le", "D\u00e4m\u00b7mer\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich an der Wahrheit zu erfreun,", "tokens": ["Sich", "an", "der", "Wahr\u00b7heit", "zu", "er\u00b7freun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der leert bedenklich seine Flasche,", "tokens": ["Der", "leert", "be\u00b7denk\u00b7lich", "sei\u00b7ne", "Fla\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hebt seine Dose aus der Tasche,", "tokens": ["Hebt", "sei\u00b7ne", "Do\u00b7se", "aus", "der", "Ta\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nimmt eine Prise, macht Habschieh!", "tokens": ["Nimmt", "ei\u00b7ne", "Pri\u00b7se", ",", "macht", "Hab\u00b7schieh", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und spricht:", "tokens": ["Und", "spricht", ":"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "\u00bbmein Sohn, die Sach ist die!", "tokens": ["\u00bb", "mein", "Sohn", ",", "die", "Sach", "ist", "die", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "ART", "NN", "VAFIN", "ART", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Eh man auf diese Welt gekommen", "tokens": ["Eh", "man", "auf", "die\u00b7se", "Welt", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "PDAT", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und noch so still vorlieb genommen,", "tokens": ["Und", "noch", "so", "still", "vor\u00b7lieb", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da hat man noch bei nichts was bei;", "tokens": ["Da", "hat", "man", "noch", "bei", "nichts", "was", "bei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "APPR", "PIS", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Man schwebt herum, ist schuldenfrei,", "tokens": ["Man", "schwebt", "he\u00b7rum", ",", "ist", "schul\u00b7den\u00b7frei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKVZ", "$,", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hat keine Uhr und keine Eile", "tokens": ["Hat", "kei\u00b7ne", "Uhr", "und", "kei\u00b7ne", "Ei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und \u00e4u\u00dferst selten Langeweile.", "tokens": ["Und", "\u00e4u\u00b7\u00dferst", "sel\u00b7ten", "Lan\u00b7ge\u00b7wei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Allein man nimmt sich nicht in acht,", "tokens": ["Al\u00b7lein", "man", "nimmt", "sich", "nicht", "in", "acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PRF", "PTKNEG", "APPR", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und schlupp! ist man zur Welt gebracht.", "tokens": ["Und", "schlupp", "!", "ist", "man", "zur", "Welt", "ge\u00b7bracht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "VAFIN", "PIS", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zuerst hast Du es gut, mein Sohn,", "tokens": ["Zu\u00b7erst", "hast", "Du", "es", "gut", ",", "mein", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADJD", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch pa\u00df mal auf, man kommt Dir schon!", "tokens": ["Doch", "pa\u00df", "mal", "auf", ",", "man", "kommt", "Dir", "schon", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ADV", "PTKVZ", "$,", "PIS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bereits Dein braves Elternpaar", "tokens": ["Be\u00b7reits", "Dein", "bra\u00b7ves", "El\u00b7tern\u00b7paar"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erscheint Dir h\u00e4ufig sonderbar.", "tokens": ["Er\u00b7scheint", "Dir", "h\u00e4u\u00b7fig", "son\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es saust der Stab, dann geht es schwapp!", "tokens": ["Es", "saust", "der", "Stab", ",", "dann", "geht", "es", "schwapp", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sieh da, mein Sohn, Du kriegst was ab.", "tokens": ["Sieh", "da", ",", "mein", "Sohn", ",", "Du", "kriegst", "was", "ab", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und schon erscheint Dir unabwendlich", "tokens": ["Und", "schon", "er\u00b7scheint", "Dir", "un\u00b7ab\u00b7wend\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Schmerzensruf: Das ist ja sch\u00e4ndlich!", "tokens": ["Der", "Schmer\u00b7zens\u00b7ruf", ":", "Das", "ist", "ja", "sch\u00e4nd\u00b7lich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PDS", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du w\u00e4chst heran, Du suchst das Weite,", "tokens": ["Du", "w\u00e4chst", "he\u00b7ran", ",", "Du", "suchst", "das", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jedoch die Welt ist voller Leute;", "tokens": ["Je\u00b7doch", "die", "Welt", "ist", "vol\u00b7ler", "Leu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vorherrschend Juden, Weiber, Christen,", "tokens": ["Vor\u00b7herr\u00b7schend", "Ju\u00b7den", ",", "Wei\u00b7ber", ",", "Chris\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Dich ganz schrecklich \u00fcberlisten,", "tokens": ["Die", "Dich", "ganz", "schreck\u00b7lich", "\u00fc\u00b7berl\u00b7is\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und die, anstatt Dir was zu schenken,", "tokens": ["Und", "die", ",", "an\u00b7statt", "Dir", "was", "zu", "schen\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "KOUI", "PPER", "PIS", "PTKZU", "VVINF", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wie Du wohl m\u00f6chtest, nicht dran denken.", "tokens": ["Wie", "Du", "wohl", "m\u00f6ch\u00b7test", ",", "nicht", "dran", "den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VMFIN", "$,", "PTKNEG", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und wieder scheint Dir unabweislich", "tokens": ["Und", "wie\u00b7der", "scheint", "Dir", "un\u00b7ab\u00b7weis\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Schmerzensruf: Das ist ja scheu\u00dflich!", "tokens": ["Der", "Schmer\u00b7zens\u00b7ruf", ":", "Das", "ist", "ja", "scheu\u00df\u00b7lich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PDS", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch siehe da, im trauten Kreis", "tokens": ["Doch", "sie\u00b7he", "da", ",", "im", "trau\u00b7ten", "Kreis"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "ADV", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sitzt J\u00fcngling, Mann und Jubelgreis,", "tokens": ["Sitzt", "J\u00fcng\u00b7ling", ",", "Mann", "und", "Ju\u00b7bel\u00b7greis", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und jeder hebt an seinen Mund", "tokens": ["Und", "je\u00b7der", "hebt", "an", "sei\u00b7nen", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Hohlgem\u00e4\u00df, was meistens rund,", "tokens": ["Ein", "Hohl\u00b7ge\u00b7m\u00e4\u00df", ",", "was", "meis\u00b7tens", "rund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Um draus in ziemlich kurzer Zeit", "tokens": ["Um", "draus", "in", "ziem\u00b7lich", "kur\u00b7zer", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PAV", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die drin enthaltne Fl\u00fcssigkeit", "tokens": ["Die", "drin", "ent\u00b7halt\u00b7ne", "Fl\u00fcs\u00b7sig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit Lust und freudigem Bem\u00fchn", "tokens": ["Mit", "Lust", "und", "freu\u00b7di\u00b7gem", "Be\u00b7m\u00fchn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zu saugen und herauszuziehn.", "tokens": ["Zu", "sau\u00b7gen", "und", "her\u00b7aus\u00b7zu\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Weil jeder dies mit Eifer tut,", "tokens": ["Weil", "je\u00b7der", "dies", "mit", "Ei\u00b7fer", "tut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "PDS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So sieht man wohl, es tut ihm gut.", "tokens": ["So", "sieht", "man", "wohl", ",", "es", "tut", "ihm", "gut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Man setzt sich auch zu diesen Herrn,", "tokens": ["Man", "setzt", "sich", "auch", "zu", "die\u00b7sen", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Man tut es h\u00e4ufig, tut es gern,", "tokens": ["Man", "tut", "es", "h\u00e4u\u00b7fig", ",", "tut", "es", "gern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und m\u00f6glichst lange tut man's auch;", "tokens": ["Und", "m\u00f6g\u00b7lichst", "lan\u00b7ge", "tut", "man's", "auch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Die Nase schwillt, es w\u00e4chst der Bauch,", "tokens": ["Die", "Na\u00b7se", "schwillt", ",", "es", "w\u00e4chst", "der", "Bauch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und bald, mein Sohn, wirst Du mit Graun", "tokens": ["Und", "bald", ",", "mein", "Sohn", ",", "wirst", "Du", "mit", "Graun"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "PPOSAT", "NN", "$,", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Im Spiegelglas Dein Bildnis schaun,", "tokens": ["Im", "Spie\u00b7gel\u00b7glas", "Dein", "Bild\u00b7nis", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und wieder scheint Dir unerl\u00e4\u00dflich", "tokens": ["Und", "wie\u00b7der", "scheint", "Dir", "un\u00b7er\u00b7l\u00e4\u00df\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Der Schmerzensruf: Das ist ja gr\u00e4\u00dflich!!", "tokens": ["Der", "Schmer\u00b7zens\u00b7ruf", ":", "Das", "ist", "ja", "gr\u00e4\u00df\u00b7lich", "!!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PDS", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Mein lieber Sohn, Du tust mir leid.", "tokens": ["Mein", "lie\u00b7ber", "Sohn", ",", "Du", "tust", "mir", "leid", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir mangelt die Enthaltsamkeit.", "tokens": ["Dir", "man\u00b7gelt", "die", "Ent\u00b7halt\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Enthaltsamkeit ist das Vergn\u00fcgen", "tokens": ["Ent\u00b7halt\u00b7sam\u00b7keit", "ist", "das", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "An Sachen, welche wir nicht kriegen.", "tokens": ["An", "Sa\u00b7chen", ",", "wel\u00b7che", "wir", "nicht", "krie\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Drum lebe m\u00e4\u00dfig, denke klug.", "tokens": ["Drum", "le\u00b7be", "m\u00e4\u00b7\u00dfig", ",", "den\u00b7ke", "klug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADJD", "$,", "VVFIN", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Wer nichts gebraucht, der hat genug!\u00ab", "tokens": ["Wer", "nichts", "ge\u00b7braucht", ",", "der", "hat", "ge\u00b7nug", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PIS", "VVPP", "$,", "PRELS", "VAFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So spricht der Weise, grau von Haar,", "tokens": ["So", "spricht", "der", "Wei\u00b7se", ",", "grau", "von", "Haar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ernst, w\u00fcrdig, sachgem\u00e4\u00df und klar,", "tokens": ["Ernst", ",", "w\u00fcr\u00b7dig", ",", "sach\u00b7ge\u00b7m\u00e4\u00df", "und", "klar", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Wie sich's geb\u00fchrt in solchen Dingen;", "tokens": ["Wie", "sich's", "ge\u00b7b\u00fchrt", "in", "sol\u00b7chen", "Din\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "L\u00e4\u00dft sich ein Dutzend Austern bringen,", "tokens": ["L\u00e4\u00dft", "sich", "ein", "Dut\u00b7zend", "Aus\u00b7tern", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "I\u00dft sie, entleert die zweite Flasche,", "tokens": ["I\u00dft", "sie", ",", "ent\u00b7leert", "die", "zwei\u00b7te", "Fla\u00b7sche", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Hebt seine Dose aus der Tasche,", "tokens": ["Hebt", "sei\u00b7ne", "Do\u00b7se", "aus", "der", "Ta\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Nimmt eine Prise, macht habsch\u00fch!", "tokens": ["Nimmt", "ei\u00b7ne", "Pri\u00b7se", ",", "macht", "hab\u00b7sch\u00fch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Schm\u00fcckt sich mit Hut und Parapl\u00fc,", "tokens": ["Schm\u00fcckt", "sich", "mit", "Hut", "und", "Pa\u00b7ra\u00b7pl\u00fc", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Bewegt sich mit Bedacht nach Haus", "tokens": ["Be\u00b7wegt", "sich", "mit", "Be\u00b7dacht", "nach", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und ruht von seinem Denken aus.", "tokens": ["Und", "ruht", "von", "sei\u00b7nem", "Den\u00b7ken", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}