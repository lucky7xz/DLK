{"dta.poem.18940": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "19.  \n  An die alte Ro\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Ro\u00df/ deine sch\u00f6nheit ist dahin/", "tokens": ["Ro\u00df", "/", "dei\u00b7ne", "sch\u00f6n\u00b7heit", "ist", "da\u00b7hin", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPOSAT", "NN", "VAFIN", "PAV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein aug gl\u00e4ntzet wie alte scherben;", "tokens": ["Dein", "aug", "gl\u00e4nt\u00b7zet", "wie", "al\u00b7te", "scher\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KOKOM", "ADJA", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Kein Knecht will deine haut mehr gerben/", "tokens": ["Kein", "Knecht", "will", "dei\u00b7ne", "haut", "mehr", "ger\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPOSAT", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er wissz dan vor/ was sein gewin:", "tokens": ["Er", "wissz", "dan", "vor", "/", "was", "sein", "ge\u00b7win", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "$(", "PWS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd solt sich einer ja verfahren/", "tokens": ["Vnd", "solt", "sich", "ei\u00b7ner", "ja", "ver\u00b7fah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "ART", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd st\u00f6llen/ als ob Er dir hold/", "tokens": ["Vnd", "st\u00f6l\u00b7len", "/", "als", "ob", "Er", "dir", "hold", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOKOM", "KOUS", "PPER", "PPER", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Glaub du/ Er lieb nicht deiner haaren/", "tokens": ["Glaub", "du", "/", "Er", "lieb", "nicht", "dei\u00b7ner", "haa\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$(", "PPER", "ADJD", "PTKNEG", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sondern nur deines beuttels gold.", "tokens": ["Son\u00b7dern", "nur", "dei\u00b7nes", "beut\u00b7tels", "gold", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}