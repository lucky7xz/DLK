{"dta.poem.21345": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der grobe Bettler, Vater Jahn,", "tokens": ["Der", "gro\u00b7be", "Bett\u00b7ler", ",", "Va\u00b7ter", "Jahn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der hie\u00dfe jetzt Grobianus.", "tokens": ["Der", "hie\u00b7\u00dfe", "jetzt", "Gro\u00b7bi\u00b7a\u00b7nus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "NE", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Me hercule! Ma\u00dfmann spr&#228;che Latein,", "tokens": ["Me", "her\u00b7cu\u00b7le", "!", "Ma\u00df\u00b7mann", "spr", "&#228;", "che", "La\u00b7tein", ","], "token_info": ["word", "word", "punct", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "NN", "VVFIN", "$(", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der Marcus Tullius Ma\u00dfmanus!", "tokens": ["Der", "Mar\u00b7cus", "Tul\u00b7li\u00b7us", "Ma\u00df\u00b7ma\u00b7nus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "NE", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Wahrheitsfreunde w\u00fcrden jetzt", "tokens": ["Die", "Wahr\u00b7heits\u00b7freun\u00b7de", "w\u00fcr\u00b7den", "jetzt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit L\u00f6wen, Hy\u00e4nen, Schakalen,", "tokens": ["Mit", "L\u00f6\u00b7wen", ",", "Hy\u00b7\u00e4\u00b7nen", ",", "Scha\u00b7ka\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sich raufen in der Arena, anstatt", "tokens": ["Sich", "rau\u00b7fen", "in", "der", "A\u00b7re\u00b7na", ",", "an\u00b7statt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PRF", "VVFIN", "APPR", "ART", "NE", "$,", "KOUI"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit Hunden in kleinen Journalen.", "tokens": ["Mit", "Hun\u00b7den", "in", "klei\u00b7nen", "Jour\u00b7na\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Wir h\u00e4tten Einen Nero jetzt", "tokens": ["Wir", "h\u00e4t\u00b7ten", "Ei\u00b7nen", "Ne\u00b7ro", "jetzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NE", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Statt Landesv\u00e4ter drey Dutzend.", "tokens": ["Statt", "Lan\u00b7des\u00b7v\u00e4\u00b7ter", "drey", "Dut\u00b7zend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "CARD", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wir schnitten uns die Adern auf,", "tokens": ["Wir", "schnit\u00b7ten", "uns", "die", "A\u00b7dern", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Schergen der Knechtschaft trutzend.", "tokens": ["Den", "Scher\u00b7gen", "der", "Knecht\u00b7schaft", "trut\u00b7zend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Der Schelling w\u00e4r\u2019 ganz ein Seneka,", "tokens": ["Der", "Schel\u00b7ling", "w\u00e4r'", "ganz", "ein", "Se\u00b7ne\u00b7ka", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und k\u00e4me in solchem Conflikt um.", "tokens": ["Und", "k\u00e4\u00b7me", "in", "sol\u00b7chem", "Con\u00b7flikt", "um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Zu uns\u2019rem Cornelius sagten wir:", "tokens": ["Zu", "un\u00b7s'\u00b7rem", "Cor\u00b7ne\u00b7li\u00b7us", "sag\u00b7ten", "wir", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NE", "VVFIN", "PPER", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Kakatum non est piktum.", "tokens": ["Ka\u00b7ka\u00b7tum", "non", "est", "pik\u00b7tum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}