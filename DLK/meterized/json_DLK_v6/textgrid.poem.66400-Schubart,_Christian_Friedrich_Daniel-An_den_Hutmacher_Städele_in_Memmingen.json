{"textgrid.poem.66400": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "An den Hutmacher St\u00e4dele in Memmingen", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hans Marx von hochgebornem Blut", "tokens": ["Hans", "Marx", "von", "hoch\u00b7ge\u00b7bor\u00b7nem", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bestellt bei dir 'n neuen Hut,", "tokens": ["Be\u00b7stellt", "bei", "dir", "'n", "neu\u00b7en", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Recht fein gest\u00fctzt, klein, fl\u00fcchtig, s\u00fc\u00df,", "tokens": ["Recht", "fein", "ge\u00b7st\u00fctzt", ",", "klein", ",", "fl\u00fcch\u00b7tig", ",", "s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach Geckenmode aus Paris;", "tokens": ["Nach", "Ge\u00b7cken\u00b7mo\u00b7de", "aus", "Pa\u00b7ris", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O St\u00e4dele, sei doch so gut,", "tokens": ["O", "St\u00e4\u00b7de\u00b7le", ",", "sei", "doch", "so", "gut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Mach ihm den Kopf gleich mit dem Hut.", "tokens": ["Mach", "ihm", "den", "Kopf", "gleich", "mit", "dem", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}