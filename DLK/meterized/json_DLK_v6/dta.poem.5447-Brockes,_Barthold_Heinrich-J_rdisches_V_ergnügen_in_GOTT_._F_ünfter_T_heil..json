{"dta.poem.5447": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auch durch so sch\u00f6n gef\u00e4rbtes Gl\u00e4ntzen sich unser Geist in", "tokens": ["Auch", "durch", "so", "sch\u00f6n", "ge\u00b7f\u00e4rb\u00b7tes", "Gl\u00e4nt\u00b7zen", "sich", "un\u00b7ser", "Geist", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADV", "ADJD", "ADJA", "NN", "PRF", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Lust entz\u00fcndet;", "tokens": ["Lust", "ent\u00b7z\u00fcn\u00b7det", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Da sich in jedem Tr\u00f6pfgen Thau ein Brenn-und Z\u00fcndungs-", "tokens": ["Da", "sich", "in", "je\u00b7dem", "Tr\u00f6pf\u00b7gen", "Thau", "ein", "Brenn\u00b7\u00b7und", "Z\u00fcn\u00b7dungs"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "PIAT", "NN", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Spiegel findet,", "tokens": ["Spie\u00b7gel", "fin\u00b7det", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Ja da wir, in den klaren Cirkeln von unsrer Sonnen,", "tokens": ["Ja", "da", "wir", ",", "in", "den", "kla\u00b7ren", "Cir\u00b7keln", "von", "uns\u00b7rer", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOUS", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "wundersch\u00f6n", "tokens": ["wun\u00b7der\u00b7sch\u00f6n"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Verkleinert zwar, doch auch vereinet, viel tauseud Son-", "tokens": ["Ver\u00b7klei\u00b7nert", "zwar", ",", "doch", "auch", "ver\u00b7ei\u00b7net", ",", "viel", "tau\u00b7seud", "Son"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "ADV", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "nen-Bilder sehn.", "tokens": ["nen\u00b7Bil\u00b7der", "sehn", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}