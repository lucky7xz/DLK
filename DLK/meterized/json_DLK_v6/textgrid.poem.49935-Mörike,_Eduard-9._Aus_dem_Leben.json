{"textgrid.poem.49935": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "9. Aus dem Leben", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00e4dchen am Waschtrog, du blondhaariges, zeige die Arme", "tokens": ["M\u00e4d\u00b7chen", "am", "Waschtrog", ",", "du", "blond\u00b7haa\u00b7ri\u00b7ges", ",", "zei\u00b7ge", "die", "Ar\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "$,", "PPER", "ADJA", "$,", "VVFIN", "ART", "NN"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Nicht und die Schultern so blo\u00df unter dem Fenster des Abts!", "tokens": ["Nicht", "und", "die", "Schul\u00b7tern", "so", "blo\u00df", "un\u00b7ter", "dem", "Fens\u00b7ter", "des", "Ab\u00b7ts", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KON", "ART", "NN", "ADV", "ADV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--++--+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der zwar sieht dich zum Gl\u00fcck nicht mehr, doch dem artigen Forstmann", "tokens": ["Der", "zwar", "sieht", "dich", "zum", "Gl\u00fcck", "nicht", "mehr", ",", "doch", "dem", "ar\u00b7ti\u00b7gen", "Forst\u00b7mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "PPER", "APPRART", "NN", "PTKNEG", "ADV", "$,", "ADV", "ART", "ADJA", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Dort bei den Akten bereits st\u00f6rst du sein stilles Konzept.", "tokens": ["Dort", "bei", "den", "Ak\u00b7ten", "be\u00b7reits", "st\u00f6rst", "du", "sein", "stil\u00b7les", "Kon\u00b7zept", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}}}}