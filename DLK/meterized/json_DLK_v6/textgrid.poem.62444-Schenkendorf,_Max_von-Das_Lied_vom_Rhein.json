{"textgrid.poem.62444": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Das Lied vom Rhein", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es klingt ein heller Klang,", "tokens": ["Es", "klingt", "ein", "hel\u00b7ler", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein sch\u00f6nes deutsches Wort", "tokens": ["Ein", "sch\u00f6\u00b7nes", "deut\u00b7sches", "Wort"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In jedem Hochgesang", "tokens": ["In", "je\u00b7dem", "Hoch\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der deutschen M\u00e4nner fort:", "tokens": ["Der", "deut\u00b7schen", "M\u00e4n\u00b7ner", "fort", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein alter K\u00f6nig hochgeboren,", "tokens": ["Ein", "al\u00b7ter", "K\u00f6\u00b7nig", "hoch\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dem jedes deutsche Herz geschworen \u2013", "tokens": ["Dem", "je\u00b7des", "deut\u00b7sche", "Herz", "ge\u00b7schwo\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie oft sein Name wiederkehrt,", "tokens": ["Wie", "oft", "sein", "Na\u00b7me", "wie\u00b7der\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Man hat ihn nie genug geh\u00f6rt.", "tokens": ["Man", "hat", "ihn", "nie", "ge\u00b7nug", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das ist der heil'ge ", "tokens": ["Das", "ist", "der", "heil'\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ein Herrscher, reich begabt,", "tokens": ["Ein", "Herr\u00b7scher", ",", "reich", "be\u00b7gabt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "De\u00df Name schon, wie Wein,", "tokens": ["De\u00df", "Na\u00b7me", "schon", ",", "wie", "Wein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die treue Seele labt.", "tokens": ["Die", "treu\u00b7e", "See\u00b7le", "labt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es regen sich in allen Herzen", "tokens": ["Es", "re\u00b7gen", "sich", "in", "al\u00b7len", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Viel vaterl\u00e4nd'sche Lust und Schmerzen,", "tokens": ["Viel", "va\u00b7ter\u00b7l\u00e4n\u00b7d'\u00b7sche", "Lust", "und", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Wenn man das deutsche Lied beginnt", "tokens": ["Wenn", "man", "das", "deut\u00b7sche", "Lied", "be\u00b7ginnt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vom Rhein, dem hohen Felsenkind.", "tokens": ["Vom", "Rhein", ",", "dem", "ho\u00b7hen", "Fel\u00b7sen\u00b7kind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie hatten ihm geraubt", "tokens": ["Sie", "hat\u00b7ten", "ihm", "ge\u00b7raubt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der alten W\u00fcrden Glanz,", "tokens": ["Der", "al\u00b7ten", "W\u00fcr\u00b7den", "Glanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von seinem K\u00f6nigshaupt", "tokens": ["Von", "sei\u00b7nem", "K\u00f6\u00b7nigs\u00b7haupt"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Den gr\u00fcnen Rebenkranz.", "tokens": ["Den", "gr\u00fc\u00b7nen", "Re\u00b7ben\u00b7kranz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In Fesseln lag der Held geschlagen:", "tokens": ["In", "Fes\u00b7seln", "lag", "der", "Held", "ge\u00b7schla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sein Z\u00fcrnen und sein stolzes Klagen,", "tokens": ["Sein", "Z\u00fcr\u00b7nen", "und", "sein", "stol\u00b7zes", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wir haben's manche Nacht belauscht,", "tokens": ["Wir", "ha\u00b7ben's", "man\u00b7che", "Nacht", "be\u00b7lauscht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von Geisterschauern hehr umrauscht.", "tokens": ["Von", "Geis\u00b7ter\u00b7schau\u00b7ern", "hehr", "um\u00b7rauscht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Was sang der alte Held? \u2013", "tokens": ["Was", "sang", "der", "al\u00b7te", "Held", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein furchtbar dr\u00e4uend Lied:", "tokens": ["Ein", "furcht\u00b7bar", "dr\u00e4u\u00b7end", "Lied", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbo weh' dir, schn\u00f6de Welt!", "tokens": ["\u00bb", "o", "weh'", "dir", ",", "schn\u00f6\u00b7de", "Welt", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wo keine Freiheit bl\u00fcht,", "tokens": ["Wo", "kei\u00b7ne", "Frei\u00b7heit", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von Treuen los, und bar von Ehren!", "tokens": ["Von", "Treu\u00b7en", "los", ",", "und", "bar", "von", "Eh\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,", "KON", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und willst du nimmer wiederkehren,", "tokens": ["Und", "willst", "du", "nim\u00b7mer", "wie\u00b7der\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mein, ach! gestorbenes Geschlecht", "tokens": ["Mein", ",", "ach", "!", "ge\u00b7stor\u00b7be\u00b7nes", "Ge\u00b7schlecht"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["PPOSAT", "$,", "ITJ", "$.", "ADJA", "NN"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.8": {"text": "Und mein gebroch'nes deutsches Recht?\u00ab", "tokens": ["Und", "mein", "ge\u00b7bro\u00b7ch'\u00b7nes", "deut\u00b7sches", "Recht", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbo meine hohe Zeit!", "tokens": ["\u00bb", "o", "mei\u00b7ne", "ho\u00b7he", "Zeit", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mein goldner Lebenstag!", "tokens": ["Mein", "gold\u00b7ner", "Le\u00b7bens\u00b7tag", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als noch in Herrlichkeit", "tokens": ["Als", "noch", "in", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mein Deutschland vor mir lag.", "tokens": ["Mein", "Deutschland", "vor", "mir", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und auf und ab am Ufer wallten", "tokens": ["Und", "auf", "und", "ab", "am", "U\u00b7fer", "wall\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKVZ", "KON", "APPR", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die stolzen adligen Gestalten,", "tokens": ["Die", "stol\u00b7zen", "ad\u00b7li\u00b7gen", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Helden, weit und breit geehrt", "tokens": ["Die", "Hel\u00b7den", ",", "weit", "und", "breit", "ge\u00b7ehrt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Durch ihre Tugend und ihr Schwert!\u00ab", "tokens": ["Durch", "ih\u00b7re", "Tu\u00b7gend", "und", "ihr", "Schwert", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbes war ein frommes Blut", "tokens": ["\u00bb", "es", "war", "ein", "from\u00b7mes", "Blut"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In ferner Riesenzeit", "tokens": ["In", "fer\u00b7ner", "Rie\u00b7sen\u00b7zeit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Voll k\u00fchnem Leuenmuth,", "tokens": ["Voll", "k\u00fch\u00b7nem", "Leu\u00b7en\u00b7muth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und mild als eine Maid.", "tokens": ["Und", "mild", "als", "ei\u00b7ne", "Maid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Man singt es noch in sp\u00e4ten Tagen,", "tokens": ["Man", "singt", "es", "noch", "in", "sp\u00e4\u00b7ten", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie den erschlug der arge Hagen,", "tokens": ["Wie", "den", "er\u00b7schlug", "der", "ar\u00b7ge", "Ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Was ihn zu solcher hat gelenkt,", "tokens": ["Was", "ihn", "zu", "sol\u00b7cher", "hat", "ge\u00b7lenkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In meinem Bette liegt's versenkt.\u00ab", "tokens": ["In", "mei\u00b7nem", "Bet\u00b7te", "liegt's", "ver\u00b7senkt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbdu S\u00fcnder! w\u00fcthe fort!", "tokens": ["\u00bb", "du", "S\u00fcn\u00b7der", "!", "w\u00fc\u00b7the", "fort", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "NN", "$.", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Bald ist dein Becher voll;", "tokens": ["Bald", "ist", "dein", "Be\u00b7cher", "voll", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Niebelungen Hort", "tokens": ["Der", "Nie\u00b7be\u00b7lun\u00b7gen", "Hort"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ersteht wol, wenn er soll.", "tokens": ["Er\u00b7steht", "wol", ",", "wenn", "er", "soll", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es wird in dir die Seele grausen,", "tokens": ["Es", "wird", "in", "dir", "die", "See\u00b7le", "grau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn meine Schrecken dich umbrausen;", "tokens": ["Wenn", "mei\u00b7ne", "Schre\u00b7cken", "dich", "um\u00b7brau\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ich habe wohl und treu bewahrt", "tokens": ["Ich", "ha\u00b7be", "wohl", "und", "treu", "be\u00b7wahrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "KON", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den Schatz der alten Kraft und Art!\u00ab \u2013", "tokens": ["Den", "Schatz", "der", "al\u00b7ten", "Kraft", "und", "Art", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "KON", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Erf\u00fcllt ist jenes Wort:", "tokens": ["Er\u00b7f\u00fcllt", "ist", "je\u00b7nes", "Wort", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der K\u00f6nig ist nun frei,", "tokens": ["Der", "K\u00f6\u00b7nig", "ist", "nun", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Niebelungen Hort", "tokens": ["Der", "Nie\u00b7be\u00b7lun\u00b7gen", "Hort"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ersteht und gl\u00e4nzet neu!", "tokens": ["Er\u00b7steht", "und", "gl\u00e4n\u00b7zet", "neu", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es sind die alten deutschen Ehren,", "tokens": ["Es", "sind", "die", "al\u00b7ten", "deut\u00b7schen", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die wieder ihren Schein bew\u00e4hren:", "tokens": ["Die", "wie\u00b7der", "ih\u00b7ren", "Schein", "be\u00b7w\u00e4h\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der V\u00e4ter Zucht und Muth und Ruhm,", "tokens": ["Der", "V\u00e4\u00b7ter", "Zucht", "und", "Muth", "und", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das heil'ge deutsche Kaiserthum!", "tokens": ["Das", "heil'\u00b7ge", "deut\u00b7sche", "Kai\u00b7ser\u00b7thum", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wir huld'gen unserm Herrn,", "tokens": ["Wir", "huld'\u00b7gen", "un\u00b7serm", "Herrn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wir trinken seinen Wein.", "tokens": ["Wir", "trin\u00b7ken", "sei\u00b7nen", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Freiheit sei der Stern!", "tokens": ["Die", "Frei\u00b7heit", "sei", "der", "Stern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Losung sei der Rhein!", "tokens": ["Die", "Lo\u00b7sung", "sei", "der", "Rhein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wir wollen ihm aufs neue schw\u00f6ren;", "tokens": ["Wir", "wol\u00b7len", "ihm", "aufs", "neu\u00b7e", "schw\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wir m\u00fcssen ihm, er uns geh\u00f6ren.", "tokens": ["Wir", "m\u00fcs\u00b7sen", "ihm", ",", "er", "uns", "ge\u00b7h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$,", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Vom Felsen kommt er frei und hehr,", "tokens": ["Vom", "Fel\u00b7sen", "kommt", "er", "frei", "und", "hehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er flie\u00dfe frei in Gottes Meer!", "tokens": ["Er", "flie\u00b7\u00dfe", "frei", "in", "Got\u00b7tes", "Meer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}