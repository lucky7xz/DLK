{"dta.poem.7185": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die lehrende Ruinen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des grauen Alterthums vorl\u00e4ngst verschwundne Zeiten,", "tokens": ["Des", "grau\u00b7en", "Al\u00b7ter\u00b7thums", "vor\u00b7l\u00e4ngst", "ver\u00b7schwund\u00b7ne", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So die Vergessenheit schon lange wegger\u00fcckt,", "tokens": ["So", "die", "Ver\u00b7ges\u00b7sen\u00b7heit", "schon", "lan\u00b7ge", "weg\u00b7ge\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verj\u00fcnget Deine Kunst, Du stellst an Seltenheiten,", "tokens": ["Ver\u00b7j\u00fcn\u00b7get", "Dei\u00b7ne", "Kunst", ",", "Du", "stellst", "an", "Sel\u00b7ten\u00b7hei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was gleichsam schon zu nichts geworden war,", "tokens": ["Was", "gleich\u00b7sam", "schon", "zu", "nichts", "ge\u00b7wor\u00b7den", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADV", "PTKA", "PIS", "VAPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Aus Moder, Asch und Graus uns auf das Neue dar.", "tokens": ["Aus", "Mo\u00b7der", ",", "Asch", "und", "Graus", "uns", "auf", "das", "Neu\u00b7e", "dar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "PPER", "APPR", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du zeigest uns nicht nur", "tokens": ["Du", "zei\u00b7gest", "uns", "nicht", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Wunder der Natur,", "tokens": ["Die", "Wun\u00b7der", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Die Lage, die Gestalt der L\u00e4nder und der Erden", "tokens": ["Die", "La\u00b7ge", ",", "die", "Ge\u00b7stalt", "der", "L\u00e4n\u00b7der", "und", "der", "Er\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Aufs allerrichtigste; Du zeigest uns zugleich", "tokens": ["Aufs", "al\u00b7ler\u00b7rich\u00b7tigs\u00b7te", ";", "Du", "zei\u00b7gest", "uns", "zu\u00b7gleich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "$.", "PPER", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Wunder, so durch Menschen Witz und St\u00e4rcke", "tokens": ["Die", "Wun\u00b7der", ",", "so", "durch", "Men\u00b7schen", "Witz", "und", "St\u00e4r\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Vor dem gewesen sind, und noch gefunden werden.", "tokens": ["Vor", "dem", "ge\u00b7we\u00b7sen", "sind", ",", "und", "noch", "ge\u00b7fun\u00b7den", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VAPP", "VAFIN", "$,", "KON", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da wir durch Deine Kunst hier das vor Augen haben,", "tokens": ["Da", "wir", "durch", "Dei\u00b7ne", "Kunst", "hier", "das", "vor", "Au\u00b7gen", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ADV", "ART", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Was so viel Secula, was so viel Sand begraben.", "tokens": ["Was", "so", "viel", "Se\u00b7cu\u00b7la", ",", "was", "so", "viel", "Sand", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "NE", "$,", "PRELS", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Denn nichts als eine Last von Steinen", "tokens": ["Denn", "nichts", "als", "ei\u00b7ne", "Last", "von", "Stei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "KOKOM", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Fehlt Deinem Zauberwerck; Erfindung, Ordonantz,", "tokens": ["Fehlt", "Dei\u00b7nem", "Zau\u00b7ber\u00b7werck", ";", "Er\u00b7fin\u00b7dung", ",", "Or\u00b7do\u00b7nantz", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der Masse Richtigkeit,", "tokens": ["Der", "Mas\u00b7se", "Rich\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Worinnen die Vollkommenheit", "tokens": ["Wo\u00b7rin\u00b7nen", "die", "Voll\u00b7kom\u00b7men\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und Herrlichkeit der Kunst bestehen,", "tokens": ["Und", "Herr\u00b7lich\u00b7keit", "der", "Kunst", "be\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Sind hier, in Deinem Ri\u00df, aufs deutlichste zu sehen.", "tokens": ["Sind", "hier", ",", "in", "Dei\u00b7nem", "Ri\u00df", ",", "aufs", "deut\u00b7lichs\u00b7te", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,", "APPRART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}