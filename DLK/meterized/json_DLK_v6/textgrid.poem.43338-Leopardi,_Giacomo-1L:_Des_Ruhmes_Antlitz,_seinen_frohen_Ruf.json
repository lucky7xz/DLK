{"textgrid.poem.43338": {"metadata": {"author": {"name": "Leopardi, Giacomo", "birth": "N.A.", "death": "N.A."}, "title": "1L: Des Ruhmes Antlitz, seinen frohen Ruf", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Ruhmes Antlitz, seinen frohen Ruf", "tokens": ["Des", "Ruh\u00b7mes", "Ant\u00b7litz", ",", "sei\u00b7nen", "fro\u00b7hen", "Ruf"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erkenne, wackrer Knabe,", "tokens": ["Er\u00b7ken\u00b7ne", ",", "wack\u00b7rer", "Kna\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wie viel herrlicher als weibische Mu\u00dfe", "tokens": ["Und", "wie", "viel", "herr\u00b7li\u00b7cher", "als", "wei\u00b7bi\u00b7sche", "Mu\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PIAT", "ADJA", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der Schwei\u00df der Tugend. Labe dich, o labe", "tokens": ["Der", "Schwei\u00df", "der", "Tu\u00b7gend", ".", "La\u00b7be", "dich", ",", "o", "la\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$.", "NN", "PPER", "$,", "FM", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dein Herz am Hochsinn (f\u00fchlst du den Beruf,", "tokens": ["Dein", "Herz", "am", "Hoch\u00b7sinn", "(", "f\u00fchlst", "du", "den", "Be\u00b7ruf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$(", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Den Namen aus der Zeiten tr\u00fcbem Flusse", "tokens": ["Den", "Na\u00b7men", "aus", "der", "Zei\u00b7ten", "tr\u00fc\u00b7bem", "Flus\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Durch edle That zu retten) und erhebe", "tokens": ["Durch", "ed\u00b7le", "That", "zu", "ret\u00b7ten", ")", "und", "er\u00b7he\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$(", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den Geist zu stolzem Wunsch. Dir jauchzte freudig", "tokens": ["Den", "Geist", "zu", "stol\u00b7zem", "Wunsch", ".", "Dir", "jauchz\u00b7te", "freu\u00b7dig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$.", "PPER", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Kampfbahn und Circus, und zu Heldentugend", "tokens": ["Kampf\u00b7bahn", "und", "Cir\u00b7cus", ",", "und", "zu", "Hel\u00b7den\u00b7tu\u00b7gend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NE", "$,", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Spornt dich des Volkes Gunst. Es will das theure", "tokens": ["Spornt", "dich", "des", "Vol\u00b7kes", "Gunst", ".", "Es", "will", "das", "theu\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "$.", "PPER", "VMFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Land deiner V\u00e4ter, stolz auf deine Jugend,", "tokens": ["Land", "dei\u00b7ner", "V\u00e4\u00b7ter", ",", "stolz", "auf", "dei\u00b7ne", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.12": {"text": "In deinem edlen Streben", "tokens": ["In", "dei\u00b7nem", "ed\u00b7len", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Die alten Muster sehn sich neu beleben.", "tokens": ["Die", "al\u00b7ten", "Mus\u00b7ter", "sehn", "sich", "neu", "be\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nicht mit Barbarenblut bei Marathon", "tokens": ["Nicht", "mit", "Bar\u00b7ba\u00b7ren\u00b7blut", "bei", "Ma\u00b7ra\u00b7thon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "NN", "APPR", "NE"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "F\u00e4rbte sich nur den Finger,", "tokens": ["F\u00e4rb\u00b7te", "sich", "nur", "den", "Fin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wer stumpfen Blicks in Elis auf die Glut", "tokens": ["Wer", "stump\u00b7fen", "Blicks", "in", "E\u00b7lis", "auf", "die", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "NN", "APPR", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Rennbahn schaut' und auf die nackten Ringer,", "tokens": ["Der", "Renn\u00b7bahn", "schaut'", "und", "auf", "die", "nack\u00b7ten", "Rin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und wem des Kranzes holder Siegeslohn", "tokens": ["Und", "wem", "des", "Kran\u00b7zes", "hol\u00b7der", "Sie\u00b7ges\u00b7lohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das Herz nicht hob. In des Alpheus Flut", "tokens": ["Das", "Herz", "nicht", "hob", ".", "In", "des", "Al\u00b7pheus", "Flut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKNEG", "VVFIN", "$.", "APPR", "ART", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Wusch sieggekr\u00f6nter Rosse M\u00e4hn und Weichen", "tokens": ["Wusch", "sieg\u00b7ge\u00b7kr\u00f6n\u00b7ter", "Ros\u00b7se", "M\u00e4hn", "und", "Wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Vom Staube Mancher rein, der dann mit Macht", "tokens": ["Vom", "Stau\u00b7be", "Man\u00b7cher", "rein", ",", "der", "dann", "mit", "Macht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "NN", "ADJD", "$,", "PRELS", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Das Griechenbanner und das Griechenschwert", "tokens": ["Das", "Grie\u00b7chen\u00b7ban\u00b7ner", "und", "das", "Grie\u00b7chen\u00b7schwert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "F\u00fchrt' in der Meder Reih'n, die schreckensbleichen,", "tokens": ["F\u00fchrt'", "in", "der", "Me\u00b7der", "Reih'n", ",", "die", "schre\u00b7ckens\u00b7blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und in die Flucht sie schlug, da\u00df durch die Nacht", "tokens": ["Und", "in", "die", "Flucht", "sie", "schlug", ",", "da\u00df", "durch", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "VVFIN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Der Jammerruf erklang", "tokens": ["Der", "Jam\u00b7mer\u00b7ruf", "er\u00b7klang"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "An Euphrats Bucht und Asiens Strand entlang,", "tokens": ["An", "Eu\u00b7ph\u00b7rats", "Bucht", "und", "A\u00b7siens", "Strand", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "KON", "NE", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch ist's nicht fruchtlos, den erloschnen Brand", "tokens": ["Doch", "ist's", "nicht", "frucht\u00b7los", ",", "den", "er\u00b7lo\u00b7schnen", "Brand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "PTKNEG", "ADJD", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der alten Thatenlust", "tokens": ["Der", "al\u00b7ten", "Tha\u00b7ten\u00b7lust"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Neu anzufachen? die darniederlagen,", "tokens": ["Neu", "an\u00b7zu\u00b7fa\u00b7chen", "?", "die", "dar\u00b7nie\u00b7der\u00b7la\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVIZU", "$.", "ART", "PAV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Lebensgeister in der kranken Brust", "tokens": ["Die", "Le\u00b7bens\u00b7geis\u00b7ter", "in", "der", "kran\u00b7ken", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Neu zu beleben? War nicht Spiel und Tand", "tokens": ["Neu", "zu", "be\u00b7le\u00b7ben", "?", "War", "nicht", "Spiel", "und", "Tand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKZU", "VVINF", "$.", "VAFIN", "PTKNEG", "NN", "KON", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Das Thun der Sterblichen, seit Ph\u00f6bus' Wagen", "tokens": ["Das", "Thun", "der", "Sterb\u00b7li\u00b7chen", ",", "seit", "Ph\u00f6\u00b7bus'", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Tr\u00fcbselig hinrollt, und ist minder eitel", "tokens": ["Tr\u00fcb\u00b7se\u00b7lig", "hin\u00b7rollt", ",", "und", "ist", "min\u00b7der", "ei\u00b7tel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "$,", "KON", "VAFIN", "ADV", "ADJD"], "meter": "+---+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Wahrheit, als L\u00fcge? Gab uns doch Natur", "tokens": ["Wahr\u00b7heit", ",", "als", "L\u00fc\u00b7ge", "?", "Gab", "uns", "doch", "Na\u00b7tur"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "NN", "$.", "VVFIN", "PPER", "ADV", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.9": {"text": "Zum Trost nur holden Wahn, der uns begl\u00fcckt,", "tokens": ["Zum", "Trost", "nur", "hol\u00b7den", "Wahn", ",", "der", "uns", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJA", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und Schattenbilder. Wo des Siegers Scheitel", "tokens": ["Und", "Schat\u00b7ten\u00b7bil\u00b7der", ".", "Wo", "des", "Sie\u00b7gers", "Schei\u00b7tel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$.", "PWAV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Kein Kranz zum Lohn des k\u00fchnen Wagens schm\u00fcckt,", "tokens": ["Kein", "Kranz", "zum", "Lohn", "des", "k\u00fch\u00b7nen", "Wa\u00b7gens", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Lebt tr\u00e4gt und dumpf umnachtet", "tokens": ["Lebt", "tr\u00e4gt", "und", "dumpf", "um\u00b7nach\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "VVFIN", "KON", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Ein Volk, das einst dem Ruhme nachgetrachtet,", "tokens": ["Ein", "Volk", ",", "das", "einst", "dem", "Ruh\u00b7me", "nach\u00b7ge\u00b7trach\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wer wei\u00df wie bald wird auf den Tr\u00fcmmerst\u00e4tten", "tokens": ["Wer", "wei\u00df", "wie", "bald", "wird", "auf", "den", "Tr\u00fcm\u00b7mer\u00b7st\u00e4t\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "KOKOM", "ADV", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Italischen Ruhms der Hirt", "tokens": ["I\u00b7ta\u00b7li\u00b7schen", "Ruhms", "der", "Hirt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Rinder weiden und der Pflug die Gipfel", "tokens": ["Die", "Rin\u00b7der", "wei\u00b7den", "und", "der", "Pflug", "die", "Gip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der sieben H\u00fcgel furchen. Bauen wird", "tokens": ["Der", "sie\u00b7ben", "H\u00fc\u00b7gel", "fur\u00b7chen", ".", "Bau\u00b7en", "wird"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "CARD", "NN", "VVINF", "$.", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nach kurzen Jahren schon in Latiums St\u00e4dten", "tokens": ["Nach", "kur\u00b7zen", "Jah\u00b7ren", "schon", "in", "La\u00b7ti\u00b7ums", "St\u00e4d\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV", "APPR", "NE", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Der schlaue Fuchs und mit dem dunklen Wipfel", "tokens": ["Der", "schlau\u00b7e", "Fuchs", "und", "mit", "dem", "dunk\u00b7len", "Wip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ein Hochwald rauschen zwischen \u00f6den Mauern,", "tokens": ["Ein", "Hoch\u00b7wald", "rau\u00b7schen", "zwi\u00b7schen", "\u00f6\u00b7den", "Mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wenn das Geschick nicht jener unheilvollen", "tokens": ["Wenn", "das", "Ge\u00b7schick", "nicht", "je\u00b7ner", "un\u00b7heil\u00b7vol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "PDAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Vergessenheit des Vaterlandes steuert", "tokens": ["Ver\u00b7ges\u00b7sen\u00b7heit", "des", "Va\u00b7ter\u00b7lan\u00b7des", "steu\u00b7ert"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Im tief verkommnen Volk, wenn nicht mit Trauern", "tokens": ["Im", "tief", "ver\u00b7komm\u00b7nen", "Volk", ",", "wenn", "nicht", "mit", "Trau\u00b7ern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJD", "ADJA", "NN", "$,", "KOUS", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Gedenkend, welch ein Muth uns einst befeuert,", "tokens": ["Ge\u00b7den\u00b7kend", ",", "welch", "ein", "Muth", "uns", "einst", "be\u00b7feu\u00b7ert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAT", "ART", "N