{"textgrid.poem.30833": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "47.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was geht es Menschen an, was mein Gewissen gleubet?", "tokens": ["Was", "geht", "es", "Men\u00b7schen", "an", ",", "was", "mein", "Ge\u00b7wis\u00b7sen", "gleu\u00b7bet", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "PTKVZ", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann sonst nur christlich Ding mein Lauff mit ihnen treibet.", "tokens": ["Wann", "sonst", "nur", "christ\u00b7lich", "Ding", "mein", "Lauff", "mit", "ih\u00b7nen", "trei\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ADJD", "NN", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gott gl\u00e4ub ich, was ich gl\u00e4ub; ich gl\u00e4ub es Menschen nicht;", "tokens": ["Gott", "gl\u00e4ub", "ich", ",", "was", "ich", "gl\u00e4ub", ";", "ich", "gl\u00e4ub", "es", "Men\u00b7schen", "nicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was richtet dann der Mensch, was Gott alleine richt?", "tokens": ["Was", "rich\u00b7tet", "dann", "der", "Mensch", ",", "was", "Gott", "al\u00b7lei\u00b7ne", "richt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}