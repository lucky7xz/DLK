{"textgrid.poem.62783": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Am fr\u00f6lichen Saturnusfest,", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am fr\u00f6lichen Saturnusfest,", "tokens": ["Am", "fr\u00f6\u00b7li\u00b7chen", "Sa\u00b7tur\u00b7nus\u00b7fest", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da die Moral an Evans Arme", "tokens": ["Da", "die", "Mo\u00b7ral", "an", "E\u00b7vans", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sich von der Scherze holdem Schwarme", "tokens": ["Sich", "von", "der", "Scher\u00b7ze", "hol\u00b7dem", "Schwar\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu Wein und M\u00e4dchen f\u00fchren l\u00e4\u00dft,", "tokens": ["Zu", "Wein", "und", "M\u00e4d\u00b7chen", "f\u00fch\u00b7ren", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gieng durch Athens belebte Gassen", "tokens": ["Gieng", "durch", "A\u00b7thens", "be\u00b7leb\u00b7te", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NE", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Ein muntrer Kopf, der ohne Bart", "tokens": ["Ein", "mun\u00b7trer", "Kopf", ",", "der", "oh\u00b7ne", "Bart"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Noch Buch zum Philosophen ward,", "tokens": ["Noch", "Buch", "zum", "Phi\u00b7lo\u00b7so\u00b7phen", "ward", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Und eben einen Schmaus verlassen.", "tokens": ["Und", "e\u00b7ben", "ei\u00b7nen", "Schmaus", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Nun dieser war vom rechten Schrot:", "tokens": ["Nun", "die\u00b7ser", "war", "vom", "rech\u00b7ten", "Schrot", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VAFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ein Demokrit lehrt auch im Lachen:", "tokens": ["Ein", "De\u00b7mo\u00b7krit", "lehrt", "auch", "im", "La\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.11": {"text": "Der Schulfuchs, der nur seufzt und droht,", "tokens": ["Der", "Schul\u00b7fuchs", ",", "der", "nur", "seufzt", "und", "droht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Wird wenig Proselyten machen.", "tokens": ["Wird", "we\u00b7nig", "Pro\u00b7se\u00b7ly\u00b7ten", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Mein Weiser schwang, indem er gieng,", "tokens": ["Mein", "Wei\u00b7ser", "schwang", ",", "in\u00b7dem", "er", "gieng", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Mit seiner Rechten eine Weide,", "tokens": ["Mit", "sei\u00b7ner", "Rech\u00b7ten", "ei\u00b7ne", "Wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Woran ein Band von bunter Seide", "tokens": ["Wo\u00b7ran", "ein", "Band", "von", "bun\u00b7ter", "Sei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Mit einer Zuckermandel hieng.", "tokens": ["Mit", "ei\u00b7ner", "Zu\u00b7cker\u00b7man\u00b7del", "hieng", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Ein ganzer Schwarm verschmitzter Knaben \u2013", "tokens": ["Ein", "gan\u00b7zer", "Schwarm", "ver\u00b7schmitz\u00b7ter", "Kna\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Auch M\u00e4dchen waren mit dabey \u2013", "tokens": ["Auch", "M\u00e4d\u00b7chen", "wa\u00b7ren", "mit", "da\u00b7bey", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "APPR", "PAV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Folgt ihm mit jauchzendem Geschrey;", "tokens": ["Folgt", "ihm", "mit", "jauch\u00b7zen\u00b7dem", "Ge\u00b7schrey", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Ein jeder will die Mandel haben.", "tokens": ["Ein", "je\u00b7der", "will", "die", "Man\u00b7del", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Ihr Kinder, rief der seltne Mann", "tokens": ["Ihr", "Kin\u00b7der", ",", "rief", "der", "selt\u00b7ne", "Mann"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Zu seinem h\u00fcpfenden Geleite,", "tokens": ["Zu", "sei\u00b7nem", "h\u00fcp\u00b7fen\u00b7den", "Ge\u00b7lei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Nur der bek\u00f6mmt die s\u00fc\u00dfe Beute,", "tokens": ["Nur", "der", "be\u00b7k\u00f6mmt", "die", "s\u00fc\u00b7\u00dfe", "Beu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Der mit dem Mund sie fangen kann.", "tokens": ["Der", "mit", "dem", "Mund", "sie", "fan\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Nun geht der rechte Spuck erst an.", "tokens": ["Nun", "geht", "der", "rech\u00b7te", "Spuck", "erst", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Sie schlie\u00dfen zehn gedr\u00e4ngte Kreise", "tokens": ["Sie", "schlie\u00b7\u00dfen", "zehn", "ge\u00b7dr\u00e4ng\u00b7te", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Und schnappen nach dem fl\u00fcchtgen Preise,", "tokens": ["Und", "schnap\u00b7pen", "nach", "dem", "fl\u00fccht\u00b7gen", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Den alle vor der Schnauze sahn,", "tokens": ["Den", "al\u00b7le", "vor", "der", "Schnau\u00b7ze", "sahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Wie Tantalus nach seinen Aepfeln", "tokens": ["Wie", "Tan\u00b7ta\u00b7lus", "nach", "sei\u00b7nen", "A\u00b7e\u00b7pfeln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.30": {"text": "Und ich nach einem Reim auf Aepfeln;", "tokens": ["Und", "ich", "nach", "ei\u00b7nem", "Reim", "auf", "A\u00b7e\u00b7pfeln", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Allein umsonst: weg war der Schatz.", "tokens": ["Al\u00b7lein", "um\u00b7sonst", ":", "weg", "war", "der", "Schatz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Ein jeder lauscht: jetzt k\u00f6mmt er wieder,", "tokens": ["Ein", "je\u00b7der", "lauscht", ":", "jetzt", "k\u00f6mmt", "er", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Und, wie auf einem Bo\u00dfelplatz", "tokens": ["Und", ",", "wie", "auf", "ei\u00b7nem", "Bo\u00b7\u00dfel\u00b7platz"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Der Kegel allzudichte Glieder,", "tokens": ["Der", "Ke\u00b7gel", "all\u00b7zu\u00b7dich\u00b7te", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "St\u00fcrzt flugs die halbe Gruppe nieder.", "tokens": ["St\u00fcrzt", "flugs", "die", "hal\u00b7be", "Grup\u00b7pe", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Hier flog ein Kranz, dort ri\u00df ein Latz,", "tokens": ["Hier", "flog", "ein", "Kranz", ",", "dort", "ri\u00df", "ein", "Latz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "So geht es, wenn man gerne naschet.", "tokens": ["So", "geht", "es", ",", "wenn", "man", "ger\u00b7ne", "na\u00b7schet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Der Weise lacht; die Gerte sinkt;", "tokens": ["Der", "Wei\u00b7se", "lacht", ";", "die", "Ger\u00b7te", "sinkt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Er merkt es nicht; Fortuna winkt,", "tokens": ["Er", "merkt", "es", "nicht", ";", "For\u00b7tu\u00b7na", "winkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$.", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Und kurz, die Mandel wird erhaschet.", "tokens": ["Und", "kurz", ",", "die", "Man\u00b7del", "wird", "er\u00b7ha\u00b7schet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ART", "NN", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Hilf Himmel, wie die Kinder schreyn!", "tokens": ["Hilf", "Him\u00b7mel", ",", "wie", "die", "Kin\u00b7der", "schreyn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Sie klopfen tanzend in die H\u00e4nde.", "tokens": ["Sie", "klop\u00b7fen", "tan\u00b7zend", "in", "die", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Doch nun hat auch der Spa\u00df ein Ende;", "tokens": ["Doch", "nun", "hat", "auch", "der", "Spa\u00df", "ein", "En\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Auf einmal stand mein Mann allein;", "tokens": ["Auf", "ein\u00b7mal", "stand", "mein", "Mann", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Es war als ob der Trupp verschw\u00e4nde.", "tokens": ["Es", "war", "als", "ob", "der", "Trupp", "ver\u00b7schw\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Die kleine braune Delia,", "tokens": ["Die", "klei\u00b7ne", "brau\u00b7ne", "De\u00b7lia", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NE", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.47": {"text": "Die niedlichste von allen Zofen,", "tokens": ["Die", "nied\u00b7lichs\u00b7te", "von", "al\u00b7len", "Zo\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Die das Turnier am Fenster sah,", "tokens": ["Die", "das", "Tur\u00b7nier", "am", "Fens\u00b7ter", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.49": {"text": "Verspottet unsern Philosophen.", "tokens": ["Ver\u00b7spot\u00b7tet", "un\u00b7sern", "Phi\u00b7lo\u00b7so\u00b7phen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.50": {"text": "O, rief er drohend, nicht gelacht,", "tokens": ["O", ",", "rief", "er", "dro\u00b7hend", ",", "nicht", "ge\u00b7lacht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "VVPP", "$,", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Mein T\u00e4ubchen! nimm dich wohl in acht!", "tokens": ["Mein", "T\u00e4ub\u00b7chen", "!", "nimm", "dich", "wohl", "in", "acht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVIMP", "PPER", "ADV", "APPR", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "So fliehn der Lecker bunte Schaaren,", "tokens": ["So", "fliehn", "der", "Le\u00b7cker", "bun\u00b7te", "Schaa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "Wenn sie der Unschuld Sieger sind.", "tokens": ["Wenn", "sie", "der", "Un\u00b7schuld", "Sie\u00b7ger", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Ihr M\u00e4dchen, Amor ist ein Kind;", "tokens": ["Ihr", "M\u00e4d\u00b7chen", ",", "A\u00b7mor", "ist", "ein", "Kind", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NE", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "La\u00dft ihm die Mandel ja nicht fahren!", "tokens": ["La\u00dft", "ihm", "die", "Man\u00b7del", "ja", "nicht", "fah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}