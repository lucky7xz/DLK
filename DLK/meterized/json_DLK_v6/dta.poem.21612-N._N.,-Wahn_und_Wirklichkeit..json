{"dta.poem.21612": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Wahn und Wirklichkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Als der Duft der ersten Veilchen", "tokens": ["Als", "der", "Duft", "der", "ers\u00b7ten", "Veil\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber meine Stirne flog,", "tokens": ["Ue\u00b7ber", "mei\u00b7ne", "Stir\u00b7ne", "flog", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "War es, da\u00df ein wundersamer", "tokens": ["War", "es", ",", "da\u00df", "ein", "wun\u00b7der\u00b7sa\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Traum in meine Seele zog.", "tokens": ["Traum", "in", "mei\u00b7ne", "See\u00b7le", "zog", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und zwei Sterne sah ich leuchten,", "tokens": ["Und", "zwei", "Ster\u00b7ne", "sah", "ich", "leuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Stilles Blinken heilger Nacht;", "tokens": ["Stil\u00b7les", "Blin\u00b7ken", "heil\u00b7ger", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mein Auge mu\u00dfte schauen", "tokens": ["Und", "mein", "Au\u00b7ge", "mu\u00df\u00b7te", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hingebannt nach solcher Macht.", "tokens": ["Hin\u00b7ge\u00b7bannt", "nach", "sol\u00b7cher", "Macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie das Angesicht der G\u00f6ttin", "tokens": ["Wie", "das", "An\u00b7ge\u00b7sicht", "der", "G\u00f6t\u00b7tin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sah der Mond herab so gut", "tokens": ["Sah", "der", "Mond", "her\u00b7ab", "so", "gut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mein Herz wallt\u2019 ihm entgegen", "tokens": ["Und", "mein", "Herz", "wallt'", "ihm", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie die liebevolle Fluth.", "tokens": ["Wie", "die", "lie\u00b7be\u00b7vol\u00b7le", "Fluth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Eine Sonne sah ich gl\u00e4nzen,", "tokens": ["Ei\u00b7ne", "Son\u00b7ne", "sah", "ich", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6nres wurde nie mir kund", "tokens": ["Sch\u00f6n\u00b7res", "wur\u00b7de", "nie", "mir", "kund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADV", "PPER", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ihr Gl\u00e4nzen war wie L\u00e4cheln", "tokens": ["Und", "ihr", "Gl\u00e4n\u00b7zen", "war", "wie", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "KOKOM", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von melodischestem Mund.", "tokens": ["Von", "me\u00b7lo\u00b7di\u00b7sches\u00b7tem", "Mund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und der Sonne warme Strahlen", "tokens": ["Und", "der", "Son\u00b7ne", "war\u00b7me", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spielten mir um meine Brust,", "tokens": ["Spiel\u00b7ten", "mir", "um", "mei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sorgsam so wie Mutterarme", "tokens": ["Sorg\u00b7sam", "so", "wie", "Mut\u00b7ter\u00b7ar\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADV", "KOKOM", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoben sie mich auf vom Dust.", "tokens": ["Ho\u00b7ben", "sie", "mich", "auf", "vom", "Dust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Trugen mich durch leichte L\u00fcfte", "tokens": ["Tru\u00b7gen", "mich", "durch", "leich\u00b7te", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach dem Glanz, dem Himmelslicht", "tokens": ["Nach", "dem", "Glanz", ",", "dem", "Him\u00b7mels\u00b7licht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An das hei\u00dfe Herz der Sonne,", "tokens": ["An", "das", "hei\u00b7\u00dfe", "Herz", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber ich verbrannte nicht.", "tokens": ["A\u00b7ber", "ich", "ver\u00b7brann\u00b7te", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Unzerst\u00f6rbar meine Glieder,", "tokens": ["Un\u00b7zer\u00b7st\u00f6r\u00b7bar", "mei\u00b7ne", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unversiegbar hei\u00df mein Blut,", "tokens": ["Un\u00b7ver\u00b7sieg\u00b7bar", "hei\u00df", "mein", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohne Leiden meine Seele,", "tokens": ["Oh\u00b7ne", "Lei\u00b7den", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unbesiegbar hehr mein Muth;", "tokens": ["Un\u00b7be\u00b7sieg\u00b7bar", "hehr", "mein", "Muth", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ohne Gr\u00e4nzen die Gedanken,", "tokens": ["Oh\u00b7ne", "Gr\u00e4n\u00b7zen", "die", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unverschleiert war die Welt, \u2014", "tokens": ["Un\u00b7ver\u00b7schlei\u00b7ert", "war", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da hat eine b\u00f6se Kr\u00e4he", "tokens": ["Da", "hat", "ei\u00b7ne", "b\u00f6\u00b7se", "Kr\u00e4\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich aus allem Traum gegellt:", "tokens": ["Mich", "aus", "al\u00b7lem", "Traum", "ge\u00b7gellt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PIS", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "\u201ethor, was n\u00e4rrst du deine Seele", "tokens": ["\u201e", "thor", ",", "was", "n\u00e4rrst", "du", "dei\u00b7ne", "See\u00b7le"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "NN", "$,", "PWS", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem n\u00e4chtlich eitlen Trug?", "tokens": ["Mit", "dem", "n\u00e4cht\u00b7lich", "eit\u00b7len", "Trug", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tag ist\u2019s; gehe hin und schaffe,", "tokens": ["Tag", "ist's", ";", "ge\u00b7he", "hin", "und", "schaf\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$.", "VVFIN", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn zu schaffen giebt\u2019s genug!\u201c", "tokens": ["Denn", "zu", "schaf\u00b7fen", "giebt's", "ge\u00b7nug", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PTKZU", "VVINF", "VVFIN", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und ich schlich besch\u00e4mt nach Hause,", "tokens": ["Und", "ich", "schlich", "be\u00b7sch\u00e4mt", "nach", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hatte wahrlich wenig Lust,", "tokens": ["Hat\u00b7te", "wahr\u00b7lich", "we\u00b7nig", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn noch spielten Veilchend\u00fcfte", "tokens": ["Denn", "noch", "spiel\u00b7ten", "Veil\u00b7chen\u00b7d\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Mir um meine Stirn und Brust.", "tokens": ["Mir", "um", "mei\u00b7ne", "Stirn", "und", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Ach, was ist mit allem M\u00fchen,", "tokens": ["Ach", ",", "was", "ist", "mit", "al\u00b7lem", "M\u00fc\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "VAFIN", "APPR", "PIS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was mit aller Qual gethan!", "tokens": ["Was", "mit", "al\u00b7ler", "Qual", "ge\u00b7than", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mein Traum erschien mir wirklich", "tokens": ["Und", "mein", "Traum", "er\u00b7schien", "mir", "wirk\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Wirklichkeit ein Wahn.", "tokens": ["Und", "die", "Wirk\u00b7lich\u00b7keit", "ein", "Wahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}