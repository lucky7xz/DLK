{"textgrid.poem.36972": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Den verdammten Kerl, den Geist,", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den verdammten Kerl, den Geist,", "tokens": ["Den", "ver\u00b7damm\u00b7ten", "Kerl", ",", "den", "Geist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00fcssen wir doch kriegen,", "tokens": ["M\u00fcs\u00b7sen", "wir", "doch", "krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df dem Demagogen nicht", "tokens": ["Da\u00df", "dem", "De\u00b7ma\u00b7go\u00b7gen", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wir noch unterliegen!", "tokens": ["Wir", "noch", "un\u00b7ter\u00b7lie\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Zehn Mal Hunderttausend Mann!", "tokens": ["Zehn", "Mal", "Hun\u00b7dert\u00b7tau\u00b7send", "Mann", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "CARD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf, Soldaten, drauf und dran!", "tokens": ["Auf", ",", "Sol\u00b7da\u00b7ten", ",", "drauf", "und", "dran", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "NN", "$,", "PAV", "KON", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ladet die Gewehre,", "tokens": ["La\u00b7det", "die", "Ge\u00b7weh\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Rettet unsre Ehre!", "tokens": ["Ret\u00b7tet", "uns\u00b7re", "Eh\u00b7re", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Und sie schie\u00dfen wuthentbrannt", "tokens": ["Und", "sie", "schie\u00b7\u00dfen", "wut\u00b7hent\u00b7brannt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Selbst sich todt, die Blinden;", "tokens": ["Selbst", "sich", "todt", ",", "die", "Blin\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PRF", "ADJD", "$,", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sie vernichten Stadt und Land:", "tokens": ["Sie", "ver\u00b7nich\u00b7ten", "Stadt", "und", "Land", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Geist \u2013 ist nicht zu finden.", "tokens": ["Geist", "\u2013", "ist", "nicht", "zu", "fin\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Das hier ist die letzte Stadt,", "tokens": ["Das", "hier", "ist", "die", "letz\u00b7te", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Hier m\u00fc\u00dft ihr ihn fassen!", "tokens": ["Hier", "m\u00fc\u00dft", "ihr", "ihn", "fas\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Seht! verwegen h\u00fcpft er dort", "tokens": ["Seht", "!", "ver\u00b7we\u00b7gen", "h\u00fcpft", "er", "dort"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "ADJD", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Munter durch die Gassen.", "tokens": ["Mun\u00b7ter", "durch", "die", "Gas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Polizei, entwickle dich!", "tokens": ["Po\u00b7li\u00b7zei", ",", "ent\u00b7wick\u00b7le", "dich", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Du ergreifst ihn sicherlich!", "tokens": ["Du", "er\u00b7greifst", "ihn", "si\u00b7cher\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ist er dein geworden,", "tokens": ["Ist", "er", "dein", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "VAPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Schm\u00fccke dich ein Orden.", "tokens": ["Schm\u00fc\u00b7cke", "dich", "ein", "Or\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Geist schaut dort, im letzten Haus,", "tokens": ["Geist", "schaut", "dort", ",", "im", "letz\u00b7ten", "Haus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus dem Erkerst\u00fcbchen,", "tokens": ["Aus", "dem", "Er\u00b7ker\u00b7st\u00fcb\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Lachet die Spione aus", "tokens": ["La\u00b7chet", "die", "Spi\u00b7o\u00b7ne", "aus"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Und schabt ihnen R\u00fcbchen.", "tokens": ["Und", "schabt", "ih\u00b7nen", "R\u00fcb\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Jetzt entwischt er uns nicht mehr,", "tokens": ["Jetzt", "ent\u00b7wischt", "er", "uns", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Jetzt ist er gefangen!", "tokens": ["Jetzt", "ist", "er", "ge\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Morgen soll der B\u00f6sewicht", "tokens": ["Mor\u00b7gen", "soll", "der", "B\u00f6\u00b7se\u00b7wicht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schon am Galgen hangen.", "tokens": ["Schon", "am", "Gal\u00b7gen", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Schnell die Stufen hier hinauf!", "tokens": ["Schnell", "die", "Stu\u00b7fen", "hier", "hin\u00b7auf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hurtig, sprengt die Th\u00fcre auf!", "tokens": ["Hur\u00b7tig", ",", "sprengt", "die", "Th\u00fc\u00b7re", "auf", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Greift den Kerl, da sitzt er!", "tokens": ["Greift", "den", "Kerl", ",", "da", "sitzt", "er", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Aus den Augen blitzt er!", "tokens": ["Aus", "den", "Au\u00b7gen", "blitzt", "er", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Geist schl\u00fcpft in ein kleines Buch,", "tokens": ["Geist", "schl\u00fcpft", "in", "ein", "klei\u00b7nes", "Buch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deckt sich zu mit Lettern;", "tokens": ["Deckt", "sich", "zu", "mit", "Let\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sicher ist er da genug,", "tokens": ["Si\u00b7cher", "ist", "er", "da", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie sie sp\u00e4hn und bl\u00e4ttern.", "tokens": ["Wie", "sie", "sp\u00e4hn", "und", "bl\u00e4t\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Schlie\u00dft das Buch und bindet's zu!", "tokens": ["Schlie\u00dft", "das", "Buch", "und", "bin\u00b7det's", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "NE", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohne zu bekennen", "tokens": ["Oh\u00b7ne", "zu", "be\u00b7ken\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "PTKZU", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Soll er auf dem Markt sogleich", "tokens": ["Soll", "er", "auf", "dem", "Markt", "sog\u00b7leich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem Buch verbrennen!", "tokens": ["Mit", "dem", "Buch", "ver\u00b7bren\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Richtet mir den Holzsto\u00df her!", "tokens": ["Rich\u00b7tet", "mir", "den", "Holz\u00b7sto\u00df", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf, Soldaten, in's Gewehr!", "tokens": ["Auf", ",", "Sol\u00b7da\u00b7ten", ",", "in's", "Ge\u00b7wehr", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "$,", "NN", "$,", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Lodert, lodert, Flammen!", "tokens": ["Lo\u00b7dert", ",", "lo\u00b7dert", ",", "Flam\u00b7men", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Gott soll ihn verdammen!", "tokens": ["Gott", "soll", "ihn", "ver\u00b7dam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Wundersame Melodien", "tokens": ["Wun\u00b7der\u00b7sa\u00b7me", "Me\u00b7lo\u00b7dien"], "token_info": ["word", "word"], "pos": ["NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rt die stumme Menge,", "tokens": ["H\u00f6rt", "die", "stum\u00b7me", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und in alle Herzen ziehn", "tokens": ["Und", "in", "al\u00b7le", "Her\u00b7zen", "ziehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jene Zauberkl\u00e4nge.", "tokens": ["Je\u00b7ne", "Zau\u00b7ber\u00b7kl\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Pl\u00f6tzlich donnert's durch den Dampf", "tokens": ["Pl\u00f6tz\u00b7lich", "don\u00b7nert's", "durch", "den", "Dampf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ein fern Gewitter;", "tokens": ["Wie", "ein", "fern", "Ge\u00b7wit\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Lichtumflossen steigt empor", "tokens": ["Lich\u00b7tum\u00b7flos\u00b7sen", "steigt", "em\u00b7por"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Draus ein goldner Ritter.", "tokens": ["Draus", "ein", "gold\u00b7ner", "Rit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Auf, ihr V\u00f6lker! ruft er laut,", "tokens": ["Auf", ",", "ihr", "V\u00f6l\u00b7ker", "!", "ruft", "er", "laut", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf zum Freiheitskriege!", "tokens": ["Auf", "zum", "Frei\u00b7heits\u00b7krie\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Wer dem ew'gen Geist vertraut,", "tokens": ["Wer", "dem", "ew'\u00b7gen", "Geist", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Den f\u00fchrt er zum Siege!", "tokens": ["Den", "f\u00fchrt", "er", "zum", "Sie\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.10": {"line.1": {"text": "Wie sie martern ihn und wie", "tokens": ["Wie", "sie", "mar\u00b7tern", "ihn", "und", "wie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "PPER", "KON", "PWAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Trachten nach dem Leben:", "tokens": ["Trach\u00b7ten", "nach", "dem", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Gott der Herr wird nun und nie", "tokens": ["Gott", "der", "Herr", "wird", "nun", "und", "nie"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VAFIN", "ADV", "KON", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seinen Geist aufgeben.", "tokens": ["Sei\u00b7nen", "Geist", "auf\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}