{"textgrid.poem.25622": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbsein Angedenken, das bei Wittwen und bei Waisen", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbsein Angedenken, das bei Wittwen und bei Waisen", "tokens": ["\u00bb", "sein", "An\u00b7ge\u00b7den\u00b7ken", ",", "das", "bei", "Witt\u00b7wen", "und", "bei", "Wai\u00b7sen"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "$,", "PRELS", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sp\u00e4t dauren wird,\u00ab", "tokens": ["Sp\u00e4t", "dau\u00b7ren", "wird", ",", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VVINF", "VAFIN", "$,", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "So stand am Grabmal' da.", "tokens": ["So", "stand", "am", "Grab\u00b7mal'", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es war auf allen meinen Reisen", "tokens": ["Es", "war", "auf", "al\u00b7len", "mei\u00b7nen", "Rei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die wahrste Grabschrift, die ich sah.", "tokens": ["Die", "wahrs\u00b7te", "Grab\u00b7schrift", ",", "die", "ich", "sah", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}