{"textgrid.poem.36548": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vogel! sprach ein Fuchs zu einem Raben,", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vogel! sprach ein Fuchs zu einem Raben,", "tokens": ["Vo\u00b7gel", "!", "sprach", "ein", "Fuchs", "zu", "ei\u00b7nem", "Ra\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "ART", "NE", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der auf einem hohen Baume sa\u00df,", "tokens": ["Der", "auf", "ei\u00b7nem", "ho\u00b7hen", "Bau\u00b7me", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und in seinem Schnabel einen sch\u00f6nen Fra\u00df,", "tokens": ["Und", "in", "sei\u00b7nem", "Schna\u00b7bel", "ei\u00b7nen", "sch\u00f6\u00b7nen", "Fra\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Einen K\u00e4s' hielt; welche Stimme mu\u00dft du haben!", "tokens": ["Ei\u00b7nen", "K\u00e4s'", "hielt", ";", "wel\u00b7che", "Stim\u00b7me", "mu\u00dft", "du", "ha\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PWAT", "NN", "VMFIN", "PPER", "VAFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Ei! du bist ja sch\u00f6n!", "tokens": ["Ei", "!", "du", "bist", "ja", "sch\u00f6n", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Solchen Vogel hab' ich nie gesehn!", "tokens": ["Sol\u00b7chen", "Vo\u00b7gel", "hab'", "ich", "nie", "ge\u00b7sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Fremdling, ohne Zweifel, hoch in Ehren,", "tokens": ["Fremd\u00b7ling", ",", "oh\u00b7ne", "Zwei\u00b7fel", ",", "hoch", "in", "Eh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Deine Stimme m\u00f6cht' ich h\u00f6ren!", "tokens": ["Dei\u00b7ne", "Stim\u00b7me", "m\u00f6cht'", "ich", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Rap, und Rap, und Rap, erschallt", "tokens": ["Rap", ",", "und", "Rap", ",", "und", "Rap", ",", "er\u00b7schallt"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "$,", "KON", "NE", "$,", "KON", "NE", "$,", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Augenblicklich durch den Wald.", "tokens": ["Au\u00b7gen\u00b7blick\u00b7lich", "durch", "den", "Wald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus des S\u00e4ngers aufgemachten Schnabel", "tokens": ["Aus", "des", "S\u00e4n\u00b7gers", "auf\u00b7ge\u00b7mach\u00b7ten", "Schna\u00b7bel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "F\u00e4llt der K\u00e4se nieder vor dem Fuchs;", "tokens": ["F\u00e4llt", "der", "K\u00e4\u00b7se", "nie\u00b7der", "vor", "dem", "Fuchs", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "APPR", "ART", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und der Schmeichler nimmt ihn flugs!", "tokens": ["Und", "der", "Schmeich\u00b7ler", "nimmt", "ihn", "flugs", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "F\u00fcrsten, merkt euch diese Fabel!", "tokens": ["F\u00fcrs\u00b7ten", ",", "merkt", "euch", "die\u00b7se", "Fa\u00b7bel", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}