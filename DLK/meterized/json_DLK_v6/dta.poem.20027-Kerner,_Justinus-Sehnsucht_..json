{"dta.poem.20027": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Sehnsucht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O k\u00f6nnt' ich einmal los", "tokens": ["O", "k\u00f6nnt'", "ich", "ein\u00b7mal", "los"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "ADV", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Von all' dem Menschentreiben,", "tokens": ["Von", "all'", "dem", "Men\u00b7schen\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Natur! in deinem Schoo\u00df", "tokens": ["Na\u00b7tur", "!", "in", "dei\u00b7nem", "Schoo\u00df"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein herzlich Kind verbleiben!", "tokens": ["Ein", "herz\u00b7lich", "Kind", "ver\u00b7blei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mich rief ein Traum so schwer", "tokens": ["Mich", "rief", "ein", "Traum", "so", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Aus deinen Mutterarmen,", "tokens": ["Aus", "dei\u00b7nen", "Mut\u00b7ter\u00b7ar\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Seitdem kann nimmermehr", "tokens": ["Seit\u00b7dem", "kann", "nim\u00b7mer\u00b7mehr"], "token_info": ["word", "word", "word"], "pos": ["PAV", "VMFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das kranke Herz erwarmen.", "tokens": ["Das", "kran\u00b7ke", "Herz", "er\u00b7war\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Menschen Treiben, ach!", "tokens": ["Der", "Men\u00b7schen", "Trei\u00b7ben", ",", "ach", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ITJ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das h\u00e4lt mich nun gefangen,", "tokens": ["Das", "h\u00e4lt", "mich", "nun", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das folgt mir st\u00f6rend nach", "tokens": ["Das", "folgt", "mir", "st\u00f6\u00b7rend", "nach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wo Erd' und Himmel prangen.", "tokens": ["Wo", "Erd'", "und", "Him\u00b7mel", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch ist dies Treiben mir", "tokens": ["Doch", "ist", "dies", "Trei\u00b7ben", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PDS", "NN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So fremd und so unherzlich,", "tokens": ["So", "fremd", "und", "so", "un\u00b7herz\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und, Mutter, ach nach dir", "tokens": ["Und", ",", "Mut\u00b7ter", ",", "ach", "nach", "dir"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "NN", "$,", "ADV", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zieht mich ein Heimweh schmerzlich!", "tokens": ["Zieht", "mich", "ein", "Heim\u00b7weh", "schmerz\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O nimm dein reuig Kind", "tokens": ["O", "nimm", "dein", "reu\u00b7ig", "Kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In deine Mutterarme,", "tokens": ["In", "dei\u00b7ne", "Mut\u00b7ter\u00b7ar\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df dir's am Busen lind", "tokens": ["Da\u00df", "dir's", "am", "Bu\u00b7sen", "lind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPRART", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zu neuer Lieb erwarme!", "tokens": ["Zu", "neu\u00b7er", "Lieb", "er\u00b7war\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wie ist's ergangen mir,", "tokens": ["Wie", "ist's", "er\u00b7gan\u00b7gen", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "VVPP", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df ich verirrt so lange!", "tokens": ["Da\u00df", "ich", "ver\u00b7irrt", "so", "lan\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mutter! zu dir, zu dir!", "tokens": ["Mut\u00b7ter", "!", "zu", "dir", ",", "zu", "dir", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "PPER", "$,", "APPR", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Wie ist's mir weh und bange;", "tokens": ["Wie", "ist's", "mir", "weh", "und", "ban\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PTKVZ", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Bis ich wie Blum' und Quell", "tokens": ["Bis", "ich", "wie", "Blum'", "und", "Quell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KOKOM", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dir darf im Herzen bleiben,", "tokens": ["Dir", "darf", "im", "Her\u00b7zen", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mutter! o f\u00fchr' mich schnell", "tokens": ["Mut\u00b7ter", "!", "o", "f\u00fchr'", "mich", "schnell"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "FM", "VVFIN", "PPER", "ADJD"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Hin, wo kein Menschentreiben!", "tokens": ["Hin", ",", "wo", "kein", "Men\u00b7schen\u00b7trei\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PIAT", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}