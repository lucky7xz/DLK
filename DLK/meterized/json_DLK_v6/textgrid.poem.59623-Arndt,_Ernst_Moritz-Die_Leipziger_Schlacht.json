{"textgrid.poem.59623": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Die Leipziger Schlacht", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo kommst du her in dem roten Kleid", "tokens": ["Wo", "kommst", "du", "her", "in", "dem", "ro\u00b7ten", "Kleid"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und f\u00e4rbst das Gras auf dem gr\u00fcnen Plan?", "tokens": ["Und", "f\u00e4rbst", "das", "Gras", "auf", "dem", "gr\u00fc\u00b7nen", "Plan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich komm' aus blutigem M\u00e4nnerstreit,", "tokens": ["Ich", "komm'", "aus", "blu\u00b7ti\u00b7gem", "M\u00e4n\u00b7ner\u00b7streit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich komme rot von der Ehrenbahn.", "tokens": ["Ich", "kom\u00b7me", "rot", "von", "der", "Eh\u00b7ren\u00b7bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wir haben die blutige Schlacht geschlagen,", "tokens": ["Wir", "ha\u00b7ben", "die", "blu\u00b7ti\u00b7ge", "Schlacht", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Drob m\u00fcssen die M\u00fctter und Br\u00e4ute klagen,", "tokens": ["Drob", "m\u00fcs\u00b7sen", "die", "M\u00fct\u00b7ter", "und", "Br\u00e4u\u00b7te", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Da ward ich so rot.", "tokens": ["Da", "ward", "ich", "so", "rot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Sag' an, Gesell, und verk\u00fcnde mir,", "tokens": ["Sag'", "an", ",", "Ge\u00b7sell", ",", "und", "ver\u00b7k\u00fcn\u00b7de", "mir", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "NN", "$,", "KON", "VVFIN", "PPER", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Wie hei\u00dft das Land, wo ihr schlugt die Schlacht?", "tokens": ["Wie", "hei\u00dft", "das", "Land", ",", "wo", "ihr", "schlugt", "die", "Schlacht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Bei Leipzig trauert das Mordrevier,", "tokens": ["Bei", "Leip\u00b7zig", "trau\u00b7ert", "das", "Mor\u00b7dre\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das manches Auge voll Tr\u00e4nen macht,", "tokens": ["Das", "man\u00b7ches", "Au\u00b7ge", "voll", "Tr\u00e4\u00b7nen", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Da flogen die Kugeln wie Winterflocken,", "tokens": ["Da", "flo\u00b7gen", "die", "Ku\u00b7geln", "wie", "Win\u00b7ter\u00b7flo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KOKOM", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und Tausenden mu\u00dfte der Atem stocken", "tokens": ["Und", "Tau\u00b7sen\u00b7den", "mu\u00df\u00b7te", "der", "A\u00b7tem", "sto\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Bei Leipzig der Stadt.", "tokens": ["Bei", "Leip\u00b7zig", "der", "Stadt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Wie hei\u00dfen, die zogen ins Todesfeld", "tokens": ["Wie", "hei\u00b7\u00dfen", ",", "die", "zo\u00b7gen", "ins", "To\u00b7des\u00b7feld"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVINF", "$,", "PRELS", "VVFIN", "APPRART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und lie\u00dfen fliegende Banner aus?", "tokens": ["Und", "lie\u00b7\u00dfen", "flie\u00b7gen\u00b7de", "Ban\u00b7ner", "aus", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es kamen V\u00f6lker aus aller Welt,", "tokens": ["Es", "ka\u00b7men", "V\u00f6l\u00b7ker", "aus", "al\u00b7ler", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die zogen gegen die Franzosen aus,", "tokens": ["Die", "zo\u00b7gen", "ge\u00b7gen", "die", "Fran\u00b7zo\u00b7sen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Russen, die Schweden, die tapfern Preu\u00dfen", "tokens": ["Die", "Rus\u00b7sen", ",", "die", "Schwe\u00b7den", ",", "die", "tap\u00b7fern", "Preu\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "NE", "$,", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und die nach dem glorreichen \u00d6streich hei\u00dfen,", "tokens": ["Und", "die", "nach", "dem", "glor\u00b7rei\u00b7chen", "\u00d6s\u00b7treich", "hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Die zogen all aus.", "tokens": ["Die", "zo\u00b7gen", "all", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Wem ward der Sieg in dem harten Streit,", "tokens": ["Wem", "ward", "der", "Sieg", "in", "dem", "har\u00b7ten", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wem ward der Preis mit der Eisenhand?", "tokens": ["Wem", "ward", "der", "Preis", "mit", "der", "Ei\u00b7sen\u00b7hand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Die Welschen hat Gott wie die Spreu zerstreut,", "tokens": ["Die", "Wel\u00b7schen", "hat", "Gott", "wie", "die", "Spreu", "zer\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "KOKOM", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die Welschen hat Gott verweht wie den Sand;", "tokens": ["Die", "Wel\u00b7schen", "hat", "Gott", "ver\u00b7weht", "wie", "den", "Sand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Viele Tausende decken den gr\u00fcnen Rasen,", "tokens": ["Vie\u00b7le", "Tau\u00b7sen\u00b7de", "de\u00b7cken", "den", "gr\u00fc\u00b7nen", "Ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Die \u00dcbriggebliebnen entflohen wie Hasen,", "tokens": ["Die", "\u00dcb\u00b7rig\u00b7ge\u00b7blieb\u00b7nen", "ent\u00b7flo\u00b7hen", "wie", "Ha\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KOKOM", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Napoleon mit.", "tokens": ["Na\u00b7po\u00b7le\u00b7on", "mit", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Nimm Gottes Lohn! Habe Dank, Gesell!", "tokens": ["Nimm", "Got\u00b7tes", "Lohn", "!", "Ha\u00b7be", "Dank", ",", "Ge\u00b7sell", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "NN", "$.", "NN", "NN", "$,", "NN", "$."], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Das war ein Klang, der das Herz erfreut!", "tokens": ["Das", "war", "ein", "Klang", ",", "der", "das", "Herz", "er\u00b7freut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das klang wie himmlische Zimbeln hell,", "tokens": ["Das", "klang", "wie", "himm\u00b7li\u00b7sche", "Zim\u00b7beln", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Habe Dank der M\u00e4r von dem blutigen Streit!", "tokens": ["Ha\u00b7be", "Dank", "der", "M\u00e4r", "von", "dem", "blu\u00b7ti\u00b7gen", "Streit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "La\u00df Witwen und Br\u00e4ute die Toten klagen,", "tokens": ["La\u00df", "Wit\u00b7wen", "und", "Br\u00e4u\u00b7te", "die", "To\u00b7ten", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Wir singen noch fr\u00f6hlich in sp\u00e4testen Tagen", "tokens": ["Wir", "sin\u00b7gen", "noch", "fr\u00f6h\u00b7lich", "in", "sp\u00e4\u00b7tes\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Die Leipziger Schlacht.", "tokens": ["Die", "Leip\u00b7zi\u00b7ger", "Schlacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "O Leipzig, freundliche Lindenstadt,", "tokens": ["O", "Leip\u00b7zig", ",", "freund\u00b7li\u00b7che", "Lin\u00b7den\u00b7stadt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dir ward ein leuchtendes Ehrenmal:", "tokens": ["Dir", "ward", "ein", "leuch\u00b7ten\u00b7des", "Eh\u00b7ren\u00b7mal", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Solange rollet der Jahre Rad,", "tokens": ["So\u00b7lan\u00b7ge", "rol\u00b7let", "der", "Jah\u00b7re", "Rad", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Solange scheinet der Sonnenstrahl,", "tokens": ["So\u00b7lan\u00b7ge", "schei\u00b7net", "der", "Son\u00b7nen\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Solange die Str\u00f6me zum Meere reisen,", "tokens": ["So\u00b7lan\u00b7ge", "die", "Str\u00f6\u00b7me", "zum", "Mee\u00b7re", "rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Wird noch der sp\u00e4teste Enkel preisen", "tokens": ["Wird", "noch", "der", "sp\u00e4\u00b7tes\u00b7te", "En\u00b7kel", "prei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Die Leipziger Schlacht.", "tokens": ["Die", "Leip\u00b7zi\u00b7ger", "Schlacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}