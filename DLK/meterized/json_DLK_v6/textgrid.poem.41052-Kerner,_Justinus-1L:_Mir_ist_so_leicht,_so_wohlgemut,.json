{"textgrid.poem.41052": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mir ist so leicht, so wohlgemut,", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir ist so leicht, so wohlgemut,", "tokens": ["Mir", "ist", "so", "leicht", ",", "so", "wohl\u00b7ge\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da f\u00e4llt mir pl\u00f6tzlich ein:", "tokens": ["Da", "f\u00e4llt", "mir", "pl\u00f6tz\u00b7lich", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist's Menschenherzen gar zu gut,", "tokens": ["Ist's", "Men\u00b7schen\u00b7her\u00b7zen", "gar", "zu", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bricht Ungl\u00fcck bald herein.", "tokens": ["Bricht", "Un\u00b7gl\u00fcck", "bald", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und in die Wonne mischt sich Schmerz,", "tokens": ["Und", "in", "die", "Won\u00b7ne", "mischt", "sich", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PRF", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch immerfort es spricht:", "tokens": ["Doch", "im\u00b7mer\u00b7fort", "es", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auch jetzt noch ist zu froh dein Herz \u2013", "tokens": ["Auch", "jetzt", "noch", "ist", "zu", "froh", "dein", "Herz", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VAFIN", "PTKA", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Herein ein Ungl\u00fcck bricht.", "tokens": ["Her\u00b7ein", "ein", "Un\u00b7gl\u00fcck", "bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}