{"dta.poem.10817": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Gedanken  \n bei der Vorstellung eines Egyptischen  \n Tempels.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist aus der Geschicht von langer Zeit\nbekandt,", "tokens": ["Es", "ist", "aus", "der", "Ge\u00b7schicht", "von", "lan\u00b7ger", "Zeit", "be\u00b7kandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df in Egypten sey der G\u00f6zzen Vater-", "tokens": ["Da\u00df", "in", "E\u00b7gyp\u00b7ten", "sey", "der", "G\u00f6z\u00b7zen", "Va\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NE", "VAFIN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und da\u00df dies weise Volck, davon wir so viel le-", "tokens": ["Und", "da\u00df", "dies", "wei\u00b7se", "Volck", ",", "da\u00b7von", "wir", "so", "viel", "le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PDS", "ADJA", "NN", "$,", "PAV", "PPER", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+----+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "In der Religion ganz rasend toll gewesen.", "tokens": ["In", "der", "Re\u00b7li\u00b7gi\u00b7on", "ganz", "ra\u00b7send", "toll", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADJD", "ADJD", "VAPP", "$."], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Des Aberglaubens Schuz, der Priester grosses", "tokens": ["Des", "A\u00b7berg\u00b7lau\u00b7bens", "Schuz", ",", "der", "Pries\u00b7ter", "gros\u00b7ses"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das machte immerfort der G\u00f6tter Anzahl mehr;", "tokens": ["Das", "mach\u00b7te", "im\u00b7mer\u00b7fort", "der", "G\u00f6t\u00b7ter", "An\u00b7zahl", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein fre\u00dfig Crocodill, ein Ochs mit bunten Flek-", "tokens": ["Ein", "fre\u00b7\u00dfig", "Cro\u00b7co\u00b7dill", ",", "ein", "Ochs", "mit", "bun\u00b7ten", "Flek"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NE", "$,", "ART", "NN", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ward als ein ", "tokens": ["Ward", "als", "ein"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "KOKOM", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Was in den G\u00e4rten wuchs, ein stinkend Knobe-", "tokens": ["Was", "in", "den", "G\u00e4r\u00b7ten", "wuchs", ",", "ein", "stin\u00b7kend", "Kno\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,", "ART", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ein thr\u00e4nend Zwiebelein, und andre Kr\u00e4uter", "tokens": ["Ein", "thr\u00e4\u00b7nend", "Zwie\u00b7be\u00b7lein", ",", "und", "and\u00b7re", "Kr\u00e4u\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Die musten G\u00f6tter seyn; Und vor der G\u00f6zzen", "tokens": ["Die", "mus\u00b7ten", "G\u00f6t\u00b7ter", "seyn", ";", "Und", "vor", "der", "G\u00f6z\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAINF", "$.", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Ward fast das ganze Land, ob es gleich gro\u00df, zu", "tokens": ["Ward", "fast", "das", "gan\u00b7ze", "Land", ",", "ob", "es", "gleich", "gro\u00df", ",", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "ADV", "ADJD", "$,", "APPR"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Des Aberglaubens Pracht ward auch daselbst zu", "tokens": ["Des", "A\u00b7berg\u00b7lau\u00b7bens", "Pracht", "ward", "auch", "da\u00b7selbst", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "PAV", "PTKZU"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "An Tempeln, welche sehr der G\u00f6zzen Werth erh\u00f6hn.", "tokens": ["An", "Tem\u00b7peln", ",", "wel\u00b7che", "sehr", "der", "G\u00f6z\u00b7zen", "Werth", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Man baute H\u00e4user auf, worin die G\u00f6tter wohnen", "tokens": ["Man", "bau\u00b7te", "H\u00e4u\u00b7ser", "auf", ",", "wo\u00b7rin", "die", "G\u00f6t\u00b7ter", "woh\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "NN", "PTKVZ", "$,", "PWAV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Aus pr\u00e4chtgen Marmorstein, die gleich den K\u00f6nigs", "tokens": ["Aus", "pr\u00e4cht\u00b7gen", "Mar\u00b7mor\u00b7stein", ",", "die", "gleich", "den", "K\u00f6\u00b7nigs"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Ein Schimmerreiches Gold verdekte Maur und Wand,", "tokens": ["Ein", "Schim\u00b7mer\u00b7rei\u00b7ches", "Gold", "ver\u00b7dek\u00b7te", "Maur", "und", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Das innre Heiligthum ward k\u00f6stlich \u00fcberspannt", "tokens": ["Das", "inn\u00b7re", "Hei\u00b7lig\u00b7thum", "ward", "k\u00f6st\u00b7lich", "\u00fc\u00b7bers\u00b7pannt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Mit Dekken die gestikt mit Perlen, Edelsteinen", "tokens": ["Mit", "Dek\u00b7ken", "die", "ges\u00b7tikt", "mit", "Per\u00b7len", ",", "E\u00b7del\u00b7stei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "ART", "VVPP", "APPR", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wer solche Tempel sah, der muste Wunder meinen.", "tokens": ["Wer", "sol\u00b7che", "Tem\u00b7pel", "sah", ",", "der", "mus\u00b7te", "Wun\u00b7der", "mei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Was vor ein grosser GOtt im Heiligsten verdekt,", "tokens": ["Was", "vor", "ein", "gros\u00b7ser", "Gott", "im", "Hei\u00b7ligs\u00b7ten", "ver\u00b7dekt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Und in der Dunkelheit die Ehrfurcht bringt, verstekt:", "tokens": ["Und", "in", "der", "Dun\u00b7kel\u00b7heit", "die", "Ehr\u00b7furcht", "bringt", ",", "vers\u00b7tekt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Allein wer sich nur wagt, ins Heiligste zu gehen", "tokens": ["Al\u00b7lein", "wer", "sich", "nur", "wagt", ",", "ins", "Hei\u00b7ligs\u00b7te", "zu", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PWS", "PRF", "ADV", "VVFIN", "$,", "APPRART", "NN", "PTKZU", "VVINF"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.24": {"text": "Der wird nur Corcodil und Affen drinnen sehen;", "tokens": ["Der", "wird", "nur", "Cor\u00b7co\u00b7dil", "und", "Af\u00b7fen", "drin\u00b7nen", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "NN", "KON", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die in der Sacristei in finstren Loch verwahrt.", "tokens": ["Die", "in", "der", "Sa\u00b7cris\u00b7tei", "in", "finst\u00b7ren", "Loch", "ver\u00b7wahrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Es fiel mir dabei ein: So ist der Heuchler Art,", "tokens": ["Es", "fiel", "mir", "da\u00b7bei", "ein", ":", "So", "ist", "der", "Heuch\u00b7ler", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PAV", "PTKVZ", "$.", "ADV", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Jhr Aussenwerk ist sch\u00f6n, und ihr scheinheiligs We-", "tokens": ["Ihr", "Aus\u00b7sen\u00b7werk", "ist", "sch\u00f6n", ",", "und", "ihr", "schein\u00b7hei\u00b7ligs", "We"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "KON", "PPOSAT", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Giebt uns was sonderbahr und herrliches zu lesen:", "tokens": ["Giebt", "uns", "was", "son\u00b7der\u00b7bahr", "und", "herr\u00b7li\u00b7ches", "zu", "le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "ADJD", "KON", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Man meinet, da\u00df sie recht des Geistes Tempel seyn:", "tokens": ["Man", "mei\u00b7net", ",", "da\u00df", "sie", "recht", "des", "Geis\u00b7tes", "Tem\u00b7pel", "seyn", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und in der That ist es ein \u00fcberg\u00fcldter Schein.", "tokens": ["Und", "in", "der", "That", "ist", "es", "ein", "\u00fc\u00b7berg\u00b7\u00fcld\u00b7ter", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wenn man genau erforscht des Herzens wahre Triebe,", "tokens": ["Wenn", "man", "ge\u00b7nau", "er\u00b7forscht", "des", "Her\u00b7zens", "wah\u00b7re", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So ist ihr G\u00f6z ein Aff; ich mein die ", "tokens": ["So", "ist", "ihr", "G\u00f6z", "ein", "Aff", ";", "ich", "mein", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ART", "NN", "$.", "PPER", "PPOSAT", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}