{"textgrid.poem.38649": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "5. Kopet", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Selten erfreut' ich mich lange des Eigners im zierlichen Schlosse.", "tokens": ["Sel\u00b7ten", "er\u00b7freut'", "ich", "mich", "lan\u00b7ge", "des", "Eig\u00b7ners", "im", "zier\u00b7li\u00b7chen", "Schlos\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Mancher Geweihte Merkurs wurde durch mich zum Baron.", "tokens": ["Man\u00b7cher", "Ge\u00b7weih\u00b7te", "Mer\u00b7kurs", "wur\u00b7de", "durch", "mich", "zum", "Ba\u00b7ron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NE", "VAFIN", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Jetzt bin ich Neckers: doch der hat Frankreichs Gebrechen zu heilen,", "tokens": ["Jetzt", "bin", "ich", "Ne\u00b7ckers", ":", "doch", "der", "hat", "Fran\u00b7kreichs", "Ge\u00b7bre\u00b7chen", "zu", "hei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$.", "ADV", "ART", "VAFIN", "NE", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Darum bescheid' ich mich gern, wenn er die meinen vergi\u00dft.", "tokens": ["Da\u00b7rum", "be\u00b7scheid'", "ich", "mich", "gern", ",", "wenn", "er", "die", "mei\u00b7nen", "ver\u00b7gi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ADV", "$,", "KOUS", "PPER", "ART", "PPOSAT", "VVFIN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}}}}}