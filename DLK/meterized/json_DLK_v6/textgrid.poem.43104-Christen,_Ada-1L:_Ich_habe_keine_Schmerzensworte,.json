{"textgrid.poem.43104": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich habe keine Schmerzensworte,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe keine Schmerzensworte,", "tokens": ["Ich", "ha\u00b7be", "kei\u00b7ne", "Schmer\u00b7zens\u00b7wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hab' keine Thr\u00e4nen, k\u00fchlend lind,", "tokens": ["Hab'", "kei\u00b7ne", "Thr\u00e4\u00b7nen", ",", "k\u00fch\u00b7lend", "lind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hab' nicht Gebete, stille fromme \u2013", "tokens": ["Hab'", "nicht", "Ge\u00b7be\u00b7te", ",", "stil\u00b7le", "from\u00b7me", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "NN", "$,", "ADJA", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sterbend liegt vor mir mein Kind!", "tokens": ["Ster\u00b7bend", "liegt", "vor", "mir", "mein", "Kind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Es pre\u00dft mir Kopf und Herz zusammen,", "tokens": ["Es", "pre\u00dft", "mir", "Kopf", "und", "Herz", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Luft, sie flimmert blutig roth \u2013", "tokens": ["Die", "Luft", ",", "sie", "flim\u00b7mert", "blu\u00b7tig", "roth", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Stirb nicht! Mit dir stirbt Alles, Alles \u2013", "tokens": ["Stirb", "nicht", "!", "Mit", "dir", "stirbt", "Al\u00b7les", ",", "Al\u00b7les", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "$.", "APPR", "PPER", "VVFIN", "PIS", "$,", "PIS", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein letzter Halt w\u00e4r' mit dir todt! \u2013 \u2013", "tokens": ["Mein", "letz\u00b7ter", "Halt", "w\u00e4r'", "mit", "dir", "todt", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "APPR", "PPER", "ADJD", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ist todt! \u2013 Ein leiser, kurzer Schrei \u2013", "tokens": ["Ist", "todt", "!", "\u2013", "Ein", "lei\u00b7ser", ",", "kur\u00b7zer", "Schrei", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$.", "$(", "ART", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das K\u00f6pfchen sinkt, das bleiche,", "tokens": ["Das", "K\u00f6pf\u00b7chen", "sinkt", ",", "das", "blei\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und an die schmerzerstarrte Brust", "tokens": ["Und", "an", "die", "schmer\u00b7zer\u00b7starr\u00b7te", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dr\u00fcck' ich die kleine Leiche.", "tokens": ["Dr\u00fcck", "ich", "die", "klei\u00b7ne", "Lei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}