{"textgrid.poem.31393": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es f\u00fchrt ein Wunder thun der Herrscher aller Welt:", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es f\u00fchrt ein Wunder thun der Herrscher aller Welt:", "tokens": ["Es", "f\u00fchrt", "ein", "Wun\u00b7der", "thun", "der", "Herr\u00b7scher", "al\u00b7ler", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wen Er erheben will / der mu\u00df die Knie vor biegen.", "tokens": ["wen", "Er", "er\u00b7he\u00b7ben", "will", "/", "der", "mu\u00df", "die", "Knie", "vor", "bie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "VMFIN", "$(", "ART", "VMFIN", "ART", "NN", "APPR", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "der mu\u00df onm\u00e4chtig seyn / der neue Krafft soll kriegen.", "tokens": ["der", "mu\u00df", "on\u00b7m\u00e4ch\u00b7tig", "seyn", "/", "der", "neu\u00b7e", "Krafft", "soll", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADJD", "VAINF", "$(", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "wer ganz nichts von sich selbst / von dem er etwas / h\u00e4lt.", "tokens": ["wer", "ganz", "nichts", "von", "sich", "selbst", "/", "von", "dem", "er", "et\u00b7was", "/", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ADV", "PIS", "APPR", "PRF", "ADV", "$(", "APPR", "PRELS", "PPER", "ADV", "$(", "VVFIN", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "Die Glut / w\u00fcrkt nach dem Ding / das ihr ist vorgestellt /", "tokens": ["Die", "Glut", "/", "w\u00fcrkt", "nach", "dem", "Ding", "/", "das", "ihr", "ist", "vor\u00b7ge\u00b7stellt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VVFIN", "APPR", "ART", "NN", "$(", "PRELS", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "nach de\u00dfen Art sich pflegt der Brunst gestalt zuf\u00fcgen.", "tokens": ["nach", "de\u00b7\u00dfen", "Art", "sich", "pflegt", "der", "Brunst", "ge\u00b7stalt", "zu\u00b7f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVFIN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Gott stutzt die Fl\u00fcgel erst / eh Er uns l\u00e4st auffliegen.", "tokens": ["Gott", "stutzt", "die", "Fl\u00fc\u00b7gel", "erst", "/", "eh", "Er", "uns", "l\u00e4st", "auf\u00b7flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "$(", "KOUS", "PPER", "PRF", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Leib-eigen mu\u00df man seyn / so herrscht man wie gemeldt.", "tokens": ["Leib\u00b7ei\u00b7gen", "mu\u00df", "man", "seyn", "/", "so", "herrscht", "man", "wie", "ge\u00b7meldt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "VAINF", "$(", "ADV", "VVFIN", "PIS", "KOKOM", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "O unersinnter Sinn! wer kan dich doch begreiffen?", "tokens": ["O", "un\u00b7er\u00b7sinn\u00b7ter", "Sinn", "!", "wer", "kan", "dich", "doch", "be\u00b7greif\u00b7fen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "du bist ja der Vernunfft ein unerzieltes Ziel /", "tokens": ["du", "bist", "ja", "der", "Ver\u00b7nunfft", "ein", "un\u00b7er\u00b7ziel\u00b7tes", "Ziel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "die man in diesem Meer der Wei\u00dfheit mu\u00df ers\u00e4uffen.", "tokens": ["die", "man", "in", "die\u00b7sem", "Meer", "der", "Wei\u00df\u00b7heit", "mu\u00df", "er\u00b7s\u00e4uf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "PDAT", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Vrsach-ursach ist / dein hoher Lebens Will /", "tokens": ["Die", "Vr\u00b7sach\u00b7ur\u00b7sach", "ist", "/", "dein", "ho\u00b7her", "Le\u00b7bens", "Will", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$(", "PPOSAT", "ADJA", "NN", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "da\u00df s\u00fc\u00df-und sch\u00f6ne Fr\u00fccht' im Allmachts Herbste reiffen.", "tokens": ["da\u00df", "s\u00fc\u00df\u00b7\u00b7und", "sch\u00f6\u00b7ne", "Fr\u00fccht'", "im", "All\u00b7machts", "Herbs\u00b7te", "reif\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "wer Gott gela\u00dfen ist / mit dem hat Er sein Spiel.", "tokens": ["wer", "Gott", "ge\u00b7la\u00b7\u00dfen", "ist", "/", "mit", "dem", "hat", "Er", "sein", "Spiel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVPP", "VAFIN", "$(", "APPR", "ART", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+--++-+", "measure": "iambic.hexa.relaxed"}}}}}