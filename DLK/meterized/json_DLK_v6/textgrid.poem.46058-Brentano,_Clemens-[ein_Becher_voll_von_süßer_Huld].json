{"textgrid.poem.46058": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[ein Becher voll von s\u00fc\u00dfer Huld]", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Becher voll von s\u00fc\u00dfer Huld", "tokens": ["Ein", "Be\u00b7cher", "voll", "von", "s\u00fc\u00b7\u00dfer", "Huld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und eine gl\u00fchnde Ungeduld", "tokens": ["Und", "ei\u00b7ne", "gl\u00fchn\u00b7de", "Un\u00b7ge\u00b7duld"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und eine arme trunkne Schuld", "tokens": ["Und", "ei\u00b7ne", "ar\u00b7me", "trunk\u00b7ne", "Schuld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie lehren mich zu flehen!", "tokens": ["Sie", "leh\u00b7ren", "mich", "zu", "fle\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du Becher voll von s\u00fc\u00dfer Huld", "tokens": ["Du", "Be\u00b7cher", "voll", "von", "s\u00fc\u00b7\u00dfer", "Huld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vergieb der gl\u00fchnden Ungeduld", "tokens": ["Ver\u00b7gieb", "der", "gl\u00fchn\u00b7den", "Un\u00b7ge\u00b7duld"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vergieb die arme trunkne Schuld,", "tokens": ["Ver\u00b7gieb", "die", "ar\u00b7me", "trunk\u00b7ne", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die ins Gericht will gehen.", "tokens": ["Die", "ins", "Ge\u00b7richt", "will", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Den Becher voll von s\u00fc\u00dfer Huld", "tokens": ["Den", "Be\u00b7cher", "voll", "von", "s\u00fc\u00b7\u00dfer", "Huld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Darf heut die gl\u00fchnde Ungeduld", "tokens": ["Darf", "heut", "die", "gl\u00fchn\u00b7de", "Un\u00b7ge\u00b7duld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zur Bu\u00dfe armer trunkner Schuld", "tokens": ["Zur", "Bu\u00b7\u00dfe", "ar\u00b7mer", "trunk\u00b7ner", "Schuld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht sehn, und m\u00f6cht' vergehen!", "tokens": ["Nicht", "sehn", ",", "und", "m\u00f6cht'", "ver\u00b7ge\u00b7hen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,", "KON", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das freut den Becher s\u00fc\u00dfer Huld", "tokens": ["Das", "freut", "den", "Be\u00b7cher", "s\u00fc\u00b7\u00dfer", "Huld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das schmerzt die gl\u00fchnde Ungeduld", "tokens": ["Das", "schmerzt", "die", "gl\u00fchn\u00b7de", "Un\u00b7ge\u00b7duld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das straft die arme trunkne Schuld", "tokens": ["Das", "straft", "die", "ar\u00b7me", "trunk\u00b7ne", "Schuld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit bittern, bittern Wehen.", "tokens": ["Mit", "bit\u00b7tern", ",", "bit\u00b7tern", "We\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O Becher voll von s\u00fc\u00dfer Huld,", "tokens": ["O", "Be\u00b7cher", "voll", "von", "s\u00fc\u00b7\u00dfer", "Huld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Woll' nicht die gl\u00fchnde Ungeduld,", "tokens": ["Woll'", "nicht", "die", "gl\u00fchn\u00b7de", "Un\u00b7ge\u00b7duld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Ob ihrer armen trunknen Schuld,", "tokens": ["Ob", "ih\u00b7rer", "ar\u00b7men", "trunk\u00b7nen", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die heute b\u00fc\u00dft, verschm\u00e4hen.", "tokens": ["Die", "heu\u00b7te", "b\u00fc\u00dft", ",", "ver\u00b7schm\u00e4\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Flie\u00df \u00fcber Becher s\u00fc\u00dfer Huld,", "tokens": ["Flie\u00df", "\u00fc\u00b7ber", "Be\u00b7cher", "s\u00fc\u00b7\u00dfer", "Huld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Werd' Asche gl\u00fchnde Ungeduld,", "tokens": ["Werd'", "A\u00b7sche", "gl\u00fchn\u00b7de", "Un\u00b7ge\u00b7duld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die mag die arme trunkne Schuld", "tokens": ["Die", "mag", "die", "ar\u00b7me", "trunk\u00b7ne", "Schuld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gemischt mit Tr\u00e4nen s\u00e4en.", "tokens": ["Ge\u00b7mischt", "mit", "Tr\u00e4\u00b7nen", "s\u00e4\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Auf da\u00df du Becher s\u00fc\u00dfer Huld", "tokens": ["Auf", "da\u00df", "du", "Be\u00b7cher", "s\u00fc\u00b7\u00dfer", "Huld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um dich in Schmerzen der Geduld,", "tokens": ["Um", "dich", "in", "Schmer\u00b7zen", "der", "Ge\u00b7duld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Still auf dem Grab der armen Schuld", "tokens": ["Still", "auf", "dem", "Grab", "der", "ar\u00b7men", "Schuld"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Lilie kann erstehen.", "tokens": ["Die", "Li\u00b7lie", "kann", "er\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die Lilie, die voll s\u00fc\u00dfer Huld,", "tokens": ["Die", "Li\u00b7lie", ",", "die", "voll", "s\u00fc\u00b7\u00dfer", "Huld", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du sahst im Garten der Geduld", "tokens": ["Du", "sahst", "im", "Gar\u00b7ten", "der", "Ge\u00b7duld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Stern und Engel ohne Schuld", "tokens": ["Mit", "Stern", "und", "En\u00b7gel", "oh\u00b7ne", "Schuld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du leuchten hast gesehen.", "tokens": ["Du", "leuch\u00b7ten", "hast", "ge\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}