{"textgrid.poem.40101": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ja, Dir wird's ", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, Dir wird's ", "tokens": ["Ja", ",", "Dir", "wird's"], "token_info": ["word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VAFIN"], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "dein Auge bricht, als ob du weinst,", "tokens": ["dein", "Au\u00b7ge", "bricht", ",", "als", "ob", "du", "weinst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und warst doch blos ein Kind der Gassen!", "tokens": ["und", "warst", "doch", "blos", "ein", "Kind", "der", "Gas\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ja, damals ahnt' ich nicht, da\u00df einst", "tokens": ["Ja", ",", "da\u00b7mals", "ahnt'", "ich", "nicht", ",", "da\u00df", "einst"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "als letzter Freund ein Hund mir bliebe:", "tokens": ["als", "letz\u00b7ter", "Freund", "ein", "Hund", "mir", "blie\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "da sucht' ich noch bei Menschen Liebe.", "tokens": ["da", "sucht'", "ich", "noch", "bei", "Men\u00b7schen", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein Hund, in deine treuen Augen", "tokens": ["Mein", "Hund", ",", "in", "dei\u00b7ne", "treu\u00b7en", "Au\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "hab' manche Frage ich versenkt,", "tokens": ["hab'", "man\u00b7che", "Fra\u00b7ge", "ich", "ver\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "f\u00fcr die nicht Menschenblicke taugen,", "tokens": ["f\u00fcr", "die", "nicht", "Men\u00b7schen\u00b7bli\u00b7cke", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PTKNEG", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "wo man ein Tier braucht, das ", "tokens": ["wo", "man", "ein", "Tier", "braucht", ",", "das"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "PIS", "ART", "NN", "VVFIN", "$,", "PRELS"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "die Ohnmacht auch in Ihm zu sehen,", "tokens": ["die", "Ohn\u00b7macht", "auch", "in", "Ihm", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "mit der wir selbst durchs Leben gehen.", "tokens": ["mit", "der", "wir", "selbst", "durchs", "Le\u00b7ben", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du hast mir nie ein Leid bereitet:", "tokens": ["Du", "hast", "mir", "nie", "ein", "Leid", "be\u00b7rei\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nun liegt dein Leib vom Tod gebreitet,", "tokens": ["Nun", "liegt", "dein", "Leib", "vom", "Tod", "ge\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "verl\u00f6scht dein tr\u00f6stend Augenlicht ...", "tokens": ["ver\u00b7l\u00f6scht", "dein", "tr\u00f6s\u00b7tend", "Au\u00b7gen\u00b7licht", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJD", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was will mir denn wie ", "tokens": ["Was", "will", "mir", "denn", "wie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "KOKOM"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "mein Hund, mein Freund: ich kann noch weinen!", "tokens": ["mein", "Hund", ",", "mein", "Freund", ":", "ich", "kann", "noch", "wei\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$.", "PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}