{"textgrid.poem.33716": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "7. Die gro\u00dfen St\u00e4mme", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mongole, deine Herden sollst du grasen", "tokens": ["Mon\u00b7go\u00b7le", ",", "dei\u00b7ne", "Her\u00b7den", "sollst", "du", "gra\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Norden, wo der Steppe Nebel grauen,", "tokens": ["Im", "Nor\u00b7den", ",", "wo", "der", "Step\u00b7pe", "Ne\u00b7bel", "grau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "NN", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu Rosse sollst du sein und Zelte bauen", "tokens": ["Zu", "Ros\u00b7se", "sollst", "du", "sein", "und", "Zel\u00b7te", "bau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "PPER", "VAINF", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und oft wie Sturm durch alle V\u00f6lker blasen.", "tokens": ["Und", "oft", "wie", "Sturm", "durch", "al\u00b7le", "V\u00f6l\u00b7ker", "bla\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zum h\u00f6chsten Leben gab ich euch, Kaukasen,", "tokens": ["Zum", "h\u00f6chs\u00b7ten", "Le\u00b7ben", "gab", "ich", "euch", ",", "Kau\u00b7ka\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "PPER", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein buchtenreiches Meer und weite Gauen.", "tokens": ["Ein", "buch\u00b7ten\u00b7rei\u00b7ches", "Meer", "und", "wei\u00b7te", "Gau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit Kunst und Mut und k\u00fchnem Weltvertrauen", "tokens": ["Mit", "Kunst", "und", "Mut", "und", "k\u00fch\u00b7nem", "Welt\u00b7ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Trotzt ihr der Sturmesflamme wildem Rasen.", "tokens": ["Trotzt", "ihr", "der", "Stur\u00b7mes\u00b7flam\u00b7me", "wil\u00b7dem", "Ra\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dir, Rothaut, geb' ich Urwald und Savanne,", "tokens": ["Dir", ",", "Rot\u00b7haut", ",", "geb'", "ich", "Ur\u00b7wald", "und", "Sa\u00b7van\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Such deine Pfade l\u00e4ngs den gro\u00dfen Fl\u00fcssen,", "tokens": ["Such", "dei\u00b7ne", "Pfa\u00b7de", "l\u00e4ngs", "den", "gro\u00b7\u00dfen", "Fl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durchschwimm die Seen und deinen Bogen spanne.", "tokens": ["Durch\u00b7schwimm", "die", "Seen", "und", "dei\u00b7nen", "Bo\u00b7gen", "span\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Den Sand wirst du mit hei\u00dfer Sohle k\u00fcssen,", "tokens": ["Den", "Sand", "wirst", "du", "mit", "hei\u00b7\u00dfer", "Soh\u00b7le", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Schwarzer, knieend vor dem wei\u00dfen Manne,", "tokens": ["O", "Schwar\u00b7zer", ",", "kni\u00b7e\u00b7end", "vor", "dem", "wei\u00b7\u00dfen", "Man\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch einst wird auch dein Elend enden m\u00fcssen.", "tokens": ["Doch", "einst", "wird", "auch", "dein", "E\u00b7lend", "en\u00b7den", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "PPOSAT", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}