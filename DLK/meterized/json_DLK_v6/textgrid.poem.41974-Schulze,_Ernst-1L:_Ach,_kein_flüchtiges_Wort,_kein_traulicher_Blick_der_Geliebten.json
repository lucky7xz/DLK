{"textgrid.poem.41974": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach, kein fl\u00fcchtiges Wort, kein traulicher Blick der Geliebten", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, kein fl\u00fcchtiges Wort, kein traulicher Blick der Geliebten", "tokens": ["Ach", ",", "kein", "fl\u00fcch\u00b7ti\u00b7ges", "Wort", ",", "kein", "trau\u00b7li\u00b7cher", "Blick", "der", "Ge\u00b7lieb\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PIAT", "ADJA", "NN", "$,", "PIAT", "ADJA", "NN", "ART", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Ist mir um ewiges Gl\u00fcck feil und um ewigen Ruhm!", "tokens": ["Ist", "mir", "um", "e\u00b7wi\u00b7ges", "Gl\u00fcck", "feil", "und", "um", "e\u00b7wi\u00b7gen", "Ruhm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "KON", "APPR", "ADJA", "NN", "$."], "meter": "---+--++--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Gl\u00fcck, was ist es? Ein lockendes Spiel mit betr\u00fcgenden G\u00f6ttern;", "tokens": ["Gl\u00fcck", ",", "was", "ist", "es", "?", "Ein", "lo\u00b7cken\u00b7des", "Spiel", "mit", "be\u00b7tr\u00fc\u00b7gen\u00b7den", "G\u00f6t\u00b7tern", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VAFIN", "PPER", "$.", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Wenn du gewannst, so macht \u00e4rmer dich oft der Gewinn.", "tokens": ["Wenn", "du", "ge\u00b7wannst", ",", "so", "macht", "\u00e4r\u00b7mer", "dich", "oft", "der", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "ADV", "VVFIN", "ADJD", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ruhm? Ein unendlicher Kampf mit der Welt und dem eigenen Herzen;", "tokens": ["Ruhm", "?", "Ein", "un\u00b7end\u00b7li\u00b7cher", "Kampf", "mit", "der", "Welt", "und", "dem", "ei\u00b7ge\u00b7nen", "Her\u00b7zen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ADJA", "NN", "APPR", "ART", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Flucht dir jene, so lohnt selten das andere dich.", "tokens": ["Flucht", "dir", "je\u00b7ne", ",", "so", "lohnt", "sel\u00b7ten", "das", "an\u00b7de\u00b7re", "dich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PDS", "$,", "ADV", "VVFIN", "ADJD", "ART", "ADJA", "PPER", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Nur in den Armen der Liebe zu ruhn, nicht sorgend um Zukunft,", "tokens": ["Nur", "in", "den", "Ar\u00b7men", "der", "Lie\u00b7be", "zu", "ruhn", ",", "nicht", "sor\u00b7gend", "um", "Zu\u00b7kunft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,", "PTKNEG", "ADJD", "APPR", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Auf dem Gefilde des Jetzt pfl\u00fcckend die Bl\u00fcthe der Lust,", "tokens": ["Auf", "dem", "Ge\u00b7fil\u00b7de", "des", "Jetzt", "pfl\u00fc\u00b7ckend", "die", "Bl\u00fc\u00b7the", "der", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADV", "ADJD", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "N\u00e4hrend im Herzen das reizende Bild der entschwundenen Freude,", "tokens": ["N\u00e4h\u00b7rend", "im", "Her\u00b7zen", "das", "rei\u00b7zen\u00b7de", "Bild", "der", "ent\u00b7schwun\u00b7de\u00b7nen", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Todt f\u00fcr die Pfeile der Qual, die dir Erinnerung schickt,", "tokens": ["Todt", "f\u00fcr", "die", "Pfei\u00b7le", "der", "Qual", ",", "die", "dir", "E\u00b7rin\u00b7ne\u00b7rung", "schickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Das nur hebt zu den G\u00f6ttern dich auf; dem friedlichen Eiland", "tokens": ["Das", "nur", "hebt", "zu", "den", "G\u00f6t\u00b7tern", "dich", "auf", ";", "dem", "fried\u00b7li\u00b7chen", "Ei\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ADV", "VVFIN", "APPR", "ART", "NN", "PPER", "PTKVZ", "$.", "ART", "ADJA", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.12": {"text": "Gleicht dein Leben, umrauscht rings von dem st\u00fcrmischen Meer.", "tokens": ["Gleicht", "dein", "Le\u00b7ben", ",", "um\u00b7rauscht", "rings", "von", "dem", "st\u00fcr\u00b7mi\u00b7schen", "Meer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}}}}}