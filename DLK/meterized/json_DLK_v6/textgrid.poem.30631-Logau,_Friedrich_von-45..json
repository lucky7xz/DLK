{"textgrid.poem.30631": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "45.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wil der mein Buch nicht lieben,", "tokens": ["Wil", "der", "mein", "Buch", "nicht", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Der be\u00dfres hat geschrieben,", "tokens": ["Der", "be\u00df\u00b7res", "hat", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wil der mein Buch vernichten,", "tokens": ["Wil", "der", "mein", "Buch", "ver\u00b7nich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Der mehres kunte tichten,", "tokens": ["Der", "meh\u00b7res", "kun\u00b7te", "tich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So la\u00df ichs so geschehen.", "tokens": ["So", "la\u00df", "ichs", "so", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Doch wird man auch wol sehen,", "tokens": ["Doch", "wird", "man", "auch", "wol", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df mancher etwas \u00e4rgers", "tokens": ["Da\u00df", "man\u00b7cher", "et\u00b7was", "\u00e4r\u00b7gers"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Geschrieben, mancher k\u00e4rgers.", "tokens": ["Ge\u00b7schrie\u00b7ben", ",", "man\u00b7cher", "k\u00e4r\u00b7gers", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}