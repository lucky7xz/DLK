{"textgrid.poem.55145": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Magisches Netz", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sind es K\u00e4mpfe, die ich sehe?", "tokens": ["Sind", "es", "K\u00e4mp\u00b7fe", ",", "die", "ich", "se\u00b7he", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind es Spiele? sind es Wunder?", "tokens": ["Sind", "es", "Spie\u00b7le", "?", "sind", "es", "Wun\u00b7der", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$.", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcnf der allerliebsten Knaben", "tokens": ["F\u00fcnf", "der", "al\u00b7ler\u00b7liebs\u00b7ten", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gegen f\u00fcnf Geschwister streitend,", "tokens": ["Ge\u00b7gen", "f\u00fcnf", "Ge\u00b7schwis\u00b7ter", "strei\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Regelm\u00e4\u00dfig, taktbest\u00e4ndig,", "tokens": ["Re\u00b7gel\u00b7m\u00e4\u00b7\u00dfig", ",", "takt\u00b7be\u00b7st\u00e4n\u00b7dig", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Einer Zaubrin zu Gebote.", "tokens": ["Ei\u00b7ner", "Zaub\u00b7rin", "zu", "Ge\u00b7bo\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Blanke Spie\u00dfe f\u00fchren jene,", "tokens": ["Blan\u00b7ke", "Spie\u00b7\u00dfe", "f\u00fch\u00b7ren", "je\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PDS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diese flechten schnelle F\u00e4den,", "tokens": ["Die\u00b7se", "flech\u00b7ten", "schnel\u00b7le", "F\u00e4\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df man glaubt, in ihren Schlingen", "tokens": ["Da\u00df", "man", "glaubt", ",", "in", "ih\u00b7ren", "Schlin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werde sich das Eisen fangen.", "tokens": ["Wer\u00b7de", "sich", "das", "Ei\u00b7sen", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bald gefangen sind die Spie\u00dfe;", "tokens": ["Bald", "ge\u00b7fan\u00b7gen", "sind", "die", "Spie\u00b7\u00dfe", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch im leichten Kriegestanze", "tokens": ["Doch", "im", "leich\u00b7ten", "Krie\u00b7ge\u00b7stan\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Stiehlt sich einer nach dem andern", "tokens": ["Stiehlt", "sich", "ei\u00b7ner", "nach", "dem", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aus der zarten Schleifenreihe,", "tokens": ["Aus", "der", "zar\u00b7ten", "Schlei\u00b7fen\u00b7rei\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Die sogleich den freien haschet,", "tokens": ["Die", "sog\u00b7leich", "den", "frei\u00b7en", "ha\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wenn sie den gebundnen l\u00f6set.", "tokens": ["Wenn", "sie", "den", "ge\u00b7bund\u00b7nen", "l\u00f6\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "So mit Ringen, Streiten, Siegen,", "tokens": ["So", "mit", "Rin\u00b7gen", ",", "Strei\u00b7ten", ",", "Sie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wechselflucht und Wiederkehren", "tokens": ["Wech\u00b7sel\u00b7flucht", "und", "Wie\u00b7der\u00b7keh\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird ein k\u00fcnstlich Netz geflochten,", "tokens": ["Wird", "ein", "k\u00fcnst\u00b7lich", "Netz", "ge\u00b7floch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJD", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Himmelsflocken gleich an Wei\u00dfe,", "tokens": ["Him\u00b7mels\u00b7flo\u00b7cken", "gleich", "an", "Wei\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die vom Lichten in das Dichte", "tokens": ["Die", "vom", "Lich\u00b7ten", "in", "das", "Dich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Musterhafte Streifen ziehen,", "tokens": ["Mus\u00b7ter\u00b7haf\u00b7te", "Strei\u00b7fen", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie es Farben kaum verm\u00f6chten.", "tokens": ["Wie", "es", "Far\u00b7ben", "kaum", "ver\u00b7m\u00f6ch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wer empf\u00e4ngt nun der Gew\u00e4nder", "tokens": ["Wer", "emp\u00b7f\u00e4ngt", "nun", "der", "Ge\u00b7w\u00e4n\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Allerw\u00fcnschtes? Wen beg\u00fcnstigt", "tokens": ["Al\u00b7ler\u00b7w\u00fcnschtes", "?", "Wen", "be\u00b7g\u00fcns\u00b7tigt"], "token_info": ["word", "punct", "word", "word"], "pos": ["PIS", "$.", "PWS", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Unsre vielgeliebte Herrin", "tokens": ["Uns\u00b7re", "viel\u00b7ge\u00b7lieb\u00b7te", "Her\u00b7rin"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Als den anerkannten Diener?", "tokens": ["Als", "den", "an\u00b7er\u00b7kann\u00b7ten", "Die\u00b7ner", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mich begl\u00fcckt des holden Loses", "tokens": ["Mich", "be\u00b7gl\u00fcckt", "des", "hol\u00b7den", "Lo\u00b7ses"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Treu und still ersehntes Zeichen!", "tokens": ["Treu", "und", "still", "er\u00b7sehn\u00b7tes", "Zei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ich f\u00fchle mich umschlungen,", "tokens": ["Und", "ich", "f\u00fch\u00b7le", "mich", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihrer Dienerschaft gewidmet.", "tokens": ["Ih\u00b7rer", "Die\u00b7ner\u00b7schaft", "ge\u00b7wid\u00b7met", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch indem ich so behaglich,", "tokens": ["Doch", "in\u00b7dem", "ich", "so", "be\u00b7hag\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aufgeschm\u00fcckt stolzierend wandle,", "tokens": ["Auf\u00b7ge\u00b7schm\u00fcckt", "stol\u00b7zie\u00b7rend", "wand\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh, da kn\u00fcpfen jene Losen,", "tokens": ["Sieh", ",", "da", "kn\u00fcp\u00b7fen", "je\u00b7ne", "Lo\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne Streit, geheim gesch\u00e4ftig,", "tokens": ["Oh\u00b7ne", "Streit", ",", "ge\u00b7heim", "ge\u00b7sch\u00e4f\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Andre Netze, fein und feiner,", "tokens": ["And\u00b7re", "Net\u00b7ze", ",", "fein", "und", "fei\u00b7ner", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "D\u00e4mmrungsf\u00e4den, Mondenblicke,", "tokens": ["D\u00e4mm\u00b7rungs\u00b7f\u00e4\u00b7den", ",", "Mon\u00b7den\u00b7bli\u00b7cke", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nachtviolenduft verwebend.", "tokens": ["Nacht\u00b7vi\u00b7o\u00b7len\u00b7duft", "ver\u00b7we\u00b7bend", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Eh wir nur das Netz bemerken,", "tokens": ["Eh", "wir", "nur", "das", "Netz", "be\u00b7mer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist ein Gl\u00fccklicher gefangen,", "tokens": ["Ist", "ein", "Gl\u00fcck\u00b7li\u00b7cher", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Den wir andern, den wir alle,", "tokens": ["Den", "wir", "an\u00b7dern", ",", "den", "wir", "al\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIS", "$,", "PRELS", "PPER", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Segnend und beneidend, gr\u00fc\u00dfen.", "tokens": ["Seg\u00b7nend", "und", "be\u00b7nei\u00b7dend", ",", "gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "KON", "VVPP", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}