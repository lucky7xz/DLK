{"textgrid.poem.26470": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Vollgenusse meines Seins \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Vollgenusse meines Seins \u2013", "tokens": ["Im", "Voll\u00b7ge\u00b7nus\u00b7se", "mei\u00b7nes", "Seins", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O seliges Behagen! \u2013", "tokens": ["O", "se\u00b7li\u00b7ges", "Be\u00b7ha\u00b7gen", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "F\u00fchl' ich, Natur, mich werden Eins", "tokens": ["F\u00fchl'", "ich", ",", "Na\u00b7tur", ",", "mich", "wer\u00b7den", "Eins"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "NN", "$,", "PPER", "VAFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit dir in diesen Tagen.", "tokens": ["Mit", "dir", "in", "die\u00b7sen", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Vergessen hab' ich Noth und Harm", "tokens": ["Ver\u00b7ges\u00b7sen", "hab'", "ich", "Noth", "und", "Harm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und menschlich banges Sorgen,", "tokens": ["Und", "menschlich", "ban\u00b7ges", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Ich ruhe still in deinem Arm,", "tokens": ["Ich", "ru\u00b7he", "still", "in", "dei\u00b7nem", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geheiligt und geborgen.", "tokens": ["Ge\u00b7hei\u00b7ligt", "und", "ge\u00b7bor\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Verstummen will in meiner Brust", "tokens": ["Ver\u00b7stum\u00b7men", "will", "in", "mei\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Ringen und das Streben \u2013", "tokens": ["Das", "Rin\u00b7gen", "und", "das", "Stre\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und heiter werd' ich mir bewu\u00dft:", "tokens": ["Und", "hei\u00b7ter", "werd'", "ich", "mir", "be\u00b7wu\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich leb' nur, um zu leben;", "tokens": ["Ich", "leb'", "nur", ",", "um", "zu", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUI", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Zu leben wie das Blatt am Strauch", "tokens": ["Zu", "le\u00b7ben", "wie", "das", "Blatt", "am", "Strauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "KOKOM", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und nichts mir zu erwerben,", "tokens": ["Und", "nichts", "mir", "zu", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als einst im k\u00fchlen Abendhauch", "tokens": ["Als", "einst", "im", "k\u00fch\u00b7len", "A\u00b7bend\u00b7hauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein leichtes, sch\u00f6nes Sterben.", "tokens": ["Ein", "leich\u00b7tes", ",", "sch\u00f6\u00b7nes", "Ster\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}