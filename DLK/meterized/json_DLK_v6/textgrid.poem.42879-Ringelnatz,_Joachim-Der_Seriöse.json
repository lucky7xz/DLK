{"textgrid.poem.42879": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Der Seri\u00f6se", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo ich abends Wei\u00dfw\u00fcrste fresse,", "tokens": ["Wo", "ich", "a\u00b7bends", "Wei\u00df\u00b7w\u00fcrs\u00b7te", "fres\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "--++-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Da sitzt oft drei Tische weit", "tokens": ["Da", "sitzt", "oft", "drei", "Ti\u00b7sche", "weit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "CARD", "NN", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Vor mir ein Herr von Noblesse,", "tokens": ["Vor", "mir", "ein", "Herr", "von", "Nob\u00b7les\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sehr gro\u00df, sehr ernst und sehr breit.", "tokens": ["Sehr", "gro\u00df", ",", "sehr", "ernst", "und", "sehr", "breit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Sein Haar und Bart, seine Kleidung", "tokens": ["Sein", "Haar", "und", "Bart", ",", "sei\u00b7ne", "Klei\u00b7dung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sind einwandfrei und gepflegt,", "tokens": ["Sind", "ein\u00b7wand\u00b7frei", "und", "ge\u00b7pflegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wie er unter steter Vermeidung", "tokens": ["Wie", "er", "un\u00b7ter", "ste\u00b7ter", "Ver\u00b7mei\u00b7dung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Sich einwandfrei sicher bewegt.", "tokens": ["Sich", "ein\u00b7wand\u00b7frei", "si\u00b7cher", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Wie ihn die Kellner bedienen,", "tokens": ["Wie", "ihn", "die", "Kell\u00b7ner", "be\u00b7die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ist er ein F\u00fcrst oder reich.", "tokens": ["Ist", "er", "ein", "F\u00fcrst", "o\u00b7der", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "KON", "ADJD", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Doch bleibt das Spiel seiner Mienen", "tokens": ["Doch", "bleibt", "das", "Spiel", "sei\u00b7ner", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Jederzeit w\u00fcrdig und gleich.", "tokens": ["Je\u00b7der\u00b7zeit", "w\u00fcr\u00b7dig", "und", "gleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Wenn diese w\u00fcrdig seri\u00f6se", "tokens": ["Wenn", "die\u00b7se", "w\u00fcr\u00b7dig", "se\u00b7ri\u00b7\u00f6\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PDS", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erscheinung vor\u00fcbergeht,", "tokens": ["Er\u00b7schei\u00b7nung", "vor\u00b7\u00fc\u00b7ber\u00b7geht", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dann ist mir, als ob mein Gekr\u00f6se", "tokens": ["Dann", "ist", "mir", ",", "als", "ob", "mein", "Ge\u00b7kr\u00f6\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KOKOM", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Hirn und Leib sich verdreht.", "tokens": ["In", "Hirn", "und", "Leib", "sich", "ver\u00b7dreht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Denn wenn er mit seinen Blicken", "tokens": ["Denn", "wenn", "er", "mit", "sei\u00b7nen", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mich streifte \u2013 das f\u00fchle ich klar \u2013,", "tokens": ["Mich", "streif\u00b7te", "\u2013", "das", "f\u00fch\u00b7le", "ich", "klar", "\u2013", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$(", "PDS", "VVFIN", "PPER", "ADJD", "$(", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich w\u00fcrde zusammenknicken", "tokens": ["Ich", "w\u00fcr\u00b7de", "zu\u00b7sam\u00b7men\u00b7kni\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und nimmer sein, was ich war.", "tokens": ["Und", "nim\u00b7mer", "sein", ",", "was", "ich", "war", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAINF", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Doch ohne seitw\u00e4rts zu schauen,", "tokens": ["Doch", "oh\u00b7ne", "seit\u00b7w\u00e4rts", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schreitet er durchs Lokal.", "tokens": ["Schrei\u00b7tet", "er", "durchs", "Lo\u00b7kal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Seine gerunzelten Brauen \u2013", "tokens": ["Sei\u00b7ne", "ge\u00b7run\u00b7zel\u00b7ten", "Brau\u00b7en", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Wie alles an ihm \u2013 sind aus Stahl.", "tokens": ["Wie", "al\u00b7les", "an", "ihm", "\u2013", "sind", "aus", "Stahl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "PPER", "$(", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und seine Schritte lenken", "tokens": ["Und", "sei\u00b7ne", "Schrit\u00b7te", "len\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich dahin, wohin man nicht sieht.", "tokens": ["Sich", "da\u00b7hin", ",", "wo\u00b7hin", "man", "nicht", "sieht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PAV", "$,", "PWAV", "PIS", "PTKNEG", "VVFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich wage nicht auszudenken,", "tokens": ["Ich", "wa\u00b7ge", "nicht", "aus\u00b7zu\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Was er dort etwa vollzieht.", "tokens": ["Was", "er", "dort", "et\u00b7wa", "voll\u00b7zieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ach, ich bin klein, ich bin b\u00f6se.", "tokens": ["Ach", ",", "ich", "bin", "klein", ",", "ich", "bin", "b\u00f6\u00b7se", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ADJD", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mein Herz ist auch nicht ganz rein.", "tokens": ["Mein", "Herz", "ist", "auch", "nicht", "ganz", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ach d\u00fcrfte ich solche seri\u00f6se", "tokens": ["Ach", "d\u00fcrf\u00b7te", "ich", "sol\u00b7che", "se\u00b7ri\u00b7\u00f6\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "PIAT", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Pers\u00f6nlichkeit einmal sein!", "tokens": ["Per\u00b7s\u00f6n\u00b7lich\u00b7keit", "ein\u00b7mal", "sein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}