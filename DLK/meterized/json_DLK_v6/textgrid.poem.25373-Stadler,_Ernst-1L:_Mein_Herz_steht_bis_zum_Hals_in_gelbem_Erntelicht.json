{"textgrid.poem.25373": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Herz steht bis zum Hals in gelbem Erntelicht", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Herz steht bis zum Hals in gelbem Erntelicht", "tokens": ["Mein", "Herz", "steht", "bis", "zum", "Hals", "in", "gel\u00b7bem", "Ern\u00b7te\u00b7licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wie unter Sommerhimmeln schnittbereites Land.", "tokens": ["wie", "un\u00b7ter", "Som\u00b7mer\u00b7him\u00b7meln", "schnitt\u00b7be\u00b7rei\u00b7tes", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bald l\u00e4utet durch die Ebenen Sichelsang:", "tokens": ["Bald", "l\u00e4u\u00b7tet", "durch", "die", "E\u00b7be\u00b7nen", "Si\u00b7chel\u00b7sang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "mein Blut lauscht tief mit Gl\u00fcck ges\u00e4ttigt", "tokens": ["mein", "Blut", "lauscht", "tief", "mit", "Gl\u00fcck", "ge\u00b7s\u00e4t\u00b7tigt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "in den Mittagsbrand.", "tokens": ["in", "den", "Mit\u00b7tags\u00b7brand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Kornkammern meines Lebens, lang ver\u00f6det,", "tokens": ["Korn\u00b7kam\u00b7mern", "mei\u00b7nes", "Le\u00b7bens", ",", "lang", "ver\u00b7\u00f6\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "alle eure Tore sollen nun wie Schleusenfl\u00fcgel offen stehn,", "tokens": ["al\u00b7le", "eu\u00b7re", "To\u00b7re", "sol\u00b7len", "nun", "wie", "Schleu\u00b7sen\u00b7fl\u00fc\u00b7gel", "of\u00b7fen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VMFIN", "ADV", "KOKOM", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "\u00dcber euern Grund wird wie Meer", "tokens": ["\u00dc\u00b7ber", "eu\u00b7ern", "Grund", "wird", "wie", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "KOKOM", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "die goldne Flut der Garben gehn.", "tokens": ["die", "gold\u00b7ne", "Flut", "der", "Gar\u00b7ben", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}