{"dta.poem.15004": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Schwerdtlied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1814", "urn": "urn:nbn:de:kobv:b4-200905193258", "language": ["de:0.99"], "booktitle": "K\u00f6rner, Theodor: Leyer und Schwerdt. Berlin, 1814."}, "poem": {"stanza.1": {"line.1": {"text": "Was soll dein heitres Blinken?", "tokens": ["Was", "soll", "dein", "heit\u00b7res", "Blin\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Schaust mich so freundlich an,", "tokens": ["Schaust", "mich", "so", "freund\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hab' meine Freude dran.", "tokens": ["Hab'", "mei\u00b7ne", "Freu\u00b7de", "dran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Hurrah! ", "tokens": ["Hur\u00b7rah", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "\u201emich tr\u00e4gt ein wackrer Reiter,", "tokens": ["\u201e", "mich", "tr\u00e4gt", "ein", "wack\u00b7rer", "Rei\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201edrum blink ich auch so heiter,", "tokens": ["\u201e", "drum", "blink", "ich", "auch", "so", "hei\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PAV", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201ebin freien Mannes Wehr,", "tokens": ["\u201e", "bin", "frei\u00b7en", "Man\u00b7nes", "Wehr", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "\u201edas freut dem Schwerdte sehr.", "tokens": ["\u201e", "das", "freut", "dem", "Schwerd\u00b7te", "sehr", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hurrah!", "tokens": ["Hur\u00b7rah", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Ja gutes Schwerdt frei bin ich,", "tokens": ["Ja", "gu\u00b7tes", "Schwerdt", "frei", "bin", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJA", "NN", "ADJD", "VAFIN", "PPER", "$,"], "meter": "-+-+-++", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Und liebe dich herzinnig,", "tokens": ["Und", "lie\u00b7be", "dich", "her\u00b7zin\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als w\u00e4rst du mir getraut,", "tokens": ["Als", "w\u00e4rst", "du", "mir", "ge\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Als eine liebe Braut.", "tokens": ["Als", "ei\u00b7ne", "lie\u00b7be", "Braut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hurrah!", "tokens": ["Hur\u00b7rah", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.4": {"line.1": {"text": "\u201edir hab' ich's ja ergeben,", "tokens": ["\u201e", "dir", "hab'", "ich's", "ja", "er\u00b7ge\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201emein lichtes Eisenleben,", "tokens": ["\u201e", "mein", "lich\u00b7tes", "Ei\u00b7sen\u00b7le\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201each w\u00e4ren wir getraut!", "tokens": ["\u201e", "ach", "w\u00e4\u00b7ren", "wir", "ge\u00b7traut", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "\u201ewann hohlst du deine Braut?", "tokens": ["\u201e", "wann", "hohlst", "du", "dei\u00b7ne", "Braut", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hurrah!", "tokens": ["Hur\u00b7rah", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.5": {"line.1": {"text": "Zur Brautnachts Morgenr\u00f6the,", "tokens": ["Zur", "Braut\u00b7nachts", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ruft festlich die Trompete,", "tokens": ["Ruft", "fest\u00b7lich", "die", "Trom\u00b7pe\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wenn die Kanonen schrei'n,", "tokens": ["Wenn", "die", "Ka\u00b7no\u00b7nen", "schrei'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Hohl ich das Liebchen ein.", "tokens": ["Hohl", "ich", "das", "Lieb\u00b7chen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hurrah!", "tokens": ["Hur\u00b7rah", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.6": {"line.1": {"text": "\u201eo seeliges Umfangen!", "tokens": ["\u201e", "o", "see\u00b7li\u00b7ges", "Um\u00b7fan\u00b7gen", "!"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201eich harre mit Verlangen.", "tokens": ["\u201e", "ich", "har\u00b7re", "mit", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201edu Br\u00e4ut'gam hohle mich,", "tokens": ["\u201e", "du", "Br\u00e4ut'\u00b7gam", "hoh\u00b7le", "mich", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "NE", "ADJA", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "\u201emein Kr\u00e4nzchen bleibt f\u00fcr dich,", "tokens": ["\u201e", "mein", "Kr\u00e4nz\u00b7chen", "bleibt", "f\u00fcr", "dich", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hurrah!", "tokens": ["Hur\u00b7rah", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.7": {"line.1": {"text": "Was klirrst du in der Scheide,", "tokens": ["Was", "klirrst", "du", "in", "der", "Schei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du helle Eisenfreude,", "tokens": ["Du", "hel\u00b7le", "Ei\u00b7sen\u00b7freu\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So wild, so schlachtenfroh?", "tokens": ["So", "wild", ",", "so", "schlach\u00b7ten\u00b7froh", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mein Schwerdt, was klingst du so?", "tokens": ["Mein", "Schwerdt", ",", "was", "klingst", "du", "so", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hurrah!", "tokens": ["Hur\u00b7rah", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.8": {"line.1": {"text": "\u201ewohl klirr ich in der Scheide,", "tokens": ["\u201e", "wohl", "klirr", "ich", "in", "der", "Schei\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201eich sehne mich zum Streite,", "tokens": ["\u201e", "ich", "seh\u00b7ne", "mich", "zum", "Strei\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201erecht wild und schlachtenfroh.", "tokens": ["\u201e", "recht", "wild", "und", "schlach\u00b7ten\u00b7froh", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "\u201edrum Reiter klirr ich so.", "tokens": ["\u201e", "drum", "Rei\u00b7ter", "klirr", "ich", "so", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PAV", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hurrah!", "tokens": ["Hur\u00b7rah", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.9": {"line.1": {"text": "Bleib doch im engen St\u00fcbchen.", "tokens": ["Bleib", "doch", "im", "en\u00b7gen", "St\u00fcb\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was willst du hier, mein Liebchen?", "tokens": ["Was", "willst", "du", "hier", ",", "mein", "Lieb\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bleib still im K\u00e4mmerlein,", "tokens": ["Bleib", "still", "im", "K\u00e4m\u00b7mer\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bleib, bald hohl' ich dich ein.", "tokens": ["Bleib", ",", "bald", "hohl'", "ich", "dich", "ein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hurrah!", "tokens": ["Hur\u00b7rah", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.10": {"line.1": {"text": "\u201ela\u00df mich nicht lange warten!", "tokens": ["\u201e", "la\u00df", "mich", "nicht", "lan\u00b7ge", "war\u00b7ten", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "\u201eo sch\u00f6ner Liebesgarten,", "tokens": ["\u201e", "o", "sch\u00f6\u00b7ner", "Lie\u00b7bes\u00b7gar\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201evoll R\u00f6slein blutigroth.", "tokens": ["\u201e", "voll", "R\u00f6s\u00b7lein", "blu\u00b7ti\u00b7groth", "."], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "\u201eund aufgebl\u00fchtem Tod.", "tokens": ["\u201e", "und", "auf\u00b7ge\u00b7bl\u00fch\u00b7tem", "Tod", "."], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hurrah!", "tokens": ["Hur\u00b7rah", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.11": {"line.1": {"text": "So komm denn aus der Scheide,", "tokens": ["So", "komm", "denn", "aus", "der", "Schei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du Reiters Augenweide,", "tokens": ["Du", "Rei\u00b7ters", "Au\u00b7gen\u00b7wei\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Heraus, mein Schwerdt, heraus!", "tokens": ["He\u00b7raus", ",", "mein", "Schwerdt", ",", "he\u00b7raus", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fchr' dich ins Vaterhaus.", "tokens": ["F\u00fchr'", "dich", "ins", "Va\u00b7ter\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hurrah!", "tokens": ["Hur\u00b7rah", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.12": {"line.1": {"text": "\u201each herrlich ists im Freien,", "tokens": ["\u201e", "ach", "herr\u00b7lich", "ists", "im", "Frei\u00b7en", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201eim r\u00fcst'gen Hochzeitreihen.", "tokens": ["\u201e", "im", "r\u00fcst'\u00b7gen", "Hoch\u00b7zei\u00b7trei\u00b7hen", "."], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201ewie gl\u00e4nzt im Sonnenstrahl", "tokens": ["\u201e", "wie", "gl\u00e4nzt", "im", "Son\u00b7nen\u00b7strahl"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "\u201eso br\u00e4utlich hell der Stahl!", "tokens": ["\u201e", "so", "br\u00e4ut\u00b7lich", "hell", "der", "Stahl", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hurrah!", "tokens": ["Hur\u00b7rah", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.13": {"line.1": {"text": "Wohlauf, ihr kecken Streiter,", "tokens": ["Wohl\u00b7auf", ",", "ihr", "ke\u00b7cken", "Strei\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohlauf, ihr deutschen Reiter!", "tokens": ["Wohl\u00b7auf", ",", "ihr", "deut\u00b7schen", "Rei\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wird euch das Herz nicht warm,", "tokens": ["Wird", "euch", "das", "Herz", "nicht", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nehmt's Liebchen in den Arm.", "tokens": ["Nehmt's", "Lieb\u00b7chen", "in", "den", "Arm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hurrah!", "tokens": ["Hur\u00b7rah", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.14": {"line.1": {"text": "Erst that es an der Linken,", "tokens": ["Erst", "that", "es", "an", "der", "Lin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "