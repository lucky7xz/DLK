{"textgrid.poem.32574": {"metadata": {"author": {"name": "Wieland, Christoph Martin", "birth": "N.A.", "death": "N.A."}, "title": "An Olympia", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im sch\u00f6nsten Haine", "tokens": ["Im", "sch\u00f6ns\u00b7ten", "Hai\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "von Amathunt", "tokens": ["von", "A\u00b7mat\u00b7hunt"], "token_info": ["word", "word"], "pos": ["APPR", "NE"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "sang j\u00fcngst, Olympia,", "tokens": ["sang", "j\u00fcngst", ",", "O\u00b7lym\u00b7pia", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "der Musen eine", "tokens": ["der", "Mu\u00b7sen", "ei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Dein Lob der Rose", "tokens": ["Dein", "Lob", "der", "Ro\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "den Grazien vor:", "tokens": ["den", "Gra\u00b7zi\u00b7en", "vor", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "ihr horcht der holde Chor", "tokens": ["ihr", "horcht", "der", "hol\u00b7de", "Chor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "mit s\u00fc\u00dfem Staunen,", "tokens": ["mit", "s\u00fc\u00b7\u00dfem", "Stau\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "und aus den Rosen", "tokens": ["und", "aus", "den", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "ragt das gespitzte Ohr", "tokens": ["ragt", "das", "ge\u00b7spitz\u00b7te", "Ohr"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.11": {"text": "der jungen Faunen", "tokens": ["der", "jun\u00b7gen", "Fau\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "entz\u00fcckt hervor:", "tokens": ["ent\u00b7z\u00fcckt", "her\u00b7vor", ":"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "und Musen, Grazien,", "tokens": ["und", "Mu\u00b7sen", ",", "Gra\u00b7zi\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "und Amoretten", "tokens": ["und", "A\u00b7mo\u00b7ret\u00b7ten"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "und Faunchen schlingen", "tokens": ["und", "Faun\u00b7chen", "schlin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "mit Rosenketten", "tokens": ["mit", "Ro\u00b7sen\u00b7ket\u00b7ten"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "sich in ein tanzend Rund,", "tokens": ["sich", "in", "ein", "tan\u00b7zend", "Rund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJD", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "und alle singen", "tokens": ["und", "al\u00b7le", "sin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "aus vollem Mund:", "tokens": ["aus", "vol\u00b7lem", "Mund", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.20": {"text": "\u00bbrosa, delicia", "tokens": ["\u00bb", "ro\u00b7sa", ",", "de\u00b7li\u00b7cia"], "token_info": ["punct", "word", "punct", "word"], "pos": ["$(", "NE", "$,", "NE"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.21": {"text": "degli Amori,", "tokens": ["de\u00b7gli", "A\u00b7mo\u00b7ri", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.22": {"text": "Rosa, bellissima", "tokens": ["Ro\u00b7sa", ",", "bel\u00b7lis\u00b7si\u00b7ma"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$,", "XY"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "de tutti i fiori!\u00ab", "tokens": ["de", "tut\u00b7ti", "i", "fi\u00b7o\u00b7ri", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["FM", "FM", "FM", "FM", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "und Alle kr\u00e4nzen", "tokens": ["und", "Al\u00b7le", "kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.25": {"text": "mit ewgen Rosen", "tokens": ["mit", "ew\u00b7gen", "Ro\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.26": {"text": "(die, dankerf\u00fcllt,", "tokens": ["(", "die", ",", "dan\u00b7ker\u00b7f\u00fcllt", ","], "token_info": ["punct", "word", "punct", "word", "punct"], "pos": ["$(", "ART", "$,", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.27": {"text": "noch sch\u00f6ner gl\u00e4nzen,", "tokens": ["noch", "sch\u00f6\u00b7ner", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.28": {"text": "noch s\u00fc\u00dfer d\u00fcften,)", "tokens": ["noch", "s\u00fc\u00b7\u00dfer", "d\u00fcf\u00b7ten", ",", ")"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,", "$("], "meter": "-+-+-", "measure": "iambic.di"}}}}}