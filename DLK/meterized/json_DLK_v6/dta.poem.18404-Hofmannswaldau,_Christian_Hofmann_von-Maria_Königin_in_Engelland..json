{"dta.poem.18404": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Maria K\u00f6nigin in Engelland.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als nacht und dunckelheit dein wesen noch bedeckt/", "tokens": ["Als", "nacht", "und", "dun\u00b7ckel\u00b7heit", "dein", "we\u00b7sen", "noch", "be\u00b7deckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPOSAT", "NN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "War deine tugend schon zu cronen auserkohren;", "tokens": ["War", "dei\u00b7ne", "tu\u00b7gend", "schon", "zu", "cro\u00b7nen", "au\u00b7ser\u00b7koh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie sehr dein vater sich durch eigne schuld befleckt/", "tokens": ["Wie", "sehr", "dein", "va\u00b7ter", "sich", "durch", "eig\u00b7ne", "schuld", "be\u00b7fleckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "PRF", "APPR", "ADJA", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So bleibt er doch ber\u00fcmt/ dieweil er dich gebohren.", "tokens": ["So", "bleibt", "er", "doch", "be\u00b7r\u00fcmt", "/", "die\u00b7weil", "er", "dich", "ge\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVPP", "$(", "KOUS", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}