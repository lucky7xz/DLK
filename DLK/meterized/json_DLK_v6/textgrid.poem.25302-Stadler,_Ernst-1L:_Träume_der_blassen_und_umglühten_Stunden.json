{"textgrid.poem.25302": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tr\u00e4ume der blassen und umgl\u00fchten Stunden", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tr\u00e4ume der blassen und umgl\u00fchten Stunden", "tokens": ["Tr\u00e4u\u00b7me", "der", "blas\u00b7sen", "und", "um\u00b7gl\u00fch\u00b7ten", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "KON", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "sinkt wieder ihr in lindem Abendwehn", "tokens": ["sinkt", "wie\u00b7der", "ihr", "in", "lin\u00b7dem", "Ab\u00b7end\u00b7wehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "aus goldgenetzter Wolken dunklem Scho\u00df", "tokens": ["aus", "gold\u00b7ge\u00b7netz\u00b7ter", "Wol\u00b7ken", "dunk\u00b7lem", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "wie Sommerregen duftend auf mein Land?", "tokens": ["wie", "Som\u00b7mer\u00b7re\u00b7gen", "duf\u00b7tend", "auf", "mein", "Land", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ihr locktet fr\u00fch das Kind zu Zauberg\u00e4rten", "tokens": ["Ihr", "lock\u00b7tet", "fr\u00fch", "das", "Kind", "zu", "Zau\u00b7berg\u00b7\u00e4r\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "verwunschnen Schl\u00f6ssern stillen gr\u00fcnen Seen", "tokens": ["ver\u00b7wun\u00b7schnen", "Schl\u00f6s\u00b7sern", "stil\u00b7len", "gr\u00fc\u00b7nen", "Seen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und brauner Wurzel quoll aus tr\u00fcbem Schacht", "tokens": ["und", "brau\u00b7ner", "Wur\u00b7zel", "quoll", "aus", "tr\u00fc\u00b7bem", "Schacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "geh\u00f6hlter Felsen unerme\u00dfnes Gold.", "tokens": ["ge\u00b7h\u00f6hl\u00b7ter", "Fel\u00b7sen", "un\u00b7er\u00b7me\u00df\u00b7nes", "Gold", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dann gingt ihr hin und euer leichtes Bild", "tokens": ["Dann", "gingt", "ihr", "hin", "und", "eu\u00b7er", "leich\u00b7tes", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "zerflo\u00df und zitterte nur traumhaft fern", "tokens": ["zer\u00b7flo\u00df", "und", "zit\u00b7ter\u00b7te", "nur", "traum\u00b7haft", "fern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "ADV", "ADJD", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "wie leuchtend durch die N\u00e4chte warmer Schein", "tokens": ["wie", "leuch\u00b7tend", "durch", "die", "N\u00e4ch\u00b7te", "war\u00b7mer", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "in d\u00e4mmerweichen Sommerl\u00fcften h\u00e4ngt.", "tokens": ["in", "d\u00e4m\u00b7mer\u00b7wei\u00b7chen", "Som\u00b7mer\u00b7l\u00fcf\u00b7ten", "h\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nun t\u00f6nt mir eure Stimme s\u00fc\u00df vertraut", "tokens": ["Nun", "t\u00f6nt", "mir", "eu\u00b7re", "Stim\u00b7me", "s\u00fc\u00df", "ver\u00b7traut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "wie einem Kind das sich im Wald verlor", "tokens": ["wie", "ei\u00b7nem", "Kind", "das", "sich", "im", "Wald", "ver\u00b7lor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "PRF", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "der Glocken L\u00e4uten still vom Abendwind", "tokens": ["der", "Glo\u00b7cken", "L\u00e4u\u00b7ten", "still", "vom", "Ab\u00b7end\u00b7wind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "durch welken Glanz der Tale hingeweht.", "tokens": ["durch", "wel\u00b7ken", "Glanz", "der", "Ta\u00b7le", "hin\u00b7ge\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}