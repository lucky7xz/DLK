{"dta.poem.16332": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "68.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O schw\u00f6re nicht, weil izt du hassest, stets zu hassen;", "tokens": ["O", "schw\u00f6\u00b7re", "nicht", ",", "weil", "izt", "du", "has\u00b7sest", ",", "stets", "zu", "has\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "$,", "KOUS", "ADV", "PPER", "VVFIN", "$,", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erla\u00df den Ha\u00df dem Feind! der Schwur sei dir erlassen.", "tokens": ["Er\u00b7la\u00df", "den", "Ha\u00df", "dem", "Feind", "!", "der", "Schwur", "sei", "dir", "er\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$.", "ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Auch schw\u00f6re nicht, wen izt du liebest, stets zu lieben;", "tokens": ["Auch", "schw\u00f6\u00b7re", "nicht", ",", "wen", "izt", "du", "lie\u00b7best", ",", "stets", "zu", "lie\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "$,", "PWS", "VVFIN", "PPER", "VVFIN", "$,", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Freundschaft kann vergehn, dann ist der Schwur geblieben.", "tokens": ["Die", "Freund\u00b7schaft", "kann", "ver\u00b7gehn", ",", "dann", "ist", "der", "Schwur", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,", "ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Treu seyn dir selber nur und Gott und der Natur,", "tokens": ["Treu", "seyn", "dir", "sel\u00b7ber", "nur", "und", "Gott", "und", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "PPER", "ADV", "ADV", "KON", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch dieses schw\u00f6re nicht, doch halt es ohne Schwur!", "tokens": ["Auch", "die\u00b7ses", "schw\u00f6\u00b7re", "nicht", ",", "doch", "halt", "es", "oh\u00b7ne", "Schwur", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "APPR", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}