{"dta.poem.7060": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Seeligkeit besteht auch ferner nicht allein", "tokens": ["Die", "See\u00b7lig\u00b7keit", "be\u00b7steht", "auch", "fer\u00b7ner", "nicht", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In einer stillen Ruh\u2019, da wir von Schmertz und Pein", "tokens": ["In", "ei\u00b7ner", "stil\u00b7len", "Ruh'", ",", "da", "wir", "von", "Schmertz", "und", "Pein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Entfernet und befreyet seyn.", "tokens": ["Ent\u00b7fer\u00b7net", "und", "be\u00b7fre\u00b7yet", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die eintzige Entfernung von den B\u00f6sen", "tokens": ["Die", "eint\u00b7zi\u00b7ge", "Ent\u00b7fer\u00b7nung", "von", "den", "B\u00f6\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ist nicht genug", "tokens": ["Ist", "nicht", "ge\u00b7nug"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ein mit Vernunfft begabtes Wesen", "tokens": ["Ein", "mit", "Ver\u00b7nunfft", "be\u00b7gab\u00b7tes", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Begl\u00fcckt zu machen, zu vergn\u00fcgen:", "tokens": ["Be\u00b7gl\u00fcckt", "zu", "ma\u00b7chen", ",", "zu", "ver\u00b7gn\u00fc\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Es mu\u00df ein etwas seyn, das w\u00fcrcklich da,", "tokens": ["Es", "mu\u00df", "ein", "et\u00b7was", "seyn", ",", "das", "w\u00fcrck\u00b7lich", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "PIS", "VAINF", "$,", "PRELS", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Das sich mit ihm verbindet, das ihm nah,", "tokens": ["Das", "sich", "mit", "ihm", "ver\u00b7bin\u00b7det", ",", "das", "ihm", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "PPER", "VVFIN", "$,", "PRELS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wodurch er wesentlich kan ein Empfinden kriegen.", "tokens": ["Wo\u00b7durch", "er", "we\u00b7sent\u00b7lich", "kan", "ein", "Emp\u00b7fin\u00b7den", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein Wesen von Natur, stets w\u00fcrckend, stets gesch\u00e4fftig,", "tokens": ["Ein", "We\u00b7sen", "von", "Na\u00b7tur", ",", "stets", "w\u00fcr\u00b7ckend", ",", "stets", "ge\u00b7sch\u00e4ff\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Vermag kein\u2019 andre Ruh\u2019, kein\u2019 andre Lust zu finden,", "tokens": ["Ver\u00b7mag", "kein'", "and\u00b7re", "Ruh'", ",", "kein'", "and\u00b7re", "Lust", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$,", "PIAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Als wenn es w\u00fcrckend ist, als wenn es kr\u00e4fftig,", "tokens": ["Als", "wenn", "es", "w\u00fcr\u00b7ckend", "ist", ",", "als", "wenn", "es", "kr\u00e4ff\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "KOKOM", "KOUS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Mit solchen Sachen sich zu gatten, zu verbinden,", "tokens": ["Mit", "sol\u00b7chen", "Sa\u00b7chen", "sich", "zu", "gat\u00b7ten", ",", "zu", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PRF", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die ewig wesentlich,", "tokens": ["Die", "e\u00b7wig", "we\u00b7sent\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Und immer mehr und mehr die Dinge zu ergr\u00fcnden,", "tokens": ["Und", "im\u00b7mer", "mehr", "und", "mehr", "die", "Din\u00b7ge", "zu", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "KON", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die ihm recht angenehm. Wir sp\u00fcren hier auf Erden", "tokens": ["Die", "ihm", "recht", "an\u00b7ge\u00b7nehm", ".", "Wir", "sp\u00fc\u00b7ren", "hier", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADJD", "$.", "PPER", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dergleichen Neigung schon, doch weil der Leib verwehrt", "tokens": ["Derg\u00b7lei\u00b7chen", "Nei\u00b7gung", "schon", ",", "doch", "weil", "der", "Leib", "ver\u00b7wehrt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "NN", "ADV", "$,", "ADV", "KOUS", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Allhier was gr\u00fcndliches zu fassen;", "tokens": ["All\u00b7hier", "was", "gr\u00fcnd\u00b7li\u00b7ches", "zu", "fas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Mu\u00df unser Geist es hier blo\u00df beym Bewundern lassen.", "tokens": ["Mu\u00df", "un\u00b7ser", "Geist", "es", "hier", "blo\u00df", "beym", "Be\u00b7wun\u00b7dern", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PPER", "ADV", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Vermuthlich mu\u00df daher sein w\u00fcrckliches Vergn\u00fcgen", "tokens": ["Ver\u00b7muth\u00b7lich", "mu\u00df", "da\u00b7her", "sein", "w\u00fcrck\u00b7li\u00b7ches", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PAV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "In gleich gearteten,", "tokens": ["In", "gleich", "ge\u00b7ar\u00b7te\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVPP", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.23": {"text": "Stets aneinander hangenden,", "tokens": ["Stets", "an\u00b7ein\u00b7an\u00b7der", "han\u00b7gen\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.24": {"text": "Und angenehmsten W\u00fcrckungen,", "tokens": ["Und", "an\u00b7ge\u00b7nehms\u00b7ten", "W\u00fcr\u00b7ckun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.25": {"text": "Womit sich der Verstand besch\u00e4fftigt, liegen.", "tokens": ["Wo\u00b7mit", "sich", "der", "Ver\u00b7stand", "be\u00b7sch\u00e4ff\u00b7tigt", ",", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}