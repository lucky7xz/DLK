{"textgrid.poem.39639": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Charakteristik eines Geschichtschreibers", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein heiser kr\u00e4hend keckes Kerlchen,", "tokens": ["Ein", "hei\u00b7ser", "kr\u00e4\u00b7hend", "ke\u00b7ckes", "Kerl\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Beinen, Kranichstelzen gleich,", "tokens": ["Auf", "Bei\u00b7nen", ",", "Kra\u00b7nich\u00b7stel\u00b7zen", "gleich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Fischt Hypothesen, klein wie Schmerlchen,", "tokens": ["Fischt", "Hy\u00b7po\u00b7the\u00b7sen", ",", "klein", "wie", "Schmerl\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus der Geschichte gro\u00dfem Teich.", "tokens": ["Aus", "der", "Ge\u00b7schich\u00b7te", "gro\u00b7\u00dfem", "Teich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie Hercules auf seinen Nacken", "tokens": ["Wie", "Her\u00b7cu\u00b7les", "auf", "sei\u00b7nen", "Na\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Des Atlas Himmelskugel nahm,", "tokens": ["Des", "At\u00b7las", "Him\u00b7mels\u00b7ku\u00b7gel", "nahm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So wagt er, Rom sich aufzupacken.", "tokens": ["So", "wagt", "er", ",", "Rom", "sich", "auf\u00b7zu\u00b7pa\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NE", "PRF", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schleppt daran sich krumm und lahm.", "tokens": ["Und", "schleppt", "da\u00b7ran", "sich", "krumm", "und", "lahm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "PRF", "ADJD", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Thersites mu\u00df das Gro\u00dfe tadeln,", "tokens": ["Ther\u00b7si\u00b7tes", "mu\u00df", "das", "Gro\u00b7\u00dfe", "ta\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "ADJA", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Das Kleine stutzt er stattlich auf,", "tokens": ["Das", "Klei\u00b7ne", "stutzt", "er", "statt\u00b7lich", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nach Dithmarsen und nach Hadeln", "tokens": ["Und", "nach", "Dith\u00b7mar\u00b7sen", "und", "nach", "Ha\u00b7deln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geht immer der Gedanken Lauf.", "tokens": ["Geht", "im\u00b7mer", "der", "Ge\u00b7dan\u00b7ken", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}