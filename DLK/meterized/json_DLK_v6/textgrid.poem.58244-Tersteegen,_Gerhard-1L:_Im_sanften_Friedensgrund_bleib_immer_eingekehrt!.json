{"textgrid.poem.58244": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im sanften Friedensgrund bleib immer eingekehrt!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im sanften Friedensgrund bleib immer eingekehrt!", "tokens": ["Im", "sanf\u00b7ten", "Frie\u00b7dens\u00b7grund", "bleib", "im\u00b7mer", "ein\u00b7ge\u00b7kehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nichts schw\u00e4chet mehr den Geist, als wenn er ist gest\u00f6rt.", "tokens": ["Nichts", "schw\u00e4\u00b7chet", "mehr", "den", "Geist", ",", "als", "wenn", "er", "ist", "ge\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "$,", "KOKOM", "KOUS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sobald du wirst bewegt, so setz dich stille nieder", "tokens": ["So\u00b7bald", "du", "wirst", "be\u00b7wegt", ",", "so", "setz", "dich", "stil\u00b7le", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VAFIN", "VVPP", "$,", "ADV", "VVFIN", "PPER", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und kehr, sobald du kannst, zu deiner Ruhe wieder!", "tokens": ["Und", "kehr", ",", "so\u00b7bald", "du", "kannst", ",", "zu", "dei\u00b7ner", "Ru\u00b7he", "wie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "VMFIN", "$,", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}