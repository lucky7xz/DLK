{"dta.poem.5317": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Eulchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Am Abend sa\u00df ich j\u00fcngst, gelassen und in Ruh,", "tokens": ["Am", "A\u00b7bend", "sa\u00df", "ich", "j\u00fcngst", ",", "ge\u00b7las\u00b7sen", "und", "in", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "$,", "VVPP", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In einem kleinem Garten-Zimmer,", "tokens": ["In", "ei\u00b7nem", "klei\u00b7nem", "Gar\u00b7ten\u00b7Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und sah durchs Fenster-Glas, wie sich des Tages Schim-", "tokens": ["Und", "sah", "durchs", "Fens\u00b7ter\u00b7Glas", ",", "wie", "sich", "des", "Ta\u00b7ges", "Schim"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,", "PWAV", "PRF", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "mer", "tokens": ["mer"], "token_info": ["word"], "pos": ["ADV"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Gemach verringerte: Die Schatten nahmen zu.", "tokens": ["Ge\u00b7mach", "ver\u00b7rin\u00b7ger\u00b7te", ":", "Die", "Schat\u00b7ten", "nah\u00b7men", "zu", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Indem erblicket\u2019 ich ein \u00e4msiges Geschwebe.", "tokens": ["In\u00b7dem", "er\u00b7bli\u00b7cket'", "ich", "ein", "\u00e4m\u00b7si\u00b7ges", "Ge\u00b7schwe\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Von einer Spinnen war ein ziemlich starck Gewebe", "tokens": ["Von", "ei\u00b7ner", "Spin\u00b7nen", "war", "ein", "ziem\u00b7lich", "starck", "Ge\u00b7we\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "ART", "ADV", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Jm Zimmer, vor den Scheiben her, gespannt,", "tokens": ["Jm", "Zim\u00b7mer", ",", "vor", "den", "Schei\u00b7ben", "her", ",", "ge\u00b7spannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "NN", "PTKVZ", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und, zwischen dieser falschen Wand,", "tokens": ["Und", ",", "zwi\u00b7schen", "die\u00b7ser", "fal\u00b7schen", "Wand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sah ich am Scheiben-Glas\u2019 ein weisses Eulchen fliegen", "tokens": ["Sah", "ich", "am", "Schei\u00b7ben\u00b7Glas'", "ein", "weis\u00b7ses", "Eul\u00b7chen", "flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Stets auf und nieder, hin und her.", "tokens": ["Stets", "auf", "und", "nie\u00b7der", ",", "hin", "und", "her", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "KON", "PTKVZ", "$,", "ADV", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Es schien, ob sucht\u2019 es blos am Lichte sein Vergn\u00fcgen,", "tokens": ["Es", "schien", ",", "ob", "sucht'", "es", "blos", "am", "Lich\u00b7te", "sein", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und, da\u00df es blos dadurch gesichert w\u00e4r.", "tokens": ["Und", ",", "da\u00df", "es", "blos", "da\u00b7durch", "ge\u00b7si\u00b7chert", "w\u00e4r", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "PAV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Jhr schwartzer Feind, die Spinne, ruhte nicht,", "tokens": ["Ihr", "schwart\u00b7zer", "Feind", ",", "die", "Spin\u00b7ne", ",", "ruh\u00b7te", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "$,", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Sie lieff\u2019 bald in die l\u00e4ng\u2019, bald in die qver,", "tokens": ["Sie", "lief\u00b7f'", "bald", "in", "die", "l\u00e4ng'", ",", "bald", "in", "die", "qver", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NE", "$,", "ADV", "APPR", "ART", "ADJA", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Mit offuen Klauen, doch des Himmels Licht,", "tokens": ["Mit", "of\u00b7fu\u00b7en", "Klau\u00b7en", ",", "doch", "des", "Him\u00b7mels", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.17": {"text": "Des Eulchens Augenmerck, wodurch es nicht zur\u00fccke,", "tokens": ["Des", "Eul\u00b7chens", "Au\u00b7gen\u00b7merck", ",", "wo\u00b7durch", "es", "nicht", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PWAV", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und nur stets vorwerts flog, befreit es von dem Stricke", "tokens": ["Und", "nur", "stets", "vor\u00b7werts", "flog", ",", "be\u00b7freit", "es", "von", "dem", "Stri\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADV", "VVFIN", "$,", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und seinem Untergang, indem es ungefehr,", "tokens": ["Und", "sei\u00b7nem", "Un\u00b7ter\u00b7gang", ",", "in\u00b7dem", "es", "un\u00b7ge\u00b7fehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Nach langem Flattern, in der Scheibe", "tokens": ["Nach", "lan\u00b7gem", "Flat\u00b7tern", ",", "in", "der", "Schei\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ART", "NN"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.21": {"text": "An eine Spalte kam,", "tokens": ["An", "ei\u00b7ne", "Spal\u00b7te", "kam", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "Und durch dieselbige sich seinem Tod\u2019 entzog,", "tokens": ["Und", "durch", "die\u00b7sel\u00b7bi\u00b7ge", "sich", "sei\u00b7nem", "Tod'", "ent\u00b7zog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die Freyheit fr\u00f6lich nahm,", "tokens": ["Die", "Frey\u00b7heit", "fr\u00f6\u00b7lich", "nahm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Und nach dem lang gesuchten Lichte flog.", "tokens": ["Und", "nach", "dem", "lang", "ge\u00b7such\u00b7ten", "Lich\u00b7te", "flog", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}