{"dta.poem.21770": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Die ernstliche Strenge steht endlich vers\u00fcsset/", "tokens": ["Die", "ernst\u00b7li\u00b7che", "Stren\u00b7ge", "steht", "end\u00b7lich", "ver\u00b7s\u00fcs\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "VVFIN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "die qweelende Seele wird einsten gesund.", "tokens": ["die", "qwee\u00b7len\u00b7de", "See\u00b7le", "wird", "eins\u00b7ten", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich habe gewonnen/ ich werde gek\u00fcsset/", "tokens": ["Ich", "ha\u00b7be", "ge\u00b7won\u00b7nen", "/", "ich", "wer\u00b7de", "ge\u00b7k\u00fcs\u00b7set", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$(", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+--+--+---+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "es schallet und knallet ihr z\u00e4rtlicher Mund.", "tokens": ["es", "schal\u00b7let", "und", "knal\u00b7let", "ihr", "z\u00e4rt\u00b7li\u00b7cher", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "die Dornen entweichen/", "tokens": ["die", "Dor\u00b7nen", "ent\u00b7wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "die Lippen verbleichen/", "tokens": ["die", "Lip\u00b7pen", "ver\u00b7blei\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "indehm sie die ihre den meinen auffdr\u00fckkt.", "tokens": ["in\u00b7dehm", "sie", "die", "ih\u00b7re", "den", "mei\u00b7nen", "auff\u00b7dr\u00fckkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "PPOSAT", "ART", "PPOSAT", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Ich werd\u2019 au\u00df der Erde zun G\u00f6ttern verschikkt.", "tokens": ["Ich", "werd'", "au\u00df", "der", "Er\u00b7de", "zun", "G\u00f6t\u00b7tern", "ver\u00b7schikkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Jhr klagende Plagen steht jetzo von fernen/", "tokens": ["Ihr", "kla\u00b7gen\u00b7de", "Pla\u00b7gen", "steht", "jet\u00b7zo", "von", "fer\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "APPR", "ADJA", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "es fliehe der achzende kr\u00e4chzende Neid!", "tokens": ["es", "flie\u00b7he", "der", "ach\u00b7zen\u00b7de", "kr\u00e4ch\u00b7zen\u00b7de", "Neid", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Mein Gang ist gegr\u00fcndet auch \u00fcber die Sternen", "tokens": ["Mein", "Gang", "ist", "ge\u00b7gr\u00fcn\u00b7det", "auch", "\u00fc\u00b7ber", "die", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "ADV", "APPR", "ART", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "ich f\u00fchle der Seeligen spielende Freud\u2019.", "tokens": ["ich", "f\u00fch\u00b7le", "der", "See\u00b7li\u00b7gen", "spie\u00b7len\u00b7de", "Freud'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Es flammen die Lippen.", "tokens": ["Es", "flam\u00b7men", "die", "Lip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die r\u00f6\u00dflichte Klippen", "tokens": ["Die", "r\u00f6\u00df\u00b7lich\u00b7te", "Klip\u00b7pen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "die bl\u00fchen und ziehen mich lieblich an sich.", "tokens": ["die", "bl\u00fc\u00b7hen", "und", "zie\u00b7hen", "mich", "lieb\u00b7lich", "an", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "VVFIN", "PPER", "ADJD", "APPR", "PRF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Was acht\u2019 ich dich Honig! was Nektar-wein dich!", "tokens": ["Was", "acht'", "ich", "dich", "Ho\u00b7nig", "!", "was", "Nek\u00b7ta\u00b7rwein", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "NN", "$.", "PWS", "NN", "PPER", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Durch dieses erwie\u00df es ihr s\u00fcsses Gem\u00fchte/", "tokens": ["Durch", "die\u00b7ses", "er\u00b7wie\u00df", "es", "ihr", "s\u00fcs\u00b7ses", "Ge\u00b7m\u00fch\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Nu h\u00f6hn\u2019 ich der K\u00f6nige Zepter und Bl\u00fcte/", "tokens": ["Nu", "h\u00f6hn'", "ich", "der", "K\u00f6\u00b7ni\u00b7ge", "Zep\u00b7ter", "und", "Bl\u00fc\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN", "KON", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "mich nimmet der Vorraht Eufrates nicht ein.", "tokens": ["mich", "nim\u00b7met", "der", "Vor\u00b7raht", "Euf\u00b7ra\u00b7tes", "nicht", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NE", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Kan ich sie nur haben:", "tokens": ["Kan", "ich", "sie", "nur", "ha\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "VAFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "was acht\u2019 ich der Gaben", "tokens": ["was", "acht'", "ich", "der", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.6": {"text": "der siegenden Krieger im Kapitolin", "tokens": ["der", "sie\u00b7gen\u00b7den", "Krie\u00b7ger", "im", "Ka\u00b7pi\u00b7to\u00b7lin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "die durch die bekr\u00e4nzeten Pforten einziehn!", "tokens": ["die", "durch", "die", "be\u00b7kr\u00e4n\u00b7ze\u00b7ten", "Pfor\u00b7ten", "ein\u00b7ziehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Ich habe die Sch\u00f6ne mit nichten gewonnen", "tokens": ["Ich", "ha\u00b7be", "die", "Sch\u00f6\u00b7ne", "mit", "nich\u00b7ten", "ge\u00b7won\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PIS", "VVPP"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "mit Solde von Golde/ mit Perlenem Wehrt/", "tokens": ["mit", "Sol\u00b7de", "von", "Gol\u00b7de", "/", "mit", "Per\u00b7le\u00b7nem", "Wehrt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NE", "$(", "APPR", "ADJA", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "und scheinenden Steinen in Bergen geronnen/", "tokens": ["und", "schei\u00b7nen\u00b7den", "Stei\u00b7nen", "in", "Ber\u00b7gen", "ge\u00b7ron\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "den Tyrischen Purpur hat sie nie begehrt.", "tokens": ["den", "Ty\u00b7ri\u00b7schen", "Pur\u00b7pur", "hat", "sie", "nie", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "die Zeilen/ die s\u00fcssen", "tokens": ["die", "Zei\u00b7len", "/", "die", "s\u00fcs\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "aus Pegasus Fl\u00fcssen", "tokens": ["aus", "Pe\u00b7ga\u00b7sus", "Fl\u00fcs\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "die haben ihr h\u00e4rtliches Hertze ger\u00fchrt:", "tokens": ["die", "ha\u00b7ben", "ihr", "h\u00e4rt\u00b7li\u00b7ches", "Hert\u00b7ze", "ge\u00b7r\u00fchrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Nu stehet mein Lorber mit Myrten geziert.", "tokens": ["Nu", "ste\u00b7het", "mein", "Lor\u00b7ber", "mit", "Myr\u00b7ten", "ge\u00b7ziert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}