{"dta.poem.8373": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Martin Opitzen Sch\u00e4fferey  \n von der Nimfen Hercinie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1630", "urn": "urn:nbn:de:kobv:b4-200905197863", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Sch\u00e4fferey Von der Nimfen Hercinie. Breslau, 1630."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr blinden sterblichen/ was zieht jhr vndt verreist", "tokens": ["Ihr", "blin\u00b7den", "sterb\u00b7li\u00b7chen", "/", "was", "zieht", "jhr", "vndt", "ver\u00b7reist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "VVINF", "$(", "PWS", "VVFIN", "PPER", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In beydes Indien? was wagt jhr seel vndt geist", "tokens": ["In", "bey\u00b7des", "In\u00b7di\u00b7en", "?", "was", "wagt", "jhr", "seel", "vndt", "geist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$.", "PWS", "VVFIN", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "F\u00fcr jhren knecht den leib? jhr holet krieg vndt streit/", "tokens": ["F\u00fcr", "jhren", "knecht", "den", "leib", "?", "jhr", "ho\u00b7let", "krieg", "vndt", "streit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$.", "PPER", "VVFIN", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Bringt au\u00df der newen welt auch eine weltvoll leidt.", "tokens": ["Bringt", "au\u00df", "der", "ne\u00b7wen", "welt", "auch", "ei\u00b7ne", "welt\u00b7voll", "leidt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ADV", "ART", "ADJD", "ADJD", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Ihr pfl\u00fcgt die wilde see/ verge\u00dfet ewer landt/", "tokens": ["Ihr", "pfl\u00fcgt", "die", "wil\u00b7de", "see", "/", "ver\u00b7ge\u00b7\u00dfet", "e\u00b7wer", "landt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$(", "VVFIN", "PPOSAT", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sucht goldt das eisern macht/ vndt habt es bey der handt.", "tokens": ["Sucht", "goldt", "das", "ei\u00b7sern", "macht", "/", "vndt", "habt", "es", "bey", "der", "handt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "VVFIN", "$(", "KON", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den demant findet kaum der schwartze Moor so wei\u00df/", "tokens": ["Den", "de\u00b7mant", "fin\u00b7det", "kaum", "der", "schwart\u00b7ze", "Moor", "so", "wei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ADV", "ART", "ADJA", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der jaspis ist vns schlecht/ die perlen tregt der Quei\u00df.", "tokens": ["Der", "jas\u00b7pis", "ist", "vns", "schlecht", "/", "die", "per\u00b7len", "tregt", "der", "Que\u00b7i\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN", "$(", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.9": {"text": "Gieher mensch/ die Natur/ die Erde tuffet dir:", "tokens": ["Gie\u00b7her", "mensch", "/", "die", "Na\u00b7tur", "/", "die", "Er\u00b7de", "tuf\u00b7fet", "dir", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "ART", "NN", "$(", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Wohin? nach guete: bleib: warumb? du hast es hier.", "tokens": ["Wo\u00b7hin", "?", "nach", "gue\u00b7te", ":", "bleib", ":", "wa\u00b7rumb", "?", "du", "hast", "es", "hier", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "APPR", "ADJA", "$.", "VVFIN", "$.", "PWAV", "$.", "PPER", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}