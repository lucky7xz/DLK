{"textgrid.poem.33801": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Tag und Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Abends vom glutroten Himmel", "tokens": ["Wenn", "A\u00b7bends", "vom", "glut\u00b7ro\u00b7ten", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der Sonne Ro\u00df heruntersteigt,", "tokens": ["Der", "Son\u00b7ne", "Ro\u00df", "her\u00b7un\u00b7ter\u00b7steigt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sohn des Lichts, der tageshelle Schimmel,", "tokens": ["Der", "Sohn", "des", "Lichts", ",", "der", "ta\u00b7ges\u00b7hel\u00b7le", "Schim\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und seinen Hals, den sch\u00f6n gebognen, neigt;", "tokens": ["Und", "sei\u00b7nen", "Hals", ",", "den", "sch\u00f6n", "ge\u00b7bog\u00b7nen", ",", "neigt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dann aus den Nebeln mit bereifter M\u00e4hne", "tokens": ["Dann", "aus", "den", "Ne\u00b7beln", "mit", "be\u00b7reif\u00b7ter", "M\u00e4h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Steigt ", "tokens": ["Steigt"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Und g\u00e4hnend weist es seine wei\u00dfen Z\u00e4hne", "tokens": ["Und", "g\u00e4h\u00b7nend", "weist", "es", "sei\u00b7ne", "wei\u00b7\u00dfen", "Z\u00e4h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den Umgekommenen der Schlacht.", "tokens": ["Den", "Um\u00b7ge\u00b7kom\u00b7me\u00b7nen", "der", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Es schl\u00e4ngelt sich gleich blauem Stahle", "tokens": ["Es", "schl\u00e4n\u00b7gelt", "sich", "gleich", "blau\u00b7em", "Stah\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch breite Str\u00f6m' und Eisgefild", "tokens": ["Durch", "brei\u00b7te", "Str\u00f6m'", "und", "Eis\u00b7ge\u00b7fild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und flie\u00dft dahin im Mondenstrahle,", "tokens": ["Und", "flie\u00dft", "da\u00b7hin", "im", "Mon\u00b7dens\u00b7trah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Blut von eines Helden Schild.", "tokens": ["Wie", "Blut", "von", "ei\u00b7nes", "Hel\u00b7den", "Schild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Sturmwind h\u00e4ngt an seinen Hufen,", "tokens": ["Der", "Sturm\u00b7wind", "h\u00e4ngt", "an", "sei\u00b7nen", "Hu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Schiffe jagt's im wilden Meer,", "tokens": ["Die", "Schif\u00b7fe", "jagt's", "im", "wil\u00b7den", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es saust vor\u00fcber, wo die W\u00e4chter rufen,", "tokens": ["Es", "saust", "vor\u00b7\u00fc\u00b7ber", ",", "wo", "die", "W\u00e4ch\u00b7ter", "ru\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PWAV", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "An Turm und Lager um ein schlafend Heer.", "tokens": ["An", "Turm", "und", "La\u00b7ger", "um", "ein", "schla\u00b7fend", "Heer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Indessen grast auf einer bunten Wiese", "tokens": ["In\u00b7des\u00b7sen", "grast", "auf", "ei\u00b7ner", "bun\u00b7ten", "Wie\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Sonnenro\u00df, gef\u00fchrt am Zaum", "tokens": ["Das", "Son\u00b7nen\u00b7ro\u00df", ",", "ge\u00b7f\u00fchrt", "am", "Zaum"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVPP", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von einem Zwergen, und es sitzt ein Riese", "tokens": ["Von", "ei\u00b7nem", "Zwer\u00b7gen", ",", "und", "es", "sitzt", "ein", "Rie\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Sattel, ein Gigant, der schwere Traum.", "tokens": ["Im", "Sat\u00b7tel", ",", "ein", "Gi\u00b7gant", ",", "der", "schwe\u00b7re", "Traum", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es ruhet aus im Waldesdunkel", "tokens": ["Es", "ru\u00b7het", "aus", "im", "Wal\u00b7des\u00b7dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "An bl\u00fchender Violen Saum,", "tokens": ["An", "bl\u00fc\u00b7hen\u00b7der", "Vi\u00b7o\u00b7len", "Saum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Wo kaum durchblinkt der Sterne m\u00fcd Gefunkel", "tokens": ["Wo", "kaum", "durch\u00b7blinkt", "der", "Ster\u00b7ne", "m\u00fcd", "Ge\u00b7fun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Esche schwarzen Zauberbaum.", "tokens": ["Der", "E\u00b7sche", "schwar\u00b7zen", "Zau\u00b7ber\u00b7baum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Auf einmal ist's, als f\u00fchl' es wieder", "tokens": ["Auf", "ein\u00b7mal", "ist's", ",", "als", "f\u00fchl'", "es", "wie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den alten Mut, die Erde bot", "tokens": ["Den", "al\u00b7ten", "Mut", ",", "die", "Er\u00b7de", "bot"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihm neue Kraft, es sch\u00fcttelt M\u00e4hn' und Glieder", "tokens": ["Ihm", "neu\u00b7e", "Kraft", ",", "es", "sch\u00fct\u00b7telt", "M\u00e4hn'", "und", "Glie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und stampft, da\u00df Feuer aus der Erde loht.", "tokens": ["Und", "stampft", ",", "da\u00df", "Feu\u00b7er", "aus", "der", "Er\u00b7de", "loht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und schnaubend st\u00fcrzt es sich ins Flutgew\u00fchle,", "tokens": ["Und", "schnau\u00b7bend", "st\u00fcrzt", "es", "sich", "ins", "Flut\u00b7ge\u00b7w\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Riese f\u00e4llt, der Zwerg ist tot;", "tokens": ["Der", "Rie\u00b7se", "f\u00e4llt", ",", "der", "Zwerg", "ist", "tot", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es wiehert und erweckt die Morgenk\u00fchle \u2013", "tokens": ["Es", "wie\u00b7hert", "und", "er\u00b7weckt", "die", "Mor\u00b7gen\u00b7k\u00fch\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "$("], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Am Himmel gl\u00fcht das Morgenrot.", "tokens": ["Am", "Him\u00b7mel", "gl\u00fcht", "das", "Mor\u00b7gen\u00b7rot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}