{"dta.poem.13229": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jedoch was fang ich an? wo bleibt der Tugend-Streit?", "tokens": ["Je\u00b7doch", "was", "fang", "ich", "an", "?", "wo", "bleibt", "der", "Tu\u00b7gen\u00b7dStreit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "PPER", "PTKVZ", "$.", "PWAV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo bleibt ", "tokens": ["Wo", "bleibt"], "token_info": ["word", "word"], "pos": ["PWAV", "VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Auf auf! du kannst zugleich von dem und jenem singen;", "tokens": ["Auf", "auf", "!", "du", "kannst", "zu\u00b7gleich", "von", "dem", "und", "je\u00b7nem", "sin\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PTKVZ", "$.", "PPER", "VMFIN", "ADV", "APPR", "ART", "KON", "PDAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "40Der Zufall macht den Thon der Cither besser klingen.", "tokens": ["Zu\u00b7fall", "macht", "den", "Thon", "der", "Cit\u00b7her", "bes\u00b7ser", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Mein Geist! erhohle dich! vollende den Gesang,", "tokens": ["Mein", "Geist", "!", "er\u00b7hoh\u00b7le", "dich", "!", "voll\u00b7en\u00b7de", "den", "Ge\u00b7sang", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPER", "$.", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gedenck: als ob dich nichts von deinem Ziel verdrang.", "tokens": ["Ge\u00b7denck", ":", "als", "ob", "dich", "nichts", "von", "dei\u00b7nem", "Ziel", "ver\u00b7drang", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KOKOM", "KOUS", "PPER", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Erz\u00e4hl nur, was du schon von ihm hast angefangen,", "tokens": ["Er\u00b7z\u00e4hl", "nur", ",", "was", "du", "schon", "von", "ihm", "hast", "an\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PWS", "PPER", "ADV", "APPR", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was in dem Tugend-Saal noch ferner vorgegangen.", "tokens": ["Was", "in", "dem", "Tu\u00b7gen\u00b7dSaal", "noch", "fer\u00b7ner", "vor\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}