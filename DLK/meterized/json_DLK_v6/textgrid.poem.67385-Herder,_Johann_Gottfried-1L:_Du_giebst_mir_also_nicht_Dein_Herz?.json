{"textgrid.poem.67385": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du giebst mir also nicht Dein Herz?", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du giebst mir also nicht Dein Herz?", "tokens": ["Du", "giebst", "mir", "al\u00b7so", "nicht", "Dein", "Herz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So gieb das meine mir!", "tokens": ["So", "gieb", "das", "mei\u00b7ne", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PDS", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn, Liebe, hab' ich Deines nicht,", "tokens": ["Denn", ",", "Lie\u00b7be", ",", "hab'", "ich", "Dei\u00b7nes", "nicht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "VAFIN", "PPER", "PPOSAT", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was soll das meine Dir?", "tokens": ["Was", "soll", "das", "mei\u00b7ne", "Dir", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PDS", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gieb es mir wieder! Doch la\u00df sein!", "tokens": ["Gieb", "es", "mir", "wie\u00b7der", "!", "Doch", "la\u00df", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "ADV", "$.", "KON", "VVIMP", "VAINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Bek\u00e4m' ich's auch zur\u00fcck,", "tokens": ["Be\u00b7k\u00e4m'", "ich's", "auch", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du stiehlst es mir ja tausendmal", "tokens": ["Du", "stiehlst", "es", "mir", "ja", "tau\u00b7send\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit jedem neuen Blick.", "tokens": ["Mit", "je\u00b7dem", "neu\u00b7en", "Blick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Behalt es! wahr in Deiner Brust", "tokens": ["Be\u00b7halt", "es", "!", "wahr", "in", "Dei\u00b7ner", "Brust"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fortan der Herzen zwei!", "tokens": ["For\u00b7tan", "der", "Her\u00b7zen", "zwei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "CARD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wol hauchet eins das andre an", "tokens": ["Wol", "hau\u00b7chet", "eins", "das", "and\u00b7re", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ART", "PIS", "PTKVZ"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Lieb' und zarter Treu.", "tokens": ["Mit", "Lieb'", "und", "zar\u00b7ter", "Treu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und weg denn, Zweifel! weg, o Schmerz!", "tokens": ["Und", "weg", "denn", ",", "Zwei\u00b7fel", "!", "weg", ",", "o", "Schmerz", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$,", "NN", "$.", "PTKVZ", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr findet keine Statt;", "tokens": ["Ihr", "fin\u00b7det", "kei\u00b7ne", "Statt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich glaub' es fest, ich hab' ihr Herz,", "tokens": ["Ich", "glaub'", "es", "fest", ",", "ich", "hab'", "ihr", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil sie das meine hat.", "tokens": ["Weil", "sie", "das", "mei\u00b7ne", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}