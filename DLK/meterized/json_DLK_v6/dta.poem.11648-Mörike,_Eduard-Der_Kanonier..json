{"dta.poem.11648": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Der Kanonier.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Auf der Erde begegneten sich die Schaaren des Himmels", "tokens": ["Auf", "der", "Er\u00b7de", "be\u00b7ge\u00b7gne\u00b7ten", "sich", "die", "Schaa\u00b7ren", "des", "Him\u00b7mels"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PRF", "ART", "NN", "ART", "NN"], "meter": "+-+---+-+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Und der H\u00f6llen; es kommt eben zur f\u00f6rmlichen Schlacht.", "tokens": ["Und", "der", "H\u00f6l\u00b7len", ";", "es", "kommt", "e\u00b7ben", "zur", "f\u00f6rm\u00b7li\u00b7chen", "Schlacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$.", "PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Vorn am H\u00fcgel steht ein Teufel bei der Kanone;", "tokens": ["Vorn", "am", "H\u00fc\u00b7gel", "steht", "ein", "Teu\u00b7fel", "bei", "der", "Ka\u00b7no\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Sein stets rauchender Schwanz dient ihm als Lunte dabei.", "tokens": ["Sein", "stets", "rau\u00b7chen\u00b7der", "Schwanz", "dient", "ihm", "als", "Lun\u00b7te", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADJA", "NN", "VVFIN", "PPER", "KOUS", "NN", "PAV", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "(etwas phantastisch geformt ist der Feuerschlund, Fl\u00fcgel", "tokens": ["(", "et\u00b7was", "phan\u00b7tas\u00b7tisch", "ge\u00b7formt", "ist", "der", "Feu\u00b7er\u00b7schlund", ",", "Fl\u00fc\u00b7gel"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["$(", "ADV", "ADJD", "VVPP", "VAFIN", "ART", "NN", "$,", "NN"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.6": {"text": "Statt der R\u00e4der, stehn h\u00fcben und dr\u00fcben empor:", "tokens": ["Statt", "der", "R\u00e4\u00b7der", ",", "stehn", "h\u00fc\u00b7ben", "und", "dr\u00fc\u00b7ben", "em\u00b7por", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "ADJD", "KON", "ADV", "PTKVZ", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.7": {"text": "Denn man braucht dies Gesch\u00fctz zuweilen \u00fcber den Wolken", "tokens": ["Denn", "man", "braucht", "dies", "Ge\u00b7sch\u00fctz", "zu\u00b7wei\u00b7len", "\u00fc\u00b7ber", "den", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "PDS", "NN", "ADV", "APPR", "ART", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Bei Blokaden, da fliegt es durch die h\u00f6llische Kunst.)", "tokens": ["Bei", "Blo\u00b7ka\u00b7den", ",", "da", "fliegt", "es", "durch", "die", "h\u00f6l\u00b7li\u00b7sche", "Kunst", ".", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+---+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "Aber der Kerl ist feige; denn w\u00e4hrend langsam der", "tokens": ["A\u00b7ber", "der", "Kerl", "ist", "fei\u00b7ge", ";", "denn", "w\u00e4h\u00b7rend", "lang\u00b7sam", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "$.", "KON", "KOUS", "ADJD", "ART"], "meter": "+--+-+--+-+--", "measure": "iambic.penta.invert"}, "line.10": {"text": "Nach dem Z\u00fcndloch bewegt, h\u00e4lt er die Ohren sich zu,", "tokens": ["Nach", "dem", "Z\u00fcnd\u00b7loch", "be\u00b7wegt", ",", "h\u00e4lt", "er", "die", "Oh\u00b7ren", "sich", "zu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Seitw\u00e4rts \u00fcber die Achsel nur schielend, jetzo die Augen", "tokens": ["Seit\u00b7w\u00e4rts", "\u00fc\u00b7ber", "die", "Ach\u00b7sel", "nur", "schie\u00b7lend", ",", "jet\u00b7zo", "die", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "ADJD", "$,", "ADV", "ART", "NN"], "meter": "+-+---+-+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "Fest zudr\u00fcckend: Tupf! folgt der entsetzliche Knall.", "tokens": ["Fest", "zu\u00b7dr\u00fc\u00b7ckend", ":", "Tupf", "!", "folgt", "der", "ent\u00b7setz\u00b7li\u00b7che", "Knall", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$.", "NN", "$.", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-++--+--+", "measure": "trochaic.hexa.relaxed"}}}}}