{"textgrid.poem.64122": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Ottar und Hilde", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Odhins Sohn war Ottar der Edle.", "tokens": ["Od\u00b7hins", "Sohn", "war", "Ot\u00b7tar", "der", "Ed\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "NN", "ART", "ADJA", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Weidlich wuchs er", "tokens": ["Weid\u00b7lich", "wuchs", "er"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Heran, der herrliche Held.", "tokens": ["He\u00b7ran", ",", "der", "herr\u00b7li\u00b7che", "Held", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Als er erwachsen,", "tokens": ["Als", "er", "er\u00b7wach\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Als dem Flinken der Flaum", "tokens": ["Als", "dem", "Flin\u00b7ken", "der", "Flaum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Br\u00e4unlichen Bartes", "tokens": ["Br\u00e4un\u00b7li\u00b7chen", "Bar\u00b7tes"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Locker und lieblich", "tokens": ["Lo\u00b7cker", "und", "lieb\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "ADJD"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Die Lippen umlockte,", "tokens": ["Die", "Lip\u00b7pen", "um\u00b7lock\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Als den spitzigen", "tokens": ["Als", "den", "spit\u00b7zi\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "ADJA"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.10": {"text": "Spangenspaltenden Speer", "tokens": ["Span\u00b7gens\u00b7pal\u00b7ten\u00b7den", "Speer"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Wuchtig er warf,", "tokens": ["Wuch\u00b7tig", "er", "warf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Erschien ihm Odhin,", "tokens": ["Er\u00b7schien", "ihm", "Od\u00b7hin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Hielt an der Hand", "tokens": ["Hielt", "an", "der", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "Hilde, die Holde,", "tokens": ["Hil\u00b7de", ",", "die", "Hol\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Die der Wahl waltende", "tokens": ["Die", "der", "Wahl", "wal\u00b7ten\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "NN"], "meter": "+-++--", "measure": "unknown.measure.tri"}, "line.16": {"text": "Walk\u00fcre.", "tokens": ["Wal\u00b7k\u00fc\u00b7re", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Aus hohem Helm", "tokens": ["Aus", "ho\u00b7hem", "Helm"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "Flo\u00df der Freudigen", "tokens": ["Flo\u00df", "der", "Freu\u00b7di\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.19": {"text": "Lang das lichte Gelock,", "tokens": ["Lang", "das", "lich\u00b7te", "Ge\u00b7lock", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.20": {"text": "Das goldig-gelbe;", "tokens": ["Das", "gol\u00b7dig\u00b7gel\u00b7be", ";"], "token_info": ["word", "word", "punct"], "pos": ["ART", "ADJA", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.21": {"text": "Sieghaft und selig", "tokens": ["Sieg\u00b7haft", "und", "se\u00b7lig"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "ADJD"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "Strahlte ihr,", "tokens": ["Strahl\u00b7te", "ihr", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.23": {"text": "Ganz goldig,", "tokens": ["Ganz", "gol\u00b7dig", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.24": {"text": "Gleich dem herrlichen Haare,", "tokens": ["Gleich", "dem", "herr\u00b7li\u00b7chen", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.25": {"text": "Das edle Auge.", "tokens": ["Das", "ed\u00b7le", "Au\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.26": {"text": "Odhin aber", "tokens": ["Od\u00b7hin", "a\u00b7ber"], "token_info": ["word", "word"], "pos": ["NE", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.27": {"text": "Legte dem Liebling", "tokens": ["Leg\u00b7te", "dem", "Lieb\u00b7ling"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.28": {"text": "Der Holden Hand in die Hand:", "tokens": ["Der", "Hol\u00b7den", "Hand", "in", "die", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.29": {"text": "\u00bbdie Schimmernde sch\u00fctzt dich", "tokens": ["\u00bb", "die", "Schim\u00b7mern\u00b7de", "sch\u00fctzt", "dich"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "VVFIN", "PPER"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.30": {"text": "In Schrecken der Schlacht.", "tokens": ["In", "Schre\u00b7cken", "der", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.31": {"text": "Nicht geschwungenes Schwert,", "tokens": ["Nicht", "ge\u00b7schwun\u00b7ge\u00b7nes", "Schwert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.32": {"text": "Nicht hauender Hammer", "tokens": ["Nicht", "hau\u00b7en\u00b7der", "Ham\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["PTKNEG", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.33": {"text": "F\u00e4llt dich Fr\u00f6hlichen,", "tokens": ["F\u00e4llt", "dich", "Fr\u00f6h\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.34": {"text": "So lange leuchtend", "tokens": ["So", "lan\u00b7ge", "leuch\u00b7tend"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.35": {"text": "Die jauchzende Jungfrau", "tokens": ["Die", "jauch\u00b7zen\u00b7de", "Jung\u00b7frau"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--++", "measure": "iambic.tri.relaxed"}, "line.36": {"text": "Schirmend den Schild", "tokens": ["Schir\u00b7mend", "den", "Schild"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.37": {"text": "Ob dem Haupte dir h\u00e4lt,", "tokens": ["Ob", "dem", "Haup\u00b7te", "dir", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.38": {"text": "Schwanenschwingig", "tokens": ["Schwa\u00b7nen\u00b7schwin\u00b7gig"], "token_info": ["word"], "pos": ["NE"], "meter": "+-+-", "measure": "trochaic.di"}, "line.39": {"text": "Dich umschwebend.", "tokens": ["Dich", "um\u00b7schwe\u00b7bend", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.40": {"text": "H\u00fcte dich, Held,", "tokens": ["H\u00fc\u00b7te", "dich", ",", "Held", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.41": {"text": "Da\u00df jemals die Jungfrau", "tokens": ["Da\u00df", "je\u00b7mals", "die", "Jung\u00b7frau"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.42": {"text": "Dir Fechtendem fehle.\u00ab", "tokens": ["Dir", "Fech\u00b7ten\u00b7dem", "feh\u00b7le", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "NN", "VVFIN", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.43": {"text": "Manchen Mond", "tokens": ["Man\u00b7chen", "Mond"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.44": {"text": "Wechselnder Winter", "tokens": ["Wech\u00b7seln\u00b7der", "Win\u00b7ter"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.45": {"text": "Von Sieg zu Siege", "tokens": ["Von", "Sieg", "zu", "Sie\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.46": {"text": "Eilte Ottar der Edle", "tokens": ["Eil\u00b7te", "Ot\u00b7tar", "der", "Ed\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "ADJA"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.47": {"text": "Unverwundet:", "tokens": ["Un\u00b7ver\u00b7wun\u00b7det", ":"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.48": {"text": "Speere sprangen", "tokens": ["Spee\u00b7re", "spran\u00b7gen"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.49": {"text": "Und geschwungene Schwerte", "tokens": ["Und", "ge\u00b7schwun\u00b7ge\u00b7ne", "Schwer\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.50": {"text": "Ihm ab von dem offenen Antlitz:", "tokens": ["Ihm", "ab", "von", "dem", "of\u00b7fe\u00b7nen", "Ant\u00b7litz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.51": {"text": "Denn sacht, auf silbernen Sohlen,", "tokens": ["Denn", "sacht", ",", "auf", "sil\u00b7ber\u00b7nen", "Soh\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.52": {"text": "Schwanenschwingig schwebte", "tokens": ["Schwa\u00b7nen\u00b7schwin\u00b7gig", "schweb\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJD", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.53": {"text": "Hoch zu H\u00e4upten ihm Hilde. \u2013", "tokens": ["Hoch", "zu", "H\u00e4up\u00b7ten", "ihm", "Hil\u00b7de", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "NN", "PPER", "NN", "$.", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "Aber als wieder im Wechsel", "tokens": ["A\u00b7ber", "als", "wie\u00b7der", "im", "Wech\u00b7sel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "APPRART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Ein Jahr sich gej\u00e4hrt,", "tokens": ["Ein", "Jahr", "sich", "ge\u00b7j\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mu\u00dfte der Mutige", "tokens": ["Mu\u00df\u00b7te", "der", "Mu\u00b7ti\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Mit arger \u00dcberzahl", "tokens": ["Mit", "ar\u00b7ger", "\u00dc\u00b7berz\u00b7ahl"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Fechten der Feinde,", "tokens": ["Fech\u00b7ten", "der", "Fein\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Einsam, allein, unbesch\u00fctzt,", "tokens": ["Ein\u00b7sam", ",", "al\u00b7lein", ",", "un\u00b7be\u00b7sch\u00fctzt", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn er darbte", "tokens": ["Denn", "er", "darb\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Der holden H\u00fcterin:", "tokens": ["Der", "hol\u00b7den", "H\u00fc\u00b7te\u00b7rin", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Nicht mehr jauchzte die Jungfrau:", "tokens": ["Nicht", "mehr", "jauchz\u00b7te", "die", "Jung\u00b7frau", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "In Wehen wand sich das Weib.", "tokens": ["In", "We\u00b7hen", "wand", "sich", "das", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Lodernder Liebe", "tokens": ["Lo\u00b7dern\u00b7der", "Lie\u00b7be"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Lechzend Verlangen", "tokens": ["Lech\u00b7zend", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADJD", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Hatte heimlich", "tokens": ["Hat\u00b7te", "heim\u00b7lich"], "token_info": ["word", "word"], "pos": ["VAFIN", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "Die herrlichen Herzen", "tokens": ["Die", "herr\u00b7li\u00b7chen", "Her\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Brennend verbunden.", "tokens": ["Bren\u00b7nend", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Auf dem Lager lag", "tokens": ["Auf", "dem", "La\u00b7ger", "lag"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.17": {"text": "St\u00f6hnend, sterbend die Stolze.", "tokens": ["St\u00f6h\u00b7nend", ",", "ster\u00b7bend", "die", "Stol\u00b7ze", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.18": {"text": "Ach, die Unsterblichkeit", "tokens": ["Ach", ",", "die", "U\u00b7nsterb\u00b7lich\u00b7keit"], "token_info": ["word", "punct", "word", "word"], "pos": ["ITJ", "$,", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.19": {"text": "War ihr gewichen", "tokens": ["War", "ihr", "ge\u00b7wi\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.20": {"text": "In der Umarmung", "tokens": ["In", "der", "Um\u00b7ar\u00b7mung"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.21": {"text": "Des Menschenmannes;", "tokens": ["Des", "Men\u00b7schen\u00b7man\u00b7nes", ";"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.22": {"text": "Und w\u00e4hrend dem Weibe", "tokens": ["Und", "w\u00e4h\u00b7rend", "dem", "Wei\u00b7be"], "tok