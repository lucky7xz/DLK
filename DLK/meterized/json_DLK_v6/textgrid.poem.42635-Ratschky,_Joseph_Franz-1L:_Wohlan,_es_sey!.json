{"textgrid.poem.42635": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohlan, es sey!", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.71", "en:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohlan, es sey!", "tokens": ["Wo\u00b7hlan", ",", "es", "sey", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Auch ich will's wagen,", "tokens": ["Auch", "ich", "will's", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Was ich in Tagen", "tokens": ["Was", "ich", "in", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Der Wonne, frey", "tokens": ["Der", "Won\u00b7ne", ",", "frey"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Vom Joch der Sorgen,", "tokens": ["Vom", "Joch", "der", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Und fern vom Zwang,", "tokens": ["Und", "fern", "vom", "Zwang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Dem grauen Morgen", "tokens": ["Dem", "grau\u00b7en", "Mor\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Entgegensang,", "tokens": ["Ent\u00b7ge\u00b7gen\u00b7sang", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Dem Vaterlande", "tokens": ["Dem", "Va\u00b7ter\u00b7lan\u00b7de"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "In diesem Bande", "tokens": ["In", "die\u00b7sem", "Ban\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Trotz allem Dr\u00e4un", "tokens": ["Trotz", "al\u00b7lem", "Dr\u00e4un"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIS", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Der Zoilaster", "tokens": ["Der", "Zoi\u00b7las\u00b7ter"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Und Kritikaster", "tokens": ["Und", "Kri\u00b7ti\u00b7kas\u00b7ter"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Getrost zu weihn.", "tokens": ["Ge\u00b7trost", "zu", "weihn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Zieht hin, ihr Spiele", "tokens": ["Zieht", "hin", ",", "ihr", "Spie\u00b7le"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Der Jugendzeit,", "tokens": ["Der", "Ju\u00b7gend\u00b7zeit", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wo, unentweiht", "tokens": ["Wo", ",", "un\u00b7ent\u00b7weiht"], "token_info": ["word", "punct", "word"], "pos": ["PWAV", "$,", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Vom Weltgew\u00fchle,", "tokens": ["Vom", "Welt\u00b7ge\u00b7w\u00fch\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "An Klio's Arm", "tokens": ["An", "Klio's", "Arm"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Mir fern vom Schwarm", "tokens": ["Mir", "fern", "vom", "Schwarm"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJD", "APPRART", "NN"], "meter": "++-+", "measure": "iambic.di"}, "line.7": {"text": "Der Sauert\u00f6pfe", "tokens": ["Der", "Sau\u00b7er\u00b7t\u00f6p\u00b7fe"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Manch Lied gedieh!", "tokens": ["Manch", "Lied", "ge\u00b7dieh", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Zieht hin, Gesch\u00f6pfe", "tokens": ["Zieht", "hin", ",", "Ge\u00b7sch\u00f6p\u00b7fe"], "token_info": ["word", "word", "punct", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Der Phantasie,", "tokens": ["Der", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Die im Genusse", "tokens": ["Die", "im", "Ge\u00b7nus\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Der frohsten Musse", "tokens": ["Der", "frohs\u00b7ten", "Mus\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Mein Geist gebar!", "tokens": ["Mein", "Geist", "ge\u00b7bar", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Zieht hin in Frieden!", "tokens": ["Zieht", "hin", "in", "Frie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Die holde Schaar", "tokens": ["Die", "hol\u00b7de", "Schaar"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Der Pieriden", "tokens": ["Der", "Pie\u00b7ri\u00b7den"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "M\u00f6g' auf der Bahn", "tokens": ["M\u00f6g'", "auf", "der", "Bahn"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.18": {"text": "Zum hochgeweihten", "tokens": ["Zum", "hoch\u00b7ge\u00b7weih\u00b7ten"], "token_info": ["word", "word"], "pos": ["APPRART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "Parnass hinan", "tokens": ["Par\u00b7nass", "hi\u00b7nan"], "token_info": ["word", "word"], "pos": ["XY", "XY"], "meter": "+-+-", "measure": "trochaic.di"}, "line.20": {"text": "Euch sanft geleiten!", "tokens": ["Euch", "sanft", "ge\u00b7lei\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Verzagt nicht gleich,", "tokens": ["Ver\u00b7zagt", "nicht", "gleich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ihr meine Lieder,", "tokens": ["Ihr", "mei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wenn hin und wieder", "tokens": ["Wenn", "hin", "und", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "KON", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Im deutschen Reich", "tokens": ["Im", "deut\u00b7schen", "Reich"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Sich Journalisten", "tokens": ["Sich", "Jour\u00b7na\u00b7lis\u00b7ten"], "token_info": ["word", "word"], "pos": ["PRF", "NE"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "K\u00fchn wider euch", "tokens": ["K\u00fchn", "wi\u00b7der", "euch"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPR", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Zur Fehde r\u00fcsten!", "tokens": ["Zur", "Feh\u00b7de", "r\u00fcs\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Oft ist ihr Muth", "tokens": ["Oft", "ist", "ihr", "Muth"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Nur Kinderwuth,", "tokens": ["Nur", "Kin\u00b7der\u00b7wuth", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Und halten M\u00e4nner,", "tokens": ["Und", "hal\u00b7ten", "M\u00e4n\u00b7ner", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Die man als Kenner", "tokens": ["Die", "man", "als", "Ken\u00b7ner"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIS", "KOKOM", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Des Sch\u00f6nen ehrt,", "tokens": ["Des", "Sch\u00f6\u00b7nen", "ehrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Euch lieb und werth,", "tokens": ["Euch", "lieb", "und", "werth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "So lasst die frechen", "tokens": ["So", "lasst", "die", "fre\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Pedanten schreyn,", "tokens": ["Pe\u00b7dan\u00b7ten", "schreyn", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Die insgemein", "tokens": ["Die", "ins\u00b7ge\u00b7mein"], "token_info": ["word", "word"], "pos": ["ART", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Nur Sylben stechen!", "tokens": ["Nur", "Syl\u00b7ben", "ste\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Doch solltet ihr", "tokens": ["Doch", "soll\u00b7tet", "ihr"], "token_info": ["word", "word", "word"], "pos": ["KON", "VMFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Mit Pfefferd\u00fcten", "tokens": ["Mit", "Pfef\u00b7fer\u00b7d\u00fc\u00b7ten"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und Zuckerh\u00fcten,", "tokens": ["Und", "Zu\u00b7cker\u00b7h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wie L\u00f6schpapier", "tokens": ["Wie", "L\u00f6schpa\u00b7pier"], "token_info": ["word", "word"], "pos": ["PWAV", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Je, klein zerst\u00fcckelt,", "tokens": ["Je", ",", "klein", "zer\u00b7st\u00fc\u00b7ckelt", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Um H\u00e4ringe", "tokens": ["Um", "H\u00e4\u00b7rin\u00b7ge"], "token_info": ["word", "word"], "pos": ["KOUI", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Herumgewickelt,", "tokens": ["Her\u00b7um\u00b7ge\u00b7wi\u00b7ckelt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Als Fl\u00fcchtlinge,", "tokens": ["Als", "Fl\u00fccht\u00b7lin\u00b7ge", ","], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Gleich hundert andern,", "tokens": ["Gleich", "hun\u00b7dert", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "CARD", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Das Land durchwandern,", "tokens": ["Das", "Land", "durch\u00b7wan\u00b7dern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "N\u00e4hm' euch das Heer", "tokens": ["N\u00e4hm'", "euch", "das", "Heer"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Der lockern Schneider", "tokens": ["Der", "lo\u00b7ckern", "Schnei\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NE"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Zum Mass f\u00fcr Kleider,", "tokens": ["Zum", "Mass", "f\u00fcr", "Klei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Ja f\u00e4nd' ich leider!", "tokens": ["Ja", "f\u00e4nd'", "ich", "lei\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Euch ungef\u00e4hr", "tokens": ["Euch", "un\u00b7ge\u00b7f\u00e4hr"], "token_info": ["word", "word"], "pos": ["PPER", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "In K\u00e4sel\u00e4den", "tokens": ["In", "K\u00e4\u00b7se\u00b7l\u00e4\u00b7den"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "Bey Leichenreden,", "tokens": ["Bey", "Lei\u00b7chen\u00b7re\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Busspredigten", "tokens": ["Bus\u00b7spre\u00b7dig\u00b7ten"], "token_info": ["word"], "pos": ["NN"], "meter": "-+--", "measure": "dactylic.init"}, "line.19": {"text": "Und Fasts Scharteken,", "tokens": ["Und", "Fasts", "Schar\u00b7te\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.20": {"text": "Bey kritischen", "tokens": ["Bey", "kri\u00b7ti\u00b7schen"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+--", "measure": "dactylic.init"}, "line.21": {"text": "Bibliotheken", "tokens": ["Bib\u00b7lio\u00b7the\u00b7ken"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.22": {"text": "Und Zeitungen", "tokens": ["Und", "Zei\u00b7tun\u00b7gen"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.23": {"text": "Zu meiner Schande,", "tokens": ["Zu", "mei\u00b7ner", "Schan\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.24": {"text": "O so verweilt", "tokens": ["O", "so", "ver\u00b7weilt"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADV", "VVFIN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.25": {"text": "In diesem Stande", "tokens": ["In", "die\u00b7sem", "Stan\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.26": {"text": "Der Schmach nicht! eilt", "tokens": ["Der", "Schmach", "nicht", "!", "eilt"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "PTKNEG", "$.", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.27": {"text": "Im schnellsten Trabe", "tokens": ["Im", "schnells\u00b7ten", "Tra\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.28": {"text": "Nur bald zu Grabe,", "tokens": ["Nur", "bald", "zu", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.29": {"text": "Und str\u00e4ubt euch nicht;", "tokens": ["Und", "str\u00e4ubt", "euch", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.30": {"text": "Ein schlecht Gedicht", "tokens": ["Ein", "schlecht", "Ge\u00b7dicht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.31": {"text": "Bringt seine Schw\u00e4chen", "tokens": ["Bringt", "sei\u00b7ne", "Schw\u00e4\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.32": {"text": "Durch Widersprechen", "tokens": ["Durch", "Wi\u00b7der\u00b7spre\u00b7chen"], "token_info": ["word", "word"], "pos": ["APPR