{"textgrid.poem.65276": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Ausforderung", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine hohe Hahnenfeder", "tokens": ["Ei\u00b7ne", "ho\u00b7he", "Hah\u00b7nen\u00b7fe\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steck' ich auf meinen Hut!", "tokens": ["Steck'", "ich", "auf", "mei\u00b7nen", "Hut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Hut hat gr\u00fcne Farbe,", "tokens": ["Mein", "Hut", "hat", "gr\u00fc\u00b7ne", "Far\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Herz hat frischen Muth.", "tokens": ["Mein", "Herz", "hat", "fri\u00b7schen", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was will die Hahnenfeder?", "tokens": ["Was", "will", "die", "Hah\u00b7nen\u00b7fe\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie ruft zu Kampf und Streit,", "tokens": ["Sie", "ruft", "zu", "Kampf", "und", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie ruft: Ich lieb' die Beste", "tokens": ["Sie", "ruft", ":", "Ich", "lieb'", "die", "Bes\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Lande weit und breit!", "tokens": ["Im", "Lan\u00b7de", "weit", "und", "breit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und kennst du eine bessre,", "tokens": ["Und", "kennst", "du", "ei\u00b7ne", "bess\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und ist sie deine Wahl,", "tokens": ["Und", "ist", "sie", "dei\u00b7ne", "Wahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Steck' auf eine h\u00f6h're Feder,", "tokens": ["Steck'", "auf", "ei\u00b7ne", "h\u00f6h'\u00b7re", "Fe\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So raufen wir einmal.", "tokens": ["So", "rau\u00b7fen", "wir", "ein\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und mu\u00df ich unterliegen,", "tokens": ["Und", "mu\u00df", "ich", "un\u00b7ter\u00b7lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und lieg' ich in dem Sand,", "tokens": ["Und", "lieg'", "ich", "in", "dem", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich halt' auf meinem Spruche", "tokens": ["Ich", "halt'", "auf", "mei\u00b7nem", "Spru\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zeitlebens festen Stand.", "tokens": ["Zeit\u00b7le\u00b7bens", "fes\u00b7ten", "Stand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Und ist dein Dirnel sch\u00f6ner,", "tokens": ["Und", "ist", "dein", "Dir\u00b7nel", "sch\u00f6\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So trag's zur Stadt hinein,", "tokens": ["So", "trag's", "zur", "Stadt", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zum Markte, zum Verkaufe,", "tokens": ["Zum", "Mark\u00b7te", ",", "zum", "Ver\u00b7kau\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr's Dorf ist's halt zu fein.", "tokens": ["F\u00fcr's", "Dorf", "ist's", "halt", "zu", "fein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "VVFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und ist dein Dirnel fr\u00f6mmer,", "tokens": ["Und", "ist", "dein", "Dir\u00b7nel", "fr\u00f6m\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So f\u00fchr' es gleich nach Rom,", "tokens": ["So", "f\u00fchr'", "es", "gleich", "nach", "Rom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und la\u00df es heilig sprechen,", "tokens": ["Und", "la\u00df", "es", "hei\u00b7lig", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zur Lieb' ist's halt zu fromm.", "tokens": ["Zur", "Lieb'", "ist's", "halt", "zu", "fromm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "VVFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}