{"textgrid.poem.57354": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: M\u00e4chtige Kunst der Neuern, Erhalterin, th\u00e4tigste Freundin", "genre": "verse", "period": "N.A.", "pub_year": 1792, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00e4chtige Kunst der Neuern, Erhalterin, th\u00e4tigste Freundin", "tokens": ["M\u00e4ch\u00b7ti\u00b7ge", "Kunst", "der", "Neu\u00b7ern", ",", "Er\u00b7hal\u00b7te\u00b7rin", ",", "th\u00e4\u00b7tigs\u00b7te", "Freun\u00b7din"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN", "$,", "NN", "$,", "ADJA", "NN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Derer, die Geist uns zeigen, und Geist", "tokens": ["De\u00b7rer", ",", "die", "Geist", "uns", "zei\u00b7gen", ",", "und", "Geist"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "$,", "ART", "NN", "PPER", "VVINF", "$,", "KON", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "F\u00e4hig sind zu geniessen, o du, die in wenigen Stunden", "tokens": ["F\u00e4\u00b7hig", "sind", "zu", "ge\u00b7nies\u00b7sen", ",", "o", "du", ",", "die", "in", "we\u00b7ni\u00b7gen", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PTKZU", "VVINF", "$,", "FM", "PPER", "$,", "PRELS", "APPR", "PIAT", "NN"], "meter": "+--+-+-+-+-+--+-", "measure": "iambic.septa.invert"}, "line.4": {"text": "Worte vertausendf\u00e4ltiget, dir,", "tokens": ["Wor\u00b7te", "ver\u00b7tau\u00b7send\u00b7f\u00e4l\u00b7ti\u00b7get", ",", "dir", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PPER", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Ja dir selbst verzeihen es sich zween deutsche Gebieter", "tokens": ["Ja", "dir", "selbst", "ver\u00b7zei\u00b7hen", "es", "sich", "zween", "deut\u00b7sche", "Ge\u00b7bie\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "PPER", "ADV", "VVFIN", "PPER", "PRF", "VVFIN", "ADJA", "NN"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Fesseln anzulegen, so bald", "tokens": ["Fes\u00b7seln", "an\u00b7zu\u00b7le\u00b7gen", ",", "so", "bald"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVIZU", "$,", "ADV", "ADV"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Du das geltende Wort des wahrheitredenden, ofnen", "tokens": ["Du", "das", "gel\u00b7ten\u00b7de", "Wort", "des", "wahr\u00b7heit\u00b7re\u00b7den\u00b7den", ",", "of\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "ART", "ADJA", "NN", "ART", "ADJA", "$,", "ADJA"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Mannes tausendstimmig umher", "tokens": ["Man\u00b7nes", "tau\u00b7sends\u00b7tim\u00b7mig", "um\u00b7her"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJD", "PTKVZ"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Willst erschallen lassen. In ihrer Weisheit erkohren", "tokens": ["Willst", "er\u00b7schal\u00b7len", "las\u00b7sen", ".", "In", "ih\u00b7rer", "Weis\u00b7heit", "er\u00b7koh\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "VVINF", "$.", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Sie nicht eben die gl\u00fccklichste Zeit.", "tokens": ["Sie", "nicht", "e\u00b7ben", "die", "gl\u00fcck\u00b7lichs\u00b7te", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Denn es ist doch kein Spiel, dass \u00bbIm vierten Jahre der Freyheit\u00ab", "tokens": ["Denn", "es", "ist", "doch", "kein", "Spiel", ",", "dass", "\u00bb", "Im", "vier\u00b7ten", "Jah\u00b7re", "der", "Frey\u00b7heit", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "PIAT", "NN", "$,", "KOUS", "$(", "APPRART", "ADJA", "NN", "ART", "NN", "$("], "meter": "--+---+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Frankreich an die Maale jetzt schreibt.", "tokens": ["Fran\u00b7kreich", "an", "die", "Maa\u00b7le", "jetzt", "schreibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.13": {"text": "Daniens Vater denkt so nicht. Von der Bothschaft des Kaisers", "tokens": ["Da\u00b7ni\u00b7ens", "Va\u00b7ter", "denkt", "so", "nicht", ".", "Von", "der", "Both\u00b7schaft", "des", "Kai\u00b7sers"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "ADV", "PTKNEG", "$.", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Unverleitet, l\u00e4sst er es stehn", "tokens": ["Un\u00b7ver\u00b7lei\u00b7tet", ",", "l\u00e4sst", "er", "es", "stehn"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "VVFIN", "PPER", "PPER", "VVINF"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.15": {"text": "Sein Gesetz, auf der goldenen Tafel: Die edel Kunst h\u00f6rt", "tokens": ["Sein", "Ge\u00b7setz", ",", "auf", "der", "gol\u00b7de\u00b7nen", "Ta\u00b7fel", ":", "Die", "e\u00b7del", "Kunst", "h\u00f6rt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "--+--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.16": {"text": "Hier nie k\u00f6niglich Fesselgeklirr,", "tokens": ["Hier", "nie", "k\u00f6\u00b7nig\u00b7lich", "Fes\u00b7sel\u00b7ge\u00b7klirr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.17": {"text": "Oh sie gleich, an dem M\u00e4ler es h\u00f6rt, an der Newa es h\u00f6ret,", "tokens": ["Oh", "sie", "gleich", ",", "an", "dem", "M\u00e4\u00b7ler", "es", "h\u00f6rt", ",", "an", "der", "Ne\u00b7wa", "es", "h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "ADV", "$,", "APPR", "ART", "NN", "PPER", "VVFIN", "$,", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.18": {"text": "Selbst an der Themse, nur leiseres Klangs.", "tokens": ["Selbst", "an", "der", "Them\u00b7se", ",", "nur", "lei\u00b7se\u00b7res", "Klangs", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADV", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.19": {"text": "Also herschet Friedrich, der Enkel meines geliebten", "tokens": ["Al\u00b7so", "her\u00b7schet", "Fried\u00b7rich", ",", "der", "En\u00b7kel", "mei\u00b7nes", "ge\u00b7lieb\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "$,", "ART", "NN", "PPOSAT", "ADJA"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "K\u00f6nigs, dess Asche heilig mir ist.", "tokens": ["K\u00f6\u00b7nigs", ",", "dess", "A\u00b7sche", "hei\u00b7lig", "mir", "ist", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELAT", "NN", "ADJD", "PPER", "VAFIN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.21": {"text": "Wohl ihm, er s\u00e4et' auch Saat zu des Landsmanns Freyung; die gr\u00fcnt nun", "tokens": ["Wohl", "ihm", ",", "er", "s\u00e4\u00b7et'", "auch", "Saat", "zu", "des", "Lands\u00b7manns", "Frey\u00b7ung", ";", "die", "gr\u00fcnt", "nun"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "PPER", "VVFIN", "ADV", "NN", "APPR", "ART", "NN", "NN", "$.", "ART", "VVFIN", "ADV"], "meter": "---+--+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Hebet der fr\u00fcheren \u00c4hren empor.", "tokens": ["He\u00b7bet", "der", "fr\u00fc\u00b7he\u00b7ren", "\u00c4h\u00b7ren", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.23": {"text": "Aber bald wird das ganze Gefild von gebogenen Halmen", "tokens": ["A\u00b7ber", "bald", "wird", "das", "gan\u00b7ze", "Ge\u00b7fild", "von", "ge\u00b7bo\u00b7ge\u00b7nen", "Hal\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.24": {"text": "Rauschen, und Wonne dem Erntenden seyn.", "tokens": ["Rau\u00b7schen", ",", "und", "Won\u00b7ne", "dem", "Ern\u00b7ten\u00b7den", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "NN", "ART", "NN", "VAINF", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.25": {"text": "Heller noch strahlet das Ziel, an dem die sch\u00f6nsten der Palmen", "tokens": ["Hel\u00b7ler", "noch", "strah\u00b7let", "das", "Ziel", ",", "an", "dem", "die", "sch\u00f6ns\u00b7ten", "der", "Pal\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "ART", "NN", "$,", "APPR", "PRELS", "ART", "ADJA", "ART", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.26": {"text": "Wehen, die je die Unsterblichkeit gab.", "tokens": ["We\u00b7hen", ",", "die", "je", "die", "U\u00b7nsterb\u00b7lich\u00b7keit", "gab", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.27": {"text": "England wollt' es erreichen, den Menschenhandel vernichten!", "tokens": ["En\u00b7gland", "wollt'", "es", "er\u00b7rei\u00b7chen", ",", "den", "Men\u00b7schen\u00b7han\u00b7del", "ver\u00b7nich\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "VVINF", "$,", "ART", "NN", "VVINF", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.28": {"text": "Aber es z\u00f6gerte, nahte sich nur.", "tokens": ["A\u00b7ber", "es", "z\u00f6\u00b7ger\u00b7te", ",", "nah\u00b7te", "sich", "nur", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "VVFIN", "PRF", "ADV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.29": {"text": "Danien hat es zuerst erreicht, hat empfangen der Palmen", "tokens": ["Da\u00b7ni\u00b7en", "hat", "es", "zu\u00b7erst", "er\u00b7reicht", ",", "hat", "emp\u00b7fan\u00b7gen", "der", "Pal\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "ADV", "VVPP", "$,", "VAFIN", "VVPP", "ART", "NN"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "Fr\u00fcheste aus der Unsterblichkeit Hand!", "tokens": ["Fr\u00fc\u00b7hes\u00b7te", "aus", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Gallien, Land, das Wunder thut, und du schlummerst? Erwach', und", "tokens": ["Gal\u00b7li\u00b7en", ",", "Land", ",", "das", "Wun\u00b7der", "thut", ",", "und", "du", "schlum\u00b7merst", "?", "Er\u00b7wach'", ",", "und"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NE", "$,", "NN", "$,", "ART", "NN", "VVFIN", "$,", "KON", "PPER", "VVFIN", "$.", "NN", "$,", "KON"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.32": {"text": "Thu diess Wunder Danien nach!", "tokens": ["Thu", "diess", "Wun\u00b7der", "Da\u00b7ni\u00b7en", "nach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "NE", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}