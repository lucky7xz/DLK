{"dta.poem.8489": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Die Golem .", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "H&#228;tt' ich dich nicht als s&#252;\u00dfes Kind gekannt,", "tokens": ["H", "&#228;", "tt'", "ich", "dich", "nicht", "als", "s", "&#252;", "\u00dfes", "Kind", "ge\u00b7kannt", ","], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "word", "word", "XML_entity", "word", "word", "word", "punct"], "pos": ["XY", "$(", "VVFIN", "PPER", "PRF", "PTKNEG", "KOUS", "PPER", "$(", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Mit deinem Seraph in den klaren Blicken,", "tokens": ["Mit", "dei\u00b7nem", "Se\u00b7raph", "in", "den", "kla\u00b7ren", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dich nicht geleitet in der M\u00e4hrchen Land,", "tokens": ["Dich", "nicht", "ge\u00b7lei\u00b7tet", "in", "der", "M\u00e4hr\u00b7chen", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVPP", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Gef\u00fchlt der kleinen H\u00e4nde zitternd Dr\u00fccken:", "tokens": ["Ge\u00b7f\u00fchlt", "der", "klei\u00b7nen", "H\u00e4n\u00b7de", "zit\u00b7ternd", "Dr\u00fc\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich w\u00fcrde jetzt dich mit Behagen sehen,", "tokens": ["Ich", "w\u00fcr\u00b7de", "jetzt", "dich", "mit", "Be\u00b7ha\u00b7gen", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Du w\u00e4rst mir eine h\u00fcbsche, brave Frau.", "tokens": ["Du", "w\u00e4rst", "mir", "ei\u00b7ne", "h\u00fcb\u00b7sche", ",", "bra\u00b7ve", "Frau", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch ach, nun mu\u00df ich unter deiner Brau,", "tokens": ["Doch", "ach", ",", "nun", "mu\u00df", "ich", "un\u00b7ter", "dei\u00b7ner", "Brau", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "ADV", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mu\u00df stets nach dem entflognen Engel sp\u00e4hen.", "tokens": ["Mu\u00df", "stets", "nach", "dem", "ent\u00b7flog\u00b7nen", "En\u00b7gel", "sp\u00e4\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und du, mit deinem Wort, bedacht und breit,", "tokens": ["Und", "du", ",", "mit", "dei\u00b7nem", "Wort", ",", "be\u00b7dacht", "und", "breit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem klugen L\u00e4cheln und der Stirne Falten,", "tokens": ["Dem", "klu\u00b7gen", "L\u00e4\u00b7cheln", "und", "der", "Stir\u00b7ne", "Fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Spricht dir kein armer Traum von jener Zeit,", "tokens": ["Spricht", "dir", "kein", "ar\u00b7mer", "Traum", "von", "je\u00b7ner", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ADJA", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo deine Glut die Felsen wollte spalten?", "tokens": ["Wo", "dei\u00b7ne", "Glut", "die", "Fel\u00b7sen", "woll\u00b7te", "spal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein braver B\u00fcrger bist du hoch zu ehren,", "tokens": ["Ein", "bra\u00b7ver", "B\u00fcr\u00b7ger", "bist", "du", "hoch", "zu", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein wahrer Heros auf der Mittelbahn,", "tokens": ["Ein", "wah\u00b7rer", "He\u00b7ros", "auf", "der", "Mit\u00b7tel\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch, o mein Flammenwirbel, mein Vulkan,", "tokens": ["Doch", ",", "o", "mein", "Flam\u00b7men\u00b7wir\u00b7bel", ",", "mein", "Vul\u00b7kan", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "FM", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ach, da\u00df die Berge M\u00e4use nur geb\u00e4ren.", "tokens": ["Ach", ",", "da\u00df", "die", "Ber\u00b7ge", "M\u00e4u\u00b7se", "nur", "ge\u00b7b\u00e4\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "ART", "NN", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Weh ihm, der lebt in des Vergangnen Schau,", "tokens": ["Weh", "ihm", ",", "der", "lebt", "in", "des", "Ver\u00b7gang\u00b7nen", "Schau", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PRELS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Um bleiche Bilder wirbt, verschwommne T\u00f6ne!", "tokens": ["Um", "blei\u00b7che", "Bil\u00b7der", "wirbt", ",", "ver\u00b7schwomm\u00b7ne", "T\u00f6\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht was gebrochen, macht das Haar ihm grau,", "tokens": ["Nicht", "was", "ge\u00b7bro\u00b7chen", ",", "macht", "das", "Haar", "ihm", "grau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PWS", "VVPP", "$,", "VVFIN", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was Tod geknickt in seiner s\u00fc\u00dfen Sch\u00f6ne;", "tokens": ["Was", "Tod", "ge\u00b7knickt", "in", "sei\u00b7ner", "s\u00fc\u00b7\u00dfen", "Sch\u00f6\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch sie, die Monumente ohne Todten,", "tokens": ["Doch", "sie", ",", "die", "Mo\u00b7nu\u00b7men\u00b7te", "oh\u00b7ne", "Tod\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die wandernden Gebilde ohne Blut,", "tokens": ["Die", "wan\u00b7dern\u00b7den", "Ge\u00b7bil\u00b7de", "oh\u00b7ne", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sie, seine Tempel ohne Opferglut,", "tokens": ["Sie", ",", "sei\u00b7ne", "Tem\u00b7pel", "oh\u00b7ne", "Op\u00b7fer\u00b7glut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und seine Haine ohne Fr\u00fchlingsboten!", "tokens": ["Und", "sei\u00b7ne", "Hai\u00b7ne", "oh\u00b7ne", "Fr\u00fch\u00b7lings\u00b7bo\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u2019s gibt eine Sage aus dem Orient", "tokens": ["'s", "gibt", "ei\u00b7ne", "Sa\u00b7ge", "aus", "dem", "O\u00b7rient"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Von Weisen, todter Masse Formen gebend,", "tokens": ["Von", "Wei\u00b7sen", ",", "tod\u00b7ter", "Mas\u00b7se", "For\u00b7men", "ge\u00b7bend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJA", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geliebte Formen, die die Sehnsucht kennt,", "tokens": ["Ge\u00b7lieb\u00b7te", "For\u00b7men", ",", "die", "die", "Sehn\u00b7sucht", "kennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und mit dem Zauberworte sie belebend;", "tokens": ["Und", "mit", "dem", "Zau\u00b7ber\u00b7wor\u00b7te", "sie", "be\u00b7le\u00b7bend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Golem wandelt mit bekanntem Schritte,", "tokens": ["Der", "Go\u00b7lem", "wan\u00b7delt", "mit", "be\u00b7kann\u00b7tem", "Schrit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Er spricht, er l\u00e4chelt mit bekanntem Hauch,", "tokens": ["Er", "spricht", ",", "er", "l\u00e4\u00b7chelt", "mit", "be\u00b7kann\u00b7tem", "Hauch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Allein es ist kein Strahl in seinem Aug\u2019,", "tokens": ["Al\u00b7lein", "es", "ist", "kein", "Strahl", "in", "sei\u00b7nem", "Aug'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Es schl\u00e4gt kein Herz in seines Busens Mitte.", "tokens": ["Es", "schl\u00e4gt", "kein", "Herz", "in", "sei\u00b7nes", "Bu\u00b7sens", "Mit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und wie sich alte Lieb\u2019 ihm unterjocht,", "tokens": ["Und", "wie", "sich", "al\u00b7te", "Lieb'", "ihm", "un\u00b7ter\u00b7jocht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PRF", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er haucht sie an mit der Verwesung Schrecken,", "tokens": ["Er", "haucht", "sie", "an", "mit", "der", "Ver\u00b7we\u00b7sung", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie angstvoll die Erinnrung ruft und pocht,", "tokens": ["Wie", "angst\u00b7voll", "die", "E\u00b7rinn\u00b7rung", "ruft", "und", "pocht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-----+-+", "measure": "dactylic.init"}, "line.4": {"text": "Es ist in ihm kein Schlafender zu wecken.", "tokens": ["Es", "ist", "in", "ihm", "kein", "Schla\u00b7fen\u00b7der", "zu", "we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und tief gebrochen sieht die Treue schwinden,", "tokens": ["Und", "tief", "ge\u00b7bro\u00b7chen", "sieht", "die", "Treu\u00b7e", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was sie so lang und heilig hat bewahrt,", "tokens": ["Was", "sie", "so", "lang", "und", "hei\u00b7lig", "hat", "be\u00b7wahrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "KON", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Was nicht des Lebens, nicht des Todes Art,", "tokens": ["Was", "nicht", "des", "Le\u00b7bens", ",", "nicht", "des", "To\u00b7des", "Art", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ART", "NN", "$,", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nicht hier und nicht im Himmel ist zu finden.", "tokens": ["Nicht", "hier", "und", "nicht", "im", "Him\u00b7mel", "ist", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KON", "PTKNEG", "APPRART", "NN", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "O kniee still an deiner Todten Gruft,", "tokens": ["O", "kni\u00b7ee", "still", "an", "dei\u00b7ner", "Tod\u00b7ten", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dort magst du milde, fromme Thr\u00e4nen weinen,", "tokens": ["Dort", "magst", "du", "mil\u00b7de", ",", "from\u00b7me", "Thr\u00e4\u00b7nen", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "$,", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit ihrem Odem s\u00e4uselt dir die Luft,", "tokens": ["Mit", "ih\u00b7rem", "O\u00b7dem", "s\u00e4u\u00b7selt", "dir", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit ihrem Antlitz wird der Mond dir scheinen,", "tokens": ["Mit", "ih\u00b7rem", "Ant\u00b7litz", "wird", "der", "Mond", "dir", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dein sind sie, dein, wie mit gebrochnen Augen,", "tokens": ["Dein", "sind", "sie", ",", "dein", ",", "wie", "mit", "ge\u00b7broch\u00b7nen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PPER", "$,", "PPOSAT", "$,", "PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie dein sie waren mit dem letzten Blick;", "tokens": ["Wie", "dein", "sie", "wa\u00b7ren", "mit", "dem", "letz\u00b7ten", "Blick", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch fliehe vor den Golem, flieh zur\u00fcck,", "tokens": ["Doch", "flie\u00b7he", "vor", "de