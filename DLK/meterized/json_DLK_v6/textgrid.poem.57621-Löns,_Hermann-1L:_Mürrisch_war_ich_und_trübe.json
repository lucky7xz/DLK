{"textgrid.poem.57621": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: M\u00fcrrisch war ich und tr\u00fcbe", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00fcrrisch war ich und tr\u00fcbe", "tokens": ["M\u00fcr\u00b7risch", "war", "ich", "und", "tr\u00fc\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "KON", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den langen, hei\u00dfen Tag,", "tokens": ["Den", "lan\u00b7gen", ",", "hei\u00b7\u00dfen", "Tag", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Hunger und Durst nach Liebe", "tokens": ["Der", "Hun\u00b7ger", "und", "Durst", "nach", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "War wieder im Herzen wach.", "tokens": ["War", "wie\u00b7der", "im", "Her\u00b7zen", "wach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich wu\u00dfte doch, da\u00df vergebens", "tokens": ["Ich", "wu\u00df\u00b7te", "doch", ",", "da\u00df", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "ADV"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Die Liebe zu mir tritt,", "tokens": ["Die", "Lie\u00b7be", "zu", "mir", "tritt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Ende meines Lebens", "tokens": ["Das", "En\u00b7de", "mei\u00b7nes", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Beschlie\u00dft ein Defizit.", "tokens": ["Be\u00b7schlie\u00dft", "ein", "De\u00b7fi\u00b7zit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Sonne erloschen am Himmel,", "tokens": ["Die", "Son\u00b7ne", "er\u00b7lo\u00b7schen", "am", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Luft war warm und schwer,", "tokens": ["Die", "Luft", "war", "warm", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom bunten Menschengewimmel", "tokens": ["Vom", "bun\u00b7ten", "Men\u00b7schen\u00b7ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wurden die Stra\u00dfen leer.", "tokens": ["Wur\u00b7den", "die", "Stra\u00b7\u00dfen", "leer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Am Rand des kleinen Flusses", "tokens": ["Am", "Rand", "des", "klei\u00b7nen", "Flus\u00b7ses"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ging ich nachdenklich hin,", "tokens": ["Ging", "ich", "nach\u00b7denk\u00b7lich", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da horch, der Schall eines Kusses", "tokens": ["Da", "horch", ",", "der", "Schall", "ei\u00b7nes", "Kus\u00b7ses"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und Liebesseufzer ziehn.", "tokens": ["Und", "Lie\u00b7bes\u00b7seuf\u00b7zer", "ziehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Am Fu\u00df der alten Esche", "tokens": ["Am", "Fu\u00df", "der", "al\u00b7ten", "E\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Eine breite Holzbank steht,", "tokens": ["Ei\u00b7ne", "brei\u00b7te", "Holz\u00b7bank", "steht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Es schimmerte wei\u00dfe W\u00e4sche,", "tokens": ["Es", "schim\u00b7mer\u00b7te", "wei\u00b7\u00dfe", "W\u00e4\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ich hab' mich umgedreht.", "tokens": ["Ich", "hab'", "mich", "um\u00b7ge\u00b7dreht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und leichter ward mir zumute", "tokens": ["Und", "leich\u00b7ter", "ward", "mir", "zu\u00b7mu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und froh ging ich zur\u00fcck,", "tokens": ["Und", "froh", "ging", "ich", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich dachte an mancher Minute,", "tokens": ["Ich", "dach\u00b7te", "an", "man\u00b7cher", "Mi\u00b7nu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "An mancher Stunde Gl\u00fcck.", "tokens": ["An", "man\u00b7cher", "Stun\u00b7de", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}