{"dta.poem.19146": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "CxXIV.  Gedancken bey dem Hymno Therest\u00e4:  \n  Virgo clamat, qvantum amat,  \n Uber eine junge Person, die gantz kranck nach  \n der Aufl\u00f6\u00dfung ist.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Die Jungfrau, die jetzt redte,", "tokens": ["Die", "Jung\u00b7frau", ",", "die", "jetzt", "red\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist eine Klette,", "tokens": ["Ist", "ei\u00b7ne", "Klet\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "An dem, der sie beredte,", "tokens": ["An", "dem", ",", "der", "sie", "be\u00b7red\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So sehr sie kan", "tokens": ["So", "sehr", "sie", "kan"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "VMFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Sie lieben in die Wette,", "tokens": ["Sie", "lie\u00b7ben", "in", "die", "Wet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie und ihr Mann:", "tokens": ["Sie", "und", "ihr", "Mann", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Sie denckt; ", "tokens": ["Sie", "denckt", ";"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Ich fl\u00f6g ins Bette,", "tokens": ["Ich", "fl\u00f6g", "ins", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Die Bau-Arbeiter-Kette,", "tokens": ["Die", "Bau\u00b7A\u00b7rbei\u00b7ter\u00b7Ket\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Steht Jhr nicht an.", "tokens": ["Steht", "Ihr", "nicht", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Sie jagt im Streiter-Wagen,", "tokens": ["Sie", "jagt", "im", "Strei\u00b7ter\u00b7Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Man m\u00f6chte fragen:", "tokens": ["Man", "m\u00f6ch\u00b7te", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Was solche Seelen jagen", "tokens": ["Was", "sol\u00b7che", "See\u00b7len", "ja\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was will das Gl\u00fccke sagen", "tokens": ["Was", "will", "das", "Gl\u00fc\u00b7cke", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nach ihrer Sprach,", "tokens": ["Nach", "ih\u00b7rer", "Sprach", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Und abgetragen,", "tokens": ["Und", "ab\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "So endigt sich ihr Klagen", "tokens": ["So", "en\u00b7digt", "sich", "ihr", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und ihre Schmach.", "tokens": ["Und", "ih\u00b7re", "Schmach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Nun Seele! sey gelinde,", "tokens": ["Nun", "See\u00b7le", "!", "sey", "ge\u00b7lin\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$.", "VAFIN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein Wunsch ist S\u00fcnde;", "tokens": ["Dein", "Wunsch", "ist", "S\u00fcn\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Bedenck das Hau\u00df-Gesinde,", "tokens": ["Be\u00b7denck", "das", "Hau\u00df\u00b7Ge\u00b7sin\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Creutz-Gemein,", "tokens": ["Die", "Creutz\u00b7Ge\u00b7mein", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Verla\u00df nicht so geschwinde", "tokens": ["Ver\u00b7la\u00df", "nicht", "so", "ge\u00b7schwin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dein Fleisch und Bein.", "tokens": ["Dein", "Fleisch", "und", "Bein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Vern\u00fcnfftger Gr\u00fcnde,", "tokens": ["Ver\u00b7n\u00fcnfft\u00b7ger", "Gr\u00fcn\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Wo ich den Br\u00e4utgam finde,", "tokens": ["Wo", "ich", "den", "Br\u00e4ut\u00b7gam", "fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Da will ich seyn.", "tokens": ["Da", "will", "ich", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ist dieses dein Begehren,", "tokens": ["Ist", "die\u00b7ses", "dein", "Be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So still die Z\u00e4hren,", "tokens": ["So", "still", "die", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Das wird dir niemand w\u00e4hren,", "tokens": ["Das", "wird", "dir", "nie\u00b7mand", "w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du hast den HErrn,", "tokens": ["Du", "hast", "den", "Herrn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Gewi\u00df nicht fern,", "tokens": ["Ge\u00b7wi\u00df", "nicht", "fern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Das Korn von Aehren,", "tokens": ["Das", "Korn", "von", "A\u00b7eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Der Safft von seinen Beeren,", "tokens": ["Der", "Safft", "von", "sei\u00b7nen", "Bee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Vom Keim der ", "tokens": ["Vom", "Keim", "der"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "Komm Freund in deinen Garten,", "tokens": ["Komm", "Freund", "in", "dei\u00b7nen", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So will ich warten,", "tokens": ["So", "will", "ich", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und bey der M\u00fch erharten", "tokens": ["Und", "bey", "der", "M\u00fch", "er\u00b7har\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ohn alle Ruh.", "tokens": ["Ohn", "al\u00b7le", "Ruh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ich eyl mit Pflug und Barten,", "tokens": ["Ich", "eyl", "mit", "Pflug", "und", "Bar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dem Felde zu", "tokens": ["Dem", "Fel\u00b7de", "zu"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wie viel die vor mir karrten", "tokens": ["Wie", "viel", "die", "vor", "mir", "karr\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In Boden scharrten,", "tokens": ["In", "Bo\u00b7den", "scharr\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Von Streiter-Schwei\u00df erstarrten,", "tokens": ["Von", "Strei\u00b7ter\u00b7Schwei\u00df", "er\u00b7starr\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Genie\u00dfens Nu.", "tokens": ["Ge\u00b7nie\u00b7\u00dfens", "Nu", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}