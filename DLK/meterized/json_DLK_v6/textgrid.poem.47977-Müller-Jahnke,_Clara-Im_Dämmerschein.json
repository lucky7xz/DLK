{"textgrid.poem.47977": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Im D\u00e4mmerschein", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verronnen ist der schw\u00fcle Tag,", "tokens": ["Ver\u00b7ron\u00b7nen", "ist", "der", "schw\u00fc\u00b7le", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "verrauscht ist Sturm und Wetterschlag,", "tokens": ["ver\u00b7rauscht", "ist", "Sturm", "und", "Wet\u00b7ter\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und durch die regenfeuchte Luft", "tokens": ["und", "durch", "die", "re\u00b7gen\u00b7feuch\u00b7te", "Luft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "weht tr\u00e4umerischer Lindenduft; \u2013", "tokens": ["weht", "tr\u00e4u\u00b7me\u00b7ri\u00b7scher", "Lin\u00b7den\u00b7duft", ";", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "es spinnt die Welt ein Zauber ein:", "tokens": ["es", "spinnt", "die", "Welt", "ein", "Zau\u00b7ber", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich harre dein!", "tokens": ["Ich", "har\u00b7re", "dein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ich harre dein seit langer Zeit;", "tokens": ["Ich", "har\u00b7re", "dein", "seit", "lan\u00b7ger", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "gewintert hat es und gemait, \u2013", "tokens": ["ge\u00b7win\u00b7tert", "hat", "es", "und", "ge\u00b7mait", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "KON", "ADJD", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "f\u00fcr jede Rose, die erblich,", "tokens": ["f\u00fcr", "je\u00b7de", "Ro\u00b7se", ",", "die", "er\u00b7blich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "entfaltet eine andre sich;", "tokens": ["ent\u00b7fal\u00b7tet", "ei\u00b7ne", "and\u00b7re", "sich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "aus jeder Nacht bricht Fr\u00fchrotsschein:", "tokens": ["aus", "je\u00b7der", "Nacht", "bricht", "Fr\u00fch\u00b7rots\u00b7schein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "ich harre dein!", "tokens": ["ich", "har\u00b7re", "dein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ich harre dein am alten Platz, \u2013", "tokens": ["Ich", "har\u00b7re", "dein", "am", "al\u00b7ten", "Platz", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "APPRART", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und wei\u00dft du's noch, herzlieber Schatz,", "tokens": ["und", "wei\u00dft", "du's", "noch", ",", "herz\u00b7lie\u00b7ber", "Schatz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "wei\u00dft noch, wie du vor Jahresfrist", "tokens": ["wei\u00dft", "noch", ",", "wie", "du", "vor", "Jah\u00b7res\u00b7frist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "PWAV", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "allabendlich gekommen bist?", "tokens": ["al\u00b7la\u00b7bend\u00b7lich", "ge\u00b7kom\u00b7men", "bist", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Allabendlich im D\u00e4mmerschein", "tokens": ["Al\u00b7la\u00b7bend\u00b7lich", "im", "D\u00e4m\u00b7mer\u00b7schein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "ich harrte dein!", "tokens": ["ich", "harr\u00b7te", "dein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Nun d\u00fcnkt's mich fast ein s\u00fc\u00dfer Traum;", "tokens": ["Nun", "d\u00fcnkt's", "mich", "fast", "ein", "s\u00fc\u00b7\u00dfer", "Traum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "vorm Haus der alte Lindenbaum,", "tokens": ["vorm", "Haus", "der", "al\u00b7te", "Lin\u00b7den\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die alte Sehnsucht in der Brust", "tokens": ["die", "al\u00b7te", "Sehn\u00b7sucht", "in", "der", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "nach M\u00e4rchenzauber, Liebeslust \u2013", "tokens": ["nach", "M\u00e4r\u00b7chen\u00b7zau\u00b7ber", ",", "Lie\u00b7bes\u00b7lust", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und rings die Welt im D\u00e4mmerschein", "tokens": ["und", "rings", "die", "Welt", "im", "D\u00e4m\u00b7mer\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und ich allein! \u2013", "tokens": ["und", "ich", "al\u00b7lein", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "ADV", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Und unten tief im B\u00f6hmerland", "tokens": ["Und", "un\u00b7ten", "tief", "im", "B\u00f6h\u00b7mer\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ein St\u00e4dtchen liegt an Bergesrand;", "tokens": ["ein", "St\u00e4dt\u00b7chen", "liegt", "an", "Ber\u00b7ges\u00b7rand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "der letzte feuchte Abendstrahl", "tokens": ["der", "letz\u00b7te", "feuch\u00b7te", "A\u00b7bend\u00b7strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "k\u00fc\u00dft Meeresstrand und Felsental \u2013", "tokens": ["k\u00fc\u00dft", "Mee\u00b7res\u00b7strand", "und", "Fel\u00b7sen\u00b7tal", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "es spinnt auch dich der Zauber ein:", "tokens": ["es", "spinnt", "auch", "dich", "der", "Zau\u00b7ber", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gedenkst du mein?", "tokens": ["Ge\u00b7denkst", "du", "mein", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}