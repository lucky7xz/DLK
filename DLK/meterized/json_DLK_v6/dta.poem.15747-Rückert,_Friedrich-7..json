{"dta.poem.15747": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Dichter, der nur ist ein Dichter, ist ein Kind", "tokens": ["Der", "Dich\u00b7ter", ",", "der", "nur", "ist", "ein", "Dich\u00b7ter", ",", "ist", "ein", "Kind"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VAFIN", "ART", "NN", "$,", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dieser Zeit, da wir gereift zu M\u00e4nnern sind.", "tokens": ["In", "die\u00b7ser", "Zeit", ",", "da", "wir", "ge\u00b7reift", "zu", "M\u00e4n\u00b7nern", "sind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "KOUS", "PPER", "ADV", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Kind bleiben, ist nicht schlimm, nicht schlimm es wieder werden,", "tokens": ["Kind", "blei\u00b7ben", ",", "ist", "nicht", "schlimm", ",", "nicht", "schlimm", "es", "wie\u00b7der", "wer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "VAFIN", "PTKNEG", "ADJD", "$,", "PTKNEG", "ADJD", "PPER", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur schlimm sind kindische statt kindlicher Geberden.", "tokens": ["Nur", "schlimm", "sind", "kin\u00b7di\u00b7sche", "statt", "kind\u00b7li\u00b7cher", "Ge\u00b7ber\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was aber seid denn ihr, die ihr so m\u00e4nnlich thut,", "tokens": ["Was", "a\u00b7ber", "seid", "denn", "ihr", ",", "die", "ihr", "so", "m\u00e4nn\u00b7lich", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "KON", "PPER", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So gr\u00e4mlich ernst aufs Spiel herabzusehn geruht?", "tokens": ["So", "gr\u00e4m\u00b7lich", "ernst", "aufs", "Spiel", "her\u00b7ab\u00b7zu\u00b7sehn", "ge\u00b7ruht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "APPRART", "NN", "VVIZU", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ihr glaubet euch gereift, und seid doch nur verbl\u00fcht,", "tokens": ["Ihr", "glau\u00b7bet", "euch", "ge\u00b7reift", ",", "und", "seid", "doch", "nur", "ver\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "$,", "KON", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Froste nur bereift, anstatt vom Thau bespr\u00fcht.", "tokens": ["Vom", "Fros\u00b7te", "nur", "be\u00b7reift", ",", "an\u00b7statt", "vom", "Thau", "be\u00b7spr\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "$,", "KOUI", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Was w\u00e4r' ein rechter Mann? der mit dem Kern sich n\u00e4hrte", "tokens": ["Was", "w\u00e4r'", "ein", "rech\u00b7ter", "Mann", "?", "der", "mit", "dem", "Kern", "sich", "n\u00e4hr\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$.", "ART", "APPR", "ART", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der ganzen Wissenschaft, und den zu Kunst verkl\u00e4rte;", "tokens": ["Der", "gan\u00b7zen", "Wis\u00b7sen\u00b7schaft", ",", "und", "den", "zu", "Kunst", "ver\u00b7kl\u00e4r\u00b7te", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ART", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der machte ganz die Welt \u2014 Bruchst\u00fccke mein' ich nicht \u2014", "tokens": ["Der", "mach\u00b7te", "ganz", "die", "Welt", "Bruchs\u00b7t\u00fc\u00b7cke", "mein'", "ich", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "$(", "NN", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Zu einem reizenden und lehrenden Gedicht.", "tokens": ["Zu", "ei\u00b7nem", "rei\u00b7zen\u00b7den", "und", "leh\u00b7ren\u00b7den", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}