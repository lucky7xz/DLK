{"textgrid.poem.60181": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier auf diesem Aschen-Kruge,", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier auf diesem Aschen-Kruge,", "tokens": ["Hier", "auf", "die\u00b7sem", "A\u00b7schen\u00b7Kru\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weint die Freundschaft ihren Schmerz", "tokens": ["Weint", "die", "Freund\u00b7schaft", "ih\u00b7ren", "Schmerz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit diamantnem Pfluge,", "tokens": ["Und", "mit", "di\u00b7a\u00b7mant\u00b7nem", "Pflu\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Zieht der Kummer Furchen in mein Herz.", "tokens": ["Zieht", "der", "Kum\u00b7mer", "Fur\u00b7chen", "in", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Finsterni\u00df und Stille,", "tokens": ["Fins\u00b7ter\u00b7ni\u00df", "und", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Unter eurer H\u00fclle,", "tokens": ["Un\u00b7ter", "eu\u00b7rer", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Lad' ich Erd und Himmel zum Geh\u00f6r", "tokens": ["Lad'", "ich", "Erd", "und", "Him\u00b7mel", "zum", "Ge\u00b7h\u00f6r"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "NN", "KON", "NN", "APPRART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Klagen will ich \u2013 ach mein Liebling,", "tokens": ["Kla\u00b7gen", "will", "ich", "\u2013", "ach", "mein", "Lieb\u00b7ling", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$(", "XY", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ist nicht mehr.", "tokens": ["Ist", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Hingeblutet ward sein Leben", "tokens": ["Hin\u00b7ge\u00b7blu\u00b7tet", "ward", "sein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mein Gedanke rief dem Tode zu:", "tokens": ["Mein", "Ge\u00b7dan\u00b7ke", "rief", "dem", "To\u00b7de", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "La\u00df dir kleinre Opfer geben!", "tokens": ["La\u00df", "dir", "klein\u00b7re", "Op\u00b7fer", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00fcrger, noch nicht satt gemacht bist du,", "tokens": ["W\u00fcr\u00b7ger", ",", "noch", "nicht", "satt", "ge\u00b7macht", "bist", "du", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "PTKNEG", "ADJD", "VVPP", "VAFIN", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Von den Myriaden,", "tokens": ["Von", "den", "My\u00b7ria\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "++-+-", "measure": "iambic.di"}, "line.6": {"text": "Die im Blute baden?", "tokens": ["Die", "im", "Blu\u00b7te", "ba\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "O Verheerer, wenns dein Hunger hei\u00dft,", "tokens": ["O", "Ver\u00b7hee\u00b7rer", ",", "wenns", "dein", "Hun\u00b7ger", "hei\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Nimm mich selber, nur verschone,", "tokens": ["Nimm", "mich", "sel\u00b7ber", ",", "nur", "ver\u00b7scho\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Meinen Kleist!", "tokens": ["Mei\u00b7nen", "Kleist", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Erde die sein Blut getrunken,", "tokens": ["Er\u00b7de", "die", "sein", "Blut", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie beneid ich diesen Tropfen dir!", "tokens": ["Wie", "be\u00b7neid", "ich", "die\u00b7sen", "Trop\u00b7fen", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PDAT", "NN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und du Thal wo er gesunken", "tokens": ["Und", "du", "Thal", "wo", "er", "ge\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "NN", "PWAV", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schauervoll und heilig bist du mir!", "tokens": ["Schau\u00b7er\u00b7voll", "und", "hei\u00b7lig", "bist", "du", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ach an dieser St\u00e4te,", "tokens": ["Ach", "an", "die\u00b7ser", "St\u00e4\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Werd auf mein Gebete,", "tokens": ["Werd", "auf", "mein", "Ge\u00b7be\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Eine Quelle, der des Wandrers Dank", "tokens": ["Ei\u00b7ne", "Quel\u00b7le", ",", "der", "des", "Wand\u00b7rers", "Dank"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Seegen l\u00e4chelt, wenn er schmachtend,", "tokens": ["See\u00b7gen", "l\u00e4\u00b7chelt", ",", "wenn", "er", "schmach\u00b7tend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Aus dir trank.", "tokens": ["Aus", "dir", "trank", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Also traurig, wie den Br\u00e4uten", "tokens": ["Al\u00b7so", "trau\u00b7rig", ",", "wie", "den", "Br\u00e4u\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die der Schlachten Schicksal hart betraf,", "tokens": ["Die", "der", "Schlach\u00b7ten", "Schick\u00b7sal", "hart", "be\u00b7traf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "ADJD", "VVFIN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Ist mir alles; mich erfreuten", "tokens": ["Ist", "mir", "al\u00b7les", ";", "mich", "er\u00b7freu\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "PIS", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sonst die Lorbeern um des Helden Schlaf;", "tokens": ["Sonst", "die", "Lor\u00b7beern", "um", "des", "Hel\u00b7den", "Schlaf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Aber jetzo stehen,", "tokens": ["A\u00b7ber", "jet\u00b7zo", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Selber die Troph\u00e4en,", "tokens": ["Sel\u00b7ber", "die", "Trop\u00b7h\u00e4\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.7": {"text": "Im Gem\u00e4hlde, mir zum Schrecken da,", "tokens": ["Im", "Ge\u00b7m\u00e4hl\u00b7de", ",", "mir", "zum", "Schre\u00b7cken", "da", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Und der Ruhm auf den mein Liebster", "tokens": ["Und", "der", "Ruhm", "auf", "den", "mein", "Liebs\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Sterbend sah!", "tokens": ["Ster\u00b7bend", "sah", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Wilder Krieg dich mu\u00df ich hassen,", "tokens": ["Wil\u00b7der", "Krieg", "dich", "mu\u00df", "ich", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mehr als M\u00fctter, die du ganz beraubt;", "tokens": ["Mehr", "als", "M\u00fct\u00b7ter", ",", "die", "du", "ganz", "be\u00b7raubt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Jede Lust hat mich verlassen", "tokens": ["Je\u00b7de", "Lust", "hat", "mich", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Trauer w\u00f6lkt sich um mein Haupt,", "tokens": ["Und", "die", "Trau\u00b7er", "w\u00f6lkt", "sich", "um", "mein", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Wenn ich Freuden l\u00fcge,", "tokens": ["Wenn", "ich", "Freu\u00b7den", "l\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Und die Welt betr\u00fcge", "tokens": ["Und", "die", "Welt", "be\u00b7tr\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "In dem Munde, der zu lachen scheint,", "tokens": ["In", "dem", "Mun\u00b7de", ",", "der", "zu", "la\u00b7chen", "scheint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Ach da f\u00fchlt mein blutend Herze,", "tokens": ["Ach", "da", "f\u00fchlt", "mein", "blu\u00b7tend", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "PPOSAT", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Da\u00df es weint.", "tokens": ["Da\u00df", "es", "weint", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Zwar des Fr\u00fchlings Foderungen", "tokens": ["Zwar", "des", "Fr\u00fch\u00b7lings", "Fo\u00b7de\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich zu freuen, die verwerf' ich nicht,", "tokens": ["Mich", "zu", "freu\u00b7en", ",", "die", "ver\u00b7wer\u00b7f'", "ich", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PRELS", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Weil von dem, der ihn gesungen,", "tokens": ["Weil", "von", "dem", ",", "der", "ihn", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedes Blat und jede Blume spricht;", "tokens": ["Je\u00b7des", "Blat", "und", "je\u00b7de", "Blu\u00b7me", "spricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Doch in dieser Freude,", "tokens": ["Doch", "in", "die\u00b7ser", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Nur geborgtem Kleide", "tokens": ["Nur", "ge\u00b7borg\u00b7tem", "Klei\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Gehet der ern\u00e4hrte Gram versteckt,", "tokens": ["Ge\u00b7het", "der", "er\u00b7n\u00e4hr\u00b7te", "Gram", "ver\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Den der Lenz zu neuen Klagen", "tokens": ["Den", "der", "Lenz", "zu", "neu\u00b7en", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Lockend weckt.", "tokens": ["Lo\u00b7ckend", "weckt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Ach in jenen goldnen Jahren,", "tokens": ["Ach", "in", "je\u00b7nen", "gold\u00b7nen", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blieben Gl\u00fcck und Freude mir getreu,", "tokens": ["Blie\u00b7ben", "Gl\u00fcck", "und", "Freu\u00b7de", "mir", "ge\u00b7treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Die in deinem Umgang waren,", "tokens": ["Die", "in", "dei\u00b7nem", "Um\u00b7gang", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und kein Tag ging ohne dich vorbey!", "tokens": ["Und", "kein", "Tag", "ging", "oh\u00b7ne", "dich", "vor\u00b7bey", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Du! der meinem Leben,", "tokens": ["Du", "!", "der", "mei\u00b7nem", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "ART", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Gr\u00f6ssern Werth gegeben;", "tokens": ["Gr\u00f6s\u00b7sern", "Werth", "ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Niemahls liebten zweene Br\u00fcder sich,", "tokens": ["Nie\u00b7mahls", "lieb\u00b7ten", "zwee\u00b7ne", "Br\u00fc\u00b7der", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "PRF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "So, als wir vereinte Wesen,", "tokens": ["So", ",", "als", "wir", "ver\u00b7ein\u00b7te", "We\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "VVFIN", "NN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.9": {"text": "Du und ich!", "tokens": ["Du", "und", "ich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.8": {"line.1": {"text": "O du hast gelebt mein S\u00fcsser!", "tokens": ["O", "du", "hast", "ge\u00b7lebt", "mein", "S\u00fcs\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "VVPP", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich blieb um dich zu weinen hier;", "tokens": ["Und", "ich", "blieb", "um", "dich", "zu", "wei\u00b7nen", "hier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPER", "PTKZU", "VVINF", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Keinen Trost hoff ich gewisser", "tokens": ["Kei\u00b7nen", "Trost", "hoff", "ich", "ge\u00b7wis\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als Befehle, die der Himmel mir", "tokens": ["Als", "Be\u00b7feh\u00b7le", ",", "die", "der", "Him\u00b7mel", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "PRELS", "ART", "NN", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "In des Todes H\u00e4nden", "tokens": ["In", "des", "To\u00b7des", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Eilet zuzusenden.", "tokens": ["Ei\u00b7let", "zu\u00b7zu\u00b7sen\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVIZU", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.7": {"text": "Meine Ungeduld erwartet sie,", "tokens": ["Mei\u00b7ne", "Un\u00b7ge\u00b7duld", "er\u00b7war\u00b7tet", "sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Dann sind unsre Seelen wieder", "tokens": ["Dann", "sind", "uns\u00b7re", "See\u00b7len", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Harmonie!", "tokens": ["Har\u00b7mo\u00b7nie", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}