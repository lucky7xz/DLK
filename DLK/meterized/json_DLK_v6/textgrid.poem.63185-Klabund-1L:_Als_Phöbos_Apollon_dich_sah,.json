{"textgrid.poem.63185": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als Ph\u00f6bos Apollon dich sah,", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.57", "en:0.28", "id:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Ph\u00f6bos Apollon dich sah,", "tokens": ["Als", "Ph\u00f6\u00b7bos", "A\u00b7pol\u00b7lon", "dich", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Adonis,", "tokens": ["A\u00b7do\u00b7nis", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "---", "measure": "unknown.measure.zero"}, "line.3": {"text": "Ergriff seine Seele ein seliger Schmerz.", "tokens": ["Er\u00b7griff", "sei\u00b7ne", "See\u00b7le", "ein", "se\u00b7li\u00b7ger", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Nicht freute ihn der Gesang der Mysten", "tokens": ["Nicht", "freu\u00b7te", "ihn", "der", "Ge\u00b7sang", "der", "Mys\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und nicht das Opfer im ragenden Heiligtum.", "tokens": ["Und", "nicht", "das", "Op\u00b7fer", "im", "ra\u00b7gen\u00b7den", "Hei\u00b7lig\u00b7tum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Er trat als Bettler staubig vor die Sibylle,", "tokens": ["Er", "trat", "als", "Bett\u00b7ler", "stau\u00b7big", "vor", "die", "Si\u00b7byl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die weissagende,", "tokens": ["Die", "weis\u00b7sa\u00b7gen\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und sprach:", "tokens": ["Und", "sprach", ":"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Sage mir das Geschick des Knaben Adonis!", "tokens": ["Sa\u00b7ge", "mir", "das", "Ge\u00b7schick", "des", "Kna\u00b7ben", "A\u00b7do\u00b7nis", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ART", "NN", "NE", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Die heiligen Nebel wallten,", "tokens": ["Die", "hei\u00b7li\u00b7gen", "Ne\u00b7bel", "wall\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die s\u00fcssen D\u00fcfte str\u00f6mten,", "tokens": ["Die", "s\u00fcs\u00b7sen", "D\u00fcf\u00b7te", "str\u00f6m\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Pythia sprach:", "tokens": ["Die", "Py\u00b7thia", "sprach", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Der Knabe Adonis wird sterben", "tokens": ["Der", "Kna\u00b7be", "A\u00b7do\u00b7nis", "wird", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VAFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "An Liebe, die zu heftig liebt.", "tokens": ["An", "Lie\u00b7be", ",", "die", "zu", "hef\u00b7tig", "liebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da ging der Gott und ging durch die seufzenden Fluren", "tokens": ["Da", "ging", "der", "Gott", "und", "ging", "durch", "die", "seuf\u00b7zen\u00b7den", "Flu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und schritt in seinen Tempel", "tokens": ["Und", "schritt", "in", "sei\u00b7nen", "Tem\u00b7pel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Unerkannt", "tokens": ["Un\u00b7er\u00b7kannt"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Und setzte sich auf die steinernen Stufen", "tokens": ["Und", "setz\u00b7te", "sich", "auf", "die", "stei\u00b7ner\u00b7nen", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und weinte", "tokens": ["Und", "wein\u00b7te"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Das b\u00e4rtige Gesicht wie ein Igel", "tokens": ["Das", "b\u00e4r\u00b7ti\u00b7ge", "Ge\u00b7sicht", "wie", "ein", "I\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Im Strauchwerk der H\u00e4nde versteckt.", "tokens": ["Im", "Strauch\u00b7werk", "der", "H\u00e4n\u00b7de", "ver\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Als er das Antlitz hob,", "tokens": ["Als", "er", "das", "Ant\u00b7litz", "hob", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Waren seine H\u00e4nde", "tokens": ["Wa\u00b7ren", "sei\u00b7ne", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Voller Perlen.", "tokens": ["Vol\u00b7ler", "Per\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Heph\u00e4stos reihte sie", "tokens": ["He\u00b7ph\u00e4s\u00b7tos", "reih\u00b7te", "sie"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zu einer Kette.", "tokens": ["Zu", "ei\u00b7ner", "Ket\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Die brachte Hermes dem Knaben,", "tokens": ["Die", "brach\u00b7te", "Her\u00b7mes", "dem", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Als er die Ziegen weidete am Taygetos,", "tokens": ["Als", "er", "die", "Zie\u00b7gen", "wei\u00b7de\u00b7te", "am", "Tay\u00b7ge\u00b7tos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und hing sie ihm um den Hals,", "tokens": ["Und", "hing", "sie", "ihm", "um", "den", "Hals", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die Tr\u00e4nen des Gottes.", "tokens": ["Die", "Tr\u00e4\u00b7nen", "des", "Got\u00b7tes", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}