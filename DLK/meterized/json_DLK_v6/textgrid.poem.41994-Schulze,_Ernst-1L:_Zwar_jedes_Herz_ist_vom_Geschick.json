{"textgrid.poem.41994": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zwar jedes Herz ist vom Geschick", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwar jedes Herz ist vom Geschick", "tokens": ["Zwar", "je\u00b7des", "Herz", "ist", "vom", "Ge\u00b7schick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu zarter Sklaverei erkohren:", "tokens": ["Zu", "zar\u00b7ter", "Skla\u00b7ve\u00b7rei", "er\u00b7koh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch segn' ich stets den Augenblick,", "tokens": ["Doch", "segn'", "ich", "stets", "den", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der meine Herrscherinn geboren.", "tokens": ["Der", "mei\u00b7ne", "Herr\u00b7sche\u00b7rinn", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der eine dient um feiles Gold,", "tokens": ["Der", "ei\u00b7ne", "dient", "um", "fei\u00b7les", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Andre, da\u00df er Ruhm gewinne:", "tokens": ["Der", "And\u00b7re", ",", "da\u00df", "er", "Ruhm", "ge\u00b7win\u00b7ne", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch nimmer ist ein Dienst so hold", "tokens": ["Doch", "nim\u00b7mer", "ist", "ein", "Dienst", "so", "hold"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als der verschwiegne Dienst der Minne.", "tokens": ["Als", "der", "ver\u00b7schwieg\u00b7ne", "Dienst", "der", "Min\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sonst war mein Herz so leicht und frei,", "tokens": ["Sonst", "war", "mein", "Herz", "so", "leicht", "und", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Rosen kr\u00e4nzten meine Stunden:", "tokens": ["Und", "Ro\u00b7sen", "kr\u00e4nz\u00b7ten", "mei\u00b7ne", "Stun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch Scherz und Lachen sind vorbei,", "tokens": ["Doch", "Scherz", "und", "La\u00b7chen", "sind", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Seit deine Fesseln mich umwunden!", "tokens": ["Seit", "dei\u00b7ne", "Fes\u00b7seln", "mich", "um\u00b7wun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und ach, doch ist mein Schmerz so s\u00fc\u00df,", "tokens": ["Und", "ach", ",", "doch", "ist", "mein", "Schmerz", "so", "s\u00fc\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Viel s\u00fc\u00dfer als die sch\u00f6nsten Freuden,", "tokens": ["Viel", "s\u00fc\u00b7\u00dfer", "als", "die", "sch\u00f6ns\u00b7ten", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wahrlich, f\u00fcr kein Paradies", "tokens": ["Und", "wahr\u00b7lich", ",", "f\u00fcr", "kein", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "M\u00f6cht' ich aus diesen Fesseln scheiden.", "tokens": ["M\u00f6cht'", "ich", "aus", "die\u00b7sen", "Fes\u00b7seln", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nimmst du, o g\u00fctiges Geschick,", "tokens": ["Nimmst", "du", ",", "o", "g\u00fc\u00b7ti\u00b7ges", "Ge\u00b7schick", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was Jeder von uns w\u00fcnscht, zu Herzen,", "tokens": ["Was", "Je\u00b7der", "von", "uns", "w\u00fcnscht", ",", "zu", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPR", "PPER", "VVFIN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So kr\u00e4nze sie mit ew'gem Gl\u00fcck,", "tokens": ["So", "kr\u00e4n\u00b7ze", "sie", "mit", "ew'\u00b7gem", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mir la\u00df' ewig meine Schmerzen.", "tokens": ["Und", "mir", "la\u00df'", "e\u00b7wig", "mei\u00b7ne", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}