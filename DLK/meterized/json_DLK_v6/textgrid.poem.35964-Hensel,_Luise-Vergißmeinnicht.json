{"textgrid.poem.35964": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "Vergi\u00dfmeinnicht", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bl\u00fchete so still und rein", "tokens": ["Ich", "bl\u00fc\u00b7he\u00b7te", "so", "still", "und", "rein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An B\u00e4chleins Ufer hier,", "tokens": ["An", "B\u00e4ch\u00b7leins", "U\u00b7fer", "hier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schaute in die Fluth hinein", "tokens": ["Und", "schau\u00b7te", "in", "die", "Fluth", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und spiegelt' mich in ihr.", "tokens": ["Und", "spie\u00b7gelt'", "mich", "in", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da suchte mich die Schwester dein,", "tokens": ["Da", "such\u00b7te", "mich", "die", "Schwes\u00b7ter", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die weiht' mich bittend dir.", "tokens": ["Die", "weiht'", "mich", "bit\u00b7tend", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVPP", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}