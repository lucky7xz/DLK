{"dta.poem.2807": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das G\u00e4hnen, das, mein Sohn, beim Lernen dich beschleicht,", "tokens": ["Das", "G\u00e4h\u00b7nen", ",", "das", ",", "mein", "Sohn", ",", "beim", "Ler\u00b7nen", "dich", "be\u00b7schleicht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "$,", "PPOSAT", "NN", "$,", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Zeichen ist es, da\u00df Aufmerksamkeit entweicht.", "tokens": ["Ein", "Zei\u00b7chen", "ist", "es", ",", "da\u00df", "Auf\u00b7merk\u00b7sam\u00b7keit", "ent\u00b7weicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es zu verbei\u00dfen hilft auch gar nicht mit den Z\u00e4hnen,", "tokens": ["Es", "zu", "ver\u00b7bei\u00b7\u00dfen", "hilft", "auch", "gar", "nicht", "mit", "den", "Z\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "VVFIN", "ADV", "ADV", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du nicht innerlich bezwingen kannst das G\u00e4hnen.", "tokens": ["Wenn", "du", "nicht", "in\u00b7ner\u00b7lich", "be\u00b7zwin\u00b7gen", "kannst", "das", "G\u00e4h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "VVINF", "VMFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Bei aufgesperrtem Mund ist selbst das Ohr geschlossen", "tokens": ["Bei", "auf\u00b7ge\u00b7sperr\u00b7tem", "Mund", "ist", "selbst", "das", "Ohr", "ge\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das \u00e4u\u00dfre, mehr noch ist das innre dann verdrossen.", "tokens": ["Das", "\u00e4u\u00df\u00b7re", ",", "mehr", "noch", "ist", "das", "inn\u00b7re", "dann", "ver\u00b7dros\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADV", "ADV", "VAFIN", "ART", "ADJA", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Noch einmal denn versuch' in muthiger Ermannung,", "tokens": ["Noch", "ein\u00b7mal", "denn", "ver\u00b7such'", "in", "mut\u00b7hi\u00b7ger", "Er\u00b7man\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ob du erhalten kannst den Geist in rechter Spannung;", "tokens": ["Ob", "du", "er\u00b7hal\u00b7ten", "kannst", "den", "Geist", "in", "rech\u00b7ter", "Span\u00b7nung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wo nicht, so lassen wir es lieber heute ruhn:", "tokens": ["Wo", "nicht", ",", "so", "las\u00b7sen", "wir", "es", "lie\u00b7ber", "heu\u00b7te", "ruhn", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn besser ist, als schlecht, die Arbeit gar nicht thun.", "tokens": ["Denn", "bes\u00b7ser", "ist", ",", "als", "schlecht", ",", "die", "Ar\u00b7beit", "gar", "nicht", "thun", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,", "KOUS", "ADJD", "$,", "ART", "NN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}