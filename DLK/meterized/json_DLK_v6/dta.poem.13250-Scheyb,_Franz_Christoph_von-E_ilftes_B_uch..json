{"dta.poem.13250": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Tugend eilte fort: der Saal blieb unterdessen", "tokens": ["Die", "Tu\u00b7gend", "eil\u00b7te", "fort", ":", "der", "Saal", "blieb", "un\u00b7ter\u00b7des\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Kostbarkeit des Stabs nur heimlich zu ermessen;", "tokens": ["Die", "Kost\u00b7bar\u00b7keit", "des", "Stabs", "nur", "heim\u00b7lich", "zu", "er\u00b7mes\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Kreuz/ welches man das\nFerdinandeische zu nennen pflegt/ weil\n\nKayser Ferdin. II. in der \u00e4ussersten Ge-\nfahr ganz besondern Trost dabey fand.", "tokens": ["Das", "Kreuz", "/", "wel\u00b7ches", "man", "das", "Fer\u00b7di\u00b7nan\u00b7de\u00b7i\u00b7sche", "zu", "nen\u00b7nen", "pflegt", "/", "weil", "Kay\u00b7ser", "Fer\u00b7din", ".", "I\u00b7i", ".", "in", "der", "\u00e4us\u00b7sers\u00b7ten", "Ge", "fahr", "ganz", "be\u00b7son\u00b7dern", "Trost", "da\u00b7bey", "fand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWS", "PIS", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$(", "KOUS", "NN", "NE", "$.", "CARD", "$.", "APPR", "ART", "ADJA", "NN", "VVFIN", "ADV", "ADJA", "NN", "PAV", "VVFIN", "$."], "meter": "-+--+-+-+-+-+--+-+-+-+-+-+---+--+-+--+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Man s\u00e4hnte sich darnach. Sieh da! von ungef\u00e4hr", "tokens": ["Man", "s\u00e4hn\u00b7te", "sich", "dar\u00b7nach", ".", "Sieh", "da", "!", "von", "un\u00b7ge\u00b7f\u00e4hr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "PRF", "PAV", "$.", "NE", "PTKVZ", "$.", "APPR", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dringt ein erweckter Lerm von Ohr zu Ohr daher.", "tokens": ["Dringt", "ein", "er\u00b7weck\u00b7ter", "Lerm", "von", "Ohr", "zu", "Ohr", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wo man von diesem Saal konnt auf die Gasse sehen,", "tokens": ["Wo", "man", "von", "die\u00b7sem", "Saal", "konnt", "auf", "die", "Gas\u00b7se", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "PDAT", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "240Dort sieht man ein Gedr\u00e4ng, ein Lust-Get\u00f6\u00df entstehen;", "tokens": ["sieht", "man", "ein", "Ge\u00b7dr\u00e4ng", ",", "ein", "Lust\u00b7Ge\u00b7t\u00f6\u00df", "ent\u00b7ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Man rei\u00dft die Fenster auf; und viele dringen hin", "tokens": ["Man", "rei\u00dft", "die", "Fens\u00b7ter", "auf", ";", "und", "vie\u00b7le", "drin\u00b7gen", "hin"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "PTKVZ", "$.", "KON", "PIS", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Als wann ein Wunderwerck vor dem Palast erschien.", "tokens": ["Als", "wann", "ein", "Wun\u00b7der\u00b7werck", "vor", "dem", "Pa\u00b7last", "er\u00b7schien", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Man sieht befremdet an, wie sich der Saal verwirret,", "tokens": ["Man", "sieht", "be\u00b7frem\u00b7det", "an", ",", "wie", "sich", "der", "Saal", "ver\u00b7wir\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJD", "PTKVZ", "$,", "PWAV", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie die Neugierigkeit durch alle Reihen irret.", "tokens": ["Wie", "die", "Neu\u00b7gie\u00b7rig\u00b7keit", "durch", "al\u00b7le", "Rei\u00b7hen", "ir\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "245Ich wei\u00df nicht, wie geschwind sich alles lebhaft macht;", "tokens": ["wei\u00df", "nicht", ",", "wie", "ge\u00b7schwind", "sich", "al\u00b7les", "leb\u00b7haft", "macht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "ADJD", "PRF", "PIS", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.13": {"text": "Man lispelt, redet, horcht; man s\u00e4hnt sich, lauft und lacht.", "tokens": ["Man", "lis\u00b7pelt", ",", "re\u00b7det", ",", "horcht", ";", "man", "s\u00e4hnt", "sich", ",", "lauft", "und", "lacht", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$.", "PIS", "VVFIN", "PRF", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Es fragt die K\u00f6niginn den Rath was es bedeute?", "tokens": ["Es", "fragt", "die", "K\u00f6\u00b7ni\u00b7ginn", "den", "Rath", "was", "es", "be\u00b7deu\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Was dann den Saal so schnell zu diesem Lerm verleite?", "tokens": ["Was", "dann", "den", "Saal", "so", "schnell", "zu", "die\u00b7sem", "Lerm", "ver\u00b7lei\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "ADV", "ADJD", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So h\u00f6r\u2019 ich, da\u00df der Grei\u00df selbst voller Wunder spricht:", "tokens": ["So", "h\u00f6r'", "ich", ",", "da\u00df", "der", "Grei\u00df", "selbst", "vol\u00b7ler", "Wun\u00b7der", "spricht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "250Der Zufall sey ihm neu, er wi\u00df' es selber nicht.", "tokens": ["Zu\u00b7fall", "sey", "ihm", "neu", ",", "er", "wi\u00df'", "es", "sel\u00b7ber", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "$,", "PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.18": {"text": "Er geht von dorten fort, vielleicht es zu ergr\u00fcnden,", "tokens": ["Er", "geht", "von", "dor\u00b7ten", "fort", ",", "viel\u00b7leicht", "es", "zu", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "PTKVZ", "$,", "ADV", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Warum dann die\u00df Ger\u00e4usch, die Regungen entst\u00fcnden.", "tokens": ["Wa\u00b7rum", "dann", "die\u00df", "Ge\u00b7r\u00e4usch", ",", "die", "Re\u00b7gun\u00b7gen", "ent\u00b7st\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PDS", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-----+--+--+-", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Weil man nichts trauriges, nur Freuden-Zeichen sp\u00fchrt,", "tokens": ["Weil", "man", "nichts", "trau\u00b7ri\u00b7ges", ",", "nur", "Freu\u00b7den\u00b7Zei\u00b7chen", "sp\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PIS", "ADJA", "$,", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So zeiget jedes Herz sich desto mehr ger\u00fchrt.", "tokens": ["So", "zei\u00b7get", "je\u00b7des", "Herz", "sich", "des\u00b7to", "mehr", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "PRF", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "255Indessen h\u00f6rt man nur ein Wort von Adler-Schaaren:", "tokens": ["h\u00f6rt", "man", "nur", "ein", "Wort", "von", "Ad\u00b7ler\u00b7Schaa\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "So will fast jeder Wiz dadurch was offenbaren.", "tokens": ["So", "will", "fast", "je\u00b7der", "Wiz", "da\u00b7durch", "was", "of\u00b7fen\u00b7ba\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PIAT", "NN", "PAV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der Alte kommt zur\u00fcck, und mit vergn\u00fcgtem Sinn", "tokens": ["Der", "Al\u00b7te", "kommt", "zu\u00b7r\u00fcck", ",", "und", "mit", "ver\u00b7gn\u00fcg\u00b7tem", "Sinn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Naht er sich zu dem Thron und sagt der K\u00f6niginn:", "tokens": ["Naht", "er", "sich", "zu", "dem", "Thron", "und", "sagt", "der", "K\u00f6\u00b7ni\u00b7ginn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "APPR", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Es sey ein Adler-Schwarm auf den Palast geflogen,", "tokens": ["Es", "sey", "ein", "Ad\u00b7ler\u00b7Schwarm", "auf", "den", "Pa\u00b7last", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "260Das habe diesen Saal, auch schon das Volck bewogen.", "tokens": ["ha\u00b7be", "die\u00b7sen", "Saal", ",", "auch", "schon", "das", "Volck", "be\u00b7wo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "$,", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.28": {"text": "Zugleich f\u00e4ngt \u00fcberall ein neues Murmeln an,", "tokens": ["Zu\u00b7gleich", "f\u00e4ngt", "\u00fc\u00b7be\u00b7rall", "ein", "neu\u00b7es", "Mur\u00b7meln", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Man zweifelt, fragt und r\u00e4tht, was es bedeuten kann.", "tokens": ["Man", "zwei\u00b7felt", ",", "fragt", "und", "r\u00e4\u00b7tht", ",", "was", "es", "be\u00b7deu\u00b7ten", "kann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}