{"textgrid.poem.24970": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Burg Rodenstein", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jahre sind's, und doch mit Schauern", "tokens": ["Jah\u00b7re", "sin\u00b7d's", ",", "und", "doch", "mit", "Schau\u00b7ern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "$,", "KON", "ADV", "APPR", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Denk' ich noch an jene Stunden,", "tokens": ["Denk'", "ich", "noch", "an", "je\u00b7ne", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da wir in den d\u00fcstern Mauern", "tokens": ["Da", "wir", "in", "den", "d\u00fcs\u00b7tern", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Deiner Ahnenburg gehaust,", "tokens": ["Dei\u00b7ner", "Ah\u00b7nen\u00b7burg", "ge\u00b7haust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jener Burg, in deren T\u00fcrmen", "tokens": ["Je\u00b7ner", "Burg", ",", "in", "de\u00b7ren", "T\u00fcr\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "APPR", "PRELAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sonst allein die Dohlen nisten,", "tokens": ["Sonst", "al\u00b7lein", "die", "Doh\u00b7len", "nis\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die der Hauch von Winterst\u00fcrmen", "tokens": ["Die", "der", "Hauch", "von", "Win\u00b7ter\u00b7st\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In Novembernacht durchsaust.", "tokens": ["In", "No\u00b7vem\u00b7ber\u00b7nacht", "durch\u00b7saust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Finster h\u00e4ngt sie, zeitgetroffen,", "tokens": ["Fins\u00b7ter", "h\u00e4ngt", "sie", ",", "zeit\u00b7ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An des Bergen kahlem Scheitel,", "tokens": ["An", "des", "Ber\u00b7gen", "kah\u00b7lem", "Schei\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ringsum W\u00fcste, nur am schroffen", "tokens": ["Ring\u00b7sum", "W\u00fcs\u00b7te", ",", "nur", "am", "schrof\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ADV", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Felsenhang ein F\u00f6hrenhain;", "tokens": ["Fel\u00b7sen\u00b7hang", "ein", "F\u00f6h\u00b7ren\u00b7hain", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihre Zinnen, spukhaft ragend,", "tokens": ["Ih\u00b7re", "Zin\u00b7nen", ",", "spuk\u00b7haft", "ra\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sieht der Wanderer mit Beben,", "tokens": ["Sieht", "der", "Wan\u00b7de\u00b7rer", "mit", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und des Kreuzes Zeichen schlagend", "tokens": ["Und", "des", "Kreu\u00b7zes", "Zei\u00b7chen", "schla\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Spricht er: Das ist Rodenstein!", "tokens": ["Spricht", "er", ":", "Das", "ist", "Ro\u00b7dens\u00b7tein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "PDS", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber wir, das n\u00e4cht'ge Grauen", "tokens": ["A\u00b7ber", "wir", ",", "das", "n\u00e4cht'\u00b7ge", "Grau\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Vorzeit Reste liebend,", "tokens": ["Und", "der", "Vor\u00b7zeit", "Res\u00b7te", "lie\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weilten oft noch sp\u00e4t im rauhen", "tokens": ["Weil\u00b7ten", "oft", "noch", "sp\u00e4t", "im", "rau\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ADJD", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Herbstmond auf dem \u00f6den Riff,", "tokens": ["Herbst\u00b7mond", "auf", "dem", "\u00f6\u00b7den", "Riff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jubelten, wenn auf dem Erker", "tokens": ["Ju\u00b7bel\u00b7ten", ",", "wenn", "auf", "dem", "Er\u00b7ker"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Wild die Wetterfahnen krachten", "tokens": ["Wild", "die", "Wet\u00b7ter\u00b7fah\u00b7nen", "krach\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der Nordwind stark und st\u00e4rker", "tokens": ["Und", "der", "Nord\u00b7wind", "stark", "und", "st\u00e4r\u00b7ker"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD", "KON", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Durch die Bogenfenster pfiff.", "tokens": ["Durch", "die", "Bo\u00b7gen\u00b7fens\u00b7ter", "pfiff", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nachts, das Holz in Haufen schichtend,", "tokens": ["Nachts", ",", "das", "Holz", "in", "Hau\u00b7fen", "schich\u00b7tend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wachten wir im alten Saale,", "tokens": ["Wach\u00b7ten", "wir", "im", "al\u00b7ten", "Saa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und, das Dunkel um uns lichtend,", "tokens": ["Und", ",", "das", "Dun\u00b7kel", "um", "uns", "lich\u00b7tend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "NN", "APPR", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00fcrten wir die Flammen an;", "tokens": ["Sch\u00fcr\u00b7ten", "wir", "die", "Flam\u00b7men", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Siehe! und mit hellem Lohen", "tokens": ["Sie\u00b7he", "!", "und", "mit", "hel\u00b7lem", "Lo\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$.", "KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schlug die Glut an das Gew\u00f6lbe,", "tokens": ["Schlug", "die", "Glut", "an", "das", "Ge\u00b7w\u00f6l\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df sie tropfend von den hohen", "tokens": ["Da\u00df", "sie", "trop\u00b7fend", "von", "den", "ho\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bogengurten niederrann;", "tokens": ["Bo\u00b7gen\u00b7gur\u00b7ten", "nie\u00b7der\u00b7rann", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und am Feuerbrande kauernd,", "tokens": ["Und", "am", "Feu\u00b7er\u00b7bran\u00b7de", "kau\u00b7ernd", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Hand den Becher Weines,", "tokens": ["In", "der", "Hand", "den", "Be\u00b7cher", "Wei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6rten wir den Nachtsturm schauernd", "tokens": ["H\u00f6r\u00b7ten", "wir", "den", "Nachts\u00b7turm", "schau\u00b7ernd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fegen durch den Bogengang,", "tokens": ["Fe\u00b7gen", "durch", "den", "Bo\u00b7gen\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie er an der Wand die Wappen", "tokens": ["Wie", "er", "an", "der", "Wand", "die", "Wap\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die R\u00fcstungen bewegte,", "tokens": ["Und", "die", "R\u00fcs\u00b7tun\u00b7gen", "be\u00b7weg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und das Schlo\u00dfthor sich mit Klappen", "tokens": ["Und", "das", "Schlo\u00df\u00b7thor", "sich", "mit", "Klap\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PRF", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In den ehrnen Angeln schwang.", "tokens": ["In", "den", "ehr\u00b7nen", "An\u00b7geln", "schwang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Leise da vom Rodensteiner", "tokens": ["Lei\u00b7se", "da", "vom", "Ro\u00b7dens\u00b7tei\u00b7ner"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprachest du, dem Fluchbeladnen,", "tokens": ["Spra\u00b7chest", "du", ",", "dem", "Fluch\u00b7be\u00b7lad\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und, erf\u00fcllt vom Schauer deiner", "tokens": ["Und", ",", "er\u00b7f\u00fcllt", "vom", "Schau\u00b7er", "dei\u00b7ner"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "VVPP", "APPRART", "NN", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sage, sp\u00e4ht' ich durch den Saal;", "tokens": ["Sa\u00b7ge", ",", "sp\u00e4ht'", "ich", "durch", "den", "Saal", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbh\u00f6rst du dort nicht Schritte schleichen?\u00ab", "tokens": ["\u00bb", "h\u00f6rst", "du", "dort", "nicht", "Schrit\u00b7te", "schlei\u00b7chen", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADV", "PTKNEG", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fragt' ich dich mit banger Stimme,", "tokens": ["Fragt'", "ich", "dich", "mit", "ban\u00b7ger", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und es traf von meinem bleichen", "tokens": ["Und", "es", "traf", "von", "mei\u00b7nem", "blei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Antlitz dich der blasse Strahl.", "tokens": ["Ant\u00b7litz", "dich", "der", "blas\u00b7se", "Strahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ja! Es war kein Traum! Ein Krachen", "tokens": ["Ja", "!", "Es", "war", "kein", "Traum", "!", "Ein", "Kra\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$.", "PPER", "VAFIN", "PIAT", "NN", "$.", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bebte durch den Bau der Erde,", "tokens": ["Beb\u00b7te", "durch", "den", "Bau", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein H\u00f6llengeisterlachen", "tokens": ["Und", "ein", "H\u00f6l\u00b7len\u00b7geis\u00b7ter\u00b7la\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlug uns gellend an das Ohr;", "tokens": ["Schlug", "uns", "gel\u00b7lend", "an", "das", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Blaue Flammen, wie von Schwefel,", "tokens": ["Blau\u00b7e", "Flam\u00b7men", ",", "wie", "von", "Schwe\u00b7fel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zuckten durch den Saal und leckten", "tokens": ["Zuck\u00b7ten", "durch", "den", "Saal", "und", "leck\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Am Gesimse und Get\u00e4fel", "tokens": ["Am", "Ge\u00b7sim\u00b7se", "und", "Ge\u00b7t\u00e4\u00b7fel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Z\u00fcngelnd bis zum Dach empor.", "tokens": ["Z\u00fcn\u00b7gelnd", "bis", "zum", "Dach", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Horch! Daher vom Schnellartgipfel", "tokens": ["Horch", "!", "Da\u00b7her", "vom", "Schnel\u00b7lart\u00b7gip\u00b7fel"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "PAV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Scholl es wie Gebell von R\u00fcden", "tokens": ["Scholl", "es", "wie", "Ge\u00b7bell", "von", "R\u00fc\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PPER", "KOKOM", "NN", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch den Sturz der Tannenwipfel", "tokens": ["Durch", "den", "Sturz", "der", "Tan\u00b7nen\u00b7wip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und den heulenden Orkan;", "tokens": ["Und", "den", "heu\u00b7len\u00b7den", "Or\u00b7kan", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "H\u00f6rner dr\u00f6hnten; aus der Fuge", "tokens": ["H\u00f6r\u00b7ner", "dr\u00f6hn\u00b7ten", ";", "aus", "der", "Fu\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "$.", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sprangen mit Gekrach die Thore,", "tokens": ["Spran\u00b7gen", "mit", "Ge\u00b7krach", "die", "Tho\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und im sturmgepeitschten Fluge", "tokens": ["Und", "im", "sturm\u00b7ge\u00b7peitschten", "Flu\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Zog die wilde Jagd heran.", "tokens": ["Zog", "die", "wil\u00b7de", "Jagd", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Rehe, denen zu den Kn\u00f6cheln", "tokens": ["Re\u00b7he", ",", "de\u00b7nen", "zu", "den", "Kn\u00f6\u00b7cheln"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measur