{"textgrid.poem.43489": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das alte Jahr vergangen ist,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das alte Jahr vergangen ist,", "tokens": ["Das", "al\u00b7te", "Jahr", "ver\u00b7gan\u00b7gen", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das neue Jahr beginnt.", "tokens": ["Das", "neu\u00b7e", "Jahr", "be\u00b7ginnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir danken Gott zu dieser Frist,", "tokens": ["Wir", "dan\u00b7ken", "Gott", "zu", "die\u00b7ser", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl uns, da\u00df wir noch sind!", "tokens": ["Wohl", "uns", ",", "da\u00df", "wir", "noch", "sind", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "KOUS", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wir sehn auf's alte Jahr zur\u00fcck,", "tokens": ["Wir", "sehn", "auf's", "al\u00b7te", "Jahr", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und haben neuen Muth:", "tokens": ["Und", "ha\u00b7ben", "neu\u00b7en", "Muth", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein neues Jahr, ein neues Gl\u00fcck!", "tokens": ["Ein", "neu\u00b7es", "Jahr", ",", "ein", "neu\u00b7es", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Zeit ist immer gut.", "tokens": ["Die", "Zeit", "ist", "im\u00b7mer", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ja, keine Zeit war jemals schlecht:", "tokens": ["Ja", ",", "kei\u00b7ne", "Zeit", "war", "je\u00b7mals", "schlecht", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PIAT", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In jeder lebet fort", "tokens": ["In", "je\u00b7der", "le\u00b7bet", "fort"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIS", "VVFIN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gef\u00fchl f\u00fcr Wahrheit, Ehr' und Recht", "tokens": ["Ge\u00b7f\u00fchl", "f\u00fcr", "Wahr\u00b7heit", ",", "Ehr'", "und", "Recht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00fcr ein freies Wort.", "tokens": ["Und", "f\u00fcr", "ein", "frei\u00b7es", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hinweg mit allem Weh und Ach!", "tokens": ["Hin\u00b7weg", "mit", "al\u00b7lem", "Weh", "und", "Ach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIS", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hinweg mit allem Leid!", "tokens": ["Hin\u00b7weg", "mit", "al\u00b7lem", "Leid", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIS", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und machen wir uns froh und gut,", "tokens": ["Und", "ma\u00b7chen", "wir", "uns", "froh", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist froh und gut die Zeit,", "tokens": ["Ist", "froh", "und", "gut", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und giebt uns Kraft und frohen Muth", "tokens": ["Und", "giebt", "uns", "Kraft", "und", "fro\u00b7hen", "Muth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei jedem neuen Leid.", "tokens": ["Bei", "je\u00b7dem", "neu\u00b7en", "Leid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und was einmal die Zeit gebracht,", "tokens": ["Und", "was", "ein\u00b7mal", "die", "Zeit", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das nimmt sie wieder hin \u2013", "tokens": ["Das", "nimmt", "sie", "wie\u00b7der", "hin", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Drum haben wir bei Tag und Nacht", "tokens": ["Drum", "ha\u00b7ben", "wir", "bei", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auch immer frohen Sinn.", "tokens": ["Auch", "im\u00b7mer", "fro\u00b7hen", "Sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und weil die Zeit nur vorw\u00e4rts will,", "tokens": ["Und", "weil", "die", "Zeit", "nur", "vor\u00b7w\u00e4rts", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So schreiten vorw\u00e4rts wir;", "tokens": ["So", "schrei\u00b7ten", "vor\u00b7w\u00e4rts", "wir", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Zeit gebeut, nie stehn wir still,", "tokens": ["Die", "Zeit", "ge\u00b7beut", ",", "nie", "stehn", "wir", "still", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir schreiten fort mit ihr.", "tokens": ["Wir", "schrei\u00b7ten", "fort", "mit", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein neues Jahr, ein neues Gl\u00fcck!", "tokens": ["Ein", "neu\u00b7es", "Jahr", ",", "ein", "neu\u00b7es", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wir ziehen froh hinein,", "tokens": ["Wir", "zie\u00b7hen", "froh", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Denn vorw\u00e4rts! vorw\u00e4rts! nie zur\u00fcck!", "tokens": ["Denn", "vor\u00b7w\u00e4rts", "!", "vor\u00b7w\u00e4rts", "!", "nie", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "PTKVZ", "$.", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Soll unsre Losung sein.", "tokens": ["Soll", "uns\u00b7re", "Lo\u00b7sung", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}