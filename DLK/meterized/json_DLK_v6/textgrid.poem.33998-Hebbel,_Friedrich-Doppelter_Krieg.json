{"textgrid.poem.33998": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Doppelter Krieg", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sollten sich des Sch\u00f6nen Priester hassen,", "tokens": ["Wie", "soll\u00b7ten", "sich", "des", "Sch\u00f6\u00b7nen", "Pries\u00b7ter", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PRF", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie sollten sie unedel sie bekriegen!", "tokens": ["Wie", "soll\u00b7ten", "sie", "un\u00b7e\u00b7del", "sie", "be\u00b7krie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Jeder wird dem Andern gern erliegen,", "tokens": ["Ein", "Je\u00b7der", "wird", "dem", "An\u00b7dern", "gern", "er\u00b7lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "ART", "ADJA", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das hei\u00dft, sich gern von ihm bewirthen lassen!", "tokens": ["Das", "hei\u00dft", ",", "sich", "gern", "von", "ihm", "be\u00b7wirt\u00b7hen", "las\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PRF", "ADV", "APPR", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch freilich werden sie das Schwert erfassen,", "tokens": ["Doch", "frei\u00b7lich", "wer\u00b7den", "sie", "das", "Schwert", "er\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Pfuscher, der den Thron der Kunst bestiegen", "tokens": ["Den", "Pfu\u00b7scher", ",", "der", "den", "Thron", "der", "Kunst", "be\u00b7stie\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ihn schon dadurch sch\u00e4ndet, zu besiegen,", "tokens": ["Und", "ihn", "schon", "da\u00b7durch", "sch\u00e4n\u00b7det", ",", "zu", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "PAV", "VVFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weil dem vor Zorn die G\u00f6tter selbst erblassen.", "tokens": ["Weil", "dem", "vor", "Zorn", "die", "G\u00f6t\u00b7ter", "selbst", "er\u00b7blas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was ist es dort? Ein anmuthsvolles Ringen,", "tokens": ["Was", "ist", "es", "dort", "?", "Ein", "an\u00b7muths\u00b7vol\u00b7les", "Rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ob Einer leisten solle, ob genie\u00dfen,", "tokens": ["Ob", "Ei\u00b7ner", "leis\u00b7ten", "sol\u00b7le", ",", "ob", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVINF", "VMFIN", "$,", "KOUS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ob f\u00fcllen oder leeren blo\u00df die Schaale.", "tokens": ["Ob", "f\u00fcl\u00b7len", "o\u00b7der", "lee\u00b7ren", "blo\u00df", "die", "Schaa\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVINF", "KON", "ADJA", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Hier gilt's, den P\u00f6belf\u00fcrsten zu bezwingen,", "tokens": ["Hier", "gilt's", ",", "den", "P\u00f6\u00b7bel\u00b7f\u00fcrs\u00b7ten", "zu", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den schn\u00f6de W\u00e4chter in den Tempel lie\u00dfen,", "tokens": ["Den", "schn\u00f6\u00b7de", "W\u00e4ch\u00b7ter", "in", "den", "Tem\u00b7pel", "lie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Damit er allen Musen B\u00e4rte male.", "tokens": ["Da\u00b7mit", "er", "al\u00b7len", "Mu\u00b7sen", "B\u00e4r\u00b7te", "ma\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}