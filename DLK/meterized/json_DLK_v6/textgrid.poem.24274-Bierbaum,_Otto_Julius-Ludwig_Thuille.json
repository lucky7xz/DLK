{"textgrid.poem.24274": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Ludwig Thuille", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine Hand,", "tokens": ["Ei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Vogelfittichleicht,", "tokens": ["Vo\u00b7gel\u00b7fit\u00b7tich\u00b7leicht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ward schwer und sank", "tokens": ["Ward", "schwer", "und", "sank"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "KON", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Von den Tasten.", "tokens": ["Von", "den", "Tas\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Ein Mund,", "tokens": ["Ein", "Mund", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Weich und \u00fcppig, wie die Frucht des S\u00fcdens,", "tokens": ["Weich", "und", "\u00fcp\u00b7pig", ",", "wie", "die", "Frucht", "des", "S\u00fc\u00b7dens", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Und w\u00fcrziger S\u00fc\u00dfe voll,", "tokens": ["Und", "w\u00fcr\u00b7zi\u00b7ger", "S\u00fc\u00b7\u00dfe", "voll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Wie die Muskatbeere an den H\u00e4ngen um Bozen,", "tokens": ["Wie", "die", "Mus\u00b7kat\u00b7bee\u00b7re", "an", "den", "H\u00e4n\u00b7gen", "um", "Bo\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.9": {"text": "Ward starr und herb", "tokens": ["Ward", "starr", "und", "herb"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Und l\u00e4chelt nun nicht mehr.", "tokens": ["Und", "l\u00e4\u00b7chelt", "nun", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Und zwei Augen sind erloschen, die leuchten konnten,", "tokens": ["Und", "zwei", "Au\u00b7gen", "sind", "er\u00b7lo\u00b7schen", ",", "die", "leuch\u00b7ten", "konn\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VAFIN", "VVPP", "$,", "PRELS", "VVINF", "VMFIN", "$,"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Wie das Ja der Braut leuchtet durch Kirchend\u00e4mmerung,", "tokens": ["Wie", "das", "Ja", "der", "Braut", "leuch\u00b7tet", "durch", "Kir\u00b7chen\u00b7d\u00e4m\u00b7me\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "--+-++--+-+--", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Und der Ruf des Knaben leuchtet in der Fr\u00fche,", "tokens": ["Und", "der", "Ruf", "des", "Kna\u00b7ben", "leuch\u00b7tet", "in", "der", "Fr\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.14": {"text": "Wenn die Gassen noch dunkel sind,", "tokens": ["Wenn", "die", "Gas\u00b7sen", "noch", "dun\u00b7kel", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "VAFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.15": {"text": "Und wie das Wort des Mannes, der einen Freund tr\u00f6stet.", "tokens": ["Und", "wie", "das", "Wort", "des", "Man\u00b7nes", ",", "der", "ei\u00b7nen", "Freund", "tr\u00f6s\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "So, Ludwig Thuille, warst Du, da\u00df wir nicht wissen, Dich abzuschildern.", "tokens": ["So", ",", "Lud\u00b7wig", "Thu\u00b7il\u00b7le", ",", "warst", "Du", ",", "da\u00df", "wir", "nicht", "wis\u00b7sen", ",", "Dich", "ab\u00b7zu\u00b7schil\u00b7dern", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "NE", "NE", "$,", "VAFIN", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "VVINF", "$,", "PPER", "VVIZU", "$."], "meter": "-+--+-+-+--+--+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Wir rufen die leichten, gelenken V\u00f6gel an,", "tokens": ["Wir", "ru\u00b7fen", "die", "leich\u00b7ten", ",", "ge\u00b7len\u00b7ken", "V\u00f6\u00b7gel", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Denken an s\u00fc\u00dfe Fr\u00fcchte, Sonne und Rebenland,", "tokens": ["Den\u00b7ken", "an", "s\u00fc\u00b7\u00dfe", "Fr\u00fcch\u00b7te", ",", "Son\u00b7ne", "und", "Re\u00b7ben\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Und alles sch\u00f6n und innig, frisch und tapfer g\u00fctig T\u00f6nende", "tokens": ["Und", "al\u00b7les", "sch\u00f6n", "und", "in\u00b7nig", ",", "frisch", "und", "tap\u00b7fer", "g\u00fc\u00b7tig", "T\u00f6\u00b7nen\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "ADJD", "KON", "ADJD", "$,", "ADJD", "KON", "ADJD", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-+--", "measure": "unknown.measure.septa"}, "line.5": {"text": "Klingt in uns wieder, ein Echo gn\u00e4diger Augenblicke,", "tokens": ["Klingt", "in", "uns", "wie\u00b7der", ",", "ein", "E\u00b7cho", "gn\u00e4\u00b7di\u00b7ger", "Au\u00b7gen\u00b7bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ADV", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Denken wir an Dich, Ludwig, der du ein Mensch warst, dessen Gegenwart", "tokens": ["Den\u00b7ken", "wir", "an", "Dich", ",", "Lud\u00b7wig", ",", "der", "du", "ein", "Mensch", "warst", ",", "des\u00b7sen", "Ge\u00b7gen\u00b7wart"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "APPR", "PPER", "$,", "NE", "$,", "PRELS", "PPER", "ART", "NN", "VAFIN", "$,", "PRELAT", "NN"], "meter": "+-+-++--+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.7": {"text": "Heiter den Geist der Schwere vertrieb und die Herzen erw\u00e4rmte.", "tokens": ["Hei\u00b7ter", "den", "Geist", "der", "Schwe\u00b7re", "ver\u00b7trieb", "und", "die", "Her\u00b7zen", "er\u00b7w\u00e4rm\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}}, "stanza.3": {"line.1": {"text": "Da\u00df Du von uns gegangen bist,", "tokens": ["Da\u00df", "Du", "von", "uns", "ge\u00b7gan\u00b7gen", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Allzuschnell,", "tokens": ["All\u00b7zu\u00b7schnell", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Allzufr\u00fch,", "tokens": ["All\u00b7zu\u00b7fr\u00fch", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Heute noch scheints uns ein h\u00e4\u00dflicher Traum, unglaubhaft.", "tokens": ["Heu\u00b7te", "noch", "scheints", "uns", "ein", "h\u00e4\u00df\u00b7li\u00b7cher", "Traum", ",", "un\u00b7glaub\u00b7haft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "ADJD", "$."], "meter": "+--+--+--++-+", "measure": "dactylic.tri.plus"}, "line.5": {"text": "Denn so voll Leben warst Du, da\u00df Du von Deiner Kraft", "tokens": ["Denn", "so", "voll", "Le\u00b7ben", "warst", "Du", ",", "da\u00df", "Du", "von", "Dei\u00b7ner", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "NN", "VAFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.6": {"text": "T\u00e4glich verschenktest, wie nur die Reichsten tun,", "tokens": ["T\u00e4g\u00b7lich", "ver\u00b7schenk\u00b7test", ",", "wie", "nur", "die", "Reichs\u00b7ten", "tun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "PWAV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Die Unersch\u00f6pflichen, denen es Wollust ist,", "tokens": ["Die", "Un\u00b7er\u00b7sch\u00f6pf\u00b7li\u00b7chen", ",", "de\u00b7nen", "es", "Wol\u00b7lust", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Herzugeben aus ihrer F\u00fclle, und die l\u00e4cheln,", "tokens": ["Her\u00b7zu\u00b7ge\u00b7ben", "aus", "ih\u00b7rer", "F\u00fcl\u00b7le", ",", "und", "die", "l\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,", "KON", "ART", "VVFIN", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "W\u00e4hrend sie schenken.", "tokens": ["W\u00e4h\u00b7rend", "sie", "schen\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Denn sie f\u00fchlen:", "tokens": ["Denn", "sie", "f\u00fch\u00b7len", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Wundervoll schwillts nach, wenn sie den \u00dcberflu\u00df", "tokens": ["Wun\u00b7der\u00b7voll", "schwillts", "nach", ",", "wenn", "sie", "den", "\u00dc\u00b7berf\u00b7lu\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.12": {"text": "Wonniger Kr\u00e4fte", "tokens": ["Won\u00b7ni\u00b7ger", "Kr\u00e4f\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Liebevoll", "tokens": ["Lie\u00b7be\u00b7voll"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.14": {"text": "Ringsum str\u00f6men lassen.", "tokens": ["Ring\u00b7sum", "str\u00f6\u00b7men", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Du warst", "tokens": ["Du", "warst"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Reich und g\u00fctig,", "tokens": ["Reich", "und", "g\u00fc\u00b7tig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Warst der geborene", "tokens": ["Warst", "der", "ge\u00b7bo\u00b7re\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.4": {"text": "K\u00fcnstler.", "tokens": ["K\u00fcnst\u00b7ler", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.5": {"line.1": {"text": "Ohne den Faltenwurf", "tokens": ["Oh\u00b7ne", "den", "Fal\u00b7ten\u00b7wurf"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Billiger Feierlichkeit,", "tokens": ["Bil\u00b7li\u00b7ger", "Fei\u00b7er\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Schlicht,", "tokens": ["Schlicht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Allem Erzwungenen feind,", "tokens": ["Al\u00b7lem", "Er\u00b7zwun\u00b7ge\u00b7nen", "feind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Bist Du nat\u00fcrlichen Gangs,", "tokens": ["Bist", "Du", "na\u00b7t\u00fcr\u00b7li\u00b7chen", "Gangs", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Leicht und zuweilen mit sp\u00f6ttischem L\u00e4cheln", "tokens": ["Leicht", "und", "zu\u00b7wei\u00b7len", "mit", "sp\u00f6t\u00b7ti\u00b7schem", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADV", "APPR", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.7": {"text": "\u00dcber berechnetes Geb\u00e4rdenspiel und den Krampf", "tokens": ["\u00dc\u00b7ber", "be\u00b7rech\u00b7ne\u00b7tes", "Ge\u00b7b\u00e4r\u00b7den\u00b7spiel", "und", "den", "Krampf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "ART", "NN"], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Allzu heftigen Applausverlangens,", "tokens": ["All\u00b7zu", "hef\u00b7ti\u00b7gen", "Ap\u00b7plaus\u00b7ver\u00b7lan\u00b7gens", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Ruhig voran- und emporgeschritten,", "tokens": ["Ru\u00b7hig", "vor\u00b7an", "und", "em\u00b7por\u00b7ge\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "TRUNC", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Sicher des Ziels, weil eine Treue", "tokens": ["Si\u00b7cher", "des", "Ziels", ",", "weil", "ei\u00b7ne", "Treu\u00b7e"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "KOUS", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Unverr\u00fcckbar Halt Dir gab und Richtung:", "tokens": ["Un\u00b7ver\u00b7r\u00fcck\u00b7bar", "Halt", "Dir", "gab", "und", "Rich\u00b7tung", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PPER", "VVFIN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Treue zu Dir und Deiner eingeborenen", "tokens": ["Treu\u00b7e", "zu", "Dir", "und", "Dei\u00b7ner", "ein\u00b7ge\u00b7bo\u00b7re\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "KON", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.13": {"text": "Art und Kunst.", "tokens": ["Art", "und", "Kunst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Nicht nur gab Dir ein Gott", "tokens": ["Nicht", "nur", "gab", "Dir", "ein", "Gott"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Auszut\u00f6nen, was Du empfandest. Er gab Dir auch", "tokens": ["Aus\u00b7zu\u00b7t\u00f6\u00b7nen", ",", "was", "Du", "emp\u00b7fan\u00b7dest", ".", "Er", "gab", "Dir", "auch"]