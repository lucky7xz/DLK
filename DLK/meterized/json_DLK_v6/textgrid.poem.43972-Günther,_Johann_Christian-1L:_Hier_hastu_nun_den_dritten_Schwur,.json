{"textgrid.poem.43972": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier hastu nun den dritten Schwur,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier hastu nun den dritten Schwur,", "tokens": ["Hier", "has\u00b7tu", "nun", "den", "drit\u00b7ten", "Schwur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wodurch ich Himmel und Natur", "tokens": ["Wo\u00b7durch", "ich", "Him\u00b7mel", "und", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu Zeugen unsres Bundes seze;", "tokens": ["Zu", "Zeu\u00b7gen", "uns\u00b7res", "Bun\u00b7des", "se\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bleib treu, getrost und achte nicht,", "tokens": ["Bleib", "treu", ",", "ge\u00b7trost", "und", "ach\u00b7te", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "VVPP", "KON", "ADJA", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn manche L\u00e4sterzunge sticht,", "tokens": ["Wenn", "man\u00b7che", "L\u00e4s\u00b7ter\u00b7zun\u00b7ge", "sticht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der falschen Freunde Mordgeschweze.", "tokens": ["Der", "fal\u00b7schen", "Freun\u00b7de", "Mord\u00b7ge\u00b7schwe\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Gl\u00fccke h\u00e4lt uns freylich auf,", "tokens": ["Das", "Gl\u00fc\u00b7cke", "h\u00e4lt", "uns", "frey\u00b7lich", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch las ihm nur den faulen Lauf,", "tokens": ["Doch", "las", "ihm", "nur", "den", "fau\u00b7len", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es sucht fein langsam auszurasen.", "tokens": ["Es", "sucht", "fein", "lang\u00b7sam", "aus\u00b7zu\u00b7ra\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So starck der Nord sich h\u00f6ren l\u00e4st,", "tokens": ["So", "starck", "der", "Nord", "sich", "h\u00f6\u00b7ren", "l\u00e4st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "PRF", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So z\u00e4rtlich wird auch bald der West", "tokens": ["So", "z\u00e4rt\u00b7lich", "wird", "auch", "bald", "der", "West"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In unsre Liebesflaggen blasen.", "tokens": ["In", "uns\u00b7re", "Lie\u00b7bes\u00b7flag\u00b7gen", "bla\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Weltlust zeigt mir nichts mehr an,", "tokens": ["Die", "Welt\u00b7lust", "zeigt", "mir", "nichts", "mehr", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIS", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Worein ich mich verlieben kan,", "tokens": ["Wo\u00b7rein", "ich", "mich", "ver\u00b7lie\u00b7ben", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als dein Gesicht und meine Baare;", "tokens": ["Als", "dein", "Ge\u00b7sicht", "und", "mei\u00b7ne", "Baa\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bekomm ich nun das erste nicht,", "tokens": ["Be\u00b7komm", "ich", "nun", "das", "ers\u00b7te", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So las ich freudig Tag und Licht", "tokens": ["So", "las", "ich", "freu\u00b7dig", "Tag", "und", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auch mitten um die besten Jahre.", "tokens": ["Auch", "mit\u00b7ten", "um", "die", "bes\u00b7ten", "Jah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich f\u00fchl am besten innerlich", "tokens": ["Ich", "f\u00fchl", "am", "bes\u00b7ten", "in\u00b7ner\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKA", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So manchen tiefen Herzensstich", "tokens": ["So", "man\u00b7chen", "tie\u00b7fen", "Her\u00b7zens\u00b7stich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und bin schon ziemlich umgetrieben;", "tokens": ["Und", "bin", "schon", "ziem\u00b7lich", "um\u00b7ge\u00b7trie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch will mir Gott gen\u00e4dig seyn,", "tokens": ["Doch", "will", "mir", "Gott", "ge\u00b7n\u00e4\u00b7dig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So l\u00e4st er mich nach aller Pein", "tokens": ["So", "l\u00e4st", "er", "mich", "nach", "al\u00b7ler", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dich einmahl noch und sicher lieben.", "tokens": ["Dich", "ein\u00b7mahl", "noch", "und", "si\u00b7cher", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Vertrau der Vorsicht, liebster Schaz,", "tokens": ["Ver\u00b7trau", "der", "Vor\u00b7sicht", ",", "liebs\u00b7ter", "Schaz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie wird uns einen Ruheplaz,", "tokens": ["Sie", "wird", "uns", "ei\u00b7nen", "Ru\u00b7he\u00b7plaz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es sey auch wo es will, bereiten;", "tokens": ["Es", "sey", "auch", "wo", "es", "will", ",", "be\u00b7rei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PWAV", "PPER", "VMFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Alsdenn belachen wir mit Lust", "tokens": ["Als\u00b7denn", "be\u00b7la\u00b7chen", "wir", "mit", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aus froh- und eintrachtsvoller Brust", "tokens": ["Aus", "froh", "und", "ein\u00b7trachts\u00b7vol\u00b7ler", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Thorheit unsrer b\u00f6sen Zeiten.", "tokens": ["Die", "Thor\u00b7heit", "uns\u00b7rer", "b\u00f6\u00b7sen", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Besinne dich, was Schweidniz wies:", "tokens": ["Be\u00b7sin\u00b7ne", "dich", ",", "was", "Schweid\u00b7niz", "wies", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von innen zwar ein Paradies,", "tokens": ["Von", "in\u00b7nen", "zwar", "ein", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von au\u00dfen Unruh, Zanck und Plagen;", "tokens": ["Von", "au\u00b7\u00dfen", "Un\u00b7ruh", ",", "Zanck", "und", "Pla\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und kommt dir Roschkwiz in den Sinn,", "tokens": ["Und", "kommt", "dir", "Roschk\u00b7wiz", "in", "den", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So denck auch dort nach Borau hin,", "tokens": ["So", "denck", "auch", "dort", "nach", "Bo\u00b7rau", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "ADV", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo mich dein Abschied wund geschlagen.", "tokens": ["Wo", "mich", "dein", "Ab\u00b7schied", "wund", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Sobald des Bruders Hochzeitsfest", "tokens": ["So\u00b7bald", "des", "Bru\u00b7ders", "Hoch\u00b7zeits\u00b7fest"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich bey der Tafel lachen l\u00e4st,", "tokens": ["Dich", "bey", "der", "Ta\u00b7fel", "la\u00b7chen", "l\u00e4st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So trinck mein Wohlseyn in Gedancken,", "tokens": ["So", "trinck", "mein", "Wohl\u00b7seyn", "in", "Ge\u00b7dan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wenn dir der Verlobten Ku\u00df", "tokens": ["Und", "wenn", "dir", "der", "Ver\u00b7lob\u00b7ten", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Zu stiller Reizung dienen mu\u00df,", "tokens": ["Zu", "stil\u00b7ler", "Rei\u00b7zung", "die\u00b7nen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So wi\u00dfe: G\u00fcnther kan nicht wancken.", "tokens": ["So", "wi\u00b7\u00dfe", ":", "G\u00fcn\u00b7ther", "kan", "nicht", "wan\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "NE", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Es hat mich innerlich erg\u00f6zt,", "tokens": ["Es", "hat", "mich", "in\u00b7ner\u00b7lich", "er\u00b7g\u00f6zt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df Lorchen meine Lieder sch\u00e4zt", "tokens": ["Da\u00df", "Lor\u00b7chen", "mei\u00b7ne", "Lie\u00b7der", "sch\u00e4zt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dann und wann noch Lieder fodert;", "tokens": ["Und", "dann", "und", "wann", "noch", "Lie\u00b7der", "fo\u00b7dert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KON", "PWAV", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Nahme soll auch ganz allein", "tokens": ["Dein", "Nah\u00b7me", "soll", "auch", "ganz", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Zierrath meiner Reime seyn,", "tokens": ["Die", "Zier\u00b7rath", "mei\u00b7ner", "Rei\u00b7me", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In welchen unsre Liebe lodert.", "tokens": ["In", "wel\u00b7chen", "uns\u00b7re", "Lie\u00b7be", "lo\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Mein Engel, nimm es selbst aus dir,", "tokens": ["Mein", "En\u00b7gel", ",", "nimm", "es", "selbst", "aus", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVIMP", "PPER", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie schwer, wie scharf und \u00e4ngstlich mir", "tokens": ["Wie", "schwer", ",", "wie", "scharf", "und", "\u00e4ngst\u00b7lich", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "KON", "ADJD", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein drittes Abschiedsk\u00fc\u00dfen falle;", "tokens": ["Dein", "drit\u00b7tes", "Ab\u00b7schieds\u00b7k\u00fc\u00b7\u00dfen", "fal\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jedoch Gedult, Vernunft und Zeit", "tokens": ["Je\u00b7doch", "Ge\u00b7dult", ",", "Ver\u00b7nunft", "und", "Zeit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "NN", "$,", "NN", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Cr\u00f6nt endlich die Best\u00e4ndigkeit", "tokens": ["Cr\u00f6nt", "end\u00b7lich", "die", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und schenckt uns Zucker auf die Galle.", "tokens": ["Und", "schenckt", "uns", "Zu\u00b7cker", "auf", "die", "Gal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Nun gute Nacht, du treues Kind!", "tokens": ["Nun", "gu\u00b7te", "Nacht", ",", "du", "treu\u00b7es", "Kind", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"