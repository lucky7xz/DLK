{"textgrid.poem.62769": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ey seht, wie dick die Amme thut,", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ey seht, wie dick die Amme thut,", "tokens": ["Ey", "seht", ",", "wie", "dick", "die", "Am\u00b7me", "thut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PWAV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Mensch tr\u00e4gt Puder auf dem Kopfe;", "tokens": ["Das", "Mensch", "tr\u00e4gt", "Pu\u00b7der", "auf", "dem", "Kop\u00b7fe", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die gn\u00e4d'ge Frau hats kaum so gut,", "tokens": ["Die", "gn\u00e4d'\u00b7ge", "Frau", "hats", "kaum", "so", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es i\u00dft mit ihr aus einem Topfe,", "tokens": ["Es", "i\u00dft", "mit", "ihr", "aus", "ei\u00b7nem", "Top\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Trinkt Firnewein und schl\u00fcrft Kaffe,", "tokens": ["Trinkt", "Fir\u00b7ne\u00b7wein", "und", "schl\u00fcrft", "Kaf\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "VVFIN", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ich mu\u00df mit Kovent mich begn\u00fcgen.", "tokens": ["Ich", "mu\u00df", "mit", "Ko\u00b7vent", "mich", "be\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn ich vor Tag am Waschtrog steh,", "tokens": ["Wenn", "ich", "vor", "Tag", "am", "Waschtrog", "steh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So bleibt die Drolle ruhig liegen.", "tokens": ["So", "bleibt", "die", "Drol\u00b7le", "ru\u00b7hig", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Mich sprengt man immer hin und her,", "tokens": ["Mich", "sprengt", "man", "im\u00b7mer", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sie darf nur tanzen, singen, lachen;", "tokens": ["Sie", "darf", "nur", "tan\u00b7zen", ",", "sin\u00b7gen", ",", "la\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$,", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Nein, Jungemagd bleib ich nicht mehr,", "tokens": ["Nein", ",", "Jun\u00b7ge\u00b7magd", "bleib", "ich", "nicht", "mehr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "VVFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ich lasse mich zur Amme machen.", "tokens": ["Ich", "las\u00b7se", "mich", "zur", "Am\u00b7me", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}