{"dta.poem.20803": {"metadata": {"author": {"name": "Candidus, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Canzone   XII .", "genre": "Epos", "period": "N.A.", "pub_year": "1854", "urn": "urn:nbn:de:kobv:b4-20090519783", "language": ["de:0.99"], "booktitle": "Candidus, Karl: Der deutsche Christus. F\u00fcnfzehn Canzonen. Leipzig, 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Den Tauftag meines Sohnes gr\u00fc\u00dfet munter", "tokens": ["Den", "Tauf\u00b7tag", "mei\u00b7nes", "Soh\u00b7nes", "gr\u00fc\u00b7\u00dfet", "mun\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit lautem Chor der Kirchenspatzen V\u00f6lklein", "tokens": ["Mit", "lau\u00b7tem", "Chor", "der", "Kir\u00b7chen\u00b7spat\u00b7zen", "V\u00f6l\u00b7klein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Wie tiefer thurmab gleitet Sonnenr\u00f6te", "tokens": ["Wie", "tie\u00b7fer", "thurm\u00b7ab", "glei\u00b7tet", "Son\u00b7nen\u00b7r\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sacht verglimmen wangengl\u00fche W\u00f6lklein.", "tokens": ["Und", "sacht", "ver\u00b7glim\u00b7men", "wan\u00b7gen\u00b7gl\u00fc\u00b7he", "W\u00f6lk\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "VVFIN", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Ein frischer Morgenwind weht sanft herunter", "tokens": ["Ein", "fri\u00b7scher", "Mor\u00b7gen\u00b7wind", "weht", "sanft", "her\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df er zum Festtag seinen Gru\u00df entb\u00f6te.", "tokens": ["Da\u00df", "er", "zum", "Fest\u00b7tag", "sei\u00b7nen", "Gru\u00df", "ent\u00b7b\u00f6\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wenn innerlich erh\u00f6hte", "tokens": ["Wenn", "in\u00b7ner\u00b7lich", "er\u00b7h\u00f6h\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gem\u00fctsstimmung das Leben uns begeistet,", "tokens": ["Ge\u00b7m\u00fcts\u00b7stim\u00b7mung", "das", "Le\u00b7ben", "uns", "be\u00b7geis\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wie ist die Erde sch\u00f6n! Und sie w\u00e4r' sch\u00f6ner", "tokens": ["Wie", "ist", "die", "Er\u00b7de", "sch\u00f6n", "!", "Und", "sie", "w\u00e4r'", "sch\u00f6\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADJD", "$.", "KON", "PPER", "VAFIN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wenn nicht wie Tagel\u00f6hner", "tokens": ["Wenn", "nicht", "wie", "Ta\u00b7ge\u00b7l\u00f6h\u00b7ner"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "KOKOM", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Die Menschen, ach! ihr K\u00f6nigswerk geleistet.", "tokens": ["Die", "Men\u00b7schen", ",", "ach", "!", "ihr", "K\u00f6\u00b7nigs\u00b7werk", "ge\u00b7leis\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ITJ", "$.", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Es bleibt die Sch\u00f6nheit der Natur verh\u00fcllet", "tokens": ["Es", "bleibt", "die", "Sch\u00f6n\u00b7heit", "der", "Na\u00b7tur", "ver\u00b7h\u00fcl\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "So lang der L\u00fcgengeist die Welt erf\u00fcllet.", "tokens": ["So", "lang", "der", "L\u00fc\u00b7gen\u00b7geist", "die", "Welt", "er\u00b7f\u00fcl\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O Gottentfremdung, einzig Eine S\u00fcnde,", "tokens": ["O", "Got\u00b7tent\u00b7frem\u00b7dung", ",", "ein\u00b7zig", "Ei\u00b7ne", "S\u00fcn\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie hast du mein Geschlecht mir zugerichtet!", "tokens": ["Wie", "hast", "du", "mein", "Ge\u00b7schlecht", "mir", "zu\u00b7ge\u00b7rich\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verfinstert der Verstand! Verstockt die Herzen!", "tokens": ["Ver\u00b7fins\u00b7tert", "der", "Ver\u00b7stand", "!", "Ver\u00b7stockt", "die", "Her\u00b7zen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und der Gesellschaft Grundortnung vernichtet!", "tokens": ["Und", "der", "Ge\u00b7sell\u00b7schaft", "Grund\u00b7ort\u00b7nung", "ver\u00b7nich\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wer ist so klug da\u00df Einklang er ergr\u00fcnde", "tokens": ["Wer", "ist", "so", "klug", "da\u00df", "Ein\u00b7klang", "er", "er\u00b7gr\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ADV", "ADJD", "KOUS", "NN", "PPER", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wo \u00fcppig wuchern Millionen Schmerzen?", "tokens": ["Wo", "\u00fcp\u00b7pig", "wu\u00b7chern", "Mil\u00b7lion\u00b7en", "Schmer\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "NN", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ach! selbst die Himmelskerzen", "tokens": ["Ach", "!", "selbst", "die", "Him\u00b7mels\u00b7ker\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des Lichtes leuchten nicht den \u201eEwigblinden\u201c", "tokens": ["Des", "Lich\u00b7tes", "leuch\u00b7ten", "nicht", "den", "\u201e", "E\u00b7wig\u00b7blin\u00b7den", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ART", "$(", "ADJA", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und \u201e\u00e4schern Stadt und Land ein\u201c zum Beweise", "tokens": ["Und", "\u201e", "\u00e4sc\u00b7hern", "Stadt", "und", "Land", "ein", "\u201c", "zum", "Be\u00b7wei\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$(", "ADJA", "NN", "KON", "NN", "ART", "$(", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Da\u00df sich im Jammerkreise", "tokens": ["Da\u00df", "sich", "im", "Jam\u00b7mer\u00b7krei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Drehn mu\u00df die Menschheit bis sie Gott mag finden.", "tokens": ["Drehn", "mu\u00df", "die", "Menschheit", "bis", "sie", "Gott", "mag", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VMFIN", "ART", "NN", "APPR", "PPER", "NN", "VMFIN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.12": {"text": "Das ist die Wahrheit jener alten Lehre", "tokens": ["Das", "ist", "die", "Wahr\u00b7heit", "je\u00b7ner", "al\u00b7ten", "Leh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Da\u00df erblich stets die S\u00fcnde wiederkehre.", "tokens": ["Da\u00df", "er\u00b7blich", "stets", "die", "S\u00fcn\u00b7de", "wie\u00b7der\u00b7keh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Denn innerstes und \u00e4u\u00dferes Verderben", "tokens": ["Denn", "in\u00b7ners\u00b7tes", "und", "\u00e4u\u00b7\u00dfe\u00b7res", "Ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "KON", "ADJA", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Erzeugen f\u00fcr und f\u00fcr sich wechselseitig", "tokens": ["Er\u00b7zeu\u00b7gen", "f\u00fcr", "und", "f\u00fcr", "sich", "wech\u00b7sel\u00b7sei\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "KON", "APPR", "PRF", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und graunvoll hoffnungslose Doppelknechtschaft", "tokens": ["Und", "graun\u00b7voll", "hoff\u00b7nungs\u00b7lo\u00b7se", "Dop\u00b7pel\u00b7knecht\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Macht was da Mensch hei\u00dft sich als Beute streitig.", "tokens": ["Macht", "was", "da", "Mensch", "hei\u00dft", "sich", "als", "Beu\u00b7te", "strei\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "ADV", "NN", "VVFIN", "PRF", "KOUS", "NN", "ADJD", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Gesteigert mu\u00df dies Elend stets vererben", "tokens": ["Ge\u00b7stei\u00b7gert", "mu\u00df", "dies", "E\u00b7lend", "stets", "ver\u00b7er\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VMFIN", "PDS", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die L\u00fcgensch\u00f6pfung welche das Geschlecht schafft.", "tokens": ["Die", "L\u00fc\u00b7gen\u00b7sch\u00f6p\u00b7fung", "wel\u00b7che", "das", "Ge\u00b7schlecht", "schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Weh! niemand ist der recht schafft.", "tokens": ["Weh", "!", "nie\u00b7mand", "ist", "der", "recht", "schafft", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PIS", "VAFIN", "ART", "ADJD", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Gesammtbeziehungslos hinschwirrend spielen", "tokens": ["Ge\u00b7sammt\u00b7be\u00b7zie\u00b7hungs\u00b7los", "hin\u00b7schwir\u00b7rend", "spie\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVPP", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der menschlichen Natur verkannte Triebe.", "tokens": ["Der", "menschli\u00b7chen", "Na\u00b7tur", "ver\u00b7kann\u00b7te", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Erkenntni\u00df gibt die Liebe", "tokens": ["Er\u00b7kennt\u00b7ni\u00df", "gibt", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Von Sonderkr\u00e4ften und gemeinen Zielen", "tokens": ["Von", "Son\u00b7der\u00b7kr\u00e4f\u00b7ten", "und", "ge\u00b7mei\u00b7nen", "Zie\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Allein ja und die Liebe ward zum Spotte.", "tokens": ["Al\u00b7lein", "ja", "und", "die", "Lie\u00b7be", "ward", "zum", "Spot\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ART", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Es schaut der Mensch den Menschen au\u00dfer Gotte.", "tokens": ["Es", "schaut", "der", "Mensch", "den", "Men\u00b7schen", "au\u00b7\u00dfer", "Got\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da ist von Menschenachtung kein Gedanke,", "tokens": ["Da", "ist", "von", "Men\u00b7schen\u00b7ach\u00b7tung", "kein", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kein Sinn f\u00fcr eigenth\u00fcmliche Entfaltung,", "tokens": ["Kein", "Sinn", "f\u00fcr", "ei\u00b7gent\u00b7h\u00fcm\u00b7li\u00b7che", "Ent\u00b7fal\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kein Ahnen da\u00df darin die Grundbest\u00e4nde", "tokens": ["Kein", "Ah\u00b7nen", "da\u00df", "da\u00b7rin", "die", "Grund\u00b7be\u00b7st\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KOUS", "PAV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vorhanden g\u00f6ttlicher Gesammtgestaltung.", "tokens": ["Vor\u00b7han\u00b7den", "g\u00f6tt\u00b7li\u00b7cher", "Ge\u00b7sammt\u00b7ge\u00b7stal\u00b7tung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Da wuchert widrig wild der Frechheit Ranke", "tokens": ["Da", "wu\u00b7chert", "wid\u00b7rig", "wild", "der", "Frech\u00b7heit", "Ran\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "ADJD", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn nicht an toter Formeln starre W\u00e4nde", "tokens": ["Wenn", "nicht", "an", "to\u00b7ter", "For\u00b7meln", "star\u00b7re", "W\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sie binden plumpe H\u00e4nde.", "tokens": ["Sie", "bin\u00b7den", "plum\u00b7pe", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wer wei\u00df von Einem Gott und vielen Kr\u00e4ften?", "tokens": ["Wer", "wei\u00df", "von", "Ei\u00b7nem", "Gott", "und", "vie\u00b7len", "Kr\u00e4f\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wer wei\u00df von Einem Leib und vielen Gliedern", "tokens": ["Wer", "wei\u00df", "von", "Ei\u00b7nem", "Leib", "und", "vie\u00b7len", "Glie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und ehret auch die niedern", "tokens": ["Und", "eh\u00b7ret", "auch", "die", "nie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Und wei\u00df von Aller n\u00fctzlichen Gesch\u00e4ften?", "tokens": ["Und", "wei\u00df", "von", "Al\u00b7ler", "n\u00fctz\u00b7li\u00b7chen", "Ge\u00b7sch\u00e4f\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambi