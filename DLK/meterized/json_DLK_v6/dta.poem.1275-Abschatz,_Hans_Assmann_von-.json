{"dta.poem.1275": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Die gantze Nacht liegt mir mein Weib in Ohren/", "tokens": ["Die", "gant\u00b7ze", "Nacht", "liegt", "mir", "mein", "Weib", "in", "Oh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie hat den Schlaff und ich die Ruh verlohren/", "tokens": ["Sie", "hat", "den", "Schlaff", "und", "ich", "die", "Ruh", "ver\u00b7loh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie schleust mich ein in Armen voller Glutt/", "tokens": ["Sie", "schleust", "mich", "ein", "in", "Ar\u00b7men", "vol\u00b7ler", "Glutt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "APPR", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verbrennt und kocht das ausgefrorne Blutt.", "tokens": ["Ver\u00b7brennt", "und", "kocht", "das", "aus\u00b7ge\u00b7fror\u00b7ne", "Blutt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ihr heisser Brand will See und Flutten haben/", "tokens": ["Ihr", "heis\u00b7ser", "Brand", "will", "See", "und", "Flut\u00b7ten", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "NN", "KON", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn andre kan ein B\u00e4chlein Necktar laben.", "tokens": ["Wenn", "and\u00b7re", "kan", "ein", "B\u00e4ch\u00b7lein", "Neck\u00b7tar", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VMFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Welch Brunnquell kan so unergr\u00fcndlich seyn/", "tokens": ["Welch", "Brunn\u00b7quell", "kan", "so", "un\u00b7er\u00b7gr\u00fcnd\u00b7lich", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADV", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der nicht von Sonn\u2019 und D\u00fcrre trocknet ein?", "tokens": ["Der", "nicht", "von", "Sonn'", "und", "D\u00fcr\u00b7re", "trock\u00b7net", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "NN", "KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mein Lebens-Oel ist meistentheils verglommen/", "tokens": ["Mein", "Le\u00b7bens\u00b7O\u00b7el", "ist", "meis\u00b7ten\u00b7theils", "ver\u00b7glom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nachdem ich bin zu frischem Feuer kommen;", "tokens": ["Nach\u00b7dem", "ich", "bin", "zu", "fri\u00b7schem", "Feu\u00b7er", "kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geb ich den Rest auff eine Zeit dahin/", "tokens": ["Geb", "ich", "den", "Rest", "auff", "ei\u00b7ne", "Zeit", "da\u00b7hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "PAV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wer leuchtet ihr/ wenn ich erloschen bin?", "tokens": ["Wer", "leuch\u00b7tet", "ihr", "/", "wenn", "ich", "er\u00b7lo\u00b7schen", "bin", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$(", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wer kan den Durst der Wassersucht bestillen/", "tokens": ["Wer", "kan", "den", "Durst", "der", "Was\u00b7ser\u00b7sucht", "be\u00b7stil\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und die Begier erregter L\u00fcste f\u00fcllen?", "tokens": ["Und", "die", "Be\u00b7gier", "er\u00b7reg\u00b7ter", "L\u00fcs\u00b7te", "f\u00fcl\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Viel besser ist getheilter Uberflu\u00df/", "tokens": ["Viel", "bes\u00b7ser", "ist", "ge\u00b7theil\u00b7ter", "U\u00b7ber\u00b7flu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als wenn man bald auff einmahl darben mu\u00df.", "tokens": ["Als", "wenn", "man", "bald", "auff", "ein\u00b7mahl", "dar\u00b7ben", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIS", "ADV", "APPR", "ADV", "PAV", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Man w\u00e4rmt sich auch bey halberstorbnen Kohlen/", "tokens": ["Man", "w\u00e4rmt", "sich", "auch", "bey", "hal\u00b7ber\u00b7storb\u00b7nen", "Koh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kan Feuer aus der lauen Asche holen/", "tokens": ["Kan", "Feu\u00b7er", "aus", "der", "lau\u00b7en", "A\u00b7sche", "ho\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "APPR", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein sp\u00e4ter Herbst gewehrt die beste Frucht/", "tokens": ["Ein", "sp\u00e4\u00b7ter", "Herbst", "ge\u00b7wehrt", "die", "bes\u00b7te", "Frucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die man umsonst im goldnen Lentzen sucht.", "tokens": ["Die", "man", "um\u00b7sonst", "im", "gold\u00b7nen", "Lent\u00b7zen", "sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Aus Felsen mu\u00df das beste Wasser springen/", "tokens": ["Aus", "Fel\u00b7sen", "mu\u00df", "das", "bes\u00b7te", "Was\u00b7ser", "sprin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wiewohl es nicht ohn M\u00fche zu erzwingen.", "tokens": ["Wie\u00b7wohl", "es", "nicht", "ohn", "M\u00fc\u00b7he", "zu", "er\u00b7zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Eckel g\u00e4llt die leicht-erworbne Lust/", "tokens": ["Der", "E\u00b7ckel", "g\u00e4llt", "die", "leicht\u00b7er\u00b7worb\u00b7ne", "Lust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Hunger w\u00fcrzt die lang\u2019 erwartte Kost.", "tokens": ["Und", "Hun\u00b7ger", "w\u00fcrzt", "die", "lang'", "er\u00b7wart\u00b7te", "Kost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Mu\u00df gr\u00fcnes Holtz mehr Rauch und Thr\u00e4nen schwi-", "tokens": ["Mu\u00df", "gr\u00fc\u00b7nes", "Holtz", "mehr", "Rauch", "und", "Thr\u00e4\u00b7nen", "schwi"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADJA", "NN", "PIAT", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein d\u00fcrrer Stock kan dennoch besser hitzen.", "tokens": ["Ein", "d\u00fcr\u00b7rer", "Stock", "kan", "den\u00b7noch", "bes\u00b7ser", "hit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die G\u00fctte/ nicht die Menge/ preist den Wein/", "tokens": ["Die", "G\u00fct\u00b7te", "/", "nicht", "die", "Men\u00b7ge", "/", "preist", "den", "Wein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PTKNEG", "ART", "NN", "$(", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Balsam fl\u00f6st man nur mit Tropffen ein.", "tokens": ["Und", "Bal\u00b7sam", "fl\u00f6st", "man", "nur", "mit", "Tropf\u00b7fen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PIS", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Drum/ Liebe/ komm mir Alten Recht zusprechen/", "tokens": ["Drum", "/", "Lie\u00b7be", "/", "komm", "mir", "Al\u00b7ten", "Recht", "zu\u00b7spre\u00b7chen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$(", "NN", "$(", "VVFIN", "PPER", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Komm ihre Glutt/ und nicht mein Ei\u00df/ zu brechen/", "tokens": ["Komm", "ih\u00b7re", "Glutt", "/", "und", "nicht", "mein", "Ei\u00df", "/", "zu", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "KON", "PTKNEG", "PPOSAT", "NN", "$(", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Damit ihr Brand/ durch meinen Schnee gek\u00fchlt/", "tokens": ["Da\u00b7mit", "ihr", "Brand", "/", "durch", "mei\u00b7nen", "Schnee", "ge\u00b7k\u00fchlt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "$(", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit sanffter Glutt und lindern Flammen spielt?", "tokens": ["Mit", "sanff\u00b7ter", "Glutt", "und", "lin\u00b7dern", "Flam\u00b7men", "spielt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}