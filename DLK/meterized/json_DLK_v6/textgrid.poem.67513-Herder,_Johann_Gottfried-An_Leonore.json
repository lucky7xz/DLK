{"textgrid.poem.67513": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "An Leonore", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Siehe, dort schwebt sie hernieder, die Hora des kommenden Jahres,", "tokens": ["Sie\u00b7he", ",", "dort", "schwebt", "sie", "her\u00b7nie\u00b7der", ",", "die", "Ho\u00b7ra", "des", "kom\u00b7men\u00b7den", "Jah\u00b7res", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Im verschwisterten Chor; Grazien f\u00fchren den Reihn.", "tokens": ["Im", "ver\u00b7schwis\u00b7ter\u00b7ten", "Chor", ";", "Gra\u00b7zi\u00b7en", "f\u00fch\u00b7ren", "den", "Reihn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Jeglichem l\u00e4chelt sie Freude, der sie mit Liebe begr\u00fc\u00dfet,", "tokens": ["Jeg\u00b7li\u00b7chem", "l\u00e4\u00b7chelt", "sie", "Freu\u00b7de", ",", "der", "sie", "mit", "Lie\u00b7be", "be\u00b7gr\u00fc\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Schenket Jedem so gern, was er bescheiden begehrt.", "tokens": ["Schen\u00b7ket", "Je\u00b7dem", "so", "gern", ",", "was", "er", "be\u00b7schei\u00b7den", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADV", "ADV", "$,", "PWS", "PPER", "ADJD", "VVPP", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Liebend nahet sie Dir, sie bekr\u00e4nzt mit \u00e4therischen Blumen,", "tokens": ["Lie\u00b7bend", "na\u00b7het", "sie", "Dir", ",", "sie", "be\u00b7kr\u00e4nzt", "mit", "\u00e4t\u00b7he\u00b7ri\u00b7schen", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Die in Elysiums Flur spro\u00dften, Dein lockiges Haar,", "tokens": ["Die", "in", "E\u00b7ly\u00b7si\u00b7ums", "Flur", "spro\u00df\u00b7ten", ",", "Dein", "lo\u00b7cki\u00b7ges", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "NN", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Schlinget den Arm um Dich und bringt Dir s\u00fc\u00dfe Geschenke,", "tokens": ["Schlin\u00b7get", "den", "Arm", "um", "Dich", "und", "bringt", "Dir", "s\u00fc\u00b7\u00dfe", "Ge\u00b7schen\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPER", "KON", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Sanfte Freuden und Scherz mit dem verschwisternden Ku\u00df.", "tokens": ["Sanf\u00b7te", "Freu\u00b7den", "und", "Scherz", "mit", "dem", "ver\u00b7schwis\u00b7tern\u00b7den", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}}}}}