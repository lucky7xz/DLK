{"dta.poem.7898": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "4.  Fr\u00fchlingsfeier .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00dfer, goldner Fr\u00fchlingstag!", "tokens": ["S\u00fc\u00b7\u00dfer", ",", "gold\u00b7ner", "Fr\u00fch\u00b7lings\u00b7tag", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Inniges Entz\u00fccken!", "tokens": ["In\u00b7ni\u00b7ges", "Ent\u00b7z\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wenn mir je ein Lied gelang,", "tokens": ["Wenn", "mir", "je", "ein", "Lied", "ge\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sollt\u2019 es heut nicht gl\u00fccken?", "tokens": ["Sollt'", "es", "heut", "nicht", "gl\u00fc\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Doch warum in dieser Zeit", "tokens": ["Doch", "wa\u00b7rum", "in", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "An die Arbeit treten?", "tokens": ["An", "die", "Ar\u00b7beit", "tre\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Fr\u00fchling ist ein hohes Fest:", "tokens": ["Fr\u00fch\u00b7ling", "ist", "ein", "ho\u00b7hes", "Fest", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00dft mich ruhn und beten!", "tokens": ["La\u00dft", "mich", "ruhn", "und", "be\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}