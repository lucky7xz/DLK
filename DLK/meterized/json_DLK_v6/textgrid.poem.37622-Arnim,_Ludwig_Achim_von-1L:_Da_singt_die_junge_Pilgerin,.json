{"textgrid.poem.37622": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da singt die junge Pilgerin,", "genre": "verse", "period": "N.A.", "pub_year": 1825, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da singt die junge Pilgerin,", "tokens": ["Da", "singt", "die", "jun\u00b7ge", "Pil\u00b7ge\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gest\u00fctzt vom Malerstab,", "tokens": ["Ge\u00b7st\u00fctzt", "vom", "Ma\u00b7ler\u00b7stab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie zieht zum fernen Kloster hin", "tokens": ["Sie", "zieht", "zum", "fer\u00b7nen", "Klos\u00b7ter", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mahnt um letzte Gab',", "tokens": ["Und", "mahnt", "um", "letz\u00b7te", "Gab'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Um fromme Bildchen in ihr Buch,", "tokens": ["Um", "from\u00b7me", "Bild\u00b7chen", "in", "ihr", "Buch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Um ein erbaulich Wort:", "tokens": ["Um", "ein", "er\u00b7bau\u00b7lich", "Wort", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "\u00bbgewarnt, ermahnt bist du genug,", "tokens": ["\u00bb", "ge\u00b7warnt", ",", "er\u00b7mahnt", "bist", "du", "ge\u00b7nug", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "$,", "VVPP", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und dennoch ziehst du fort.\u00ab", "tokens": ["Und", "den\u00b7noch", "ziehst", "du", "fort", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich schenk' als Bild den Abschiedsblick", "tokens": ["Ich", "schenk'", "als", "Bild", "den", "Ab\u00b7schieds\u00b7blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als Wort den H\u00e4ndedruck,", "tokens": ["Als", "Wort", "den", "H\u00e4n\u00b7de\u00b7druck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du suchst zu fern des Himmels Gl\u00fcck", "tokens": ["Du", "suchst", "zu", "fern", "des", "Him\u00b7mels", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKA", "ADJD", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In s\u00fcdlich ird'schen Schmuck:", "tokens": ["In", "s\u00fcd\u00b7lich", "ird'\u00b7schen", "Schmuck", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Des S\u00fcdens Kunde dir verhie\u00df", "tokens": ["Des", "S\u00fc\u00b7dens", "Kun\u00b7de", "dir", "ver\u00b7hie\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Bilder reiche Wahl,", "tokens": ["Der", "Bil\u00b7der", "rei\u00b7che", "Wahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich sah das sch\u00f6nste Paradies", "tokens": ["Ich", "sah", "das", "sch\u00f6ns\u00b7te", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bei dir im nord'schen Thal.", "tokens": ["Bei", "dir", "im", "nord'\u00b7schen", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was du dir suchst, du sch\u00f6nes Kind,", "tokens": ["Was", "du", "dir", "suchst", ",", "du", "sch\u00f6\u00b7nes", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVFIN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es ist schon alles dein,", "tokens": ["Es", "ist", "schon", "al\u00b7les", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dir nach im seltnen Wirbelwind", "tokens": ["Dir", "nach", "im", "selt\u00b7nen", "Wir\u00b7bel\u00b7wind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fliegt mit der goldne Schein,", "tokens": ["Fliegt", "mit", "der", "gold\u00b7ne", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dir folgt der Strom wie in das Meer,", "tokens": ["Dir", "folgt", "der", "Strom", "wie", "in", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOKOM", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dir folgt, was du gesandt,", "tokens": ["Dir", "folgt", ",", "was", "du", "ge\u00b7sandt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und hier wird alles \u00f6d' und leer,", "tokens": ["Und", "hier", "wird", "al\u00b7les", "\u00f6d'", "und", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn du dich abgewandt.", "tokens": ["Wenn", "du", "dich", "ab\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "So wie Magnet mit Eisen spielt,", "tokens": ["So", "wie", "Mag\u00b7net", "mit", "Ei\u00b7sen", "spielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Es aus dem Auge zieht,", "tokens": ["Es", "aus", "dem", "Au\u00b7ge", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So nimmst du mit, was wir gef\u00fchlt,", "tokens": ["So", "nimmst", "du", "mit", ",", "was", "wir", "ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der s\u00fc\u00dfe Schmerz uns flieht;", "tokens": ["Der", "s\u00fc\u00b7\u00dfe", "Schmerz", "uns", "flieht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wir sehen wieder in die Welt", "tokens": ["Wir", "se\u00b7hen", "wie\u00b7der", "in", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie in den Wintertag,", "tokens": ["Wie", "in", "den", "Win\u00b7ter\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das gr\u00fcne Laub schon wieder f\u00e4llt", "tokens": ["Das", "gr\u00fc\u00b7ne", "Laub", "schon", "wie\u00b7der", "f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und fliegt dir rauschend nach.", "tokens": ["Und", "fliegt", "dir", "rau\u00b7schend", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}