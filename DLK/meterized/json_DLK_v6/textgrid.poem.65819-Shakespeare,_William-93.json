{"textgrid.poem.65819": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "93", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So lebt' ich nur im Wahne deiner Treu,", "tokens": ["So", "lebt'", "ich", "nur", "im", "Wah\u00b7ne", "dei\u00b7ner", "Treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie ein betrogner Gatte; die Geb\u00e4rde", "tokens": ["Wie", "ein", "be\u00b7trog\u00b7ner", "Gat\u00b7te", ";", "die", "Ge\u00b7b\u00e4r\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur noch der Liebe Trugbild, die vorbei;", "tokens": ["Nur", "noch", "der", "Lie\u00b7be", "Trug\u00b7bild", ",", "die", "vor\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "$,", "PRELS", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Blick bei mir, das Herz auf andrer F\u00e4hrte.", "tokens": ["Der", "Blick", "bei", "mir", ",", "das", "Herz", "auf", "an\u00b7drer", "F\u00e4hr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$,", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Denn weil kein Groll sich deinem Auge naht,", "tokens": ["Denn", "weil", "kein", "Groll", "sich", "dei\u00b7nem", "Au\u00b7ge", "naht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIAT", "NN", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Kann ich darin nicht deinen Wandel schaun.", "tokens": ["Kann", "ich", "da\u00b7rin", "nicht", "dei\u00b7nen", "Wan\u00b7del", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PAV", "PTKNEG", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Aus manchem Blick spricht falscher Herzen Rat", "tokens": ["Aus", "man\u00b7chem", "Blick", "spricht", "fal\u00b7scher", "Her\u00b7zen", "Rat"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Durch Unmut, Ingrimm, Furchen, finstre Brau'n:", "tokens": ["Durch", "Un\u00b7mut", ",", "In\u00b7grimm", ",", "Fur\u00b7chen", ",", "finst\u00b7re", "Brau'n", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NE", "$,", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Dir aber gab des Himmels Sch\u00f6pfersegen", "tokens": ["Dir", "a\u00b7ber", "gab", "des", "Him\u00b7mels", "Sch\u00f6p\u00b7fer\u00b7se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Zu ew'ger ", "tokens": ["Zu", "ew'\u00b7ger"], "token_info": ["word", "word"], "pos": ["APPR", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Da\u00df, wie auch Herz und Sinn sich dir bewegen,", "tokens": ["Da\u00df", ",", "wie", "auch", "Herz", "und", "Sinn", "sich", "dir", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "ADV", "NN", "KON", "NN", "PRF", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Nur Holdes uns von dort entgegenspricht.", "tokens": ["Nur", "Hol\u00b7des", "uns", "von", "dort", "ent\u00b7ge\u00b7gen\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPER", "APPR", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Wie Evas Apfel gleich w\u00e4r deine Jugend,", "tokens": ["Wie", "E\u00b7vas", "Ap\u00b7fel", "gleich", "w\u00e4r", "dei\u00b7ne", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NE", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Glich deinem Schein nicht deine holde Tugend!", "tokens": ["Glich", "dei\u00b7nem", "Schein", "nicht", "dei\u00b7ne", "hol\u00b7de", "Tu\u00b7gend", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "PTKNEG", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}