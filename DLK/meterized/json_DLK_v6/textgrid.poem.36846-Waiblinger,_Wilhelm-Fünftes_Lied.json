{"textgrid.poem.36846": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fcnftes Lied", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie aus heiterstem Gr\u00fcn, o erhabenste Tempel Girgentis,", "tokens": ["Wie", "aus", "hei\u00b7ters\u00b7tem", "Gr\u00fcn", ",", "o", "er\u00b7ha\u00b7bens\u00b7te", "Tem\u00b7pel", "Gir\u00b7gen\u00b7tis", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "$,", "FM", "ADJA", "NN", "NE", "$,"], "meter": "--+--+--+--+-+-+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Wie vom Himmel umgl\u00e4nzt steigt ihr der Nachwelt empor!", "tokens": ["Wie", "vom", "Him\u00b7mel", "um\u00b7gl\u00e4nzt", "steigt", "ihr", "der", "Nach\u00b7welt", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "VVPP", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Zwar in Tr\u00fcmmer schlug euch die Zeit; wohin ich mich wende,", "tokens": ["Zwar", "in", "Tr\u00fcm\u00b7mer", "schlug", "euch", "die", "Zeit", ";", "wo\u00b7hin", "ich", "mich", "wen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "PPER", "ART", "NN", "$.", "PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Zu des olympischen Zeus altem, titanischen Haus,", "tokens": ["Zu", "des", "o\u00b7lym\u00b7pi\u00b7schen", "Zeus", "al\u00b7tem", ",", "ti\u00b7ta\u00b7ni\u00b7schen", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NE", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Sei's zum furchtbaren Schutt des Herakles, sei's zu dem H\u00fcgel,", "tokens": ["Sei's", "zum", "furcht\u00b7ba\u00b7ren", "Schutt", "des", "Her\u00b7ak\u00b7les", ",", "sei's", "zu", "dem", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "ART", "NN", "$,", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Wo vom Fr\u00fchling umbl\u00fcht, Juno Lucina, du einst,", "tokens": ["Wo", "vom", "Fr\u00fch\u00b7ling", "um\u00b7bl\u00fcht", ",", "Ju\u00b7no", "Lu\u00b7ci\u00b7na", ",", "du", "einst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "VVFIN", "$,", "NE", "NE", "$,", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.7": {"text": "Oder die Eintracht dort in dorischer Sch\u00f6ne gewohnet,", "tokens": ["O\u00b7der", "die", "Ein\u00b7tracht", "dort", "in", "do\u00b7ri\u00b7scher", "Sch\u00f6\u00b7ne", "ge\u00b7woh\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Sei's wo der Tempel Vulkans \u00fcber der blumigen Kluft", "tokens": ["Sei's", "wo", "der", "Tem\u00b7pel", "Vul\u00b7kans", "\u00fc\u00b7ber", "der", "blu\u00b7mi\u00b7gen", "Kluft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PWAV", "ART", "NN", "NE", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Von Limonen umduftet, umlacht von Indiens Feigen,", "tokens": ["Von", "Li\u00b7mo\u00b7nen", "um\u00b7duf\u00b7tet", ",", "um\u00b7lacht", "von", "In\u00b7di\u00b7ens", "Fei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "VVFIN", "APPR", "NE", "NN", "$,"], "meter": "--+--+--+--+-+-", "measure": "anapaest.tetra.plus"}, "line.10": {"text": "Kaum den Blick mir zum Strand, kaum bis zum Meere gew\u00e4hrt.", "tokens": ["Kaum", "den", "Blick", "mir", "zum", "Strand", ",", "kaum", "bis", "zum", "Mee\u00b7re", "ge\u00b7w\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "APPRART", "NN", "$,", "ADV", "APPR", "APPRART", "NN", "VVPP", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Euch umgl\u00fchet Natur, und selbst aus dem Grab in der Mauer", "tokens": ["Euch", "um\u00b7gl\u00fc\u00b7het", "Na\u00b7tur", ",", "und", "selbst", "aus", "dem", "Grab", "in", "der", "Mau\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "KON", "ADV", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Strebt der bl\u00fchende Baum m\u00e4chtiger Aloe noch.", "tokens": ["Strebt", "der", "bl\u00fc\u00b7hen\u00b7de", "Baum", "m\u00e4ch\u00b7ti\u00b7ger", "A\u00b7loe", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADJA", "NN", "ADV", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.13": {"text": "J\u00fcngst so irrt' ich im Gr\u00fcn, mir lachten goldene Fr\u00fcchte,", "tokens": ["J\u00fcngst", "so", "irrt'", "ich", "im", "Gr\u00fcn", ",", "mir", "lach\u00b7ten", "gol\u00b7de\u00b7ne", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$,", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "Hier entsprang der Granat, dort die Orange dem Laub.", "tokens": ["Hier", "ent\u00b7sprang", "der", "Gra\u00b7nat", ",", "dort", "die", "O\u00b7ran\u00b7ge", "dem", "Laub", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Eine Nachtigall schlug und die Tempel entragten den Hainen,", "tokens": ["Ei\u00b7ne", "Nach\u00b7ti\u00b7gall", "schlug", "und", "die", "Tem\u00b7pel", "en\u00b7trag\u00b7ten", "den", "Hai\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Da erf\u00fcllete mir Wehmuth das einsame Herz,", "tokens": ["Da", "er\u00b7f\u00fcl\u00b7le\u00b7te", "mir", "Weh\u00b7muth", "das", "ein\u00b7sa\u00b7me", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+-+-+-+", "measure": "anapaest.init"}, "line.17": {"text": "Unaussprechliche fast. So oft ins zerfallene Leben,", "tokens": ["Un\u00b7aus\u00b7sprech\u00b7li\u00b7che", "fast", ".", "So", "oft", "ins", "zer\u00b7fal\u00b7le\u00b7ne", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADV", "$.", "ADV", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Oft in die Tr\u00fcmmer des Gl\u00fccks, oft in der Liebe Verlust", "tokens": ["Oft", "in", "die", "Tr\u00fcm\u00b7mer", "des", "Gl\u00fccks", ",", "oft", "in", "der", "Lie\u00b7be", "Ver\u00b7lust"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Klagt ein s\u00fc\u00dfer, ein seliger Laut mit der Nachtigall Stimme,", "tokens": ["Klagt", "ein", "s\u00fc\u00b7\u00dfer", ",", "ein", "se\u00b7li\u00b7ger", "Laut", "mit", "der", "Nach\u00b7ti\u00b7gall", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,", "ART", "ADJA", "APPR", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.20": {"text": "Und das Sch\u00f6ne vielleicht wohnet am liebsten im Schmerz.", "tokens": ["Und", "das", "Sch\u00f6\u00b7ne", "viel\u00b7leicht", "woh\u00b7net", "am", "liebs\u00b7ten", "im", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "PTKA", "ADJD", "APPRART", "NN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}