{"textgrid.poem.27628": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als aus deiner Sinnen St\u00e4rcke", "tokens": ["Als", "aus", "dei\u00b7ner", "Sin\u00b7nen", "St\u00e4r\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jupiter nahm ein gemerke,", "tokens": ["Ju\u00b7pi\u00b7ter", "nahm", "ein", "ge\u00b7mer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Da\u00df du durch so k\u00fchnes streiten", "tokens": ["Da\u00df", "du", "durch", "so", "k\u00fch\u00b7nes", "strei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00fcrdest bi\u00df zum Himmel schreiten,", "tokens": ["W\u00fcr\u00b7dest", "bi\u00df", "zum", "Him\u00b7mel", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sprach er: Uns die Ehre bleibe!", "tokens": ["Sprach", "er", ":", "Uns", "die", "Eh\u00b7re", "blei\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dannenher ich einverleibe", "tokens": ["Dan\u00b7nen\u00b7her", "ich", "ein\u00b7ver\u00b7lei\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["PAV", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Diesen Held nach Himmels-Rechte", "tokens": ["Die\u00b7sen", "Held", "nach", "Him\u00b7mels\u00b7Rech\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In der G\u00f6tter alt Geschlechte;", "tokens": ["In", "der", "G\u00f6t\u00b7ter", "alt", "Ge\u00b7schlech\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Denn er m\u00f6cht au\u00df eignen Thaten", "tokens": ["Denn", "er", "m\u00f6cht", "au\u00df", "eig\u00b7nen", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "F\u00fcr sich selbst hierher gerathen.", "tokens": ["F\u00fcr", "sich", "selbst", "hier\u00b7her", "ge\u00b7ra\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "PAV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}