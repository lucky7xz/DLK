{"dta.poem.15043": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Von den vortrefflichen Umst\u00e4nden  \n f\u00fcr die Poesie unter den Kaisern aus  \n dem schw\u00e4bischen Hause.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198476", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "geliches willen mueste iehen", "tokens": ["ge\u00b7li\u00b7ches", "wil\u00b7len", "mu\u00b7es\u00b7te", "ie\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VMFIN", "VVINF"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "schiefdeire wart von in gesehen", "tokens": ["schief\u00b7dei\u00b7re", "wart", "von", "in", "ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "VVFIN", "APPR", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "in harte kurzer wile", "tokens": ["in", "har\u00b7te", "kur\u00b7zer", "wi\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "mit snelleclicher ile", "tokens": ["mit", "snel\u00b7le\u00b7cli\u00b7cher", "i\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "kamen si geriten dar", "tokens": ["ka\u00b7men", "si", "ge\u00b7ri\u00b7ten", "dar"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "ein tal von bluomen lieh gefar", "tokens": ["ein", "tal", "von", "blu\u00b7o\u00b7men", "lieh", "ge\u00b7far"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "NN", "VVFIN", "NE"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "lac da bi mit stete", "tokens": ["lac", "da", "bi", "mit", "ste\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "dar inne Gaudin hete", "tokens": ["dar", "in\u00b7ne", "Gau\u00b7din", "he\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKVZ", "ADJA", "NN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "empfangen wunneclich gemach", "tokens": ["emp\u00b7fan\u00b7gen", "wun\u00b7ne\u00b7clich", "ge\u00b7mach"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "ein herberge und ein obedach", "tokens": ["ein", "her\u00b7ber\u00b7ge", "und", "ein", "o\u00b7be\u00b7dach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "was im alda gewunnen", "tokens": ["was", "im", "al\u00b7da", "ge\u00b7wun\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPRART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "bi eime kalten brunnen", "tokens": ["bi", "ei\u00b7me", "kal\u00b7ten", "brun\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "da gruene boume stuonden obe", "tokens": ["da", "gru\u00b7e\u00b7ne", "bou\u00b7me", "stu\u00b7on\u00b7den", "o\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "ein pavilune wol ze lobe", "tokens": ["ein", "pa\u00b7vi\u00b7lu\u00b7ne", "wol", "ze", "lo\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "XY", "XY"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "waz in geslagen uf das velt", "tokens": ["waz", "in", "ges\u00b7la\u00b7gen", "uf", "das", "velt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "unde stuont vil schoene sin gezelt", "tokens": ["un\u00b7de", "stu\u00b7ont", "vil", "schoe\u00b7ne", "sin", "ge\u00b7zelt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "ADJA", "NN", "VVPP"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.17": {"text": "mit loumen und mit grase best", "tokens": ["mit", "lou\u00b7men", "und", "mit", "gra\u00b7se", "best"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "VVINF", "KON", "APPR", "ADJA", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "der meie hete do gevroeut", "tokens": ["der", "mei\u00b7e", "he\u00b7te", "do", "ge\u00b7vro\u00b7eut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.19": {"text": "mit der lichten k&#250;nfte sin", "tokens": ["mit", "der", "lich\u00b7ten", "k", "&#250;", "nf\u00b7te", "sin"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.20": {"text": "diu wilden walt vogelin", "tokens": ["diu", "wil\u00b7den", "walt", "vo\u00b7ge\u00b7lin"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.21": {"text": "dar umbe alda ze prise", "tokens": ["dar", "um\u00b7be", "al\u00b7da", "ze", "pri\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.22": {"text": "ir suezen sumer wise", "tokens": ["ir", "sue\u00b7zen", "su\u00b7mer", "wi\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "wurden lute erklenket", "tokens": ["wur\u00b7den", "lu\u00b7te", "er\u00b7klen\u00b7ket"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "NN", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.24": {"text": "si heten sich gesenket", "tokens": ["si", "he\u00b7ten", "sich", "ge\u00b7sen\u00b7ket"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "in die schoenen bomes bluot", "tokens": ["in", "die", "schoe\u00b7nen", "bo\u00b7mes", "bluot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "vnde liezen sueze stimme guot", "tokens": ["vn\u00b7de", "lie\u00b7zen", "sue\u00b7ze", "stim\u00b7me", "guot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.27": {"text": "des males hellen &#250;beral", "tokens": ["des", "ma\u00b7les", "hel\u00b7len", "&#250;", "be\u00b7ral"], "token_info": ["word", "word", "word", "XML_entity", "word"], "pos": ["ART", "PIS", "VVINF", "$(", "ADJD"], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.28": {"text": "in disem wunneclichen tal", "tokens": ["in", "di\u00b7sem", "wun\u00b7ne\u00b7cli\u00b7chen", "tal"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Gaudin sich nider schiere", "tokens": ["Gau\u00b7din", "sich", "ni\u00b7der", "schie\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PRF", "PTKVZ", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.30": {"text": "lie mit Partinopiere", "tokens": ["lie", "mit", "Par\u00b7ti\u00b7no\u00b7pie\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.31": {"text": "der im die nacht vil eren bot", "tokens": ["der", "im", "die", "nacht", "vil", "e\u00b7ren", "bot"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "ART", "NN", "ADV", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "des tages do das morgen rot", "tokens": ["des", "ta\u00b7ges", "do", "das", "mor\u00b7gen", "rot"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "durch den lichten himmel schein", "tokens": ["durch", "den", "lich\u00b7ten", "him\u00b7mel", "schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}