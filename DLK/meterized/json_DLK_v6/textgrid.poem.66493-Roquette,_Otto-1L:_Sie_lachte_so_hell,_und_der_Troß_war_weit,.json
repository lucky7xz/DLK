{"textgrid.poem.66493": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie lachte so hell, und der Tro\u00df war weit,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie lachte so hell, und der Tro\u00df war weit,", "tokens": ["Sie", "lach\u00b7te", "so", "hell", ",", "und", "der", "Tro\u00df", "war", "weit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "KON", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Jung Diethelm ritt an der Herzogin Seit.", "tokens": ["Jung", "Die\u00b7thelm", "ritt", "an", "der", "Her\u00b7zo\u00b7gin", "Seit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Holde Rast hier am Waldesrande!", "tokens": ["Hol\u00b7de", "Rast", "hier", "am", "Wal\u00b7des\u00b7ran\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Er hob sie vom Zelter: \u00bbO Herrin mein,", "tokens": ["Er", "hob", "sie", "vom", "Zel\u00b7ter", ":", "\u00bb", "O", "Her\u00b7rin", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$.", "$(", "NE", "NN", "PPOSAT", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "So halt' ich dich, la\u00df mich begnadet sein!", "tokens": ["So", "halt'", "ich", "dich", ",", "la\u00df", "mich", "be\u00b7gna\u00b7det", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "VVIMP", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Jorinde, du Sch\u00f6nste im Lande!\u00ab", "tokens": ["Jo\u00b7rin\u00b7de", ",", "du", "Sch\u00f6ns\u00b7te", "im", "Lan\u00b7de", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "PPER", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Sie lacht: Jung Diethelm, ich seh' Euch gern,", "tokens": ["Sie", "lacht", ":", "Jung", "Die\u00b7thelm", ",", "ich", "seh'", "Euch", "gern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NE", "NE", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Doch bieten mir Kronen viel edle Herrn,", "tokens": ["Doch", "bie\u00b7ten", "mir", "Kro\u00b7nen", "viel", "ed\u00b7le", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Was seid Ihr zu bieten im Stande?", "tokens": ["Was", "seid", "Ihr", "zu", "bie\u00b7ten", "im", "Stan\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PTKZU", "VVINF", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "\u00bbich biete mein Herz dir, mein junges Blut!", "tokens": ["\u00bb", "ich", "bie\u00b7te", "mein", "Herz", "dir", ",", "mein", "jun\u00b7ges", "Blut", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPOSAT", "NN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Meinen Lebensdurst, meinen Todesmuth,", "tokens": ["Mei\u00b7nen", "Le\u00b7bens\u00b7durst", ",", "mei\u00b7nen", "To\u00b7des\u00b7muth", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Jorinde, du Sch\u00f6nste im Lande!\u00ab", "tokens": ["Jo\u00b7rin\u00b7de", ",", "du", "Sch\u00f6ns\u00b7te", "im", "Lan\u00b7de", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "PPER", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Jung Diethelm, Ihr hegt viel kecken Muth,", "tokens": ["Jung", "Die\u00b7thelm", ",", "Ihr", "hegt", "viel", "ke\u00b7cken", "Muth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ihr werbt wie ein Knab' um der Minne Gut,", "tokens": ["Ihr", "werbt", "wie", "ein", "Knab'", "um", "der", "Min\u00b7ne", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Sie will gar verschwiegene Bande!", "tokens": ["Sie", "will", "gar", "ver\u00b7schwie\u00b7ge\u00b7ne", "Ban\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "\u00bbo Herrin, ich schweige bis in das Grab,", "tokens": ["\u00bb", "o", "Her\u00b7rin", ",", "ich", "schwei\u00b7ge", "bis", "in", "das", "Grab", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wenn ich alle Seligkeit funden hab,", "tokens": ["Wenn", "ich", "al\u00b7le", "Se\u00b7lig\u00b7keit", "fun\u00b7den", "hab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "VAFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Jorinde, du Sch\u00f6nste im Lande!\u00ab", "tokens": ["Jo\u00b7rin\u00b7de", ",", "du", "Sch\u00f6ns\u00b7te", "im", "Lan\u00b7de", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "PPER", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Sie lacht und sie neckt mit verwirrendem Spiel,", "tokens": ["Sie", "lacht", "und", "sie", "neckt", "mit", "ver\u00b7wir\u00b7ren\u00b7dem", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Aufstieg der Mond, und die D\u00e4mmrung fiel,", "tokens": ["Auf\u00b7stieg", "der", "Mond", ",", "und", "die", "D\u00e4mm\u00b7rung", "fiel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und die Rosse scharrten im Sande.", "tokens": ["Und", "die", "Ros\u00b7se", "scharr\u00b7ten", "im", "San\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "\u00bbo Herrin, du lachst mir mit Augen und Mund,", "tokens": ["\u00bb", "o", "Her\u00b7rin", ",", "du", "lachst", "mir", "mit", "Au\u00b7gen", "und", "Mund", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "PPER", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Mein mu\u00dft du werden zu dieser Stund,", "tokens": ["Mein", "mu\u00dft", "du", "wer\u00b7den", "zu", "die\u00b7ser", "Stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VMFIN", "PPER", "VAFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Jorinde, du Sch\u00f6nste im Lande!\u00ab", "tokens": ["Jo\u00b7rin\u00b7de", ",", "du", "Sch\u00f6ns\u00b7te", "im", "Lan\u00b7de", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "PPER", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Zu Ro\u00df! Horch, horch, des Jagdhorns Ton!", "tokens": ["Zu", "Ro\u00df", "!", "Horch", ",", "horch", ",", "des", "Jagd\u00b7horns", "Ton", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "NE", "$,", "VVIMP", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df ab, du schaffest dir bittern Lohn,", "tokens": ["La\u00df", "ab", ",", "du", "schaf\u00b7fest", "dir", "bit\u00b7tern", "Lohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und schaffest mir Zorn und Schande!", "tokens": ["Und", "schaf\u00b7fest", "mir", "Zorn", "und", "Schan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "\u00bbich la\u00df dich nicht! Wenn ich denn sterben mu\u00df,", "tokens": ["\u00bb", "ich", "la\u00df", "dich", "nicht", "!", "Wenn", "ich", "denn", "ster\u00b7ben", "mu\u00df", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVIMP", "PPER", "PTKNEG", "$.", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So sterb' ich jauchzend in deinem Ku\u00df,", "tokens": ["So", "sterb'", "ich", "jauch\u00b7zend", "in", "dei\u00b7nem", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Jorinde, du Sch\u00f6nste im Lande!\u00ab", "tokens": ["Jo\u00b7rin\u00b7de", ",", "du", "Sch\u00f6ns\u00b7te", "im", "Lan\u00b7de", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "PPER", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Herbrauset der Zug, und ein Schrei wird laut,", "tokens": ["Her\u00b7brau\u00b7set", "der", "Zug", ",", "und", "ein", "Schrei", "wird", "laut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Blank zucket ein Schwert, von Blut bethaut,", "tokens": ["Blank", "zu\u00b7cket", "ein", "Schwert", ",", "von", "Blut", "be\u00b7thaut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Und ein Odem erstirbt auf dem Sande.", "tokens": ["Und", "ein", "O\u00b7dem", "er\u00b7stirbt", "auf", "dem", "San\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Und als sie heimw\u00e4rts ritten die Stra\u00df,", "tokens": ["Und", "als", "sie", "heim\u00b7w\u00e4rts", "rit\u00b7ten", "die", "Stra\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Hoch sa\u00df sie zu Ro\u00df, wie der Tod so bla\u00df,", "tokens": ["Hoch", "sa\u00df", "sie", "zu", "Ro\u00df", ",", "wie", "der", "Tod", "so", "bla\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "NN", "$,", "PWAV", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Jorinde, die Sch\u00f6nste im Lande.", "tokens": ["Jo\u00b7rin\u00b7de", ",", "die", "Sch\u00f6ns\u00b7te", "im", "Lan\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}