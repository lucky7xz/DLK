{"textgrid.poem.40872": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Warnung", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf Flanderns Fluren liegt noch des Winters Mantel von Schnee", "tokens": ["Auf", "Flan\u00b7derns", "Flu\u00b7ren", "liegt", "noch", "des", "Win\u00b7ters", "Man\u00b7tel", "von", "Schnee"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "VVFIN", "ADV", "ART", "NN", "NN", "APPR", "NN"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Drauf gl\u00e4nzen wie Silberverbr\u00e4mung Kan\u00e4le, Weiher und See.", "tokens": ["Drauf", "gl\u00e4n\u00b7zen", "wie", "Sil\u00b7ber\u00b7ver\u00b7br\u00e4\u00b7mung", "Ka\u00b7n\u00e4\u00b7le", ",", "Wei\u00b7her", "und", "See", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVINF", "KOKOM", "NN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+-+--+", "measure": "amphibrach.tetra.plus"}, "line.3": {"text": "Und Br\u00fcgge's Th\u00fcrme und Zinnen erschimmern in blauer Fern',", "tokens": ["Und", "Br\u00fcg\u00b7ge's", "Th\u00fcr\u00b7me", "und", "Zin\u00b7nen", "er\u00b7schim\u00b7mern", "in", "blau\u00b7er", "Fern'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "KON", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wie auf dem wei\u00dfen Mantel ein goldgestickter Stern.", "tokens": ["Wie", "auf", "dem", "wei\u00b7\u00dfen", "Man\u00b7tel", "ein", "gold\u00b7ge\u00b7stick\u00b7ter", "Stern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Da ritt ein Zug von M\u00e4nnern, tief in die W\u00e4mser verh\u00fcllt,", "tokens": ["Da", "ritt", "ein", "Zug", "von", "M\u00e4n\u00b7nern", ",", "tief", "in", "die", "W\u00e4m\u00b7ser", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$,", "ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die B\u00e4rte vom Reife perlend, durchs knisternde Schneegefild.", "tokens": ["Die", "B\u00e4r\u00b7te", "vom", "Rei\u00b7fe", "per\u00b7lend", ",", "durchs", "knis\u00b7tern\u00b7de", "Schnee\u00b7ge\u00b7fild", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ADJD", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "An K\u00f6nig Maxens Seite trieb Kunz manch lustigen Scherz,", "tokens": ["An", "K\u00f6\u00b7nig", "Ma\u00b7xens", "Sei\u00b7te", "trieb", "Kunz", "manch", "lus\u00b7ti\u00b7gen", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "NN", "VVFIN", "NE", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ihm weinte vor Frost das Auge, ihm lachte vor W\u00e4rme das Herz.", "tokens": ["Ihm", "wein\u00b7te", "vor", "Frost", "das", "Au\u00b7ge", ",", "ihm", "lach\u00b7te", "vor", "W\u00e4r\u00b7me", "das", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN", "$,", "PPER", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+--+-+--+--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Da schaute ger\u00fchrt der K\u00f6nig die Flur, die vor ihm lag:", "tokens": ["Da", "schau\u00b7te", "ge\u00b7r\u00fchrt", "der", "K\u00f6\u00b7nig", "die", "Flur", ",", "die", "vor", "ihm", "lag", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVPP", "ART", "NN", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbseht, welch gewaltiger Zeuge, was Menschenkunst vermag!", "tokens": ["\u00bb", "seht", ",", "welch", "ge\u00b7wal\u00b7ti\u00b7ger", "Zeu\u00b7ge", ",", "was", "Men\u00b7schen\u00b7kunst", "ver\u00b7mag", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "PWAT", "ADJA", "NN", "$,", "PWS", "NN", "VVFIN", "$."], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Da liegst du wie im Brautschmuck, mein liebes Flandernland,", "tokens": ["Da", "liegst", "du", "wie", "im", "Brautsc\u00b7hmuck", ",", "mein", "lie\u00b7bes", "Flan\u00b7dern\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "APPRART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Im wei\u00dfen Schlummerkleide, umg\u00fcrtet vom Silberband.\u00ab", "tokens": ["Im", "wei\u00b7\u00dfen", "Schlum\u00b7mer\u00b7klei\u00b7de", ",", "um\u00b7g\u00fcr\u00b7tet", "vom", "Sil\u00b7ber\u00b7band", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Drauf Kunz: \u00bbJa, ja, da liegst du wie eine Dirne der Stadt,", "tokens": ["Drauf", "Kunz", ":", "\u00bb", "Ja", ",", "ja", ",", "da", "liegst", "du", "wie", "ei\u00b7ne", "Dir\u00b7ne", "der", "Stadt", ","], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NE", "$.", "$(", "PTKANT", "$,", "PTKANT", "$,", "ADV", "VVFIN", "PPER", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-++-+--+", "measure": "iambic.septa.chol"}, "line.2": {"text": "So ohne reizende H\u00fcgel, so flach, so glatt und platt,", "tokens": ["So", "oh\u00b7ne", "rei\u00b7zen\u00b7de", "H\u00fc\u00b7gel", ",", "so", "flach", ",", "so", "glatt", "und", "platt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Den G\u00fcrtel etwas locker, geschm\u00fcckt bei Nacht und Tag,", "tokens": ["Den", "G\u00fcr\u00b7tel", "et\u00b7was", "lo\u00b7cker", ",", "ge\u00b7schm\u00fcckt", "bei", "Nacht", "und", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und findest doch deinen Buhlen; Beweis, was Kunst vermag!", "tokens": ["Und", "fin\u00b7dest", "doch", "dei\u00b7nen", "Buh\u00b7len", ";", "Be\u00b7weis", ",", "was", "Kunst", "ver\u00b7mag", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "$.", "NN", "$,", "PWS", "NN", "VVFIN", "$."], "meter": "-+-++-+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.5": {"line.1": {"text": "All' jene Seen und Lachen hast du zu Spiegeln da,", "tokens": ["All'", "je\u00b7ne", "Seen", "und", "La\u00b7chen", "hast", "du", "zu", "Spie\u00b7geln", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PDAT", "NN", "KON", "NN", "VAFIN", "PPER", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die alten Jungfern brauchen gar viele Spiegel ja!", "tokens": ["Die", "al\u00b7ten", "Jung\u00b7fern", "brau\u00b7chen", "gar", "vie\u00b7le", "Spie\u00b7gel", "ja", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Zum Gl\u00fcck sind sie gar winzig, von etwas tr\u00fcbem Glanz,", "tokens": ["Zum", "Gl\u00fcck", "sind", "sie", "gar", "win\u00b7zig", ",", "von", "et\u00b7was", "tr\u00fc\u00b7bem", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$,", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Denn sonst erschr\u00e4k'st du selber, s\u00e4hst du dich treu und ganz.", "tokens": ["Denn", "sonst", "er\u00b7schr\u00e4\u00b7k'st", "du", "sel\u00b7ber", ",", "s\u00e4hst", "du", "dich", "treu", "und", "ganz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "PPER", "PRF", "ADJD", "KON", "ADV", "$."], "meter": "-+-+--+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Und eine Sprache lallst du, wie ein W\u00e4lscher in Deutschland spricht,", "tokens": ["Und", "ei\u00b7ne", "Spra\u00b7che", "lallst", "du", ",", "wie", "ein", "W\u00e4l\u00b7scher", "in", "Deutschland", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+++", "measure": "unknown.measure.octa.plus"}, "line.2": {"text": "Sein W\u00e4lsch hat er vergessen, und Deutsch erlernt' er nicht; \u2013", "tokens": ["Sein", "W\u00e4lsch", "hat", "er", "ver\u00b7ges\u00b7sen", ",", "und", "Deutsch", "er\u00b7lernt'", "er", "nicht", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,", "KON", "NN", "VVFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Als gute Hausfrau fegst du stets Speicher und Scheuern rein,", "tokens": ["Als", "gu\u00b7te", "Haus\u00b7frau", "fegst", "du", "stets", "Spei\u00b7cher", "und", "Scheu\u00b7ern", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "PPER", "ADV", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und, sie nicht zu best\u00e4uben, f\u00fchrst du kein Korn hinein.", "tokens": ["Und", ",", "sie", "nicht", "zu", "be\u00b7st\u00e4u\u00b7ben", ",", "f\u00fchrst", "du", "kein", "Korn", "hin\u00b7ein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPER", "PTKNEG", "PTKZU", "VVINF", "$,", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Und ach dein Keller, o Jammer! wie ein w\u00e4ss'rig Trinkgedicht,", "tokens": ["Und", "ach", "dein", "Kel\u00b7ler", ",", "o", "Jam\u00b7mer", "!", "wie", "ein", "w\u00e4ss'\u00b7rig", "Trink\u00b7ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "$,", "FM", "NN", "$.", "PWAV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Vor lauter Wasser findet heraus den Wein man nicht.", "tokens": ["Vor", "lau\u00b7ter", "Was\u00b7ser", "fin\u00b7det", "he\u00b7raus", "den", "Wein", "man", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ADV", "ART", "NN", "PIS", "PTKNEG", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Mein Herr und K\u00f6nig, ich d\u00e4chte, ihr lie\u00dft die Dirne sein,", "tokens": ["Mein", "Herr", "und", "K\u00f6\u00b7nig", ",", "ich", "d\u00e4ch\u00b7te", ",", "ihr", "lie\u00dft", "die", "Dir\u00b7ne", "sein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Es hei\u00dft, wen sie umarme, dem krache Ripp' und Bein.", "tokens": ["Es", "hei\u00dft", ",", "wen", "sie", "um\u00b7ar\u00b7me", ",", "dem", "kra\u00b7che", "Ripp'", "und", "Bein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Denkt nur des alten Recken \u2013 die M\u00e4hr' ist euch bekannt \u2013", "tokens": ["Denkt", "nur", "des", "al\u00b7ten", "Re\u00b7cken", "\u2013", "die", "M\u00e4hr'", "ist", "euch", "be\u00b7kannt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$(", "ART", "NN", "VAFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der schlief bei einer Dirne einst im Philisterland,", "tokens": ["Der", "schlief", "bei", "ei\u00b7ner", "Dir\u00b7ne", "einst", "im", "Phi\u00b7lis\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da stahl sie ihm bei Nachtzeit den sch\u00f6nen goldnen Zopf!", "tokens": ["Da", "stahl", "sie", "ihm", "bei", "Nacht\u00b7zeit", "den", "sch\u00f6\u00b7nen", "gold\u00b7nen", "Zopf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "NN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Bei Jungfer Flandria, d\u00e4ucht mir, gilt's gleich zum Schopf den Kopf.\u00ab", "tokens": ["Bei", "Jung\u00b7fer", "Fland\u00b7ria", ",", "d\u00e4ucht", "mir", ",", "gilt's", "gleich", "zum", "Schopf", "den", "Kopf", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "NE", "$,", "VVFIN", "PPER", "$,", "VVFIN", "ADV", "APPRART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Darauf ihn schalt der K\u00f6nig: \u00bbEi la\u00df den Grillenfang!", "tokens": ["Da\u00b7rauf", "ihn", "schalt", "der", "K\u00f6\u00b7nig", ":", "\u00bb", "Ei", "la\u00df", "den", "Gril\u00b7len\u00b7fang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVFIN", "ART", "NN", "$.", "$(", "NN", "VVIMP", "ART", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dem eidgetreuen F\u00fcrsten sei vor Gefahr nicht bang;", "tokens": ["Dem", "eid\u00b7ge\u00b7treu\u00b7en", "F\u00fcrs\u00b7ten", "sei", "vor", "Ge\u00b7fahr", "nicht", "bang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dann strahlt sein Wort und Glaube als Leuchte durch die Nacht,", "tokens": ["Dann", "strahlt", "sein", "Wort", "und", "Glau\u00b7be", "als", "Leuch\u00b7te", "durch", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "KON", "NN", "KOUS", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "An seinen goldnen Pforten steht dann die Treu' als Wacht.\u00ab", "tokens": ["An", "sei\u00b7nen", "gold\u00b7nen", "Pfor\u00b7ten", "steht", "dann", "die", "Treu'", "als", "Wacht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "KOUS", "NN", "$.", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "\u00bbnarr bin ich und Junggeselle, so trug ich nie die Kron',", "tokens": ["\u00bb", "narr", "bin", "ich", "und", "Jung\u00b7ge\u00b7sel\u00b7le", ",", "so", "trug", "ich", "nie", "die", "Kron'", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "PPER", "KON", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Drum ward solch hohe Erkenntni\u00df bis jetzt mir nicht zum Lohn;", "tokens": ["Drum", "ward", "solch", "ho\u00b7he", "Er\u00b7kennt\u00b7ni\u00df", "bis", "jetzt", "mir", "nicht", "zum", "Lohn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PIAT", "ADJA", "NN", "APPR", "ADV", "PPER", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Doch, denk' ich, sei's wohl besser, ein freier Narr zu sein,", "tokens": ["Doch", ",", "denk'", "ich", ",", "sei's", "wohl", "bes\u00b7ser", ",", "ein", "frei\u00b7er", "Narr", "zu", "sein", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "$,", "VAFIN", "ADV", "ADJD", "$,", "ART", "ADJA", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Als ein gefangner Weiser, und K\u00f6nig obendrein.", "tokens": ["Als", "ein", "ge\u00b7fang\u00b7ner", "Wei\u00b7ser", ",", "und", "K\u00f6\u00b7nig", "o\u00b7ben\u00b7drein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "KON", "NN", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "Item, es lebt ein Sprichwort auf jedem Mund im Land:", "tokens": ["I\u00b7tem", ",", "es", "lebt", "ein", "Sprich\u00b7wort", "auf", "je\u00b7dem", "Mund", "im", "Land", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "La\u00dft kochen der M\u00f6nche Keuschheit, verliebter Leute Verstand,", "tokens": ["La\u00dft", "ko\u00b7chen", "der", "M\u00f6n\u00b7che", "Keuschheit", ",", "ver\u00b7lieb\u00b7ter", "Leu\u00b7te", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "ART", "ADJA", "NN", "$,", "ADJA", "NN", "NN", "$,"], "meter": "-+--+-+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Als Br\u00fch' gie\u00dft Junkerdemut und Fl\u00e4mmlingstreue drauf,", "tokens": ["Als", "Br\u00fch'", "gie\u00dft", "Jun\u00b7ker\u00b7de\u00b7mut", "und", "Fl\u00e4mm\u00b7lings\u00b7treu\u00b7e", "drauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Gebt's einer Milbe zum Imbi\u00df, so steht sie n\u00fcchtern auf.", "tokens": ["Gebt's", "ei\u00b7ner", "Mil\u00b7be", "zum", "Im\u00b7bi\u00df", ",", "so", "steht", "sie", "n\u00fcch\u00b7tern", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$,", "ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.12": {"line.1": {"text": "Nicht lustet's mich, Genosse im Kerker euch zu sein,", "tokens": ["Nicht", "lus\u00b7tet's", "mich", ",", "Ge\u00b7nos\u00b7se", "im", "Ker\u00b7ker", "euch", "zu", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "$,", "NN", "APPRART", "NN", "PPER", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Darum lebt wohl, mein K\u00f6nig, und denkt der Warnung und mein'", "tokens": ["Da\u00b7rum", "lebt", "wohl", ",", "mein", "K\u00f6\u00b7nig", ",", "und", "denkt", "der", "War\u00b7nung", "und", "mein'"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "ART", "NN", "KON", "PPOSAT"], "meter": "--+--+--+-+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Mein lieber Max, o h\u00f6re den Ruf, den Treue spricht,", "tokens": ["Mein", "lie\u00b7ber", "Max", ",", "o", "h\u00f6\u00b7re", "den", "Ruf", ",", "den", "Treu\u00b7e", "spricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "NE", "$,", "FM", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Noch einmal fleh' ich, wandle durch Br\u00fcgge's Pforten nicht!\u00ab", "tokens": ["Noch", "ein\u00b7mal", "fleh'", "ich", ",", "wand\u00b7le", "durch", "Br\u00fcg\u00b7ge's", "Pfor\u00b7ten", "nicht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "VVFIN", "APPR", "NE", "NN", "PTKNEG", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.13": {"line.