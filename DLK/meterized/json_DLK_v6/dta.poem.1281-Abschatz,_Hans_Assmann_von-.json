{"dta.poem.1281": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Kein Monde gleichet sich dem hellen Sonnen-Lichte/", "tokens": ["Kein", "Mon\u00b7de", "glei\u00b7chet", "sich", "dem", "hel\u00b7len", "Son\u00b7nen\u00b7Lich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr tausend Sternen hat ihr Glantz und Schein den", "tokens": ["F\u00fcr", "tau\u00b7send", "Ster\u00b7nen", "hat", "ihr", "Glantz", "und", "Schein", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "VAFIN", "PPOSAT", "NN", "KON", "NN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Prei\u00df.", "tokens": ["Prei\u00df", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Weil/ ausser Cloris/ ich nun keine Sonne wei\u00df/", "tokens": ["Weil", "/", "aus\u00b7ser", "Clo\u00b7ris", "/", "ich", "nun", "kei\u00b7ne", "Son\u00b7ne", "wei\u00df", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "APPR", "NE", "$(", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Verehr ich auch allein ihr sch\u00f6nes Angesichte.", "tokens": ["Ver\u00b7ehr", "ich", "auch", "al\u00b7lein", "ihr", "sch\u00f6\u00b7nes", "An\u00b7ge\u00b7sich\u00b7te", "."], "token_i