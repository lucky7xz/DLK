{"dta.poem.9944": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "53.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die vernunfft die schl\u00e4ff ich ein/ und ermuntre vieler sin\u0303en/", "tokens": ["Die", "ver\u00b7nunfft", "die", "schl\u00e4ff", "ich", "ein", "/", "und", "er\u00b7munt\u00b7re", "vie\u00b7ler", "si\u00f1en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "PPER", "ART", "$(", "KON", "VVFIN", "PIAT", "NN", "$("], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ich mach arm/ und mache reich/ t\u00f6dte gifft/ und gleiche spin\u0303en.", "tokens": ["Ich", "mach", "arm", "/", "und", "ma\u00b7che", "reich", "/", "t\u00f6d\u00b7te", "gifft", "/", "und", "glei\u00b7che", "spi\u00f1en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$(", "KON", "VVFIN", "ADJD", "$(", "ADJA", "NN", "$(", "KON", "ADJA", "VVINF", "$."], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Lang und kurtz mach ich das leben/ mach es schlim\u0303 und mach es", "tokens": ["Lang", "und", "kurtz", "mach", "ich", "das", "le\u00b7ben", "/", "mach", "es", "schlim\u0303", "und", "mach", "es"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VVFIN", "PPER", "PDS", "VVINF", "$(", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "PPER"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "wohl/", "tokens": ["wohl", "/"], "token_info": ["word", "punct"], "pos": ["ADV", "$("], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Ich sterb in demselben leibe/ dessen krafft ich st\u00e4rcken soll.", "tokens": ["Ich", "sterb", "in", "dem\u00b7sel\u00b7ben", "lei\u00b7be", "/", "des\u00b7sen", "krafft", "ich", "st\u00e4r\u00b7cken", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "$(", "PDS", "VVFIN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}}}}}