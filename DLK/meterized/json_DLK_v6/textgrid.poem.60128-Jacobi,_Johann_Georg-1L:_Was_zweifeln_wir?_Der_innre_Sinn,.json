{"textgrid.poem.60128": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was zweifeln wir? Der innre Sinn,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was zweifeln wir? Der innre Sinn,", "tokens": ["Was", "zwei\u00b7feln", "wir", "?", "Der", "inn\u00b7re", "Sinn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der, ohne T\u00e4uschung, kleine Bienen", "tokens": ["Der", ",", "oh\u00b7ne", "T\u00e4u\u00b7schung", ",", "klei\u00b7ne", "Bie\u00b7nen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "$,", "KOUI", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Belehrt, um eine K\u00f6niginn", "tokens": ["Be\u00b7lehrt", ",", "um", "ei\u00b7ne", "K\u00f6\u00b7ni\u00b7ginn"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "KOUI", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Schwarm versammelt, ihr zu dienen;", "tokens": ["Den", "Schwarm", "ver\u00b7sam\u00b7melt", ",", "ihr", "zu", "die\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der, wenn das Heer von St\u00f6rchen reist,", "tokens": ["Der", ",", "wenn", "das", "Heer", "von", "St\u00f6r\u00b7chen", "reist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihm fern ein be\u00dfres Land verhei\u00dft;", "tokens": ["Ihm", "fern", "ein", "be\u00df\u00b7res", "Land", "ver\u00b7hei\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Derselbe zeugt in uns vom Unsichtbaren,", "tokens": ["Der\u00b7sel\u00b7be", "zeugt", "in", "uns", "vom", "Un\u00b7sicht\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Von dem, was k\u00fcnftig ist, vom Bleibenden und Wahren.", "tokens": ["Von", "dem", ",", "was", "k\u00fcnf\u00b7tig", "ist", ",", "vom", "Blei\u00b7ben\u00b7den", "und", "Wah\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADJD", "VAFIN", "$,", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}