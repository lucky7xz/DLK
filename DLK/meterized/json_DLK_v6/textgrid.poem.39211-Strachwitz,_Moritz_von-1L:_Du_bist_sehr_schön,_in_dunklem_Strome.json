{"textgrid.poem.39211": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du bist sehr sch\u00f6n, in dunklem Strome", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist sehr sch\u00f6n, in dunklem Strome", "tokens": ["Du", "bist", "sehr", "sch\u00f6n", ",", "in", "dunk\u00b7lem", "Stro\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Rollt Dein Gelock, vom Wind gebl\u00e4ht,", "tokens": ["Rollt", "Dein", "Ge\u00b7lock", ",", "vom", "Wind", "ge\u00b7bl\u00e4ht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Deiner Stirne Marmordome,", "tokens": ["Von", "Dei\u00b7ner", "Stir\u00b7ne", "Mar\u00b7mor\u00b7do\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Siegspanier der Majest\u00e4t!", "tokens": ["Ein", "Siegs\u00b7pa\u00b7nier", "der", "Ma\u00b7jes\u00b7t\u00e4t", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als wie die Palme windgebogen,", "tokens": ["Als", "wie", "die", "Pal\u00b7me", "wind\u00b7ge\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wogt Deines Wuchses schlanke H\u00f6h',", "tokens": ["Wogt", "Dei\u00b7nes", "Wuch\u00b7ses", "schlan\u00b7ke", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und Deines Kleides samtnen Wogen", "tokens": ["Und", "Dei\u00b7nes", "Klei\u00b7des", "samt\u00b7nen", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Entsteigt Dein Hals, wie Schaum der See.", "tokens": ["Ent\u00b7steigt", "Dein", "Hals", ",", "wie", "Schaum", "der", "See", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PWAV", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Musik ist unter Deinen F\u00fc\u00dfen,", "tokens": ["Mu\u00b7sik", "ist", "un\u00b7ter", "Dei\u00b7nen", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Es gr\u00fcnt die Flur, die Dich umgibt,", "tokens": ["Es", "gr\u00fcnt", "die", "Flur", ",", "die", "Dich", "um\u00b7gibt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ich h\u00f6r' es klingen, seh' es sprie\u00dfen,", "tokens": ["Ich", "h\u00f6r'", "es", "klin\u00b7gen", ",", "seh'", "es", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$,", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und doch \u2013 ", "tokens": ["Und", "doch", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADV", "$("], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Du bist sehr sch\u00f6n, und dank dem Gl\u00fccke,", "tokens": ["Du", "bist", "sehr", "sch\u00f6n", ",", "und", "dank", "dem", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Samt Deiner Sch\u00f6nheit warst Du ", "tokens": ["Samt", "Dei\u00b7ner", "Sch\u00f6n\u00b7heit", "warst", "Du"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "VAFIN", "PPER"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Ich bin erwacht an Deinem Blicke,", "tokens": ["Ich", "bin", "er\u00b7wacht", "an", "Dei\u00b7nem", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Deinem Kusse schlief ich ein;", "tokens": ["Mit", "Dei\u00b7nem", "Kus\u00b7se", "schlief", "ich", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein war der Mund, der liebesschw\u00fcle,", "tokens": ["Mein", "war", "der", "Mund", ",", "der", "lie\u00b7bes\u00b7schw\u00fc\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und mein der Busen ohnegleich,", "tokens": ["Und", "mein", "der", "Bu\u00b7sen", "oh\u00b7ne\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In dessen sel'ger Wogenk\u00fchle", "tokens": ["In", "des\u00b7sen", "sel'\u00b7ger", "Wo\u00b7gen\u00b7k\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Kaiser gern vertr\u00e4umt sein Reich.", "tokens": ["Ein", "Kai\u00b7ser", "gern", "ver\u00b7tr\u00e4umt", "sein", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da war kein Zug um Deine Stirne,", "tokens": ["Da", "war", "kein", "Zug", "um", "Dei\u00b7ne", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der mir geh\u00f6rt' nicht ganz und gar;", "tokens": ["Der", "mir", "ge\u00b7h\u00f6rt'", "nicht", "ganz", "und", "gar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "PTKNEG", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Da war kein Hauch in Deinem Hirne,", "tokens": ["Da", "war", "kein", "Hauch", "in", "Dei\u00b7nem", "Hir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Der mir nicht ganz verfallen war.", "tokens": ["Der", "mir", "nicht", "ganz", "ver\u00b7fal\u00b7len", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "ADV", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du warst sehr sch\u00f6n, Du bist's noch heute,", "tokens": ["Du", "warst", "sehr", "sch\u00f6n", ",", "Du", "bist's", "noch", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du warst sehr stolz, das ist vorbei!", "tokens": ["Du", "warst", "sehr", "stolz", ",", "das", "ist", "vor\u00b7bei", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PDS", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich war mit Dir in langem Streite,", "tokens": ["Ich", "war", "mit", "Dir", "in", "lan\u00b7gem", "Strei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als Hunderte zu Deinen Sohlen", "tokens": ["Als", "Hun\u00b7der\u00b7te", "zu", "Dei\u00b7nen", "Soh\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr stolzes Haupt zum Staub geb\u00fcckt,", "tokens": ["Ihr", "stol\u00b7zes", "Haupt", "zum", "Staub", "ge\u00b7b\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hab' ich die Rache mir befohlen,", "tokens": ["Hab'", "ich", "die", "Ra\u00b7che", "mir", "be\u00b7foh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und wunderbarlich ist's gegl\u00fcckt.", "tokens": ["Und", "wun\u00b7der\u00b7bar\u00b7lich", "ist's", "ge\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zwei Jahr' an Deinem Siegeswagen", "tokens": ["Zwei", "Jahr'", "an", "Dei\u00b7nem", "Sie\u00b7ges\u00b7wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Hab' ich den Hals im Joch ge\u00fcbt,", "tokens": ["Hab'", "ich", "den", "Hals", "im", "Joch", "ge\u00b7\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Nur, um Dir heute kurz zu sagen:", "tokens": ["Nur", ",", "um", "Dir", "heu\u00b7te", "kurz", "zu", "sa\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUI", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und doch, ich hab' Dich nie geliebt!", "tokens": ["Und", "doch", ",", "ich", "hab'", "Dich", "nie", "ge\u00b7liebt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}