{"textgrid.poem.40914": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "1L: Chumm, Agethli, und f\u00f6rcht der nit,", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Chumm, Agethli, und f\u00f6rcht der nit,", "tokens": ["Chumm", ",", "A\u00b7geth\u00b7li", ",", "und", "f\u00f6rcht", "der", "nit", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "KON", "VVFIN", "ART", "PTKNEG", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "i merk scho, was de sage witt.", "tokens": ["i", "merk", "scho", ",", "was", "de", "sa\u00b7ge", "witt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "$,", "PRELS", "NE", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Chumm, bschau di G\u00f6tti nonemol,", "tokens": ["Chumm", ",", "bschau", "di", "G\u00f6t\u00b7ti", "no\u00b7ne\u00b7mol", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und brieg nit so, es isch em wohl.", "tokens": ["und", "brieg", "nit", "so", ",", "es", "isch", "em", "wohl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "$,", "PPER", "ADJD", "ADJA", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er lit so still und fr\u00fcndli do,", "tokens": ["Er", "lit", "so", "still", "und", "fr\u00fcnd\u00b7li", "do", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "me meint, er los und h\u00f6r mi no,", "tokens": ["me", "meint", ",", "er", "los", "und", "h\u00f6r", "mi", "no", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "$,", "PPER", "PTKVZ", "KON", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "er l\u00e4chlet frei, o Jesis Gott,", "tokens": ["er", "l\u00e4ch\u00b7let", "frei", ",", "o", "Je\u00b7sis", "Gott", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "FM", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "as wenn er n\u00e4umis sage wott.", "tokens": ["as", "wenn", "er", "n\u00e4u\u00b7mis", "sa\u00b7ge", "wott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "ADV", "VVFIN", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Er het e schweri Chranket gha.", "tokens": ["Er", "het", "e", "schwe\u00b7ri", "Chran\u00b7ket", "gha", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Er seit: \u00bbEs griift mi n\u00fcmmen a,", "tokens": ["Er", "seit", ":", "\u00bb", "Es", "griift", "mi", "n\u00fcm\u00b7men", "a", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "$.", "$(", "PPER", "VVFIN", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "der Tod het jez mi Wunsch erf\u00fcllt", "tokens": ["der", "Tod", "het", "jez", "mi", "Wunsch", "er\u00b7f\u00fcllt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "NE", "NE", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "und het mi hitzig Fieber gstillt.\u00ab", "tokens": ["und", "het", "mi", "hit\u00b7zig", "Fie\u00b7ber", "gstillt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "NE", "ADJD", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Er het au menge Chummer gha.", "tokens": ["Er", "het", "au", "men\u00b7ge", "Chum\u00b7mer", "gha", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NE", "NE", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.14": {"text": "Er seit: \u00bbEs ficht mi n\u00fcmmen a,", "tokens": ["Er", "seit", ":", "\u00bb", "Es", "ficht", "mi", "n\u00fcm\u00b7men", "a", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "$.", "$(", "PPER", "VVFIN", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "und wienes goht, und was es git,", "tokens": ["und", "wie\u00b7nes", "goht", ",", "und", "was", "es", "git", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "KON", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "im Chilchhof niede h\u00f6ri's nit.\u00ab", "tokens": ["im", "Chilch\u00b7hof", "nie\u00b7de", "h\u00f6\u00b7ri's", "nit", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NE", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Er het e b\u00f6se Nochber gha.", "tokens": ["Er", "het", "e", "b\u00f6\u00b7se", "Noch\u00b7ber", "gha", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Er seit: \u00bbI denk em n\u00fcmme dra,", "tokens": ["Er", "seit", ":", "\u00bb", "I", "denk", "em", "n\u00fcm\u00b7me", "dra", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "$.", "$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "und was em fehlt, das tr\u00f6st en Gott", "tokens": ["und", "was", "em", "fehlt", ",", "das", "tr\u00f6st", "en", "Gott"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ADJA", "VVFIN", "$,", "PDS", "VVFIN", "FM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "und gebem au ne sanfte Tod.\u00ab", "tokens": ["und", "ge\u00b7bem", "au", "ne", "sanf\u00b7te", "Tod", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJA", "NE", "NE", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Er het au sini Fehler gha.", "tokens": ["Er", "het", "au", "si\u00b7ni", "Feh\u00b7ler", "gha", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NE", "NN", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.22": {"text": "'s macht n\u00fct! Mer denke n\u00fcmme dra.", "tokens": ["'s", "macht", "n\u00fct", "!", "Mer", "den\u00b7ke", "n\u00fcm\u00b7me", "dra", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.23": {"text": "Er seit: \u00bbI bi jez frei dervo,", "tokens": ["Er", "seit", ":", "\u00bb", "I", "bi", "jez", "frei", "der\u00b7vo", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "$.", "$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "'s isch nie us b\u00f6sem Herze cho.\u00ab", "tokens": ["'s", "isch", "nie", "us", "b\u00f6\u00b7sem", "Her\u00b7ze", "cho", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJD", "ADV", "NE", "ADJA", "VVFIN", "NE", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.25": {"text": "Er schloft, und luegt di n\u00fcmmen a,", "tokens": ["Er", "schloft", ",", "und", "luegt", "di", "n\u00fcm\u00b7men", "a", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "und het so gern si Gotte gha.", "tokens": ["und", "het", "so", "gern", "si", "Got\u00b7te", "gha", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "NE", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Er seit: \u00bbWills Gott, mer werde scho", "tokens": ["Er", "seit", ":", "\u00bb", "Wills", "Gott", ",", "mer", "wer\u00b7de", "scho"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "APPR", "$.", "$(", "VMFIN", "NN", "$,", "ADV", "VAFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "im Himmel wieder zsemme cho!\u00ab", "tokens": ["im", "Him\u00b7mel", "wie\u00b7der", "zsem\u00b7me", "cho", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Gang, Agethli, und denk mer dra!", "tokens": ["Gang", ",", "A\u00b7geth\u00b7li", ",", "und", "denk", "mer", "dra", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "KON", "VVFIN", "ADV", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.30": {"text": "De hesch e brave G\u00f6tti gha.", "tokens": ["De", "hesch", "e", "bra\u00b7ve", "G\u00f6t\u00b7ti", "gha", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Gang, Agethli, und halt di wohl!", "tokens": ["Gang", ",", "A\u00b7geth\u00b7li", ",", "und", "halt", "di", "wohl", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "KON", "VVFIN", "NE", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.32": {"text": "Di St\u00fcndli schlacht der au ne mol.", "tokens": ["Di", "St\u00fcnd\u00b7li", "schlacht", "der", "au", "ne", "mol", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ART", "NE", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}