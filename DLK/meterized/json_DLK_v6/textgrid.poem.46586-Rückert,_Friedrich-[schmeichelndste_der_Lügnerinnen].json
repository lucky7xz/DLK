{"textgrid.poem.46586": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[schmeichelndste der L\u00fcgnerinnen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schmeichelndste der L\u00fcgnerinnen,", "tokens": ["Schmei\u00b7chelnds\u00b7te", "der", "L\u00fcg\u00b7ne\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Hoffnung,", "tokens": ["Hoff\u00b7nung", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "La\u00df die T\u00e4uschung nicht zerrinnen,", "tokens": ["La\u00df", "die", "T\u00e4u\u00b7schung", "nicht", "zer\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoffnung.", "tokens": ["Hoff\u00b7nung", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Webe zu dein Truggewebe,", "tokens": ["We\u00b7be", "zu", "dein", "Trug\u00b7ge\u00b7we\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fahr' nur", "tokens": ["Fahr'", "nur"], "token_info": ["word", "word"], "pos": ["NE", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Fort, den goldnen Duft zu spinnen,", "tokens": ["Fort", ",", "den", "gold\u00b7nen", "Duft", "zu", "spin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoffnung!", "tokens": ["Hoff\u00b7nung", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Einen Schleier \u00fcber Mutter-", "tokens": ["Ei\u00b7nen", "Schlei\u00b7er", "\u00fc\u00b7ber", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "TRUNC"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Augen,", "tokens": ["Au\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Blendwerk \u00fcber Muttersinnen,", "tokens": ["Blend\u00b7werk", "\u00fc\u00b7ber", "Mut\u00b7ter\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoffnung!", "tokens": ["Hoff\u00b7nung", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "Da\u00df sie w\u00e4hne, m\u00fctterliche", "tokens": ["Da\u00df", "sie", "w\u00e4h\u00b7ne", ",", "m\u00fct\u00b7ter\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Pflege", "tokens": ["Pfle\u00b7ge"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "M\u00fc\u00df' es \u00fcber'n Tod gewinnen,", "tokens": ["M\u00fc\u00df'", "es", "\u00fc\u00b7ber'n", "Tod", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoffnung!", "tokens": ["Hoff\u00b7nung", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.5": {"line.1": {"text": "Da\u00df sie in des Sterbefl\u00e4mmchens", "tokens": ["Da\u00df", "sie", "in", "des", "Ster\u00b7be\u00b7fl\u00e4mm\u00b7chens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zucken", "tokens": ["Zu\u00b7cken"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Neues Leben seh' beginnen,", "tokens": ["Neu\u00b7es", "Le\u00b7ben", "seh'", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoffnung!", "tokens": ["Hoff\u00b7nung", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.6": {"line.1": {"text": "Halt am Krankenbett die Kranke", "tokens": ["Halt", "am", "Kran\u00b7ken\u00b7bett", "die", "Kran\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aufrecht,", "tokens": ["Auf\u00b7recht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "W\u00e4rterin der W\u00e4rterinnen,", "tokens": ["W\u00e4r\u00b7te\u00b7rin", "der", "W\u00e4r\u00b7te\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoffnung!", "tokens": ["Hoff\u00b7nung", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.7": {"line.1": {"text": "Weil du von mir bist entflohen,", "tokens": ["Weil", "du", "von", "mir", "bist", "ent\u00b7flo\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Floh ich;", "tokens": ["Floh", "ich", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Bleib du bei der Mutter drinnen,", "tokens": ["Bleib", "du", "bei", "der", "Mut\u00b7ter", "drin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoffnung!", "tokens": ["Hoff\u00b7nung", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.8": {"line.1": {"text": "Wer, wenn alle sie verlassen,", "tokens": ["Wer", ",", "wenn", "al\u00b7le", "sie", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "KOUS", "PIS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bleibt ihr,", "tokens": ["Bleibt", "ihr", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Wenn auch du noch gehst von hinnen,", "tokens": ["Wenn", "auch", "du", "noch", "gehst", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ADV", "VVFIN", "APPR", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoffnung?", "tokens": ["Hoff\u00b7nung", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.9": {"line.1": {"text": "Einer bleibt, von dem gesandt du", "tokens": ["Ei\u00b7ner", "bleibt", ",", "von", "dem", "ge\u00b7sandt", "du"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "APPR", "ART", "VVPP", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Selber", "tokens": ["Sel\u00b7ber"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Schwebst herab von jenen Zinnen,", "tokens": ["Schwebst", "her\u00b7ab", "von", "je\u00b7nen", "Zin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoffnung!", "tokens": ["Hoff\u00b7nung", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}