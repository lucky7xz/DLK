{"textgrid.poem.46039": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Am vierzehnten Sonntage nach Pfingsten", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Niemand kann zwei Herren dienen,", "tokens": ["Nie\u00b7mand", "kann", "zwei", "Her\u00b7ren", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "CARD", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gleich der eine uns mi\u00dff\u00e4llt,", "tokens": ["Gleich", "der", "ei\u00b7ne", "uns", "mi\u00df\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ART", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn der andre lieb erschienen;", "tokens": ["Wenn", "der", "and\u00b7re", "lieb", "er\u00b7schie\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer Gott dient, dient nicht dem Geld.", "tokens": ["Wer", "Gott", "dient", ",", "dient", "nicht", "dem", "Geld", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$,", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sorg' nicht, wer wird Speise geben,", "tokens": ["Sor\u00b7g'", "nicht", ",", "wer", "wird", "Spei\u00b7se", "ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PWS", "VAFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer h\u00e4lt mir ein Kleid bereit?", "tokens": ["Wer", "h\u00e4lt", "mir", "ein", "Kleid", "be\u00b7reit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mehr als Speise ja ist Leben,", "tokens": ["Mehr", "als", "Spei\u00b7se", "ja", "ist", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "NN", "ADV", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mehr als Kleid ja ist der Leib.", "tokens": ["Mehr", "als", "Kleid", "ja", "ist", "der", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "NN", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sieh, des Himmels V\u00f6glein s\u00e4en,", "tokens": ["Sieh", ",", "des", "Him\u00b7mels", "V\u00f6\u00b7glein", "s\u00e4\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ernten nicht, Gott sie ern\u00e4hrt,", "tokens": ["Ern\u00b7ten", "nicht", ",", "Gott", "sie", "er\u00b7n\u00e4hrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "NN", "PPER", "VVPP", "$,"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Wird auf eure Not auch sehen,", "tokens": ["Wird", "auf", "eu\u00b7re", "Not", "auch", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr seid mehr als sie ja wert.", "tokens": ["Ihr", "seid", "mehr", "als", "sie", "ja", "wert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "KOUS", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Wer mit allem Sorgen, Sinnen", "tokens": ["Wer", "mit", "al\u00b7lem", "Sor\u00b7gen", ",", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "APPR", "PIS", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4chst auch eine Elle nur?", "tokens": ["W\u00e4chst", "auch", "ei\u00b7ne", "El\u00b7le", "nur", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seht die Lilien, die nicht spinnen,", "tokens": ["Seht", "die", "Li\u00b7li\u00b7en", ",", "die", "nicht", "spin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Die nicht weben, auf der Flur.", "tokens": ["Die", "nicht", "we\u00b7ben", ",", "auf", "der", "Flur", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VVINF", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Salomo, voll Herrlichkeiten,", "tokens": ["Sa\u00b7lo\u00b7mo", ",", "voll", "Herr\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trug kein Kleid von solcher Zier,", "tokens": ["Trug", "kein", "Kleid", "von", "sol\u00b7cher", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Speis' und Kleid ist Sorg' der Heiden,", "tokens": ["Speis'", "und", "Kleid", "ist", "Sor\u00b7g'", "der", "Hei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VAFIN", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Mehr seid ja als Blumen ihr.", "tokens": ["Mehr", "seid", "ja", "als", "Blu\u00b7men", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "KOUS", "NN", "PPER", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Das Bed\u00fcrfnis von euch allen", "tokens": ["Das", "Be\u00b7d\u00fcrf\u00b7nis", "von", "euch", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kennt der Vater. Gottes Reich", "tokens": ["Kennt", "der", "Va\u00b7ter", ".", "Got\u00b7tes", "Reich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Suchet erst, und sein Gefallen", "tokens": ["Su\u00b7chet", "erst", ",", "und", "sein", "Ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gibt euch all das andre gleich.", "tokens": ["Gibt", "euch", "all", "das", "and\u00b7re", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "PDS", "PIS", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}