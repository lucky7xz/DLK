{"textgrid.poem.56533": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Stra\u00dfe wird zu einem breiten Strich.", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Stra\u00dfe wird zu einem breiten Strich.", "tokens": ["Die", "Stra\u00b7\u00dfe", "wird", "zu", "ei\u00b7nem", "brei\u00b7ten", "Strich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die H\u00e4user werden wei\u00df wie eine Wand.", "tokens": ["Die", "H\u00e4u\u00b7ser", "wer\u00b7den", "wei\u00df", "wie", "ei\u00b7ne", "Wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAINF", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Sonne wird ein Mond. Und unbekannt,", "tokens": ["Die", "Son\u00b7ne", "wird", "ein", "Mond", ".", "Und", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$.", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gleichg\u00fcltig, fremd, ein jedes Angesicht.", "tokens": ["Gleich\u00b7g\u00fcl\u00b7tig", ",", "fremd", ",", "ein", "je\u00b7des", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie sehen aus wie Bl\u00e4tter von Papier,", "tokens": ["Sie", "se\u00b7hen", "aus", "wie", "Bl\u00e4t\u00b7ter", "von", "Pa\u00b7pier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KOKOM", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wei\u00df, unbeschrieben. Aber hinten winkt.", "tokens": ["Wei\u00df", ",", "un\u00b7be\u00b7schrie\u00b7ben", ".", "A\u00b7ber", "hin\u00b7ten", "winkt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$.", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein schlankes blaues Kleid, das fern versinkt", "tokens": ["Ein", "schlan\u00b7kes", "blau\u00b7es", "Kleid", ",", "das", "fern", "ver\u00b7sinkt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "PRELS", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und wieder auftaucht, und sich fern verliert.", "tokens": ["Und", "wie\u00b7der", "auf\u00b7taucht", ",", "und", "sich", "fern", "ver\u00b7liert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$,", "KON", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Auf seinem Nacken sitzt die Eifersucht.", "tokens": ["Auf", "sei\u00b7nem", "Na\u00b7cken", "sitzt", "die", "Ei\u00b7fer\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein altes Weib, gestiefelt. Einen Dorn", "tokens": ["Ein", "al\u00b7tes", "Weib", ",", "ge\u00b7stie\u00b7felt", ".", "Ei\u00b7nen", "Dorn"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP", "$.", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bohrt in das Hirn sie ihm, und haut den Sporn", "tokens": ["Bohrt", "in", "das", "Hirn", "sie", "ihm", ",", "und", "haut", "den", "Sporn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "PPER", "PPER", "$,", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In ihres Reittiers weicher Flanken Bucht.", "tokens": ["In", "ih\u00b7res", "Reit\u00b7tiers", "wei\u00b7cher", "Flan\u00b7ken", "Bucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}