{"dta.poem.13838": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die ernsthafte Fastnacht  1814.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Bleich schon fallen Abendlichter \u2014", "tokens": ["Bleich", "schon", "fal\u00b7len", "A\u00b7bend\u00b7lich\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Reiter bl\u00e4\u00dft nur zu,", "tokens": ["Und", "der", "Rei\u00b7ter", "bl\u00e4\u00dft", "nur", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nacht schon webt sich dicht und dichter \u2014", "tokens": ["Nacht", "schon", "webt", "sich", "dicht", "und", "dich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PRF", "ADJD", "KON", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch das Thor bleibt immer zu.", "tokens": ["Doch", "das", "Thor", "bleibt", "im\u00b7mer", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}