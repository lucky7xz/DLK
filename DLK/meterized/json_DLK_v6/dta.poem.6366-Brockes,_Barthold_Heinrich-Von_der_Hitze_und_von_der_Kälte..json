{"dta.poem.6366": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es haben ebenfalls, auch im Bewegen,", "tokens": ["Es", "ha\u00b7ben", "e\u00b7ben\u00b7falls", ",", "auch", "im", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So Hitz als Frost durchaus nicht gleiche Gr\u00fcnde:", "tokens": ["So", "Hitz", "als", "Frost", "durc\u00b7haus", "nicht", "glei\u00b7che", "Gr\u00fcn\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOUS", "NN", "ADV", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Hitze stammt aus der Bewegung in der N\u00fcnde;", "tokens": ["Die", "Hit\u00b7ze", "stammt", "aus", "der", "Be\u00b7we\u00b7gung", "in", "der", "N\u00fcn\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Frost entsteht, wenn sich gerade Striche regen.", "tokens": ["Der", "Frost", "ent\u00b7steht", ",", "wenn", "sich", "ge\u00b7ra\u00b7de", "Stri\u00b7che", "re\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PRF", "ADV", "ADJA", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das werden wir gewahr, wenn der beeiste Nord,", "tokens": ["Das", "wer\u00b7den", "wir", "ge\u00b7wahr", ",", "wenn", "der", "be\u00b7eis\u00b7te", "Nord", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So wild, als grausamlich, das Feld verheert.", "tokens": ["So", "wild", ",", "als", "grau\u00b7sam\u00b7lich", ",", "das", "Feld", "ver\u00b7heert", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "ADJD", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Lufft mu\u00df im geraden Strich", "tokens": ["Die", "Lufft", "mu\u00df", "im", "ge\u00b7ra\u00b7den", "Strich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sodann mit seinem W\u00fcten fort,", "tokens": ["So\u00b7dann", "mit", "sei\u00b7nem", "W\u00fc\u00b7ten", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sie drehet sich nicht mehr im Circkel: es erstarrt", "tokens": ["Sie", "dre\u00b7het", "sich", "nicht", "mehr", "im", "Cir\u00b7ckel", ":", "es", "er\u00b7starrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "ADV", "APPRART", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Selbst die Natur durchs Winters Gegenwart.", "tokens": ["Selbst", "die", "Na\u00b7tur", "durchs", "Win\u00b7ters", "Ge\u00b7gen\u00b7wart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}