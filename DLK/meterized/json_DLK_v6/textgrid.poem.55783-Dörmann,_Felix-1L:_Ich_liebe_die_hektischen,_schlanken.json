{"textgrid.poem.55783": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich liebe die hektischen, schlanken", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich liebe die hektischen, schlanken", "tokens": ["Ich", "lie\u00b7be", "die", "hek\u00b7ti\u00b7schen", ",", "schlan\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Narzissen mit blutrothem Mund;", "tokens": ["Nar\u00b7zis\u00b7sen", "mit", "blut\u00b7ro\u00b7them", "Mund", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich liebe die Qualengedanken,", "tokens": ["Ich", "lie\u00b7be", "die", "Qua\u00b7len\u00b7ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Herzen zerstochen und wund;", "tokens": ["Die", "Her\u00b7zen", "zer\u00b7sto\u00b7chen", "und", "wund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Ich liebe die Fahlen und Bleichen,", "tokens": ["Ich", "lie\u00b7be", "die", "Fah\u00b7len", "und", "Blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Frauen mit m\u00fcdem Gesicht,", "tokens": ["Die", "Frau\u00b7en", "mit", "m\u00fc\u00b7dem", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Aus welchen in flammenden Zeichen,", "tokens": ["Aus", "wel\u00b7chen", "in", "flam\u00b7men\u00b7den", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Verzehrende Sinnenglut spricht;", "tokens": ["Ver\u00b7zeh\u00b7ren\u00b7de", "Sin\u00b7nen\u00b7glut", "spricht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Ich liebe die schillernden Schlangen,", "tokens": ["Ich", "lie\u00b7be", "die", "schil\u00b7lern\u00b7den", "Schlan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "So schmiegsam und biegsam und k\u00fchl:", "tokens": ["So", "schmieg\u00b7sam", "und", "bieg\u00b7sam", "und", "k\u00fchl", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich liebe die klagenden, bangen,", "tokens": ["Ich", "lie\u00b7be", "die", "kla\u00b7gen\u00b7den", ",", "ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Lieder von Todesgef\u00fchl;", "tokens": ["Die", "Lie\u00b7der", "von", "To\u00b7des\u00b7ge\u00b7f\u00fchl", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Ich liebe die herzlosen, gr\u00fcnen", "tokens": ["Ich", "lie\u00b7be", "die", "herz\u00b7lo\u00b7sen", ",", "gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ART", "VVINF", "$,", "ADJA"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Smaragde vor jedem Gestein;", "tokens": ["Sma\u00b7rag\u00b7de", "vor", "je\u00b7dem", "Ge\u00b7stein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich liebe die gelblichen D\u00fcnen", "tokens": ["Ich", "lie\u00b7be", "die", "gelb\u00b7li\u00b7chen", "D\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Im bl\u00e4ulichen Mondenschein;", "tokens": ["Im", "bl\u00e4u\u00b7li\u00b7chen", "Mon\u00b7den\u00b7schein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Ich liebe die glutendurchtr\u00e4nkten,", "tokens": ["Ich", "lie\u00b7be", "die", "glu\u00b7ten\u00b7durch\u00b7tr\u00e4nk\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die D\u00fcfte, berauschend und schwer;", "tokens": ["Die", "D\u00fcf\u00b7te", ",", "be\u00b7rau\u00b7schend", "und", "schwer", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "KON", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die Wolken, die blitzedurchsengten,", "tokens": ["Die", "Wol\u00b7ken", ",", "die", "bli\u00b7tze\u00b7durch\u00b7seng\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Das graue wuthsch\u00e4umende Meer;", "tokens": ["Das", "grau\u00b7e", "wuth\u00b7sch\u00e4u\u00b7men\u00b7de", "Meer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Ich liebe, was niemand erlesen,", "tokens": ["Ich", "lie\u00b7be", ",", "was", "nie\u00b7mand", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PIS", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Was keinem zu lieben gelang:", "tokens": ["Was", "kei\u00b7nem", "zu", "lie\u00b7ben", "ge\u00b7lang", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Mein eigenes, urinnerstes Wesen", "tokens": ["Mein", "ei\u00b7ge\u00b7nes", ",", "u\u00b7rin\u00b7ners\u00b7tes", "We\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und alles, was seltsam und krank.", "tokens": ["Und", "al\u00b7les", ",", "was", "selt\u00b7sam", "und", "krank", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}