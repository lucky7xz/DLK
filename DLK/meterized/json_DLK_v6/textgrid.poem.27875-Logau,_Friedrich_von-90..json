{"textgrid.poem.27875": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "90.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Kretze ward wie Gott geehrt bey manchen Heyden,", "tokens": ["Die", "Kret\u00b7ze", "ward", "wie", "Gott", "ge\u00b7ehrt", "bey", "man\u00b7chen", "Hey\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "NN", "VVPP", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Damit sie ihre Pein nicht etwa d\u00fcrfften leiden.", "tokens": ["Da\u00b7mit", "sie", "ih\u00b7re", "Pein", "nicht", "et\u00b7wa", "d\u00fcrff\u00b7ten", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Rauber und ein Dieb wird darum ietzt gesch\u00e4tzt,", "tokens": ["Ein", "Rau\u00b7ber", "und", "ein", "Dieb", "wird", "da\u00b7rum", "ietzt", "ge\u00b7sch\u00e4tzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VAFIN", "PAV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht da\u00df er uns gef\u00e4llt, nur da\u00df er nicht verletzt.", "tokens": ["Nicht", "da\u00df", "er", "uns", "ge\u00b7f\u00e4llt", ",", "nur", "da\u00df", "er", "nicht", "ver\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "PPER", "PPER", "VVPP", "$,", "ADV", "KOUS", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}