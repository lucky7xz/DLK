{"textgrid.poem.43523": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Abendstern", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du lieblicher Stern,", "tokens": ["Du", "lieb\u00b7li\u00b7cher", "Stern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Du leuchtest so fern.", "tokens": ["Du", "leuch\u00b7test", "so", "fern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Doch hab' ich dich dennoch", "tokens": ["Doch", "hab'", "ich", "dich", "den\u00b7noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PRF", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Von Herzen so gern.", "tokens": ["Von", "Her\u00b7zen", "so", "gern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Wie lieb' ich doch dich", "tokens": ["Wie", "lieb'", "ich", "doch", "dich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "So herzinniglich!", "tokens": ["So", "her\u00b7zin\u00b7nig\u00b7lich", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "--+-+", "measure": "anapaest.init"}, "line.3": {"text": "Dein funkelndes Aeuglein", "tokens": ["Dein", "fun\u00b7keln\u00b7des", "A\u00b7e\u00b7u\u00b7glein"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Blickt immer auf mich.", "tokens": ["Blickt", "im\u00b7mer", "auf", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "So blick' ich nach dir,", "tokens": ["So", "blick'", "ich", "nach", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Sei's dort oder hier:", "tokens": ["Sei's", "dort", "o\u00b7der", "hier", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "KON", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Dein freundliches Aeuglein", "tokens": ["Dein", "freund\u00b7li\u00b7ches", "A\u00b7e\u00b7u\u00b7glein"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Steht immer vor mir.", "tokens": ["Steht", "im\u00b7mer", "vor", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Wie nickst du mir zu", "tokens": ["Wie", "nickst", "du", "mir", "zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "PTKZU"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "In fr\u00f6hlicher Ruh!", "tokens": ["In", "fr\u00f6h\u00b7li\u00b7cher", "Ruh", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "O liebliches Sternlein,", "tokens": ["O", "lieb\u00b7li\u00b7ches", "Stern\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "O w\u00e4r' ich wie du!", "tokens": ["O", "w\u00e4r'", "ich", "wie", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PWAV", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}