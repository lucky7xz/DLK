{"textgrid.poem.26690": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Totenfeier am Grabe", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch zermalmt der Schrecken unsre Glieder \u2013", "tokens": ["Noch", "zer\u00b7malmt", "der", "Schre\u00b7cken", "uns\u00b7re", "Glie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Rieger tot!", "tokens": ["Rie\u00b7ger", "tot", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Noch in unsern Ohren heult der Donner wider \u2013", "tokens": ["Noch", "in", "un\u00b7sern", "Oh\u00b7ren", "heult", "der", "Don\u00b7ner", "wi\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Rieger, Rieger tot!", "tokens": ["Rie\u00b7ger", ",", "Rie\u00b7ger", "tot", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wie ein Blitz, im Niedergang entz\u00fcndet,", "tokens": ["Wie", "ein", "Blitz", ",", "im", "Nie\u00b7der\u00b7gang", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Schon im Aufgang schwindet,", "tokens": ["Schon", "im", "Auf\u00b7gang", "schwin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Flog der Held zu Gott!", "tokens": ["Flog", "der", "Held", "zu", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Sollen Klagen um die Leiche hallen,", "tokens": ["Sol\u00b7len", "Kla\u00b7gen", "um", "die", "Lei\u00b7che", "hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Klagen um den gro\u00dfen Mann?", "tokens": ["Kla\u00b7gen", "um", "den", "gro\u00b7\u00dfen", "Mann", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder d\u00f6rfen warme Tr\u00e4nen fallen,", "tokens": ["O\u00b7der", "d\u00f6r\u00b7fen", "war\u00b7me", "Tr\u00e4\u00b7nen", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Tr\u00e4nen um den guten, lieben Mann?", "tokens": ["Tr\u00e4\u00b7nen", "um", "den", "gu\u00b7ten", ",", "lie\u00b7ben", "Mann", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "D\u00f6rfen wir mit Riegers S\u00f6hnen weinen?", "tokens": ["D\u00f6r\u00b7fen", "wir", "mit", "Rie\u00b7gers", "S\u00f6h\u00b7nen", "wei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mit den Patrioten uns vereinen?", "tokens": ["Mit", "den", "Pat\u00b7ri\u00b7o\u00b7ten", "uns", "ver\u00b7ei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "O so feire weinender Gesang", "tokens": ["O", "so", "fei\u00b7re", "wei\u00b7nen\u00b7der", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Einer Sonne Untergang!", "tokens": ["Ei\u00b7ner", "Son\u00b7ne", "Un\u00b7ter\u00b7gang", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Gro\u00df, o Rieger, gro\u00df war deine Stufe,", "tokens": ["Gro\u00df", ",", "o", "Rie\u00b7ger", ",", "gro\u00df", "war", "dei\u00b7ne", "Stu\u00b7fe", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "NN", "$,", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gro\u00df dein Geist zu Seinem gro\u00dfen Rufe,", "tokens": ["Gro\u00df", "dein", "Geist", "zu", "Sei\u00b7nem", "gro\u00b7\u00dfen", "Ru\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Gr\u00f6\u00dfer war \u2013 dein Herz!", "tokens": ["Gr\u00f6\u00b7\u00dfer", "war", "\u2013", "dein", "Herz", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$(", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Engelhuld und g\u00f6ttliches Erbarmen", "tokens": ["En\u00b7gel\u00b7huld", "und", "g\u00f6tt\u00b7li\u00b7ches", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Rief den Freund zu deinen offnen Armen;", "tokens": ["Rief", "den", "Freund", "zu", "dei\u00b7nen", "off\u00b7nen", "Ar\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Froher unschuldsvoller Scherz", "tokens": ["Fro\u00b7her", "un\u00b7schulds\u00b7vol\u00b7ler", "Scherz"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Lachte noch im silbergrauen Weisen,", "tokens": ["Lach\u00b7te", "noch", "im", "sil\u00b7ber\u00b7grau\u00b7en", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Jugendfeuer brannte noch im Greisen,", "tokens": ["Ju\u00b7gend\u00b7feu\u00b7er", "brann\u00b7te", "noch", "im", "Grei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "In dem Krieger betete \u2013 der Christ.", "tokens": ["In", "dem", "Krie\u00b7ger", "be\u00b7te\u00b7te", "\u2013", "der", "Christ", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$(", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "H\u00f6her als das L\u00e4cheln deines F\u00fcrsten,", "tokens": ["H\u00f6\u00b7her", "als", "das", "L\u00e4\u00b7cheln", "dei\u00b7nes", "F\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "(ach! wornach so manche geizig d\u00fcrsten!)", "tokens": ["(", "ach", "!", "wor\u00b7nach", "so", "man\u00b7che", "gei\u00b7zig", "d\u00fcrs\u00b7ten", "!", ")"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$.", "PWAV", "ADV", "PIS", "ADJD", "VMFIN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.13": {"line.1": {"text": "H\u00f6her war dir der, der ewig ist.", "tokens": ["H\u00f6\u00b7her", "war", "dir", "der", ",", "der", "e\u00b7wig", "ist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ART", "$,", "PRELS", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.14": {"line.1": {"text": "Nicht um Erdeng\u00f6tter klein zu kriechen,", "tokens": ["Nicht", "um", "Er\u00b7den\u00b7g\u00f6t\u00b7ter", "klein", "zu", "krie\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "F\u00fcrstengunst mit Untertanenfl\u00fcchen", "tokens": ["F\u00fcrs\u00b7ten\u00b7gunst", "mit", "Un\u00b7ter\u00b7ta\u00b7nen\u00b7fl\u00fc\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.15": {"line.1": {"text": "Zu erwuchern, war dein Trachten nie.", "tokens": ["Zu", "er\u00b7wuc\u00b7hern", ",", "war", "dein", "Trach\u00b7ten", "nie", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "VAFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.16": {"line.1": {"text": "Elende beim F\u00fcrsten zu vertreten,", "tokens": ["E\u00b7len\u00b7de", "beim", "F\u00fcrs\u00b7ten", "zu", "ver\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "F\u00fcr die Unschuld an dem Thron zu beten,", "tokens": ["F\u00fcr", "die", "Un\u00b7schuld", "an", "dem", "Thron", "zu", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.17": {"line.1": {"text": "War dein Stolz auf Erden hie.", "tokens": ["War", "dein", "Stolz", "auf", "Er\u00b7den", "hie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Rang und Macht, die l\u00e4cherlichen Flitter,", "tokens": ["Rang", "und", "Macht", ",", "die", "l\u00e4\u00b7cher\u00b7li\u00b7chen", "Flit\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Fallen ab am Tage des Gerichts,", "tokens": ["Fal\u00b7len", "ab", "am", "Ta\u00b7ge", "des", "Ge\u00b7richts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Fallen ab wie Bl\u00e4tter im Gewitter,", "tokens": ["Fal\u00b7len", "ab", "wie", "Bl\u00e4t\u00b7ter", "im", "Ge\u00b7wit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KOKOM", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und der Pomp \u2013 ist Nichts! \u2013 \u2013", "tokens": ["Und", "der", "Pomp", "\u2013", "ist", "Nichts", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "ART", "NN", "$(", "VAFIN", "PIS", "$.", "$(", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.19": {"line.1": {"text": "Krieger KARLS! erlaubt mir, hier zu halten,", "tokens": ["Krie\u00b7ger", "KaRLS", "!", "er\u00b7laubt", "mir", ",", "hier", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "VVFIN", "PPER", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Tretet her, ihr lorbeervollen Alten!", "tokens": ["Tre\u00b7tet", "her", ",", "ihr", "lor\u00b7beer\u00b7vol\u00b7len", "Al\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.20": {"line.1": {"text": "(das Gewissen brenne flammenrot)", "tokens": ["(", "das", "Ge\u00b7wis\u00b7sen", "bren\u00b7ne", "flam\u00b7men\u00b7rot", ")"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "ADJD", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.21": {"line.1": {"text": "Dumpfig hohl aus eures Riegers Bahre", "tokens": ["Dump\u00b7fig", "hohl", "aus", "eu\u00b7res", "Rie\u00b7gers", "Bah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Spricht zu euch, ihr S\u00f6hne vieler Jahre,", "tokens": ["Spricht", "zu", "euch", ",", "ihr", "S\u00f6h\u00b7ne", "vie\u00b7ler", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "PPOSAT", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.22": {"line.1": {"text": "Spricht zu euch \u2013 der Tod:", "tokens": ["Spricht", "zu", "euch", "\u2013", "der", "Tod", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$(", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.23": {"line.1": {"text": "\u00bberdeng\u00f6tter! \u2013 glaubt ihr ungerochen", "tokens": ["\u00bb", "er\u00b7den\u00b7g\u00f6t\u00b7ter", "!", "\u2013", "glaubt", "ihr", "un\u00b7ge\u00b7ro\u00b7chen"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "ADJD", "$.", "$(", "VVFIN", "PPOSAT", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.24": {"line.1": {"text": "Mit der Gr\u00f6\u00dfe kindischkleinem Stolz", "tokens": ["Mit", "der", "Gr\u00f6\u00b7\u00dfe", "kin\u00b7dischklei\u00b7nem", "Stolz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "(alles fa\u00dft der schmale Raum von Holz)", "tokens": ["(", "al\u00b7les", "fa\u00dft", "der", "schma\u00b7le", "Raum", "von", "Holz", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.25": {"line.1": {"text": "Gegen mich zu pochen?", "tokens": ["Ge\u00b7gen", "mich", "zu", "po\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Hilft euch des Monarchen Gunst,", "tokens": ["Hilft", "euch", "des", "Mon\u00b7ar\u00b7chen", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die oft nur am Rittersterne funkelt,", "tokens": ["Die", "oft", "nur", "am", "Rit\u00b7ters\u00b7ter\u00b7ne", "fun\u00b7kelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hilft des H\u00f6flings Schlangenkunst,", "tokens": ["Hilft", "des", "H\u00f6f\u00b7lings", "Schlan\u00b7gen\u00b7kunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn sich brechend euer Aug verdunkelt?", "tokens": ["Wenn", "sich", "bre\u00b7chend", "eu\u00b7er", "Aug", "ver\u00b7dun\u00b7kelt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADJD", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Erdeng\u00f6tter, redet doch,", "tokens": ["Er\u00b7den\u00b7g\u00f6t\u00b7ter", ",", "re\u00b7det", "doch", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn der G\u00f6tterdunst zerstiebet,", "tokens": ["Wenn", "d