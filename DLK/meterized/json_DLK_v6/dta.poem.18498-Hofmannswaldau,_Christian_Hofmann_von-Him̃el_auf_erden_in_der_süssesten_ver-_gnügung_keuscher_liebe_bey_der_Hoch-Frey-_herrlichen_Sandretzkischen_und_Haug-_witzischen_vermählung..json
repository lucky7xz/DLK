{"dta.poem.18498": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Him\u0303el auf erden/ in der s\u00fcssesten ver-  \n gn\u00fcgung keuscher liebe/ bey der Hoch-Frey-  \n herrlichen Sandretzkischen und Haug-  \n witzischen verm\u00e4hlung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer ist wol auf der welt/ der nicht vergn\u00fcgung sucht?", "tokens": ["Wer", "ist", "wol", "auf", "der", "welt", "/", "der", "nicht", "ver\u00b7gn\u00fc\u00b7gung", "sucht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "APPR", "ART", "NN", "$(", "ART", "PTKNEG", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die kurtze lebens-zeit ist eine bittre frucht/", "tokens": ["Die", "kurt\u00b7ze", "le\u00b7bens\u00b7zeit", "ist", "ei\u00b7ne", "bitt\u00b7re", "frucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die man mit freude mu\u00df/ als wie mit zucker/ w\u00fcrtzen.", "tokens": ["Die", "man", "mit", "freu\u00b7de", "mu\u00df", "/", "als", "wie", "mit", "zu\u00b7cker", "/", "w\u00fcrt\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIS", "APPR", "NN", "VMFIN", "$(", "KOUS", "KOKOM", "APPR", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein unmensch gr\u00e4bt sich nur in blasse traurigkeit/", "tokens": ["Ein", "un\u00b7mensch", "gr\u00e4bt", "sich", "nur", "in", "blas\u00b7se", "trau\u00b7rig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "PRF", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und wer sein leben ha\u00dft/ der stirbet vor der zeit/", "tokens": ["Und", "wer", "sein", "le\u00b7ben", "ha\u00dft", "/", "der", "stir\u00b7bet", "vor", "der", "zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "VVINF", "VVFIN", "$(", "ART", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn er sich selbsten wil in gram und sorge st\u00fcrtzen.", "tokens": ["Wenn", "er", "sich", "selbs\u00b7ten", "wil", "in", "gram", "und", "sor\u00b7ge", "st\u00fcrt\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VMFIN", "APPR", "NE", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Des kummers folter steht nur feigen hertzen an/", "tokens": ["Des", "kum\u00b7mers", "fol\u00b7ter", "steht", "nur", "fei\u00b7gen", "hert\u00b7zen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein aufgeweckter geist bricht lilgen in den nesseln/", "tokens": ["Ein", "auf\u00b7ge\u00b7weck\u00b7ter", "geist", "bricht", "lil\u00b7gen", "in", "den", "nes\u00b7seln", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Und kocht aus wermuth selbst was zucker gleichen kan;", "tokens": ["Und", "kocht", "aus", "wer\u00b7muth", "selbst", "was", "zu\u00b7cker", "glei\u00b7chen", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ADV", "PWS", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Kein niedertr\u00e4chtig schmertz mag seine sinnen fesseln/", "tokens": ["Kein", "nie\u00b7der\u00b7tr\u00e4ch\u00b7tig", "schmertz", "mag", "sei\u00b7ne", "sin\u00b7nen", "fes\u00b7seln", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "ADJD", "VMFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Weil er die freudigkeit als seine schwester k\u00fc\u00dft/", "tokens": ["Weil", "er", "die", "freu\u00b7dig\u00b7keit", "als", "sei\u00b7ne", "schwes\u00b7ter", "k\u00fc\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "KOKOM", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und die vergn\u00fcgung ihm das halbe leben ist.", "tokens": ["Und", "die", "ver\u00b7gn\u00fc\u00b7gung", "ihm", "das", "hal\u00b7be", "le\u00b7ben", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "ART", "ADJA", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Jm schachte findt man gold/ und in der asche glut/", "tokens": ["Jm", "schach\u00b7te", "findt", "man", "gold", "/", "und", "in", "der", "asc\u00b7he", "glut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "PIS", "NN", "$(", "KON", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der mohr fischt aus der see das theure perlen-gut/", "tokens": ["Der", "mohr", "fischt", "aus", "der", "see", "das", "theu\u00b7re", "per\u00b7len\u00b7gut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wo aber sol der mensch vergng\u00fcgungs-sch\u00e4tze graben?", "tokens": ["Wo", "a\u00b7ber", "sol", "der", "mensch", "ver\u00b7gng\u00fc\u00b7gungs\u00b7sch\u00e4t\u00b7ze", "gra\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "ART", "NN", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der nimmer-satte geist lacht seinen klumpen an/", "tokens": ["Der", "nim\u00b7mer\u00b7sat\u00b7te", "geist", "lacht", "sei\u00b7nen", "klum\u00b7pen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Das marck der erden ist was ihn vergn\u00fcgen kan/", "tokens": ["Das", "marck", "der", "er\u00b7den", "ist", "was", "ihn", "ver\u00b7gn\u00fc\u00b7gen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PWS", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und was die h\u00e4nde f\u00fcllt/ sol auch das hertze laben.", "tokens": ["Und", "was", "die", "h\u00e4n\u00b7de", "f\u00fcllt", "/", "sol", "auch", "das", "hert\u00b7ze", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "VVFIN", "$(", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Allein! wie schw\u00e4rmt der geist bey dieser eitelkeit?", "tokens": ["Al\u00b7lein", "!", "wie", "schw\u00e4rmt", "der", "geist", "bey", "die\u00b7ser", "ei\u00b7tel\u00b7keit", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PWAV", "VVFIN", "ART", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Er bleibt ein selave doch auch in den g\u00f6ldnen banden/", "tokens": ["Er", "bleibt", "ein", "se\u00b7la\u00b7ve", "doch", "auch", "in", "den", "g\u00f6ld\u00b7nen", "ban\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Des reichthums \u00fcberflu\u00df fri\u00dft die zufriedenheit/", "tokens": ["Des", "reicht\u00b7hums", "\u00fc\u00b7berf\u00b7lu\u00df", "fri\u00dft", "die", "zu\u00b7frie\u00b7den\u00b7heit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wenn vor den schatz kein schutz in glut und raub verhanden/", "tokens": ["Wenn", "vor", "den", "schatz", "kein", "schutz", "in", "glut", "und", "raub", "ver\u00b7han\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PIAT", "NN", "APPR", "NN", "KON", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Da\u00df offt ein leeres hertz beym vollen kasten liegt/", "tokens": ["Da\u00df", "offt", "ein", "lee\u00b7res", "hertz", "beym", "vol\u00b7len", "kas\u00b7ten", "liegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und diese worte seufzt: Hier lebt man unvergn\u00fcgt.", "tokens": ["Und", "die\u00b7se", "wor\u00b7te", "seufzt", ":", "Hier", "lebt", "man", "un\u00b7ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "$.", "ADV", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ein andrer speiset sich mit einer hand voll wind/", "tokens": ["Ein", "an\u00b7drer", "spei\u00b7set", "sich", "mit", "ei\u00b7ner", "hand", "voll", "wind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PRF", "APPR", "ART", "NN", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wenn pracht und herrligkeit sein ander leben sind.", "tokens": ["Wenn", "pracht", "und", "herr\u00b7lig\u00b7keit", "sein", "an\u00b7der", "le\u00b7ben", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPOSAT", "ADJD", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Allein die erhsucht springt/ wie leichte wasser-blasen/", "tokens": ["Al\u00b7lein", "die", "erh\u00b7sucht", "springt", "/", "wie", "leich\u00b7te", "was\u00b7ser\u00b7bla\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$(", "KOKOM", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Offt wird vor Diamant nur bley und glas getauscht/", "tokens": ["Offt", "wird", "vor", "Di\u00b7a\u00b7mant", "nur", "bley", "und", "glas", "ge\u00b7tauscht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "ADV", "NN", "KON", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wenn hoheit durch das gl\u00fcck wie eine flut verrauscht/", "tokens": ["Wenn", "ho\u00b7heit", "durch", "das", "gl\u00fcck", "wie", "ei\u00b7ne", "flut", "ver\u00b7rauscht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "KOKOM", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und ungl\u00fccks-donner oft auf kron und zepter rasen.", "tokens": ["Und", "un\u00b7gl\u00fccks\u00b7don\u00b7ner", "oft", "auf", "kron", "und", "zep\u00b7ter", "ra\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPR", "NN", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Viel suchen ihre ruh in hoher wissenschafft/", "tokens": ["Viel", "su\u00b7chen", "ih\u00b7re", "ruh", "in", "ho\u00b7her", "wis\u00b7sen\u00b7schafft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die auch ein Salomon als leeres st\u00fcckwerck preiset/", "tokens": ["Die", "auch", "ein", "Sa\u00b7lo\u00b7mon", "als", "lee\u00b7res", "st\u00fcck\u00b7werck", "prei\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NE", "KOUS", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ein weicher Sybarit l\u00e4chzt nur nach trauben-safft/", "tokens": ["Ein", "wei\u00b7cher", "Sy\u00b7ba\u00b7rit", "l\u00e4chzt", "nur", "nach", "trau\u00b7ben\u00b7\u00b7safft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ob gleich Egyptens topff nur Coloquinten speiset.", "tokens": ["Ob", "gleich", "E\u00b7gyp\u00b7tens", "topff", "nur", "Co\u00b7loq\u00b7uin\u00b7ten", "spei\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NE", "NE", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ach sagt: Jhr sterblichen/ wo wird man nun vergn\u00fcgt?", "tokens": ["Ach", "sagt", ":", "Ihr", "sterb\u00b7li\u00b7chen", "/", "wo", "wird", "man", "nun", "ver\u00b7gn\u00fcgt", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$.", "PPER", "VVINF", "$(", "PWAV", "VAFIN", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wenn hier vor perl und kern nur schaum und schale liegt.", "tokens": ["Wenn", "hier", "vor", "perl", "und", "kern", "nur", "schaum", "und", "scha\u00b7le", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "NE", "KON", "VVFIN", "ADV", "ADV", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Der himmel bleibet wohl der seelen h\u00f6chstes gut/", "tokens": ["Der", "him\u00b7mel", "blei\u00b7bet", "wohl", "der", "see\u00b7len", "h\u00f6chs\u00b7tes", "gut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "ADJA", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Der macht uns recht vergn\u00fcgt/ wenn die geweyhte glut/", "tokens": ["Der", "macht", "uns", "recht", "ver\u00b7gn\u00fcgt", "/", "wenn", "die", "ge\u00b7weyh\u00b7te", "glut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "VVPP", "$(", "KOUS", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Die aus dem hertzen bricht/ nur nach den wolcken steiget.", "tokens": ["Die", "aus", "dem", "hert\u00b7zen", "bricht", "/", "nur", "nach", "den", "wol\u00b7cken", "stei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$(", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Doch ausser diesem ist der ehe s\u00fcssigkeit", "tokens": ["Doch", "aus\u00b7ser", "die\u00b7sem", "ist", "der", "e\u00b7he", "s\u00fcs\u00b7sig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "VAFIN", "ART", "KOUS", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ein himmel auf der welt/ wo die zufriedenheit", "tokens": ["Ein", "him\u00b7mel", "auf", "der", "welt", "/", "wo", "die", "zu\u00b7frie\u00b7den\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$(", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Ein irdisch paradi\u00df und lichtes Gosen zeiget.", "tokens": ["Ein", "ir\u00b7disch", "pa\u00b7ra\u00b7di\u00df", "und", "lich\u00b7tes", "Go\u00b7sen", "zei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PTKVZ", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Zwar von den farben schreibt ein blinder ziemlich blind/", "tokens": ["Zwar", "von", "den", "far\u00b7ben", "schreibt", "ein", "blin\u00b7der", "ziem\u00b7lich", "blind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Und wen die liebe nicht mit ihrem moste tr\u00e4ncket/", "tokens": ["Und", "wen", "die", "lie\u00b7be", "nicht", "mit", "ih\u00b7rem", "mos\u00b7te", "tr\u00e4n\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Der kennt die blumen nicht/ die ihr gewachsen sind.", "tokens": ["Der", "kennt", "die", "blu\u00b7men", "nicht", "/", "die", "ihr", "ge\u00b7wach\u00b7sen", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKNEG", "$(", "PRELS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.46": {"text": "Doch wer die augen nur auf diese hertzen lencket", "tokens": ["Doch", "wer", "die", "au\u00b7gen", "nur", "auf", "die\u00b7se", "hert\u00b7zen", "len\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ART", "NN", "ADV", "APPR", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Die dieser tag verkn\u00fcpfft/ der f\u00e4llt mir gerne bey/", "tokens": ["Die", "die\u00b7ser", "tag", "ver\u00b7kn\u00fcpfft", "/", "der", "f\u00e4llt", "mir", "ger\u00b7ne", "bey", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "VVPP", "$(", "ART", "VVFIN", "PPER", "ADV", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Da\u00df nichts vergn\u00fcgters ist/ als s\u00fcsse liebes-treu", "tokens": ["Da\u00df", "nichts", "ver\u00b7gn\u00fcg\u00b7ters", "ist", "/", "als", "s\u00fcs\u00b7se", "lie\u00b7bes\u00b7treu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADJA", "VAFIN", "$(", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Hier speist man g\u00f6tter-brod; vergn\u00fcgung ist die tracht", "tokens": ["Hier", "speist", "man", "g\u00f6t\u00b7ter\u00b7brod", ";", "ver\u00b7gn\u00fc\u00b7gung", "ist", "die", "tracht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "NN", "$.", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Die immer s\u00e4ttiget/ und immer hungrig macht.", "tokens": ["Die", "im\u00b7mer", "s\u00e4t\u00b7ti\u00b7get", "/", "und", "im\u00b7mer", "hung\u00b7rig", "macht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$(", "KON", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Jhr s\u00fcsses Manna trotzt die seltnen Marmeladen/", "tokens": ["Ihr", "s\u00fcs\u00b7ses", "Man\u00b7na", "trotzt", "die", "selt\u00b7nen", "Mar\u00b7me\u00b7la\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Wenn hier ein keuscher ku\u00df auf treuen lippen schwimmt/", "tokens": ["Wenn", "hier", "ein", "keu\u00b7scher", "ku\u00df", "auf", "treu\u00b7en", "lip\u00b7pen", "schwimmt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Wenn in dem hertzen selbst ein ewigs opffer glimmt/", "tokens": ["Wenn", "in", "dem", "hert\u00b7zen", "selbst", "ein", "e\u00b7wigs", "opf\u00b7fer", "glimmt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Und geister voller gluth in tugend-rosen baden.", "tokens": ["Und", "geis\u00b7ter", "vol\u00b7ler", "gluth", "in", "tu\u00b7gen\u00b7dro\u00b7sen", "ba\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Ein jeder morgen thaut auff ihren liebes-klee/", "tokens": ["Ein", "je\u00b7der", "mor\u00b7gen", "thaut", "auff", "ih\u00b7ren", "lie\u00b7bes\u00b7klee", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Ein jeder abend wiegt sie auf vergn\u00fcgungs-betten.", "tokens": ["Ein", "je\u00b7der", "a\u00b7bend", "wiegt", "sie", "auf", "ver\u00b7gn\u00fc\u00b7gungs\u00b7bet\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Hier bl\u00fcht der fr\u00fchling auf auch unter ei\u00df und schnee/", "tokens": ["Hier", "bl\u00fcht", "der", "fr\u00fch\u00b7ling", "auf", "auch", "un\u00b7ter", "ei\u00df", "und", "schnee", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "APPR", "ADV", "APPR", "NN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Und was das hertze kn\u00fcpft sind lauter sanfte ketten/", "tokens": ["Und", "was", "das", "hert\u00b7ze", "kn\u00fcpft", "sind", "lau\u00b7ter", "sanf\u00b7te", "ket\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "VAFIN", "PIAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Wo nun dergleichen lust aus einem brunnen quillt/", "tokens": ["Wo", "nun", "derg\u00b7lei\u00b7chen", "lust", "aus", "ei\u00b7nem", "brun\u00b7nen", "quillt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIS", "VVFIN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Da wird die keusche brust mit reiner lust erf\u00fcllt.", "tokens": ["Da", "wird", "die", "keu\u00b7sche", "brust", "mit", "rei\u00b7ner", "lust", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Gesetzt; da\u00df Myrrhen oft bey diesem zucker sind/", "tokens": ["Ge\u00b7setzt", ";", "da\u00df", "Myr\u00b7rhen", "oft", "bey", "die\u00b7sem", "zu\u00b7cker", "sind", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "KOUS", "NN", "ADV", "APPR", "PDAT", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Und da\u00df die eh sich auch mit einem weh verbindt/", "tokens": ["Und", "da\u00df", "die", "eh", "sich", "auch", "mit", "ei\u00b7nem", "weh", "ver\u00b7bindt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "KOUS", "PRF", "ADV", "APPR", "ART", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Doch mu\u00df die last in lust/ schmertz sich in schertz verkehren;", "tokens": ["Doch", "mu\u00df", "die", "last", "in", "lust", "/", "schmertz", "sich", "in", "schertz", "ver\u00b7keh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "VVFIN", "APPR", "NN", "$(", "ADJD", "PRF", "APPR", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Wann diese b\u00fcrde nur auf zweyen schultern liegt/", "tokens": ["Wann", "die\u00b7se", "b\u00fcr\u00b7de", "nur", "auf", "zwe\u00b7yen", "schul\u00b7tern", "liegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VVFIN", "ADV", "APPR", "VVINF", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Da bleibt ein doppelt hertz im trauren unbesiegt/", "tokens": ["Da", "bleibt", "ein", "dop\u00b7pelt", "hertz", "im", "trau\u00b7ren", "un\u00b7be\u00b7siegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN", "APPRART", "VVINF", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Getheilter kummer mu\u00df das labsal selbst gewehren.", "tokens": ["Ge\u00b7theil\u00b7ter", "kum\u00b7mer", "mu\u00df", "das", "lab\u00b7sal", "selbst", "ge\u00b7weh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Der gleichgesinnte geist lacht/ wenn das ungl\u00fcck kracht/", "tokens": ["Der", "gleich\u00b7ge\u00b7sinn\u00b7te", "geist", "lacht", "/", "wenn", "das", "un\u00b7gl\u00fcck", "kracht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "KOUS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Ein angenehmer blick vertreibet manche grille/", "tokens": ["Ein", "an\u00b7ge\u00b7neh\u00b7mer", "blick", "ver\u00b7trei\u00b