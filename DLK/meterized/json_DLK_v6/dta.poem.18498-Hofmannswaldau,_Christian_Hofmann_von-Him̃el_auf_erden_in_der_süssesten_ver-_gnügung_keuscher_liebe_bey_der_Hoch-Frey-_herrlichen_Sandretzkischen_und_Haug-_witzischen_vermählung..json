ine.iambic.hexa"}, "line.73": {"text": "Wer sagt/ da\u00df lieben nicht vergn\u00fcgtes leben sey?", "tokens": ["Wer", "sagt", "/", "da\u00df", "lie\u00b7ben", "nicht", "ver\u00b7gn\u00fcg\u00b7tes", "le\u00b7ben", "sey", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "KOUS", "ADJA", "PTKNEG", "ADJA", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.74": {"text": "Wer hei\u00dft die ehe nicht des himmels conterfey?", "tokens": ["Wer", "hei\u00dft", "die", "e\u00b7he", "nicht", "des", "him\u00b7mels", "con\u00b7ter\u00b7fey", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "KOUS", "PTKNEG", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.75": {"text": "Kan auch ein d\u00fcrrer kern in s\u00fcssen schalen wohnen?", "tokens": ["Kan", "auch", "ein", "d\u00fcr\u00b7rer", "kern", "in", "s\u00fcs\u00b7sen", "scha\u00b7len", "woh\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "APPR", "ADJA", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.76": {"text": "Auf einem nelcken-strauch w\u00e4chst keine distel nicht.", "tokens": ["Auf", "ei\u00b7nem", "nel\u00b7cken\u00b7strauch", "w\u00e4chst", "kei\u00b7ne", "dis\u00b7tel", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PIAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.77": {"text": "Hier ist ein gr\u00fcner baum/ wo man vergn\u00fcgung bricht/", "tokens": ["Hier", "ist", "ein", "gr\u00fc\u00b7ner", "baum", "/", "wo", "man", "ver\u00b7gn\u00fc\u00b7gung", "bricht", "/"], "token_info