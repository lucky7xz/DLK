{"textgrid.poem.49384": {"metadata": {"author": {"name": "Ball, Hugo", "birth": "N.A.", "death": "N.A."}, "title": "Der gefallene Cherub", "genre": "verse", "period": "N.A.", "pub_year": 1906, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er kreiste um die gl\u00e4sernen Pilaster", "tokens": ["Er", "kreis\u00b7te", "um", "die", "gl\u00e4\u00b7ser\u00b7nen", "Pi\u00b7las\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und hob die Stimme, da\u00df er gellend riefe.", "tokens": ["Und", "hob", "die", "Stim\u00b7me", ",", "da\u00df", "er", "gel\u00b7lend", "rie\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es gl\u00e4nzte seines Fluges Hieroglyphe", "tokens": ["Es", "gl\u00e4nz\u00b7te", "sei\u00b7nes", "Flu\u00b7ges", "Hie\u00b7ro\u00b7gly\u00b7phe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Im Tempelbau der gro\u00dfen Zoroaster.", "tokens": ["Im", "Tem\u00b7pel\u00b7bau", "der", "gro\u00b7\u00dfen", "Zo\u00b7roas\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Da war's, als ob der Atem uns entschliefe.", "tokens": ["Da", "wa\u00b7r's", ",", "als", "ob", "der", "A\u00b7tem", "uns", "ent\u00b7schlie\u00b7fe", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KOKOM", "KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Es sank sein Haupt, wie eine Riesenaster,", "tokens": ["Es", "sank", "sein", "Haupt", ",", "wie", "ei\u00b7ne", "Rie\u00b7sen\u00b7as\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Umh\u00fcllt von schweren Schwingen seiner Laster", "tokens": ["Um\u00b7h\u00fcllt", "von", "schwe\u00b7ren", "Schwin\u00b7gen", "sei\u00b7ner", "Las\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verschlang ihn eine bodenlose Tiefe.", "tokens": ["Ver\u00b7schlang", "ihn", "ei\u00b7ne", "bo\u00b7den\u00b7lo\u00b7se", "Tie\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wir sahens wohl und uns beschlich ein Sehnen", "tokens": ["Wir", "sa\u00b7hens", "wohl", "und", "uns", "be\u00b7schlich", "ein", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "KON", "PPER", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach Untergang und gallgetr\u00e4nkten Tr\u00e4nen", "tokens": ["Nach", "Un\u00b7ter\u00b7gang", "und", "gall\u00b7ge\u00b7tr\u00e4nk\u00b7ten", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu schl\u00fcrfen aller Trauermeere Flut.", "tokens": ["Zu", "schl\u00fcr\u00b7fen", "al\u00b7ler", "Trau\u00b7er\u00b7mee\u00b7re", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Vergiftet f\u00fchlten wir das eigene W\u00e4hnen", "tokens": ["Ver\u00b7gif\u00b7tet", "f\u00fchl\u00b7ten", "wir", "das", "ei\u00b7ge\u00b7ne", "W\u00e4h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und ein Verlangen, uns dort anzulehnen,", "tokens": ["Und", "ein", "Ver\u00b7lan\u00b7gen", ",", "uns", "dort", "an\u00b7zu\u00b7leh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPER", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo der versunkenste der Engel ruht.", "tokens": ["Wo", "der", "ver\u00b7sun\u00b7kens\u00b7te", "der", "En\u00b7gel", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}