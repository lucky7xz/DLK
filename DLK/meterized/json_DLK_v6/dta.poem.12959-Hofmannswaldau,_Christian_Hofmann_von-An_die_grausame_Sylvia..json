{"dta.poem.12959": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An die grausame Sylvia.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein hertz ist nun kein hertz, indem es, Sylvia!", "tokens": ["Mein", "hertz", "ist", "nun", "kein", "hertz", ",", "in\u00b7dem", "es", ",", "Syl\u00b7via", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PIAT", "NN", "$,", "KOUS", "PPER", "$,", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch deinen grimm zu einer h\u00f6lle worden,", "tokens": ["Durch", "dei\u00b7nen", "grimm", "zu", "ei\u00b7ner", "h\u00f6l\u00b7le", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "APPR", "ART", "ADJA", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In der die martern, angst und pein", "tokens": ["In", "der", "die", "mar\u00b7tern", ",", "angst", "und", "pein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "$,", "VVPP", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ewig und unsterblich seyn.", "tokens": ["E\u00b7wig", "und", "uns\u00b7terb\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da wirst du (wo dir vor manch ehren-dienst geschehn)", "tokens": ["Da", "wirst", "du", "(", "wo", "dir", "vor", "manch", "eh\u00b7ren\u00b7dienst", "ge\u00b7schehn", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$(", "PWAV", "PPER", "APPR", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Auch wenn du allereits den orden", "tokens": ["Auch", "wenn", "du", "al\u00b7le\u00b7reits", "den", "or\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der todten f\u00fchren wirst, der pein kein ende sehn.", "tokens": ["Der", "tod\u00b7ten", "f\u00fch\u00b7ren", "wirst", ",", "der", "pein", "kein", "en\u00b7de", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVINF", "VAFIN", "$,", "ART", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Schau, strenge Sylvia!", "tokens": ["Schau", ",", "stren\u00b7ge", "Syl\u00b7via", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "NE", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Die straf\u2019 ist w\u00fcrcklich da.", "tokens": ["Die", "straf'", "ist", "w\u00fcrck\u00b7lich", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Es hat dein eigensinn, dem du zu viel vertrauet,", "tokens": ["Es", "hat", "dein", "ei\u00b7gen\u00b7sinn", ",", "dem", "du", "zu", "viel", "ver\u00b7trau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PRELS", "PPER", "PTKA", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Aus deinem tempel dir ein h\u00f6llen-reich gebauet.", "tokens": ["Aus", "dei\u00b7nem", "tem\u00b7pel", "dir", "ein", "h\u00f6l\u00b7len\u00b7reich", "ge\u00b7bau\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "ART", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}