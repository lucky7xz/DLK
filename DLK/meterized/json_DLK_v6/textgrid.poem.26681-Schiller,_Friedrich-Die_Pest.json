{"textgrid.poem.26681": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Pest", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gr\u00e4\u00dflich preisen Gottes Kraft", "tokens": ["Gr\u00e4\u00df\u00b7lich", "prei\u00b7sen", "Got\u00b7tes", "Kraft"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Pestilenzen, w\u00fcrgende Seuchen,", "tokens": ["Pes\u00b7ti\u00b7len\u00b7zen", ",", "w\u00fcr\u00b7gen\u00b7de", "Seu\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Die mit der grausen Br\u00fcderschaft", "tokens": ["Die", "mit", "der", "grau\u00b7sen", "Br\u00fc\u00b7der\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durchs \u00f6de Tal der Grabnacht schleichen.", "tokens": ["Durchs", "\u00f6\u00b7de", "Tal", "der", "Grab\u00b7nacht", "schlei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Bang ergreifts das klopfende Herz,", "tokens": ["Bang", "er\u00b7greifts", "das", "klop\u00b7fen\u00b7de", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Gichtrisch zuckt die starre Sehne,", "tokens": ["Gich\u00b7trisch", "zuckt", "die", "star\u00b7re", "Seh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gr\u00e4\u00dflich lacht der Wahnsinn in das Angstgest\u00f6hne,", "tokens": ["Gr\u00e4\u00df\u00b7lich", "lacht", "der", "Wahn\u00b7sinn", "in", "das", "Angst\u00b7ge\u00b7st\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "In heulende Triller ergeu\u00dft sich der Schmerz.", "tokens": ["In", "heu\u00b7len\u00b7de", "Tril\u00b7ler", "er\u00b7geu\u00dft", "sich", "der", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Raserei w\u00e4lzt tobend sich im Bette \u2013", "tokens": ["Ra\u00b7se\u00b7rei", "w\u00e4lzt", "to\u00b7bend", "sich", "im", "Bet\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "PRF", "APPRART", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Giftger Nebel wallt um ausgestorbne St\u00e4dte,", "tokens": ["Gift\u00b7ger", "Ne\u00b7bel", "wallt", "um", "aus\u00b7ge\u00b7storb\u00b7ne", "St\u00e4d\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Menschen \u2013 hager \u2013 hohl und bleich \u2013", "tokens": ["Men\u00b7schen", "\u2013", "ha\u00b7ger", "\u2013", "hohl", "und", "bleich", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADJD", "$(", "ADJD", "KON", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wimmeln in das finstre Reich.", "tokens": ["Wim\u00b7meln", "in", "das", "finst\u00b7re", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Br\u00fctend liegt der Tod auf dumpfen L\u00fcften,", "tokens": ["Br\u00fc\u00b7tend", "liegt", "der", "Tod", "auf", "dum\u00b7pfen", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "H\u00e4uft sich Sch\u00e4tze in gestopften Gr\u00fcften \u2013", "tokens": ["H\u00e4uft", "sich", "Sch\u00e4t\u00b7ze", "in", "ge\u00b7stopf\u00b7ten", "Gr\u00fcf\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NN", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Pestilenz sein Jubelfest.", "tokens": ["Pes\u00b7ti\u00b7lenz", "sein", "Ju\u00b7bel\u00b7fest", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Leichenschweigen \u2013 Kirchhofstille", "tokens": ["Lei\u00b7chen\u00b7schwei\u00b7gen", "\u2013", "Kirch\u00b7hof\u00b7stil\u00b7le"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$(", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wechseln mit dem Lustgebr\u00fclle,", "tokens": ["Wech\u00b7seln", "mit", "dem", "Lust\u00b7ge\u00b7br\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schr\u00f6cklich preiset Gott die Pest.", "tokens": ["Schr\u00f6ck\u00b7lich", "prei\u00b7set", "Gott", "die", "Pest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}