{"dta.poem.9882": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Er ist gl\u00fcckselig/ aber nur im traum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was hilfft mir mein verliebter sinn/", "tokens": ["Was", "hilfft", "mir", "mein", "ver\u00b7lieb\u00b7ter", "sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil ihre gottheit ihn verschm\u00e4het?", "tokens": ["Weil", "ih\u00b7re", "got\u00b7theit", "ihn", "ver\u00b7schm\u00e4\u00b7het", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Jhr leichten winde nehmt ihn hin/", "tokens": ["Ihr", "leich\u00b7ten", "win\u00b7de", "nehmt", "ihn", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bi\u00df ihr sein eitles thun verwehet:", "tokens": ["Bi\u00df", "ihr", "sein", "eit\u00b7les", "thun", "ver\u00b7we\u00b7het", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sein urtheil ist ihm schon gesprochen/", "tokens": ["Sein", "ur\u00b7theil", "ist", "ihm", "schon", "ge\u00b7spro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die st\u00e4be sind entzwey gebrochen.", "tokens": ["Die", "st\u00e4\u00b7be", "sind", "ent\u00b7zwey", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die gantze erd\u2019 ist mir zu klein/", "tokens": ["Die", "gant\u00b7ze", "erd'", "ist", "mir", "zu", "klein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "PTKA", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der himmel ists/ der mich nicht kennet;", "tokens": ["Der", "him\u00b7mel", "ists", "/", "der", "mich", "nicht", "ken\u00b7net", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$(", "PRELS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie kan ich bey mir selber seyn/", "tokens": ["Wie", "kan", "ich", "bey", "mir", "sel\u00b7ber", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "PPER", "ADV", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil mir umsonst die hoffnung brennet?", "tokens": ["Weil", "mir", "um\u00b7sonst", "die", "hoff\u00b7nung", "bren\u00b7net", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich ruffe/ erde/ himmel/ hoffen!", "tokens": ["Ich", "ruf\u00b7fe", "/", "er\u00b7de", "/", "him\u00b7mel", "/", "hof\u00b7fen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "NN", "$(", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und keines hat die ohren offen.", "tokens": ["Und", "kei\u00b7nes", "hat", "die", "oh\u00b7ren", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was hab ich armer doch gemacht/", "tokens": ["Was", "hab", "ich", "ar\u00b7mer", "doch", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADJA", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O tugend-g\u00f6ttin aller sch\u00f6nen!", "tokens": ["O", "tu\u00b7gen\u00b7dg\u00f6t\u00b7tin", "al\u00b7ler", "sch\u00f6\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PIAT", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df sie mich niemahls w\u00fcrdig acht", "tokens": ["Da\u00df", "sie", "mich", "nie\u00b7mahls", "w\u00fcr\u00b7dig", "acht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADJD", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ihrer gottheit zu vers\u00f6hnen?", "tokens": ["Mit", "ih\u00b7rer", "got\u00b7theit", "zu", "ver\u00b7s\u00f6h\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Soll ich denn keine ruhe finden", "tokens": ["Soll", "ich", "denn", "kei\u00b7ne", "ru\u00b7he", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vor nie begangne liebes-s\u00fcnden?", "tokens": ["Vor", "nie", "be\u00b7gang\u00b7ne", "lie\u00b7bes\u00b7s\u00fcn\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "L\u00e4ngst hab ich ein altar gesetzt/", "tokens": ["L\u00e4ngst", "hab", "ich", "ein", "al\u00b7tar", "ge\u00b7setzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein denckmahl harter bu\u00df zu stifften/", "tokens": ["Ein", "denck\u00b7mahl", "har\u00b7ter", "bu\u00df", "zu", "stiff\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In welchen Amors hand geetzt", "tokens": ["In", "wel\u00b7chen", "A\u00b7mors", "hand", "ge\u00b7etzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NE", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit diamant und g\u00fcldnen schrifften", "tokens": ["Mit", "di\u00b7a\u00b7mant", "und", "g\u00fcld\u00b7nen", "schriff\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "KON", "ADJA", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der sch\u00f6nsten g\u00f6ttin von der erden", "tokens": ["Der", "sch\u00f6ns\u00b7ten", "g\u00f6t\u00b7tin", "von", "der", "er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Soll dieser eintzig heilig werden.", "tokens": ["Soll", "die\u00b7ser", "eint\u00b7zig", "hei\u00b7lig", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "ADJD", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Darauff wenn sich der morgen r\u00f6th/", "tokens": ["Dar\u00b7auff", "wenn", "sich", "der", "mor\u00b7gen", "r\u00f6th", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "PRF", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df ich mein hertz als weyrauch gl\u00fchen/", "tokens": ["La\u00df", "ich", "mein", "hertz", "als", "wey\u00b7rauch", "gl\u00fc\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "KOUS", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn mir Ph\u00f6bus untergeht/", "tokens": ["Und", "wenn", "mir", "Ph\u00f6\u00b7bus", "un\u00b7ter\u00b7geht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NE", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verge\u00df ich nicht davor zu knien.", "tokens": ["Ver\u00b7ge\u00df", "ich", "nicht", "da\u00b7vor", "zu", "kni\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "PAV", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es hat mich nie der schlaff bezwungen/", "tokens": ["Es", "hat", "mich", "nie", "der", "schlaff", "be\u00b7zwun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bi\u00df ich ihr g\u00f6ttlich thun besungen.", "tokens": ["Bi\u00df", "ich", "ihr", "g\u00f6tt\u00b7lich", "thun", "be\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dann leg ich mich zur sanfften ruh/", "tokens": ["Dann", "leg", "ich", "mich", "zur", "sanff\u00b7ten", "ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu meiner mutter neuer schmertzen:", "tokens": ["Zu", "mei\u00b7ner", "mut\u00b7ter", "neu\u00b7er", "schmert\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich schliesse zwar die angen zu/", "tokens": ["Ich", "schlies\u00b7se", "zwar", "die", "an\u00b7gen", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "PTKZU", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wache dennoch stets im hertzen;", "tokens": ["Und", "wa\u00b7che", "den\u00b7noch", "stets", "im", "hert\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Jm schatten bringts die nacht getragen/", "tokens": ["Jm", "schat\u00b7ten", "bringts", "die", "nacht", "ge\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was mir der tag pflegt zu versagen.", "tokens": ["Was", "mir", "der", "tag", "pflegt", "zu", "ver\u00b7sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "Dann tret ich endlich zu ihr hin/", "tokens": ["Dann", "tret", "ich", "end\u00b7lich", "zu", "ihr", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir singen/ spielen/ tantzen/ lachen/", "tokens": ["Wir", "sin\u00b7gen", "/", "spie\u00b7len", "/", "tant\u00b7zen", "/", "la\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "VVINF", "$(", "VVINF", "$(", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie hasset gar nicht meinen sinn/", "tokens": ["Sie", "has\u00b7set", "gar", "nicht", "mei\u00b7nen", "sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie pflegt es so/ wie ich/ zu machen/", "tokens": ["Sie", "pflegt", "es", "so", "/", "wie", "ich", "/", "zu", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$(", "PWAV", "PPER", "$(", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wir wechseln mund und h\u00e4nd zusammen", "tokens": ["Wir", "wech\u00b7seln", "mund", "und", "h\u00e4nd", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und doppeln unsre liebes-flammen.", "tokens": ["Und", "dop\u00b7peln", "uns\u00b7re", "lie\u00b7bes\u00b7flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ach aber der ich schatten lieb", "tokens": ["Ach", "a\u00b7ber", "der", "ich", "schat\u00b7ten", "lieb"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "ART", "PPER", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und tr\u00e4umend nur auf rosen gehe/", "tokens": ["Und", "tr\u00e4u\u00b7mend", "nur", "auf", "ro\u00b7sen", "ge\u00b7he", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPR", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich fange wasser durch ein sieb!", "tokens": ["Ich", "fan\u00b7ge", "was\u00b7ser", "durch", "ein", "sieb", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "VVIMP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn dis/ was ich erwachet sehe/", "tokens": ["Denn", "dis", "/", "was", "ich", "er\u00b7wa\u00b7chet", "se\u00b7he", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$(", "PWS", "PPER", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ist leichtes stroh und feder-k\u00fcssen/", "tokens": ["Ist", "leich\u00b7tes", "stroh", "und", "fe\u00b7der\u00b7k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "ADJD", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die blumen sind hinweg gerissen.", "tokens": ["Die", "blu\u00b7men", "sind", "hin\u00b7weg", "ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Hinweg! verlognes schatten-werck/", "tokens": ["Hin\u00b7weg", "!", "ver\u00b7log\u00b7nes", "schat\u00b7ten\u00b7\u00b7werck", "/"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$.", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du irrwisch der betr\u00fcbten hertzen/", "tokens": ["Du", "irr\u00b7wisch", "der", "be\u00b7tr\u00fcb\u00b7ten", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du starcke glut/ doch sonder st\u00e4rck/", "tokens": ["Du", "star\u00b7cke", "glut", "/", "doch", "son\u00b7der", "st\u00e4rck", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$(", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du falscher zunder meiner schmertzen/", "tokens": ["Du", "fal\u00b7scher", "zun\u00b7der", "mei\u00b7ner", "schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPOSAT", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Soll ihre gnad im traum nur scheinen/", "tokens": ["Soll", "ih\u00b7re", "gnad", "im", "traum", "nur", "schei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "APPRART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So mu\u00df ich wachend dr\u00fcber weinen.", "tokens": ["So", "mu\u00df", "ich", "wa\u00b7chend", "dr\u00fc\u00b7ber", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}