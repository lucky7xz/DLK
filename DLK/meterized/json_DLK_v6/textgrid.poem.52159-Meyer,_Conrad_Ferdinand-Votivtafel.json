{"textgrid.poem.52159": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Votivtafel", "genre": "verse", "period": "N.A.", "pub_year": 1861, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit k\u00fcmmernden Gedanken schlief", "tokens": ["Mit", "k\u00fcm\u00b7mern\u00b7den", "Ge\u00b7dan\u00b7ken", "schlief"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich ein auf meinem Krankenbett,", "tokens": ["Ich", "ein", "auf", "mei\u00b7nem", "Kran\u00b7ken\u00b7bett", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da kam sie, da erschien sie mir", "tokens": ["Da", "kam", "sie", ",", "da", "er\u00b7schien", "sie", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In einem wunderklaren Traum.", "tokens": ["In", "ei\u00b7nem", "wun\u00b7der\u00b7kla\u00b7ren", "Traum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie war ein M\u00e4dchen gro\u00df und schlank", "tokens": ["Sie", "war", "ein", "M\u00e4d\u00b7chen", "gro\u00df", "und", "schlank"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit feurig blauem Augenlicht,", "tokens": ["Mit", "feu\u00b7rig", "blau\u00b7em", "Au\u00b7gen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie kam und nahm mich bei der Hand", "tokens": ["Sie", "kam", "und", "nahm", "mich", "bei", "der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sagte freundlich: \u00bbWirb um mich!", "tokens": ["Und", "sag\u00b7te", "freund\u00b7lich", ":", "\u00bb", "Wirb", "um", "mich", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$.", "$(", "VVIMP", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Vertraue! Habe Zuversicht!", "tokens": ["Ver\u00b7trau\u00b7e", "!", "Ha\u00b7be", "Zu\u00b7ver\u00b7sicht", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$.", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Halt an und \u00fcberleg es nicht!", "tokens": ["Halt", "an", "und", "\u00fc\u00b7ber\u00b7leg", "es", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Halt an und \u00fcberla\u00df es mir!", "tokens": ["Halt", "an", "und", "\u00fc\u00b7berl\u00b7a\u00df", "es", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "KON", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erbitte mich! Erbitte mich!\u00ab \u2013", "tokens": ["Er\u00b7bit\u00b7te", "mich", "!", "Er\u00b7bit\u00b7te", "mich", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "PPER", "$.", "NN", "PPER", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da wacht ich auf im Morgenlicht", "tokens": ["Da", "wacht", "ich", "auf", "im", "Mor\u00b7gen\u00b7licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hob die H\u00e4nde hoch empor:", "tokens": ["Und", "hob", "die", "H\u00e4n\u00b7de", "hoch", "em\u00b7por", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gebt sie, versaget sie mir nicht,", "tokens": ["Gebt", "sie", ",", "ver\u00b7sa\u00b7get", "sie", "mir", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "VVFIN", "PPER", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr G\u00f6tter, sonst bin ich dahin.", "tokens": ["Ihr", "G\u00f6t\u00b7ter", ",", "sonst", "bin", "ich", "da\u00b7hin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VAFIN", "PPER", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die G\u00f6ttlichen erh\u00f6rten mich,", "tokens": ["Die", "G\u00f6tt\u00b7li\u00b7chen", "er\u00b7h\u00f6r\u00b7ten", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wieder atm ich leichter schon,", "tokens": ["Und", "wie\u00b7der", "atm", "ich", "leich\u00b7ter", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn, siehe, die Genesung war's,", "tokens": ["Denn", ",", "sie\u00b7he", ",", "die", "Ge\u00b7ne\u00b7sung", "wa\u00b7r's", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVIMP", "$,", "ART", "NN", "VAFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die mir erschien im Morgentraum.", "tokens": ["Die", "mir", "er\u00b7schien", "im", "Mor\u00b7gen\u00b7traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}