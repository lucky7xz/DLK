{"textgrid.poem.47646": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ein solcher ist mein Freund]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein solcher ist mein Freund,", "tokens": ["Ein", "sol\u00b7cher", "ist", "mein", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein solcher ist mein Buhle,", "tokens": ["Ein", "sol\u00b7cher", "ist", "mein", "Buh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein einziger, erlesener", "tokens": ["Ein", "ein\u00b7zi\u00b7ger", ",", "er\u00b7le\u00b7se\u00b7ner"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "ADJA", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus s\u00e4mmtlicher M\u00e4nner Schaar:", "tokens": ["Aus", "s\u00e4mmt\u00b7li\u00b7cher", "M\u00e4n\u00b7ner", "Schaar", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wie ein Rabe, so schwarz sein Haar,", "tokens": ["Wie", "ein", "Ra\u00b7be", ",", "so", "schwarz", "sein", "Haar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und seine milden Augen", "tokens": ["Und", "sei\u00b7ne", "mil\u00b7den", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ein Tauben-Augenpaar.", "tokens": ["Ein", "Tau\u00b7ben\u00b7Au\u00b7gen\u00b7paar", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Sein Leib, wie Elfenbein,", "tokens": ["Sein", "Leib", ",", "wie", "El\u00b7fen\u00b7bein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Und wie gehau'n aus Marmor", "tokens": ["Und", "wie", "ge\u00b7hau'n", "aus", "Mar\u00b7mor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VVPP", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Der Schenkel Prachtgebilde,", "tokens": ["Der", "Schen\u00b7kel", "Pracht\u00b7ge\u00b7bil\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Wie L\u00fcfte, die gelinde", "tokens": ["Wie", "L\u00fcf\u00b7te", ",", "die", "ge\u00b7lin\u00b7de"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWAV", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Hins\u00e4useln \u00fcber Rosen,", "tokens": ["Hin\u00b7s\u00e4u\u00b7seln", "\u00fc\u00b7ber", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Sind seine Minnegr\u00fc\u00dfe,", "tokens": ["Sind", "sei\u00b7ne", "Min\u00b7ne\u00b7gr\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Wie des Honiges Seim an S\u00fc\u00dfe", "tokens": ["Wie", "des", "Ho\u00b7ni\u00b7ges", "Seim", "an", "S\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.15": {"text": "Die K\u00fcsse, die er beut,", "tokens": ["Die", "K\u00fcs\u00b7se", ",", "die", "er", "beut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Sein ganzes Sein und Wesen", "tokens": ["Sein", "gan\u00b7zes", "Sein", "und", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Nur Gl\u00fcck und Seligkeit.", "tokens": ["Nur", "Gl\u00fcck", "und", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}