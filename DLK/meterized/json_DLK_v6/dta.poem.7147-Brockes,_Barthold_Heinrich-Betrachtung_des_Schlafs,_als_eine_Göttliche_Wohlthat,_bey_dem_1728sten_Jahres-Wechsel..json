{"dta.poem.7147": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jedoch, mich leitet dies vielleicht zu weit", "tokens": ["Je\u00b7doch", ",", "mich", "lei\u00b7tet", "dies", "viel\u00b7leicht", "zu", "weit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "VVFIN", "PDS", "ADV", "PTKA", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von meinem ersten Zweck, und aus den Schrancken,", "tokens": ["Von", "mei\u00b7nem", "ers\u00b7ten", "Zweck", ",", "und", "aus", "den", "Schran\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die ich mir vorgesetzt zu dieser Wechsel-Zeit.", "tokens": ["Die", "ich", "mir", "vor\u00b7ge\u00b7setzt", "zu", "die\u00b7ser", "Wech\u00b7sel\u00b7Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}