{"textgrid.poem.53873": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "An den Botschafter", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "der Vereinigten Staaten ist folgendes Schreiben abgegangen:", "tokens": ["der", "Ver\u00b7ei\u00b7nig\u00b7ten", "Staa\u00b7ten", "ist", "fol\u00b7gen\u00b7des", "Schrei\u00b7ben", "ab\u00b7ge\u00b7gan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+--+--+-+-+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Euer Exzellenz!", "tokens": ["Eu\u00b7er", "Ex\u00b7zel\u00b7lenz", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ich habe die Ehre, Ihnen folgende Angelegenheit zu unterbreiten:", "tokens": ["Ich", "ha\u00b7be", "die", "Eh\u00b7re", ",", "Ih\u00b7nen", "fol\u00b7gen\u00b7de", "An\u00b7ge\u00b7le\u00b7gen\u00b7heit", "zu", "un\u00b7ter\u00b7brei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}}}}