{"textgrid.poem.56072": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Persisches Heliotrop", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es k\u00f6nnte sein, da\u00df dir der Rose Lob", "tokens": ["Es", "k\u00f6nn\u00b7te", "sein", ",", "da\u00df", "dir", "der", "Ro\u00b7se", "Lob"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VAINF", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "zu laut erscheint f\u00fcr deine Freundin: Nimm", "tokens": ["zu", "laut", "er\u00b7scheint", "f\u00fcr", "dei\u00b7ne", "Freun\u00b7din", ":", "Nimm"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PTKA", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "das sch\u00f6n gestickte Kraut und \u00fcberstimm", "tokens": ["das", "sch\u00f6n", "ge\u00b7stick\u00b7te", "Kraut", "und", "\u00fc\u00b7bers\u00b7timm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "KON", "APPRART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "mit dringend fl\u00fcsterndem Heliotrop", "tokens": ["mit", "drin\u00b7gend", "fl\u00fcs\u00b7tern\u00b7dem", "He\u00b7li\u00b7o\u00b7trop"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "den B\u00fclb\u00fcl, der an ihren Lieblingspl\u00e4tzen", "tokens": ["den", "B\u00fcl\u00b7b\u00fcl", ",", "der", "an", "ih\u00b7ren", "Lieb\u00b7lings\u00b7pl\u00e4t\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "sie schreiend preist und sie nicht kennt.", "tokens": ["sie", "schrei\u00b7end", "preist", "und", "sie", "nicht", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "KON", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn sieh: wie s\u00fc\u00dfe Worte nachts in S\u00e4tzen", "tokens": ["Denn", "sieh", ":", "wie", "s\u00fc\u00b7\u00dfe", "Wor\u00b7te", "nachts", "in", "S\u00e4t\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "$.", "PWAV", "ADJA", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "beisammenstehn ganz dicht, durch nichts getrennt,", "tokens": ["bei\u00b7sam\u00b7men\u00b7stehn", "ganz", "dicht", ",", "durch", "nichts", "ge\u00b7trennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$,", "APPR", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "aus der Vokale wachem Violett", "tokens": ["aus", "der", "Vo\u00b7ka\u00b7le", "wa\u00b7chem", "Vi\u00b7o\u00b7lett"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "hind\u00fcftend durch das stille Himmelbett \u2013:", "tokens": ["hin\u00b7d\u00fcf\u00b7tend", "durch", "das", "stil\u00b7le", "Him\u00b7mel\u00b7bett", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "so schlie\u00dfen sich vor dem gesteppten Laube", "tokens": ["so", "schlie\u00b7\u00dfen", "sich", "vor", "dem", "ges\u00b7tepp\u00b7ten", "Lau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "deutliche Sterne zu der seidnen Traube", "tokens": ["deut\u00b7li\u00b7che", "Ster\u00b7ne", "zu", "der", "seid\u00b7nen", "Trau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "und mischen, da\u00df sie fast davon verschwimmt,", "tokens": ["und", "mi\u00b7schen", ",", "da\u00df", "sie", "fast", "da\u00b7von", "ver\u00b7schwimmt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADV", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "die Stille mit Vanille und mit Zimmt.", "tokens": ["die", "Stil\u00b7le", "mit", "Va\u00b7nil\u00b7le", "und", "mit", "Zimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}