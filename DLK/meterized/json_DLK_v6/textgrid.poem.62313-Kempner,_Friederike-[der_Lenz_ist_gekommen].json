{"textgrid.poem.62313": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[der Lenz ist gekommen]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Lenz ist gekommen", "tokens": ["Der", "Lenz", "ist", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und mit ihm das Gl\u00fcck,", "tokens": ["Und", "mit", "ihm", "das", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Doch wer es nicht glaubet,", "tokens": ["Doch", "wer", "es", "nicht", "glau\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Dem weicht es zur\u00fcck.", "tokens": ["Dem", "weicht", "es", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "O, weiche nicht, bleibe", "tokens": ["O", ",", "wei\u00b7che", "nicht", ",", "blei\u00b7be"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "$,", "ADJA", "PTKNEG", "$,", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "O, weich' nicht zur\u00fcck,", "tokens": ["O", ",", "weich'", "nicht", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Was Einer auch treibe,", "tokens": ["Was", "Ei\u00b7ner", "auch", "trei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Er brauchet das Gl\u00fcck.", "tokens": ["Er", "brau\u00b7chet", "das", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}