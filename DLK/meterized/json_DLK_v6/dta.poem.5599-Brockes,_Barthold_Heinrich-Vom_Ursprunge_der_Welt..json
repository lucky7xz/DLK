{"dta.poem.5599": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vom Ursprunge der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es wanckt der Geist auf beyden Seiten,", "tokens": ["Es", "wanckt", "der", "Geist", "auf", "bey\u00b7den", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn mit dem Alterthum zu streiten,", "tokens": ["Wenn", "mit", "dem", "Al\u00b7ter\u00b7thum", "zu", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ob unsre Welt zu seyn einst angefangen?", "tokens": ["Ob", "uns\u00b7re", "Welt", "zu", "seyn", "einst", "an\u00b7ge\u00b7fan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKZU", "VAINF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie, oder ob die Dau\u2019r der Welt von Ewigkeit?", "tokens": ["Wie", ",", "o\u00b7der", "ob", "die", "Dau'r", "der", "Welt", "von", "E\u00b7wig\u00b7keit", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KON", "KOUS", "ART", "NN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Frage, welche man bek\u00e4mpfft so lange Zeit,", "tokens": ["Die", "Fra\u00b7ge", ",", "wel\u00b7che", "man", "be\u00b7k\u00e4mpfft", "so", "lan\u00b7ge", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ist aller Menschen Witz von je her \u00fcbergangen.", "tokens": ["Ist", "al\u00b7ler", "Men\u00b7schen", "Witz", "von", "je", "her", "\u00fc\u00b7ber\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "NN", "APPR", "ADV", "APZR", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}