{"dta.poem.10912": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "L\u00e4\u00dft sich ein vergn\u00fcglichs Schrecken, ein geheim und schau-", "tokens": ["L\u00e4\u00dft", "sich", "ein", "ver\u00b7gn\u00fcg\u00b7lichs", "Schre\u00b7cken", ",", "ein", "ge\u00b7heim", "und", "schau"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$,", "ART", "ADJD", "KON", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Jm gewachsnen Walde, f\u00fchlen; l\u00e4\u00dft hier dieser schwarze Wald", "tokens": ["Jm", "ge\u00b7wachs\u00b7nen", "Wal\u00b7de", ",", "f\u00fch\u00b7len", ";", "l\u00e4\u00dft", "hier", "die\u00b7ser", "schwar\u00b7ze", "Wald"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "VVFIN", "$.", "VVFIN", "ADV", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Eben die Empfindungen, in fast sichtbarer Gestalt,", "tokens": ["E\u00b7ben", "die", "Emp\u00b7fin\u00b7dun\u00b7gen", ",", "in", "fast", "sicht\u00b7ba\u00b7rer", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.4": {"text": "Den in ihm vertieften Blick, mit nicht mindrer Regung,", "tokens": ["Den", "in", "ihm", "ver\u00b7tief\u00b7ten", "Blick", ",", "mit", "nicht", "mind\u00b7rer", "Re\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "ADJA", "NN", "$,", "APPR", "PTKNEG", "ADJA", "NN", "$,"], "meter": "----+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Aber, welch ein stark Ger\u00e4usch! o! zur\u00fcck! ein schrecklich", "tokens": ["A\u00b7ber", ",", "welch", "ein", "stark", "Ge\u00b7r\u00e4usch", "!", "o", "!", "zu\u00b7r\u00fcck", "!", "ein", "schreck\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "$,", "PWAT", "ART", "ADJD", "NN", "$.", "FM", "$.", "PTKVZ", "$.", "ART", "ADJD"], "meter": "+---+-+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Liegt erhitzt, dort in der S\u00fchle, seinen d\u00fcrren Brand zu k\u00fchlen,", "tokens": ["Liegt", "er\u00b7hitzt", ",", "dort", "in", "der", "S\u00fch\u00b7le", ",", "sei\u00b7nen", "d\u00fcr\u00b7ren", "Brand", "zu", "k\u00fch\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "ADV", "APPR", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Unterm Busch, im feuchten Schilf. H\u00f6rt es schnaufen! seht", "tokens": ["Un\u00b7term", "Busch", ",", "im", "feuch\u00b7ten", "Schilf", ".", "H\u00f6rt", "es", "schnau\u00b7fen", "!", "seht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "APPRART", "ADJA", "NN", "$.", "VVFIN", "PPER", "VVINF", "$.", "VVFIN"], "meter": "+-+-+-++-+-+", "measure": "unknown.measure.septa"}, "line.8": {"text": "Schaut, wie stroblich seine Borsten, wie so lang die Waffen seyn!", "tokens": ["Schaut", ",", "wie", "strob\u00b7lich", "sei\u00b7ne", "Bors\u00b7ten", ",", "wie", "so", "lang", "die", "Waf\u00b7fen", "seyn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ADJD", "PPOSAT", "NN", "$,", "PWAV", "ADV", "ADJD", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Wie des schlammigten Morasts kaltes Bad es innig r\u00fchre;", "tokens": ["Wie", "des", "schlam\u00b7mig\u00b7ten", "Mo\u00b7rasts", "kal\u00b7tes", "Bad", "es", "in\u00b7nig", "r\u00fch\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADJA", "NE", "PPER", "ADJD", "VVFIN", "$."], "meter": "--++-+-+-+-+-+-", "measure": "anapaest.init"}, "line.10": {"text": "Deucht mich, da\u00df ich in den wilden halb geschlo\u00dfnen Augen sp\u00fcre.", "tokens": ["Deucht", "mich", ",", "da\u00df", "ich", "in", "den", "wil\u00b7den", "halb", "ge\u00b7schlo\u00df\u00b7nen", "Au\u00b7gen", "sp\u00fc\u00b7re", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "APPR", "ART", "ADJA", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Ein paar kluge Striche zeigen, von dem ungeheuren Thiere,", "tokens": ["Ein", "paar", "klu\u00b7ge", "Stri\u00b7che", "zei\u00b7gen", ",", "von", "dem", "un\u00b7ge\u00b7heu\u00b7ren", "Thie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVINF", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "Selbst die innre Leidenschaft. Mu\u00df man denn nicht dessen Geist,", "tokens": ["Selbst", "die", "inn\u00b7re", "Lei\u00b7den\u00b7schaft", ".", "Mu\u00df", "man", "denn", "nicht", "des\u00b7sen", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$.", "VMFIN", "PIS", "ADV", "PTKNEG", "PRELAT", "NN", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.13": {"text": "Der, in seiner edlen Kunst, uns so viele Wunder weist;", "tokens": ["Der", ",", "in", "sei\u00b7ner", "ed\u00b7len", "Kunst", ",", "uns", "so", "vie\u00b7le", "Wun\u00b7der", "weist", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Der, was Gott in wilden W\u00e4ldern f\u00fcr uns schuf, uns zu be-", "tokens": ["Der", ",", "was", "Gott", "in", "wil\u00b7den", "W\u00e4l\u00b7dern", "f\u00fcr", "uns", "schuf", ",", "uns", "zu", "be"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "PRELS", "NN", "APPR", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$,", "PPER", "APPR", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.15": {"text": "Recht als wenn es lebte, zeiget; nicht bem\u00fcht seyn hochzuachten?", "tokens": ["Recht", "als", "wenn", "es", "leb\u00b7te", ",", "zei\u00b7get", ";", "nicht", "be\u00b7m\u00fcht", "seyn", "hoch\u00b7zu\u00b7ach\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "$.", "PTKNEG", "VVPP", "VAINF", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}