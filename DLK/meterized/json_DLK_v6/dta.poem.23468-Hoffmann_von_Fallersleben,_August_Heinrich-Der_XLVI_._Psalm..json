{"dta.poem.23468": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der   XLVI   . Psalm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein feste Burg ist unser Gott,", "tokens": ["Ein", "fes\u00b7te", "Burg", "ist", "un\u00b7ser", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein gute Wehr und Waffen.", "tokens": ["Ein", "gu\u00b7te", "Wehr", "und", "Waf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er hilft uns frei aus aller Noth,", "tokens": ["Er", "hilft", "uns", "frei", "aus", "al\u00b7ler", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die uns itzt hat betroffen.", "tokens": ["Die", "uns", "itzt", "hat", "be\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der alt b\u00f6se Feind,", "tokens": ["Der", "alt", "b\u00f6\u00b7se", "Feind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Mit Ernst er's itzt meint.", "tokens": ["Mit", "Ernst", "er's", "itzt", "meint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "ADV", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Gro\u00df Macht und viel List", "tokens": ["Gro\u00df", "Macht", "und", "viel", "List"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "PIAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Sein grausam R\u00fcstung ist,", "tokens": ["Sein", "grau\u00b7sam", "R\u00fcs\u00b7tung", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Auf Erd' ist nicht seins Gleichen.", "tokens": ["Auf", "Erd'", "ist", "nicht", "seins", "Glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mit unser Macht ist nichts gethan:", "tokens": ["Mit", "un\u00b7ser", "Macht", "ist", "nichts", "ge\u00b7than", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir sind gar bald verloren,", "tokens": ["Wir", "sind", "gar", "bald", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es streit f\u00fcr uns der rechte Mann,", "tokens": ["Es", "streit", "f\u00fcr", "uns", "der", "rech\u00b7te", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Gott hat selbs erkoren.", "tokens": ["Den", "Gott", "hat", "selbs", "er\u00b7ko\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Fragst du, wer der ist?", "tokens": ["Fragst", "du", ",", "wer", "der", "ist", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "ART", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Er hei\u00dft Jesus Christ,", "tokens": ["Er", "hei\u00dft", "Je\u00b7sus", "Christ", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NE", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Der Herr Zebaoth,", "tokens": ["Der", "Herr", "Ze\u00b7bao\u00b7th", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Und ist kein ander Gott:", "tokens": ["Und", "ist", "kein", "an\u00b7der", "Gott", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Das Feld mu\u00df er behalten.", "tokens": ["Das", "Feld", "mu\u00df", "er", "be\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wenn die Welt voll Teufel w\u00e4r", "tokens": ["Und", "wenn", "die", "Welt", "voll", "Teu\u00b7fel", "w\u00e4r"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wollt uns gar verschlingen,", "tokens": ["Und", "wollt", "uns", "gar", "ver\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So f\u00fcrchten wir uns nicht so sehr,", "tokens": ["So", "f\u00fcrch\u00b7ten", "wir", "uns", "nicht", "so", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKNEG", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es soll uns doch gelingen.", "tokens": ["Es", "soll", "uns", "doch", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der F\u00fcrst dieser Welt,", "tokens": ["Der", "F\u00fcrst", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Wie saur er sich stellt,", "tokens": ["Wie", "saur", "er", "sich", "stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Thut er uns doch nicht.", "tokens": ["Thut", "er", "uns", "doch", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Das macht, er ist gericht;", "tokens": ["Das", "macht", ",", "er", "ist", "ge\u00b7richt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Ein W\u00f6rtlein kann ihn f\u00e4llen.", "tokens": ["Ein", "W\u00f6rt\u00b7lein", "kann", "ihn", "f\u00e4l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das Wort sie sollen lassen stahn", "tokens": ["Das", "Wort", "sie", "sol\u00b7len", "las\u00b7sen", "stahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VMFIN", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und kein Dank dazu haben.", "tokens": ["Und", "kein", "Dank", "da\u00b7zu", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PAV", "VAFIN", "$."], "meter": "-+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Er ist bei uns wohl auf dem Plan", "tokens": ["Er", "ist", "bei", "uns", "wohl", "auf", "dem", "Plan"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+++-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Mit seinem Geist und Gaben.", "tokens": ["Mit", "sei\u00b7nem", "Geist", "und", "Ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nehmen sie den Leib,", "tokens": ["Neh\u00b7men", "sie", "den", "Leib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Gut, Ehr, Kind und Weib:", "tokens": ["Gut", ",", "Ehr", ",", "Kind", "und", "Weib", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "La\u00df fahren dahin!", "tokens": ["La\u00df", "fah\u00b7ren", "da\u00b7hin", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PAV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Sie habens kein Gewinn:", "tokens": ["Sie", "ha\u00b7bens", "kein", "Ge\u00b7winn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Das Reich mu\u00df uns doch bleiben.", "tokens": ["Das", "Reich", "mu\u00df", "uns", "doch", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}