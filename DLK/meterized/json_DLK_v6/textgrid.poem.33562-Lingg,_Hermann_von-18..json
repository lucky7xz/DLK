{"textgrid.poem.33562": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie blinkte durch die Nacht", "tokens": ["Wie", "blink\u00b7te", "durch", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Um deinen Hals gewunden", "tokens": ["Um", "dei\u00b7nen", "Hals", "ge\u00b7wun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "VAPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der goldnen Kette Pracht!", "tokens": ["Der", "gold\u00b7nen", "Ket\u00b7te", "Pracht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie flogen uns die Stunden!", "tokens": ["Wie", "flo\u00b7gen", "uns", "die", "Stun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Durch heller B\u00e4ume Glanz", "tokens": ["Durch", "hel\u00b7ler", "B\u00e4u\u00b7me", "Glanz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Erklang Musik her\u00fcber", "tokens": ["Er\u00b7klang", "Mu\u00b7sik", "her\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["NN", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ging so eigen ganz", "tokens": ["Und", "ging", "so", "ei\u00b7gen", "ganz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In unser Schw\u00e4rmen \u00fcber.", "tokens": ["In", "un\u00b7ser", "Schw\u00e4r\u00b7men", "\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was wir uns da entdeckt,", "tokens": ["Was", "wir", "uns", "da", "ent\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wie viel wir uns vertrauten,", "tokens": ["Wie", "viel", "wir", "uns", "ver\u00b7trau\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie viel wir halbversteckt", "tokens": ["Wie", "viel", "wir", "halb\u00b7ver\u00b7steckt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Errieten und durchschauten,", "tokens": ["Er\u00b7rie\u00b7ten", "und", "durch\u00b7schau\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Zur Maske ward das Wort", "tokens": ["Zur", "Mas\u00b7ke", "ward", "das", "Wort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zur heitern, bald zur ernsten,", "tokens": ["Zur", "hei\u00b7tern", ",", "bald", "zur", "erns\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADV", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wob sich spielend fort", "tokens": ["Und", "wob", "sich", "spie\u00b7lend", "fort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PRF", "ADJD", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bis zu der Sterne fernsten.", "tokens": ["Bis", "zu", "der", "Ster\u00b7ne", "ferns\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Oft sah uns an im Flug", "tokens": ["Oft", "sah", "uns", "an", "im", "Flug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Aus d\u00fcstrem Schlangenhaare", "tokens": ["Aus", "d\u00fcst\u00b7rem", "Schlan\u00b7gen\u00b7haa\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Schmerz, o gro\u00df genug", "tokens": ["Ein", "Schmerz", ",", "o", "gro\u00df", "ge\u00b7nug"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "FM", "ADJD", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr lange Leidensjahre!", "tokens": ["F\u00fcr", "lan\u00b7ge", "Lei\u00b7dens\u00b7jah\u00b7re", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Doch sank davor sogleich", "tokens": ["Doch", "sank", "da\u00b7vor", "sog\u00b7leich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PAV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein Elfenschleier nieder", "tokens": ["Ein", "El\u00b7fen\u00b7schlei\u00b7er", "nie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und lie\u00df uns in ein Reich", "tokens": ["Und", "lie\u00df", "uns", "in", "ein", "Reich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Des Gl\u00fccks und Friedens nieder.", "tokens": ["Des", "Gl\u00fccks", "und", "Frie\u00b7dens", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "In einem Augenblick", "tokens": ["In", "ei\u00b7nem", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Kam Freud' und Leid wie Wogen", "tokens": ["Kam", "Freud'", "und", "Leid", "wie", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "NN", "KOKOM", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der rauschenden Musik", "tokens": ["Der", "rau\u00b7schen\u00b7den", "Mu\u00b7sik"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "An uns vorbeigeflogen.", "tokens": ["An", "uns", "vor\u00b7bei\u00b7ge\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Vorbei flog fr\u00fche Zeit", "tokens": ["Vor\u00b7bei", "flog", "fr\u00fc\u00b7he", "Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mit goldnen Kinderjahren", "tokens": ["Mit", "gold\u00b7nen", "Kin\u00b7der\u00b7jah\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Zukunft im Geleit", "tokens": ["Und", "Zu\u00b7kunft", "im", "Ge\u00b7leit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bekr\u00e4nzter Hoffnungsscharen.", "tokens": ["Be\u00b7kr\u00e4nz\u00b7ter", "Hoff\u00b7nungs\u00b7scha\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Vorbei flog Sturm im See", "tokens": ["Vor\u00b7bei", "flog", "Sturm", "im", "See"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und Fels und Palmenk\u00fcste,", "tokens": ["Und", "Fels", "und", "Pal\u00b7men\u00b7k\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Indes ich, s\u00fc\u00dfe Fee,", "tokens": ["In\u00b7des", "ich", ",", "s\u00fc\u00b7\u00dfe", "Fee", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dein H\u00e4ndchen hielt und k\u00fc\u00dfte.", "tokens": ["Dein", "H\u00e4nd\u00b7chen", "hielt", "und", "k\u00fc\u00df\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}