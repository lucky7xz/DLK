{"textgrid.poem.42900": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "B\u00e4r aus dem K\u00e4fig entkommen", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist nun jetzt?", "tokens": ["Was", "ist", "nun", "jetzt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wo sind auf einmal die Stangen,", "tokens": ["Wo", "sind", "auf", "ein\u00b7mal", "die", "Stan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "APPR", "ADV", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "An denen die w\u00fcnschende Nase sich wetzt?", "tokens": ["An", "de\u00b7nen", "die", "w\u00fcn\u00b7schen\u00b7de", "Na\u00b7se", "sich", "wetzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Was soll er nun anfangen?", "tokens": ["Was", "soll", "er", "nun", "an\u00b7fan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er schnuppert neugierig und scheu.", "tokens": ["Er", "schnup\u00b7pert", "neu\u00b7gie\u00b7rig", "und", "scheu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Wie ist das alles vor ihm so weit", "tokens": ["Wie", "ist", "das", "al\u00b7les", "vor", "ihm", "so", "weit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "PIS", "APPR", "PPER", "ADV", "ADJD"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und so wundersch\u00f6n neu!", "tokens": ["Und", "so", "wun\u00b7der\u00b7sch\u00f6n", "neu", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ADJD", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Aber wie schrecklich die Menschheit schreit!", "tokens": ["A\u00b7ber", "wie", "schreck\u00b7lich", "die", "Menschheit", "schreit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Und er n\u00e4hert sich geduckt", "tokens": ["Und", "er", "n\u00e4\u00b7hert", "sich", "ge\u00b7duckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "VVPP"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Einem fremden Gegenstande. \u2013", "tokens": ["Ei\u00b7nem", "frem\u00b7den", "Ge\u00b7gen\u00b7stan\u00b7de", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Pl\u00f6tzlich w\u00e4lzt er sich im Sande,", "tokens": ["Pl\u00f6tz\u00b7lich", "w\u00e4lzt", "er", "sich", "im", "San\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil ihn etwas juckt.", "tokens": ["Weil", "ihn", "et\u00b7was", "juckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Kippt ein Tisch. Genau wie Baum.", "tokens": ["Kippt", "ein", "Tisch", ".", "Ge\u00b7nau", "wie", "Baum", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "NN", "KOKOM", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber eine Peitsche knallt.", "tokens": ["A\u00b7ber", "ei\u00b7ne", "Peit\u00b7sche", "knallt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der B\u00e4r flieht seitw\u00e4rts, macht dann halt.", "tokens": ["Und", "der", "B\u00e4r", "flieht", "seit\u00b7w\u00e4rts", ",", "macht", "dann", "halt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "$,", "VVFIN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und der Raum um ihn ist schlimmer Traum.", "tokens": ["Und", "der", "Raum", "um", "ihn", "ist", "schlim\u00b7mer", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPER", "VAFIN", "ADJD", "NN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "L\u00e4\u00dft der B\u00e4r sich locken. Doch er br\u00fcllt.", "tokens": ["L\u00e4\u00dft", "der", "B\u00e4r", "sich", "lo\u00b7cken", ".", "Doch", "er", "br\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "VVINF", "$.", "KON", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "L\u00e4\u00dft sich treiben, l\u00e4\u00dft sich fangen.", "tokens": ["L\u00e4\u00dft", "sich", "trei\u00b7ben", ",", "l\u00e4\u00dft", "sich", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVINF", "$,", "VVFIN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Angsterf\u00fcllt und ha\u00dferf\u00fcllt", "tokens": ["Angs\u00b7ter\u00b7f\u00fcllt", "und", "ha\u00b7\u00dfer\u00b7f\u00fcllt"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00fcnscht er sich nach seines K\u00e4figs Stangen.", "tokens": ["W\u00fcnscht", "er", "sich", "nach", "sei\u00b7nes", "K\u00e4\u00b7figs", "Stan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}