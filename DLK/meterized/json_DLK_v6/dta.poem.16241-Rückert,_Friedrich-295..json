{"dta.poem.16241": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "295.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In Schulen plagte man uns mit der Steigerung", "tokens": ["In", "Schu\u00b7len", "plag\u00b7te", "man", "uns", "mit", "der", "Stei\u00b7ge\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PIS", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von M\u00f6glich-, Wirklich- und Nothwendigkeit genung.", "tokens": ["Von", "M\u00f6g\u00b7lich", ",", "Wirk\u00b7lich", "und", "Noth\u00b7wen\u00b7dig\u00b7keit", "ge\u00b7nung", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "$,", "TRUNC", "KON", "NN", "ADV", "$."], "meter": "-+-+--+---+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Von M\u00f6glich ging man aus, zu Wirklich schritt man weiter,", "tokens": ["Von", "M\u00f6g\u00b7lich", "ging", "man", "aus", ",", "zu", "Wirk\u00b7lich", "schritt", "man", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PIS", "PTKVZ", "$,", "PTKA", "ADJD", "VVFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und legte endlich ans Nothwendige die Leiter.", "tokens": ["Und", "leg\u00b7te", "end\u00b7lich", "ans", "Noth\u00b7wen\u00b7di\u00b7ge", "die", "Lei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Gering sei M\u00f6glichkeit, und Wirklichkeit vornehmer,", "tokens": ["Ge\u00b7ring", "sei", "M\u00f6g\u00b7lich\u00b7keit", ",", "und", "Wirk\u00b7lich\u00b7keit", "vor\u00b7neh\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NN", "$,", "KON", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nothwendigkeit noch mehr, und desto unbequemer.", "tokens": ["Noth\u00b7wen\u00b7dig\u00b7keit", "noch", "mehr", ",", "und", "des\u00b7to", "un\u00b7be\u00b7que\u00b7mer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "$,", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch M\u00f6glichkeit ist leicht, Nothwendigkeit so schwer;", "tokens": ["Doch", "M\u00f6g\u00b7lich\u00b7keit", "ist", "leicht", ",", "Noth\u00b7wen\u00b7dig\u00b7keit", "so", "schwer", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADJD", "$,", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist Leichtes unten wol, und Schweres obenher?", "tokens": ["Ist", "Leich\u00b7tes", "un\u00b7ten", "wol", ",", "und", "Schwe\u00b7res", "o\u00b7ben\u00b7her", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "ADV", "$,", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Drum kehren wir es um, das erste sei das dritte,", "tokens": ["Drum", "keh\u00b7ren", "wir", "es", "um", ",", "das", "ers\u00b7te", "sei", "das", "drit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "ART", "ADJA", "VAFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch zwischen beiden bleibt dem zweiten stets die Mitte.", "tokens": ["Doch", "zwi\u00b7schen", "bei\u00b7den", "bleibt", "dem", "zwei\u00b7ten", "stets", "die", "Mit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VVFIN", "ART", "ADJA", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die Wirklichkeit, die sich nicht senken darf noch heben,", "tokens": ["Die", "Wirk\u00b7lich\u00b7keit", ",", "die", "sich", "nicht", "sen\u00b7ken", "darf", "noch", "he\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "PTKNEG", "VVINF", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bleibt zwischen M\u00f6glich- und Nothwendigkeit im Schweben.", "tokens": ["Bleibt", "zwi\u00b7schen", "M\u00f6g\u00b7lich", "und", "Noth\u00b7wen\u00b7dig\u00b7keit", "im", "Schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "TRUNC", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Nothwendigkeit ist ganz nothwendig Sklaverei,", "tokens": ["Noth\u00b7wen\u00b7dig\u00b7keit", "ist", "ganz", "noth\u00b7wen\u00b7dig", "Skla\u00b7ve\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Halbfrei ist Wirklichkeit, nur M\u00f6glichkeit ganz frei.", "tokens": ["Halb\u00b7frei", "ist", "Wirk\u00b7lich\u00b7keit", ",", "nur", "M\u00f6g\u00b7lich\u00b7keit", "ganz", "frei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,", "ADV", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Nothwendig ist der Grund, und Wirklich steht darauf,", "tokens": ["Noth\u00b7wen\u00b7dig", "ist", "der", "Grund", ",", "und", "Wirk\u00b7lich", "steht", "da\u00b7rauf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "KON", "ADJD", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dar\u00fcber aber nimmt das M\u00f6gliche den Lauf.", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "a\u00b7ber", "nimmt", "das", "M\u00f6g\u00b7li\u00b7che", "den", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "ART", "ADJA", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "La\u00dft aus Nothwendigkeit zur Wirklichkeit uns schreiten,", "tokens": ["La\u00dft", "aus", "Noth\u00b7wen\u00b7dig\u00b7keit", "zur", "Wirk\u00b7lich\u00b7keit", "uns", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "NN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aufschweben dann befreit ins Reich der M\u00f6glichkeiten.", "tokens": ["Auf\u00b7schwe\u00b7ben", "dann", "be\u00b7freit", "ins", "Reich", "der", "M\u00f6g\u00b7lich\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}