{"textgrid.poem.40808": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbwie eine Rose aussieht, w\u00fc\u00dft ich gerne!", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwie eine Rose aussieht, w\u00fc\u00dft ich gerne!", "tokens": ["\u00bb", "wie", "ei\u00b7ne", "Ro\u00b7se", "aus\u00b7sieht", ",", "w\u00fc\u00dft", "ich", "ger\u00b7ne", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KOKOM", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohl wu\u00dft' ich's einst, doch hab' ich's traun, vergessen,", "tokens": ["Wohl", "wu\u00dft'", "ich's", "einst", ",", "doch", "hab'", "ich's", "traun", ",", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$,", "ADV", "VAFIN", "PIS", "VVINF", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn zwischen mir und jenes Fr\u00fchlings Ferne", "tokens": ["Denn", "zwi\u00b7schen", "mir", "und", "je\u00b7nes", "Fr\u00fch\u00b7lings", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "KON", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dehnt l\u00e4ngst der Knechtschaft Nacht sich unermessen!", "tokens": ["Dehnt", "l\u00e4ngst", "der", "Knecht\u00b7schaft", "Nacht", "sich", "un\u00b7er\u00b7mes\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich sah die Rose einst in einem Garten,", "tokens": ["Ich", "sah", "die", "Ro\u00b7se", "einst", "in", "ei\u00b7nem", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch den die Spiele meiner Kindheit flogen;", "tokens": ["Durch", "den", "die", "Spie\u00b7le", "mei\u00b7ner", "Kind\u00b7heit", "flo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich sah sie einst auf flatternden Standarten", "tokens": ["Ich", "sah", "sie", "einst", "auf", "flat\u00b7tern\u00b7den", "Stand\u00b7ar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Heere, die zum blut'gen Kampfe zogen.", "tokens": ["Der", "Hee\u00b7re", ",", "die", "zum", "blut'\u00b7gen", "Kamp\u00b7fe", "zo\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich sah sie einst im Dom vor'm Brautaltare", "tokens": ["Ich", "sah", "sie", "einst", "im", "Dom", "vor'm", "Brau\u00b7tal\u00b7ta\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An einer Jungfrau Herz sich z\u00e4rtlich schmiegen;", "tokens": ["An", "ei\u00b7ner", "Jung\u00b7frau", "Herz", "sich", "z\u00e4rt\u00b7lich", "schmie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich sah sie einst in meines Vaters Haare,", "tokens": ["Ich", "sah", "sie", "einst", "in", "mei\u00b7nes", "Va\u00b7ters", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als Tod ihn auf den Schragen streckte, liegen.", "tokens": ["Als", "Tod", "ihn", "auf", "den", "Schra\u00b7gen", "streck\u00b7te", ",", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich sah, wie an der Brust der M\u00f6rder einer", "tokens": ["Ich", "sah", ",", "wie", "an", "der", "Brust", "der", "M\u00f6r\u00b7der", "ei\u00b7ner"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "APPR", "ART", "NN", "ART", "NN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie mit zur Richtstatt f\u00fchrt' im S\u00fcnderwagen;", "tokens": ["Sie", "mit", "zur", "Richt\u00b7statt", "f\u00fchrt'", "im", "S\u00fcn\u00b7der\u00b7wa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "APPRART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O da\u00df ich s\u00e4\u00df' im Karren anstatt seiner,", "tokens": ["O", "da\u00df", "ich", "s\u00e4\u00df'", "im", "Kar\u00b7ren", "an\u00b7statt", "sei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "APPR", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df ich die Rose k\u00f6nnt' am Herzen tragen!\u00ab", "tokens": ["Da\u00df", "ich", "die", "Ro\u00b7se", "k\u00f6nnt'", "am", "Her\u00b7zen", "tra\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VMFIN", "APPRART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}