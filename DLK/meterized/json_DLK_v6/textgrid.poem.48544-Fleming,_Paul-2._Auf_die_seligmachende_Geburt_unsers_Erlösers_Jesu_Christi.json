{"textgrid.poem.48544": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "2. Auf die seligmachende Geburt unsers Erl\u00f6sers Jesu Christi", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Taue doch, o Himmel, taue!", "tokens": ["Tau\u00b7e", "doch", ",", "o", "Him\u00b7mel", ",", "tau\u00b7e", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "FM", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Brecht, ihr Wolken, regnet her,", "tokens": ["Brecht", ",", "ihr", "Wol\u00b7ken", ",", "reg\u00b7net", "her", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "da\u00df man den Gerechten schaue,", "tokens": ["da\u00df", "man", "den", "Ge\u00b7rech\u00b7ten", "schau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "dessen nun nicht ohn' Beschwer", "tokens": ["des\u00b7sen", "nun", "nicht", "ohn'", "Be\u00b7schwer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "PTKNEG", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "die betr\u00fcbte Welt so lange", "tokens": ["die", "be\u00b7tr\u00fcb\u00b7te", "Welt", "so", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "sich versieht und ihr macht bange!", "tokens": ["sich", "ver\u00b7sieht", "und", "ihr", "macht", "ban\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "KON", "PPER", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ja, es treufelt, ja, es tauet,", "tokens": ["Ja", ",", "es", "treu\u00b7felt", ",", "ja", ",", "es", "tau\u00b7et", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "$,", "PTKANT", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "der gesunde Regen f\u00e4llt.", "tokens": ["der", "ge\u00b7sun\u00b7de", "Re\u00b7gen", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schauet hin, ihr Menschen, schauet:", "tokens": ["Schau\u00b7et", "hin", ",", "ihr", "Men\u00b7schen", ",", "schau\u00b7et", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "dort, dort liegt das Heil der Welt.", "tokens": ["dort", ",", "dort", "liegt", "das", "Heil", "der", "Welt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Di\u00df Kind ist der Tau, der Regen,", "tokens": ["Di\u00df", "Kind", "ist", "der", "Tau", ",", "der", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "der die Erde soll bewegen.", "tokens": ["der", "die", "Er\u00b7de", "soll", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Deucht michs oder ists im Wesen,", "tokens": ["Deucht", "michs", "o\u00b7der", "ists", "im", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wie das Land schon weit und breit", "tokens": ["wie", "das", "Land", "schon", "weit", "und", "breit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "von der Unart ist genesen", "tokens": ["von", "der", "Un\u00b7art", "ist", "ge\u00b7ne\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "VVPP"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.4": {"text": "durch die fromme Feuchtigkeit,", "tokens": ["durch", "die", "from\u00b7me", "Feuch\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "wie da\u00df T\u00e4ler, Feld und H\u00f6hen", "tokens": ["wie", "da\u00df", "T\u00e4\u00b7ler", ",", "Feld", "und", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "schon in sch\u00f6nerm Schmucke gehen?", "tokens": ["schon", "in", "sch\u00f6\u00b7nerm", "Schmu\u00b7cke", "ge\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sei, gew\u00fcndschte Nacht, gegr\u00fc\u00dfet,", "tokens": ["Sei", ",", "ge\u00b7w\u00fcnd\u00b7schte", "Nacht", ",", "ge\u00b7gr\u00fc\u00b7\u00dfet", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "$,", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "da der keusche Jungfermund", "tokens": ["da", "der", "keu\u00b7sche", "Jung\u00b7fer\u00b7mund"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "einen jungen Sohn gek\u00fcsset,", "tokens": ["ei\u00b7nen", "jun\u00b7gen", "Sohn", "ge\u00b7k\u00fcs\u00b7set", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "eh' sie ihn recht sehen kunt',", "tokens": ["eh'", "sie", "ihn", "recht", "se\u00b7hen", "kunt'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "einen Sohn, den sie mit Rechte", "tokens": ["ei\u00b7nen", "Sohn", ",", "den", "sie", "mit", "Rech\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "doch wol Vater hei\u00dfen m\u00f6chte!", "tokens": ["doch", "wol", "Va\u00b7ter", "hei\u00b7\u00dfen", "m\u00f6ch\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Unser Himmel ist im Stalle.", "tokens": ["Un\u00b7ser", "Him\u00b7mel", "ist", "im", "Stal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Recht so, Hirte Sybotus,", "tokens": ["Recht", "so", ",", "Hir\u00b7te", "Sy\u00b7bo\u00b7tus", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "da\u00df du mit der Pfeifen Schalle", "tokens": ["da\u00df", "du", "mit", "der", "Pfei\u00b7fen", "Schal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "ihm verehrest deinen Gru\u00df!", "tokens": ["ihm", "ver\u00b7eh\u00b7rest", "dei\u00b7nen", "Gru\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bei der Engel lauten Ch\u00f6ren", "tokens": ["Bei", "der", "En\u00b7gel", "lau\u00b7ten", "Ch\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "l\u00e4ssest du dich billich h\u00f6ren.", "tokens": ["l\u00e4s\u00b7sest", "du", "dich", "bil\u00b7lich", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Fleug, gemalter West, und streue", "tokens": ["Fleug", ",", "ge\u00b7mal\u00b7ter", "West", ",", "und", "streu\u00b7e"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "aus dem Blumen-Himmel Klee!", "tokens": ["aus", "dem", "Blu\u00b7men\u00b7Him\u00b7mel", "Klee", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df die Luft Narzissen speie,", "tokens": ["Da\u00df", "die", "Luft", "Nar\u00b7zis\u00b7sen", "spei\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lilgen f\u00fcr den wei\u00dfen Schnee,", "tokens": ["Lil\u00b7gen", "f\u00fcr", "den", "wei\u00b7\u00dfen", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "da\u00df das Kind als in der Wiege", "tokens": ["da\u00df", "das", "Kind", "als", "in", "der", "Wie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "KOKOM", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und in hellen Windeln liege!", "tokens": ["und", "in", "hel\u00b7len", "Win\u00b7deln", "lie\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ihr, ihr eingestallten Tiere,", "tokens": ["Ihr", ",", "ihr", "ein\u00b7ge\u00b7stall\u00b7ten", "Tie\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "haucht ihm warmen Atem zu,", "tokens": ["haucht", "ihm", "war\u00b7men", "A\u00b7tem", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "da\u00df es keine K\u00e4lte r\u00fchre!", "tokens": ["da\u00df", "es", "kei\u00b7ne", "K\u00e4l\u00b7te", "r\u00fch\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00f6rt es nicht aus seiner Ruh!", "tokens": ["St\u00f6rt", "es", "nicht", "aus", "sei\u00b7ner", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jungfrau Mutter, denk indessen,", "tokens": ["Jung\u00b7frau", "Mut\u00b7ter", ",", "denk", "in\u00b7des\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "da\u00df du Amme bist, und wessen!", "tokens": ["da\u00df", "du", "Am\u00b7me", "bist", ",", "und", "wes\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VAFIN", "$,", "KON", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "O ihr hochgelobten Krippen,", "tokens": ["O", "ihr", "hoch\u00b7ge\u00b7lob\u00b7ten", "Krip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "unsers Heilands Schirm und Rast,", "tokens": ["un\u00b7sers", "Hei\u00b7lands", "Schirm", "und", "Rast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und o Stall, da\u00df du nicht Lippen,", "tokens": ["und", "o", "Stall", ",", "da\u00df", "du", "nicht", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "NN", "$,", "KOUS", "PPER", "PTKNEG", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "da\u00df du doch nicht Zungen hast,", "tokens": ["da\u00df", "du", "doch", "nicht", "Zun\u00b7gen", "hast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "da\u00df du selbsten k\u00f6ntest singen", "tokens": ["da\u00df", "du", "selbs\u00b7ten", "k\u00f6n\u00b7test", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "von den wundersamen Dingen!", "tokens": ["von", "den", "wun\u00b7der\u00b7sa\u00b7men", "Din\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Kleiner Gast, doch auch zugleiche", "tokens": ["Klei\u00b7ner", "Gast", ",", "doch", "auch", "zu\u00b7glei\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADV", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "gro\u00dfer Wirt der weiten Welt,", "tokens": ["gro\u00b7\u00dfer", "Wirt", "der", "wei\u00b7ten", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "gib doch k\u00fcnftig unserm Reiche,", "tokens": ["gib", "doch", "k\u00fcnf\u00b7tig", "un\u00b7serm", "Rei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADJD", "PPOSAT", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "da\u00df es sich zufrieden stelt,", "tokens": ["da\u00df", "es", "sich", "zu\u00b7frie\u00b7den", "stelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "da\u00df doch mit dem alten Jahre", "tokens": ["da\u00df", "doch", "mit", "dem", "al\u00b7ten", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "hin auch alle Plage fahre!", "tokens": ["hin", "auch", "al\u00b7le", "Pla\u00b7ge", "fah\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Segne k\u00fcnftig unsre Linden,", "tokens": ["Seg\u00b7ne", "k\u00fcnf\u00b7tig", "uns\u00b7re", "Lin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PPOSAT", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "unsre halbgestorbne Stadt,", "tokens": ["uns\u00b7re", "halb\u00b7ge\u00b7storb\u00b7ne", "Stadt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "da\u00df sich m\u00f6ge wieder finden", "tokens": ["da\u00df", "sich", "m\u00f6\u00b7ge", "wie\u00b7der", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "VMFIN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "was der Krieg verderbet hat!", "tokens": ["was", "der", "Krieg", "ver\u00b7der\u00b7bet", "hat", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Reinige die faulen L\u00fcfte,", "tokens": ["Rei\u00b7ni\u00b7ge", "die", "fau\u00b7len", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "die so schwanger sein mit Gifte!", "tokens": ["die", "so", "schwan\u00b7ger", "sein", "mit", "Gif\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VAINF", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}