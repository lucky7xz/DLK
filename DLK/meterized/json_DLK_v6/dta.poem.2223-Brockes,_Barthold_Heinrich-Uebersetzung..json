{"dta.poem.2223": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Uebersetzung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der wilde Zorn entsteht aus Leiden, ist selbst ein Leid,", "tokens": ["Der", "wil\u00b7de", "Zorn", "ent\u00b7steht", "aus", "Lei\u00b7den", ",", "ist", "selbst", "ein", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$,", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "gebieret Leiden,", "tokens": ["ge\u00b7bie\u00b7ret", "Lei\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Er kann vergangne, gegenw\u00e4rtge, auch k\u00fcnftge Plagen", "tokens": ["Er", "kann", "ver\u00b7gang\u00b7ne", ",", "ge\u00b7gen\u00b7w\u00e4rt\u00b7ge", ",", "auch", "k\u00fcnft\u00b7ge", "Pla\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJA", "$,", "ADJA", "$,", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "nicht vermeiden.", "tokens": ["nicht", "ver\u00b7mei\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}