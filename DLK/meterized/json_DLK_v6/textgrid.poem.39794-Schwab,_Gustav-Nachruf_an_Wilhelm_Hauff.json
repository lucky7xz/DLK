{"textgrid.poem.39794": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Nachruf an Wilhelm Hauff", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O heller Tage dunkles Ende, Tod!", "tokens": ["O", "hel\u00b7ler", "Ta\u00b7ge", "dunk\u00b7les", "En\u00b7de", ",", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADJA", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Schleier wirfst du \u00fcber viel Gestalten,", "tokens": ["Den", "Schlei\u00b7er", "wirfst", "du", "\u00fc\u00b7ber", "viel", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die reich an Leben und von Jugend rot", "tokens": ["Die", "reich", "an", "Le\u00b7ben", "und", "von", "Ju\u00b7gend", "rot"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "NN", "KON", "APPR", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vor diesem Auge, das du schlossest, wallten;", "tokens": ["Vor", "die\u00b7sem", "Au\u00b7ge", ",", "das", "du", "schlos\u00b7sest", ",", "wall\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ach! was die Welt im Keim von Bildern bot,", "tokens": ["Ach", "!", "was", "die", "Welt", "im", "Keim", "von", "Bil\u00b7dern", "bot", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWS", "ART", "NN", "APPRART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das wu\u00dft' es schnell und gl\u00e4nzend zu entfalten,", "tokens": ["Das", "wu\u00dft'", "es", "schnell", "und", "gl\u00e4n\u00b7zend", "zu", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das wandelt ", "tokens": ["Das", "wan\u00b7delt"], "token_info": ["word", "word"], "pos": ["PDS", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Nur, der es schuf, der schauet es nicht mehr.", "tokens": ["Nur", ",", "der", "es", "schuf", ",", "der", "schau\u00b7et", "es", "nicht", "mehr", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Aus Lust und Laune mu\u00dft' Er bald heraus,", "tokens": ["Aus", "Lust", "und", "Lau\u00b7ne", "mu\u00dft'", "Er", "bald", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VMFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und kurze Frist ward Ihm zum Ernst gegeben:", "tokens": ["Und", "kur\u00b7ze", "Frist", "ward", "Ihm", "zum", "Ernst", "ge\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Liebe f\u00fchrt' ihn in des Freundes Haus,", "tokens": ["Die", "Lie\u00b7be", "f\u00fchrt'", "ihn", "in", "des", "Freun\u00b7des", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem j\u00e4her Sturz zerschmetterte das Leben,", "tokens": ["Dem", "j\u00e4\u00b7her", "Sturz", "zer\u00b7schmet\u00b7ter\u00b7te", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da ward vertraut Er mit des Grabes Graus,", "tokens": ["Da", "ward", "ver\u00b7traut", "Er", "mit", "des", "Gra\u00b7bes", "Graus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und neue Bilder sah Er um sich schweben:", "tokens": ["Und", "neu\u00b7e", "Bil\u00b7der", "sah", "Er", "um", "sich", "schwe\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Den Blick' versenkt' Er in den schwarzen Schlund:", "tokens": ["Den", "Blick'", "ver\u00b7senkt'", "Er", "in", "den", "schwar\u00b7zen", "Schlund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "La\u00df mich zum Freunde! sprach sein kranker Mund.", "tokens": ["La\u00df", "mich", "zum", "Freun\u00b7de", "!", "sprach", "sein", "kran\u00b7ker", "Mund", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "$.", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und Weib, und Kind, und Ruhm \u2013 bescheeret kaum \u2013", "tokens": ["Und", "Weib", ",", "und", "Kind", ",", "und", "Ruhm", "\u2013", "be\u00b7schee\u00b7ret", "kaum", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "KON", "NN", "$,", "KON", "NN", "$(", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schon schwand's vor ihm, und immer ward er stummer;", "tokens": ["Schon", "schwan\u00b7d's", "vor", "ihm", ",", "und", "im\u00b7mer", "ward", "er", "stum\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "$,", "KON", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Vom Traum des Dichters ging's in Fiebertraum,", "tokens": ["Vom", "Traum", "des", "Dich\u00b7ters", "ging's", "in", "Fie\u00b7ber\u00b7traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vom bunten Fiebertraum in tiefen Schlummer,", "tokens": ["Vom", "bun\u00b7ten", "Fie\u00b7ber\u00b7traum", "in", "tie\u00b7fen", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und so, im Schlummer, in den engen Raum,", "tokens": ["Und", "so", ",", "im", "Schlum\u00b7mer", ",", "in", "den", "en\u00b7gen", "Raum", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPRART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In den er sinkt zu seiner Freunde Kummer;", "tokens": ["In", "den", "er", "sinkt", "zu", "sei\u00b7ner", "Freun\u00b7de", "Kum\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Uns bleibt von seines Hauptes Jugendglanz", "tokens": ["Uns", "bleibt", "von", "sei\u00b7nes", "Haup\u00b7tes", "Ju\u00b7gend\u00b7glanz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nichts als der fr\u00fche, gr\u00fcne Lorbeerkranz.", "tokens": ["Nichts", "als", "der", "fr\u00fc\u00b7he", ",", "gr\u00fc\u00b7ne", "Lor\u00b7beer\u00b7kranz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch weinet ihr um den entflohnen Geist", "tokens": ["Doch", "wei\u00b7net", "ihr", "um", "den", "ent\u00b7floh\u00b7nen", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In hoffnungsloser Trauer nicht, ihr Lieben!", "tokens": ["In", "hoff\u00b7nungs\u00b7lo\u00b7ser", "Trau\u00b7er", "nicht", ",", "ihr", "Lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKNEG", "$,", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Uns ist, was uns der Wahrheit Wort verhei\u00dft,", "tokens": ["Uns", "ist", ",", "was", "uns", "der", "Wahr\u00b7heit", "Wort", "ver\u00b7hei\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit Flammenschrift in unsre Brust geschrieben:", "tokens": ["Mit", "Flam\u00b7men\u00b7schrift", "in", "uns\u00b7re", "Brust", "ge\u00b7schrie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Kraft, die sch\u00f6pferisch den Sch\u00f6pfer preist,", "tokens": ["Die", "Kraft", ",", "die", "sch\u00f6p\u00b7fe\u00b7risch", "den", "Sch\u00f6p\u00b7fer", "preist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die denkt und dichtet, sie kann nicht verstieben;", "tokens": ["Die", "denkt", "und", "dich\u00b7tet", ",", "sie", "kann", "nicht", "ver\u00b7stie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sie schwindet nicht hin\u00fcber in das Nichts,", "tokens": ["Sie", "schwin\u00b7det", "nicht", "hin\u00b7\u00fc\u00b7ber", "in", "das", "Nichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "APPR", "ART", "PIS", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sie schaffet droben in dem Reich des Lichts.", "tokens": ["Sie", "schaf\u00b7fet", "dro\u00b7ben", "in", "dem", "Reich", "des", "Lichts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}