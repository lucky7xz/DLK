{"textgrid.poem.48669": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "32. Treue Pflicht", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Ungl\u00fcck ist zu gro\u00df,", "tokens": ["Mein", "Un\u00b7gl\u00fcck", "ist", "zu", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "zu schwer die Not,", "tokens": ["zu", "schwer", "die", "Not", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "so mancher Herzenssto\u00df", "tokens": ["so", "man\u00b7cher", "Her\u00b7zens\u00b7sto\u00df"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "dreut mir den Tod.", "tokens": ["dreut", "mir", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Mein Schmerze wei\u00df von keiner Zahl.", "tokens": ["Mein", "Schmer\u00b7ze", "wei\u00df", "von", "kei\u00b7ner", "Zahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vor, nach und allemal", "tokens": ["Vor", ",", "nach", "und", "al\u00b7le\u00b7mal"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["APPR", "$,", "APPR", "KON", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "h\u00e4uft sich die Qual.", "tokens": ["h\u00e4uft", "sich", "die", "Qual", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Ein Mensch hat alle Schuld,", "tokens": ["Ein", "Mensch", "hat", "al\u00b7le", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "das mich doch liebt.", "tokens": ["das", "mich", "doch", "liebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Das, weil es mir ist huld,", "tokens": ["Das", ",", "weil", "es", "mir", "ist", "huld", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "PPER", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "mich so betr\u00fcbt.", "tokens": ["mich", "so", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Von Liebe k\u00f6mmt mir alles Leid.", "tokens": ["Von", "Lie\u00b7be", "k\u00f6mmt", "mir", "al\u00b7les", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich wei\u00df von keiner Zeit,", "tokens": ["Ich", "wei\u00df", "von", "kei\u00b7ner", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "die mich erfreut.", "tokens": ["die", "mich", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Preist jemand ihre Pracht,", "tokens": ["Preist", "je\u00b7mand", "ih\u00b7re", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "so wird mir weh.", "tokens": ["so", "wird", "mir", "weh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wer ihr gedenkt, der macht,", "tokens": ["Wer", "ihr", "ge\u00b7denkt", ",", "der", "macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "da\u00df ich vergeh'.", "tokens": ["da\u00df", "ich", "ver\u00b7geh'", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Erinner' ich mich denn der Pflicht,", "tokens": ["Er\u00b7in\u00b7ner'", "ich", "mich", "denn", "der", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "was Wunder ists, da\u00df nicht", "tokens": ["was", "Wun\u00b7der", "ists", ",", "da\u00df", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "NN", "VAFIN", "$,", "KOUS", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "mein Herze bricht.", "tokens": ["mein", "Her\u00b7ze", "bricht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Licht ist ihr Augenglanz,", "tokens": ["Licht", "ist", "ihr", "Au\u00b7gen\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "klar ihre Zier.", "tokens": ["klar", "ih\u00b7re", "Zier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Das macht, da\u00df ich mich ganz", "tokens": ["Das", "macht", ",", "da\u00df", "ich", "mich", "ganz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "PPER", "PRF", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "verlier in ihr.", "tokens": ["ver\u00b7lier", "in", "ihr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Sie hat es, was mein Herze sucht,", "tokens": ["Sie", "hat", "es", ",", "was", "mein", "Her\u00b7ze", "sucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Scham, Sch\u00f6nheit, Jugend, Zucht,", "tokens": ["Scham", ",", "Sch\u00f6n\u00b7heit", ",", "Ju\u00b7gend", ",", "Zucht", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "der Tugend Frucht.", "tokens": ["der", "Tu\u00b7gend", "Frucht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "An ihr liegt Alles mir.", "tokens": ["An", "ihr", "liegt", "Al\u00b7les", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIS", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Was acht' ich mich?", "tokens": ["Was", "acht'", "ich", "mich", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mein Sinn ist Freund mit ihr", "tokens": ["Mein", "Sinn", "ist", "Freund", "mit", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "und hasset sich.", "tokens": ["und", "has\u00b7set", "sich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Was ich beginne spat und fr\u00fch,", "tokens": ["Was", "ich", "be\u00b7gin\u00b7ne", "spat", "und", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was ich gedenk, ist sie,", "tokens": ["Was", "ich", "ge\u00b7denk", ",", "ist", "sie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "die Werthe, die.", "tokens": ["die", "Wert\u00b7he", ",", "die", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Sie hat mich ganz bei sich,", "tokens": ["Sie", "hat", "mich", "ganz", "bei", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "das sch\u00f6ne Kind;", "tokens": ["das", "sch\u00f6\u00b7ne", "Kind", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "jhr auch zu lassen mich", "tokens": ["jhr", "auch", "zu", "las\u00b7sen", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "bin ich gesinnt.", "tokens": ["bin", "ich", "ge\u00b7sinnt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Die Treue, die sie mir verspricht,", "tokens": ["Die", "Treu\u00b7e", ",", "die", "sie", "mir", "ver\u00b7spricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "find' ich in solcher Pflicht,", "tokens": ["find'", "ich", "in", "sol\u00b7cher", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "sonst nirgends nicht.", "tokens": ["sonst", "nir\u00b7gends", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Und leb ich mich gleich tot", "tokens": ["Und", "leb", "ich", "mich", "gleich", "tot"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "in solcher Pein,", "tokens": ["in", "sol\u00b7cher", "Pein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "noch hat es keine Not;", "tokens": ["noch", "hat", "es", "kei\u00b7ne", "Not", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "sie, sie kans sein,", "tokens": ["sie", ",", "sie", "kans", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "VMFIN", "VAINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "die mir das Leben wiedergiebt,", "tokens": ["die", "mir", "das", "Le\u00b7ben", "wie\u00b7der\u00b7giebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "die mich so sehr betr\u00fcbt,", "tokens": ["die", "mich", "so", "sehr", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "als sie mich liebt.", "tokens": ["als", "sie", "mich", "liebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Ach! da\u00df ich ihr mein Leid", "tokens": ["Ach", "!", "da\u00df", "ich", "ihr", "mein", "Leid"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "KOUS", "PPER", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "nicht klagen kan!", "tokens": ["nicht", "kla\u00b7gen", "kan", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ich bin von ihr zu weit", "tokens": ["Ich", "bin", "von", "ihr", "zu", "weit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "PTKA", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "itzt abgetan.", "tokens": ["itzt", "ab\u00b7ge\u00b7tan", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Von Scheiden k\u00f6mmt mir alle Not;", "tokens": ["Von", "Schei\u00b7den", "k\u00f6mmt", "mir", "al\u00b7le", "Not", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "di\u00df macht mich bla\u00df f\u00fcr rot,", "tokens": ["di\u00df", "macht", "mich", "bla\u00df", "f\u00fcr", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "APPR", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "f\u00fcr lebend tot.", "tokens": ["f\u00fcr", "le\u00b7bend", "tot", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJD", "$."], "meter": "---+", "measure": "unknown.measure.single"}}, "stanza.9": {"line.1": {"text": "L\u00e4uft nun mein Gl\u00fccke so?", "tokens": ["L\u00e4uft", "nun", "mein", "Gl\u00fc\u00b7cke", "so", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ach wehe mir!", "tokens": ["Ach", "we\u00b7he", "mir", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "O! warum ward ich froh", "tokens": ["O", "!", "wa\u00b7rum", "ward", "ich", "froh"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "PWAV", "VAFIN", "PPER", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "von ihrer Zier?", "tokens": ["von", "ih\u00b7rer", "Zier", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "F\u00fcr jene kurze Fr\u00f6lichkeit", "tokens": ["F\u00fcr", "je\u00b7ne", "kur\u00b7ze", "Fr\u00f6\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "hab' ich ein langes Leid", "tokens": ["hab'", "ich", "ein", "lan\u00b7ges", "Leid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "auf allezeit.", "tokens": ["auf", "al\u00b7le\u00b7zeit", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Bekenne selbst auf dich,", "tokens": ["Be\u00b7ken\u00b7ne", "selbst", "auf", "dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "mein kranker Sinn,", "tokens": ["mein", "kran\u00b7ker", "Sinn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "hast du nicht Schuld, da\u00df ich", "tokens": ["hast", "du", "nicht", "Schuld", ",", "da\u00df", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "NN", "$,", "KOUS", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "so elend bin?", "tokens": ["so", "e\u00b7lend", "bin", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+", "mea