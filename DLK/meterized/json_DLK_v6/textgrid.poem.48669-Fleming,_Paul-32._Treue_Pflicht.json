": ["PWS", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.16": {"line.1": {"text": "Itzt ist es Mitternacht,", "tokens": ["Itzt", "ist", "es", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "da alles ruht.", "tokens": ["da", "al\u00b7les", "ruht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mein munter Herze wacht,", "tokens": ["Mein", "mun\u00b7ter", "Her\u00b7ze", "wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "tut, was es tut.", "tokens": ["tut", ",", "was", "es", "tut", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Es denkt, von m\u00fcden Thr\u00e4nen na\u00df,", "tokens": ["Es", "denkt", ",", "von", "m\u00fc\u00b7den", "Thr\u00e4\u00b7nen", "na\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "von ihr ohn' Unterla\u00df", "tokens": ["von", "ihr", "ohn'", "Un\u00b7ter\u00b7la\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "und wei\u00df nicht was.", "tokens": ["und", "wei\u00df", "nicht", "was", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PIS", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.17": {"line.1": {"text": "Ein Kranker, der gewi\u00df", "tokens": ["Ein", "Kran\u00b7ker", ",", "der", "ge\u00b7wi\u00df"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "am Tode liegt,", "tokens": ["am", "To\u00b7de", "liegt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "der tr\u00f6stet sich auf di\u00df,", "tokens": ["der", "tr\u00f6s\u00b7tet", "sich", "auf", "di\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PRF", "APPR", "PDS", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "was er auch kriegt.", "tokens": ["was", "er", "auch", "kriegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Das ist gewi\u00df, ich mu\u00df dahin,", "tokens": ["Das", "ist", "ge\u00b7wi\u00df", ",", "ich", "mu\u00df", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$,", "PPER", "VMFIN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "doch bleib' ich, wie ich bin,", "tokens": ["doch", "bleib'", "ich", ",", "wie", "ich", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "frisch ohne Sinn.", "tokens": ["frisch", "oh\u00b7ne", "Sinn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.18": {"line.1": {"text": "Erbarmens bin ich wert.", "tokens": ["Er\u00b7bar\u00b7mens", "bin", "ich", "wert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Doch klagt mich nicht,", "tokens": ["Doch", "klagt", "mich", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "bis da\u00df sie von mir kehrt", "tokens": ["bis", "da\u00df", "sie", "von", "mir", "kehrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "der Liebe Pflicht.", "tokens": ["der", "Lie\u00b7be", "Pflicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Doch wird Dianens Brudern Schein", "tokens": ["Doch", "wird", "Di\u00b7a\u00b7nens", "Bru\u00b7dern", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "NE", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "eh' gehn am Himmel ein,", "tokens": ["eh'", "gehn", "am", "Him\u00b7mel", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "als dieses sein.", "tokens": ["als", "die\u00b7ses", "sein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.19": {"line.1": {"text": "Mit Gott und mit der Zeit", "tokens": ["Mit", "Gott", "und", "mit", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "mu\u00df Alles sein.", "tokens": ["mu\u00df", "Al\u00b7les", "sein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ein Wechsel kehrt mein Leid", "tokens": ["Ein", "Wech\u00b7sel", "kehrt", "mein", "Leid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "und ganze Pein.", "tokens": ["und", "gan\u00b7ze", "Pein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Hat nichts als Unbestand Bestand,", "tokens": ["Hat", "nichts", "als", "Un\u00b7be\u00b7stand", "Be\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "KOKOM", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "so wird mein Ach zuhand", "tokens": ["so", "wird", "mein", "Ach", "zu\u00b7hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "in Lust verwant.", "tokens": ["in", "Lust", "ver\u00b7want", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.20": {"line.1": {"text": "Habt Achtung auf mein Leid,", "tokens": ["Habt", "Ach\u00b7tung", "auf", "mein", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "auf meine Qual,", "tokens": ["auf", "mei\u00b7ne", "Qual", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "ihr, die ihr W\u00e4chter seid", "tokens": ["ihr", ",", "die", "ihr", "W\u00e4ch\u00b7ter", "seid"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "NE", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "in Amors Saal'.", "tokens": ["in", "A\u00b7mors", "Saal'", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Hebt alle meine Tr\u00e4nen auf", "tokens": ["Hebt", "al\u00b7le", "mei\u00b7ne", "Tr\u00e4\u00b7nen", "auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und schafft mir Freude drauf", "tokens": ["und", "schafft", "mir", "Freu\u00b7de", "drauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN", "PAV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "f\u00fcr guten Kauf.", "tokens": ["f\u00fcr", "gu\u00b7ten", "Kauf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.21": {"line.1": {"text": "Ihr Sternen auch, die ihr", "tokens": ["Ihr", "Ster\u00b7nen", "auch", ",", "die", "ihr"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "$,", "PRELS", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "vor habt geliebt", "tokens": ["vor", "habt", "ge\u00b7liebt"], "token_info": ["word", "word", "word"], "pos": ["APPR", "VAFIN", "VVPP"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "und oftmals, wie itzt wir,", "tokens": ["und", "oft\u00b7mals", ",", "wie", "itzt", "wir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "ADV", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "auch wart betr\u00fcbt,", "tokens": ["auch", "wart", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "tut, wie man hat an euch getan,", "tokens": ["tut", ",", "wie", "man", "hat", "an", "euch", "ge\u00b7tan", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PIS", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "schreibt meine Seufzer an", "tokens": ["schreibt", "mei\u00b7ne", "Seuf\u00b7zer", "an"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "in Jovis Plan.", "tokens": ["in", "Jo\u00b7vis", "Plan", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.22": {"line.1": {"text": "Vergess' ich meiner Pflicht,", "tokens": ["Ver\u00b7ge\u00b7ss'", "ich", "mei\u00b7ner", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "ja, s\u00e4um ich nur", "tokens": ["ja", ",", "s\u00e4um", "ich", "nur"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUI", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "und halt' ich dieses nicht,", "tokens": ["und", "halt'", "ich", "die\u00b7ses", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDS", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "was ich ihr schwur,", "tokens": ["was", "ich", "ihr", "schwur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "so sei mir Venus nimmer gut,", "tokens": ["so", "sei", "mir", "Ve\u00b7nus", "nim\u00b7mer", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "so qu\u00e4le sich mein Mut,", "tokens": ["so", "qu\u00e4\u00b7le", "sich", "mein", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "wie er itzt tut.", "tokens": ["wie", "er", "itzt", "tut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.23": {"line.1": {"text": "Nein! Ich will feste stehn.", "tokens": ["Nein", "!", "Ich", "will", "fes\u00b7te", "stehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VMFIN", "ADJA", "VVINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Sie, wie sie mir verspricht,", "tokens": ["Sie", ",", "wie", "sie", "mir", "ver\u00b7spricht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "wird auch mir gleiche gehn", "tokens": ["wird", "auch", "mir", "glei\u00b7che", "gehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPER", "ADJA", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "und wanken nicht.", "tokens": ["und", "wan\u00b7ken", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Des Herzens, das sich selbst nicht schont,", "tokens": ["Des", "Her\u00b7zens", ",", "das", "sich", "selbst", "nicht", "schont", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "mit Treue Treue lohnt,", "tokens": ["mit", "Treu\u00b7e", "Treu\u00b7e", "lohnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "bin ich gewohnt.", "tokens": ["bin", "ich", "ge\u00b7wohnt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.24": {"line.1": {"text": "So steht mein fester Schlu\u00df", "tokens": ["So", "steht", "mein", "fes\u00b7ter", "Schlu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "unwiderruft.", "tokens": ["un\u00b7wi\u00b7der\u00b7ruft", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Drauf schick' ich diesen Ku\u00df", "tokens": ["Drauf", "schick'", "ich", "die\u00b7sen", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "ihr durch die Luft.", "tokens": ["ihr", "durch", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Di\u00df Lied auch sei von meiner Hand", "tokens": ["Di\u00df", "Lied", "auch", "sei", "von", "mei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "ADV", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "als meiner Liebe Pfand", "tokens": ["als", "mei\u00b7ner", "Lie\u00b7be", "Pfand"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "ihr zugesant.", "tokens": ["ihr", "zu\u00b7ge\u00b7sant", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.25": {"line.1": {"text": "Gl\u00fcckt mirs und sagt nicht nein,", "tokens": ["Gl\u00fcckt", "mirs", "und", "sagt", "nicht", "nein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "KON", "VVFIN", "PTKNEG", "PTKANT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "der Alles f\u00fcgt,", "tokens": ["der", "Al\u00b7les", "f\u00fcgt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "so soll sies einig sein,", "tokens": ["so", "soll", "sies", "ei\u00b7nig", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "die mich vergn\u00fcgt,", "tokens": ["die", "mich", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Mein letztes Wort ist: Treue Pflicht.", "tokens": ["Mein", "letz\u00b7tes", "Wort", "ist", ":", "Treu\u00b7e", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Treu' ist es: der es spricht", "tokens": ["Treu'", "ist", "es", ":", "der", "es", "spricht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$.", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "mehr kan er nicht.", "tokens": ["mehr", "kan", "er", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}