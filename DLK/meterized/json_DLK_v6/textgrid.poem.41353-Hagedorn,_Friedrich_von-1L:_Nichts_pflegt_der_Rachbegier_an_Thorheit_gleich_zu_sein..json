{"textgrid.poem.41353": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nichts pflegt der Rachbegier an Thorheit gleich zu sein.", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nichts pflegt der Rachbegier an Thorheit gleich zu sein.", "tokens": ["Nichts", "pflegt", "der", "Rach\u00b7be\u00b7gier", "an", "Thor\u00b7heit", "gleich", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPR", "NN", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Mann, der unverhofft sein feistes Kalb vermi\u00dfte,", "tokens": ["Ein", "Mann", ",", "der", "un\u00b7ver\u00b7hofft", "sein", "feis\u00b7tes", "Kalb", "ver\u00b7mi\u00df\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Schwur, wenn er seinen Dieb nur zu entdecken w\u00fc\u00dfte,", "tokens": ["Schwur", ",", "wenn", "er", "sei\u00b7nen", "Dieb", "nur", "zu", "ent\u00b7de\u00b7cken", "w\u00fc\u00df\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wollt' er einen Bock dem Pan zum Opfer weihn.", "tokens": ["So", "wollt'", "er", "ei\u00b7nen", "Bock", "dem", "Pan", "zum", "Op\u00b7fer", "weihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NE", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sein Wunsch ward ihm gew\u00e4hrt. Es kam ein Pantherthier,", "tokens": ["Sein", "Wunsch", "ward", "ihm", "ge\u00b7w\u00e4hrt", ".", "Es", "kam", "ein", "Pan\u00b7ther\u00b7thier", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das gafft' und bleckt' ihn an, und droht' ihn zu verschlingen.", "tokens": ["Das", "gafft'", "und", "bleckt'", "ihn", "an", ",", "und", "droht'", "ihn", "zu", "ver\u00b7schlin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da seufzt' er: ich will gern mein Opfer zehnfach bringen,", "tokens": ["Da", "seufzt'", "er", ":", "ich", "will", "gern", "mein", "Op\u00b7fer", "zehn\u00b7fach", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PPER", "VMFIN", "ADV", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur treib', o starker Pan! den nahen Feind von hier.", "tokens": ["Nur", "treib'", ",", "o", "star\u00b7ker", "Pan", "!", "den", "na\u00b7hen", "Feind", "von", "hier", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "FM", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Betrogne Sterblichen, wer kennt sein wahres Wohl,", "tokens": ["Be\u00b7trog\u00b7ne", "Sterb\u00b7li\u00b7chen", ",", "wer", "kennt", "sein", "wah\u00b7res", "Wohl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWS", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So oft Gel\u00fcbd' und Wunsch den Rath der Allmacht st\u00f6ret?", "tokens": ["So", "oft", "Ge\u00b7l\u00fcbd'", "und", "Wunsch", "den", "Rath", "der", "All\u00b7macht", "st\u00f6\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "KON", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn uns des Himmels Zorn, zu unsrer Straf', erh\u00f6ret,", "tokens": ["Wenn", "uns", "des", "Him\u00b7mels", "Zorn", ",", "zu", "uns\u00b7rer", "Straf'", ",", "er\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So lernt man allererst, warum man bitten soll.", "tokens": ["So", "lernt", "man", "al\u00b7le\u00b7rerst", ",", "wa\u00b7rum", "man", "bit\u00b7ten", "soll", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$,", "PWAV", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}