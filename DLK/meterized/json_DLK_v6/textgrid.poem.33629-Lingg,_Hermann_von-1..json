{"textgrid.poem.33629": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht nur der Nebel ist's, der heute", "tokens": ["Nicht", "nur", "der", "Ne\u00b7bel", "ist's", ",", "der", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "ADV", "ART", "NN", "VAFIN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So d\u00fcster an den Bergen h\u00e4ngt,", "tokens": ["So", "d\u00fcs\u00b7ter", "an", "den", "Ber\u00b7gen", "h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Herbst ist's, der mich einst erfreute", "tokens": ["Der", "Herbst", "ist's", ",", "der", "mich", "einst", "er\u00b7freu\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und jetzt mit Schwermut mich bedr\u00e4ngt.", "tokens": ["Und", "jetzt", "mit", "Schwer\u00b7mut", "mich", "be\u00b7dr\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn h\u00f6her in die Berge, weiter", "tokens": ["Denn", "h\u00f6\u00b7her", "in", "die", "Ber\u00b7ge", ",", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "$,", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit unsern Tagen zieht er hin,", "tokens": ["Mit", "un\u00b7sern", "Ta\u00b7gen", "zieht", "er", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit diesem Himmel, der so heiter", "tokens": ["Mit", "die\u00b7sem", "Him\u00b7mel", ",", "der", "so", "hei\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf unsre Sommermorgen schien.", "tokens": ["Auf", "uns\u00b7re", "Som\u00b7mer\u00b7mor\u00b7gen", "schien", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zuweilen knallt vom Weingel\u00e4nde", "tokens": ["Zu\u00b7wei\u00b7len", "knallt", "vom", "Wein\u00b7ge\u00b7l\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Schu\u00df ins Tal, ins stille Land;", "tokens": ["Ein", "Schu\u00df", "ins", "Tal", ",", "ins", "stil\u00b7le", "Land", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am Weiher m\u00e4hen Schnitterh\u00e4nde", "tokens": ["Am", "Wei\u00b7her", "m\u00e4\u00b7hen", "Schnit\u00b7ter\u00b7h\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das hohe Schilf im Ufersand.", "tokens": ["Das", "ho\u00b7he", "Schilf", "im", "U\u00b7fer\u00b7sand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Zuweilen unterbricht das Schweigen", "tokens": ["Zu\u00b7wei\u00b7len", "un\u00b7ter\u00b7bricht", "das", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein roter Apfel, der im Traum", "tokens": ["Ein", "ro\u00b7ter", "Ap\u00b7fel", ",", "der", "im", "Traum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Herunterkugelt aus den Zweigen,", "tokens": ["Her\u00b7un\u00b7ter\u00b7ku\u00b7gelt", "aus", "den", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und nach folgt welkes Laub vom Baum.", "tokens": ["Und", "nach", "folgt", "wel\u00b7kes", "Laub", "vom", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "VVFIN", "PWAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach, all das mahnt so sterbensm\u00fcde!", "tokens": ["Ach", ",", "all", "das", "mahnt", "so", "ster\u00b7bens\u00b7m\u00fc\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIAT", "PDS", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir ist, als ob man eine Welt", "tokens": ["Mir", "ist", ",", "als", "ob", "man", "ei\u00b7ne", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "PIS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Freuden auf die Wagen l\u00fcde,", "tokens": ["Von", "Freu\u00b7den", "auf", "die", "Wa\u00b7gen", "l\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die heimzu wenden aus dem Feld.", "tokens": ["Die", "heim\u00b7zu", "wen\u00b7den", "aus", "dem", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Getrost! Bald f\u00fcllen sich die Kelter,", "tokens": ["Ge\u00b7trost", "!", "Bald", "f\u00fcl\u00b7len", "sich", "die", "Kel\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Traube g\u00e4rt, man pre\u00dft den Wein,", "tokens": ["Die", "Trau\u00b7be", "g\u00e4rt", ",", "man", "pre\u00dft", "den", "Wein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dich und Manchen, der noch \u00e4lter,", "tokens": ["Und", "dich", "und", "Man\u00b7chen", ",", "der", "noch", "\u00e4l\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KON", "NN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "L\u00e4dt man zu vollen Bechern ein.", "tokens": ["L\u00e4dt", "man", "zu", "vol\u00b7len", "Be\u00b7chern", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}