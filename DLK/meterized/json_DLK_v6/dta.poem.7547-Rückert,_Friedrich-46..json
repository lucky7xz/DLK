{"dta.poem.7547": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "46.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nichts hast du schlecht gemacht, auch was du machtest schlecht,", "tokens": ["Nichts", "hast", "du", "schlecht", "ge\u00b7macht", ",", "auch", "was", "du", "mach\u00b7test", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADJD", "VVPP", "$,", "ADV", "PWS", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es half dir da\u00df du nur was andres machtest recht.", "tokens": ["Es", "half", "dir", "da\u00df", "du", "nur", "was", "and\u00b7res", "mach\u00b7test", "recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOUS", "PPER", "ADV", "PWS", "PIS", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du h\u00e4ttest nur vielleicht dem Unverstand verschweigen", "tokens": ["Du", "h\u00e4t\u00b7test", "nur", "viel\u00b7leicht", "dem", "Un\u00b7ver\u00b7stand", "ver\u00b7schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Eine sollen und allein das Andre zeigen.", "tokens": ["Das", "Ei\u00b7ne", "sol\u00b7len", "und", "al\u00b7lein", "das", "And\u00b7re", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "PIAT", "KON", "ADV", "ART", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Man sieht den Weg dich gehn, nicht blos am Ziel dich stehn,", "tokens": ["Man", "sieht", "den", "Weg", "dich", "gehn", ",", "nicht", "blos", "am", "Ziel", "dich", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "PPER", "VVINF", "$,", "PTKNEG", "ADV", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und immer lehrreich ist auch jenes anzusehn.", "tokens": ["Und", "im\u00b7mer", "lehr\u00b7reich", "ist", "auch", "je\u00b7nes", "an\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "ADV", "PDS", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}