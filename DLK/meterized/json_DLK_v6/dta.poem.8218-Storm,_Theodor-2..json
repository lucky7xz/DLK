{"dta.poem.8218": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df durch die Tannen bricht der Strahl;", "tokens": ["Da\u00df", "durch", "die", "Tan\u00b7nen", "bricht", "der", "Strahl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Grunde wachen die Elfen auf,", "tokens": ["Im", "Grun\u00b7de", "wa\u00b7chen", "die", "El\u00b7fen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Silberh\u00f6rnlein rufen durch's Thal.", "tokens": ["Die", "Sil\u00b7ber\u00b7h\u00f6rn\u00b7lein", "ru\u00b7fen", "durch's", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Zu Tanz, zu Tanz am Felsenhang,", "tokens": ["Zu", "Tanz", ",", "zu", "Tanz", "am", "Fel\u00b7sen\u00b7hang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am hellen Bach, im schwarzen Tann!", "tokens": ["Am", "hel\u00b7len", "Bach", ",", "im", "schwar\u00b7zen", "Tann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sch\u00f6n Jungfr\u00e4ulein, was wird dir bang?", "tokens": ["Sch\u00f6n", "Jung\u00b7fr\u00e4u\u00b7lein", ",", "was", "wird", "dir", "bang", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PWS", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wach auf, und schlag die Saiten an!", "tokens": ["Wach", "auf", ",", "und", "schlag", "die", "Sai\u00b7ten", "an", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6n Jungfr\u00e4ulein, die sitzt im Traum;", "tokens": ["Sch\u00f6n", "Jung\u00b7fr\u00e4u\u00b7lein", ",", "die", "sitzt", "im", "Traum", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tannk\u00f6nig tritt zu ihr herein,", "tokens": ["Tann\u00b7k\u00f6\u00b7nig", "tritt", "zu", "ihr", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und k\u00fc\u00dft ihr leis des Mundes Saum,", "tokens": ["Und", "k\u00fc\u00dft", "ihr", "leis", "des", "Mun\u00b7des", "Saum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nimmt vom Hals das G\u00fcldkettlein.", "tokens": ["Und", "nimmt", "vom", "Hals", "das", "G\u00fcld\u00b7ket\u00b7tlein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da schl\u00e4gt sie hell die Augen auf \u2014", "tokens": ["Da", "schl\u00e4gt", "sie", "hell", "die", "Au\u00b7gen", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was hilft ihr Weinen all und Flehn!", "tokens": ["Was", "hilft", "ihr", "Wei\u00b7nen", "all", "und", "Flehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "PIAT", "KON", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Tannk\u00f6nig, la\u00df mich ziehn nach Haus,", "tokens": ["Tann\u00b7k\u00f6\u00b7nig", ",", "la\u00df", "mich", "ziehn", "nach", "Haus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "La\u00df mich zu meinen Schwestern gehn.", "tokens": ["La\u00df", "mich", "zu", "mei\u00b7nen", "Schwes\u00b7tern", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "In meinem Walde fing ich dich,", "tokens": ["In", "mei\u00b7nem", "Wal\u00b7de", "fing", "ich", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tannk\u00f6nig spricht, so bist du mein!", "tokens": ["Tann\u00b7k\u00f6\u00b7nig", "spricht", ",", "so", "bist", "du", "mein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "PPOSAT", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Was hattest du die Me\u00df vers\u00e4umt?", "tokens": ["Was", "hat\u00b7test", "du", "die", "Me\u00df", "ver\u00b7s\u00e4umt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Komm mit, komm mit zum Elfenreihn! \u2014", "tokens": ["Komm", "mit", ",", "komm", "mit", "zum", "El\u00b7fen\u00b7reihn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "APPR", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Elf! Elf! das klingt so wunderlich", "tokens": ["Elf", "!", "Elf", "!", "das", "klingt", "so", "wun\u00b7der\u00b7lich"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "NE", "$.", "PDS", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Elf! Elf! mir graut vor dem Elfenreihn;", "tokens": ["Elf", "!", "Elf", "!", "mir", "graut", "vor", "dem", "El\u00b7fen\u00b7reihn", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die haben gewi\u00df kein Christenthum,", "tokens": ["Die", "ha\u00b7ben", "ge\u00b7wi\u00df", "kein", "Chris\u00b7ten\u00b7thum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "O la\u00df mich zu Vater und Mutter mein! \u2014", "tokens": ["O", "la\u00df", "mich", "zu", "Va\u00b7ter", "und", "Mut\u00b7ter", "mein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "PPOSAT", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Und denkst du an Vater und Mutter noch,", "tokens": ["Und", "denkst", "du", "an", "Va\u00b7ter", "und", "Mut\u00b7ter", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sitz aber hundert Jahre allein!", "tokens": ["Sitz", "a\u00b7ber", "hun\u00b7dert", "Jah\u00b7re", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "CARD", "NN", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Die Elfen ziehn zu Tanz, zu Tanz!", "tokens": ["Die", "El\u00b7fen", "ziehn", "zu", "Tanz", ",", "zu", "Tanz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er h\u00e4ngt ihr um das G\u00fcldkettlein.", "tokens": ["Er", "h\u00e4ngt", "ihr", "um", "das", "G\u00fcld\u00b7ket\u00b7tlein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}