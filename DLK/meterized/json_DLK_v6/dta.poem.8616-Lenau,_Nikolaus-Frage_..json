{"dta.poem.8616": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Frage .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Mir hat noch deine Stimme nicht geklungen,", "tokens": ["Mir", "hat", "noch", "dei\u00b7ne", "Stim\u00b7me", "nicht", "ge\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich sah nur erst dein holdes Angesicht;", "tokens": ["Ich", "sah", "nur", "erst", "dein", "hol\u00b7des", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch hat der Strom der Sch\u00f6nheit mich bezwungen,", "tokens": ["Doch", "hat", "der", "Strom", "der", "Sch\u00f6n\u00b7heit", "mich", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der hell von dir in meine Seele bricht.", "tokens": ["Der", "hell", "von", "dir", "in", "mei\u00b7ne", "See\u00b7le", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In's Tiefste ist er m\u00e4chtig mir gedrungen,", "tokens": ["In's", "Tiefs\u00b7te", "ist", "er", "m\u00e4ch\u00b7tig", "mir", "ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VAFIN", "PPER", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was dort bis nun gelebt, nun lebt es nicht,", "tokens": ["Was", "dort", "bis", "nun", "ge\u00b7lebt", ",", "nun", "lebt", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ADV", "VVPP", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "S\u00fc\u00df sterbend ward es von der Fluth verschlungen.", "tokens": ["S\u00fc\u00df", "ster\u00b7bend", "ward", "es", "von", "der", "Fluth", "ver\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das ist der Liebe himmlisches Gericht!", "tokens": ["Das", "ist", "der", "Lie\u00b7be", "himm\u00b7li\u00b7sches", "Ge\u00b7richt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O da\u00df mein k\u00fchnes Hoffen, banges Zagen,", "tokens": ["O", "da\u00df", "mein", "k\u00fch\u00b7nes", "Hof\u00b7fen", ",", "ban\u00b7ges", "Za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPOSAT", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein milder Spruch aus deinem Munde gr\u00fc\u00dfte!", "tokens": ["Ein", "mil\u00b7der", "Spruch", "aus", "dei\u00b7nem", "Mun\u00b7de", "gr\u00fc\u00df\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Wellen, die so laut mein Herz durchschlagen,", "tokens": ["Die", "Wel\u00b7len", ",", "die", "so", "laut", "mein", "Herz", "durch\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wohin doch werden sie die Seele tragen?", "tokens": ["Wo\u00b7hin", "doch", "wer\u00b7den", "sie", "die", "See\u00b7le", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An der Erh\u00f6rung Paradiesesk\u00fcste? \u2014", "tokens": ["An", "der", "Er\u00b7h\u00f6\u00b7rung", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7k\u00fcs\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In der Versto\u00dfung trauervolle W\u00fcste? \u2014", "tokens": ["In", "der", "Ver\u00b7sto\u00b7\u00dfung", "trau\u00b7er\u00b7vol\u00b7le", "W\u00fcs\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}