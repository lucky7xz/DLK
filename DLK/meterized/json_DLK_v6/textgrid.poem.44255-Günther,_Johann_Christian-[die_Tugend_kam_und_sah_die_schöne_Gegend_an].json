{"textgrid.poem.44255": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[die Tugend kam und sah die sch\u00f6ne Gegend an]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Tugend kam und sah die sch\u00f6ne Gegend an,", "tokens": ["Die", "Tu\u00b7gend", "kam", "und", "sah", "die", "sch\u00f6\u00b7ne", "Ge\u00b7gend", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hier, wo der Elben Strom das Kuckusufer nezet", "tokens": ["Hier", ",", "wo", "der", "El\u00b7ben", "Strom", "das", "Ku\u00b7cku\u00b7su\u00b7fer", "ne\u00b7zet"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und wo, was Kunst und Wiz und Lust erdencken kan,", "tokens": ["Und", "wo", ",", "was", "Kunst", "und", "Wiz", "und", "Lust", "er\u00b7den\u00b7cken", "kan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "$,", "PWS", "NN", "KON", "NN", "KON", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An Schl\u00f6\u00dfern, Fluth und Stein so Aug als Ohr erg\u00f6zet.", "tokens": ["An", "Schl\u00f6\u00b7\u00dfern", ",", "Fluth", "und", "Stein", "so", "Aug", "als", "Ohr", "er\u00b7g\u00f6\u00b7zet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "ADV", "NN", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie nahm den s\u00fc\u00dfen Schall des scharfen Jagdhorns ein,", "tokens": ["Sie", "nahm", "den", "s\u00fc\u00b7\u00dfen", "Schall", "des", "schar\u00b7fen", "Jagd\u00b7horns", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie sah Dianen selbst in unsrer Gr\u00e4fin scherzen,", "tokens": ["Sie", "sah", "Di\u00b7a\u00b7nen", "selbst", "in", "uns\u00b7rer", "Gr\u00e4\u00b7fin", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ja, was noch mehr, sie sah in unsres Grafen Herzen", "tokens": ["Ja", ",", "was", "noch", "mehr", ",", "sie", "sah", "in", "uns\u00b7res", "Gra\u00b7fen", "Her\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PRELS", "ADV", "ADV", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Warheit und das Recht zwo holde Schwestern seyn.", "tokens": ["Die", "War\u00b7heit", "und", "das", "Recht", "zwo", "hol\u00b7de", "Schwes\u00b7tern", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "CARD", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach, sprach sie, ist der Welt die guldne Zeit entflogen?", "tokens": ["Ach", ",", "sprach", "sie", ",", "ist", "der", "Welt", "die", "guld\u00b7ne", "Zeit", "ent\u00b7flo\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "$,", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "O nein, sie hat sich nur ins Kuckusbad gezogen.", "tokens": ["O", "nein", ",", "sie", "hat", "sich", "nur", "ins", "Ku\u00b7ckus\u00b7bad", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$,", "PPER", "VAFIN", "PRF", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}