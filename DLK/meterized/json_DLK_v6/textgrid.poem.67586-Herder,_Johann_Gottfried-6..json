{"textgrid.poem.67586": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein altes, blindes Weib lag krank;", "tokens": ["Ein", "al\u00b7tes", ",", "blin\u00b7des", "Weib", "lag", "krank", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Aerzte docterten sie lang',", "tokens": ["Die", "A\u00b7erz\u00b7te", "doc\u00b7ter\u00b7ten", "sie", "lang'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Und jeder nahm f\u00fcr jeden Gang", "tokens": ["Und", "je\u00b7der", "nahm", "f\u00fcr", "je\u00b7den", "Gang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein St\u00fcckchen Hausrath mit zum Dank.", "tokens": ["Ein", "St\u00fcck\u00b7chen", "Haus\u00b7rath", "mit", "zum", "Dank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So ging's ein' Weile hin und her;", "tokens": ["So", "ging's", "ein'", "Wei\u00b7le", "hin", "und", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Weib ward seh'nd, das Haus war leer.", "tokens": ["Das", "Weib", "ward", "seh'nd", ",", "das", "Haus", "war", "leer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbbezahlt uns nun f\u00fcr viele Kunst und M\u00fch!\u00ab", "tokens": ["\u00bb", "be\u00b7zahlt", "uns", "nun", "f\u00fcr", "vie\u00b7le", "Kunst", "und", "M\u00fch", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "\u00bbach!\u00ab sagte sie,", "tokens": ["\u00bb", "ach", "!", "\u00ab", "sag\u00b7te", "sie", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "ITJ", "$.", "$(", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "\u00bbtrotz meines neuen Angesichts,", "tokens": ["\u00bb", "trotz", "mei\u00b7nes", "neu\u00b7en", "An\u00b7ge\u00b7sichts", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ihr Herrn, seh' ich jetzt \u2013 nichts.\u00ab", "tokens": ["Ihr", "Herrn", ",", "seh'", "ich", "jetzt", "\u2013", "nichts", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "ADV", "$(", "PIS", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der alte, blinde Mensch liegt krank,", "tokens": ["Der", "al\u00b7te", ",", "blin\u00b7de", "Mensch", "liegt", "krank", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Herren doctert ihn so lang'", "tokens": ["Ihr", "Her\u00b7ren", "doc\u00b7tert", "ihn", "so", "lang'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Syllogismus-Arzenei,", "tokens": ["Mit", "Syl\u00b7lo\u00b7gis\u00b7mus\u00b7A\u00b7rze\u00b7nei", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Metaphysik, Politik bei,", "tokens": ["Me\u00b7ta\u00b7phy\u00b7sik", ",", "Po\u00b7li\u00b7tik", "bei", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und nehmt ihm allen Saft und Kraft,", "tokens": ["Und", "nehmt", "ihm", "al\u00b7len", "Saft", "und", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wo und wie er etwas schafft;", "tokens": ["Und", "wo", "und", "wie", "er", "et\u00b7was", "schafft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "KON", "PWAV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nun sieht er \u2013 Himmel, ei! \u2013", "tokens": ["Nun", "sieht", "er", "\u2013", "Him\u00b7mel", ",", "ei", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "NN", "$,", "ITJ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Kraft Euers neuen Angesichts,", "tokens": ["Kraft", "Eu\u00b7ers", "neu\u00b7en", "An\u00b7ge\u00b7sichts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ihr Herrn, sieht er nun \u2013 nichts!", "tokens": ["Ihr", "Herrn", ",", "sieht", "er", "nun", "\u2013", "nichts", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "ADV", "$(", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}