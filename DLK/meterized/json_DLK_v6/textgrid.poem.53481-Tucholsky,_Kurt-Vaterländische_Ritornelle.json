{"textgrid.poem.53481": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Vaterl\u00e4ndische Ritornelle", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer nimmt es mit mir auf in Ritornellen?", "tokens": ["Wer", "nimmt", "es", "mit", "mir", "auf", "in", "Ri\u00b7tor\u00b7nel\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "PPER", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Vorrat hab ich noch sechs Pferdelasten.", "tokens": ["Im", "Vor\u00b7rat", "hab", "ich", "noch", "sechs", "Pfer\u00b7de\u00b7las\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADV", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer sch\u00f6nere wei\u00df als ich, der mag sich stellen.", "tokens": ["Wer", "sch\u00f6\u00b7ne\u00b7re", "wei\u00df", "als", "ich", ",", "der", "mag", "sich", "stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "VVFIN", "KOUS", "PPER", "$,", "PRELS", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Du bunter Blumenstrau\u00df!", "tokens": ["Du", "bun\u00b7ter", "Blu\u00b7men\u00b7strau\u00df", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hier, Leser, steck die Nase in die Pflanzen,", "tokens": ["Hier", ",", "Le\u00b7ser", ",", "steck", "die", "Na\u00b7se", "in", "die", "Pflan\u00b7zen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "beriech sie, und die sch\u00f6nsten such heraus!", "tokens": ["be\u00b7riech", "sie", ",", "und", "die", "sch\u00f6ns\u00b7ten", "such", "he\u00b7raus", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Bl\u00fchende Geranien!", "tokens": ["Bl\u00fc\u00b7hen\u00b7de", "Ge\u00b7ra\u00b7ni\u00b7en", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Ihr seid wohlfeil und ein billiger Schmuck", "tokens": ["Ihr", "seid", "wohl\u00b7feil", "und", "ein", "bil\u00b7li\u00b7ger", "Schmuck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "wie K\u00f6nigsthrone in dem Land Albanien.", "tokens": ["wie", "K\u00f6\u00b7nigs\u00b7thro\u00b7ne", "in", "dem", "Land", "Al\u00b7ba\u00b7ni\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Bescheidenes Veilchen!", "tokens": ["Be\u00b7schei\u00b7de\u00b7nes", "Veil\u00b7chen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und wenn du denkst, ein neues Wahlrecht kommt \u2013", "tokens": ["Und", "wenn", "du", "denkst", ",", "ein", "neu\u00b7es", "Wahl\u00b7recht", "kommt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "wir sind in Preu\u00dfen . . . warte noch ein Weilchen!", "tokens": ["wir", "sind", "in", "Preu\u00b7\u00dfen", ".", ".", ".", "war\u00b7te", "noch", "ein", "Weil\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NE", "$.", "$.", "$.", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Jel\u00e4ngerjelieber!", "tokens": ["Je\u00b7l\u00e4n\u00b7ger\u00b7je\u00b7lie\u00b7ber", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ja, \u00fcber unsern Kanzler und den Gardefl\u00fcgelmann \u2013", "tokens": ["Ja", ",", "\u00fc\u00b7ber", "un\u00b7sern", "Kanz\u00b7ler", "und", "den", "Gar\u00b7de\u00b7fl\u00fc\u00b7gel\u00b7mann", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPR", "PPOSAT", "NN", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "da geht nichts drieber.", "tokens": ["da", "geht", "nichts", "drie\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Ihr Rosen, Tulpen und Narzissen!", "tokens": ["Ihr", "Ro\u00b7sen", ",", "Tul\u00b7pen", "und", "Nar\u00b7zis\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Hitze lie\u00df uns auf der Wiese rasten . . .", "tokens": ["Die", "Hit\u00b7ze", "lie\u00df", "uns", "auf", "der", "Wie\u00b7se", "ras\u00b7ten", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$.", "$.", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dort \u00fcben die Soldaten . . . Horch, wer ruft?", "tokens": ["Dort", "\u00fc\u00b7ben", "die", "Sol\u00b7da\u00b7ten", ".", ".", ".", "Horch", ",", "wer", "ruft", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$.", "$.", "NE", "$,", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbeinj\u00e4hriger Rosenbaum, drei Tage Kasten!\u00ab", "tokens": ["\u00bb", "ein\u00b7j\u00e4h\u00b7ri\u00b7ger", "Ro\u00b7sen\u00b7baum", ",", "drei", "Ta\u00b7ge", "Kas\u00b7ten", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "CARD", "NN", "NN", "$.", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Du welkes Blatt!", "tokens": ["Du", "wel\u00b7kes", "Blatt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wenn du im trocknen Laube raschelst, mu\u00df ich denken,", "tokens": ["Wenn", "du", "im", "trock\u00b7nen", "Lau\u00b7be", "ra\u00b7schelst", ",", "mu\u00df", "ich", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da\u00df unser Kanzler was geredet hat.", "tokens": ["da\u00df", "un\u00b7ser", "Kanz\u00b7ler", "was", "ge\u00b7re\u00b7det", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PWS", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "S\u00fc\u00df duftende Banane!", "tokens": ["S\u00fc\u00df", "duf\u00b7ten\u00b7de", "Ba\u00b7na\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der S\u00e4ugling heult. Die Misses legt ihn trocken.", "tokens": ["Der", "S\u00e4ug\u00b7ling", "heult", ".", "Die", "Mis\u00b7ses", "legt", "ihn", "tro\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als Windel dient die Votes-for-womens-Fahne.", "tokens": ["Als", "Win\u00b7del", "dient", "die", "Vo\u00b7tes\u00b7for\u00b7wo\u00b7mens\u00b7Fah\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "Vaterl\u00e4ndisches Gartenland!", "tokens": ["Va\u00b7ter\u00b7l\u00e4n\u00b7di\u00b7sches", "Gar\u00b7ten\u00b7land", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Ein fetter Humus, doch was w\u00e4chst, ist ohne Reiz.", "tokens": ["Ein", "fet\u00b7ter", "Hu\u00b7mus", ",", "doch", "was", "w\u00e4chst", ",", "ist", "oh\u00b7ne", "Reiz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "PWS", "VVFIN", "$,", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Fehlt wohl des guten G\u00e4rtners leichte Hand?", "tokens": ["Fehlt", "wohl", "des", "gu\u00b7ten", "G\u00e4rt\u00b7ners", "leich\u00b7te", "Hand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da lohnte sich, es besser zu begie\u00dfen", "tokens": ["Da", "lohn\u00b7te", "sich", ",", "es", "bes\u00b7ser", "zu", "be\u00b7gie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "$,", "PPER", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "(mit Spucke nicht, mit Wasser!) \u2013 dann gedeihts.", "tokens": ["(", "mit", "Spu\u00b7cke", "nicht", ",", "mit", "Was\u00b7ser", "!", ")", "\u2013", "dann", "ge\u00b7deihts", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "PTKNEG", "$,", "APPR", "NN", "$.", "$(", "$(", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und tausend sch\u00f6nre Blumen werden darauf sprie\u00dfen!", "tokens": ["Und", "tau\u00b7send", "sch\u00f6n\u00b7re", "Blu\u00b7men", "wer\u00b7den", "da\u00b7rauf", "sprie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "ADJA", "NN", "VAFIN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}