{"dta.poem.5259": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht weniger gefiel auch mir", "tokens": ["Nicht", "we\u00b7ni\u00b7ger", "ge\u00b7fiel", "auch", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VVFIN", "ADV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Buchs-Baums r\u00f6thlich-gr\u00fcne Zier,", "tokens": ["Des", "Buchs\u00b7Baums", "r\u00f6\u00b7th\u00b7lich\u00b7gr\u00fc\u00b7ne", "Zier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Auf welchem, da ein jedes Blatt,", "tokens": ["Auf", "wel\u00b7chem", ",", "da", "ein", "je\u00b7des", "Blatt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "$,", "KOUS", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Recht wie Orangen-Laub, fast Spiegel-glatt,", "tokens": ["Recht", "wie", "O\u00b7ran\u00b7gen\u00b7Laub", ",", "fast", "Spie\u00b7gel\u00b7glatt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der heitern Sonne helles Licht", "tokens": ["Der", "hei\u00b7tern", "Son\u00b7ne", "hel\u00b7les", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schnell r\u00fcckwerts strahlt und sich so lieblich bricht,", "tokens": ["Schnell", "r\u00fcck\u00b7werts", "strahlt", "und", "sich", "so", "lieb\u00b7lich", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "KON", "PRF", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df, mit der gr\u00fcnen Pracht, an manchem Ort, vereint,", "tokens": ["Da\u00df", ",", "mit", "der", "gr\u00fc\u00b7nen", "Pracht", ",", "an", "man\u00b7chem", "Ort", ",", "ver\u00b7eint", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "ART", "ADJA", "NN", "$,", "APPR", "PIAT", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das Kraut, an manchem Ort, versilbert scheint.", "tokens": ["Das", "Kraut", ",", "an", "man\u00b7chem", "Ort", ",", "ver\u00b7sil\u00b7bert", "scheint", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PIAT", "NN", "$,", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ein lieblich-bitter-s\u00fcsser Duft", "tokens": ["Ein", "lieb\u00b7lich\u00b7bit\u00b7ter\u00b7s\u00fcs\u00b7ser", "Duft"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Aus diesem Kraut erf\u00fcllt umher die Luft.", "tokens": ["Aus", "die\u00b7sem", "Kraut", "er\u00b7f\u00fcllt", "um\u00b7her", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PTKVZ", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "So gar durch die Figur, worin man es gesetzet,", "tokens": ["So", "gar", "durch", "die", "Fi\u00b7gur", ",", "wo\u00b7rin", "man", "es", "ge\u00b7set\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,", "PWAV", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da es bald als ein runder Krantz formirt,", "tokens": ["Da", "es", "bald", "als", "ein", "run\u00b7der", "Krantz", "for\u00b7mirt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "KOKOM", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Bald als ein Rahmen, der geviert,", "tokens": ["Bald", "als", "ein", "Rah\u00b7men", ",", "der", "ge\u00b7viert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "In netter Symmetrie die Garten-Beeten ziert,", "tokens": ["In", "net\u00b7ter", "Sym\u00b7me\u00b7trie", "die", "Gar\u00b7ten\u00b7Bee\u00b7ten", "ziert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wird unser Aug\u2019 ergetzet.", "tokens": ["Wird", "un\u00b7ser", "Aug'", "er\u00b7get\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Di\u00df alles nahm mit einem neuen Schein", "tokens": ["Di\u00df", "al\u00b7les", "nahm", "mit", "ei\u00b7nem", "neu\u00b7en", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PIS", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Das innerste der Seelen ein.", "tokens": ["Das", "in\u00b7ners\u00b7te", "der", "See\u00b7len", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.18": {"text": "Die Hofnung, da\u00df der Winter bald vorbey,", "tokens": ["Die", "Hof\u00b7nung", ",", "da\u00df", "der", "Win\u00b7ter", "bald", "vor\u00b7bey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Der laue Fr\u00fchling nahe, sey,", "tokens": ["Der", "lau\u00b7e", "Fr\u00fch\u00b7ling", "na\u00b7he", ",", "sey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Erf\u00fcllt mich zum voraus mit k\u00fcnftigem Vergn\u00fcgen.", "tokens": ["Er\u00b7f\u00fcllt", "mich", "zum", "vo\u00b7raus", "mit", "k\u00fcnf\u00b7ti\u00b7gem", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Aus der so oft gesehnen Lentzen-Zier", "tokens": ["Aus", "der", "so", "oft", "ge\u00b7seh\u00b7nen", "Lent\u00b7zen\u00b7Zier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Stell\u2019 ich mir die zuk\u00fcnftge Sch\u00f6nheit f\u00fcr.", "tokens": ["Stell'", "ich", "mir", "die", "zu\u00b7k\u00fcnft\u00b7ge", "Sch\u00f6n\u00b7heit", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ART", "ADJA", "NN", "APPR", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Die Seele scheint sich zu bem\u00fchn,", "tokens": ["Die", "See\u00b7le", "scheint", "sich", "zu", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Durch die Erinnerung herbey zu ziehn", "tokens": ["Durch", "die", "E\u00b7rin\u00b7ne\u00b7rung", "her\u00b7bey", "zu", "ziehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "PTKZU", "VVINF"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.25": {"text": "Und sonder Gegenwart zu f\u00fcgen,", "tokens": ["Und", "son\u00b7der", "Ge\u00b7gen\u00b7wart", "zu", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Das was noch nicht, mit dem was nicht mehr, ist.", "tokens": ["Das", "was", "noch", "nicht", ",", "mit", "dem", "was", "nicht", "mehr", ",", "ist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "PRELS", "ADV", "PTKNEG", "$,", "APPR", "ART", "PRELS", "PTKNEG", "ADV", "$,", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}