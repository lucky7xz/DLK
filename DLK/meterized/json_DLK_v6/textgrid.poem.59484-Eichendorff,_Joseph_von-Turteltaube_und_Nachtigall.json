{"textgrid.poem.59484": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Turteltaube und Nachtigall", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "B\u00e4chlein, das so k\u00fchle rauschet,", "tokens": ["B\u00e4ch\u00b7lein", ",", "das", "so", "k\u00fch\u00b7le", "rau\u00b7schet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00f6stest alle V\u00f6gelein,", "tokens": ["Tr\u00f6s\u00b7test", "al\u00b7le", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur das Turtelt\u00e4ubchen trauert,", "tokens": ["Nur", "das", "Tur\u00b7tel\u00b7t\u00e4ub\u00b7chen", "trau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil's verwitwet und allein.", "tokens": ["Weil's", "ver\u00b7wit\u00b7wet", "und", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nachtigallenm\u00e4nnchen drau\u00dfen", "tokens": ["Nach\u00b7ti\u00b7gal\u00b7len\u00b7m\u00e4nn\u00b7chen", "drau\u00b7\u00dfen"], "token_info": ["word", "word"], "pos": ["NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schmettert so verlockend drein:", "tokens": ["Schmet\u00b7tert", "so", "ver\u00b7lo\u00b7ckend", "drein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbmir vertraue, s\u00fc\u00dfe Fraue,", "tokens": ["\u00bb", "mir", "ver\u00b7trau\u00b7e", ",", "s\u00fc\u00b7\u00dfe", "Frau\u00b7e", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will dein Lieb, dein Liebster sein!\u00ab", "tokens": ["Will", "dein", "Lieb", ",", "dein", "Liebs\u00b7ter", "sein", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAINF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbb\u00f6ser, la\u00df die falschen Lieder!", "tokens": ["\u00bb", "b\u00f6\u00b7ser", ",", "la\u00df", "die", "fal\u00b7schen", "Lie\u00b7der", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "VVIMP", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruh auf keinem Zweig, der bl\u00fcht,", "tokens": ["Ruh", "auf", "kei\u00b7nem", "Zweig", ",", "der", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df auf keiner Au mich nieder,", "tokens": ["La\u00df", "auf", "kei\u00b7ner", "Au", "mich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PIAT", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die von sch\u00f6nen Blumen gl\u00fcht.", "tokens": ["Die", "von", "sch\u00f6\u00b7nen", "Blu\u00b7men", "gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wo ich finde eine Quelle", "tokens": ["Wo", "ich", "fin\u00b7de", "ei\u00b7ne", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Helle in dem gr\u00fcnen Haus,", "tokens": ["Hel\u00b7le", "in", "dem", "gr\u00fc\u00b7nen", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit dem Schnabel erst die Welle", "tokens": ["Mit", "dem", "Schna\u00b7bel", "erst", "die", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00fcb ich, eh ich trink daraus.", "tokens": ["Tr\u00fcb", "ich", ",", "eh", "ich", "trink", "da\u00b7raus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Einsam soll man mich begraben,", "tokens": ["Ein\u00b7sam", "soll", "man", "mich", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PIS", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df mich trauernd hier allein,", "tokens": ["La\u00df", "mich", "trau\u00b7ernd", "hier", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will nicht Trost, nicht Lust mehr haben,", "tokens": ["Will", "nicht", "Trost", ",", "nicht", "Lust", "mehr", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "NN", "$,", "PTKNEG", "NN", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht dein Weib, noch Liebchen sein!\u00ab", "tokens": ["Nicht", "dein", "Weib", ",", "noch", "Lieb\u00b7chen", "sein", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "$,", "ADV", "NN", "VAINF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}