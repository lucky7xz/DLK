{"dta.poem.9866": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Von der sommer-zeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Soll ich/ o sommer/ dich beschreiben?", "tokens": ["Soll", "ich", "/", "o", "som\u00b7mer", "/", "dich", "be\u00b7schrei\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$(", "FM", "ADV", "$(", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer f\u00fchlet deine st\u00e4rcke nicht/", "tokens": ["Wer", "f\u00fch\u00b7let", "dei\u00b7ne", "st\u00e4r\u00b7cke", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "VVFIN", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die offt den leib so hefftig sticht/", "tokens": ["Die", "offt", "den", "leib", "so", "heff\u00b7tig", "sticht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df er nicht mehr bedeckt kan bleiben?", "tokens": ["Da\u00df", "er", "nicht", "mehr", "be\u00b7deckt", "kan", "blei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vermehrt doch schon dein hoher prei\u00df", "tokens": ["Ver\u00b7mehrt", "doch", "schon", "dein", "ho\u00b7her", "prei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf beyden wangen meinen schwei\u00df.", "tokens": ["Auf", "bey\u00b7den", "wan\u00b7gen", "mei\u00b7nen", "schwei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du bist ein br\u00e4utigam der erden/", "tokens": ["Du", "bist", "ein", "br\u00e4u\u00b7ti\u00b7gam", "der", "er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NE", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der in der liebe sich erhitzt/", "tokens": ["Der", "in", "der", "lie\u00b7be", "sich", "er\u00b7hitzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "VVFIN", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Bey dem die braut entbl\u00f6sset sitzt/", "tokens": ["Bey", "dem", "die", "braut", "ent\u00b7bl\u00f6s\u00b7set", "sitzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "VVFIN", "NE", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wenn sie von dir will fruchtbar werden/", "tokens": ["Wenn", "sie", "von", "dir", "will", "frucht\u00b7bar", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VMFIN", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Weil ihre kinder nicht dein brod", "tokens": ["Weil", "ih\u00b7re", "kin\u00b7der", "nicht", "dein", "brod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Umkommen l\u00e4st in hungers-noth.", "tokens": ["Um\u00b7kom\u00b7men", "l\u00e4st", "in", "hun\u00b7ger\u00b7snoth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Du giebst in allerl\u00e4ngsten tagen", "tokens": ["Du", "giebst", "in", "al\u00b7ler\u00b7l\u00e4ngs\u00b7ten", "ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Der arbeit ihr gew\u00fcnschtes licht/", "tokens": ["Der", "ar\u00b7beit", "ihr", "ge\u00b7w\u00fcnschtes", "licht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Erfreuest aller angesicht", "tokens": ["Er\u00b7freu\u00b7est", "al\u00b7ler", "an\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "An dem/ was b\u00e4um und \u00e4cker tragen/", "tokens": ["An", "dem", "/", "was", "b\u00e4um", "und", "\u00e4c\u00b7ker", "tra\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$(", "PWS", "NE", "KON", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "L\u00e4st nicht der bauren h\u00e4user leer/", "tokens": ["L\u00e4st", "nicht", "der", "bau\u00b7ren", "h\u00e4u\u00b7ser", "leer", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Ernehrst der thiere grosses heer.", "tokens": ["Er\u00b7nehrst", "der", "thie\u00b7re", "gros\u00b7ses", "heer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Dein heu speist rinder/ schafe/ pferde/", "tokens": ["Dein", "heu", "speist", "rin\u00b7der", "/", "scha\u00b7fe", "/", "pfer\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$(", "VVFIN", "$(", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Wenn sich der winter eingestellt/", "tokens": ["Wenn", "sich", "der", "win\u00b7ter", "ein\u00b7ge\u00b7stellt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Der/ was uns giebt dein wald und feld/", "tokens": ["Der", "/", "was", "uns", "giebt", "dein", "wald", "und", "feld", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PWS", "PPER", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Tr\u00e4gt auf den tisch von seinem heerde/", "tokens": ["Tr\u00e4gt", "auf", "den", "tisch", "von", "sei\u00b7nem", "heer\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Weil ihm sein thun nichts leget ein.", "tokens": ["Weil", "ihm", "sein", "thun", "nichts", "le\u00b7get", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAINF", "VVFIN", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.24": {"text": "Kan deines reichthums erbe seyn.", "tokens": ["Kan", "dei\u00b7nes", "reicht\u00b7hums", "er\u00b7be", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Doch m\u00fcssen dich viel ohren hassen/", "tokens": ["Doch", "m\u00fcs\u00b7sen", "dich", "viel", "oh\u00b7ren", "has\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Wenn dein gewitter blitzt und schl\u00e4gt/", "tokens": ["Wenn", "dein", "ge\u00b7wit\u00b7ter", "blitzt", "und", "schl\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Die furcht in zarte hertzen pr\u00e4gt/", "tokens": ["Die", "furcht", "in", "zar\u00b7te", "hert\u00b7zen", "pr\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Niemanden will in ruhe lassen.", "tokens": ["Nie\u00b7man\u00b7den", "will", "in", "ru\u00b7he", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Denn h\u00f6ret man des h\u00f6chsten stimm:", "tokens": ["Denn", "h\u00f6\u00b7ret", "man", "des", "h\u00f6chs\u00b7ten", "stimm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ART", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Jhr s\u00fcnder/ f\u00fcrchtet meinen grimm.", "tokens": ["Ihr", "s\u00fcn\u00b7der", "/", "f\u00fcrch\u00b7tet", "mei\u00b7nen", "grimm", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$(", "VVFIN", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}