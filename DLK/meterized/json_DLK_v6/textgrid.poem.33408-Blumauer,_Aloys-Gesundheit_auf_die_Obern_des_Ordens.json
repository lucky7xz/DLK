{"textgrid.poem.33408": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Gesundheit auf die Obern des Ordens", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Edlen, die in einen Bund", "tokens": ["Den", "Ed\u00b7len", ",", "die", "in", "ei\u00b7nen", "Bund"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Uns all' zusammen schlingen,", "tokens": ["Uns", "all'", "zu\u00b7sam\u00b7men", "schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00dft uns anheut' aus Einem Mund", "tokens": ["La\u00dft", "uns", "an\u00b7heut'", "aus", "Ei\u00b7nem", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Dankesopfer bringen!", "tokens": ["Ein", "Dan\u00b7ke\u00b7sop\u00b7fer", "brin\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie lieben keine Schmeichelei'n,", "tokens": ["Sie", "lie\u00b7ben", "kei\u00b7ne", "Schmei\u00b7che\u00b7lei'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Womit man F\u00fcrsten fr\u00f6hnet:", "tokens": ["Wo\u00b7mit", "man", "F\u00fcrs\u00b7ten", "fr\u00f6h\u00b7net", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie lieben Wahrheit nur, die rein", "tokens": ["Sie", "lie\u00b7ben", "Wahr\u00b7heit", "nur", ",", "die", "rein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJA", "NN", "ADV", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus freiem Munde t\u00f6net.", "tokens": ["Aus", "frei\u00b7em", "Mun\u00b7de", "t\u00f6\u00b7net", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr Reich ist nicht der Freiheit Grab,", "tokens": ["Ihr", "Reich", "ist", "nicht", "der", "Frei\u00b7heit", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Wink kein Wink vom Throne;", "tokens": ["Ihr", "Wink", "kein", "Wink", "vom", "Thro\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Scepter ist der Friedensstab,", "tokens": ["Ihr", "Scep\u00b7ter", "ist", "der", "Frie\u00b7dens\u00b7stab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Weisheit ihre Krone.", "tokens": ["Und", "Weis\u00b7heit", "ih\u00b7re", "Kro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie haben keinen Unterthan;", "tokens": ["Sie", "ha\u00b7ben", "kei\u00b7nen", "Un\u00b7ter\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir heissen ihnen \u2013 Br\u00fcder:", "tokens": ["Wir", "heis\u00b7sen", "ih\u00b7nen", "\u2013", "Br\u00fc\u00b7der", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und so nennt sie der freie Mann", "tokens": ["Und", "so", "nennt", "sie", "der", "frei\u00b7e", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und freie Maurer wieder.", "tokens": ["Und", "frei\u00b7e", "Mau\u00b7rer", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie herrschen nur durch Lieb' und Rath:", "tokens": ["Sie", "herr\u00b7schen", "nur", "durch", "Lieb'", "und", "Rath", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Glaub' ist keine Bibel,", "tokens": ["Ihr", "Glaub'", "ist", "kei\u00b7ne", "Bi\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Wink kein k\u00f6niglich Mandat,", "tokens": ["Ihr", "Wink", "kein", "k\u00f6\u00b7nig\u00b7lich", "Man\u00b7dat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PIAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Wort nicht infallibel.", "tokens": ["Ihr", "Wort", "nicht", "in\u00b7fal\u00b7li\u00b7bel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sie lenken als der weisere", "tokens": ["Sie", "len\u00b7ken", "als", "der", "wei\u00b7se\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "KOKOM", "ART", "ADJA"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und bess're Theil den Orden!", "tokens": ["Und", "bess'\u00b7re", "Theil", "den", "Or\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Heil uns, da\u00df sie nicht K\u00f6nige,", "tokens": ["Heil", "uns", ",", "da\u00df", "sie", "nicht", "K\u00f6\u00b7ni\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Noch P\u00e4pste sind geworden!", "tokens": ["Noch", "P\u00e4ps\u00b7te", "sind", "ge\u00b7wor\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}