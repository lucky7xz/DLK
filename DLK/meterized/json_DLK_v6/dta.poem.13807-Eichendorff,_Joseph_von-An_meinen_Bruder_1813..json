{"dta.poem.13807": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An meinen Bruder 1813.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Mein lieber Herzensbruder!", "tokens": ["Mein", "lie\u00b7ber", "Her\u00b7zens\u00b7bru\u00b7der", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Still war der Morgen \u2014 ", "tokens": ["Still", "war", "der", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wie war die Welt voll Freude!", "tokens": ["Wie", "war", "die", "Welt", "voll", "Freu\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du fa\u00dftest ritterlich das schwanke Ruder,", "tokens": ["Du", "fa\u00df\u00b7test", "rit\u00b7ter\u00b7lich", "das", "schwan\u00b7ke", "Ru\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Uns beide treulich lenkend,", "tokens": ["Uns", "bei\u00b7de", "treu\u00b7lich", "len\u00b7kend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auf froher Fahrt nur einen Stern bedenkend.", "tokens": ["Auf", "fro\u00b7her", "Fahrt", "nur", "ei\u00b7nen", "Stern", "be\u00b7den\u00b7kend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}