{"dta.poem.23359": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Kuhschnappelsche Thorsperre.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Einen Leibzoll zu entrichten", "tokens": ["Ei\u00b7nen", "Leib\u00b7zoll", "zu", "ent\u00b7rich\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcr das Vieh, mag menschlich sein:", "tokens": ["F\u00fcr", "das", "Vieh", ",", "mag", "menschlich", "sein", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "VMFIN", "ADJD", "VAINF", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ochsen d\u00fcrfen doch mit Nichten", "tokens": ["Och\u00b7sen", "d\u00fcr\u00b7fen", "doch", "mit", "Nich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ungestraft zur Stadt hinein!", "tokens": ["Un\u00b7ge\u00b7straft", "zur", "Stadt", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch da\u00df man den Ochsen gleich gilt,", "tokens": ["Doch", "da\u00df", "man", "den", "Och\u00b7sen", "gleich", "gilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Ochsig zahlen mu\u00df und soll,", "tokens": ["Och\u00b7sig", "zah\u00b7len", "mu\u00df", "und", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VMFIN", "KON", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn man kommt zu sp\u00e4t ins Weichbild \u2014", "tokens": ["Wenn", "man", "kommt", "zu", "sp\u00e4t", "ins", "Weich\u00b7bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "PTKA", "ADJD", "APPRART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bestialisch ist ", "tokens": ["Bes\u00b7ti\u00b7a\u00b7lisch", "ist"], "token_info": ["word", "word"], "pos": ["ADJD", "VAFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}