{"dta.poem.4214": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ermunterung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ermuntre dich, Seele! beschaue, betrachte, wie helle", "tokens": ["Er\u00b7munt\u00b7re", "dich", ",", "See\u00b7le", "!", "be\u00b7schau\u00b7e", ",", "be\u00b7trach\u00b7te", ",", "wie", "hel\u00b7le"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "NN", "$.", "VVFIN", "$,", "VVFIN", "$,", "PWAV", "ADJA"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.2": {"text": "der Himmel, wie herrlich die Welt!", "tokens": ["der", "Him\u00b7mel", ",", "wie", "herr\u00b7lich", "die", "Welt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADJD", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Bewundre die Allmacht, die Weisheit, die Liebe, die alles", "tokens": ["Be\u00b7wund\u00b7re", "die", "All\u00b7macht", ",", "die", "Weis\u00b7heit", ",", "die", "Lie\u00b7be", ",", "die", "al\u00b7les"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "PIS"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.4": {"text": "so richtig in Ordnung erh\u00e4lt,", "tokens": ["so", "rich\u00b7tig", "in", "Ord\u00b7nung", "er\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Der neben den Wundern das herrliche Licht,", "tokens": ["Der", "ne\u00b7ben", "den", "Wun\u00b7dern", "das", "herr\u00b7li\u00b7che", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "----+--+--+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Auch neben demselben das edle Gesicht,", "tokens": ["Auch", "ne\u00b7ben", "dem\u00b7sel\u00b7ben", "das", "ed\u00b7le", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Und endlich der Seelen die Kr\u00e4fte zu denken", "tokens": ["Und", "end\u00b7lich", "der", "See\u00b7len", "die", "Kr\u00e4f\u00b7te", "zu", "den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "In ihrem Vergn\u00fcgen Jhn Selber zu ehren, den Menschen", "tokens": ["In", "ih\u00b7rem", "Ver\u00b7gn\u00fc\u00b7gen", "Jhn", "Sel\u00b7ber", "zu", "eh\u00b7ren", ",", "den", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "NN", "PTKZU", "VVINF", "$,", "ART", "NN"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.9": {"text": "aus Gnaden gew\u00fcrdigt zu schenken.", "tokens": ["aus", "Gna\u00b7den", "ge\u00b7w\u00fcr\u00b7digt", "zu", "schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "Ergetze dich! such Jhm ein Opfer zu bringen!", "tokens": ["Er\u00b7get\u00b7ze", "dich", "!", "such", "Jhm", "ein", "Op\u00b7fer", "zu", "brin\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+++-+--+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Komm, la\u00df uns in Seinen so pr\u00e4chtigen Werken ", "tokens": ["Komm", ",", "la\u00df", "uns", "in", "Sei\u00b7nen", "so", "pr\u00e4ch\u00b7ti\u00b7gen", "Wer\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVIMP", "PPER", "APPR", "PPOSAT", "ADV", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.12": {"text": "Allmacht erheben, die Liebe besingen!", "tokens": ["All\u00b7macht", "er\u00b7he\u00b7ben", ",", "die", "Lie\u00b7be", "be\u00b7sin\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.14": {"text": "Die Kraft der Betrachtung best\u00e4ndig vermehren!", "tokens": ["Die", "Kraft", "der", "Be\u00b7trach\u00b7tung", "be\u00b7st\u00e4n\u00b7dig", "ver\u00b7meh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}