{"textgrid.poem.57111": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Verzicht, das ist der Wein, das ist die Waffe.", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verzicht, das ist der Wein, das ist die Waffe.", "tokens": ["Ver\u00b7zicht", ",", "das", "ist", "der", "Wein", ",", "das", "ist", "die", "Waf\u00b7fe", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VAFIN", "ART", "NN", "$,", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von diesem Safte wirst du stark und still.", "tokens": ["Von", "die\u00b7sem", "Saf\u00b7te", "wirst", "du", "stark", "und", "still", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wenn dein Wunsch sich nicht ergeben will,", "tokens": ["Und", "wenn", "dein", "Wunsch", "sich", "nicht", "er\u00b7ge\u00b7ben", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "PRF", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "sieh zu, da\u00df dir dies Schwert den Frieden schaffe.", "tokens": ["sieh", "zu", ",", "da\u00df", "dir", "dies", "Schwert", "den", "Frie\u00b7den", "schaf\u00b7fe", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "KOUS", "PPER", "PDS", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mit diesem Wein im Kruge lebst du gut.", "tokens": ["Mit", "die\u00b7sem", "Wein", "im", "Kru\u00b7ge", "lebst", "du", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPRART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit dieser Waffe wirst du m\u00e4chtig sein.", "tokens": ["Mit", "die\u00b7ser", "Waf\u00b7fe", "wirst", "du", "m\u00e4ch\u00b7tig", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verzicht \u2013 so sticht ein Stahl ins Herz hinein.", "tokens": ["Ver\u00b7zicht", "\u2013", "so", "sticht", "ein", "Stahl", "ins", "Herz", "hin\u00b7ein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verzicht \u2013 so l\u00f6st den Krampf der Rebe Blut.", "tokens": ["Ver\u00b7zicht", "\u2013", "so", "l\u00f6st", "den", "Krampf", "der", "Re\u00b7be", "Blut", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}