{"dta.poem.4301": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wunsch im Herbst zur Saat-Zeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott Lob! der Acker ist gepfl\u00fcgt, der Saame wieder", "tokens": ["Gott", "Lob", "!", "der", "A\u00b7cker", "ist", "ge\u00b7pfl\u00fcgt", ",", "der", "Saa\u00b7me", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$.", "ART", "NN", "VAFIN", "VVPP", "$,", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ausgestreut", "tokens": ["aus\u00b7ge\u00b7streut"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Bey einer guten Witterung, zu einer recht bequehmen Zeit.", "tokens": ["Bey", "ei\u00b7ner", "gu\u00b7ten", "Wit\u00b7te\u00b7rung", ",", "zu", "ei\u00b7ner", "recht", "be\u00b7queh\u00b7men", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Die Furchen sind, da man geeget,", "tokens": ["Die", "Fur\u00b7chen", "sind", ",", "da", "man", "gee\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "KOUS", "PIS", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Gef\u00fcllet wieder zugeleget,", "tokens": ["Ge\u00b7f\u00fcl\u00b7let", "wie\u00b7der", "zu\u00b7ge\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die sanfte Saat bedeckt, und unser Feld", "tokens": ["Die", "sanf\u00b7te", "Saat", "be\u00b7deckt", ",", "und", "un\u00b7ser", "Feld"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Aufs neue wiederum bestellt.", "tokens": ["Aufs", "neu\u00b7e", "wie\u00b7de\u00b7rum", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Man sieht, da es so zubereitet,", "tokens": ["Man", "sieht", ",", "da", "es", "so", "zu\u00b7be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die Fl\u00e4che sonder Lust nicht an,", "tokens": ["Die", "Fl\u00e4\u00b7che", "son\u00b7der", "Lust", "nicht", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wenn auf der braunen ebnen Bahn", "tokens": ["Wenn", "auf", "der", "brau\u00b7nen", "eb\u00b7nen", "Bahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Das Auge, sonder Ansto\u00df, gleitet.", "tokens": ["Das", "Au\u00b7ge", ",", "son\u00b7der", "An\u00b7sto\u00df", ",", "glei\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die Hoffnung f\u00e4ngt schon an zu bl\u00fch\u2019n,", "tokens": ["Die", "Hoff\u00b7nung", "f\u00e4ngt", "schon", "an", "zu", "bl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Aus der nunmehr beschlo\u00dfnen Arbeit den k\u00fcnftigen Ge-", "tokens": ["Aus", "der", "nun\u00b7mehr", "be\u00b7schlo\u00df\u00b7nen", "Ar\u00b7beit", "den", "k\u00fcnf\u00b7ti\u00b7gen", "Ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "nu\u00df zu zieh'n.", "tokens": ["nu\u00df", "zu", "zieh'", "n."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["ADV", "PTKZU", "VVFIN", "NE"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Da aber, HErr! an Deinem Segen", "tokens": ["Da", "a\u00b7ber", ",", "Herr", "!", "an", "Dei\u00b7nem", "Se\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "NN", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Es alles blo\u00df allein gelegen;", "tokens": ["Es", "al\u00b7les", "blo\u00df", "al\u00b7lein", "ge\u00b7le\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "So gieb, durch fr\u00fch- und spaten Regen,", "tokens": ["So", "gieb", ",", "durch", "fr\u00fch", "und", "spa\u00b7ten", "Re\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "$,", "APPR", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Durch Sonnenschein zu rechter Zeit,", "tokens": ["Durch", "Son\u00b7nen\u00b7schein", "zu", "rech\u00b7ter", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Zu der erw\u00fcnschten Fruchtbarkeit,", "tokens": ["Zu", "der", "er\u00b7w\u00fcnschten", "Frucht\u00b7bar\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Und reichem Wachsthum, Dein Gedeyen,", "tokens": ["Und", "rei\u00b7chem", "Wach\u00b7sthum", ",", "Dein", "Ge\u00b7de\u00b7yen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Da\u00df wir der reichen Erndt\u2019 uns freuen,", "tokens": ["Da\u00df", "wir", "der", "rei\u00b7chen", "Erndt'", "uns", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Und, mit erkenntlichem Gem\u00fcht, Dir herzlich daf\u00fcr dan-", "tokens": ["Und", ",", "mit", "er\u00b7kennt\u00b7li\u00b7chem", "Ge\u00b7m\u00fcht", ",", "Dir", "herz\u00b7lich", "da\u00b7f\u00fcr", "dan"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "APPR", "ADJA", "NN", "$,", "PPER", "ADJD", "PAV", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.23": {"text": "ken m\u00f6gen!", "tokens": ["ken", "m\u00f6\u00b7gen", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}