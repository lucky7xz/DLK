{"dta.poem.20651": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An                  \n  den einj\u00e4hrigen Wilhelm von K.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du kennst noch nicht den Regenbogen                 ", "tokens": ["Du", "kennst", "noch", "nicht", "den", "Re\u00b7gen\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und nicht die Sonnenstrahlen, Kind!", "tokens": ["Und", "nicht", "die", "Son\u00b7nen\u00b7strah\u00b7len", ",", "Kind", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein erstes Jahr ist hingeflogen,", "tokens": ["Dein", "ers\u00b7tes", "Jahr", "ist", "hin\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo Deiner V\u00e4ter Jahre sind;", "tokens": ["Wo", "Dei\u00b7ner", "V\u00e4\u00b7ter", "Jah\u00b7re", "sind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und niemals ward der sch\u00f6ne Morgen", "tokens": ["Und", "nie\u00b7mals", "ward", "der", "sch\u00f6\u00b7ne", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Dir bemerkt, von Dir gedacht,", "tokens": ["Von", "Dir", "be\u00b7merkt", ",", "von", "Dir", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn ihn Dein Herz gleich ohne Sorgen", "tokens": ["Wenn", "ihn", "Dein", "Herz", "gleich", "oh\u00b7ne", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus offnen Augen angelacht.", "tokens": ["Aus", "off\u00b7nen", "Au\u00b7gen", "an\u00b7ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die gro\u00dfe Kraft zu unterscheiden,", "tokens": ["Die", "gro\u00b7\u00dfe", "Kraft", "zu", "un\u00b7ter\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Liegt in der Seele noch verh\u00fcllt;", "tokens": ["Liegt", "in", "der", "See\u00b7le", "noch", "ver\u00b7h\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dennoch wird sie schon mit Freuden", "tokens": ["Und", "den\u00b7noch", "wird", "sie", "schon", "mit", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und auch mit Traurigkeit erf\u00fcllt.", "tokens": ["Und", "auch", "mit", "Trau\u00b7rig\u00b7keit", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein stiller Gram schw\u00e4cht Deine Blicke,", "tokens": ["Ein", "stil\u00b7ler", "Gram", "schw\u00e4cht", "Dei\u00b7ne", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und zieht von Deines Mundes Rand", "tokens": ["Und", "zieht", "von", "Dei\u00b7nes", "Mun\u00b7des", "Rand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein s\u00fc\u00dfes L\u00e4cheln oft zur\u00fccke", "tokens": ["Dein", "s\u00fc\u00b7\u00dfes", "L\u00e4\u00b7cheln", "oft", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ins Herz, das den Verlust empfand.", "tokens": ["Ins", "Herz", ",", "das", "den", "Ver\u00b7lust", "emp\u00b7fand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dem J\u00fcnglinge, dem unwillkommen", "tokens": ["Dem", "J\u00fcng\u00b7lin\u00b7ge", ",", "dem", "un\u00b7will\u00b7kom\u00b7men"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Der Tod sein liebes M\u00e4dchen nahm;", "tokens": ["Der", "Tod", "sein", "lie\u00b7bes", "M\u00e4d\u00b7chen", "nahm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Reichen ward nicht mehr genommen,", "tokens": ["Dem", "Rei\u00b7chen", "ward", "nicht", "mehr", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn in sein Haus ein R\u00e4uber kam,", "tokens": ["Wenn", "in", "sein", "Haus", "ein", "R\u00e4u\u00b7ber", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Als Dir in Deinem ersten Gl\u00fccke", "tokens": ["Als", "Dir", "in", "Dei\u00b7nem", "ers\u00b7ten", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In jener so geliebten Brust;", "tokens": ["In", "je\u00b7ner", "so", "ge\u00b7lieb\u00b7ten", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seitdem erstarb in Deinem Blicke", "tokens": ["Seit\u00b7dem", "er\u00b7starb", "in", "Dei\u00b7nem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Feuer und des Lachens Lust.", "tokens": ["Das", "Feu\u00b7er", "und", "des", "La\u00b7chens", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O holder angenehmer Knabe!", "tokens": ["O", "hol\u00b7der", "an\u00b7ge\u00b7neh\u00b7mer", "Kna\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch mancher Kummer wartet Dein", "tokens": ["Noch", "man\u00b7cher", "Kum\u00b7mer", "war\u00b7tet", "Dein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis Du gest\u00fctzt an einem Stabe", "tokens": ["Bis", "Du", "ge\u00b7st\u00fctzt", "an", "ei\u00b7nem", "Sta\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dich Deiner Nachwelt wirst erfreun.", "tokens": ["Dich", "Dei\u00b7ner", "Nach\u00b7welt", "wirst", "er\u00b7freun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Du lernest Welt und Menschen kennen,", "tokens": ["Du", "ler\u00b7nest", "Welt", "und", "Men\u00b7schen", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und seufzend wirst Du laut und schwer", "tokens": ["Und", "seuf\u00b7zend", "wirst", "Du", "laut", "und", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "VAFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Oft Welt und Menschen schrecklich nennen,", "tokens": ["Oft", "Welt", "und", "Men\u00b7schen", "schreck\u00b7lich", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn keine Tugend in ihr w\u00e4r;", "tokens": ["Wenn", "kei\u00b7ne", "Tu\u00b7gend", "in", "ihr", "w\u00e4r", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und \u00fcber ihr ein Gott nicht wohnte,", "tokens": ["Und", "\u00fc\u00b7ber", "ihr", "ein", "Gott", "nicht", "wohn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der seine Frommen kennt und liebt,", "tokens": ["Der", "sei\u00b7ne", "From\u00b7men", "kennt", "und", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und stille Tugenden belohnte,", "tokens": ["Und", "stil\u00b7le", "Tu\u00b7gen\u00b7den", "be\u00b7lohn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und nach dem Kummer Freuden giebt.", "tokens": ["Und", "nach", "dem", "Kum\u00b7mer", "Freu\u00b7den", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}