{"textgrid.poem.25194": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00dcber ru\u00dfbestaubten D\u00e4cherwogen,", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber ru\u00dfbestaubten D\u00e4cherwogen,", "tokens": ["\u00dc\u00b7ber", "ru\u00df\u00b7be\u00b7staub\u00b7ten", "D\u00e4\u00b7cher\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Stra\u00dfendunst und dumpfem Werkgetose,", "tokens": ["Stra\u00b7\u00dfen\u00b7dunst", "und", "dum\u00b7pfem", "Werk\u00b7ge\u00b7to\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "\u00dcber all dem bang beladnen Volke", "tokens": ["\u00dc\u00b7ber", "all", "dem", "bang", "be\u00b7lad\u00b7nen", "Vol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Schwebt die Wolke", "tokens": ["Schwebt", "die", "Wol\u00b7ke"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Blendend wei\u00df, wie eine Riesenwasserrose", "tokens": ["Blen\u00b7dend", "wei\u00df", ",", "wie", "ei\u00b7ne", "Rie\u00b7sen\u00b7was\u00b7ser\u00b7ro\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "\u00dcber schwarzem Kolke.", "tokens": ["\u00dc\u00b7ber", "schwar\u00b7zem", "Kol\u00b7ke", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Und hernieder blickt die Reine", "tokens": ["Und", "her\u00b7nie\u00b7der", "blickt", "die", "Rei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In den d\u00fcstern Hof, wo zwischen Mauern,", "tokens": ["In", "den", "d\u00fcs\u00b7tern", "Hof", ",", "wo", "zwi\u00b7schen", "Mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ungeliebt vom Sonnenscheine,", "tokens": ["Un\u00b7ge\u00b7liebt", "vom", "Son\u00b7nen\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein gebeugtes Weib die Jugend mu\u00df vertrauern", "tokens": ["Ein", "ge\u00b7beug\u00b7tes", "Weib", "die", "Ju\u00b7gend", "mu\u00df", "ver\u00b7trau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Bei der Nadel fieberhaftem Rasseln. \u2013", "tokens": ["Bei", "der", "Na\u00b7del", "fie\u00b7ber\u00b7haf\u00b7tem", "Ras\u00b7seln", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Blasses Weib, erhebe dein Gesicht", "tokens": ["Blas\u00b7ses", "Weib", ",", "er\u00b7he\u00b7be", "dein", "Ge\u00b7sicht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Zu der Wolke hehrem Licht!", "tokens": ["Zu", "der", "Wol\u00b7ke", "heh\u00b7rem", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und ihr Werkelm\u00e4nner arbeitshei\u00df,", "tokens": ["Und", "ihr", "Wer\u00b7kel\u00b7m\u00e4n\u00b7ner", "ar\u00b7beits\u00b7hei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "La\u00dft das H\u00e4mmern, la\u00dft des Schwungrads Treiben,", "tokens": ["La\u00dft", "das", "H\u00e4m\u00b7mern", ",", "la\u00dft", "des", "Schwun\u00b7grads", "Trei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$,", "VVIMP", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Tretet an die tr\u00fcben Werkstattscheiben,", "tokens": ["Tre\u00b7tet", "an", "die", "tr\u00fc\u00b7ben", "Werk\u00b7statt\u00b7schei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Trocknet von der Stirn den Schwei\u00df,", "tokens": ["Trock\u00b7net", "von", "der", "Stirn", "den", "Schwei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Andachtsvoll den Blick erhoben", "tokens": ["An\u00b7dachts\u00b7voll", "den", "Blick", "er\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zu der wei\u00dfen Wolke droben!", "tokens": ["Zu", "der", "wei\u00b7\u00dfen", "Wol\u00b7ke", "dro\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Alle, die durch graue Gassen", "tokens": ["Al\u00b7le", ",", "die", "durch", "grau\u00b7e", "Gas\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gr\u00fcbelnd hasten und einander hassen", "tokens": ["Gr\u00fc\u00b7belnd", "has\u00b7ten", "und", "ein\u00b7an\u00b7der", "has\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "KON", "PRF", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Um ein karges, hartes Brod,", "tokens": ["Um", "ein", "kar\u00b7ges", ",", "har\u00b7tes", "Brod", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die um armen Leibes Not", "tokens": ["Die", "um", "ar\u00b7men", "Lei\u00b7bes", "Not"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In das Morgen schaun mit Bangen,", "tokens": ["In", "das", "Mor\u00b7gen", "schaun", "mit", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die gebrochen und verlassen", "tokens": ["Die", "ge\u00b7bro\u00b7chen", "und", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "H\u00fcsteln mit geh\u00f6hlten Wangen,", "tokens": ["H\u00fcs\u00b7teln", "mit", "ge\u00b7h\u00f6hl\u00b7ten", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die den Tod verzweifelnd suchen,", "tokens": ["Die", "den", "Tod", "ver\u00b7zwei\u00b7felnd", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Oder hinter Eisenstangen", "tokens": ["O\u00b7der", "hin\u00b7ter", "Ei\u00b7sen\u00b7stan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Schmachtend fluchen, \u2013", "tokens": ["Schmach\u00b7tend", "flu\u00b7chen", ",", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJD", "VVINF", "$,", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "All die Fensteraugen jener langen", "tokens": ["All", "die", "Fens\u00b7ter\u00b7au\u00b7gen", "je\u00b7ner", "lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PDAT", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "H\u00e4userreihen sollen aufw\u00e4rts schauen", "tokens": ["H\u00e4u\u00b7ser\u00b7rei\u00b7hen", "sol\u00b7len", "auf\u00b7w\u00e4rts", "schau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Zur verkl\u00e4rten Wolke.", "tokens": ["Zur", "ver\u00b7kl\u00e4r\u00b7ten", "Wol\u00b7ke", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "In dem matten, wasserblauen", "tokens": ["In", "dem", "mat\u00b7ten", ",", "was\u00b7ser\u00b7blau\u00b7en"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Abendhimmel schwimmt das selige Eiland", "tokens": ["A\u00b7bend\u00b7him\u00b7mel", "schwimmt", "das", "se\u00b7li\u00b7ge", "Ei\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ruhevoll und gl\u00e4nzend wei\u00df,", "tokens": ["Ru\u00b7he\u00b7voll", "und", "gl\u00e4n\u00b7zend", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie auf Hochgebirgen keusches Eis.", "tokens": ["Wie", "auf", "Hoch\u00b7ge\u00b7bir\u00b7gen", "keu\u00b7sches", "Eis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Sanfte Th\u00e4ler thun sich droben auf; ich schaue", "tokens": ["Sanf\u00b7te", "Th\u00e4\u00b7ler", "thun", "sich", "dro\u00b7ben", "auf", ";", "ich", "schau\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PRF", "ADV", "PTKVZ", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Seidenzarte, schneeige Hyazinthenfelder,", "tokens": ["Sei\u00b7denz\u00b7ar\u00b7te", ",", "schne\u00b7ei\u00b7ge", "Hy\u00b7a\u00b7zin\u00b7then\u00b7fel\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Auf den H\u00fcgeln duftige Apfelbl\u00fctenw\u00e4lder", "tokens": ["Auf", "den", "H\u00fc\u00b7geln", "duf\u00b7ti\u00b7ge", "Ap\u00b7fel\u00b7bl\u00fc\u00b7ten\u00b7w\u00e4l\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Und dazwischen, blitzend gleich dem Thaue,", "tokens": ["Und", "da\u00b7zwi\u00b7schen", ",", "blit\u00b7zend", "gleich", "dem", "Thau\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "$,", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Alabasterne Pal\u00e4ste.", "tokens": ["A\u00b7la\u00b7bas\u00b7ter\u00b7ne", "Pa\u00b7l\u00e4s\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Um Gebl\u00fcm und Bl\u00fcten\u00e4ste", "tokens": ["Um", "Ge\u00b7bl\u00fcm", "und", "Bl\u00fc\u00b7ten\u00b7\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Hauchen L\u00fcfte, frisch wie auf der Alpenaue,", "tokens": ["Hau\u00b7chen", "L\u00fcf\u00b7te", ",", "frisch", "wie", "auf", "der", "Al\u00b7pe\u00b7nau\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJD", "KOKOM", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "Und da singt es wie von Kinderstimmen.", "tokens": ["Und", "da", "singt", "es", "wie", "von", "Kin\u00b7der\u00b7stim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "KOKOM", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Doch wo weilen ", "tokens": ["Doch", "wo", "wei\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWAV", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "Rein und selig wohnen?", "tokens": ["Rein", "und", "se\u00b7lig", "woh\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Dort an wei\u00dfer H\u00fcgel R\u00e4ndern", "tokens": ["Dort", "an", "wei\u00b7\u00dfer", "H\u00fc\u00b7gel", "R\u00e4n\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stehen sie in schimmernden Gew\u00e4ndern,", "tokens": ["Ste\u00b7hen", "sie", "in", "schim\u00b7mern\u00b7den", "Ge\u00b7w\u00e4n\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Eng geschaart. Und sieh, die Einen", "tokens": ["Eng", "ge\u00b7schaart", ".", "Und", "sieh", ",", "die", "Ei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVPP", "$.", "KON", "VVFIN", "$,", "PRELS", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00fcllen ihr Gesicht und weinen,", "tokens": ["H\u00fcl\u00b7len", "ihr", "Ge\u00b7sicht", "und", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Andre schauen starr und trauernd,", "tokens": ["And\u00b7re", "schau\u00b7en", "starr", "und", "trau\u00b7ernd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Oft zusammenschauernd,", "tokens": ["Oft", "zu\u00b7sam\u00b7men\u00b7schau\u00b7ernd", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Wie entsetzt, hernieder", "tokens": ["Wie", "ent\u00b7setzt", ",", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "punct", "word"], "pos": ["PWAV", "VVPP", "$,", "PTKVZ"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Auf der Weltstadt w\u00fcste Riesenglieder,", "tokens": ["Auf", "der", "Welt\u00b7stadt", "w\u00fcs\u00b7te", "Rie\u00b7sen\u00b7glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Die in Staub und S\u00fcnde angstvoll keucht.", "tokens": ["Die", "in", "Staub", "und", "S\u00fcn\u00b7de", "angst\u00b7voll", "keucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Und in liebendem Erbarmen", "tokens": ["Und", "in", "lie\u00b7ben\u00b7dem", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "M\u00f6chten sie die Stadt umarmen:", "tokens": ["M\u00f6ch\u00b7ten", "sie", "die", "Stadt", "um\u00b7ar\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": 