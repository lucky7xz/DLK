{"textgrid.poem.26546": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich wollte nie als Dichter mich geberden,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wollte nie als Dichter mich geberden,", "tokens": ["Ich", "woll\u00b7te", "nie", "als", "Dich\u00b7ter", "mich", "ge\u00b7ber\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "KOUS", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ob ich's gewesen auch zu allen Stunden,", "tokens": ["Ob", "ich's", "ge\u00b7we\u00b7sen", "auch", "zu", "al\u00b7len", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VAPP", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ob ich anders stets als ihr empfunden,", "tokens": ["Und", "ob", "ich", "an\u00b7ders", "stets", "als", "ihr", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es sollte nie f\u00fcr euch zum Vorwurf werden.", "tokens": ["Es", "soll\u00b7te", "nie", "f\u00fcr", "euch", "zum", "Vor\u00b7wurf", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPER", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich sch\u00e4tzte jegliches Verdienst auf Erden", "tokens": ["Ich", "sch\u00e4tz\u00b7te", "jeg\u00b7li\u00b7ches", "Ver\u00b7dienst", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und alles Gute, wo ich es gefunden;", "tokens": ["Und", "al\u00b7les", "Gu\u00b7te", ",", "wo", "ich", "es", "ge\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PWAV", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie oft man mir auch schlug die tiefsten Wunden \u2013", "tokens": ["Wie", "oft", "man", "mir", "auch", "schlug", "die", "tiefs\u00b7ten", "Wun\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIS", "PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als R\u00e4cher wollt' ich Niemand doch gef\u00e4hrden.", "tokens": ["Als", "R\u00e4\u00b7cher", "wollt'", "ich", "Nie\u00b7mand", "doch", "ge\u00b7f\u00e4hr\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VMFIN", "PPER", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Auf mein Verst\u00e4ndni\u00df konnte Jeder z\u00e4hlen,", "tokens": ["Auf", "mein", "Ver\u00b7st\u00e4nd\u00b7ni\u00df", "konn\u00b7te", "Je\u00b7der", "z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Mitleid, sanfte Tr\u00f6stung lie\u00df ich walten,", "tokens": ["Und", "Mit\u00b7leid", ",", "sanf\u00b7te", "Tr\u00f6s\u00b7tung", "lie\u00df", "ich", "wal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJA", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sah ich vom Schmerz zerrissen eu're Seelen.", "tokens": ["Sah", "ich", "vom", "Schmerz", "zer\u00b7ris\u00b7sen", "eu'\u00b7re", "See\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich r\u00fchrte nicht an eu'res Wahns Gestalten,", "tokens": ["Ich", "r\u00fchr\u00b7te", "nicht", "an", "eu'\u00b7res", "Wahns", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich schwieg zu eu'ren Schw\u00e4chen, eu'ren Fehlen \u2013:", "tokens": ["Ich", "schwieg", "zu", "eu'\u00b7ren", "Schw\u00e4\u00b7chen", ",", "eu'\u00b7ren", "Feh\u00b7len", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "ADJA", "NN", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "D'rum habt ihr mich auch niemals hoch gehalten.", "tokens": ["D'\u00b7rum", "habt", "ihr", "mich", "auch", "nie\u00b7mals", "hoch", "ge\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PRF", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}