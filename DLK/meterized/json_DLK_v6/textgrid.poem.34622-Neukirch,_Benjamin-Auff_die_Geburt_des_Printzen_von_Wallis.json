{"textgrid.poem.34622": {"metadata": {"author": {"name": "Neukirch, Benjamin", "birth": "N.A.", "death": "N.A."}, "title": "Auff die Geburt des Printzen von Wallis", "genre": "verse", "period": "N.A.", "pub_year": 1697, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist das nicht wunderlich! Printz Monmouth muste sterben/", "tokens": ["Ist", "das", "nicht", "wun\u00b7der\u00b7lich", "!", "Printz", "Mon\u00b7mouth", "mus\u00b7te", "ster\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "ADJD", "$.", "NN", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun soll ein m\u00fcller-kind den thron von England erben.", "tokens": ["Nun", "soll", "ein", "m\u00fcl\u00b7ler\u00b7kind", "den", "thron", "von", "En\u00b7gland", "er\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ART", "NN", "APPR", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie stimmet doch der tausch mit den gesetzen ein?", "tokens": ["Wie", "stim\u00b7met", "doch", "der", "tausch", "mit", "den", "ge\u00b7set\u00b7zen", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch Pater Peter ist hier klug genug gewesen.", "tokens": ["Doch", "Pa\u00b7ter", "Pe\u00b7ter", "ist", "hier", "klug", "ge\u00b7nug", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NE", "VAFIN", "ADV", "ADJD", "ADV", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er wolte/ da\u00df das reich gestohlen solte seyn:", "tokens": ["Er", "wol\u00b7te", "/", "da\u00df", "das", "reich", "ge\u00b7stoh\u00b7len", "sol\u00b7te", "seyn", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "KOUS", "ART", "ADJD", "VVINF", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Drum hat er einen dieb zum Printzen auserlesen.", "tokens": ["Drum", "hat", "er", "ei\u00b7nen", "dieb", "zum", "Print\u00b7zen", "au\u00b7ser\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}