{"dta.poem.11610": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Liebesgl\u00fcck.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn Dichter oft in warmen Phantasieen,", "tokens": ["Wenn", "Dich\u00b7ter", "oft", "in", "war\u00b7men", "Phan\u00b7ta\u00b7si\u00b7e\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von Liebesgl\u00fcck und schmerzlichem Vergn\u00fcgen", "tokens": ["Von", "Lie\u00b7bes\u00b7gl\u00fcck", "und", "schmerz\u00b7li\u00b7chem", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich oder uns, nach ihrer Art, bel\u00fcgen,", "tokens": ["Sich", "o\u00b7der", "uns", ",", "nach", "ih\u00b7rer", "Art", ",", "be\u00b7l\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "KON", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So sey dies Spielwerk ihnen gern verziehen.", "tokens": ["So", "sey", "dies", "Spiel\u00b7werk", "ih\u00b7nen", "gern", "ver\u00b7zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDS", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mir aber hat ein g\u00fct'ger Gott verliehen,", "tokens": ["Mir", "a\u00b7ber", "hat", "ein", "g\u00fct'\u00b7ger", "Gott", "ver\u00b7lie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Himmel, den sie tr\u00e4umen, zu durchfliegen,", "tokens": ["Den", "Him\u00b7mel", ",", "den", "sie", "tr\u00e4u\u00b7men", ",", "zu", "durch\u00b7flie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich sah die Anmuth mir im Arm sich schmiegen,", "tokens": ["Ich", "sah", "die", "An\u00b7muth", "mir", "im", "Arm", "sich", "schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Unschuld Blick von raschem Feuer gl\u00fchen.", "tokens": ["Der", "Un\u00b7schuld", "Blick", "von", "ra\u00b7schem", "Feu\u00b7er", "gl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Auch ich trug einst der Liebe M\u00fch' und Lasten,", "tokens": ["Auch", "ich", "trug", "einst", "der", "Lie\u00b7be", "M\u00fch'", "und", "Las\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Verschm\u00e4hte nicht, den herben Kelch zu trinken,", "tokens": ["Ver\u00b7schm\u00e4h\u00b7te", "nicht", ",", "den", "her\u00b7ben", "Kelch", "zu", "trin\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Damit ich seine Lust nun ganz empfinde.", "tokens": ["Da\u00b7mit", "ich", "sei\u00b7ne", "Lust", "nun", "ganz", "emp\u00b7fin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und dennoch gleich' ich jenen Erz-Phantasten:", "tokens": ["Und", "den\u00b7noch", "gleich'", "ich", "je\u00b7nen", "Er\u00b7zPhan\u00b7tas\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir will mein Gl\u00fcck so unerme\u00dflich d\u00fcnken,", "tokens": ["Mir", "will", "mein", "Gl\u00fcck", "so", "un\u00b7er\u00b7me\u00df\u00b7lich", "d\u00fcn\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ich mir oft im wachen Traum verschwinde.", "tokens": ["Da\u00df", "ich", "mir", "oft", "im", "wa\u00b7chen", "Traum", "ver\u00b7schwin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}