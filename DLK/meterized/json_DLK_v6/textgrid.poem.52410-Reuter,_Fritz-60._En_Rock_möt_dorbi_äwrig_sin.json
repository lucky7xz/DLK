{"textgrid.poem.52410": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "60. En Rock m\u00f6t dorbi \u00e4wrig sin", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "De oll Schipper Bradenal,", "tokens": ["De", "oll", "Schip\u00b7per", "Bra\u00b7de\u00b7nal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "De f\u00fchrt f\u00f6r Kopmann Bolzendalen", "tokens": ["De", "f\u00fchrt", "f\u00f6r", "Kop\u00b7mann", "Bol\u00b7zen\u00b7da\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NE", "NE", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von Rostock oft nah Fehmern dal,", "tokens": ["Von", "Ros\u00b7tock", "oft", "nah", "Feh\u00b7mern", "dal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "ADJD", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00dcm T\u00fcften, Speck un Botter dor tau halen", "tokens": ["\u00dcm", "T\u00fcf\u00b7ten", ",", "Speck", "un", "Bot\u00b7ter", "dor", "tau", "ha\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "NN", "FM", "FM", "FM", "FM", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Un andere Vertehrungsmiddel.", "tokens": ["Un", "an\u00b7de\u00b7re", "Ver\u00b7teh\u00b7rungs\u00b7mid\u00b7del", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "De Oll, de drog en slichten Kittel,", "tokens": ["De", "Oll", ",", "de", "drog", "en", "slich\u00b7ten", "Kit\u00b7tel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "FM", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Denn sin Verdeinst, de was man swack;", "tokens": ["Denn", "sin", "Ver\u00b7deinst", ",", "de", "was", "man", "swack", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Un was sin Utkunft ok man spirlich,", "tokens": ["Un", "was", "sin", "Ut\u00b7kunft", "ok", "man", "spir\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "PIS", "ADJD", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Hei was doch \u00fcmmer tru un ihrlich,", "tokens": ["Hei", "was", "doch", "\u00fcm\u00b7mer", "tru", "un", "ihr\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "ADV", "ADV", "ADJD", "FM", "PPER", "$,"], "meter": "+--+-+---", "measure": "iambic.tri.invert"}, "line.10": {"text": "Rein was sin Hand un rein sin Jack.", "tokens": ["Rein", "was", "sin", "Hand", "un", "rein", "sin", "Jack", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.11": {"text": "\u00bbje\u00ab, seggt denn mal ein von sin Fr\u00fcn'n,", "tokens": ["\u00bb", "je", "\u00ab", ",", "seggt", "denn", "mal", "ein", "von", "sin", "Fr\u00fcn'n", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$(", "$,", "VVFIN", "ADV", "ADV", "ART", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "\u00bbdu deihst wohrhaftig wohre S\u00fcn'n", "tokens": ["\u00bb", "du", "deihst", "wohr\u00b7haf\u00b7tig", "woh\u00b7re", "S\u00fcn'n"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "ADV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "An di un an din Fru un Kinner.", "tokens": ["An", "di", "un", "an", "din", "Fru", "un", "Kin\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "FM", "FM", "APPR", "PPOSAT", "NN", "FM", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.14": {"text": "Schriw doch en Rock dor mal mit rinner", "tokens": ["Schriw", "doch", "en", "Rock", "dor", "mal", "mit", "rin\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "FM", "FM", "FM", "ADV", "APPR", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Mang all de Botter un den Speck!", "tokens": ["Mang", "all", "de", "Bot\u00b7ter", "un", "den", "Speck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "FM", "FM", "FM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "De Kopmann hett allein Gewinn,", "tokens": ["De", "Kop\u00b7mann", "hett", "al\u00b7lein", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Du arbeitst f\u00f6r en puren Dreck:", "tokens": ["Du", "ar\u00b7beitst", "f\u00f6r", "en", "pu\u00b7ren", "Dreck", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "FM", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "En Rock m\u00f6t dorbi \u00e4wrig sin.\u00ab", "tokens": ["En", "Rock", "m\u00f6t", "dor\u00b7bi", "\u00e4w\u00b7rig", "sin", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Hei markt sick dit. \u2013 Dat negste Mal", "tokens": ["Hei", "markt", "sick", "dit", ".", "\u2013", "Dat", "negs\u00b7te", "Mal"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "$.", "$(", "NN", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "K\u00fcmmt hei taum Kopmann Bolzendal", "tokens": ["K\u00fcmmt", "hei", "taum", "Kop\u00b7mann", "Bol\u00b7zen\u00b7dal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Un reikt em sine Reknung in.", "tokens": ["Un", "reikt", "em", "si\u00b7ne", "Re\u00b7knung", "in", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJA", "ADJA", "NN", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "De Kopmann kickt de Reknung nah.", "tokens": ["De", "Kop\u00b7mann", "kickt", "de", "Re\u00b7knung", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "NE", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sei stimmt. \u00bbJa\u00ab, seggt hei, \u00bbja!", "tokens": ["Sei", "stimmt", ".", "\u00bb", "Ja", "\u00ab", ",", "seggt", "hei", ",", "\u00bb", "ja", "!"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["VAIMP", "VVFIN", "$.", "$(", "PTKANT", "$(", "$,", "VVFIN", "PTKVZ", "$,", "$(", "PTKANT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sei's richtig. \u00c4wer dese Posten? \u2013", "tokens": ["Sei's", "rich\u00b7tig", ".", "\u00c4\u00b7wer", "de\u00b7se", "Pos\u00b7ten", "?", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADJD", "$.", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "+--+---+-", "measure": "dactylic.di.plus"}, "line.7": {"text": "F\u00f6r'n nigen Rock, de sall tw\u00f6lf Daler kosten?", "tokens": ["F\u00f6r'n", "ni\u00b7gen", "Rock", ",", "de", "sall", "tw\u00f6lf", "Da\u00b7ler", "kos\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "FM", "FM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wo k\u00fcmmt de nige Rock hir rin?\u00ab", "tokens": ["Wo", "k\u00fcmmt", "de", "ni\u00b7ge", "Rock", "hir", "rin", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "NE", "ADJA", "NN", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "\u00bbje\u00ab, seggt nu uns' oll Bradenal,", "tokens": ["\u00bb", "je", "\u00ab", ",", "seggt", "nu", "un\u00b7s'", "oll", "Bra\u00b7de\u00b7nal", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$(", "$,", "VVFIN", "ADV", "ADV", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "\u00bbsei segg'n jo all', Herr Bolzendal,", "tokens": ["\u00bb", "sei", "seg\u00b7g'n", "jo", "all'", ",", "Herr", "Bol\u00b7zen\u00b7dal", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "NN", "NE", "$,"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.11": {"text": "En Rock m\u00f6t dorbi \u00e4wrig sin.\u00ab", "tokens": ["En", "Rock", "m\u00f6t", "dor\u00b7bi", "\u00e4w\u00b7rig", "sin", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "\u00bbwat hir, wat dor!\u00ab seggt Bolzendal,", "tokens": ["\u00bb", "wat", "hir", ",", "wat", "dor", "!", "\u00ab", "seggt", "Bol\u00b7zen\u00b7dal", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "$,", "PRELS", "ADV", "$.", "$(", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "\u00bbso l\u00fcdt bi mi hir nich de Klock!\u00ab", "tokens": ["\u00bb", "so", "l\u00fcdt", "bi", "mi", "hir", "nich", "de", "Klock", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "FM", "FM", "FM", "FM", "PTKNEG", "NE", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Un strickt den Oll'n, der Deuwel hal!,", "tokens": ["Un", "strickt", "den", "Oll'n", ",", "der", "Deu\u00b7wel", "hal", "!", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ADJD", "$.", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Dwas d\u00f6rch den sch\u00f6nen, nigen Rock.", "tokens": ["Dwas", "d\u00f6rch", "den", "sch\u00f6\u00b7nen", ",", "ni\u00b7gen", "Rock", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "De Oll, de geiht. Sin Fr\u00fcnd, de trefft en", "tokens": ["De", "Oll", ",", "de", "geiht", ".", "Sin", "Fr\u00fcnd", ",", "de", "trefft", "en"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "NE", "VVPP", "$.", "PPOSAT", "NN", "$,", "FM", "FM", "FM"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Abends in en Gastlokal.", "tokens": ["Des", "A\u00b7bends", "in", "en", "Gast\u00b7lo\u00b7kal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbgu'n Abend, Jochen Bradenal!", "tokens": ["\u00bb", "gu'n", "A\u00b7bend", ",", "Jo\u00b7chen", "Bra\u00b7de\u00b7nal", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Na, wo g\u00fcng't denn mit din Gesch\u00e4ften?\u00ab", "tokens": ["Na", ",", "wo", "g\u00fcng't", "denn", "mit", "din", "Ge\u00b7sch\u00e4f\u00b7ten", "?", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "\u00bbje, dor kamm'ck in Verlegenheiten.", "tokens": ["\u00bb", "je", ",", "dor", "kam\u00b7m'ck", "in", "Ver\u00b7le\u00b7gen\u00b7hei\u00b7ten", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Denn von den Rock wull hei nicks weiten,", "tokens": ["Denn", "von", "den", "Rock", "wull", "hei", "nicks", "wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VMFIN", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Den wull hei mi nich mit bereken,", "tokens": ["Den", "wull", "hei", "mi", "nich", "mit", "be\u00b7re\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "FM", "FM", "FM", "PTKNEG", "APPR", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Den hett hei glatt mi rute streken.\u00ab", "tokens": ["Den", "hett", "hei", "glatt", "mi", "ru\u00b7te", "stre\u00b7ken", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "FM", "FM", "FM", "FM", "FM", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "\u00bbwo? Di mag jo de D\u00fcwel plagen!", "tokens": ["\u00bb", "wo", "?", "Di", "mag", "jo", "de", "D\u00fc\u00b7wel", "pla\u00b7gen", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "$.", "NE", "VMFIN", "NE", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Hest du den d\u00fcdlich rinne set't?\u00ab", "tokens": ["Hest", "du", "den", "d\u00fcd\u00b7lich", "rin\u00b7ne", "set't", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "VVFIN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "\u00bbja, in de Reknung heww'ck en dragen", "tokens": ["\u00bb", "ja", ",", "in", "de", "Re\u00b7knung", "hew\u00b7w'ck", "en", "dra\u00b7gen"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PTKANT", "$,", "APPR", "NE", "NN", "NE", "NE", "VVFIN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.12": {"text": "Un heww em up tw\u00f6lw Daler sch\u00e4t't.\u00ab", "tokens": ["Un", "heww", "em", "up", "tw\u00f6lw", "Da\u00b7ler", "sch\u00e4t'", "t.", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "\u00bbje, ja, je, ja! Kennst du nich Bolzendalen?", "tokens": ["\u00bb", "je", ",", "ja", ",", "je", ",", "ja", "!", "Kennst", "du", "nich", "Bol\u00b7zen\u00b7da\u00b7len", "?"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "PTKANT", "$,", "ADV", "$,", "ADV", "$.", "VVFIN", "PPER", "PTKNEG", "NN", "$."], "meter": "-----+-+-+-", "measure": "unknown.measure.tri"}, "line.14": {"text": "De Ort, de ward di sch\u00f6n bi 'Sch\u00e4tten',", "tokens": ["De", "Ort", ",", "de", "ward", "di", "sch\u00f6n", "bi", "'", "Sch\u00e4t\u00b7ten", "'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["NE", "NN", "$,", "NE", "VAFIN", "FM", "FM", "FM", "$(", "NN", "$(", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "De ward f\u00f6r di en Rock betahlen!", "tokens": ["De", "ward", "f\u00f6r", "di", "en", "Rock", "be\u00b7tah\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "FM", "FM", "FM", "NN", "VVINF", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.16": {"text": "Ne, mang de Botter m\u00f6\u00df't du'n setten,", "tokens": ["Ne", ",", "mang", "de", "Bot\u00b7ter", "m\u00f6\u00df't", "du'n", "set\u00b7ten", ","], "token_info": ["word", "pun