{"textgrid.poem.23969": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Gebet zwischen bl\u00fchenden Kastanien", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fchling, oh du s\u00fc\u00dfer Junge!", "tokens": ["Fr\u00fch\u00b7ling", ",", "oh", "du", "s\u00fc\u00b7\u00dfer", "Jun\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine Beine sind so z\u00e4rtlich", "tokens": ["Dei\u00b7ne", "Bei\u00b7ne", "sind", "so", "z\u00e4rt\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schlank und deine schmalen Lippen", "tokens": ["Schlank", "und", "dei\u00b7ne", "schma\u00b7len", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Feucht.", "tokens": ["Feucht", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Wie du schreitest! Wie die Locken fliegen", "tokens": ["Wie", "du", "schrei\u00b7test", "!", "Wie", "die", "Lo\u00b7cken", "flie\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$.", "PWAV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und das blaue Band im blonden Haare!", "tokens": ["Und", "das", "blau\u00b7e", "Band", "im", "blon\u00b7den", "Haa\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wie es duftet, wo dein Mantel wehte!", "tokens": ["Wie", "es", "duf\u00b7tet", ",", "wo", "dein", "Man\u00b7tel", "weh\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Fr\u00fchling, s\u00fc\u00dfer, saftgebenedeiter", "tokens": ["Fr\u00fch\u00b7ling", ",", "s\u00fc\u00b7\u00dfer", ",", "saft\u00b7ge\u00b7be\u00b7ne\u00b7dei\u00b7ter"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NE", "$,", "ADJD", "$,", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sieger-Knabe mit den M\u00e4dchenbr\u00fcsten,", "tokens": ["Sie\u00b7ger\u00b7Kna\u00b7be", "mit", "den", "M\u00e4d\u00b7chen\u00b7br\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Hauch mich an mit deinem Blumenatem,", "tokens": ["Hauch", "mich", "an", "mit", "dei\u00b7nem", "Blu\u00b7me\u00b7na\u00b7tem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der ich dich jetzt tiefer kenn und liebe,", "tokens": ["Der", "ich", "dich", "jetzt", "tie\u00b7fer", "kenn", "und", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADV", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Deiner Br\u00fcnste voller bin als ehmals.", "tokens": ["Dei\u00b7ner", "Br\u00fcns\u00b7te", "vol\u00b7ler", "bin", "als", "eh\u00b7mals", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VAFIN", "KOKOM", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Neig dich mir, oh s\u00fc\u00dfer Knabe, s\u00fc\u00dfres", "tokens": ["Neig", "dich", "mir", ",", "oh", "s\u00fc\u00b7\u00dfer", "Kna\u00b7be", ",", "s\u00fc\u00df\u00b7res"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "PPER", "$,", "ADJD", "ADJA", "NN", "$,", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "M\u00e4dchen! Ich vergehe sonst vor Sehnsucht,", "tokens": ["M\u00e4d\u00b7chen", "!", "Ich", "ver\u00b7ge\u00b7he", "sonst", "vor", "Sehn\u00b7sucht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Dich zu f\u00fchlen.", "tokens": ["Dich", "zu", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}