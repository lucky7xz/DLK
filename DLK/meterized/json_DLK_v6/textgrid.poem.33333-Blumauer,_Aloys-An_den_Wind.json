{"textgrid.poem.33333": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "An den Wind", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er, ", "tokens": ["Er", ","], "token_info": ["word", "punct"], "pos": ["PPER", "$,"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Von uns'rer Atmosph\u00e4re,", "tokens": ["Von", "un\u00b7s'\u00b7rer", "At\u00b7mo\u00b7sph\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Macht wahrlich seinem Regiment", "tokens": ["Macht", "wahr\u00b7lich", "sei\u00b7nem", "Re\u00b7gi\u00b7ment"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am Himmel wenig Ehre.", "tokens": ["Am", "Him\u00b7mel", "we\u00b7nig", "Eh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "D'rum, Herr Spavento! h\u00f6r' er mich:", "tokens": ["D'\u00b7rum", ",", "Herr", "Spa\u00b7ven\u00b7to", "!", "h\u00f6r'", "er", "mich", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "NN", "NE", "$.", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Frisch von der Leber weg will ich", "tokens": ["Frisch", "von", "der", "Le\u00b7ber", "weg", "will", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "ADV", "VMFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihm f\u00fcr sein tolles Wesen", "tokens": ["Ihm", "f\u00fcr", "sein", "tol\u00b7les", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Jetzt die Leviten lesen.", "tokens": ["Jetzt", "die", "Le\u00b7vi\u00b7ten", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er ist ein wahrer Erztyrann:", "tokens": ["Er", "ist", "ein", "wah\u00b7rer", "Erz\u00b7ty\u00b7rann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es b\u00fcckt in seinem Reiche", "tokens": ["Es", "b\u00fcckt", "in", "sei\u00b7nem", "Rei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vor ihm sich jeder Unterthan,", "tokens": ["Vor", "ihm", "sich", "je\u00b7der", "Un\u00b7ter\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sei's Gr\u00e4schen oder Eiche:", "tokens": ["Sei's", "Gr\u00e4sc\u00b7hen", "o\u00b7der", "Ei\u00b7che", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ja, wenn's ihm einf\u00e4llt, m\u00fcssen gar", "tokens": ["Ja", ",", "wenn's", "ihm", "ein\u00b7f\u00e4llt", ",", "m\u00fcs\u00b7sen", "gar"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "VVPP", "$,", "VMFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit augenscheinlicher Gefahr,", "tokens": ["Mit", "au\u00b7gen\u00b7schein\u00b7li\u00b7cher", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Trotz ihrem steifen R\u00fccken,", "tokens": ["Trotz", "ih\u00b7rem", "stei\u00b7fen", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sich Thurm und Schornstein b\u00fccken.", "tokens": ["Sich", "Thurm", "und", "Schorn\u00b7stein", "b\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und ziehet er als Feind heran", "tokens": ["Und", "zie\u00b7het", "er", "als", "Feind", "he\u00b7ran"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "KOUS", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In einem Donnerwetter,", "tokens": ["In", "ei\u00b7nem", "Don\u00b7ner\u00b7wet\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So k\u00fcndigt er den Krieg uns an,", "tokens": ["So", "k\u00fcn\u00b7digt", "er", "den", "Krieg", "uns", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie uns're Erdeng\u00f6tter;", "tokens": ["Wie", "un\u00b7s'\u00b7re", "Er\u00b7den\u00b7g\u00f6t\u00b7ter", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Da nimmt er beide Backen voll,", "tokens": ["Da", "nimmt", "er", "bei\u00b7de", "Ba\u00b7cken", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und streut, als w\u00e4r' er noch so toll,", "tokens": ["Und", "streut", ",", "als", "w\u00e4r'", "er", "noch", "so", "toll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOKOM", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von Rechten, die nichts taugen,", "tokens": ["Von", "Rech\u00b7ten", ",", "die", "nichts", "tau\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Brav Staub uns in die Augen.", "tokens": ["Brav", "Staub", "uns", "in", "die", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er pflegt hienieden weit und breit", "tokens": ["Er", "pflegt", "hien\u00b7ie\u00b7den", "weit", "und", "breit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In alles sich zu mischen,", "tokens": ["In", "al\u00b7les", "sich", "zu", "mi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sucht, wie seine Heiligkeit,", "tokens": ["Und", "sucht", ",", "wie", "sei\u00b7ne", "Hei\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Tr\u00fcben nur zu fischen;", "tokens": ["Im", "Tr\u00fc\u00b7ben", "nur", "zu", "fi\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und ist dann die Konfusion", "tokens": ["Und", "ist", "dann", "die", "Kon\u00b7fu\u00b7si\u00b7on"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Recht gro\u00df, so macht er sich davon,", "tokens": ["Recht", "gro\u00df", ",", "so", "macht", "er", "sich", "da\u00b7von", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PRF", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und l\u00e4\u00dft die Welt in Kriegen,", "tokens": ["Und", "l\u00e4\u00dft", "die", "Welt", "in", "Krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die er erst anblies, liegen.", "tokens": ["Die", "er", "erst", "an\u00b7blies", ",", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es soll nach seinem Eigensinn", "tokens": ["Es", "soll", "nach", "sei\u00b7nem", "Ei\u00b7gen\u00b7sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hienieden alles gehen;", "tokens": ["Hien\u00b7ie\u00b7den", "al\u00b7les", "ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir Menschen sollen nur, wohin", "tokens": ["Wir", "Men\u00b7schen", "sol\u00b7len", "nur", ",", "wo\u00b7hin"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "NN", "VMFIN", "ADV", "$,", "PWAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es ihm beliebt, uns drehen;", "tokens": ["Es", "ihm", "be\u00b7liebt", ",", "uns", "dre\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PPER", "ADJD", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Allein wir kehren seinem Grimm", "tokens": ["Al\u00b7lein", "wir", "keh\u00b7ren", "sei\u00b7nem", "Grimm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "PPOSAT", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den R\u00fccken zu, und zeigen ihm,", "tokens": ["Den", "R\u00fc\u00b7cken", "zu", ",", "und", "zei\u00b7gen", "ihm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "(mag er auch noch so rasen)", "tokens": ["(", "mag", "er", "auch", "noch", "so", "ra\u00b7sen", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "ADV", "ADV", "ADV", "VVINF", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Wohin er uns soll blasen.", "tokens": ["Wo\u00b7hin", "er", "uns", "soll", "bla\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Er handhabt die Gerechtigkeit", "tokens": ["Er", "hand\u00b7habt", "die", "Ge\u00b7rech\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Just so, wie manche Richter:", "tokens": ["Just", "so", ",", "wie", "man\u00b7che", "Rich\u00b7ter", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PWAV", "PIAT", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Statt da\u00df er Wolken oft zerstreut,", "tokens": ["Statt", "da\u00df", "er", "Wol\u00b7ken", "oft", "zer\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "NN", "ADV", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Macht er sie nur noch dichter:", "tokens": ["Macht", "er", "sie", "nur", "noch", "dich\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Die kleinen Lichter bl\u00e4st er aus,", "tokens": ["Die", "klei\u00b7nen", "Lich\u00b7ter", "bl\u00e4st", "er", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die grossen aber, die uns Haus", "tokens": ["Die", "gros\u00b7sen", "a\u00b7ber", ",", "die", "uns", "Haus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "ADV", "$,", "PRELS", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und Hof verheeren k\u00f6nnen,", "tokens": ["Und", "Hof", "ver\u00b7hee\u00b7ren", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Macht er noch st\u00e4rker brennen.", "tokens": ["Macht", "er", "noch", "st\u00e4r\u00b7ker", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "Von seiner Raubsucht hat man auch", "tokens": ["Von", "sei\u00b7ner", "Raub\u00b7sucht", "hat", "man", "auch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PIS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Manch' gr\u00e4uliches Exempel:", "tokens": ["Man\u00b7ch'", "gr\u00e4u\u00b7li\u00b7ches", "Ex\u00b7em\u00b7pel", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Er fegt oft Nachts mit seinem Hauch", "tokens": ["Er", "fegt", "oft", "Nachts", "mit", "sei\u00b7nem", "Hauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Rein Kirchen aus und Tempel;", "tokens": ["Rein", "Kir\u00b7chen", "aus", "und", "Tem\u00b7pel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "D'rum haben auch die Menschen ihn", "tokens": ["D'\u00b7rum", "ha\u00b7ben", "auch", "die", "Men\u00b7schen", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ADV", "ART", "NN", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Verdammt, da\u00df er mu\u00df Schiffe zieh'n,", "tokens": ["Ver\u00b7dammt", ",", "da\u00df", "er", "mu\u00df", "Schif\u00b7fe", "zieh'n", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPER", "VMFIN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ja, ", "tokens": ["Ja", ","], "token_info": ["word", "punct"], "pos": ["PTKANT", "$,"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "L\u00e4\u00dft man ihn Gassen kehren.", "tokens": ["L\u00e4\u00dft", "man", "ihn", "Gas\u00b7sen", "keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "An Sitten und Manier ist er", "tokens": ["An", "Sit\u00b7ten", "und", "Ma\u00b7nier", "ist", "er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "PPER"], "meter": "-+--+++-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein wahrer Engell\u00e4nder:", "tokens": ["Ein", "wah\u00b7rer", "En\u00b7gel\u00b7l\u00e4n\u00b7der", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn, wenn's ihm einf\u00e4llt, geht er her,", "tokens": ["Denn", ",", "wenn's", "ihm", "ein\u00b7f\u00e4llt", ",", "geht", "er", "her", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "VVPP", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zerrei\u00dft uns die Gew\u00e4nder,", "tokens": ["Zer\u00b7rei\u00dft", "uns", "die", "Ge\u00b7w\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wirft uns mit Schlossen, pfeift uns aus,", "tokens": ["Wirft", "uns", "mit", "Schlos\u00b7sen", ",", "pfeift", "uns", "aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als wenn die Welt, dies Narrenhaus,", "tokens": ["Als", "wenn", "die", "Welt", ",", "dies", "Nar\u00b7ren\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "$,", "PDS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nur ein Theater w\u00e4re,", "tokens": ["Nur", "ein", "The\u00b7a\u00b7ter", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und wir f\u00fcr ihn Acteure.", "tokens": ["Und", "wir", "f\u00fcr", "ihn", "Ac\u00b7teu\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und wird nicht gleich ihm aufgethan", "tokens": ["Und", "wird", "nicht", "gleich", "ihm", "auf\u00b7ge\u00b7than"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PTKNEG", "ADV", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So macht er ein Get\u00fcmmel,", "tokens": ["So", "macht", "er", "ein", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schl\u00e4gt uns an Th\u00fcr und Fenster an", "tokens": ["Schl\u00e4gt", "uns", "an", "Th\u00fcr", "und", "Fens\u00b7ter", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und poltert wie ein L\u00fcmmel,", "tokens": ["Und", "pol\u00b7tert", "wie", "ein", "L\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "L\u00e4\u00dft keine Fahne ungetrillt,", "tokens": ["L\u00e4\u00dft", "kei\u00b7ne", "Fah\u00b7ne", "un\u00b7ge\u00b7trillt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und machet jeden Aush\u00e4ngschild,", "tokens": ["Und", "ma\u00b7chet", "je\u00b7den", "Aus\u00b7h\u00e4ng\u00b7schild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sei's Kaiser oder Engel,", "tokens": ["Sei's", "Kai\u00b7ser", "o\u00b7der", "En\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zu einem Galgenschwengel.", "tokens": ["Zu", "ei\u00b7nem", "Gal\u00b7gen\u00b7schwen\u00b7gel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Auch wollen ihn, er geiler Bock,", "tokens": ["Auch", "wol\u00b7len", "ihn", ",", "er", "gei\u00b7ler", "Bock", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "PPER", "ADJA", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die M\u00e4dchen gar nicht loben:", "tokens": ["Die", "M\u00e4d\u00b7chen", "gar", "nicht", "lo\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es ist ja fast kein Unterrock,", "tokens": ["Es", "ist", "ja", "fast", "kein", "Un\u00b7ter\u00b7rock", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den er nicht aufgehoben:", "tokens": ["Den", "er", "nicht", "auf\u00b7ge\u00b7ho\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VVPP", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Geht das nicht an, so legt er sich", "tokens": ["Geht", "das", "nicht", "an", ",", "so", "legt", "er", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PDS", "PTKNEG", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf sie, und wei\u00df, dann meisterlich,", "tokens": ["Auf", "sie", ",", "und", "wei\u00df", ",", "dann", "meis\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KON", "VVFIN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Trotz allem Protestiren,", "tokens": ["Trotz", "al\u00b7lem", "Pro\u00b7tes\u00b7ti\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.8": {"text": "Sie abzumodelliren.", "tokens": ["Sie", "ab\u00b7zu\u00b7mo\u00b7del\u00b7li\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Nichts ist ihm, wenn er saust und braust,", "tokens": ["Nichts", "ist", "ihm", ",", "wenn", "er", "saust", "und", "braust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Erden zu vergleichen:", "tokens": ["Auf", "Er\u00b7den", "zu", "ver\u00b7glei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Allein am aller\u00e4rgsten haust", "tokens": ["Al\u00b7lein", "am", "al\u00b7ler\u00b7\u00e4rgs\u00b7ten", "haust"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er noch in unsern B\u00e4uchen:", "tokens": ["Er", "noch", "in", "un\u00b7sern", "B\u00e4u\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da brummt und keift und zwickt und qu\u00e4lt", "tokens": ["Da", "brummt", "und", "keift", "und", "zwickt", "und", "qu\u00e4lt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "KON", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er uns, so lang es ihm gef\u00e4llt,", "tokens": ["Er", "uns", ",", "so", "lang", "es", "ihm", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "$,", "ADV", "ADJD", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und neckt dann durch sein Blasen", "tokens": ["Und", "neckt", "dann", "durch", "sein", "Bla\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sogar noch uns're Nasen.", "tokens": ["So\u00b7gar", "noch", "un\u00b7s'\u00b7re", "Na\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Allein, da sing ich armer Narr", "tokens": ["Al\u00b7lein", ",", "da", "sing", "ich", "ar\u00b7mer", "Narr"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich athemlos und m\u00fcde:", "tokens": ["Mich", "at\u00b7hem\u00b7los", "und", "m\u00fc\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und er bl\u00e4st fort, und brummt wohl gar", "tokens": ["Und", "er", "bl\u00e4st", "fort", ",", "und", "brummt", "wohl", "gar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Ba\u00df zu meinem Liede,", "tokens": ["Den", "Ba\u00df", "zu", "mei\u00b7nem", "Lie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "D'rum Punktum! und kein W\u00f6rtchen mehr:", "tokens": ["D'\u00b7rum", "Punk\u00b7tum", "!", "und", "kein", "W\u00f6rt\u00b7chen", "mehr", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "$.", "KON", "PIAT", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Denn alle die Moral, mit der", "tokens": ["Denn", "al\u00b7le", "die", "Mo\u00b7ral", ",", "mit", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PIS", "ART", "NN", "$,", "APPR", "ART"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Man ihm kommt angestochen,", "tokens": ["Man", "ihm", "kommt", "an\u00b7ge\u00b7sto\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ist in den Wind gesprochen.", "tokens": ["Ist", "in", "den", "Wind", "ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}