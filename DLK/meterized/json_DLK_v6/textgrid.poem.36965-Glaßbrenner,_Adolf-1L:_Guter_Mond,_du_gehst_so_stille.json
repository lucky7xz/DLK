": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "VVPP", "$.", "PPER", "VAFIN", "ADV", "CARD"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Morgen kochst du Kl\u00f6\u00dfe wieder;", "tokens": ["Mor\u00b7gen", "kochst", "du", "Kl\u00f6\u00b7\u00dfe", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "La\u00df' uns jetzt zu Bette gehn.", "tokens": ["La\u00df'", "uns", "jetzt", "zu", "Bet\u00b7te", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Guter Mond, du gehst so stille", "tokens": ["Gu\u00b7ter", "Mond", ",", "du", "gehst", "so", "stil\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber Deutschlands Fluren hin!", "tokens": ["Ue\u00b7ber", "Deutschlands", "Flu\u00b7ren", "hin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Doctor Bos legt ab die Brille,", "tokens": ["Doc\u00b7tor", "Bos", "legt", "ab", "die", "Bril\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denkt des Tages Hochgewinn;", "tokens": ["Denkt", "des", "Ta\u00b7ges", "Hoch\u00b7ge\u00b7winn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Einer Ode von Horazen", "tokens": ["Ei\u00b7ner", "O\u00b7de", "von", "Ho\u00b7ra\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.6": {"text": "Gab er neuen Commentar;", "tokens": ["Gab", "er", "neu\u00b7en", "Com\u00b7men\u00b7tar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Froh bringt er, nach den Strapatzen,", "tokens": ["Froh", "bringt", "er", ",", "nach", "den", "Stra\u00b7pat\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.8": {"text": "Morpheus nun sein Opfer dar.", "tokens": ["Mor\u00b7pheus", "nun", "sein", "Op\u00b7fer", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Guter Mond, du gehst so stille", "tokens": ["Gu\u00b7ter", "Mond", ",", "du", "gehst", "so", "stil\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber Deutschlands Fluren hin!", "tokens": ["Ue\u00b7ber", "Deutschlands", "Flu\u00b7ren", "hin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Vor der alten Hauspostille", "tokens": ["Vor", "der", "al\u00b7ten", "Haus\u00b7pos\u00b7til\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sitzt die fromme Kupplerin;", "tokens": ["Sitzt", "die", "from\u00b7me", "Kupp\u00b7le\u00b7rin", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Von Theater-Liebsgeschichtchen", "tokens": ["Von", "The\u00b7a\u00b7ter\u00b7Liebs\u00b7ge\u00b7schicht\u00b7chen"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kehret heim der Intendant;", "tokens": ["Keh\u00b7ret", "heim", "der", "In\u00b7ten\u00b7dant", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dr\u00fcben ist das Dreierlichtchen", "tokens": ["Dr\u00fc\u00b7ben", "ist", "das", "Drei\u00b7er\u00b7licht\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Beim Studenten abgebrannt.", "tokens": ["Beim", "Stu\u00b7den\u00b7ten", "ab\u00b7ge\u00b7brannt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Guter Mond, du gehst so stille", "tokens": ["Gu\u00b7ter", "Mond", ",", "du", "gehst", "so", "stil\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber Deutschlands Fluren hin!", "tokens": ["Ue\u00b7ber", "Deutschlands", "Flu\u00b7ren", "hin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Des Ministers letzter Wille", "tokens": ["Des", "Mi\u00b7nis\u00b7ters", "letz\u00b7ter", "Wil\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zeugt von h\u00f6chst loyalem Sinn:", "tokens": ["Zeugt", "von", "h\u00f6chst", "lo\u00b7ya\u00b7lem", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hundert Schriften sind verboten,", "tokens": ["Hun\u00b7dert", "Schrif\u00b7ten", "sind", "ver\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sagt das neue Abendblatt;", "tokens": ["Sagt", "das", "neu\u00b7e", "A\u00b7bend\u00b7blatt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auch find't k\u00fcnftighin bei Todten", "tokens": ["Auch", "find't", "k\u00fcnf\u00b7tig\u00b7hin", "bei", "Tod\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Nur censirtes Reden Statt.", "tokens": ["Nur", "cen\u00b7sir\u00b7tes", "Re\u00b7den", "Statt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Guter Mond, du gehst so stille", "tokens": ["Gu\u00b7ter", "Mond", ",", "du", "gehst", "so", "stil\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "ADV", "ADJA"], "meter": "+-+-+-+-",