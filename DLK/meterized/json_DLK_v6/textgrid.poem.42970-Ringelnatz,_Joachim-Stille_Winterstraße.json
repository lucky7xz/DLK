{"textgrid.poem.42970": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Stille Winterstra\u00dfe", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es heben sich vernebelt braun", "tokens": ["Es", "he\u00b7ben", "sich", "ver\u00b7ne\u00b7belt", "braun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "VVPP", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Berge aus dem klaren Wei\u00df,", "tokens": ["Die", "Ber\u00b7ge", "aus", "dem", "kla\u00b7ren", "Wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und aus dem Wei\u00df ragt braun ein Zaun,", "tokens": ["Und", "aus", "dem", "Wei\u00df", "ragt", "braun", "ein", "Zaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Steht eine Stange wie ein Stei\u00df.", "tokens": ["Steht", "ei\u00b7ne", "Stan\u00b7ge", "wie", "ein", "Stei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Rabe fliegt, so schwarz und scharf,", "tokens": ["Ein", "Ra\u00b7be", "fliegt", ",", "so", "schwarz", "und", "scharf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ihn kein Maler malen darf,", "tokens": ["Wie", "ihn", "kein", "Ma\u00b7ler", "ma\u00b7len", "darf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn er's nicht etwa kann.", "tokens": ["Wenn", "er's", "nicht", "et\u00b7wa", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "ADV", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich stapfe einsam durch den Schnee.", "tokens": ["Ich", "stap\u00b7fe", "ein\u00b7sam", "durch", "den", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vielleicht steht links im Busch ein Reh", "tokens": ["Viel\u00b7leicht", "steht", "links", "im", "Busch", "ein", "Reh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und denkt: Dort geht ein Mann.", "tokens": ["Und", "denkt", ":", "Dort", "geht", "ein", "Mann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}