{"textgrid.poem.38770": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was ist doch, fragt der Irdische, die Liebe? \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist doch, fragt der Irdische, die Liebe? \u2013", "tokens": ["Was", "ist", "doch", ",", "fragt", "der", "Ir\u00b7di\u00b7sche", ",", "die", "Lie\u00b7be", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "ADV", "$,", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr euch, ihr Armen, nur ein tief Verh\u00fcllen,", "tokens": ["F\u00fcr", "euch", ",", "ihr", "Ar\u00b7men", ",", "nur", "ein", "tief", "Ver\u00b7h\u00fcl\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein dunkler Tod im eignen Widerwillen,", "tokens": ["Ein", "dunk\u00b7ler", "Tod", "im", "eig\u00b7nen", "Wi\u00b7der\u00b7wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Aengsten, das gern stumm verschlossen bliebe.", "tokens": ["Ein", "A\u00b7engs\u00b7ten", ",", "das", "gern", "stumm", "ver\u00b7schlos\u00b7sen", "blie\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch wen anl\u00e4chelt Aug' und Mund der Liebe,", "tokens": ["Doch", "wen", "an\u00b7l\u00e4\u00b7chelt", "Aug'", "und", "Mund", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der f\u00fchlt im Herzen Wunderstr\u00f6me quillen,", "tokens": ["Der", "f\u00fchlt", "im", "Her\u00b7zen", "Wun\u00b7der\u00b7str\u00f6\u00b7me", "quil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein seelig Ahnden, niemals zu erf\u00fcllen: \u2013", "tokens": ["Ein", "see\u00b7lig", "Ahn\u00b7den", ",", "nie\u00b7mals", "zu", "er\u00b7f\u00fcl\u00b7len", ":", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ADV", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wozu, da\u00df ich den Geist im Wort beschriebe?", "tokens": ["Wo\u00b7zu", ",", "da\u00df", "ich", "den", "Geist", "im", "Wort", "be\u00b7schrie\u00b7be", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wem einmal T\u00f6ne, Lichter, Farben, Sterne", "tokens": ["Wem", "ein\u00b7mal", "T\u00f6\u00b7ne", ",", "Lich\u00b7ter", ",", "Far\u00b7ben", ",", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["PWS", "ADV", "NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geschwisterlich aufgingen, und im Bl\u00fchen", "tokens": ["Ge\u00b7schwis\u00b7ter\u00b7lich", "auf\u00b7gin\u00b7gen", ",", "und", "im", "Bl\u00fc\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVPP", "$,", "KON", "APPRART", "NN"], "meter": "-+-+++-+-+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Aus Thr\u00e4nen ihre Nahrung sog die Blume:", "tokens": ["Aus", "Thr\u00e4\u00b7nen", "ih\u00b7re", "Nah\u00b7rung", "sog", "die", "Blu\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "F\u00fchlt der in Gott ein Nahe noch und Ferne?", "tokens": ["F\u00fchlt", "der", "in", "Gott", "ein", "Na\u00b7he", "noch", "und", "Fer\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPR", "NN", "ART", "NN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mu\u00df nicht sein Herz in Ewigkeiten gl\u00fchen?", "tokens": ["Mu\u00df", "nicht", "sein", "Herz", "in", "E\u00b7wig\u00b7kei\u00b7ten", "gl\u00fc\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Antworte du, wohnend im Heiligthume.", "tokens": ["Ant\u00b7wor\u00b7te", "du", ",", "woh\u00b7nend", "im", "Hei\u00b7lig\u00b7thu\u00b7me", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "+---+--+-+-", "measure": "trochaic.tetra.relaxed"}}}}}