{"textgrid.poem.60263": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun ist es Herbst, die Bl\u00e4tter fallen,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun ist es Herbst, die Bl\u00e4tter fallen,", "tokens": ["Nun", "ist", "es", "Herbst", ",", "die", "Bl\u00e4t\u00b7ter", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Wald durchbraust des Scheidens Weh;", "tokens": ["Den", "Wald", "durch\u00b7braust", "des", "Schei\u00b7dens", "Weh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Lenz und seine Nachtigallen", "tokens": ["Den", "Lenz", "und", "sei\u00b7ne", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vers\u00e4umt ich auf der w\u00fcsten See.", "tokens": ["Ver\u00b7s\u00e4umt", "ich", "auf", "der", "w\u00fcs\u00b7ten", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Himmel schien so mild, so helle,", "tokens": ["Der", "Him\u00b7mel", "schien", "so", "mild", ",", "so", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$,", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verloren ging sein warmes Licht;", "tokens": ["Ver\u00b7lo\u00b7ren", "ging", "sein", "war\u00b7mes", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es bl\u00fchte nicht die Meereswelle,", "tokens": ["Es", "bl\u00fch\u00b7te", "nicht", "die", "Mee\u00b7res\u00b7wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die rohen Winde sangen nicht.", "tokens": ["Die", "ro\u00b7hen", "Win\u00b7de", "san\u00b7gen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und mir verging die Jugend traurig,", "tokens": ["Und", "mir", "ver\u00b7ging", "die", "Ju\u00b7gend", "trau\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Fr\u00fchlings Wonne blieb vers\u00e4umt;", "tokens": ["Des", "Fr\u00fch\u00b7lings", "Won\u00b7ne", "blieb", "ver\u00b7s\u00e4umt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Herbst durchweht mich trennungschaurig,", "tokens": ["Der", "Herbst", "durch\u00b7weht", "mich", "tren\u00b7nung\u00b7schau\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Herz dem Tod entgegentr\u00e4umt.", "tokens": ["Mein", "Herz", "dem", "Tod", "ent\u00b7ge\u00b7gen\u00b7tr\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}