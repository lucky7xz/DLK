{"textgrid.poem.62170": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[sperrt euch ein in gro\u00dfe St\u00e4dte]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sperrt euch ein in gro\u00dfe St\u00e4dte,", "tokens": ["Sperrt", "euch", "ein", "in", "gro\u00b7\u00dfe", "St\u00e4d\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Atmet ein die dicke Luft,", "tokens": ["At\u00b7met", "ein", "die", "di\u00b7cke", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die ein And'rer ausgeatmet \u2013", "tokens": ["Die", "ein", "An\u00b7d'\u00b7rer", "aus\u00b7ge\u00b7at\u00b7met", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Unbeschreiblich s\u00fc\u00dfer Duft!", "tokens": ["Un\u00b7be\u00b7schreib\u00b7lich", "s\u00fc\u00b7\u00dfer", "Duft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Brauchet dann noch eine Kur,", "tokens": ["Brau\u00b7chet", "dann", "noch", "ei\u00b7ne", "Kur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine Morphium-Mixtur,", "tokens": ["Ei\u00b7ne", "Mor\u00b7phi\u00b7um\u00b7Mix\u00b7tur", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ihr bauet eine Kluft", "tokens": ["Und", "ihr", "bau\u00b7et", "ei\u00b7ne", "Kluft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Zwischen euch und der Natur:", "tokens": ["Zwi\u00b7schen", "euch", "und", "der", "Na\u00b7tur", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Ach, ihr bauet eure Gruft!", "tokens": ["Ach", ",", "ihr", "bau\u00b7et", "eu\u00b7re", "Gruft", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}