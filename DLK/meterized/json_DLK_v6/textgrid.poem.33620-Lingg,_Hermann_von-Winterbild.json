{"textgrid.poem.33620": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Winterbild", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum Forste geht des Raben Flug.", "tokens": ["Zum", "Fors\u00b7te", "geht", "des", "Ra\u00b7ben", "Flug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Fuchs, der auf den Raub sich wagte,", "tokens": ["Ein", "Fuchs", ",", "der", "auf", "den", "Raub", "sich", "wag\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "APPR", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der k\u00fchn sich durch die Hunde schlug,", "tokens": ["Der", "k\u00fchn", "sich", "durch", "die", "Hun\u00b7de", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erfroren lag er, als es tagte.", "tokens": ["Er\u00b7fro\u00b7ren", "lag", "er", ",", "als", "es", "tag\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er hatte schon den Wald erblickt,", "tokens": ["Er", "hat\u00b7te", "schon", "den", "Wald", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der sicher ihn geborgen h\u00e4tte,", "tokens": ["Der", "si\u00b7cher", "ihn", "ge\u00b7bor\u00b7gen", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da hat der Schneefall ihn erstickt,", "tokens": ["Da", "hat", "der", "Schnee\u00b7fall", "ihn", "er\u00b7stickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So nahe schon der Lagerst\u00e4tte.", "tokens": ["So", "na\u00b7he", "schon", "der", "La\u00b7ger\u00b7st\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sonst hat er hier manch junges Reh", "tokens": ["Sonst", "hat", "er", "hier", "manch", "jun\u00b7ges", "Reh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Hals erwischt und totgebissen,", "tokens": ["Am", "Hals", "er\u00b7wischt", "und", "tot\u00b7ge\u00b7bis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wildenten gab es auch im See,", "tokens": ["Wil\u00b7den\u00b7ten", "gab", "es", "auch", "im", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Davon er manche hat zerrissen.", "tokens": ["Da\u00b7von", "er", "man\u00b7che", "hat", "zer\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PIS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie sp\u00fcrt noch aus dem Schnee so klug", "tokens": ["Wie", "sp\u00fcrt", "noch", "aus", "dem", "Schnee", "so", "klug"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "APPR", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Schnauze vor! Doch n\u00e4her kreisen", "tokens": ["Die", "Schnau\u00b7ze", "vor", "!", "Doch", "n\u00e4\u00b7her", "krei\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$.", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Raben schon in raschem Flug,", "tokens": ["Die", "Ra\u00b7ben", "schon", "in", "ra\u00b7schem", "Flug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den alten Reinhart aufzuspeisen.", "tokens": ["Den", "al\u00b7ten", "Rein\u00b7hart", "auf\u00b7zu\u00b7spei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}