{"textgrid.poem.38874": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sch\u00f6n ist's, wie Berge auf zum Himmel steigen,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n ist's, wie Berge auf zum Himmel steigen,", "tokens": ["Sch\u00f6n", "ist's", ",", "wie", "Ber\u00b7ge", "auf", "zum", "Him\u00b7mel", "stei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PWAV", "NN", "APPR", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie sich der Strom im ewgen Leben reget,", "tokens": ["Wie", "sich", "der", "Strom", "im", "ew\u00b7gen", "Le\u00b7ben", "re\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der laute Sturm mit seinen Fl\u00fcgeln schl\u00e4get,", "tokens": ["Der", "lau\u00b7te", "Sturm", "mit", "sei\u00b7nen", "Fl\u00fc\u00b7geln", "schl\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der gr\u00fcne Wald mit seinen dunkeln Schweigen.", "tokens": ["Der", "gr\u00fc\u00b7ne", "Wald", "mit", "sei\u00b7nen", "dun\u00b7keln", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Noch schauer, wann sich rothe Flammen zeigen,", "tokens": ["Noch", "schau\u00b7er", ",", "wann", "sich", "ro\u00b7the", "Flam\u00b7men", "zei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "PRF", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Sonnenkranz im Schimmer sich beweget,", "tokens": ["Der", "Son\u00b7nen\u00b7kranz", "im", "Schim\u00b7mer", "sich", "be\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Roth-brennend auf den Meeresspiegel leget,", "tokens": ["Ro\u00b7th\u00b7bren\u00b7nend", "auf", "den", "Mee\u00b7res\u00b7spie\u00b7gel", "le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Gl\u00fchwolken sich zu seinen F\u00fc\u00dfen beugen.", "tokens": ["Gl\u00fch\u00b7wol\u00b7ken", "sich", "zu", "sei\u00b7nen", "F\u00fc\u00b7\u00dfen", "beu\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie sind geheimni\u00dfvolle Hieroglyphen.", "tokens": ["Sie", "sind", "ge\u00b7heim\u00b7ni\u00df\u00b7vol\u00b7le", "Hie\u00b7ro\u00b7gly\u00b7phen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Ein stilles Wunder wei\u00df ich noch zu nennen,", "tokens": ["Ein", "stil\u00b7les", "Wun\u00b7der", "wei\u00df", "ich", "noch", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du kennst die Sage vom Karfunkelsteine,", "tokens": ["Du", "kennst", "die", "Sa\u00b7ge", "vom", "Kar\u00b7fun\u00b7kel\u00b7stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "De\u00df Strahlen auch entfernt vom Sonnenscheine,", "tokens": ["De\u00df", "Strah\u00b7len", "auch", "ent\u00b7fernt", "vom", "Son\u00b7nen\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Magisch mit eignem innern Feuer brennen,", "tokens": ["Ma\u00b7gisch", "mit", "eig\u00b7nem", "in\u00b7nern", "Feu\u00b7er", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo sonst kein Licht wohnt, in der Erde Tiefen.", "tokens": ["Wo", "sonst", "kein", "Licht", "wohnt", ",", "in", "der", "Er\u00b7de", "Tie\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "NN", "VVFIN", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}