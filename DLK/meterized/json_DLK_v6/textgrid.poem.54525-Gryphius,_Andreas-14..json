{"textgrid.poem.54525": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach Sch\u00f6pffer! Vater Ach! der du mich mit dem Tod/", "tokens": ["Ach", "Sch\u00f6pf\u00b7fer", "!", "Va\u00b7ter", "Ach", "!", "der", "du", "mich", "mit", "dem", "Tod", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "NN", "ITJ", "$.", "ART", "PPER", "PRF", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als unser S\u00fcnden Straff in Adam hast beschweret;", "tokens": ["Als", "un\u00b7ser", "S\u00fcn\u00b7den", "Straff", "in", "A\u00b7dam", "hast", "be\u00b7schwe\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der du mir hast die Straff in ein Geschenck verkehret/", "tokens": ["Der", "du", "mir", "hast", "die", "Straff", "in", "ein", "Ge\u00b7schenck", "ver\u00b7keh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als Jesus dich vers\u00f6hnt durch seine Sterbens-Noth.", "tokens": ["Als", "Je\u00b7sus", "dich", "ver\u00b7s\u00f6hnt", "durch", "sei\u00b7ne", "Ster\u00b7bens\u00b7Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein Herr/ durch welchen mir der Todt ein Lebens-Bot/", "tokens": ["Mein", "Herr", "/", "durch", "wel\u00b7chen", "mir", "der", "Todt", "ein", "Le\u00b7bens\u00b7Bot", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "APPR", "PWAT", "PPER", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gib/ wenn ich auf der Welt den Rest der Zeit verzehret/", "tokens": ["Gib", "/", "wenn", "ich", "auf", "der", "Welt", "den", "Rest", "der", "Zeit", "ver\u00b7zeh\u00b7ret", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "KOUS", "PPER", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Gib dem/ vor den dein Kind sich in den Tod gewehret/", "tokens": ["Gib", "dem", "/", "vor", "den", "dein", "Kind", "sich", "in", "den", "Tod", "ge\u00b7weh\u00b7ret", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "$(", "APPR", "ART", "PPOSAT", "NN", "PRF", "APPR", "ART", "NN", "VVPP", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Nur einen Gnaden-Blick/ unendlich guter Gott.", "tokens": ["Nur", "ei\u00b7nen", "Gna\u00b7den\u00b7Blick", "/", "un\u00b7end\u00b7lich", "gu\u00b7ter", "Gott", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$(", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hast du aus Mutterleib ins Licht mich leiten k\u00f6nnen;", "tokens": ["Hast", "du", "aus", "Mut\u00b7ter\u00b7leib", "ins", "Licht", "mich", "lei\u00b7ten", "k\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "APPRART", "NN", "PPER", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Warum denn woltest du nicht H\u00fclff und Beystand g\u00f6nnen?", "tokens": ["Wa\u00b7rum", "denn", "wol\u00b7test", "du", "nicht", "H\u00fclff", "und", "Beys\u00b7tand", "g\u00f6n\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "PTKNEG", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wenn die beklemmte Seel ins neue Leben ringt.", "tokens": ["Wenn", "die", "be\u00b7klemm\u00b7te", "Seel", "ins", "neu\u00b7e", "Le\u00b7ben", "ringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dir war es leicht aus nichts mich/ Sch\u00f6pffer/ vorzubringen/", "tokens": ["Dir", "war", "es", "leicht", "aus", "nichts", "mich", "/", "Sch\u00f6pf\u00b7fer", "/", "vor\u00b7zu\u00b7brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "APPR", "PIS", "PPER", "$(", "NN", "$(", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wie k\u00f6nte es dir den zu retten mi\u00dfgelingen/", "tokens": ["Wie", "k\u00f6n\u00b7te", "es", "dir", "den", "zu", "ret\u00b7ten", "mi\u00df\u00b7ge\u00b7lin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPER", "ART", "PTKZU", "VVINF", "VVINF", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Der voll Vertrauens sich/ Herr/ in dein Armen schwingt.", "tokens": ["Der", "voll", "Ver\u00b7trau\u00b7ens", "sich", "/", "Herr", "/", "in", "dein", "Ar\u00b7men", "schwingt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PRF", "$(", "NN", "$(", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}