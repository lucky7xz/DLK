{"dta.poem.20638": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Auf  \n  die Geburt  \n des  \n  jungen Prinzen von Preussen  \n K\u00f6nigl. Hoheit .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Meine Seele taumelt, nicht berauscht vom Weine,                 ", "tokens": ["Mei\u00b7ne", "See\u00b7le", "tau\u00b7melt", ",", "nicht", "be\u00b7rauscht", "vom", "Wei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PTKNEG", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Im bemoosten Fasse hergebracht vom Rheine,", "tokens": ["Im", "be\u00b7moos\u00b7ten", "Fas\u00b7se", "her\u00b7ge\u00b7bracht", "vom", "Rhei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "APPRART", "NE", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Oder \u00fcber\u2019s Meer gesandt;", "tokens": ["O\u00b7der", "\u00fc\u00b7ber's", "Meer", "ge\u00b7sandt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich bin wonnetrunken, mich erf\u00fcllen deine", "tokens": ["Ich", "bin", "won\u00b7ne\u00b7trun\u00b7ken", ",", "mich", "er\u00b7f\u00fcl\u00b7len", "dei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVINF", "$,", "PPER", "VVFIN", "PPOSAT"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Freuden \u2014 liebes Vaterland!", "tokens": ["Freu\u00b7den", "lie\u00b7bes", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Alle Kinder jauchzen, alle Greise gl\u00fchen;", "tokens": ["Al\u00b7le", "Kin\u00b7der", "jauch\u00b7zen", ",", "al\u00b7le", "Grei\u00b7se", "gl\u00fc\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$,", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Friedrich Dein Erhalter, wiegt auf seinen Knieen                 ", "tokens": ["Fried\u00b7rich", "Dein", "Er\u00b7hal\u00b7ter", ",", "wiegt", "auf", "sei\u00b7nen", "Kni\u00b7e\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Diesen K\u00f6niglichen Sohn,", "tokens": ["Die\u00b7sen", "K\u00f6\u00b7nig\u00b7li\u00b7chen", "Sohn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Den Er dir zum Herrscher weislich wird erziehen,", "tokens": ["Den", "Er", "dir", "zum", "Herr\u00b7scher", "weis\u00b7lich", "wird", "er\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "APPRART", "NN", "ADV", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Und der Z\u00f6gling horchet schon.", "tokens": ["Und", "der", "Z\u00f6g\u00b7ling", "hor\u00b7chet", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u201eliebling meines Herzens, spricht der ", "tokens": ["\u201e", "lieb\u00b7ling", "mei\u00b7nes", "Her\u00b7zens", ",", "spricht", "der"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u201ewie der m\u00fcde Wandrer schmachtend Trank und", "tokens": ["\u201e", "wie", "der", "m\u00fc\u00b7de", "Wand\u00b7rer", "schmach\u00b7tend", "Trank", "und"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "ART", "ADJA", "NN", "ADJD", "NN", "KON"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Speise,", "tokens": ["Spei\u00b7se", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "\u201ewie der Steuermann den Rand", "tokens": ["\u201e", "wie", "der", "Steu\u00b7er\u00b7mann", "den", "Rand"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u201etiefer Fluthen w\u00fcnschet auf der weiten Reise;", "tokens": ["\u201e", "tie\u00b7fer", "Flut\u00b7hen", "w\u00fcn\u00b7schet", "auf", "der", "wei\u00b7ten", "Rei\u00b7se", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "\u201ealso w\u00fcnschte Dich das Land.", "tokens": ["\u201e", "al\u00b7so", "w\u00fcnschte", "Dich", "das", "Land", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "\u201eheil mir, da\u00df Du kamest, Heil sey Deiner keuschen", "tokens": ["\u201e", "heil", "mir", ",", "da\u00df", "Du", "ka\u00b7mest", ",", "Heil", "sey", "Dei\u00b7ner", "keu\u00b7schen"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,", "NN", "VAFIN", "PPOSAT", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "\u201e", "tokens": ["\u201e"], "token_info": ["punct"], "pos": ["$("]}, "line.3": {"text": "heischen", "tokens": ["hei\u00b7schen"], "token_info": ["word"], "pos": ["ADJA"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "\u201emeines Volkes hat gestillt.", "tokens": ["\u201e", "mei\u00b7nes", "Vol\u00b7kes", "hat", "ge\u00b7stillt", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u201edu wirst meine Hofnung nimmer, nimmer t\u00e4uschen;", "tokens": ["\u201e", "du", "wirst", "mei\u00b7ne", "Hof\u00b7nung", "nim\u00b7mer", ",", "nim\u00b7mer", "t\u00e4u\u00b7schen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPOSAT", "NN", "ADV", "$,", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "\u201esie wird ganz in Dir erf\u00fcllt.", "tokens": ["\u201e", "sie", "wird", "ganz", "in", "Dir", "er\u00b7f\u00fcllt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u201efr\u00fch wirst Du erkennen, da\u00df man auf der Erde,", "tokens": ["\u201e", "fr\u00fch", "wirst", "Du", "er\u00b7ken\u00b7nen", ",", "da\u00df", "man", "auf", "der", "Er\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "PPER", "VVINF", "$,", "KOUS", "PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "\u201edurch die Tugend jenem Herrscher \u00e4hnlich werde,", "tokens": ["\u201e", "durch", "die", "Tu\u00b7gend", "je\u00b7nem", "Herr\u00b7scher", "\u00e4hn\u00b7lich", "wer\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "PDAT", "NN", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "\u201edessen Herrschaft ewig ist;", "tokens": ["\u201e", "des\u00b7sen", "Herr\u00b7schaft", "e\u00b7wig", "ist", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRELAT", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u201eund da\u00df Du dem Hirten bey der kleinsten Heerde", "tokens": ["\u201e", "und", "da\u00df", "Du", "dem", "Hir\u00b7ten", "bey", "der", "kleins\u00b7ten", "Heer\u00b7de"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "KOUS", "PPER", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "\u201edeine G\u00fcte schuldig bist.", "tokens": ["\u201e", "dei\u00b7ne", "G\u00fc\u00b7te", "schul\u00b7dig", "bist", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "\u201edeine h\u00f6chste Wollust wirst Du mit Entz\u00fccken", "tokens": ["\u201e", "dei\u00b7ne", "h\u00f6chs\u00b7te", "Wol\u00b7lust", "wirst", "Du", "mit", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "\u201ein der Uebung finden, Menschen zu begl\u00fccken,", "tokens": ["\u201e", "in", "der", "Ue\u00b7bung", "fin\u00b7den", ",", "Men\u00b7schen", "zu", "be\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "VVINF", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "\u201eund daf\u00fcr geliebt zu seyn.", "tokens": ["\u201e", "und", "da\u00b7f\u00fcr", "ge\u00b7liebt", "zu", "seyn", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PAV", "VVPP", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u201ekeinem als dem Schmeichler wirst Du zornig blicken,", "tokens": ["\u201e", "kei\u00b7nem", "als", "dem", "Schmeich\u00b7ler", "wirst", "Du", "zor\u00b7nig", "bli\u00b7cken", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "KOKOM", "ART", "NN", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "\u201eund ihm nie Dein Ohr verleihn.\u201e", "tokens": ["\u201e", "und", "ihm", "nie", "Dein", "Ohr", "ver\u00b7leihn", ".", "\u201e"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Also redet ", "tokens": ["Al\u00b7so", "re\u00b7det"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Diese Stirne, welche dermaleinst wird leuchten", "tokens": ["Die\u00b7se", "Stir\u00b7ne", ",", "wel\u00b7che", "der\u00b7ma\u00b7leinst", "wird", "leuch\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELS", "ADV", "VAFIN", "VVINF"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Ueber uns voll Gnad und Huld.", "tokens": ["Ue\u00b7ber", "uns", "voll", "Gnad", "und", "Huld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wohl uns, da\u00df wir unsrer W\u00fcnsche Ziel erreichten,", "tokens": ["Wohl", "uns", ",", "da\u00df", "wir", "uns\u00b7rer", "W\u00fcn\u00b7sche", "Ziel", "er\u00b7reich\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "KOUS", "PPER", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Nach so langer Ungeduld.", "tokens": ["Nach", "so", "lan\u00b7ger", "Un\u00b7ge\u00b7duld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Die verlebten M\u00e4nner nebst den grauen M\u00fcttern,", "tokens": ["Die", "ver\u00b7leb\u00b7ten", "M\u00e4n\u00b7ner", "nebst", "den", "grau\u00b7en", "M\u00fct\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Sprechen: \u201eWohl Euch Enkel! Eure Kinder zittern", "tokens": ["Spre\u00b7chen", ":", "\u201e", "Wohl", "Euch", "En\u00b7kel", "!", "Eu\u00b7re", "Kin\u00b7der", "zit\u00b7tern"], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "$(", "ADV", "PPER", "NN", "$.", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "\u201enie vor dem Erobrungsgeist!", "tokens": ["\u201e", "nie", "vor", "dem", "E\u00b7rob\u00b7rungs\u00b7ge\u00b7ist", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "\u201ekeine Donner werden diesen Thron ersch\u00fcttern!", "tokens": ["\u201e", "kei\u00b7ne", "Don\u00b7ner", "wer\u00b7den", "die\u00b7sen", "Thron", "er\u00b7sch\u00fct\u00b7tern", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "VAFIN", "PDAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "\u201e", "tokens": ["\u201e"], "token_info": ["punct"], "pos": ["$("]}}, "stanza.9": {"line.1": {"text": "\u201et\u00f6chter, streuet Blumen, bringet Opfergaben", "tokens": ["\u201e", "t\u00f6ch\u00b7ter", ",", "streu\u00b7et", "Blu\u00b7men", ",", "brin\u00b7get", "Op\u00b7fer\u00b7ga\u00b7ben"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADJD", "$,", "VVFIN", "NN", "$,", "VVFIN", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "\u201eum die goldne Wiege; kleine muntre Knaben", "tokens": ["\u201e", "um", "die", "gold\u00b7ne", "Wie\u00b7ge", ";", "klei\u00b7ne", "mun\u00b7tre", "Kna\u00b7ben"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "APPR", "ART", "ADJA", "NN", "$.", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "\u201emacht ein Singechor, und sprecht:", "tokens": ["\u201e", "macht", "ein", "Sin\u00b7ge\u00b7chor", ",", "und", "sprecht", ":"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u201each! Du sollst zum Opfer unsre Herzen haben,", "tokens": ["\u201e", "ach", "!", "Du", "sollst", "zum", "Op\u00b7fer", "uns\u00b7re", "Her\u00b7zen", "ha\u00b7ben", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$.", "PPER", "VMFIN", "APPRART", "NN", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "\u201e", "tokens": ["\u201e"], "token_info": ["punct"], "pos": ["$("]}}}}}