{"dta.poem.10570": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "V.  \n  Der Gesang der Okeaniden .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Abendlich blasser wird es am Meere,", "tokens": ["A\u00b7bend\u00b7lich", "blas\u00b7ser", "wird", "es", "am", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und einsam, mit seiner einsamen Seele,", "tokens": ["Und", "ein\u00b7sam", ",", "mit", "sei\u00b7ner", "ein\u00b7sa\u00b7men", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sitzt dort ein Mann auf dem kahlen Strand,", "tokens": ["Sitzt", "dort", "ein", "Mann", "auf", "dem", "kah\u00b7len", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und schaut, todtkalten Blickes, hinauf", "tokens": ["Und", "schaut", ",", "todt\u00b7kal\u00b7ten", "Bli\u00b7ckes", ",", "hin\u00b7auf"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "$,", "ADJA", "NN", "$,", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Nach der weiten, todtkalten Himmelsw\u00f6lbung,", "tokens": ["Nach", "der", "wei\u00b7ten", ",", "todt\u00b7kal\u00b7ten", "Him\u00b7mels\u00b7w\u00f6l\u00b7bung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Und schaut auf das weite, wogende Meer,", "tokens": ["Und", "schaut", "auf", "das", "wei\u00b7te", ",", "wo\u00b7gen\u00b7de", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und \u00fcber das weite, wogende Meer,", "tokens": ["Und", "\u00fc\u00b7ber", "das", "wei\u00b7te", ",", "wo\u00b7gen\u00b7de", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Wie L\u00fcftesegler, ziehn seine Seufzer,", "tokens": ["Wie", "L\u00fcf\u00b7te\u00b7seg\u00b7ler", ",", "ziehn", "sei\u00b7ne", "Seuf\u00b7zer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Und kehren wieder, tr\u00fcbselig,", "tokens": ["Und", "keh\u00b7ren", "wie\u00b7der", ",", "tr\u00fcb\u00b7se\u00b7lig", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und hatten verschlossen gefunden das Herz,", "tokens": ["Und", "hat\u00b7ten", "ver\u00b7schlos\u00b7sen", "ge\u00b7fun\u00b7den", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "VVPP", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.11": {"text": "Worin sie ankern wollten \u2014", "tokens": ["Wo\u00b7rin", "sie", "an\u00b7kern", "woll\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Und er st\u00f6hnt so laut, da\u00df die wei\u00dfen M\u00f6ven,", "tokens": ["Und", "er", "st\u00f6hnt", "so", "laut", ",", "da\u00df", "die", "wei\u00b7\u00dfen", "M\u00f6\u00b7ven", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Aufgescheucht aus den sandigen Nestern,", "tokens": ["Auf\u00b7ge\u00b7scheucht", "aus", "den", "san\u00b7di\u00b7gen", "Nes\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.14": {"text": "Ihn heerdenweis' umflattern,", "tokens": ["Ihn", "he\u00b7er\u00b7den\u00b7weis'", "um\u00b7flat\u00b7tern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Und er spricht zu ihnen die lachenden Worte:", "tokens": ["Und", "er", "spricht", "zu", "ih\u00b7nen", "die", "la\u00b7chen\u00b7den", "Wor\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Schwarzbeinigte V\u00f6gel,", "tokens": ["Schwarz\u00b7bei\u00b7nig\u00b7te", "V\u00f6\u00b7gel", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Mit wei\u00dfen Fl\u00fcgeln Meer-\u00fcberflatternde,", "tokens": ["Mit", "wei\u00b7\u00dfen", "Fl\u00fc\u00b7geln", "Meer\u00b7\u00fcber\u00b7flat\u00b7tern\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Mit krummen Schn\u00e4beln Seewasser-saufende\u201a", "tokens": ["Mit", "krum\u00b7men", "Schn\u00e4\u00b7beln", "See\u00b7was\u00b7ser\u00b7sau\u00b7fen\u00b7de", "\u201a"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und thranigtes Robbenfleisch-fressende,", "tokens": ["Und", "thra\u00b7nig\u00b7tes", "Rob\u00b7ben\u00b7fleischfres\u00b7sen\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Eu'r Leben ist bitter wie Eure Nahrung!", "tokens": ["Eu'r", "Le\u00b7ben", "ist", "bit\u00b7ter", "wie", "Eu\u00b7re", "Nah\u00b7rung", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Ich aber, der Gl\u00fcckliche, koste nur S\u00fc\u00dfes!", "tokens": ["Ich", "a\u00b7ber", ",", "der", "Gl\u00fcck\u00b7li\u00b7che", ",", "kos\u00b7te", "nur", "S\u00fc\u00b7\u00dfes", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ART", "NN", "$,", "VVFIN", "ADV", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Ich koste den s\u00fc\u00dfen Duft der Rose,", "tokens": ["Ich", "kos\u00b7te", "den", "s\u00fc\u00b7\u00dfen", "Duft", "der", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Der Mondschein-gef\u00fctterten Nachtigallbraut;", "tokens": ["Der", "Mond\u00b7schein\u00b7ge\u00b7f\u00fct\u00b7ter\u00b7ten", "Nach\u00b7ti\u00b7gall\u00b7braut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "Ich koste noch s\u00fc\u00dfere Josty-Baisers,", "tokens": ["Ich", "kos\u00b7te", "noch", "s\u00fc\u00b7\u00dfe\u00b7re", "Jos\u00b7ty\u00b7Bai\u00b7sers", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Mit wei\u00dfer Seligkeit gef\u00fcllte;", "tokens": ["Mit", "wei\u00b7\u00dfer", "Se\u00b7lig\u00b7keit", "ge\u00b7f\u00fcll\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und das Allers\u00fc\u00dfeste kost' ich:", "tokens": ["Und", "das", "Al\u00b7ler\u00b7s\u00fc\u00b7\u00dfes\u00b7te", "kost'", "ich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "PPER", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "S\u00fc\u00dfe Liebe und s\u00fc\u00dfes Geliebtseyn.", "tokens": ["S\u00fc\u00b7\u00dfe", "Lie\u00b7be", "und", "s\u00fc\u00b7\u00dfes", "Ge\u00b7liebt\u00b7seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Sie liebt mich! Sie liebt mich! die holde Jungfrau!", "tokens": ["Sie", "liebt", "mich", "!", "Sie", "liebt", "mich", "!", "die", "hol\u00b7de", "Jung\u00b7frau", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "PPER", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Jetzt steht sie daheim, am Erker des Hauses,", "tokens": ["Jetzt", "steht", "sie", "da\u00b7heim", ",", "am", "Er\u00b7ker", "des", "Hau\u00b7ses", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und schaut in die D\u00e4mm'rung hinaus, auf die Landstra\u00df',", "tokens": ["Und", "schaut", "in", "die", "D\u00e4m\u00b7m'\u00b7rung", "hin\u00b7aus", ",", "auf", "die", "Land\u00b7stra\u00df'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "APZR", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und horcht, und sehnt sich nach mir \u2014 wahrhaftig!", "tokens": ["Und", "horcht", ",", "und", "sehnt", "sich", "nach", "mir", "wahr\u00b7haf\u00b7tig", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "VVFIN", "PRF", "APPR", "PPER", "$(", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vergebens sp\u00e4ht sie umher und sie seufzet,", "tokens": ["Ver\u00b7ge\u00b7bens", "sp\u00e4ht", "sie", "um\u00b7her", "und", "sie", "seuf\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und seufzend steigt sie hinab in den Garten,", "tokens": ["Und", "seuf\u00b7zend", "steigt", "sie", "hin\u00b7ab", "in", "den", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und wandelt in Duft und Mondschein,", "tokens": ["Und", "wan\u00b7delt", "in", "Duft", "und", "Mond\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und spricht mit den Blumen, erz\u00e4hlet ihnen:", "tokens": ["Und", "spricht", "mit", "den", "Blu\u00b7men", ",", "er\u00b7z\u00e4h\u00b7let", "ih\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Wie ich, der Geliebte, so lieblich bin", "tokens": ["Wie", "ich", ",", "der", "Ge\u00b7lieb\u00b7te", ",", "so", "lieb\u00b7lich", "bin"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "$,", "ART", "NN", "$,", "ADV", "ADJD", "VAFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Und so liebensw\u00fcrdig \u2014 wahrhaftig!", "tokens": ["Und", "so", "lie\u00b7bens\u00b7w\u00fcr\u00b7dig", "wahr\u00b7haf\u00b7tig", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$(", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Nachher im Bette, im Schlafe, im Traum,", "tokens": ["Nach\u00b7her", "im", "Bet\u00b7te", ",", "im", "Schla\u00b7fe", ",", "im", "Traum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.12": {"text": "Umgaukelt sie selig mein theures Bild,", "tokens": ["Um\u00b7gau\u00b7kelt", "sie", "se\u00b7lig", "mein", "theu\u00b7res", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Sogar des Morgens, beim Fr\u00fchst\u00fcck,", "tokens": ["So\u00b7gar", "des", "Mor\u00b7gens", ",", "beim", "Fr\u00fch\u00b7st\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "ADV", "$,", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Auf dem gl\u00e4nzenden Butterbrodte,", "tokens": ["Auf", "dem", "gl\u00e4n\u00b7zen\u00b7den", "But\u00b7ter\u00b7brod\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Sieht sie mein l\u00e4chelndes Antlitz,", "tokens": ["Sieht", "sie", "mein", "l\u00e4\u00b7cheln\u00b7des", "Ant\u00b7litz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Und sie fri\u00dft es auf vor Liebe \u2014 wahrhaftig!", "tokens": ["Und", "sie", "fri\u00dft", "es", "auf", "vor", "Lie\u00b7be", "wahr\u00b7haf\u00b7tig", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "APPR", "NN", "$(", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Also prahlt er und prahlt er,", "tokens": ["Al\u00b7so", "prahlt", "er", "und", "prahlt", "er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Und zwischendrein schrillen die M\u00f6ven,", "tokens": ["Und", "zwi\u00b7schen\u00b7drein", "schril\u00b7len", "die", "M\u00f6\u00b7ven", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wie kaltes, ironisches Kichern;", "tokens": ["Wie", "kal\u00b7tes", ",", "i\u00b7ron\u00b7i\u00b7sches", "Ki\u00b7chern", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die D\u00e4mm'rungsnebel steigen herauf;", "tokens": ["Die", "D\u00e4m\u00b7m'\u00b7rungs\u00b7ne\u00b7bel", "stei\u00b7gen", "her\u00b7auf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Aus violettem Gew\u00f6lk, unheimlich,", "tokens": ["Aus", "vi\u00b7o\u00b7let\u00b7tem", "Ge\u00b7w\u00f6lk", ",", "un\u00b7heim\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Schaut hervor der grasgelbe Mond;", "tokens": ["Schaut", "her\u00b7vor", "der", "gras\u00b7gel\u00b7be", "Mond", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "AR