{"dta.poem.18168": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Das Schiksahl der Kirche JE-  \n su und der ersten Christen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Angst, Verfolgung, Marter, Tod", "tokens": ["Angst", ",", "Ver\u00b7fol\u00b7gung", ",", "Mar\u00b7ter", ",", "Tod"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist der Lohn der ersten Christen;", "tokens": ["Ist", "der", "Lohn", "der", "ers\u00b7ten", "Chris\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn sie sich auf GOtts Gebot", "tokens": ["Wenn", "sie", "sich", "auf", "Gotts", "Ge\u00b7bot"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NE", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wieder H\u00f6ll und Teufel r\u00fcsten.", "tokens": ["Wie\u00b7der", "H\u00f6ll", "und", "Teu\u00b7fel", "r\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Christi Kirche schwimmt im Blut", "tokens": ["Chris\u00b7ti", "Kir\u00b7che", "schwimmt", "im", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "We", "tokens": ["We"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.7": {"t