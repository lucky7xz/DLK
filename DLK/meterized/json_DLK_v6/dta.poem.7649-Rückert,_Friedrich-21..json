{"dta.poem.7649": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wozu begehrst du Gut, mehr als du hast, und Ehre?", "tokens": ["Wo\u00b7zu", "be\u00b7gehrst", "du", "Gut", ",", "mehr", "als", "du", "hast", ",", "und", "Eh\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "$,", "ADV", "KOUS", "PPER", "VAFIN", "$,", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie? da\u00df es dir dein Gl\u00fcck, dein innres Wohlseyn mehre?", "tokens": ["Wie", "?", "da\u00df", "es", "dir", "dein", "Gl\u00fcck", ",", "dein", "inn\u00b7res", "Wohl\u00b7seyn", "meh\u00b7re", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KOUS", "PPER", "PPER", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gut, Ehre, such' ich nicht, damit ich schwelg' in ihnen,", "tokens": ["Gut", ",", "Eh\u00b7re", ",", "such'", "ich", "nicht", ",", "da\u00b7mit", "ich", "schwel\u00b7g'", "in", "ih\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Als Mittel such' ich sie, die meinem Zwecke dienen,", "tokens": ["Als", "Mit\u00b7tel", "such'", "ich", "sie", ",", "die", "mei\u00b7nem", "Zwe\u00b7cke", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "PPER", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zu sch\u00e4rfen gl\u00e4nzender des Selbstgef\u00fchles Waffen,", "tokens": ["Zu", "sch\u00e4r\u00b7fen", "gl\u00e4n\u00b7zen\u00b7der", "des", "Selbst\u00b7ge\u00b7f\u00fch\u00b7les", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um Sch\u00f6nes, meine Lust, nachdr\u00fccklicher zu schaffen.", "tokens": ["Um", "Sch\u00f6\u00b7nes", ",", "mei\u00b7ne", "Lust", ",", "nach\u00b7dr\u00fcck\u00b7li\u00b7cher", "zu", "schaf\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "$,", "PPOSAT", "NN", "$,", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}