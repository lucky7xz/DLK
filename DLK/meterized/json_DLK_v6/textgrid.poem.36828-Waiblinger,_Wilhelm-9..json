{"textgrid.poem.36828": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Einsamkeit, wo ihre schweren S\u00fcnden", "tokens": ["O", "Ein\u00b7sam\u00b7keit", ",", "wo", "ih\u00b7re", "schwe\u00b7ren", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PWAV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Weltbeherrschers Tochter einst beweinte,", "tokens": ["Des", "Welt\u00b7be\u00b7herr\u00b7schers", "Toch\u00b7ter", "einst", "be\u00b7wein\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie l\u00e4\u00df'st du ganz mich ihre Qual empfinden!", "tokens": ["Wie", "l\u00e4\u00df'st", "du", "ganz", "mich", "ih\u00b7re", "Qual", "emp\u00b7fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die einst ihr der Verbannten Haus umgrauet,", "tokens": ["Die", "einst", "ihr", "der", "Ver\u00b7bann\u00b7ten", "Haus", "um\u00b7grau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schreckbare Felsen, deren kahle Wildni\u00df", "tokens": ["Schreck\u00b7ba\u00b7re", "Fel\u00b7sen", ",", "de\u00b7ren", "kah\u00b7le", "Wild\u00b7ni\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELAT", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Ins \u00f6de gr\u00fcne Meer hinunterschauet,", "tokens": ["Ins", "\u00f6\u00b7de", "gr\u00fc\u00b7ne", "Meer", "hin\u00b7un\u00b7ter\u00b7schau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Verla\u00dfner Strand, wo nur die Woge brandet,", "tokens": ["Ver\u00b7la\u00df\u00b7ner", "Strand", ",", "wo", "nur", "die", "Wo\u00b7ge", "bran\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo an der hochumrauschten Fischerh\u00fctte", "tokens": ["Wo", "an", "der", "hoch\u00b7um\u00b7rauschten", "Fi\u00b7scher\u00b7h\u00fct\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Schon lange Boot und Kahn nicht mehr gelandet;", "tokens": ["Schon", "lan\u00b7ge", "Boot", "und", "Kahn", "nicht", "mehr", "ge\u00b7lan\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ihr bargt ein Herz, in Sinnenlust verwildert,", "tokens": ["Ihr", "bargt", "ein", "Herz", ",", "in", "Sin\u00b7nen\u00b7lust", "ver\u00b7wil\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Qualen einer Leidenschaft durchw\u00fchlet,", "tokens": ["Von", "Qua\u00b7len", "ei\u00b7ner", "Lei\u00b7den\u00b7schaft", "durch\u00b7w\u00fch\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie keines Byrons Schmerz sie noch geschildert.", "tokens": ["Wie", "kei\u00b7nes", "By\u00b7rons", "Schmerz", "sie", "noch", "ge\u00b7schil\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "NE", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Leicht ist dem besten Herzen ein Verbrechen,", "tokens": ["Leicht", "ist", "dem", "bes\u00b7ten", "Her\u00b7zen", "ein", "Ver\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sobald es liebt, noch leichter ist's dem kalten", "tokens": ["So\u00b7bald", "es", "liebt", ",", "noch", "leich\u00b7ter", "ist's", "dem", "kal\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ADV", "ADJD", "VAFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fchllosen Zorn zu strafen und zu r\u00e4chen.", "tokens": ["F\u00fchl\u00b7lo\u00b7sen", "Zorn", "zu", "stra\u00b7fen", "und", "zu", "r\u00e4\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Schnell ist die strenge Welt bereit zu richten,", "tokens": ["Schnell", "ist", "die", "stren\u00b7ge", "Welt", "be\u00b7reit", "zu", "rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weil sie ein flammendes Gef\u00fchl der Liebe", "tokens": ["Weil", "sie", "ein", "flam\u00b7men\u00b7des", "Ge\u00b7f\u00fchl", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht schaffen kann, so will sie's doch zernichten.", "tokens": ["Nicht", "schaf\u00b7fen", "kann", ",", "so", "will", "sie's", "doch", "zer\u00b7nich\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "$,", "ADV", "VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "O Julia, la\u00df mich theilen deine Thr\u00e4nen,", "tokens": ["O", "Ju\u00b7lia", ",", "la\u00df", "mich", "thei\u00b7len", "dei\u00b7ne", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVIMP", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Schwermuth der Verbannung, die Erinn'rung", "tokens": ["Die", "Schwer\u00b7muth", "der", "Ver\u00b7ban\u00b7nung", ",", "die", "Er\u00b7inn'\u00b7rung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vergangner Lust, verlorner Heimath Sehnen.", "tokens": ["Ver\u00b7gang\u00b7ner", "Lust", ",", "ver\u00b7lor\u00b7ner", "Hei\u00b7math", "Seh\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Auch meine Liebe hat sie schlimm gedeutet,", "tokens": ["Auch", "mei\u00b7ne", "Lie\u00b7be", "hat", "sie", "schlimm", "ge\u00b7deu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die fluchbeladne Welt, und ihre Blumen", "tokens": ["Die", "fluch\u00b7be\u00b7lad\u00b7ne", "Welt", ",", "und", "ih\u00b7re", "Blu\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie giftig Unkraut g\u00e4nzlich ausgereutet.", "tokens": ["Wie", "gif\u00b7tig", "Un\u00b7kraut", "g\u00e4nz\u00b7lich", "aus\u00b7ge\u00b7reu\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Auch mir lag eine Julia in den Armen,", "tokens": ["Auch", "mir", "lag", "ei\u00b7ne", "Ju\u00b7lia", "in", "den", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ART", "NE", "APPR", "ART", "NN", "$,"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und Schuld und Unschuld, ach sie nannte beides", "tokens": ["Und", "Schuld", "und", "Un\u00b7schuld", ",", "ach", "sie", "nann\u00b7te", "bei\u00b7des"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "$,", "XY", "PPER", "VVFIN", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verbrechen ohne Scheu und ohn' Erbarmen.", "tokens": ["Ver\u00b7bre\u00b7chen", "oh\u00b7ne", "Scheu", "und", "ohn'", "Er\u00b7bar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "So schlie\u00dfe denn der Felsen alte Trauer", "tokens": ["So", "schlie\u00b7\u00dfe", "denn", "der", "Fel\u00b7sen", "al\u00b7te", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Uns ein, und gern, versto\u00dfne Kaisertochter,", "tokens": ["Uns", "ein", ",", "und", "gern", ",", "ver\u00b7sto\u00df\u00b7ne", "Kai\u00b7ser\u00b7toch\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "KON", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Umarm' ich hier dich ohne Furcht und Schauer.", "tokens": ["Um\u00b7arm'", "ich", "hier", "dich", "oh\u00b7ne", "Furcht", "und", "Schau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "APPR", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Sie m\u00f6gen h\u00f6hnisch unsre Namen schm\u00e4hen,", "tokens": ["Sie", "m\u00f6\u00b7gen", "h\u00f6h\u00b7nisch", "uns\u00b7re", "Na\u00b7men", "schm\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir bleibt mein Herz, und jene matten Stimmen", "tokens": ["Mir", "bleibt", "mein", "Herz", ",", "und", "je\u00b7ne", "mat\u00b7ten", "Stim\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KON", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df sie im Meeresbrausen untergehen.", "tokens": ["La\u00df", "sie", "im", "Mee\u00b7res\u00b7brau\u00b7sen", "un\u00b7ter\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}