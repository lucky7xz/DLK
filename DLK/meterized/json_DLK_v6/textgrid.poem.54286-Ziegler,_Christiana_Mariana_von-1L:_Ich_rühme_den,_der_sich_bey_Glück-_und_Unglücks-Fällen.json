{"textgrid.poem.54286": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich r\u00fchme den, der sich bey Gl\u00fcck- und Ungl\u00fccks-F\u00e4llen", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich r\u00fchme den, der sich bey Gl\u00fcck- und Ungl\u00fccks-F\u00e4llen", "tokens": ["Ich", "r\u00fch\u00b7me", "den", ",", "der", "sich", "bey", "Gl\u00fc\u00b7ck", "und", "Un\u00b7gl\u00fccks\u00b7F\u00e4l\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "$,", "PRELS", "PRF", "APPR", "TRUNC", "KON", "NN"], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.2": {"text": "Kan freudig und vergn\u00fcgt und unerschrocken stellen.", "tokens": ["Kan", "freu\u00b7dig", "und", "ver\u00b7gn\u00fcgt", "und", "un\u00b7er\u00b7schro\u00b7cken", "stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "KON", "VVPP", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was hilfft der Harm und Gram? er bringt kein Brod ins Hau\u00df,", "tokens": ["Was", "hilfft", "der", "Harm", "und", "Gram", "?", "er", "bringt", "kein", "Brod", "ins", "Hau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "KON", "NN", "$.", "PPER", "VVFIN", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und h\u00e4nget vor der Zeit des Todes Schild heraus.", "tokens": ["Und", "h\u00e4n\u00b7get", "vor", "der", "Zeit", "des", "To\u00b7des", "Schild", "he\u00b7raus", "."], "token_info