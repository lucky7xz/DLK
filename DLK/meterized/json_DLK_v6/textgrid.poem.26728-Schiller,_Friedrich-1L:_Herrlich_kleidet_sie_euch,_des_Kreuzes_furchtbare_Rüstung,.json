{"textgrid.poem.26728": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herrlich kleidet sie euch, des Kreuzes furchtbare R\u00fcstung,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herrlich kleidet sie euch, des Kreuzes furchtbare R\u00fcstung,", "tokens": ["Herr\u00b7lich", "klei\u00b7det", "sie", "euch", ",", "des", "Kreu\u00b7zes", "furcht\u00b7ba\u00b7re", "R\u00fcs\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Wenn ihr, L\u00f6wen der Schlacht, Akkon und Rhodus besch\u00fctzt,", "tokens": ["Wenn", "ihr", ",", "L\u00f6\u00b7wen", "der", "Schlacht", ",", "Ak\u00b7kon", "und", "Rho\u00b7dus", "be\u00b7sch\u00fctzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "NN", "ART", "NN", "$,", "NN", "KON", "NE", "VVPP", "$,"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Durch die syrische W\u00fcste den bangen Pilgrim geleitet", "tokens": ["Durch", "die", "sy\u00b7ri\u00b7sche", "W\u00fcs\u00b7te", "den", "ban\u00b7gen", "Pil\u00b7grim", "ge\u00b7lei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP"], "meter": "+-+--+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Und mit der Cherubim Schwert steht vor dem heiligen Grab.", "tokens": ["Und", "mit", "der", "Che\u00b7ru\u00b7bim", "Schwert", "steht", "vor", "dem", "hei\u00b7li\u00b7gen", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NE", "NE", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Aber ein sch\u00f6nerer Schmuck umgibt euch die Sch\u00fcrze des W\u00e4rters,", "tokens": ["A\u00b7ber", "ein", "sch\u00f6\u00b7ne\u00b7rer", "Schmuck", "um\u00b7gibt", "euch", "die", "Sch\u00fcr\u00b7ze", "des", "W\u00e4r\u00b7ters", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Wenn ihr, L\u00f6wen der Schlacht, S\u00f6hne des edelsten Stamms,", "tokens": ["Wenn", "ihr", ",", "L\u00f6\u00b7wen", "der", "Schlacht", ",", "S\u00f6h\u00b7ne", "des", "e\u00b7dels\u00b7ten", "Stamms", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "NN", "ART", "NN", "$,", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Dient an des Kranken Bett, dem Lechzenden Labung bereitet", "tokens": ["Dient", "an", "des", "Kran\u00b7ken", "Bett", ",", "dem", "Lech\u00b7zen\u00b7den", "La\u00b7bung", "be\u00b7rei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,", "ART", "NN", "NN", "VVFIN"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Und die niedrige Pflicht christlicher Milde vollbringt.", "tokens": ["Und", "die", "nied\u00b7ri\u00b7ge", "Pflicht", "christ\u00b7li\u00b7cher", "Mil\u00b7de", "voll\u00b7bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--++--+-+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Religion des Kreuzes, nur du verkn\u00fcpftest, in einem", "tokens": ["Re\u00b7li\u00b7gi\u00b7on", "des", "Kreu\u00b7zes", ",", "nur", "du", "ver\u00b7kn\u00fcpf\u00b7test", ",", "in", "ei\u00b7nem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "ADV", "PPER", "VVFIN", "$,", "APPR", "ART"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Kranze, der Demut und Kraft doppelte Palme zugleich!", "tokens": ["Kran\u00b7ze", ",", "der", "De\u00b7mut", "und", "Kraft", "dop\u00b7pel\u00b7te", "Pal\u00b7me", "zu\u00b7gleich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "KON", "NN", "ADJA", "NN", "ADV", "$."], "meter": "+---+-+-+-+--+", "measure": "iambic.hexa.chol"}}}}}