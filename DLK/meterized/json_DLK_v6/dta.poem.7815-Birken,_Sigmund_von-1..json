], "pos": ["KON", "ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ostland! deinen Heldgeschichten", "tokens": ["Ost\u00b7land", "!", "dei\u00b7nen", "Held\u00b7ge\u00b7schich\u00b7ten"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "werd ich meine Pflicht entrichten.", "tokens": ["werd", "ich", "mei\u00b7ne", "Pflicht", "ent\u00b7rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch man mu\u00df auch einmal singen.", "tokens": ["Doch", "man", "mu\u00df", "auch", "ein\u00b7mal", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wohin r\u00e4ist sichs/ ihr Gedanken?", "tokens": ["Wo\u00b7hin", "r\u00e4\u00b7ist", "sichs", "/", "ihr", "Ge\u00b7dan\u00b7ken", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "$(", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "hier im Feld der W\u00e4nde Schranken", "tokens": ["hier", "im", "Feld", "der", "W\u00e4n\u00b7de", "Schran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "nicht den Geist zu Kerker bringen.", "tokens": ["nicht", "den", "Geist", "zu", "Ker\u00b7ker", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nehmt mich mit/ ihr Schnabelfl\u00f6ten/", "tokens": ["Nehmt", "mich", "mit", "/", "ihr", "Schna\u00b7bel\u00b7fl\u00f6\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "$(", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "ihr befederte Poeten!", "tokens": ["ihr", "be\u00b7fe\u00b7der\u00b7te", "Po\u00b7et\u00b7en", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Wart! ich bin der Tereus nicht/", "tokens": ["Wart", "!", "ich", "bin", "der", "Te\u00b7reus", "nicht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "ART", "NN", "PTKNEG", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Luft Sirene Filomele!", "tokens": ["Luft", "Si\u00b7re\u00b7ne", "Fi\u00b7lo\u00b7me\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Mein Geh\u00f6r das Urtheil spricht:", "tokens": ["Mein", "Ge\u00b7h\u00f6r", "das", "Ur\u00b7theil", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "du bist unsers Lentzen Seele.", "tokens": ["du", "bist", "un\u00b7sers", "Lent\u00b7zen", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich h\u00f6r euch/ ihr Fittichbr\u00fcder!", "tokens": ["Ich", "h\u00f6r", "euch", "/", "ihr", "Fit\u00b7tich\u00b7br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Aber/ wer h\u00f6rt meine Lieder?", "tokens": ["A\u00b7ber", "/", "wer", "h\u00f6rt", "mei\u00b7ne", "Lie\u00b7der", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+++-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "F\u00f6bus ist itzund nit Richter.", "tokens": ["F\u00f6\u00b7bus", "ist", "it\u00b7zund", "nit", "Rich\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "PTKNEG", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du streust Gold von deinen H\u00f6hen/", "tokens": ["Du", "streust", "Gold", "von", "dei\u00b7nen", "H\u00f6\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "unsre Wasen anzukleen/", "tokens": ["uns\u00b7re", "Wa\u00b7sen", "an\u00b7zu\u00b7kle\u00b7en", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVIZU", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "du erdichter GOtt der Dichter.", "tokens": ["du", "er\u00b7dich\u00b7ter", "Gott", "der", "Dich\u00b7ter."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Nein! mein F\u00f6bus ist auf Erden/", "tokens": ["Nein", "!", "mein", "F\u00f6\u00b7bus", "ist", "auf", "Er\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPOSAT", "NN", "VAFIN", "APPR", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "der mich liebt und meine Heerden.", "tokens": ["der", "mich", "liebt", "und", "mei\u00b7ne", "Heer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Hab ich schon izt nicht die Gnad/", "tokens": ["Hab", "ich", "schon", "izt", "nicht", "die", "Gnad", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "PTKNEG", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "singend selbst vor ihm zustehen:", "tokens": ["sin\u00b7gend", "selbst", "vor", "ihm", "zu\u00b7ste\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.11": {"text": "dannoch pflegt mein Mec\u00e4nat", "tokens": ["dan\u00b7noch", "pflegt", "mein", "Me\u00b7c\u00e4\u00b7nat"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "meine Lieder gern zusehen.", "tokens": ["mei\u00b7ne", "Lie\u00b7der", "gern", "zu\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dorthin reiset/ ihr Gedanken!", "tokens": ["Dor\u00b7thin", "rei\u00b7set", "/", "ihr", "Ge\u00b7dan\u00b7ken", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "denkt/ wie \u00fcm den Vorzug zanken", "tokens": ["denkt", "/", "wie", "\u00fcm", "den", "Vor\u00b7zug", "zan\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$(", "PWAV", "ADJD", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "sein Verstand/ sein Stand und Gl\u00fccke.", "tokens": ["sein", "Ver\u00b7stand", "/", "sein", "Stand", "und", "Gl\u00fc\u00b7cke", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Reiche/ zehlen nicht die Heerden:", "tokens": ["Rei\u00b7che", "/", "zeh\u00b7len", "nicht", "die", "Heer\u00b7den", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "ich mu\u00df arm an Worten werden/", "tokens": ["ich", "mu\u00df", "arm", "an", "Wor\u00b7ten", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "APPR", "NN", "VAINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "wann ich dankbar denk zur\u00fccke.", "tokens": ["wann", "ich", "dank\u00b7bar", "denk", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ich verehre mehr mit Schweigen/", "tokens": ["Ich", "ver\u00b7eh\u00b7re", "mehr", "mit", "Schwei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "ungemessnes Gnad-bezeigen.", "tokens": ["un\u00b7ge\u00b7mess\u00b7nes", "Gna\u00b7dbe\u00b7zei\u00b7gen", "."], "token_info": ["wor