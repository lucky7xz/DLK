{"dta.poem.7815": {"metadata": {"author": {"name": "Birken, Sigmund von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1665", "urn": "urn:nbn:de:kobv:b4-200905198447", "language": ["de:0.99"], "booktitle": "Birken, Sigmund von: Pegnesische Gespr\u00e4chspiel-Gesellschaft von Nymfen und Hirten. N\u00fcrnberg, 1665."}, "poem": {"stanza.1": {"line.1": {"text": "Schweiget ihr noch/ meine Halmen!", "tokens": ["Schwei\u00b7get", "ihr", "noch", "/", "mei\u00b7ne", "Hal\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$(", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "sollen nur die Lerchen-psalmen", "tokens": ["sol\u00b7len", "nur", "die", "Ler\u00b7chen\u00b7psal\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "diese Fr\u00fclingsfreud ausschreyen?", "tokens": ["die\u00b7se", "Fr\u00fc\u00b7lings\u00b7freud", "aus\u00b7schre\u00b7yen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "D\u00fcnkt mich doch/ die Pegnitzfluten", "tokens": ["D\u00fcnkt", "mich", "doch", "/", "die", "Peg\u00b7nitz\u00b7flu\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$(", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "mahnen mich mit Wudelstruten:", "tokens": ["mah\u00b7nen", "mich", "mit", "Wu\u00b7del\u00b7stru\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sing uns etwas von dem M\u00e4yen!", "tokens": ["Sing", "uns", "et\u00b7was", "von", "dem