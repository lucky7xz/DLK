{"textgrid.poem.42910": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Streit", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00e4chtig ist die Ehrlichkeit.", "tokens": ["M\u00e4ch\u00b7tig", "ist", "die", "Ehr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gl\u00fcckt es listigen Gewalten,", "tokens": ["Gl\u00fcckt", "es", "lis\u00b7ti\u00b7gen", "Ge\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie im Gradweg aufzuhalten,", "tokens": ["Sie", "im", "Grad\u00b7weg", "auf\u00b7zu\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer nur f\u00fcr kurze Zeit.", "tokens": ["Im\u00b7mer", "nur", "f\u00fcr", "kur\u00b7ze", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch die kurze Zeit kann lang sein,", "tokens": ["Doch", "die", "kur\u00b7ze", "Zeit", "kann", "lang", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4nger als ein Fl\u00fcgelheben,", "tokens": ["L\u00e4n\u00b7ger", "als", "ein", "Fl\u00fc\u00b7gel\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4nger als ein wartend Leben,", "tokens": ["L\u00e4n\u00b7ger", "als", "ein", "war\u00b7tend", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "ADJD", "NN", "$,"], "meter": "+-+++-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Und das Ehrliche kann bang sein.", "tokens": ["Und", "das", "Ehr\u00b7li\u00b7che", "kann", "bang", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "ADJD", "VAINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Die um Falsch und Ehrlich deuten,", "tokens": ["Die", "um", "Falsch", "und", "Ehr\u00b7lich", "deu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00c4ltere mit j\u00fcngren Leuten,", "tokens": ["\u00c4l\u00b7te\u00b7re", "mit", "j\u00fcng\u00b7ren", "Leu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Irreleitend, irrgeleitet,", "tokens": ["Ir\u00b7re\u00b7lei\u00b7tend", ",", "irr\u00b7ge\u00b7lei\u00b7tet", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie's um Falsch und Ehrlich streitet \u2013 \u2013,", "tokens": ["Wie's", "um", "Falsch", "und", "Ehr\u00b7lich", "strei\u00b7tet", "\u2013", "\u2013", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "VVFIN", "$(", "$(", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "All die Zeit, die sie vergeuden,", "tokens": ["All", "die", "Zeit", ",", "die", "sie", "ver\u00b7geu\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00f6nnte die mit Lustspielfreuden", "tokens": ["K\u00f6nn\u00b7te", "die", "mit", "Lust\u00b7spiel\u00b7freu\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ART", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Besser ausgenossen sein?", "tokens": ["Bes\u00b7ser", "aus\u00b7ge\u00b7nos\u00b7sen", "sein", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ich sag: Nein!", "tokens": ["Ich", "sag", ":", "Nein", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PTKANT", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Wenn ich doch so ehrlich w\u00e4re", "tokens": ["Wenn", "ich", "doch", "so", "ehr\u00b7lich", "w\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ein neugebornes Kind,", "tokens": ["Wie", "ein", "neu\u00b7ge\u00b7bor\u00b7nes", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mich tr\u00fcge dann ein Wind \u2013", "tokens": ["Und", "mich", "tr\u00fc\u00b7ge", "dann", "ein", "Wind", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Freiballons \u2013 ins Ungef\u00e4hre.", "tokens": ["Frei\u00b7bal\u00b7lons", "\u2013", "ins", "Un\u00b7ge\u00b7f\u00e4h\u00b7re", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$(", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Schlag mich einer flach und breit:", "tokens": ["Schlag", "mich", "ei\u00b7ner", "flach", "und", "breit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00e4chtig ist die Ehrlichkeit.", "tokens": ["M\u00e4ch\u00b7tig", "ist", "die", "Ehr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}