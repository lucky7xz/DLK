{"textgrid.poem.58872": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "91. Schlecht und recht", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df all dein Tun und Wesen sein", "tokens": ["La\u00df", "all", "dein", "Tun", "und", "We\u00b7sen", "sein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PIAT", "PPOSAT", "NN", "KON", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Einf\u00e4ltig, schlecht und recht und rein!", "tokens": ["Ein\u00b7f\u00e4l\u00b7tig", ",", "schlecht", "und", "recht", "und", "rein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Seele, die sich nicht verstellet,", "tokens": ["Die", "See\u00b7le", ",", "die", "sich", "nicht", "ver\u00b7stel\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gleichwie ein Engel Gott gef\u00e4llet.", "tokens": ["Gleich\u00b7wie", "ein", "En\u00b7gel", "Gott", "ge\u00b7f\u00e4l\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}