{"textgrid.poem.56346": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Die Predigt des guten Britten", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Anno dreiundachtzig sich zum Krieg", "tokens": ["Als", "An\u00b7no", "drei\u00b7un\u00b7dacht\u00b7zig", "sich", "zum", "Krieg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "CARD", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ger\u00fcstet Engeland und Niederland,", "tokens": ["Ge\u00b7r\u00fcs\u00b7tet", "En\u00b7ge\u00b7land", "und", "Nie\u00b7der\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ward beiderseits gebetet um den Sieg.", "tokens": ["Ward", "bei\u00b7der\u00b7seits", "ge\u00b7be\u00b7tet", "um", "den", "Sieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein ausgeschriebner Bu\u00df- und Bettag fand", "tokens": ["Ein", "aus\u00b7ge\u00b7schrieb\u00b7ner", "Bu\u00df", "und", "Bet\u00b7tag", "fand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "TRUNC", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In beiden L\u00e4ndern statt, doch um acht Tage", "tokens": ["In", "bei\u00b7den", "L\u00e4n\u00b7dern", "statt", ",", "doch", "um", "acht", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$,", "ADV", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Fr\u00fcher in Holland, als in Engeland.", "tokens": ["Fr\u00fc\u00b7her", "in", "Hol\u00b7land", ",", "als", "in", "En\u00b7ge\u00b7land", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$,", "KOUS", "APPR", "NE", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Hier stand ein Prediger vom alten Schlage,", "tokens": ["Hier", "stand", "ein", "Pre\u00b7di\u00b7ger", "vom", "al\u00b7ten", "Schla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nach kr\u00e4ft'ger Predigt betend am Altar,", "tokens": ["Nach", "kr\u00e4ft'\u00b7ger", "Pre\u00b7digt", "be\u00b7tend", "am", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "APPRART", "NN", "$,"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.9": {"text": "Und f\u00fchrte vor dem H\u00f6chsten seine Klage:", "tokens": ["Und", "f\u00fchr\u00b7te", "vor", "dem", "H\u00f6chs\u00b7ten", "sei\u00b7ne", "Kla\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "\u00bbdu wirst dich noch erinnern, Herr, es war", "tokens": ["\u00bb", "du", "wirst", "dich", "noch", "e\u00b7rin\u00b7nern", ",", "Herr", ",", "es", "war"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "PPER", "ADV", "VVINF", "$,", "NN", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Am letzten Sonntag, die Holl\u00e4nder brachten,", "tokens": ["Am", "letz\u00b7ten", "Sonn\u00b7tag", ",", "die", "Hol\u00b7l\u00e4n\u00b7der", "brach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Wie heute wir, dir Bu\u00dfgebete dar.", "tokens": ["Wie", "heu\u00b7te", "wir", ",", "dir", "Bu\u00df\u00b7ge\u00b7be\u00b7te", "dar", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "$,", "PPER", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Wie Jakob einst den Bruder Esau, dachten", "tokens": ["Wie", "Ja\u00b7kob", "einst", "den", "Bru\u00b7der", "E\u00b7sau", ",", "dach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "NE", "ADV", "ART", "NN", "NE", "$,", "VVFIN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "Sie uns um deinen Segen zu betr\u00fcgen,", "tokens": ["Sie", "uns", "um", "dei\u00b7nen", "Se\u00b7gen", "zu", "be\u00b7tr\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Wenn sie die ersten an dein Ohr sich machten.", "tokens": ["Wenn", "sie", "die", "ers\u00b7ten", "an", "dein", "Ohr", "sich", "mach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "APPR", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Glaub ihnen nicht! trau nicht den Winkelz\u00fcgen", "tokens": ["Glaub", "ih\u00b7nen", "nicht", "!", "trau", "nicht", "den", "Win\u00b7kel\u00b7z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PTKNEG", "$.", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.17": {"text": "Der falschen Otterbrut; ihr gutes Recht", "tokens": ["Der", "fal\u00b7schen", "Ot\u00b7ter\u00b7brut", ";", "ihr", "gu\u00b7tes", "Recht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Und frommes Tun sind eitel, eitel L\u00fcgen!", "tokens": ["Und", "from\u00b7mes", "Tun", "sind", "ei\u00b7tel", ",", "ei\u00b7tel", "L\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "ADJD", "$,", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Glaub uns und mir, ich bin dein treuer Knecht,", "tokens": ["Glaub", "uns", "und", "mir", ",", "ich", "bin", "dein", "treu\u00b7er", "Knecht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "PPER", "$,", "PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Ich habe mit der L\u00fcge nichts zu schaffen;", "tokens": ["Ich", "ha\u00b7be", "mit", "der", "L\u00fc\u00b7ge", "nichts", "zu", "schaf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Wir Engel\u00e4nder sind ein fromm Geschlecht;", "tokens": ["Wir", "En\u00b7ge\u00b7l\u00e4n\u00b7der", "sind", "ein", "fromm", "Ge\u00b7schlecht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Sei du mit uns und segne unsre Waffen!\u00ab", "tokens": ["Sei", "du", "mit", "uns", "und", "seg\u00b7ne", "uns\u00b7re", "Waf\u00b7fen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "KON", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}