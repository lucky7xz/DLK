{"dta.poem.3109": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "154.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du bist, mein Filosof, vollkommen \u00fcberzeugt,", "tokens": ["Du", "bist", ",", "mein", "Fi\u00b7lo\u00b7sof", ",", "voll\u00b7kom\u00b7men", "\u00fc\u00b7berz\u00b7eugt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPOSAT", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df jeder irrgeht, wer von deinem Pfad abbeugt.", "tokens": ["Da\u00df", "je\u00b7der", "irr\u00b7geht", ",", "wer", "von", "dei\u00b7nem", "Pfad", "ab\u00b7beugt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "PWS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und deine Zuversicht schl\u00e4gt das mitnichten nieder,", "tokens": ["Und", "dei\u00b7ne", "Zu\u00b7ver\u00b7sicht", "schl\u00e4gt", "das", "mit\u00b7nich\u00b7ten", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df jener, was von ihm du glaubst, von dir glaubt wieder.", "tokens": ["Da\u00df", "je\u00b7ner", ",", "was", "von", "ihm", "du", "glaubst", ",", "von", "dir", "glaubt", "wie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "$,", "PRELS", "APPR", "PPER", "PPER", "VVFIN", "$,", "APPR", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich aber, ungewis, nach welchem Stern ich lenken", "tokens": ["Ich", "a\u00b7ber", ",", "un\u00b7ge\u00b7wis", ",", "nach", "wel\u00b7chem", "Stern", "ich", "len\u00b7ken"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "ADJD", "$,", "APPR", "PWAT", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein armes Schifflein soll, mu\u00df eins von beiden denken:", "tokens": ["Mein", "ar\u00b7mes", "Schif\u00b7flein", "soll", ",", "mu\u00df", "eins", "von", "bei\u00b7den", "den\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "$,", "VMFIN", "PIS", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Entweder da\u00df ihr beid' irr seid auf eurer Fahrt,", "tokens": ["Ent\u00b7we\u00b7der", "da\u00df", "ihr", "beid'", "irr", "seid", "auf", "eu\u00b7rer", "Fahrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIAT", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Oder jeder von euch Recht hat auf seine Art.", "tokens": ["O\u00b7der", "je\u00b7der", "von", "euch", "Recht", "hat", "auf", "sei\u00b7ne", "Art", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "PPER", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Nun w\u00fcrd' es alle Lust am Wissen gar mir rauben,", "tokens": ["Nun", "w\u00fcrd'", "es", "al\u00b7le", "Lust", "am", "Wis\u00b7sen", "gar", "mir", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "APPRART", "NN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Glaubt' ich das erste, drum la\u00df mich das andre glauben.", "tokens": ["Glaubt'", "ich", "das", "ers\u00b7te", ",", "drum", "la\u00df", "mich", "das", "and\u00b7re", "glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "$,", "PAV", "VVFIN", "PPER", "ART", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}