{"dta.poem.342": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An die harte  Pantales.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Pantales mein Licht und Leben", "tokens": ["Pan\u00b7ta\u00b7les", "mein", "Licht", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "H\u00f6r doch meinem Seuffzen zu!", "tokens": ["H\u00f6r", "doch", "mei\u00b7nem", "Seuff\u00b7zen", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wilt du mir denn noch nicht geben", "tokens": ["Wilt", "du", "mir", "denn", "noch", "nicht", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ADV", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die so lang begehrte Ruh?", "tokens": ["Die", "so", "lang", "be\u00b7gehr\u00b7te", "Ruh", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Soll ich stets in Aengsten schweben", "tokens": ["Soll", "ich", "stets", "in", "A\u00b7engs\u00b7ten", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Pantales mein Licht und Leben", "tokens": ["Pan\u00b7ta\u00b7les", "mein", "Licht", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Sey mitleidig strenge Sch\u00f6ne.", "tokens": ["Sey", "mit\u00b7lei\u00b7dig", "stren\u00b7ge", "Sch\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Qu\u00e4hle mich nicht allzusehr/", "tokens": ["Qu\u00e4h\u00b7le", "mich", "nicht", "all\u00b7zu\u00b7sehr", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schau wie ich mich nach dir sehne/", "tokens": ["Schau", "wie", "ich", "mich", "nach", "dir", "seh\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieh doch an mein Thr\u00e4nen-Meer.", "tokens": ["Sieh", "doch", "an", "mein", "Thr\u00e4\u00b7nen\u00b7Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00f6re doch mein Sterb-Geth\u00f6ne/", "tokens": ["H\u00f6\u00b7re", "doch", "mein", "Ster\u00b7bGet\u00b7h\u00f6\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sey mitleidig strenge Sch\u00f6ne.", "tokens": ["Sey", "mit\u00b7lei\u00b7dig", "stren\u00b7ge", "Sch\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Stein und Eisen l\u00e4\u00dft sich zwingen/", "tokens": ["Stein", "und", "Ei\u00b7sen", "l\u00e4\u00dft", "sich", "zwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PRF", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber durch dein hartes Hertz", "tokens": ["A\u00b7ber", "durch", "dein", "har\u00b7tes", "Hertz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kan mein Jammer-Stand nicht dringen", "tokens": ["Kan", "mein", "Jam\u00b7mer\u00b7Stand", "nicht", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Noht ist dir ein Schertz.", "tokens": ["Mei\u00b7ne", "Noht", "ist", "dir", "ein", "Schertz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tauben Ohren mu\u00df ich singen/", "tokens": ["Tau\u00b7ben", "Oh\u00b7ren", "mu\u00df", "ich", "sin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VMFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Stein und Eisen l\u00e4\u00dft sich zwingen.", "tokens": ["Stein", "und", "Ei\u00b7sen", "l\u00e4\u00dft", "sich", "zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Alles kan mein Hertz ertragen", "tokens": ["Al\u00b7les", "kan", "mein", "Hertz", "er\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hitze/ Frost und H\u00f6llen Pein/", "tokens": ["Hit\u00b7ze", "/", "Frost", "und", "H\u00f6l\u00b7len", "Pein", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "KON", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber nicht die harten Plagen", "tokens": ["A\u00b7ber", "nicht", "die", "har\u00b7ten", "Pla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Womit eure Augen dr\u00e4un.", "tokens": ["Wo\u00b7mit", "eu\u00b7re", "Au\u00b7gen", "dr\u00e4un", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gantz vergebens mu\u00df ich klagen", "tokens": ["Gantz", "ver\u00b7ge\u00b7bens", "mu\u00df", "ich", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alles kan mein Hertz ertragen.", "tokens": ["Al\u00b7les", "kan", "mein", "Hertz", "er\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch ich bin damit vergn\u00fcget/", "tokens": ["Doch", "ich", "bin", "da\u00b7mit", "ver\u00b7gn\u00fc\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PAV", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn dein Hertz durch meinen Tod/", "tokens": ["Wenn", "dein", "Hertz", "durch", "mei\u00b7nen", "Tod", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Pantales/ nur wird besieget;", "tokens": ["Pan\u00b7ta\u00b7les", "/", "nur", "wird", "be\u00b7sie\u00b7get", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Weine doch ob meiner Noht/", "tokens": ["Wei\u00b7ne", "doch", "ob", "mei\u00b7ner", "Noht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KOUS", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn der Himmel dieses f\u00fcget", "tokens": ["Wenn", "der", "Him\u00b7mel", "die\u00b7ses", "f\u00fc\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PDAT", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bin ich schon damit vergn\u00fcget.", "tokens": ["Bin", "ich", "schon", "da\u00b7mit", "ver\u00b7gn\u00fc\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PAV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}