{"textgrid.poem.43046": {"metadata": {"author": {"name": "Jacoby, Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Abbitte", "genre": "verse", "period": "N.A.", "pub_year": 1867, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der du von deinem Himmel droben", "tokens": ["Der", "du", "von", "dei\u00b7nem", "Him\u00b7mel", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit g\u00fct'gem Aug' und mildem L\u00e4cheln", "tokens": ["Mit", "g\u00fct'\u00b7gem", "Aug'", "und", "mil\u00b7dem", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Menschen ungez\u00e4hlte Thorheit schaust,", "tokens": ["Der", "Men\u00b7schen", "un\u00b7ge\u00b7z\u00e4hl\u00b7te", "Thor\u00b7heit", "schaust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verzeihe mir, allg\u00fct'ger Vater!", "tokens": ["Ver\u00b7zei\u00b7he", "mir", ",", "all\u00b7g\u00fct'\u00b7ger", "Va\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Verzeih' die Thorheit mir, die ich beging,", "tokens": ["Ver\u00b7zeih'", "die", "Thor\u00b7heit", "mir", ",", "die", "ich", "be\u00b7ging", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da ich einst sprach: Ich will nicht lieben.", "tokens": ["Da", "ich", "einst", "sprach", ":", "Ich", "will", "nicht", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$.", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Verzeih' die Thorheit mir, die ich begehe nun,", "tokens": ["Ver\u00b7zeih'", "die", "Thor\u00b7heit", "mir", ",", "die", "ich", "be\u00b7ge\u00b7he", "nun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da ich gestehen mu\u00df: ich lieb', ich liebe!", "tokens": ["Da", "ich", "ge\u00b7ste\u00b7hen", "mu\u00df", ":", "ich", "lieb'", ",", "ich", "lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Da alles in mir wiederklingt von Liebe,", "tokens": ["Da", "al\u00b7les", "in", "mir", "wie\u00b7der\u00b7klingt", "von", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und ich herzkrank und elend", "tokens": ["Und", "ich", "herz\u00b7krank", "und", "e\u00b7lend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Und doch selig bin durch Liebe.", "tokens": ["Und", "doch", "se\u00b7lig", "bin", "durch", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wohl ist sie, die ich lieb', von jenen keine,", "tokens": ["Wohl", "ist", "sie", ",", "die", "ich", "lieb'", ",", "von", "je\u00b7nen", "kei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$,", "APPR", "PDAT", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Die mit ihren Gaben prunkend gl\u00e4nzt,", "tokens": ["Die", "mit", "ih\u00b7ren", "Ga\u00b7ben", "prun\u00b7kend", "gl\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.14": {"text": "Und deren Sch\u00f6nheit wie ein Rufer unter Trommelschlag", "tokens": ["Und", "de\u00b7ren", "Sch\u00f6n\u00b7heit", "wie", "ein", "Ru\u00b7fer", "un\u00b7ter", "Trom\u00b7mel\u00b7schlag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRELAT", "NN", "KOKOM", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.15": {"text": "Vorgeht und ausruft:", "tokens": ["Vor\u00b7geht", "und", "aus\u00b7ruft", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.16": {"text": "Kommt, kommt her und huld'get mir!", "tokens": ["Kommt", ",", "kommt", "her", "und", "huld'\u00b7get", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PTKVZ", "KON", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Die ich verspottet habe tausendmal", "tokens": ["Die", "ich", "ver\u00b7spot\u00b7tet", "ha\u00b7be", "tau\u00b7send\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Mit deinem Wohlgefallen, Herr,", "tokens": ["Mit", "dei\u00b7nem", "Wohl\u00b7ge\u00b7fal\u00b7len", ",", "Herr", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Die man bewundern mag, doch nimmer lieben.", "tokens": ["Die", "man", "be\u00b7wun\u00b7dern", "mag", ",", "doch", "nim\u00b7mer", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVINF", "VMFIN", "$,", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Nein, die ich liebe, ist von seltner Art,", "tokens": ["Nein", ",", "die", "ich", "lie\u00b7be", ",", "ist", "von", "selt\u00b7ner", "Art", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Ist eine von den Blumen, denen du", "tokens": ["Ist", "ei\u00b7ne", "von", "den", "Blu\u00b7men", ",", "de\u00b7nen", "du"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ART", "APPR", "ART", "NN", "$,", "PRELS", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Das himmlische Geheimni\u00df hast gegeben,", "tokens": ["Das", "himm\u00b7li\u00b7sche", "Ge\u00b7heim\u00b7ni\u00df", "hast", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Da\u00df sie mit ihrem Reiz entz\u00fccken m\u00fcssen,", "tokens": ["Da\u00df", "sie", "mit", "ih\u00b7rem", "Reiz", "ent\u00b7z\u00fc\u00b7cken", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Ganz ihrer eig'nen Sch\u00f6nheit unbewu\u00dft,", "tokens": ["Ganz", "ih\u00b7rer", "eig'\u00b7nen", "Sch\u00f6n\u00b7heit", "un\u00b7be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Und strahlen doch in Bl\u00fcthenduft und Anmuth.", "tokens": ["Und", "strah\u00b7len", "doch", "in", "Bl\u00fc\u00b7then\u00b7duft", "und", "An\u00b7muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Ja so ist sie", "tokens": ["Ja", "so", "ist", "sie"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "VAFIN", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.27": {"text": "Von zaubervollem Anmuthduft umflossen,", "tokens": ["Von", "zau\u00b7ber\u00b7vol\u00b7lem", "An\u00b7muth\u00b7duft", "um\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Von unbewu\u00dftem Liebreiz und so sch\u00f6n,", "tokens": ["Von", "un\u00b7be\u00b7wu\u00df\u00b7tem", "Lieb\u00b7reiz", "und", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Da\u00df auch, wenn sie zu lieben Thorheit w\u00e4r',", "tokens": ["Da\u00df", "auch", ",", "wenn", "sie", "zu", "lie\u00b7ben", "Thor\u00b7heit", "w\u00e4r'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Du diese schon im Voraus hast vergeben,", "tokens": ["Du", "die\u00b7se", "schon", "im", "Vo\u00b7raus", "hast", "ver\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDS", "ADV", "APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Da\u00df ob der Thorheit aber, Herr, sie nicht zu lieben,", "tokens": ["Da\u00df", "ob", "der", "Thor\u00b7heit", "a\u00b7ber", ",", "Herr", ",", "sie", "nicht", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "ADV", "$,", "NN", "$,", "PPER", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Du z\u00fcrnen m\u00fc\u00dftest f\u00fcr und f\u00fcr.", "tokens": ["Du", "z\u00fcr\u00b7nen", "m\u00fc\u00df\u00b7test", "f\u00fcr", "und", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Und also bin ich fromm", "tokens": ["Und", "al\u00b7so", "bin", "ich", "fromm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.34": {"text": "Und liebe sie mit nie geglaubter Gluth,", "tokens": ["Und", "lie\u00b7be", "sie", "mit", "nie", "ge\u00b7glaub\u00b7ter", "Gluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Und liebe sie mit nie empfund'ner Lust", "tokens": ["Und", "lie\u00b7be", "sie", "mit", "nie", "emp\u00b7fun\u00b7d'\u00b7ner", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.36": {"text": "Und mit dem ganzen Heer von Qual und Plagen,", "tokens": ["Und", "mit", "dem", "gan\u00b7zen", "Heer", "von", "Qual", "und", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Das so getreulich einer treuen Liebe folgt,", "tokens": ["Das", "so", "ge\u00b7treu\u00b7lich", "ei\u00b7ner", "treu\u00b7en", "Lie\u00b7be", "folgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Als wie ein Bienenschwarm der K\u00f6nigin,", "tokens": ["Als", "wie", "ein", "Bie\u00b7nen\u00b7schwarm", "der", "K\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Doch so versch\u00f6nend folget treuer Liebe,", "tokens": ["Doch", "so", "ver\u00b7sch\u00f6\u00b7nend", "fol\u00b7get", "treu\u00b7er", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Als wie der Abendstern dem Mond,", "tokens": ["Als", "wie", "der", "A\u00b7bends\u00b7tern", "dem", "Mond", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.41": {"text": "Wann er die stillen, blauen Pfade wandelt.", "tokens": ["Wann", "er", "die", "stil\u00b7len", ",", "blau\u00b7en", "Pfa\u00b7de", "wan\u00b7delt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}