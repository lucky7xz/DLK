{"textgrid.poem.41546": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am sechsten Sonntage nach Ostern", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erwacht! der Zeitenseiger hat", "tokens": ["Er\u00b7wacht", "!", "der", "Zei\u00b7tens\u00b7ei\u00b7ger", "hat"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf die Minute sich gestellt;", "tokens": ["Auf", "die", "Mi\u00b7nu\u00b7te", "sich", "ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem rostigen Getriebe matt", "tokens": ["Dem", "ros\u00b7ti\u00b7gen", "Ge\u00b7trie\u00b7be", "matt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein neues Rad ist zugesellt;", "tokens": ["Ein", "neu\u00b7es", "Rad", "ist", "zu\u00b7ge\u00b7sellt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Feder steigt, der Hammer f\u00e4llt.", "tokens": ["Die", "Fe\u00b7der", "steigt", ",", "der", "Ham\u00b7mer", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie den Soldaten auf der Wacht", "tokens": ["Wie", "den", "Sol\u00b7da\u00b7ten", "auf", "der", "Wacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Ronde schreckt aus dumpfer Ruh,", "tokens": ["Die", "Ron\u00b7de", "schreckt", "aus", "dum\u00b7pfer", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So durch gewitterschw\u00fcle Nacht", "tokens": ["So", "durch", "ge\u00b7wit\u00b7ter\u00b7schw\u00fc\u00b7le", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ruft uns die Glockenstimme zu:", "tokens": ["Ruft", "uns", "die", "Glo\u00b7ckens\u00b7tim\u00b7me", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie nennst du dich, wer bist denn du?", "tokens": ["Wie", "nennst", "du", "dich", ",", "wer", "bist", "denn", "du", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$,", "PWS", "VAFIN", "KON", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und mancher der im langen Traum", "tokens": ["Und", "man\u00b7cher", "der", "im", "lan\u00b7gen", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "ART", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den eignen Namen fast verschlief,", "tokens": ["Den", "eig\u00b7nen", "Na\u00b7men", "fast", "ver\u00b7schlief", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Stie\u00df nun von sich den schn\u00f6den Flaum", "tokens": ["Stie\u00df", "nun", "von", "sich", "den", "schn\u00f6\u00b7den", "Flaum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hastig die Parole rief,", "tokens": ["Und", "has\u00b7tig", "die", "Pa\u00b7ro\u00b7le", "rief", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So ernst die Glocke sprach und tief.", "tokens": ["So", "ernst", "die", "Glo\u00b7cke", "sprach", "und", "tief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wer m\u00f6chte sich in solcher Zeit", "tokens": ["Wer", "m\u00f6ch\u00b7te", "sich", "in", "sol\u00b7cher", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PRF", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von deinem Heere schlie\u00dfen aus?", "tokens": ["Von", "dei\u00b7nem", "Hee\u00b7re", "schlie\u00b7\u00dfen", "aus", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was Lenz und Sonne hat zerstreut,", "tokens": ["Was", "Lenz", "und", "Son\u00b7ne", "hat", "zer\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das sucht im Sturme wohl sein Haus,", "tokens": ["Das", "sucht", "im", "Stur\u00b7me", "wohl", "sein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur Vagabunde bleiben drau\u00df.", "tokens": ["Nur", "Va\u00b7ga\u00b7bun\u00b7de", "blei\u00b7ben", "drau\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dem Kleinsten ward sein wichtig Teil,", "tokens": ["Dem", "Kleins\u00b7ten", "ward", "sein", "wich\u00b7tig", "Teil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Umsonst hatt' keiner seinen Stand.", "tokens": ["Um\u00b7sonst", "hatt'", "kei\u00b7ner", "sei\u00b7nen", "Stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mag was da hoch, zu Kraft und Heil", "tokens": ["Mag", "was", "da", "hoch", ",", "zu", "Kraft", "und", "Heil"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PWS", "ADV", "ADJD", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns leuchten von der Zinne Rand;", "tokens": ["Uns", "leuch\u00b7ten", "von", "der", "Zin\u00b7ne", "Rand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch nur die Masse sch\u00fctzt das Land.", "tokens": ["Doch", "nur", "die", "Mas\u00b7se", "sch\u00fctzt", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ist es ein schwacher Posten auch,", "tokens": ["Ist", "es", "ein", "schwa\u00b7cher", "Pos\u00b7ten", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf den mich deine Hand gestellt:", "tokens": ["Auf", "den", "mich", "dei\u00b7ne", "Hand", "ge\u00b7stellt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So ward mir doch des Wortes Hauch,", "tokens": ["So", "ward", "mir", "doch", "des", "Wor\u00b7tes", "Hauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das furchtlos wandelt durch die Welt,", "tokens": ["Das", "furcht\u00b7los", "wan\u00b7delt", "durch", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gleich ob es dunkelt oder hellt.", "tokens": ["Gleich", "ob", "es", "dun\u00b7kelt", "o\u00b7der", "hellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Tu nur ein jeder was er kann,", "tokens": ["Tu", "nur", "ein", "je\u00b7der", "was", "er", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "PIAT", "PWS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df h\u00fclfreich stehe Schaft an Schaft;", "tokens": ["Da\u00df", "h\u00fclf\u00b7reich", "ste\u00b7he", "Schaft", "an", "Schaft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Niedre schlie\u00dfe treulich an,", "tokens": ["Der", "Nied\u00b7re", "schlie\u00b7\u00dfe", "treu\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Hohe zeige seine Kraft:", "tokens": ["Der", "Ho\u00b7he", "zei\u00b7ge", "sei\u00b7ne", "Kraft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann wei\u00df ich wohl wer Rettung schafft!", "tokens": ["Dann", "wei\u00df", "ich", "wohl", "wer", "Ret\u00b7tung", "schafft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PWS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}