{"textgrid.poem.32976": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1. Meiner Mutter", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wozu denn das ewige Sorgen,", "tokens": ["Wo\u00b7zu", "denn", "das", "e\u00b7wi\u00b7ge", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "lieb M\u00fctterchen! gib es doch auf!", "tokens": ["lieb", "M\u00fct\u00b7ter\u00b7chen", "!", "gib", "es", "doch", "auf", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$.", "VVIMP", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sorgen macht alles nur schlimmer", "tokens": ["Sor\u00b7gen", "macht", "al\u00b7les", "nur", "schlim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PIS", "ADV", "ADJD"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "und \u00e4ndert doch nichts im Lauf!", "tokens": ["und", "\u00e4n\u00b7dert", "doch", "nichts", "im", "Lauf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIS", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Auf deine alten Tage", "tokens": ["Auf", "dei\u00b7ne", "al\u00b7ten", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "m\u00f6cht ich, da\u00df froh du w\u00e4rst", "tokens": ["m\u00f6cht", "ich", ",", "da\u00df", "froh", "du", "w\u00e4rst"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "$,", "KOUS", "ADJD", "PPER", "VAFIN"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.7": {"text": "und nicht mit Gedanken um uns,", "tokens": ["und", "nicht", "mit", "Ge\u00b7dan\u00b7ken", "um", "uns", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NN", "APPR", "PPER", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "deine Kinder, das Herz dir beschwerst.", "tokens": ["dei\u00b7ne", "Kin\u00b7der", ",", "das", "Herz", "dir", "be\u00b7schwerst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Du hast dich in deinem Leben", "tokens": ["Du", "hast", "dich", "in", "dei\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wahrlich genug gesorgt,", "tokens": ["wahr\u00b7lich", "ge\u00b7nug", "ge\u00b7sorgt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "du gabest mit Zinseszins ihm", "tokens": ["du", "ga\u00b7best", "mit", "Zin\u00b7ses\u00b7zins", "ihm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "PPER"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "zur\u00fcck, was es dir geborgt ...", "tokens": ["zu\u00b7r\u00fcck", ",", "was", "es", "dir", "ge\u00b7borgt", "..."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "PWS", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Du bist bald siebzig Jahre", "tokens": ["Du", "bist", "bald", "sieb\u00b7zig", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "und mich d\u00fcnkt, du h\u00e4ttest nun", "tokens": ["und", "mich", "d\u00fcnkt", ",", "du", "h\u00e4t\u00b7test", "nun"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "nicht blo\u00df ein Recht mehr, nein,", "tokens": ["nicht", "blo\u00df", "ein", "Recht", "mehr", ",", "nein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "ADV", "$,", "PTKANT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "auch die Pflicht, dich auszuruhn.", "tokens": ["auch", "die", "Pflicht", ",", "dich", "aus\u00b7zu\u00b7ruhn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PPER", "VVIZU", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Du trugest Leid und Schmerzen,", "tokens": ["Du", "tru\u00b7gest", "Leid", "und", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "ohn da\u00df sich ein Wort dir entrang,", "tokens": ["ohn", "da\u00df", "sich", "ein", "Wort", "dir", "ent\u00b7rang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PRF", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "du gingest mit schwerem Herzen", "tokens": ["du", "gin\u00b7gest", "mit", "schwe\u00b7rem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "so manchen schweren Gang ...", "tokens": ["so", "man\u00b7chen", "schwe\u00b7ren", "Gang", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "und als der Vater erblindet,", "tokens": ["und", "als", "der", "Va\u00b7ter", "er\u00b7blin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "die ganze, lange Zeit,", "tokens": ["die", "gan\u00b7ze", ",", "lan\u00b7ge", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "du wurdest nie m\u00fcde in treuer", "tokens": ["du", "wur\u00b7dest", "nie", "m\u00fc\u00b7de", "in", "treu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.16": {"text": "frohwilliger Freudigkeit!", "tokens": ["froh\u00b7wil\u00b7li\u00b7ger", "Freu\u00b7dig\u00b7keit", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Nur als er dann starb, da freilich", "tokens": ["Nur", "als", "er", "dann", "starb", ",", "da", "frei\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "ADV", "VVFIN", "$,", "KOUS", "ADV"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "wurde merklich wei\u00dfer dein Haar,", "tokens": ["wur\u00b7de", "merk\u00b7lich", "wei\u00b7\u00dfer", "dein", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJA", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.19": {"text": "doch deine Liebe zu uns", "tokens": ["doch", "dei\u00b7ne", "Lie\u00b7be", "zu", "uns"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "PPER"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.20": {"text": "blieb so jung, wie sie immer war.", "tokens": ["blieb", "so", "jung", ",", "wie", "sie", "im\u00b7mer", "war", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$,", "PWAV", "PPER", "ADV", "VAFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.21": {"text": "Und nun sind wir gro\u00df geworden", "tokens": ["Und", "nun", "sind", "wir", "gro\u00df", "ge\u00b7wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "VAPP"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.22": {"text": "und wanderten in die Welt,", "tokens": ["und", "wan\u00b7der\u00b7ten", "in", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.23": {"text": "und ein jedes hat sich f\u00fcrs Leben", "tokens": ["und", "ein", "je\u00b7des", "hat", "sich", "f\u00fcrs", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PIAT", "VAFIN", "PRF", "APPRART", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.24": {"text": "sein gutes Ziel gestellt ...", "tokens": ["sein", "gu\u00b7tes", "Ziel", "ge\u00b7stellt", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Du aber, lieb M\u00fctterchen, gib jetzt", "tokens": ["Du", "a\u00b7ber", ",", "lieb", "M\u00fct\u00b7ter\u00b7chen", ",", "gib", "jetzt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "$,", "ADJD", "NN", "$,", "VVIMP", "ADV"], "meter": "-+-++---+", "measure": "unknown.measure.tetra"}, "line.26": {"text": "dein Sorgen endlich auf,", "tokens": ["dein", "Sor\u00b7gen", "end\u00b7lich", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Sorgen sieht alles nur schw\u00e4rzer", "tokens": ["Sor\u00b7gen", "sieht", "al\u00b7les", "nur", "schw\u00e4r\u00b7zer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PIS", "ADV", "ADJA"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.28": {"text": "und \u00e4ndert doch nichts im Lauf!", "tokens": ["und", "\u00e4n\u00b7dert", "doch", "nichts", "im", "Lauf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIS", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Du wei\u00dft ja, wir haben niemals", "tokens": ["Du", "wei\u00dft", "ja", ",", "wir", "ha\u00b7ben", "nie\u00b7mals"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Arbeit und Umtrieb gescheut,", "tokens": ["Ar\u00b7beit", "und", "Um\u00b7trieb", "ge\u00b7scheut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "wir haben, im Gegenteil, immer", "tokens": ["wir", "ha\u00b7ben", ",", "im", "Ge\u00b7gen\u00b7teil", ",", "im\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "$,", "APPRART", "NN", "$,", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "uns jeglicher M\u00fche gefreut,", "tokens": ["uns", "jeg\u00b7li\u00b7cher", "M\u00fc\u00b7he", "ge\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "und wenn auch nicht alles ging,", "tokens": ["und", "wenn", "auch", "nicht", "al\u00b7les", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PTKNEG", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "wie man w\u00fcnschte, es m\u00f6chte gehn,", "tokens": ["wie", "man", "w\u00fcnschte", ",", "es", "m\u00f6ch\u00b7te", "gehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "so blieb doch keines mutlos", "tokens": ["so", "blieb", "doch", "kei\u00b7nes", "mut\u00b7los"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PIAT", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "oder m\u00fc\u00dfig am Markte stehn.", "tokens": ["o\u00b7der", "m\u00fc\u00b7\u00dfig", "am", "Mark\u00b7te", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.9": {"text": "Wir haben uns, Gott sei Dank,", "tokens": ["Wir", "ha\u00b7ben", "uns", ",", "Gott", "sei", "Dank", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "NN", "VAFIN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "immer selber zu raten vermocht,", "tokens": ["im\u00b7mer", "sel\u00b7ber", "zu", "ra\u00b7ten", "ver\u00b7mocht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKZU", "VVINF", "VVPP", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "und schlug auch vieles fehl,", "tokens": ["und", "schlug", "auch", "vie\u00b7les", "fehl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIS", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "hat uns doch nichts unterjocht.", "tokens": ["hat", "uns", "doch", "nichts", "un\u00b7ter\u00b7jocht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PIS", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Da\u00df einem das Herz einmal schwer", "tokens": ["Da\u00df", "ei\u00b7nem", "das", "Herz", "ein\u00b7mal", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ART", "NN", "ADV", "ADJD"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.14": {"text": "und da\u00df man weniger froh,", "tokens": ["und", "da\u00df", "man", "we\u00b7ni\u00b7ger", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "das will nichts hei\u00dfen, Mutter,", "tokens": ["das", "will", "nichts", "hei\u00b7\u00dfen", ",", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VMFIN", "PIS", "VVINF", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "das geht einem jeden so.", "tokens": ["das", "geht", "ei\u00b7nem", "je\u00b7den", "so", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "PIAT", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Man h\u00e4tte mitunter ja manches", "tokens": ["Man", "h\u00e4t\u00b7te", "mi\u00b7tun\u00b7ter", "ja", "man\u00b7ches"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "ADV", "ADV", "PIS"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "leichter und schneller erreicht,", "tokens": ["leich\u00b7ter", "und", "schnel\u00b7ler", "er\u00b7reicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "wenn man weniger.. stolz gewesen", "tokens": ["wenn", "man", "we\u00b7ni\u00b7ger", "..", "stolz", "ge\u00b7we\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "$.", "ADJD", "VAPP"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "und r\u00fccksichtsloser vielleicht,", "tokens": ["und", "r\u00fcck\u00b7sichts\u00b7lo\u00b7ser", "viel\u00b7leicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "und wenn ... ja, ja, wenn du fr\u00fcher", "tokens": ["und", "wenn", "...", "ja", ",", "ja", ",", "wenn", "du", "fr\u00fc\u00b7her"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "$(", "PTKANT", "$,", "PTKANT", "$,", "KOUS", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "nicht immer so abgewehrt,", "tokens": ["nicht", "im\u00b7mer", "so", "ab\u00b7ge\u00b7wehrt", ","], "token_info": ["word", "word", "word", "word",