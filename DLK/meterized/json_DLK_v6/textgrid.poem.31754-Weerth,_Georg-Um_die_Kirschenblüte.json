{"textgrid.poem.31754": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Um die Kirschenbl\u00fcte", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und um die Kirschenbl\u00fcte,", "tokens": ["Und", "um", "die", "Kir\u00b7schen\u00b7bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da haben wir logiert,", "tokens": ["Da", "ha\u00b7ben", "wir", "lo\u00b7giert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohl um die Kirschenbl\u00fcte", "tokens": ["Wohl", "um", "die", "Kir\u00b7schen\u00b7bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Frankfurt einst logiert.", "tokens": ["In", "Frank\u00b7furt", "einst", "lo\u00b7giert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es sprach der Herbergsvater:", "tokens": ["Es", "sprach", "der", "Her\u00b7bergs\u00b7va\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbhabt schlechte R\u00f6cke an!\u00ab", "tokens": ["\u00bb", "habt", "schlech\u00b7te", "R\u00f6\u00b7cke", "an", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbdu laus'ger Herbergsvater,", "tokens": ["\u00bb", "du", "laus'\u00b7ger", "Her\u00b7bergs\u00b7va\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das geht dich gar nichts an!", "tokens": ["Das", "geht", "dich", "gar", "nichts", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PIS", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gib uns von deinem Weine,", "tokens": ["Gib", "uns", "von", "dei\u00b7nem", "Wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gib uns von deinem Bier;", "tokens": ["Gib", "uns", "von", "dei\u00b7nem", "Bier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gib uns zu Bier und Weine", "tokens": ["Gib", "uns", "zu", "Bier", "und", "Wei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch ein gebraten Tier!\u00ab", "tokens": ["Auch", "ein", "ge\u00b7bra\u00b7ten", "Tier", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da kr\u00e4ht der Hahn im Spunde \u2013", "tokens": ["Da", "kr\u00e4ht", "der", "Hahn", "im", "Spun\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das ist ein guter Flu\u00df!", "tokens": ["Das", "ist", "ein", "gu\u00b7ter", "Flu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es schmeckt in unsrem Munde", "tokens": ["Es", "schmeckt", "in", "uns\u00b7rem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als wie Urinius.", "tokens": ["Als", "wie", "U\u00b7ri\u00b7ni\u00b7us", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Da bracht er einen Hasen", "tokens": ["Da", "bracht", "er", "ei\u00b7nen", "Ha\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Petersilienkraut:", "tokens": ["In", "Pe\u00b7ter\u00b7si\u00b7li\u00b7en\u00b7kraut", ":"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NE", "$."], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Vor diesem toten Hasen", "tokens": ["Vor", "die\u00b7sem", "to\u00b7ten", "Ha\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat es uns sehr gegraut.", "tokens": ["Hat", "es", "uns", "sehr", "ge\u00b7graut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und als wir war'n im Bette", "tokens": ["Und", "als", "wir", "wa\u00b7r'n", "im", "Bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit unsrem Nachtgebet:", "tokens": ["Mit", "uns\u00b7rem", "Nacht\u00b7ge\u00b7bet", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da stachen uns im Bette", "tokens": ["Da", "sta\u00b7chen", "uns", "im", "Bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Wanzen fr\u00fch und sp\u00e4t.", "tokens": ["Die", "Wan\u00b7zen", "fr\u00fch", "und", "sp\u00e4t", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Dies ist geschehn zu Frankfurt,", "tokens": ["Dies", "ist", "ge\u00b7schehn", "zu", "Frank\u00b7furt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "APPR", "NE", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Wohl in der sch\u00f6nen Stadt,", "tokens": ["Wohl", "in", "der", "sch\u00f6\u00b7nen", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das wei\u00df, wer dort gelebet", "tokens": ["Das", "wei\u00df", ",", "wer", "dort", "ge\u00b7le\u00b7bet"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "PWS", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und dort gelitten hat.", "tokens": ["Und", "dort", "ge\u00b7lit\u00b7ten", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}