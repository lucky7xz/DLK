{"textgrid.poem.53568": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Die Schule", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer die Schule hat, hat das Land.", "tokens": ["Wer", "die", "Schu\u00b7le", "hat", ",", "hat", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "$,", "VAFIN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Aber wer hat die bei uns in der Hand!", "tokens": ["A\u00b7ber", "wer", "hat", "die", "bei", "uns", "in", "der", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "ART", "APPR", "PPER", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Du h\u00f6rst schon von weitem die Sch\u00fcler schnarchen.", "tokens": ["Du", "h\u00f6rst", "schon", "von", "wei\u00b7tem", "die", "Sch\u00fc\u00b7ler", "schnar\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PIS", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da sitzen noch immer die alten Scholarchen,", "tokens": ["Da", "sit\u00b7zen", "noch", "im\u00b7mer", "die", "al\u00b7ten", "Scho\u00b7lar\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "die alten Pauker mit blinden Brillen,", "tokens": ["die", "al\u00b7ten", "Pau\u00b7ker", "mit", "blin\u00b7den", "Bril\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "sie b\u00e4ndigen und t\u00f6ten den Sch\u00fclerwillen.", "tokens": ["sie", "b\u00e4n\u00b7di\u00b7gen", "und", "t\u00f6\u00b7ten", "den", "Sch\u00fc\u00b7ler\u00b7wil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und lesen noch immer die alte Fibel", "tokens": ["Und", "le\u00b7sen", "noch", "im\u00b7mer", "die", "al\u00b7te", "Fi\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "und lehren noch immer den alten Stiebel:", "tokens": ["und", "leh\u00b7ren", "noch", "im\u00b7mer", "den", "al\u00b7ten", "Stie\u00b7bel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie in den alten Zeiten die wichtigen Schlachten", "tokens": ["Wie", "in", "den", "al\u00b7ten", "Zei\u00b7ten", "die", "wich\u00b7ti\u00b7gen", "Schlach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "die gro\u00dfen V\u00f6lkerentscheidungen brachten,", "tokens": ["die", "gro\u00b7\u00dfen", "V\u00f6l\u00b7ke\u00b7rent\u00b7schei\u00b7dun\u00b7gen", "brach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "wie die F\u00fcrsten und die S\u00f6ldnerlanzen", "tokens": ["wie", "die", "F\u00fcrs\u00b7ten", "und", "die", "S\u00f6ld\u00b7ner\u00b7lan\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "den gro\u00dfen blutigen Contre tanzen,", "tokens": ["den", "gro\u00b7\u00dfen", "blu\u00b7ti\u00b7gen", "Con\u00b7tre", "tan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "und ohne die heilige Monarchie", "tokens": ["und", "oh\u00b7ne", "die", "hei\u00b7li\u00b7ge", "Mon\u00b7ar\u00b7chie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+--", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "sei die H\u00f6lle auf Erden \u2013 und schlie\u00dflich, wie", "tokens": ["sei", "die", "H\u00f6l\u00b7le", "auf", "Er\u00b7den", "\u2013", "und", "schlie\u00df\u00b7lich", ",", "wie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN", "$(", "KON", "ADJD", "$,", "PWAV"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "die V\u00f6lker nur eigentlich Statisten seien.", "tokens": ["die", "V\u00f6l\u00b7ker", "nur", "ei\u00b7gent\u00b7lich", "Sta\u00b7tis\u00b7ten", "sei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "NN", "VAFIN", "$."], "meter": "-+--+---+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Man m\u00fc\u00dfte ihnen die Dumpfheit verzeihen.", "tokens": ["Man", "m\u00fc\u00df\u00b7te", "ih\u00b7nen", "die", "Dum\u00b7pf\u00b7heit", "ver\u00b7zei\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+--++--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "K\u00f6nnten eben nichts weiter daf\u00fcr . . .", "tokens": ["K\u00f6nn\u00b7ten", "e\u00b7ben", "nichts", "wei\u00b7ter", "da\u00b7f\u00fcr", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VMFIN", "ADV", "PIS", "ADV", "PAV", "$.", "$.", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Und sie lernen vom Kupfercyan\u00fcr.", "tokens": ["Und", "sie", "ler\u00b7nen", "vom", "Kup\u00b7fer\u00b7cya\u00b7n\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Und von den braven Kohlehydraten.", "tokens": ["Und", "von", "den", "bra\u00b7ven", "Koh\u00b7le\u00b7hyd\u00b7ra\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Und von den beiden Koordinaten.", "tokens": ["Und", "von", "den", "bei\u00b7den", "Ko\u00b7or\u00b7di\u00b7na\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "PIAT", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und von der Verbindung mit dem Chrome.", "tokens": ["Und", "von", "der", "Ver\u00b7bin\u00b7dung", "mit", "dem", "Chro\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Lernen auch allerhand fremde Idiome.", "tokens": ["Ler\u00b7nen", "auch", "al\u00b7ler\u00b7hand", "frem\u00b7de", "I\u00b7dio\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Ut regiert den Konjunktiv.", "tokens": ["Ut", "re\u00b7giert", "den", "Kon\u00b7junk\u00b7tiv", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Polichinelle ist ein Diminutiv.", "tokens": ["Po\u00b7li\u00b7chi\u00b7nel\u00b7le", "ist", "ein", "Di\u00b7mi\u00b7nu\u00b7tiv", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und was so dergleichen an Stoff und an Wissen.", "tokens": ["Und", "was", "so", "derg\u00b7lei\u00b7chen", "an", "Stoff", "und", "an", "Wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "PIS", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Himmelherrgott! ist die Schule beschmissen!", "tokens": ["Him\u00b7mel\u00b7herr\u00b7gott", "!", "ist", "die", "Schu\u00b7le", "be\u00b7schmis\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Seelenmord und Seelenraub!", "tokens": ["See\u00b7len\u00b7mord", "und", "See\u00b7len\u00b7raub", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter die Kruste von grauem Staub", "tokens": ["Un\u00b7ter", "die", "Krus\u00b7te", "von", "grau\u00b7em", "Staub"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "drang auch kein Luftzug der neuen Zeit.", "tokens": ["drang", "auch", "kein", "Luft\u00b7zug", "der", "neu\u00b7en", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Der alte Schulrat im alten Kleid.", "tokens": ["Der", "al\u00b7te", "Schul\u00b7rat", "im", "al\u00b7ten", "Kleid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wundert euch nicht! Was kommt aus dem Haus", "tokens": ["Wun\u00b7dert", "euch", "nicht", "!", "Was", "kommt", "aus", "dem", "Haus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$.", "PWS", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "schlie\u00dflich nach Oberprima heraus?", "tokens": ["schlie\u00df\u00b7lich", "nach", "O\u00b7berp\u00b7ri\u00b7ma", "he\u00b7raus", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Ein nationalistischer langer L\u00fcmmel.", "tokens": ["Ein", "na\u00b7ti\u00b7o\u00b7na\u00b7lis\u00b7ti\u00b7scher", "lan\u00b7ger", "L\u00fcm\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gut genug f\u00fcr den \u00c4mterschimmel.", "tokens": ["Gut", "ge\u00b7nug", "f\u00fcr", "den", "\u00c4m\u00b7ter\u00b7schim\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Gut genug f\u00fcr die alten Karrieren \u2013", "tokens": ["Gut", "ge\u00b7nug", "f\u00fcr", "die", "al\u00b7ten", "Kar\u00b7rie\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "als ob die heute noch notwendig w\u00e4ren!", "tokens": ["als", "ob", "die", "heu\u00b7te", "noch", "not\u00b7wen\u00b7dig", "w\u00e4\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "ART", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "T\u00fcren auf und Fenster auf!", "tokens": ["T\u00fc\u00b7ren", "auf", "und", "Fens\u00b7ter", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lege deine Hand darauf,", "tokens": ["Le\u00b7ge", "dei\u00b7ne", "Hand", "da\u00b7rauf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "lieber Herr Haenisch, und zeige den Jungen,", "tokens": ["lie\u00b7ber", "Herr", "Hae\u00b7nisch", ",", "und", "zei\u00b7ge", "den", "Jun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "NE", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "wie die alten Griechen sungen \u2013", "tokens": ["wie", "die", "al\u00b7ten", "Grie\u00b7chen", "sun\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "aber ohne die Philologie", "tokens": ["a\u00b7ber", "oh\u00b7ne", "die", "Phi\u00b7lo\u00b7lo\u00b7gie"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "und ohne die Kriegervereinsmelodie!", "tokens": ["und", "oh\u00b7ne", "die", "Krie\u00b7ger\u00b7ver\u00b7eins\u00b7me\u00b7lo\u00b7die", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.9": {"line.1": {"text": "Wer die Jugend hat, hat das Land.", "tokens": ["Wer", "die", "Ju\u00b7gend", "hat", ",", "hat", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "$,", "VAFIN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Unsre Kinder wachsen uns aus der Hand.", "tokens": ["Uns\u00b7re", "Kin\u00b7der", "wach\u00b7sen", "uns", "aus", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Und eh wir uns recht umgesehn,", "tokens": ["Und", "eh", "wir", "uns", "recht", "um\u00b7ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "im Handumdrehn,", "tokens": ["im", "Han\u00b7dum\u00b7drehn", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "sind durch die Schulen im S\u00fcden und Norden", "tokens": ["sind", "durch", "die", "Schu\u00b7len", "im", "S\u00fc\u00b7den", "und", "Nor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "APPRART", "NN", "KON", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "aus ihnen rechte Spie\u00dfb\u00fcrger geworden.", "tokens": ["aus", "ih\u00b7nen", "rech\u00b7te", "Spie\u00df\u00b7b\u00fcr\u00b7ger", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJA", "NN", "VAPP", "$."], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}}}}}