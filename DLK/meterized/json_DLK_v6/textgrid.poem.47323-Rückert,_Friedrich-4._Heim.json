{"textgrid.poem.47323": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4. Heim", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott geleite die armen traurigen Kranken heim!", "tokens": ["Gott", "ge\u00b7lei\u00b7te", "die", "ar\u00b7men", "trau\u00b7ri\u00b7gen", "Kran\u00b7ken", "heim", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Gott geleite die m\u00fcden irren Gedanken heim!", "tokens": ["Gott", "ge\u00b7lei\u00b7te", "die", "m\u00fc\u00b7den", "ir\u00b7ren", "Ge\u00b7dan\u00b7ken", "heim", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Gott verleihe dir einen Stab der Geduld, mein Herz!", "tokens": ["Gott", "ver\u00b7lei\u00b7he", "dir", "ei\u00b7nen", "Stab", "der", "Ge\u00b7duld", ",", "mein", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "M\u00fcder Wandrer! um am Stabe zu wanken heim.", "tokens": ["M\u00fc\u00b7der", "Wand\u00b7rer", "!", "um", "am", "Sta\u00b7be", "zu", "wan\u00b7ken", "heim", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "APPR", "APPRART", "NN", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Gott verleihe dir einen gn\u00e4digen Hauch, mein Schiff!", "tokens": ["Gott", "ver\u00b7lei\u00b7he", "dir", "ei\u00b7nen", "gn\u00e4\u00b7di\u00b7gen", "Hauch", ",", "mein", "Schiff", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Aus den Wogen des Unbestandes zu schwanken heim.", "tokens": ["Aus", "den", "Wo\u00b7gen", "des", "Un\u00b7be\u00b7stan\u00b7des", "zu", "schwan\u00b7ken", "heim", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Alle Triebe, dem dunklen Scho\u00dfe der Erd' entbl\u00fcht,", "tokens": ["Al\u00b7le", "Trie\u00b7be", ",", "dem", "dunk\u00b7len", "Scho\u00b7\u00dfe", "der", "Erd'", "ent\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Aufw\u00e4rts ringen sie, sich zum Lichte zu ranken heim.", "tokens": ["Auf\u00b7w\u00e4rts", "rin\u00b7gen", "sie", ",", "sich", "zum", "Lich\u00b7te", "zu", "ran\u00b7ken", "heim", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRF", "APPRART", "NN", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Alle duftigen Bl\u00fctenst\u00e4ubchen der Fr\u00fchlingslust,", "tokens": ["Al\u00b7le", "duf\u00b7ti\u00b7gen", "Bl\u00fc\u00b7ten\u00b7st\u00e4ub\u00b7chen", "der", "Fr\u00fch\u00b7lings\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Rastlos spr\u00fchen sie, bis zum Staube sie sanken heim.", "tokens": ["Rast\u00b7los", "spr\u00fc\u00b7hen", "sie", ",", "bis", "zum", "Stau\u00b7be", "sie", "san\u00b7ken", "heim", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "KOUS", "APPRART", "NN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Also sehnet Hafisens Seele sich himmelw\u00e4rts,", "tokens": ["Al\u00b7so", "seh\u00b7net", "Ha\u00b7fi\u00b7sens", "See\u00b7le", "sich", "him\u00b7mel\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NN", "PRF", "ADV", "$,"], "meter": "+-+-+--+--+--", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Und sein Irdisches zu den irdischen Schranken heim.", "tokens": ["Und", "sein", "Ir\u00b7di\u00b7sches", "zu", "den", "ir\u00b7di\u00b7schen", "Schran\u00b7ken", "heim", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}}}}}