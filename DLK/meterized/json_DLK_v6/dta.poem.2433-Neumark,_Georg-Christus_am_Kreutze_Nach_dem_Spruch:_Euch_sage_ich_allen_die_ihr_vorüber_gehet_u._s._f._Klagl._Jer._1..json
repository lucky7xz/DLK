{"dta.poem.2433": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Christus am Kreutze  \n Nach dem Spruch:  \n Euch sage ich allen die ihr vor\u00fcber gehet/  \n u. s. f. Klagl. Jer. 1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Steht still' ihr Sterbliche die ihr vor\u00fcber wandelt/", "tokens": ["Steht", "still'", "ihr", "Sterb\u00b7li\u00b7che", "die", "ihr", "vor\u00b7\u00fc\u00b7ber", "wan\u00b7delt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ART", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Seht meine Schmertzen an ach seht doch Liebste seht", "tokens": ["Seht", "mei\u00b7ne", "Schmert\u00b7zen", "an", "ach", "seht", "doch", "Liebs\u00b7te", "seht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ADV", "VVFIN", "ADV", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und nehmt es wol zu Sinn wie man mit mir gehandelt/", "tokens": ["Und", "nehmt", "es", "wol", "zu", "Sinn", "wie", "man", "mit", "mir", "ge\u00b7han\u00b7delt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NN", "KOKOM", "PIS", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist auch ein Schmertze wol der meinen \u00fcbergeht?", "tokens": ["Ist", "auch", "ein", "Schmert\u00b7ze", "wol", "der", "mei\u00b7nen", "\u00fc\u00b7ber\u00b7geht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "ART", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nein. In der gantzen Welt ist keiner nicht zu finden.", "tokens": ["Nein", ".", "In", "der", "gant\u00b7zen", "Welt", "ist", "kei\u00b7ner", "nicht", "zu", "fin\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "APPR", "ART", "ADJA", "NN", "VAFIN", "PIS", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wer war denn Schuld daran? Es waren eure S\u00fcnden.", "tokens": ["Wer", "war", "denn", "Schuld", "da\u00b7ran", "?", "Es", "wa\u00b7ren", "eu\u00b7re", "S\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "NN", "PAV", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}