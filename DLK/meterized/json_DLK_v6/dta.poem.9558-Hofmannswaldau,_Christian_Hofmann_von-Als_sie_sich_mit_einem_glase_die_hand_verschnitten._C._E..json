{"dta.poem.9558": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie sich mit einem glase die hand  \n verschnitten.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So recht! meyneid'ge hand! so werd' ich einst gerochen/", "tokens": ["So", "recht", "!", "mey\u00b7nei\u00b7d'\u00b7ge", "hand", "!", "so", "werd'", "ich", "einst", "ge\u00b7ro\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "ADJA", "NN", "$.", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und deine falschheit wird mit gleicher maa\u00df belohnt.", "tokens": ["Und", "dei\u00b7ne", "falschheit", "wird", "mit", "glei\u00b7cher", "maa\u00df", "be\u00b7lohnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ha! unbest\u00e4ndige! noch leichter als der mond/", "tokens": ["Ha", "!", "un\u00b7be\u00b7st\u00e4n\u00b7di\u00b7ge", "!", "noch", "leich\u00b7ter", "als", "der", "mond", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADJA", "$.", "ADV", "ADJD", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie pflag ich nicht vor dem auff deine treu zu pochen?", "tokens": ["Wie", "pflag", "ich", "nicht", "vor", "dem", "auff", "dei\u00b7ne", "treu", "zu", "po\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "APPR", "PPOSAT", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was hab ich mir nicht stets vor gunst von dir versprochen?", "tokens": ["Was", "hab", "ich", "mir", "nicht", "stets", "vor", "gunst", "von", "dir", "ver\u00b7spro\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PPER", "PTKNEG", "ADV", "APPR", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da aber deine huld auch frembden dienste frohnt/", "tokens": ["Da", "a\u00b7ber", "dei\u00b7ne", "huld", "auch", "fremb\u00b7den", "diens\u00b7te", "frohnt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "ADV", "ADJA", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und so viel andre mehr dein schmeicheln schon gewohnt/", "tokens": ["Und", "so", "viel", "and\u00b7re", "mehr", "dein", "schmei\u00b7cheln", "schon", "ge\u00b7wohnt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PIS", "ADV", "PPOSAT", "VVFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So wirst du billich itzt bi\u00df auff das blut gestochen.", "tokens": ["So", "wirst", "du", "bil\u00b7lich", "itzt", "bi\u00df", "auff", "das", "blut", "ge\u00b7sto\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADV", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wol mir! ein schlechtes gla\u00df sucht meinen schimpf zu r\u00e4chen!", "tokens": ["Wol", "mir", "!", "ein", "schlech\u00b7tes", "gla\u00df", "sucht", "mei\u00b7nen", "schimpf", "zu", "r\u00e4\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$.", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.10": {"text": "Die rache geht nach wunsch. Was hab ich nun zu sprechen?", "tokens": ["Die", "ra\u00b7che", "geht", "nach", "wunsch", ".", "Was", "hab", "ich", "nun", "zu", "spre\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "ADJD", "$.", "PWS", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Flie\u00df dann/ treuloses blut/ bi\u00df auf den letzten tropffen/", "tokens": ["Flie\u00df", "dann", "/", "treu\u00b7lo\u00b7ses", "blut", "/", "bi\u00df", "auf", "den", "letz\u00b7ten", "tropf\u00b7fen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$(", "ADJA", "NN", "$(", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "Ich werde nimmermehr die wunde dir verstopffen:", "tokens": ["Ich", "wer\u00b7de", "nim\u00b7mer\u00b7mehr", "die", "wun\u00b7de", "dir", "ver\u00b7stopf\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und w\u00fcrde deiner hand das leben selbst entf\u00fchret/", "tokens": ["Und", "w\u00fcr\u00b7de", "dei\u00b7ner", "hand", "das", "le\u00b7ben", "selbst", "ent\u00b7f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "PDS", "VVFIN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Gut! wenn kein ander nur mehr wird durch sie ber\u00fchret.", "tokens": ["Gut", "!", "wenn", "kein", "an\u00b7der", "nur", "mehr", "wird", "durch", "sie", "be\u00b7r\u00fch\u00b7ret", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "KOUS", "PIAT", "ADJD", "ADV", "ADV", "VAFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}