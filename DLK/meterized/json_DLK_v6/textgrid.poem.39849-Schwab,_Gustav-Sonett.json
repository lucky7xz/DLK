{"textgrid.poem.39849": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Sonett", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein ", "tokens": ["Mein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Auch heute send' ich dich auf eine Reise,", "tokens": ["Auch", "heu\u00b7te", "send'", "ich", "dich", "auf", "ei\u00b7ne", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn du noch s\u00e4ngst, zu dieser Wandrung Preise,", "tokens": ["Wenn", "du", "noch", "s\u00e4ngst", ",", "zu", "die\u00b7ser", "Wand\u00b7rung", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,", "APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcrwahr, nicht w\u00fcrdest du die Stimme sparen.", "tokens": ["F\u00fcr\u00b7wahr", ",", "nicht", "w\u00fcr\u00b7dest", "du", "die", "Stim\u00b7me", "spa\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKNEG", "VAFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Im Maienmond, von dem in jungen Jahren", "tokens": ["Im", "Mai\u00b7en\u00b7mond", ",", "von", "dem", "in", "jun\u00b7gen", "Jah\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du trunken sangest fast in jeder Weise,", "tokens": ["Du", "trun\u00b7ken", "san\u00b7gest", "fast", "in", "je\u00b7der", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durchwanderst du das Land in stetem Kreise", "tokens": ["Durch\u00b7wan\u00b7derst", "du", "das", "Land", "in", "ste\u00b7tem", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von ros'gen B\u00e4umen, Reben, Blumenscharen.", "tokens": ["Von", "ros'\u00b7gen", "B\u00e4u\u00b7men", ",", "Re\u00b7ben", ",", "Blu\u00b7men\u00b7scha\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Umringt von grauen Burgen, s\u00fc\u00dfer Mahnung", "tokens": ["Um\u00b7ringt", "von", "grau\u00b7en", "Bur\u00b7gen", ",", "s\u00fc\u00b7\u00dfer", "Mah\u00b7nung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An deine Zeit, begr\u00fc\u00dft dich Winzerhausen,", "tokens": ["An", "dei\u00b7ne", "Zeit", ",", "be\u00b7gr\u00fc\u00dft", "dich", "Win\u00b7zer\u00b7hau\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und f\u00fcllt dein Dichterherz mit Herbstesahnung.", "tokens": ["Und", "f\u00fcllt", "dein", "Dich\u00b7ter\u00b7herz", "mit", "Herbs\u00b7te\u00b7sah\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und, o wie lie\u00dfest du dein Brautlied schallen,", "tokens": ["Und", ",", "o", "wie", "lie\u00b7\u00dfest", "du", "dein", "Braut\u00b7lied", "schal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "FM", "KOKOM", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "S\u00e4hst du zur dichten Rebenlaube drau\u00dfen", "tokens": ["S\u00e4hst", "du", "zur", "dich\u00b7ten", "Re\u00b7ben\u00b7lau\u00b7be", "drau\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein selig Paar im Abendlichte wallen!", "tokens": ["Ein", "se\u00b7lig", "Paar", "im", "A\u00b7bend\u00b7lich\u00b7te", "wal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}