: ["NE", "$,", "ART", "NN", "APPR", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.75": {"text": "Tod? \u2013 Wir wollen beide weinen,", "tokens": ["Tod", "?", "\u2013", "Wir", "wol\u00b7len", "bei\u00b7de", "wei\u00b7nen", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "PPER", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.76": {"text": "Bis der Thr\u00e4nen Quell versiegt.", "tokens": ["Bis", "der", "Thr\u00e4\u00b7nen", "Quell", "ver\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.77": {"text": "Wer uns liebet, o! der weine", "tokens": ["Wer", "uns", "lie\u00b7bet", ",", "o", "!", "der", "wei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "FM", "$.", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.78": {"text": "Mit uns! Wer ihn hat gekannt,", "tokens": ["Mit", "uns", "!", "Wer", "ihn", "hat", "ge\u00b7kannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$.", "PWS", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.79": {"text": "Weint von selbst um ihn, dem keine", "tokens": ["Weint", "von", "selbst", "um", "ihn", ",", "dem", "kei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "ADV", "APPR", "PPER", "$,", "PRELS", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.80": {"text": "Mutter, jemals leer die Hand", "tokens": ["Mut\u00b7ter", ",", "je\u00b7mals", "leer", "die", "Hand"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "ADJD", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.81": {"text": "Reichte, ach! um ihn, der seine", "tokens": ["Reich\u00b7te", ",", "ach", "!", "um", "ihn", ",", "der", "sei\u00b7ne"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "ITJ", "$.", "APPR", "PPER", "$,", "PRELS", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.82": {"text": "Schmerzen, wie ein Mann bestand!", "tokens": ["Schmer\u00b7zen", ",", "wie", "ein", "Mann", "be\u00b7stand", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.83": {"text": "W\u00e4r' er einstens auf dem langen", "tokens": ["W\u00e4r'", "er", "eins\u00b7tens", "auf", "dem", "lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.84": {"text": "Rauhen Pfad', ins Heiligthum", "tokens": ["Rau\u00b7hen", "Pfad'", ",", "ins", "Hei\u00b7lig\u00b7thum"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.85": {"text": "Hoher Weisheit, eingegangen:", "tokens": ["Ho\u00b7her", "Weis\u00b7heit", ",", "ein\u00b7ge\u00b7gan\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.86": {"text": "Aller seiner Ahnen Ruhm", "tokens": ["Al\u00b7ler", "sei\u00b7ner", "Ah\u00b7nen", "Ruhm"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.87": {"text": "H\u00e4tt' er sicher \u00fcberschattet,", "tokens": ["H\u00e4tt'", "er", "si\u00b7cher", "\u00fc\u00b7bersc\u00b7hat\u00b7tet", "