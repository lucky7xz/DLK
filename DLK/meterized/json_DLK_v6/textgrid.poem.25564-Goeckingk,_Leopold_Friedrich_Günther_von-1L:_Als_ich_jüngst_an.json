{"textgrid.poem.25564": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als ich j\u00fcngst an ", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich j\u00fcngst an ", "tokens": ["Als", "ich", "j\u00fcngst", "an"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Mich des Wonnemondes freute;", "tokens": ["Mich", "des", "Won\u00b7ne\u00b7mon\u00b7des", "freu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als ich an la ", "tokens": ["Als", "ich", "an", "la"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NE"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Jedem Rheinschiff' das Geleite", "tokens": ["Je\u00b7dem", "Rhein\u00b7schiff'", "das", "Ge\u00b7lei\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit den Augen gab am Strand',", "tokens": ["Mit", "den", "Au\u00b7gen", "gab", "am", "Strand'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis in dunkelblauer Weite", "tokens": ["Bis", "in", "dun\u00b7kel\u00b7blau\u00b7er", "Wei\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mast und Wimpel uns verschwand;", "tokens": ["Mast", "und", "Wim\u00b7pel", "uns", "ver\u00b7schwand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Als bei ", "tokens": ["Als", "bei"], "token_info": ["word", "word"], "pos": ["KOUS", "APPR"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Ihm mein Herz entgegen h\u00fcpfte,", "tokens": ["Ihm", "mein", "Herz", "ent\u00b7ge\u00b7gen", "h\u00fcpf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "APPO", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und, vor ", "tokens": ["Und", ",", "vor"], "token_info": ["word", "punct", "word"], "pos": ["KON", "$,", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Ich mit ", "tokens": ["Ich", "mit"], "token_info": ["word", "word"], "pos": ["PPER", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.12": {"text": "Wie Petrarch und Laura kn\u00fcpfte;", "tokens": ["Wie", "Pe\u00b7trarch", "und", "Lau\u00b7ra", "kn\u00fcpf\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "KON", "NN", "VVFIN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.13": {"text": "Als ich weinend vor dem blinden,", "tokens": ["Als", "ich", "wei\u00b7nend", "vor", "dem", "blin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Doch zufriednen ", "tokens": ["Doch", "zu\u00b7fried\u00b7nen"], "token_info": ["word", "word"], "pos": ["KON", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Als in seinen Veilchengr\u00fcnden", "tokens": ["Als", "in", "sei\u00b7nen", "Veil\u00b7chen\u00b7gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Als so rasch am Kr\u00fcckenstabe", "tokens": ["Als", "so", "rasch", "am", "Kr\u00fc\u00b7cken\u00b7sta\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Und mein Herz, als kleine Gabe", "tokens": ["Und", "mein", "Herz", ",", "als", "klei\u00b7ne", "Ga\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "KOUS", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Auf der Pilgrimschaft zum Grabe,", "tokens": ["Auf", "der", "Pil\u00b7grim\u00b7schaft", "zum", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Nah am Ziel', noch mit sich nahm;", "tokens": ["Nah", "am", "Ziel'", ",", "noch", "mit", "sich", "nahm", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "ADV", "APPR", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Als mit mir bei Mondenscheine", "tokens": ["Als", "mit", "mir", "bei", "Mon\u00b7den\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "In dem bl\u00fchnden Lindenhaine", "tokens": ["In", "dem", "bl\u00fchn\u00b7den", "Lin\u00b7den\u00b7hai\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Ich am Fall' des Rheins, von Schaume", "tokens": ["Ich", "am", "Fall'", "des", "Rheins", ",", "von", "Schau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "APPRART", "NN", "ART", "NE", "$,", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Na\u00dfgespr\u00fctzt, ihm wie im Traume,", "tokens": ["Na\u00df\u00b7ge\u00b7spr\u00fctzt", ",", "ihm", "wie", "im", "Trau\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "KOKOM", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Staunend an dem Arme hing:", "tokens": ["Stau\u00b7nend", "an", "dem", "Ar\u00b7me", "hing", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Ach! da war mir wohl! Noch besser,", "tokens": ["Ach", "!", "da", "war", "mir", "wohl", "!", "Noch", "bes\u00b7ser", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VAFIN", "PPER", "ADV", "$.", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "(seufzt' ich dann f\u00fcr mich allein,)", "tokens": ["(", "seufzt'", "ich", "dann", "f\u00fcr", "mich", "al\u00b7lein", ",", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADV", "APPR", "PPER", "ADV", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Als am lieblichsten Gew\u00e4sser,", "tokens": ["Als", "am", "lieb\u00b7lichs\u00b7ten", "Ge\u00b7w\u00e4s\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "ADJA", "NN", "$,"], "meter": "--+---+-", "measure": "anapaest.init"}, "line.28": {"text": "Wird am Zorgaflu\u00df' dir seyn,", "tokens": ["Wird", "am", "Zor\u00b7gaflu\u00df'", "dir", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "PPER", "VAINF", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.29": {"text": "Wenn dein ", "tokens": ["Wenn", "dein"], "token_info": ["word", "word"], "pos": ["KOUS", "PPOSAT"], "meter": "-+", "measure": "iambic.single"}, "line.30": {"text": "Auf dem Steckenpferde springt,", "tokens": ["Auf", "dem", "Ste\u00b7cken\u00b7pfer\u00b7de", "springt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Und dir alle sein Verm\u00f6gen \u2013", "tokens": ["Und", "dir", "al\u00b7le", "sein", "Ver\u00b7m\u00f6\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PIAT", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Seine bunte Trommel \u2013 bringt;", "tokens": ["Sei\u00b7ne", "bun\u00b7te", "Trom\u00b7mel", "\u2013", "bringt", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Um mein Knie die Arme schl\u00e4gt,", "tokens": ["Um", "mein", "Knie", "die", "Ar\u00b7me", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.34": {"text": "Hererz\u00e4hlet seine Thaten", "tokens": ["Her\u00b7er\u00b7z\u00e4h\u00b7let", "sei\u00b7ne", "Tha\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Und Vocabeln, und mich fr\u00e4gt:", "tokens": ["Und", "Vo\u00b7ca\u00b7beln", ",", "und", "mich", "fr\u00e4gt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.36": {"text": "Bleibst nun bei uns \u00fcber Nacht?", "tokens": ["Bleibst", "nun", "bei", "uns", "\u00fc\u00b7ber", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "APPR", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.37": {"text": "Hast nicht bleyerne Soldaten", "tokens": ["Hast", "nicht", "bley\u00b7er\u00b7ne", "Sol\u00b7da\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Mir von ", "tokens": ["Mir", "von"], "token_info": ["word", "word"], "pos": ["NE", "APPR"], "meter": "++", "measure": "spondeus"}, "line.39": {"text": "Aber ach! mit bleichen Wangen,", "tokens": ["A\u00b7ber", "ach", "!", "mit", "blei\u00b7chen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Und in traurendem Gewand',", "tokens": ["Und", "in", "trau\u00b7ren\u00b7dem", "Ge\u00b7wand'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Kam die Mutter, an der Hand", "tokens": ["Kam", "die", "Mut\u00b7ter", ",", "an", "der", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.42": {"text": "Unsern Fritz, dahergegangen.", "tokens": ["Un\u00b7sern", "Fritz", ",", "da\u00b7her\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "NE", "$,", "PAV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Beide schwiegen; ich verstand", "tokens": ["Bei\u00b7de", "schwie\u00b7gen", ";", "ich", "ver\u00b7stand"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.44": {"text": "Dieses f\u00fcrchterliche Schweigen. \u2013", "tokens": ["Die\u00b7ses", "f\u00fcrch\u00b7ter\u00b7li\u00b7che", "Schwei\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PDAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Sch\u00f6nes Veilchen, mu\u00dftest du", "tokens": ["Sch\u00f6\u00b7nes", "Veil\u00b7chen", ",", "mu\u00df\u00b7test", "du"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "VMFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.46": {"text": "Schon so fr\u00fch der Erde zu", "tokens": ["Schon", "so", "fr\u00fch", "der", "Er\u00b7de", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "ART", "NN", "PTKZU"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Deinen Kelch mit Balsam, neigen?", "tokens": ["Dei\u00b7nen", "Kelch", "mit", "Bal\u00b7sam", ",", "nei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NE", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Wein' dich aus, du volles Herz!", "tokens": ["Wein'", "dich", "aus", ",", "du", "vol\u00b7les", "Herz", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.49": {"text": "Thr\u00e4nen kannst du nur vergeuden.", "tokens": ["Thr\u00e4\u00b7nen", "kannst", "du", "nur", "ver\u00b7geu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Meiner Liebe lange Leiden,", "tokens": ["Mei\u00b7ner", "Lie\u00b7be", "lan\u00b7ge", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Meiner Augen Folterschmerz,", "tokens": ["Mei\u00b7ner", "Au\u00b7gen", "Fol\u00b7ter\u00b7schmerz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.52": {"text": "Konnt' ich mir versingen. Doch", "tokens": ["Konnt'", "ich", "mir", "ver\u00b7sin\u00b7gen", ".", "Doch"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$.", "KON"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.53": {"text": "Meine Lipp' ist itzt verstummet!", "tokens": ["Mei\u00b7ne", "Lipp'", "ist", "itzt", "ver\u00b7stum\u00b7met", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Denn vor meinem Ohre summet", "tokens": ["Denn", "vor", "mei\u00b7nem", "Oh\u00b7re", "sum\u00b7met"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "H\u00e4tt' ich deinen Ruf geh\u00f6rt:", "tokens": ["H\u00e4tt'", "ich", "dei\u00b7nen", "Ruf", "ge\u00b7h\u00f6rt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.56": {"text": "Ach mein Sohn! aus fernem Lande", "tokens": ["Ach", "mein", "Sohn", "!", "aus", "fer\u00b7nem", "Lan\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "PPOSAT", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.57": {"text": "W\u00e4r' ich schnell zur\u00fcckgekehrt.", "tokens": ["W\u00e4r'", "ich", "schnell", "zu\u00b7r\u00fcck\u00b7ge\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.58": {"text": "Doch wozu? Um dich im Sande", "tokens": ["Doch", "wo\u00b7zu", "?", "Um", "dich", "im", "San\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "$.", "KOUI", "PRF", "APPRART", "NN"], "meter": "+-----+-", "measure": "dactylic.init"}, "line.59": {"text": "Zu verscharren? O mein Sohn!", "tokens": ["Zu", "ver\u00b7schar\u00b7ren", "?", "O", "mein", "Sohn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$.", "NE", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.60": {"text": "Trankest du den s\u00fc\u00dfen Mohn", "tokens": ["Tran\u00b7kest", "du", "den", "s\u00fc\u00b7\u00dfen", "Mohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.61": {"text": "Aus des Todes Becher schon,", "tokens": ["Aus", "des", "To\u00b7des", "Be\u00b7cher", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.62": {"text": "Eh' ich selbst ihn kosten durfte?", "tokens": ["Eh'", "ich", "selbst", "ihn", "kos\u00b7ten", "durf\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.63": {"text": "W\u00e4r' es m\u00f6glich: Gott! ich schlurfte", "tokens": ["W\u00e4r'", "es", "m\u00f6g\u00b7lich", ":", "Gott", "!", "ich", "schlurf\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "$.", "NN", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.64": {"text": "Rein, f\u00fcr dich, noch itzt ihn aus,", "tokens": ["Rein", ",", "f\u00fcr", "dich", ",", "noch", "itzt", "ihn", "aus", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPER", "$,", "ADV", "ADV", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.65": {"text": "H\u00fclfe dir aus deinem Grabe", "tokens": ["H\u00fcl\u00b7fe", "dir", "aus", "dei\u00b7nem", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.66": {"text": "Wieder an das Licht heraus!", "tokens": ["Wie\u00b7der", "an", "das", "Licht", "he\u00b7raus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.67": {"text": "Denn seit ich nicht dich mehr habe,", "tokens": ["Denn", "seit", "ich", "nicht", "dich", "mehr", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.68": {"text": "Losch die Freud' ihr L\u00e4mpchen aus.", "tokens": ["Losch", "die", "Freud'", "ihr", "L\u00e4mp\u00b7chen", "aus", "