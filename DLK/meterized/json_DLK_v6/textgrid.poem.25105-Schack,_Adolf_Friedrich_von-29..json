{"textgrid.poem.25105": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der mich geboren, zweiter August,", "tokens": ["Der", "mich", "ge\u00b7bo\u00b7ren", ",", "zwei\u00b7ter", "Au\u00b7gust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Deiner tauigen D\u00e4mmerung Lust,", "tokens": ["Dei\u00b7ner", "tau\u00b7i\u00b7gen", "D\u00e4m\u00b7me\u00b7rung", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "K\u00f6nnt' ich je sie vers\u00e4umen?", "tokens": ["K\u00f6nnt'", "ich", "je", "sie", "ver\u00b7s\u00e4u\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Eh noch ein Lichtstrahl die Lerche weckt,", "tokens": ["Eh", "noch", "ein", "Licht\u00b7strahl", "die", "Ler\u00b7che", "weckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Auf den H\u00fcgel lieg' ich gestreckt", "tokens": ["Auf", "den", "H\u00fc\u00b7gel", "lieg'", "ich", "ge\u00b7streckt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "VVPP"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Unter den schlummernden B\u00e4umen;", "tokens": ["Un\u00b7ter", "den", "schlum\u00b7mern\u00b7den", "B\u00e4u\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "H\u00f6re den Bach im Morgenwind", "tokens": ["H\u00f6\u00b7re", "den", "Bach", "im", "Mor\u00b7gen\u00b7wind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "APPRART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Lallen wie ein erwachendes Kind,", "tokens": ["Lal\u00b7len", "wie", "ein", "er\u00b7wa\u00b7chen\u00b7des", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und das frohe Geschmetter", "tokens": ["Und", "das", "fro\u00b7he", "Ge\u00b7schmet\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "All der gefiederten S\u00e4nger umher,", "tokens": ["All", "der", "ge\u00b7fie\u00b7der\u00b7ten", "S\u00e4n\u00b7ger", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Wie sie mit Fl\u00fcgeln, von Tau noch schwer,", "tokens": ["Wie", "sie", "mit", "Fl\u00fc\u00b7geln", ",", "von", "Tau", "noch", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "$,", "APPR", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Huschen durch zitternde Bl\u00e4tter.", "tokens": ["Hu\u00b7schen", "durch", "zit\u00b7tern\u00b7de", "Bl\u00e4t\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Und in der Fr\u00fche s\u00e4uselndem Hauch", "tokens": ["Und", "in", "der", "Fr\u00fc\u00b7he", "s\u00e4u\u00b7seln\u00b7dem", "Hauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Alle die munteren Geister auch", "tokens": ["Al\u00b7le", "die", "mun\u00b7te\u00b7ren", "Geis\u00b7ter", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "ART", "ADJA", "NN", "ADV"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "F\u00fchl' ich im Herzen erwachen;", "tokens": ["F\u00fchl'", "ich", "im", "Her\u00b7zen", "er\u00b7wa\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wie, wenn die Stunde des Lernens vorbei,", "tokens": ["Wie", ",", "wenn", "die", "Stun\u00b7de", "des", "Ler\u00b7nens", "vor\u00b7bei", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Knaben sich jagen mit Jubelgeschrei,", "tokens": ["Kna\u00b7ben", "sich", "ja\u00b7gen", "mit", "Ju\u00b7bel\u00b7ge\u00b7schrei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVFIN", "APPR", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Tummeln sie sich und lachen,", "tokens": ["Tum\u00b7meln", "sie", "sich", "und", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "KON", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Wecken zum Singen die V\u00f6gel im Nest,", "tokens": ["We\u00b7cken", "zum", "Sin\u00b7gen", "die", "V\u00f6\u00b7gel", "im", "Nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Sch\u00fctteln mir Aepfel herab f\u00fcr das Fest,", "tokens": ["Sch\u00fct\u00b7teln", "mir", "A\u00b7e\u00b7pfel", "her\u00b7ab", "f\u00fcr", "das", "Fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+--++--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "N\u00fcsse vom Haselgest\u00e4ude \u2013", "tokens": ["N\u00fcs\u00b7se", "vom", "Ha\u00b7sel\u00b7ge\u00b7st\u00e4u\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Zweiter August, du, der mich gebar,", "tokens": ["Zwei\u00b7ter", "Au\u00b7gust", ",", "du", ",", "der", "mich", "ge\u00b7bar", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "$,", "PRELS", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Immer verj\u00fcnge von Jahr zu Jahr", "tokens": ["Im\u00b7mer", "ver\u00b7j\u00fcn\u00b7ge", "von", "Jahr", "zu", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "APPR", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "So mir der Kindheit Freude!", "tokens": ["So", "mir", "der", "Kind\u00b7heit", "Freu\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}