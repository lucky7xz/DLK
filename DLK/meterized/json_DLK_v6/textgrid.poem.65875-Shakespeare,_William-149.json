{"textgrid.poem.65875": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "149", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sagst du, Harte, da\u00df ich lieblos sei,", "tokens": ["Wie", "sagst", "du", ",", "Har\u00b7te", ",", "da\u00df", "ich", "lieb\u00b7los", "sei", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "NN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn ich mich opfernd selbst f\u00fcr dich gef\u00e4hrde?", "tokens": ["Wenn", "ich", "mich", "op\u00b7fernd", "selbst", "f\u00fcr", "dich", "ge\u00b7f\u00e4hr\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vergess' ich dich, wenn ich mir selbst nicht treu,", "tokens": ["Ver\u00b7ge\u00b7ss'", "ich", "dich", ",", "wenn", "ich", "mir", "selbst", "nicht", "treu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "$,", "KOUS", "PPER", "PPER", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Mein eigner Peiniger um deinetwillen werde?", "tokens": ["Mein", "eig\u00b7ner", "Pei\u00b7ni\u00b7ger", "um", "dei\u00b7net\u00b7wil\u00b7len", "wer\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer will dir \u00fcbel, dem ich freundlich w\u00e4r?", "tokens": ["Wer", "will", "dir", "\u00fc\u00b7bel", ",", "dem", "ich", "freund\u00b7lich", "w\u00e4r", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADJD", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wem grolltest du, vor dem ich mich gebogen?", "tokens": ["Wem", "groll\u00b7test", "du", ",", "vor", "dem", "ich", "mich", "ge\u00b7bo\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "APPR", "PRELS", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wenn du mir finster sahst, hab' ich nicht schwer", "tokens": ["Wenn", "du", "mir", "fins\u00b7ter", "sahst", ",", "hab'", "ich", "nicht", "schwer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$,", "VAFIN", "PPER", "PTKNEG", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die Rach' an mir mit strengem Gram vollzogen?", "tokens": ["Die", "Rach'", "an", "mir", "mit", "stren\u00b7gem", "Gram", "voll\u00b7zo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Welch ein Verdienst in mir acht' ich so gro\u00df,", "tokens": ["Welch", "ein", "Ver\u00b7dienst", "in", "mir", "acht'", "ich", "so", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "APPR", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "Das deinen Dienst so stolz w\u00e4r zu verschm\u00e4hen,", "tokens": ["Das", "dei\u00b7nen", "Dienst", "so", "stolz", "w\u00e4r", "zu", "ver\u00b7schm\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ADV", "ADJD", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Da all mein Bestes deinen M\u00e4ngeln kost,", "tokens": ["Da", "all", "mein", "Bes\u00b7tes", "dei\u00b7nen", "M\u00e4n\u00b7geln", "kost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Befehligt schon von deiner Augen Drehen?", "tokens": ["Be\u00b7feh\u00b7ligt", "schon", "von", "dei\u00b7ner", "Au\u00b7gen", "Dre\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Doch, hasse nur! ich wei\u00df wie du gesinnt:", "tokens": ["Doch", ",", "has\u00b7se", "nur", "!", "ich", "wei\u00df", "wie", "du", "ge\u00b7sinnt", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "ADV", "$.", "PPER", "VVFIN", "KOKOM", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Du liebst nur Sehende, und ich bin blind.", "tokens": ["Du", "liebst", "nur", "Se\u00b7hen\u00b7de", ",", "und", "ich", "bin", "blind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$,", "KON", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}