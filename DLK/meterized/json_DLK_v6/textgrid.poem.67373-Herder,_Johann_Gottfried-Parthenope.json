{"textgrid.poem.67373": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Parthenope", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erm\u00fcdet von des Tages schwerem Brande,", "tokens": ["Er\u00b7m\u00fc\u00b7det", "von", "des", "Ta\u00b7ges", "schwe\u00b7rem", "Bran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Setzt' ich danieder mich ans k\u00fchle Meer;", "tokens": ["Setzt'", "ich", "da\u00b7nie\u00b7der", "mich", "ans", "k\u00fch\u00b7le", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Wellen wallten liebend hin zum Strande", "tokens": ["Die", "Wel\u00b7len", "wall\u00b7ten", "lie\u00b7bend", "hin", "zum", "Stran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des holden Ufers, das mich rings umher", "tokens": ["Des", "hol\u00b7den", "U\u00b7fers", ",", "das", "mich", "rings", "um\u00b7her"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Umfing mit seinem zaubrischen Gewande,", "tokens": ["Um\u00b7fing", "mit", "sei\u00b7nem", "zaub\u00b7ri\u00b7schen", "Ge\u00b7wan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit seiner gaukelnden Sylphiden Heer;", "tokens": ["Mit", "sei\u00b7ner", "gau\u00b7keln\u00b7den", "Syl\u00b7phi\u00b7den", "Heer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Liebe luft'ger Schleier, rings umflogen", "tokens": ["Der", "Lie\u00b7be", "luft'\u00b7ger", "Schlei\u00b7er", ",", "rings", "um\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Von Zephyretten, spielte mit den Wogen.", "tokens": ["Von", "Ze\u00b7phy\u00b7ret\u00b7ten", ",", "spiel\u00b7te", "mit", "den", "Wo\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und \u00fcber mir, hoch \u00fcber mir in L\u00fcften", "tokens": ["Und", "\u00fc\u00b7ber", "mir", ",", "hoch", "\u00fc\u00b7ber", "mir", "in", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "$,", "ADJD", "APPR", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des blauen Aethers s\u00e4uselte der Baum,", "tokens": ["Des", "blau\u00b7en", "A\u00b7e\u00b7thers", "s\u00e4u\u00b7sel\u00b7te", "der", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Der, reingel\u00e4utert von der Erde D\u00fcften,", "tokens": ["Der", ",", "rein\u00b7ge\u00b7l\u00e4u\u00b7tert", "von", "der", "Er\u00b7de", "D\u00fcf\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein himmlisches Gew\u00e4chs, den runden Saum", "tokens": ["Ein", "himm\u00b7li\u00b7sches", "Ge\u00b7w\u00e4chs", ",", "den", "run\u00b7den", "Saum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Umschreibet mit der Sonne goldnen Schriften", "tokens": ["Um\u00b7schrei\u00b7bet", "mit", "der", "Son\u00b7ne", "gold\u00b7nen", "Schrif\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und giebt dem Fluge der Begeistrung Raum;", "tokens": ["Und", "giebt", "dem", "Flu\u00b7ge", "der", "Be\u00b7geis\u00b7trung", "Raum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die schlanke, sch\u00f6ne K\u00f6nigin der B\u00e4ume,", "tokens": ["Die", "schlan\u00b7ke", ",", "sch\u00f6\u00b7ne", "K\u00f6\u00b7ni\u00b7gin", "der", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Pinje, rauschte mich in goldne Tr\u00e4ume.", "tokens": ["Die", "Pin\u00b7je", ",", "rauschte", "mich", "in", "gold\u00b7ne", "Tr\u00e4u\u00b7me", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich h\u00f6rt', und aus des Meeres leisen Wogen", "tokens": ["Ich", "h\u00f6rt'", ",", "und", "aus", "des", "Mee\u00b7res", "lei\u00b7sen", "Wo\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erhob sich einer Stimme Silberton:", "tokens": ["Er\u00b7hob", "sich", "ei\u00b7ner", "Stim\u00b7me", "Sil\u00b7ber\u00b7ton", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbvernimm mich! Nie hat Dich Dein Herz betrogen;", "tokens": ["\u00bb", "ver\u00b7nimm", "mich", "!", "Nie", "hat", "Dich", "Dein", "Herz", "be\u00b7tro\u00b7gen", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "$.", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du liebtest Wahrheit und verdienst zum Lohn,", "tokens": ["Du", "lieb\u00b7test", "Wahr\u00b7heit", "und", "ver\u00b7dienst", "zum", "Lohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df Dir die H\u00fclle werd' emporgezogen,", "tokens": ["Da\u00df", "Dir", "die", "H\u00fcl\u00b7le", "werd'", "em\u00b7por\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die alle Wesen bis zum lichten Thron", "tokens": ["Die", "al\u00b7le", "We\u00b7sen", "bis", "zum", "lich\u00b7ten", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "APPR", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der schaffenden Natur in Schatten h\u00fcllet.", "tokens": ["Der", "schaf\u00b7fen\u00b7den", "Na\u00b7tur", "in", "Schat\u00b7ten", "h\u00fcl\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Vernimm mich, und Dein Herz wird Dir gestillet!\u00ab", "tokens": ["Ver\u00b7nimm", "mich", ",", "und", "Dein", "Herz", "wird", "Dir", "ge\u00b7stil\u00b7let", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "$,", "KON", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich sah, und aus des Meeres zarten Wellen", "tokens": ["Ich", "sah", ",", "und", "aus", "des", "Mee\u00b7res", "zar\u00b7ten", "Wel\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hob eine Nymphe g\u00f6ttlich sich empor.", "tokens": ["Hob", "ei\u00b7ne", "Nym\u00b7phe", "g\u00f6tt\u00b7lich", "sich", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJD", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Antlitz schien die D\u00e4mmrung aufzuhellen", "tokens": ["Ihr", "Ant\u00b7litz", "schien", "die", "D\u00e4mm\u00b7rung", "auf\u00b7zu\u00b7hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis an der Sonne goldnes Abendthor;", "tokens": ["Bis", "an", "der", "Son\u00b7ne", "gold\u00b7nes", "A\u00b7bend\u00b7thor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Wogen k\u00fc\u00dften sie mit sanftem Schwellen,", "tokens": ["Die", "Wo\u00b7gen", "k\u00fc\u00df\u00b7ten", "sie", "mit", "sanf\u00b7tem", "Schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Um ihren Busen wallt' ein reger Flor.", "tokens": ["Um", "ih\u00b7ren", "Bu\u00b7sen", "wallt'", "ein", "re\u00b7ger", "Flor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sie sang; ein Saitenspiel von zarten Saiten", "tokens": ["Sie", "sang", ";", "ein", "Sai\u00b7ten\u00b7spiel", "von", "zar\u00b7ten", "Sai\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "War sch\u00fcchtern, ihre Stimme zu begleiten.", "tokens": ["War", "sch\u00fcch\u00b7tern", ",", "ih\u00b7re", "Stim\u00b7me", "zu", "be\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVINF", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sie sang: \u00bbWas rings Dir Deine Blicke zeigen,", "tokens": ["Sie", "sang", ":", "\u00bb", "Was", "rings", "Dir", "Dei\u00b7ne", "Bli\u00b7cke", "zei\u00b7gen", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PWS", "ADV", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was alldurchwallend die Natur bewegt,", "tokens": ["Was", "all\u00b7durch\u00b7wal\u00b7lend", "die", "Na\u00b7tur", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was droben dort in jenem heil'gen Schweigen", "tokens": ["Was", "dro\u00b7ben", "dort", "in", "je\u00b7nem", "heil'\u00b7gen", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADV", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Aethers, drunten sich im Staube regt", "tokens": ["Des", "A\u00b7e\u00b7thers", ",", "drun\u00b7ten", "sich", "im", "Stau\u00b7be", "regt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "PRF", "APPRART", "NN", "VVFIN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und in der Welle spielt und in den Zweigen", "tokens": ["Und", "in", "der", "Wel\u00b7le", "spielt", "und", "in", "den", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Fichte rauscht und Dir im Herzen schl\u00e4gt", "tokens": ["Der", "Fich\u00b7te", "rauscht", "und", "Dir", "im", "Her\u00b7zen", "schl\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und Dir im Auge, jetzt von Thr\u00e4nen tr\u00fcbe,", "tokens": ["Und", "Dir", "im", "Au\u00b7ge", ",", "jetzt", "von", "Thr\u00e4\u00b7nen", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "$,", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Jetzt freudetrunken, himmlisch gl\u00e4nzt, ist \u2013 ", "tokens": ["Jetzt", "freu\u00b7de\u00b7trun\u00b7ken", ",", "himm\u00b7lisch", "gl\u00e4nzt", ",", "ist", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "ADJD", "VVFIN", "$,", "VAFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Nur Liebe war die Sch\u00f6pferin der Wesen", "tokens": ["Nur", "Lie\u00b7be", "war", "die", "Sch\u00f6p\u00b7fe\u00b7rin", "der", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ward der Liebgebornen Lehrerin;", "tokens": ["Und", "ward", "der", "Lieb\u00b7ge\u00b7bor\u00b7nen", "Leh\u00b7re\u00b7rin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Willst Du den Sinn des gro\u00dfen Buches lesen,", "tokens": ["Willst", "Du", "den", "Sinn", "des", "gro\u00b7\u00dfen", "Bu\u00b7ches", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das vor Dir liegt: sie ist die Seele drin.", "tokens": ["Das", "vor", "Dir", "liegt", ":", "sie", "ist", "die", "See\u00b7le", "drin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPER", "VVFIN", "$.", "PPER", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und will Dein Geist und soll Dein Herz genesen,", "tokens": ["Und", "will", "Dein", "Geist", "und", "soll", "Dein", "Herz", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "KON", "VMFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So folge treu der hohen F\u00fchrerin!", "tokens": ["So", "fol\u00b7ge", "treu", "der", "ho\u00b7hen", "F\u00fch\u00b7re\u00b7rin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wer au\u00dfer ihr, der Mutter alles Lebens,", "tokens": ["Wer", "au\u00b7\u00dfer", "ihr", ",", "der", "Mut\u00b7ter", "al\u00b7les", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "$,", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Natur und Wahrheit suchet, sucht vergebens.", "tokens": ["Na\u00b7tur", "und", "Wahr\u00b7heit", "su\u00b7chet", ",", "sucht", "ver\u00b7ge\u00b7bens", "."], "