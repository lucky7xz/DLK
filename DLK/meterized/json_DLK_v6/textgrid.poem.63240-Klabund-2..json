{"textgrid.poem.63240": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ich dir hier singe,", "tokens": ["Was", "ich", "dir", "hier", "sin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ist nur f\u00fcr dich gemacht.", "tokens": ["Ist", "nur", "f\u00fcr", "dich", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die violette Syringe,", "tokens": ["Die", "vi\u00b7o\u00b7let\u00b7te", "Sy\u00b7rin\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Der Mond und das Ding der Dinge", "tokens": ["Der", "Mond", "und", "das", "Ding", "der", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ist nur f\u00fcr dich gemacht.", "tokens": ["Ist", "nur", "f\u00fcr", "dich", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die heimliche Lust der L\u00fcste", "tokens": ["Die", "heim\u00b7li\u00b7che", "Lust", "der", "L\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ist nur f\u00fcr dich gemacht,", "tokens": ["Ist", "nur", "f\u00fcr", "dich", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O gib mir deine Br\u00fcste.", "tokens": ["O", "gib", "mir", "dei\u00b7ne", "Br\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVIMP", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ebbe und Flut unsrer K\u00fcste", "tokens": ["Eb\u00b7be", "und", "Flut", "uns\u00b7rer", "K\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "PPOSAT", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Sind nur f\u00fcr dich gemacht.", "tokens": ["Sind", "nur", "f\u00fcr", "dich", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das breite Bett, ich d\u00e4chte", "tokens": ["Das", "brei\u00b7te", "Bett", ",", "ich", "d\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es ist f\u00fcr dich gemacht.", "tokens": ["Es", "ist", "f\u00fcr", "dich", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Komm, l\u00f6se deine Flechte,", "tokens": ["Komm", ",", "l\u00f6\u00b7se", "dei\u00b7ne", "Flech\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Denn diese Nacht der N\u00e4chte,", "tokens": ["Denn", "die\u00b7se", "Nacht", "der", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie ist f\u00fcr uns gemacht.", "tokens": ["Sie", "ist", "f\u00fcr", "uns", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}