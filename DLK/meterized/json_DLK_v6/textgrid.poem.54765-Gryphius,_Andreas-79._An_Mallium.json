{"textgrid.poem.54765": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "79. An Mallium", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du spilst: ich glaub es nicht. Wann dich das Gl\u00fcck anlacht;", "tokens": ["Du", "spilst", ":", "ich", "glaub", "es", "nicht", ".", "Wann", "dich", "das", "Gl\u00fcck", "an\u00b7lacht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da ist kein Mensch den nicht dein leichtes Maul verlacht.", "tokens": ["Da", "ist", "kein", "Mensch", "den", "nicht", "dein", "leich\u00b7tes", "Maul", "ver\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "ART", "PTKNEG", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn sich das Blatt verkehrt und andere gewinnen:", "tokens": ["Wenn", "sich", "das", "Blatt", "ver\u00b7kehrt", "und", "an\u00b7de\u00b7re", "ge\u00b7win\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVPP", "KON", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dann sitzst und tobest du als da\u00df du sonder Sinnen.", "tokens": ["Dann", "sitz\u00b7st", "und", "to\u00b7best", "du", "als", "da\u00df", "du", "son\u00b7der", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "KOKOM", "KOUS", "PPER", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Du fluchst/ du pochst/ du schnarchst/ du schreyst den Himmel an/", "tokens": ["Du", "fluchst", "/", "du", "pochst", "/", "du", "schnarchst", "/", "du", "schreyst", "den", "Him\u00b7mel", "an", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "$(", "PPER", "VVFIN", "$(", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So rasend als kein Hund/ der t\u00f6richt/ w\u00fctten kan.", "tokens": ["So", "ra\u00b7send", "als", "kein", "Hund", "/", "der", "t\u00f6\u00b7richt", "/", "w\u00fct\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PIAT", "NN", "$(", "ART", "NN", "$(", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du weist da\u00df ich di\u00df wei\u00df' und lockst mich: kom ich will", "tokens": ["Du", "weist", "da\u00df", "ich", "di\u00df", "wei\u00df'", "und", "lockst", "mich", ":", "kom", "ich", "will"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "PDS", "PTKVZ", "KON", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Itzt mit dir spilen. Nein! dein spilen ist kein Spill.", "tokens": ["Itzt", "mit", "dir", "spi\u00b7len", ".", "Nein", "!", "dein", "spi\u00b7len", "ist", "kein", "Spill", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVINF", "$.", "PTKANT", "$.", "PPOSAT", "ADJA", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}