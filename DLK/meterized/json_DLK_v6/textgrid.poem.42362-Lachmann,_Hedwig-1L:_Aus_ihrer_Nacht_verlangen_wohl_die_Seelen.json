{"textgrid.poem.42362": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus ihrer Nacht verlangen wohl die Seelen", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus ihrer Nacht verlangen wohl die Seelen", "tokens": ["Aus", "ih\u00b7rer", "Nacht", "ver\u00b7lan\u00b7gen", "wohl", "die", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Licht der Welt einander zu begegnen.", "tokens": ["Im", "Licht", "der", "Welt", "ein\u00b7an\u00b7der", "zu", "be\u00b7geg\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und suchend wagen sie sich zu entlegnen", "tokens": ["Und", "su\u00b7chend", "wa\u00b7gen", "sie", "sich", "zu", "ent\u00b7leg\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Pfadlosen Wolkenh\u00f6hn hinan und w\u00e4hlen.", "tokens": ["Pfad\u00b7lo\u00b7sen", "Wol\u00b7ken\u00b7h\u00f6hn", "hi\u00b7nan", "und", "w\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKVZ", "KON", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Aber verwirrt und sonnentrunken fehlen", "tokens": ["A\u00b7ber", "ver\u00b7wirrt", "und", "son\u00b7nen\u00b7trun\u00b7ken", "feh\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "NN", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und straucheln sie auf den unsichern Steigen", "tokens": ["Und", "strau\u00b7cheln", "sie", "auf", "den", "un\u00b7si\u00b7chern", "Stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und m\u00fcssen sich verh\u00fcllt hinunterstehlen", "tokens": ["Und", "m\u00fcs\u00b7sen", "sich", "ver\u00b7h\u00fcllt", "hin\u00b7un\u00b7ter\u00b7steh\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PRF", "VVPP", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und m\u00fcssen ihre bange Flucht verschweigen.", "tokens": ["Und", "m\u00fcs\u00b7sen", "ih\u00b7re", "ban\u00b7ge", "Flucht", "ver\u00b7schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nur wenn ein Stern sich annimmt der Verwegnen,", "tokens": ["Nur", "wenn", "ein", "Stern", "sich", "an\u00b7nimmt", "der", "Ver\u00b7weg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "PRF", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dann d\u00fcrfen sie dort oben sich verm\u00e4hlen,", "tokens": ["Dann", "d\u00fcr\u00b7fen", "sie", "dort", "o\u00b7ben", "sich", "ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sich von ihrem Sehnsuchtsflug erz\u00e4hlen", "tokens": ["Und", "sich", "von", "ih\u00b7rem", "Sehn\u00b7suchts\u00b7flug", "er\u00b7z\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ihren Stern mit stummen Zeichen segnen.", "tokens": ["Und", "ih\u00b7ren", "Stern", "mit", "stum\u00b7men", "Zei\u00b7chen", "seg\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}