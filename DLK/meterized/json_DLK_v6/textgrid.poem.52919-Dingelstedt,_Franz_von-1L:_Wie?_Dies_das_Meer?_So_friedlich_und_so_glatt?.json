{"textgrid.poem.52919": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie? Dies das Meer? So friedlich und so glatt?", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie? Dies das Meer? So friedlich und so glatt?", "tokens": ["Wie", "?", "Dies", "das", "Meer", "?", "So", "fried\u00b7lich", "und", "so", "glatt", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PDS", "ART", "NN", "$.", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nichts weiter, als die blanke Wasserfl\u00e4che?", "tokens": ["Nichts", "wei\u00b7ter", ",", "als", "die", "blan\u00b7ke", "Was\u00b7ser\u00b7fl\u00e4\u00b7che", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKVZ", "$,", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So zahm, wie ein politisch' Wochenblatt,", "tokens": ["So", "zahm", ",", "wie", "ein", "po\u00b7li\u00b7tisch'", "Wo\u00b7chen\u00b7blatt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "So hell, wie deutsche Philosophen-B\u00e4che?", "tokens": ["So", "hell", ",", "wie", "deut\u00b7sche", "Phi\u00b7lo\u00b7so\u00b7phen\u00b7B\u00e4\u00b7che", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie anders, anders hab' ich mirs getr\u00e4umt,", "tokens": ["Wie", "an\u00b7ders", ",", "an\u00b7ders", "hab'", "ich", "mirs", "ge\u00b7tr\u00e4umt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "ADV", "VAFIN", "PPER", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Daheim am Ofen, \u00fcber B\u00fcchern br\u00fctend;", "tokens": ["Da\u00b7heim", "am", "O\u00b7fen", ",", "\u00fc\u00b7ber", "B\u00fc\u00b7chern", "br\u00fc\u00b7tend", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist das ein Meer, das D\u00e4mme \u00fcbersch\u00e4umt", "tokens": ["Ist", "das", "ein", "Meer", ",", "das", "D\u00e4m\u00b7me", "\u00fc\u00b7bersc\u00b7h\u00e4umt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PDS", "ART", "NN", "$,", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Schiff und Fels verschlingt, gen Himmel w\u00fctend?", "tokens": ["Und", "Schiff", "und", "Fels", "ver\u00b7schlingt", ",", "gen", "Him\u00b7mel", "w\u00fc\u00b7tend", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "$,", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Fort schlich ich zur Kaj\u00fctentur hinein", "tokens": ["Fort", "schlich", "ich", "zur", "Ka\u00b7j\u00fc\u00b7ten\u00b7tur", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und setzte mich, wo viele andre sa\u00dfen;", "tokens": ["Und", "setz\u00b7te", "mich", ",", "wo", "vie\u00b7le", "and\u00b7re", "sa\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "PIS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie heimisch dort! Die M\u00e4nnlein tranken Wein,", "tokens": ["Wie", "hei\u00b7misch", "dort", "!", "Die", "M\u00e4nn\u00b7lein", "tran\u00b7ken", "Wein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "$.", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Indes die Fr\u00e4ulein strickten, g\u00e4hnten, lasen.", "tokens": ["In\u00b7des", "die", "Fr\u00e4u\u00b7lein", "strick\u00b7ten", ",", "g\u00e4hn\u00b7ten", ",", "la\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich tat wie sie und griff ein Zeitungsblatt", "tokens": ["Ich", "tat", "wie", "sie", "und", "griff", "ein", "Zei\u00b7tungs\u00b7blatt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "PPER", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und k\u00e4ute, was schon Hundert wiederk\u00e4uten;", "tokens": ["Und", "k\u00e4u\u00b7te", ",", "was", "schon", "Hun\u00b7dert", "wie\u00b7der\u00b7k\u00e4u\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "ADV", "CARD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das will, so seufzt' ich bald und hatt' es satt,", "tokens": ["Das", "will", ",", "so", "seufzt'", "ich", "bald", "und", "hatt'", "es", "satt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "KON", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein deutsches Meer, ein deutsches Volk bedeuten?", "tokens": ["Ein", "deut\u00b7sches", "Meer", ",", "ein", "deut\u00b7sches", "Volk", "be\u00b7deu\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}