{"textgrid.poem.60885": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "An den Dichter", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Treu der Natur und entwachsen der fl\u00fcchtigen Mode, beginne,", "tokens": ["Treu", "der", "Na\u00b7tur", "und", "ent\u00b7wach\u00b7sen", "der", "fl\u00fcch\u00b7ti\u00b7gen", "Mo\u00b7de", ",", "be\u00b7gin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Dichter, wiewohl einsam deinen unsterblichen Ton!", "tokens": ["Dich\u00b7ter", ",", "wie\u00b7wohl", "ein\u00b7sam", "dei\u00b7nen", "uns\u00b7terb\u00b7li\u00b7chen", "Ton", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "La\u00df ephemere Gesellen beschrein dich oder verkleinern:", "tokens": ["La\u00df", "ep\u00b7he\u00b7me\u00b7re", "Ge\u00b7sel\u00b7len", "be\u00b7schrein", "dich", "o\u00b7der", "ver\u00b7klei\u00b7nern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "ADJD", "PPER", "KON", "VVINF", "$."], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Jene vergehn, dir ward liebliche Dauer zu Teil.", "tokens": ["Je\u00b7ne", "ver\u00b7gehn", ",", "dir", "ward", "lieb\u00b7li\u00b7che", "Dau\u00b7er", "zu", "Teil", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "$,", "PPER", "VAFIN", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.5": {"text": "Ungleich ist ja der Kampf, es bewaffnet Jene der Wahn blo\u00df,", "tokens": ["Un\u00b7gleich", "ist", "ja", "der", "Kampf", ",", "es", "be\u00b7waff\u00b7net", "Je\u00b7ne", "der", "Wahn", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "ART", "NN", "$,", "PPER", "VVFIN", "PDS", "ART", "NN", "ADV", "$,"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "W\u00e4hrend, wie Pfeile, du wirfst Liebe, Gesang, Melodie.", "tokens": ["W\u00e4h\u00b7rend", ",", "wie", "Pfei\u00b7le", ",", "du", "wirfst", "Lie\u00b7be", ",", "Ge\u00b7sang", ",", "Me\u00b7lo\u00b7die", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "NN", "$,", "PPER", "VAFIN", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}}}}}