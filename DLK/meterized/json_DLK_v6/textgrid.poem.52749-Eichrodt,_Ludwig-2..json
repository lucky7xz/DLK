{"textgrid.poem.52749": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eichenstark und fischgesund,", "tokens": ["Ei\u00b7chen\u00b7stark", "und", "fischge\u00b7sund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Immer frisch im Herzensgrund,", "tokens": ["Im\u00b7mer", "frisch", "im", "Her\u00b7zens\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fromm und frei,", "tokens": ["Fromm", "und", "frei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Fr\u00f6hlich nebenbei!", "tokens": ["Fr\u00f6h\u00b7lich", "ne\u00b7ben\u00b7bei", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Turner schreckt kein Geizkobold,", "tokens": ["Tur\u00b7ner", "schreckt", "kein", "Geiz\u00b7ko\u00b7bold", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil Gewissen nie ihm grollt,", "tokens": ["Weil", "Ge\u00b7wis\u00b7sen", "nie", "ihm", "grollt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vaterland", "tokens": ["Va\u00b7ter\u00b7land"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Dr\u00fcckt er gern die Hand.", "tokens": ["Dr\u00fcckt", "er", "gern", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Hosentr\u00e4ger er verdammt,", "tokens": ["Ho\u00b7sen\u00b7tr\u00e4\u00b7ger", "er", "ver\u00b7dammt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hasset Seide, Gold und Sammt;", "tokens": ["Has\u00b7set", "Sei\u00b7de", ",", "Gold", "und", "Sammt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hemd von Hanf,", "tokens": ["Hemd", "von", "Hanf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Stuhlgang leicht und sanft.", "tokens": ["Stuhl\u00b7gang", "leicht", "und", "sanft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Vater Jahn, der ist sein Stern,", "tokens": ["Va\u00b7ter", "Jahn", ",", "der", "ist", "sein", "Stern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PRELS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Geht zur Kirch' am Tag des Herrn;", "tokens": ["Geht", "zur", "Kirch'", "am", "Tag", "des", "Herrn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schaffig, sch\u00f6n", "tokens": ["Schaf\u00b7fig", ",", "sch\u00f6n"], "token_info": ["word", "punct", "word"], "pos": ["ADJD", "$,", "ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Will als Mann er steh'n.", "tokens": ["Will", "als", "Mann", "er", "steh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["VMFIN", "KOUS", "NN", "PPER", "VVFIN", "NE"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Dr\u00fcckt im Geist die Hand dem Mann,", "tokens": ["Dr\u00fcckt", "im", "Geist", "die", "Hand", "dem", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der den Todessprung ersann,", "tokens": ["Der", "den", "To\u00b7des\u00b7sprung", "er\u00b7sann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trug und Schand',", "tokens": ["Trug", "und", "Schand'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Hat er nie gekannt.", "tokens": ["Hat", "er", "nie", "ge\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}