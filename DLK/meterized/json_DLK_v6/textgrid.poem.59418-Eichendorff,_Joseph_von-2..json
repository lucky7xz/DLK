{"textgrid.poem.59418": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "N\u00e4chtlich wandern alle Fl\u00fcsse", "tokens": ["N\u00e4cht\u00b7lich", "wan\u00b7dern", "al\u00b7le", "Fl\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Himmel, Stern auf Stern,", "tokens": ["Und", "der", "Him\u00b7mel", ",", "Stern", "auf", "Stern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Sendet so viel tausend Gr\u00fc\u00dfe,", "tokens": ["Sen\u00b7det", "so", "viel", "tau\u00b7send", "Gr\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df die W\u00e4lder nah und fern", "tokens": ["Da\u00df", "die", "W\u00e4l\u00b7der", "nah", "und", "fern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schauernd rauschen in den Gr\u00fcnden;", "tokens": ["Schau\u00b7ernd", "rau\u00b7schen", "in", "den", "Gr\u00fcn\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur der Mensch, dem Tod geweiht,", "tokens": ["Nur", "der", "Mensch", ",", "dem", "Tod", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Tr\u00e4umet fort von seinen S\u00fcnden", "tokens": ["Tr\u00e4u\u00b7met", "fort", "von", "sei\u00b7nen", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In der stillen Gnadenzeit.", "tokens": ["In", "der", "stil\u00b7len", "Gna\u00b7den\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}