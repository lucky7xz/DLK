{"dta.poem.21523": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Das war ein lust\u2019ges Feiern \u2026", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Das war ein lust\u2019ges Feiern,", "tokens": ["Das", "war", "ein", "lust'\u00b7ges", "Fei\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Schw\u00e4rmen bei Nacht und bei Tag \u2014", "tokens": ["Ein", "Schw\u00e4r\u00b7men", "bei", "Nacht", "und", "bei", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Nun liegt\u2019s auf mir so felsenhart,", "tokens": ["Nun", "liegt's", "auf", "mir", "so", "fel\u00b7sen\u00b7hart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jach sind mir Freud und Lust erstarrt,", "tokens": ["Jach", "sind", "mir", "Freud", "und", "Lust", "er\u00b7starrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Nun liegt\u2019s auf mir so bleiern", "tokens": ["Nun", "liegt's", "auf", "mir", "so", "blei\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nach all\u2019 dem lust\u2019gen Feiern,", "tokens": ["Nach", "all'", "dem", "lust'\u00b7gen", "Fei\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dem Schw\u00e4rmen bei Nacht und bei Tag \u2026", "tokens": ["Dem", "Schw\u00e4r\u00b7men", "bei", "Nacht", "und", "bei", "Tag", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Das war ein tolles Zechen \u2014", "tokens": ["Das", "war", "ein", "tol\u00b7les", "Ze\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir wurden\u2019s schier nicht satt \u2014", "tokens": ["Wir", "wur\u00b7den's", "schier", "nicht", "satt"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jach starb mir da der bl\u00fch\u2019nde Scherz,", "tokens": ["Jach", "starb", "mir", "da", "der", "bl\u00fch'n\u00b7de", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun liegt\u2019s auf mir wie schweres Erz,", "tokens": ["Nun", "liegt's", "auf", "mir", "wie", "schwe\u00b7res", "Erz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als wollte das Hirn mir zerbrechen \u2014", "tokens": ["Als", "woll\u00b7te", "das", "Hirn", "mir", "zer\u00b7bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "ART", "NN", "PPER", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Nach all\u2019 dem lust\u2019gen Zechen,", "tokens": ["Nach", "all'", "dem", "lust'\u00b7gen", "Ze\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dem Schw\u00e4rmen bei Nacht und bei Tag \u2026", "tokens": ["Dem", "Schw\u00e4r\u00b7men", "bei", "Nacht", "und", "bei", "Tag", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Das war ein keck Erfassen", "tokens": ["Das", "war", "ein", "keck", "Er\u00b7fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Lebens in jauchzender Lust \u2014", "tokens": ["Des", "Le\u00b7bens", "in", "jauch\u00b7zen\u00b7der", "Lust"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Nun liegt\u2019s mir vor Augen so todt und so fahl,", "tokens": ["Nun", "liegt's", "mir", "vor", "Au\u00b7gen", "so", "todt", "und", "so", "fahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Aufschreit in der Brust mir Titanenqual \u2014", "tokens": ["Auf\u00b7schreit", "in", "der", "Brust", "mir", "Ti\u00b7ta\u00b7nen\u00b7qual"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPER", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Als sollte die Welt ich nun ", "tokens": ["Als", "soll\u00b7te", "die", "Welt", "ich", "nun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "ART", "NN", "PPER", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "So ward mir nach all\u2019 dem Erfassen", "tokens": ["So", "ward", "mir", "nach", "all'", "dem", "Er\u00b7fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PIS", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Des Lebens in jauchzender Lust!", "tokens": ["Des", "Le\u00b7bens", "in", "jauch\u00b7zen\u00b7der", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}