{"textgrid.poem.57048": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Der Papagei", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.85", "lt:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Palma Kunkels Papagei", "tokens": ["Pal\u00b7ma", "Kun\u00b7kels", "Pa\u00b7pa\u00b7gei"], "token_info": ["word", "word", "word"], "pos": ["NE", "NE", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "spekuliert nicht auf Applaus:", "tokens": ["spe\u00b7ku\u00b7liert", "nicht", "auf", "Ap\u00b7plaus", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "niemals, was auch immer sei,", "tokens": ["nie\u00b7mals", ",", "was", "auch", "im\u00b7mer", "sei", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "ADV", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "spricht er seine W\u00f6rter aus.", "tokens": ["spricht", "er", "sei\u00b7ne", "W\u00f6r\u00b7ter", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Deren Zahl ist ohne Zahl:", "tokens": ["De\u00b7ren", "Zahl", "ist", "oh\u00b7ne", "Zahl", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "denn er ist das kl\u00fcgste Tier,", "tokens": ["denn", "er", "ist", "das", "kl\u00fcgs\u00b7te", "Tier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "das man je zum Kauf empfahl,", "tokens": ["das", "man", "je", "zum", "Kauf", "emp\u00b7fahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "und der Zucht vollkommne Zier.", "tokens": ["und", "der", "Zucht", "voll\u00b7komm\u00b7ne", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch indem er streng dich mi\u00dft,", "tokens": ["Doch", "in\u00b7dem", "er", "streng", "dich", "mi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "scheint sein Zungenglied verdorrt:", "tokens": ["scheint", "sein", "Zun\u00b7gen\u00b7glied", "ver\u00b7dorrt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "gleichviel, wer du immer bist,", "tokens": ["gleich\u00b7viel", ",", "wer", "du", "im\u00b7mer", "bist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "PPER", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "er verr\u00e4t dir nicht ein Wort.", "tokens": ["er", "ver\u00b7r\u00e4t", "dir", "nicht", "ein", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}