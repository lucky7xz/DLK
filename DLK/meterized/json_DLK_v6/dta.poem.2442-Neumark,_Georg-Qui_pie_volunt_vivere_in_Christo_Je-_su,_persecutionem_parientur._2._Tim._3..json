{"dta.poem.2442": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Qui pie volunt vivere in Christo Je-  \n su, persecutionem parientur. 2.  Tim.  3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Nechster schimpfte mich ich schalt' und hasste ihn", "tokens": ["Mein", "Nechs\u00b7ter", "schimpf\u00b7te", "mich", "ich", "schalt'", "und", "hass\u00b7te", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PPER", "VVFIN", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wieder", "tokens": ["wie\u00b7der"], "token_info": ["word"], "pos": ["ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Er schlug mich ins Gesicht/ ich schlug ihn wieder nieder.", "tokens": ["Er", "schlug", "mich", "ins", "Ge\u00b7sicht", "/", "ich", "schlug", "ihn", "wie\u00b7der", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$(", "PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So trieben wir es lang. Die Liebe sah\u2019 es an/", "tokens": ["So", "trie\u00b7ben", "wir", "es", "lang", ".", "Die", "Lie\u00b7be", "sah'", "es", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJD", "$.", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie kam zu mir herab vom Himmel her gestiegen/", "tokens": ["Sie", "kam", "zu", "mir", "her\u00b7ab", "vom", "Him\u00b7mel", "her", "ge\u00b7stie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "APPRART", "NN", "APZR", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und sprach mir freundlich zu: Freund witstu also siegen?", "tokens": ["Und", "sprach", "mir", "freund\u00b7lich", "zu", ":", "Freund", "wits\u00b7tu", "al\u00b7so", "sie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "NN", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Ach nein du irrest weit. So ists nicht wot gethan.", "tokens": ["Ach", "nein", "du", "ir\u00b7rest", "weit", ".", "So", "ists", "nicht", "wot", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "PPER", "VVFIN", "ADJD", "$.", "ADV", "VAFIN", "PTKNEG", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wer Christlich lebenwil der mu\u00df Verfol-", "tokens": ["Wer", "Christ\u00b7lich", "le\u00b7ben\u00b7wil", "der", "mu\u00df", "Ver\u00b7fol"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "NE", "NE", "ART", "VMFIN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "gung leiden/", "tokens": ["gung", "lei\u00b7den", "/"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Und allen Gegenspott wie eine Schlange", "tokens": ["Und", "al\u00b7len", "Ge\u00b7gen\u00b7spott", "wie", "ei\u00b7ne", "Schlan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "meiden.", "tokens": ["mei\u00b7den", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}