{"dta.poem.9848": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An einen seil-t\u00e4ntzer.  \n  Sonnet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Beschreite der l\u00fcfte zerflossene bahn/", "tokens": ["Be\u00b7schrei\u00b7te", "der", "l\u00fcf\u00b7te", "zer\u00b7flos\u00b7se\u00b7ne", "bahn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Verwandle die glieder in segel und fl\u00fcgel/", "tokens": ["Ver\u00b7wand\u00b7le", "die", "glie\u00b7der", "in", "se\u00b7gel", "und", "fl\u00fc\u00b7gel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NE", "KON", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Dein Pegasus schwinge sich \u00fcber die h\u00fcgel/", "tokens": ["Dein", "Pe\u00b7ga\u00b7sus", "schwin\u00b7ge", "sich", "\u00fc\u00b7ber", "die", "h\u00fc\u00b7gel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und trete herunter in Acherons kahn/", "tokens": ["Und", "tre\u00b7te", "her\u00b7un\u00b7ter", "in", "A\u00b7che\u00b7rons", "kahn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "NE", "NE", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Streck alle die kr\u00e4fte des leibes noch an/", "tokens": ["Streck", "al\u00b7le", "die", "kr\u00e4f\u00b7te", "des", "lei\u00b7bes", "noch", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "PDS", "VVFIN", "ART", "ADJA", "ADV", "PTKVZ", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und l\u00f6se der schenckel gebundene z\u00fcgel/", "tokens": ["Und", "l\u00f6\u00b7se", "der", "schen\u00b7ckel", "ge\u00b7bun\u00b7de\u00b7ne", "z\u00fc\u00b7gel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Dein wackelfu\u00df rennet mit offenem b\u00fcgel", "tokens": ["Dein", "wa\u00b7ckel\u00b7fu\u00df", "ren\u00b7net", "mit", "of\u00b7fe\u00b7nem", "b\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Viel schneller als irgend ein westwind gethan.", "tokens": ["Viel", "schnel\u00b7ler", "als", "ir\u00b7gend", "ein", "west\u00b7wind", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "So webet Arachne die netze zusammen/", "tokens": ["So", "we\u00b7bet", "A\u00b7rach\u00b7ne", "die", "net\u00b7ze", "zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ART", "ADJA", "PTKVZ", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "So drehet und endert ein irrsal den gang/", "tokens": ["So", "dre\u00b7het", "und", "en\u00b7dert", "ein", "irr\u00b7sal", "den", "gang", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "NN", "ART", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "So pflegen die flammen verschrencket zu stammen/", "tokens": ["So", "pfle\u00b7gen", "die", "flam\u00b7men", "ver\u00b7schren\u00b7cket", "zu", "stam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "VVINF", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "So mischt sich der vielmals vermengete klang.", "tokens": ["So", "mischt", "sich", "der", "viel\u00b7mals", "ver\u00b7men\u00b7ge\u00b7te", "klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADV", "VVFIN", "VVFIN", "$."], "meter": "-+--+-+---+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Di\u00df springen/ di\u00df treten/ di\u00df rencken und schwencken", "tokens": ["Di\u00df", "sprin\u00b7gen", "/", "di\u00df", "tre\u00b7ten", "/", "di\u00df", "ren\u00b7cken", "und", "schwen\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVINF", "$(", "PDS", "VVINF", "$(", "PDS", "VVFIN", "KON", "VVINF"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Kan alle gebeine wie r\u00e4derwerck lencken.", "tokens": ["Kan", "al\u00b7le", "ge\u00b7bei\u00b7ne", "wie", "r\u00e4\u00b7der\u00b7werck", "len\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVFIN", "KOKOM", "ADJD", "VVINF", "$."], "meter": "-+--+--+-+--", "measure": "amphibrach.tri.plus"}}}}}