{"textgrid.poem.35052": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Heilesbotschaft", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ging ein Heil von oben aus,", "tokens": ["Es", "ging", "ein", "Heil", "von", "o\u00b7ben", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Paradies, vom Vaterhaus.", "tokens": ["Vom", "Pa\u00b7ra\u00b7dies", ",", "vom", "Va\u00b7ter\u00b7haus", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Engel trugen es zur Erde,", "tokens": ["Die", "En\u00b7gel", "tru\u00b7gen", "es", "zur", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Damit es uns zu eigen werde.", "tokens": ["Da\u00b7mit", "es", "uns", "zu", "ei\u00b7gen", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKA", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch bleibt dem menschlichen Verstand", "tokens": ["Doch", "bleibt", "dem", "menschli\u00b7chen", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Die Gottesbotschaft unbekannt,", "tokens": ["Die", "Got\u00b7tes\u00b7bot\u00b7schaft", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil er das, was er denkt und dichtet,", "tokens": ["Weil", "er", "das", ",", "was", "er", "denkt", "und", "dich\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "$,", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach au\u00dfen, nicht nach innen richtet.", "tokens": ["Nach", "au\u00b7\u00dfen", ",", "nicht", "nach", "in\u00b7nen", "rich\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "PTKNEG", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er fa\u00dft in seiner Prosa nicht", "tokens": ["Er", "fa\u00dft", "in", "sei\u00b7ner", "Pro\u00b7sa", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Himmels herrlichstes Gedicht.", "tokens": ["Des", "Him\u00b7mels", "herr\u00b7lichs\u00b7tes", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum Herzen nur ist es gekommen", "tokens": ["Zum", "Her\u00b7zen", "nur", "ist", "es", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VAFIN", "PPER", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und wird von ihm allein vernommen.", "tokens": ["Und", "wird", "von", "ihm", "al\u00b7lein", "ver\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}