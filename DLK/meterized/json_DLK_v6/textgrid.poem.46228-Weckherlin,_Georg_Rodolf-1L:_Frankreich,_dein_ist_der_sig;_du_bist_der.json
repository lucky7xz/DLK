{"textgrid.poem.46228": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Frankreich, dein ist der sig; du bist der ", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frankreich, dein ist der sig; du bist der ", "tokens": ["Fran\u00b7kreich", ",", "dein", "ist", "der", "sig", ";", "du", "bist", "der"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "VAFIN", "ART", "ADJD", "$.", "PPER", "VAFIN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "das beste reich der welt, gleichlos durch gottes segen;", "tokens": ["das", "bes\u00b7te", "reich", "der", "welt", ",", "gleich\u00b7los", "durch", "got\u00b7tes", "se\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "ART", "NN", "$,", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "zwar nicht, weil fruchtreich du der fremden trost und hort,", "tokens": ["zwar", "nicht", ",", "weil", "fruch\u00b7treich", "du", "der", "frem\u00b7den", "trost", "und", "hort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "$,", "KOUS", "ADJD", "PPER", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "auch nicht, weil deine leut, wie du, gut zu thun pflegen;", "tokens": ["auch", "nicht", ",", "weil", "dei\u00b7ne", "leut", ",", "wie", "du", ",", "gut", "zu", "thun", "pfle\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "$,", "KOUS", "PPOSAT", "NN", "$,", "PWAV", "PPER", "$,", "ADJD", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Nicht weil du der lieb sitz, der sturmleidenden port,", "tokens": ["Nicht", "weil", "du", "der", "lieb", "sitz", ",", "der", "sturm\u00b7lei\u00b7den\u00b7den", "port", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "PPER", "ART", "ADJD", "PTKVZ", "$,", "ART", "ADJA", "NE", "$,"], "meter": "-+--+---+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "nicht deines weisen rats und dapfern adels wegen,", "tokens": ["nicht", "dei\u00b7nes", "wei\u00b7sen", "rats", "und", "dap\u00b7fern", "a\u00b7dels", "we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "ADJA", "NN", "KON", "ADJA", "NN", "APPR", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "noch, weil dein k\u00f6nig, gro\u00df durch seine werk und wort,", "tokens": ["noch", ",", "weil", "dein", "k\u00f6\u00b7nig", ",", "gro\u00df", "durch", "sei\u00b7ne", "werk", "und", "wort", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPOSAT", "ADJD", "$,", "ADJD", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "bezeuget, da\u00df ihm nichts, dan dein heil angelegen;", "tokens": ["be\u00b7zeu\u00b7get", ",", "da\u00df", "ihm", "nichts", ",", "dan", "dein", "heil", "an\u00b7ge\u00b7le\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PIS", "$,", "ADV", "PPOSAT", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nein! sondern, weil dir wert ein solcher Cardinal,", "tokens": ["Nein", "!", "son\u00b7dern", ",", "weil", "dir", "wert", "ein", "sol\u00b7cher", "Car\u00b7di\u00b7nal", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "KON", "$,", "KOUS", "PPER", "VVFIN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "da\u00df ihm auch an verdienst sunst kein mensch gleich zu finden,", "tokens": ["da\u00df", "ihm", "auch", "an", "ver\u00b7dienst", "sunst", "kein", "mensch", "gleich", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ADV", "ADV", "PIAT", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "wan in der weiten welt dir got schon g\u00e4b die wahl;", "tokens": ["wan", "in", "der", "wei\u00b7ten", "welt", "dir", "got", "schon", "g\u00e4b", "die", "wahl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.12": {"text": "Darum la\u00df weder forcht noch hofnung dich verblinden,", "tokens": ["Da\u00b7rum", "la\u00df", "we\u00b7der", "forcht", "noch", "hof\u00b7nung", "dich", "ver\u00b7blin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "KON", "VVFIN", "ADV", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "dieweil ganz selig du durch got und ihn zumal", "tokens": ["die\u00b7weil", "ganz", "se\u00b7lig", "du", "durch", "got", "und", "ihn", "zu\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "PPER", "APPR", "NE", "KON", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "kanst, so du wilt, die welt, dich kein feind \u00fcberwinden.", "tokens": ["kanst", ",", "so", "du", "wilt", ",", "die", "welt", ",", "dich", "kein", "feind", "\u00fc\u00b7berw\u00b7in\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "ADV", "PPER", "VMFIN", "$,", "ART", "NN", "$,", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}