{"textgrid.poem.41777": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo hoch empor die sieben Kuppen ragen \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo hoch empor die sieben Kuppen ragen \u2013", "tokens": ["Wo", "hoch", "em\u00b7por", "die", "sie\u00b7ben", "Kup\u00b7pen", "ra\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PTKVZ", "ART", "CARD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die das Gebirg auf festem Scheitel trug,", "tokens": ["Die", "das", "Ge\u00b7birg", "auf", "fes\u00b7tem", "Schei\u00b7tel", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gleich Kronen, die sie auf den H\u00e4uptern tragen,", "tokens": ["Gleich", "Kro\u00b7nen", ",", "die", "sie", "auf", "den", "H\u00e4up\u00b7tern", "tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und die der Sturm der Zeit herunterschlug \u2013", "tokens": ["Und", "die", "der", "Sturm", "der", "Zeit", "her\u00b7un\u00b7ter\u00b7schlug", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ruinen stehn von Schl\u00f6ssern und Kapellen,", "tokens": ["Ru\u00b7i\u00b7nen", "stehn", "von", "Schl\u00f6s\u00b7sern", "und", "Ka\u00b7pel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die sich bespiegeln in des Rheines Wellen,", "tokens": ["Die", "sich", "be\u00b7spie\u00b7geln", "in", "des", "Rhei\u00b7nes", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dem Schiffer k\u00fcndend von vergangner Zeit,", "tokens": ["Dem", "Schif\u00b7fer", "k\u00fcn\u00b7dend", "von", "ver\u00b7gang\u00b7ner", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Entschwundner und versunkner Herrlichkeit:", "tokens": ["Ent\u00b7schwund\u00b7ner", "und", "ver\u00b7sun\u00b7kner", "Herr\u00b7lich\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da hebt der Drachenfels sich majest\u00e4tisch,", "tokens": ["Da", "hebt", "der", "Dra\u00b7chen\u00b7fels", "sich", "ma\u00b7jes\u00b7t\u00e4\u00b7tisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der vorderste, dicht an des Rheines Flut,", "tokens": ["Der", "vor\u00b7ders\u00b7te", ",", "dicht", "an", "des", "Rhei\u00b7nes", "Flut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Es ist als spr\u00e4ch' er warnend und prophetisch", "tokens": ["Es", "ist", "als", "spr\u00e4ch'", "er", "war\u00b7nend", "und", "pro\u00b7phe\u00b7tisch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM", "VVFIN", "PPER", "VVPP", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu einer Welt die scheinbar friedlich ruht:", "tokens": ["Zu", "ei\u00b7ner", "Welt", "die", "schein\u00b7bar", "fried\u00b7lich", "ruht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Auch Du hast noch zu k\u00e4mpfen mit den Drachen,", "tokens": ["Auch", "Du", "hast", "noch", "zu", "k\u00e4mp\u00b7fen", "mit", "den", "Dra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die an der Zukunft Paradies bewachen", "tokens": ["Die", "an", "der", "Zu\u00b7kunft", "Pa\u00b7ra\u00b7dies", "be\u00b7wa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Den Eingang in der Liebe sch\u00f6nes Reich,", "tokens": ["Den", "Ein\u00b7gang", "in", "der", "Lie\u00b7be", "sch\u00f6\u00b7nes", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wo ", "tokens": ["Wo"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Von einem Drachen k\u00fcndet hier die Sage,", "tokens": ["Von", "ei\u00b7nem", "Dra\u00b7chen", "k\u00fcn\u00b7det", "hier", "die", "Sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Menschenopfer heischte lange Zeit \u2013", "tokens": ["Der", "Men\u00b7schen\u00b7op\u00b7fer", "heischte", "lan\u00b7ge", "Zeit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Davon kein Fluch, kein Schwert und keine Klage,", "tokens": ["Da\u00b7von", "kein", "Fluch", ",", "kein", "Schwert", "und", "kei\u00b7ne", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "NN", "$,", "PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die St\u00e4mme, die hier wohnten, je befreit,", "tokens": ["Die", "St\u00e4m\u00b7me", ",", "die", "hier", "wohn\u00b7ten", ",", "je", "be\u00b7freit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Verbrecher, die an ihnen sich vergangen,", "tokens": ["Ver\u00b7bre\u00b7cher", ",", "die", "an", "ih\u00b7nen", "sich", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und Feinde, die sie in der Schlacht gefangen", "tokens": ["Und", "Fein\u00b7de", ",", "die", "sie", "in", "der", "Schlacht", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die stie\u00df man zu des Drachen H\u00f6hle fort,", "tokens": ["Die", "stie\u00df", "man", "zu", "des", "Dra\u00b7chen", "H\u00f6h\u00b7le", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "APPR", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Er w\u00fcrgte sie im schauerlichen Mord. \u2013", "tokens": ["Er", "w\u00fcrg\u00b7te", "sie", "im", "schau\u00b7er\u00b7li\u00b7chen", "Mord", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Einst aus dem Krieg mit einem fremden Stamme", "tokens": ["Einst", "aus", "dem", "Krieg", "mit", "ei\u00b7nem", "frem\u00b7den", "Stam\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fiel eine Jungfrau in der Heiden Hand,", "tokens": ["Fiel", "ei\u00b7ne", "Jung\u00b7frau", "in", "der", "Hei\u00b7den", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von deren Sch\u00f6ne eine Liebesflamme", "tokens": ["Von", "de\u00b7ren", "Sch\u00f6\u00b7ne", "ei\u00b7ne", "Lie\u00b7bes\u00b7flam\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In zweier Helden Brust zugleich entbrannt.", "tokens": ["In", "zwei\u00b7er", "Hel\u00b7den", "Brust", "zu\u00b7gleich", "ent\u00b7brannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zwei H\u00e4uptlinge sich um die Beute stritten \u2013", "tokens": ["Zwei", "H\u00e4upt\u00b7lin\u00b7ge", "sich", "um", "die", "Beu\u00b7te", "strit\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-++-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Da lautete der Richterspruch des dritten:", "tokens": ["Da", "lau\u00b7te\u00b7te", "der", "Rich\u00b7ter\u00b7spruch", "des", "drit\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df f\u00fcrder nicht bestehe solcher Streit,", "tokens": ["Da\u00df", "f\u00fcr\u00b7der", "nicht", "be\u00b7ste\u00b7he", "sol\u00b7cher", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PTKNEG", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die Jungfrau sei dem Drachentod geweiht,", "tokens": ["Die", "Jung\u00b7frau", "sei", "dem", "Dra\u00b7chen\u00b7tod", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und sch\u00f6n geschm\u00fcckt, im wei\u00dfen Opferkleide", "tokens": ["Und", "sch\u00f6n", "ge\u00b7schm\u00fcckt", ",", "im", "wei\u00b7\u00dfen", "Op\u00b7fer\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Br\u00e4utliche Blumen in dem goldnen Haar,", "tokens": ["Br\u00e4ut\u00b7li\u00b7che", "Blu\u00b7men", "in", "dem", "gold\u00b7nen", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Am Hals ein reiches goldenes Geschmeide,", "tokens": ["Am", "Hals", "ein", "rei\u00b7ches", "gol\u00b7de\u00b7nes", "Ge\u00b7schmei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das schon daheim dort prangte immerdar,", "tokens": ["Das", "schon", "da\u00b7heim", "dort", "prang\u00b7te", "im\u00b7mer\u00b7dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und das man ihr zum Opfertod gelassen \u2013", "tokens": ["Und", "das", "man", "ihr", "zum", "Op\u00b7fer\u00b7tod", "ge\u00b7las\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIS", "PPER", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So schritt sie hin \u2013 es malte kein Erblassen.", "tokens": ["So", "schritt", "sie", "hin", "\u2013", "es", "mal\u00b7te", "kein", "Er\u00b7blas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Kein bleicher Schrecken ihre Wangen wei\u00df,", "tokens": ["Kein", "blei\u00b7cher", "Schre\u00b7cken", "ih\u00b7re", "Wan\u00b7gen", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Gefa\u00dft und mutig stand sie in dem Kreis.", "tokens": ["Ge\u00b7fa\u00dft", "und", "mu\u00b7tig", "stand", "sie", "in", "dem", "Kreis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und da sie nahe zu der H\u00f6hle kamen,", "tokens": ["Und", "da", "sie", "na\u00b7he", "zu", "der", "H\u00f6h\u00b7le", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und schon der Drache ihr entgegen spr\u00fcht',", "tokens": ["Und", "schon", "der", "Dra\u00b7che", "ihr", "ent\u00b7ge\u00b7gen", "spr\u00fcht'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PPER", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da sprach sie fromm und leis ein heilig Amen", "tokens": ["Da", "sprach", "sie", "fromm", "und", "leis", "ein", "hei\u00b7lig", "A\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sang \u2013 gleich wie im h\u00f6hern Chor \u2013 ein Lied.", "tokens": ["Und", "sang", "\u2013", "gleich", "wie", "im", "h\u00f6\u00b7hern", "Chor", "\u2013", "ein", "Lied", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "ADV", "KOKOM", "APPRART", "ADJA", "NN", "$(", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Aus ihrer Brust zog sie am Goldgeschmeide", "tokens": ["Aus", "ih\u00b7rer", "Brust", "zog", "sie", "am", "Gold\u00b7ge\u00b7schmei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Hervor ein Kreuz \u2013 ihr h\u00f6chstes Gut im Leide!", "tokens": ["Her\u00b7vor", "ein", "Kreuz", "\u2013", "ihr", "h\u00f6chs\u00b7tes", "Gut", "im", "Lei\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$(", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und hielt es \u2013 als geweihten Talisman", "tokens": ["Und", "hielt", "es", "\u2013", "als", "ge\u00b7weih\u00b7ten", "Ta\u00b7lis\u00b7man"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$(", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Dem Drachen hin \u2013 er starrt es w\u00fctend an \u2013", "tokens": ["Dem", "Dra\u00b7chen", "hin", "\u2013", "er", "starrt", "es", "w\u00fc\u00b7tend", "an", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$(", "PPER", "