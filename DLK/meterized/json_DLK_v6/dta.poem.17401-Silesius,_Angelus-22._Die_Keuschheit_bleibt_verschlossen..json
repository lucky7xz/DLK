{"dta.poem.17401": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "22.  Die Keuschheit bleibt verschlossen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Die Keuschheit ist ein Schlo\u00df das niemand auf kan-", "tokens": ["Die", "Keuschheit", "ist", "ein", "Schlo\u00df", "das", "nie\u00b7mand", "auf", "kan"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ART", "PIS", "APPR", "TRUNC"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "schliessen.", "tokens": ["schlies\u00b7sen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Was sie im innern ist/ das mag kein fremder wissen.", "tokens": ["Was", "sie", "im", "in\u00b7nern", "ist", "/", "das", "mag", "kein", "frem\u00b7der", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "ADJA", "VAFIN", "$(", "PDS", "VMFIN", "PIAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}