{"textgrid.poem.41629": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie sehr lieb, Sch\u00f6ne, ich den sanften, gr\u00fcnen Schimmer", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sehr lieb, Sch\u00f6ne, ich den sanften, gr\u00fcnen Schimmer", "tokens": ["Wie", "sehr", "lieb", ",", "Sch\u00f6\u00b7ne", ",", "ich", "den", "sanf\u00b7ten", ",", "gr\u00fc\u00b7nen", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "$,", "NN", "$,", "PPER", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus deinen Augen, doch scheint alles heut mir schwer,", "tokens": ["Aus", "dei\u00b7nen", "Au\u00b7gen", ",", "doch", "scheint", "al\u00b7les", "heut", "mir", "schwer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PIS", "ADV", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und nichts, nicht deine Lieb, der Herd nicht, noch dein Zimmer", "tokens": ["Und", "nichts", ",", "nicht", "dei\u00b7ne", "Lieb", ",", "der", "Herd", "nicht", ",", "noch", "dein", "Zim\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "$,", "PTKNEG", "PPOSAT", "NN", "$,", "ART", "NN", "PTKNEG", "$,", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist wie die Sonne mir, die leuchtet \u00fcber Meer.", "tokens": ["Ist", "wie", "die", "Son\u00b7ne", "mir", ",", "die", "leuch\u00b7tet", "\u00fc\u00b7ber", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "PPER", "$,", "PRELS", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und dennoch liebe mich mit m\u00fctterlicher S\u00fc\u00dfe,", "tokens": ["Und", "den\u00b7noch", "lie\u00b7be", "mich", "mit", "m\u00fct\u00b7ter\u00b7li\u00b7cher", "S\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mag ich auch undankbar und b\u00f6sen Sinnes sein;", "tokens": ["Mag", "ich", "auch", "un\u00b7dank\u00b7bar", "und", "b\u00f6\u00b7sen", "Sin\u00b7nes", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "KON", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Lieb oder Schwester, sei der Duft der sp\u00e4ten Gr\u00fc\u00dfe,", "tokens": ["Lieb", "o\u00b7der", "Schwes\u00b7ter", ",", "sei", "der", "Duft", "der", "sp\u00e4\u00b7ten", "Gr\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Ein Herbst in Strahlenpracht, ein m\u00fcder Sonnenschein.", "tokens": ["Ein", "Herbst", "in", "Strah\u00b7len\u00b7pracht", ",", "ein", "m\u00fc\u00b7der", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Bald ists getan. Schon harrt auf mich des Grabes K\u00fchle!", "tokens": ["Bald", "ists", "ge\u00b7tan", ".", "Schon", "harrt", "auf", "mich", "des", "Gra\u00b7bes", "K\u00fch\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$.", "ADV", "VVFIN", "APPR", "PRF", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O la\u00df auf deinen Knien mein Haupt ruhn noch einmal", "tokens": ["O", "la\u00df", "auf", "dei\u00b7nen", "Kni\u00b7en", "mein", "Haupt", "ruhn", "noch", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und f\u00fchlen, trauernd um des wei\u00dfen Sommers Schw\u00fcle,", "tokens": ["Und", "f\u00fch\u00b7len", ",", "trau\u00b7ernd", "um", "des", "wei\u00b7\u00dfen", "Som\u00b7mers", "Schw\u00fc\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJD", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der sp\u00e4ten Jahreszeit ges\u00e4nftigt-goldnen Strahl!", "tokens": ["Der", "sp\u00e4\u00b7ten", "Jah\u00b7res\u00b7zeit", "ge\u00b7s\u00e4nf\u00b7tig\u00b7tgold\u00b7nen", "Strahl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}