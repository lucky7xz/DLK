{"textgrid.poem.26914": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Moral der Pflicht und der Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jede, wohin sie geh\u00f6rt! Erhabene Seelen nur kleidet", "tokens": ["Je\u00b7de", ",", "wo\u00b7hin", "sie", "ge\u00b7h\u00f6rt", "!", "Er\u00b7ha\u00b7be\u00b7ne", "See\u00b7len", "nur", "klei\u00b7det"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "$,", "PWAV", "PPER", "VVFIN", "$.", "ADJA", "NN", "ADV", "VVFIN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Jene, die andere steht sch\u00f6nen Gem\u00fctern nur an.", "tokens": ["Je\u00b7ne", ",", "die", "an\u00b7de\u00b7re", "steht", "sch\u00f6\u00b7nen", "Ge\u00b7m\u00fc\u00b7tern", "nur", "an", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PIS", "VVFIN", "ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Aber Widrigers kenn ich auch nichts, als wenn sich durch Bande", "tokens": ["A\u00b7ber", "Wid\u00b7ri\u00b7gers", "kenn", "ich", "auch", "nichts", ",", "als", "wenn", "sich", "durch", "Ban\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADV", "PIS", "$,", "KOKOM", "KOUS", "PRF", "APPR", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Zarter geistiger Lieb Grobes mit Grobem verm\u00e4hlt;", "tokens": ["Zar\u00b7ter", "geis\u00b7ti\u00b7ger", "Lieb", "Gro\u00b7bes", "mit", "Gro\u00b7bem", "ver\u00b7m\u00e4hlt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Und ver\u00e4chtlicher nichts als die Moral der D\u00e4monen", "tokens": ["Und", "ver\u00b7\u00e4cht\u00b7li\u00b7cher", "nichts", "als", "die", "Mo\u00b7ral", "der", "D\u00e4\u00b7mo\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PIS", "KOKOM", "ART", "NN", "ART", "NN"], "meter": "--+--+---+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "In dem Munde des Volks, dem noch die Menschlichkeit fehlt.", "tokens": ["In", "dem", "Mun\u00b7de", "des", "Volks", ",", "dem", "noch", "die", "Menschlich\u00b7keit", "fehlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}}}}}