{"dta.poem.12771": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die besch\u00e4mte Hygea,  \n Bey G. A. Luja,  Med. D.  schmertz-  \n lichem absterben,  \n Durch J. F. O.  D.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "HyGEA, unser trost und Libitinens schrecken,", "tokens": ["Hy\u00b7GEA", ",", "un\u00b7ser", "trost", "und", "Li\u00b7bi\u00b7ti\u00b7nens", "schre\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "War, wie sie stets gewohnt, vor unser heyl bem\u00fcht;", "tokens": ["War", ",", "wie", "sie", "stets", "ge\u00b7wohnt", ",", "vor", "un\u00b7ser", "heyl", "be\u00b7m\u00fcht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWAV", "PPER", "ADV", "VVPP", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie gab bew\u00e4hrten rath vor blattern, gifft und flecken,", "tokens": ["Sie", "gab", "be\u00b7w\u00e4hr\u00b7ten", "rath", "vor", "blat\u00b7tern", ",", "gifft", "und", "fle\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "APPR", "VVINF", "$,", "VVFIN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wenn der matte leib von brand und schmertzen gl\u00fcht:", "tokens": ["Und", "wenn", "der", "mat\u00b7te", "leib", "von", "brand", "und", "schmert\u00b7zen", "gl\u00fcht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "APPR", "NN", "KON", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Besonders wurde sie von unmuth eingenommen,", "tokens": ["Be\u00b7son\u00b7ders", "wur\u00b7de", "sie", "von", "un\u00b7muth", "ein\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als ihr, Geehrter Herr! sein ruff zu ohren kommen:", "tokens": ["Als", "ihr", ",", "Ge\u00b7ehr\u00b7ter", "Herr", "!", "sein", "ruff", "zu", "oh\u00b7ren", "kom\u00b7men", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJA", "NN", "$.", "PPOSAT", "NN", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Liebsten leben steht in \u00e4usserster gefahr,", "tokens": ["Der", "Liebs\u00b7ten", "le\u00b7ben", "steht", "in", "\u00e4us\u00b7sers\u00b7ter", "ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es ist das aussenwerck schon allbereits erstiegen,", "tokens": ["Es", "ist", "das", "aus\u00b7sen\u00b7werck", "schon", "all\u00b7be\u00b7reits", "er\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Drum mache deine kunst doch eilend offenbar,", "tokens": ["Drum", "ma\u00b7che", "dei\u00b7ne", "kunst", "doch", "ei\u00b7lend", "of\u00b7fen\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und hilff durch deine macht den frechen feind besiegen.", "tokens": ["Und", "hilff", "durch", "dei\u00b7ne", "macht", "den", "fre\u00b7chen", "feind", "be\u00b7sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die g\u00f6ttin ward hierauf mit bleicher furcht bestricket,", "tokens": ["Die", "g\u00f6t\u00b7tin", "ward", "hier\u00b7auf", "mit", "blei\u00b7cher", "furcht", "be\u00b7stri\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PAV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie dachte: Fehlet dem die offt erfahrne kunst,", "tokens": ["Sie", "dach\u00b7te", ":", "Feh\u00b7let", "dem", "die", "offt", "er\u00b7fahr\u00b7ne", "kunst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "ART", "ART", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dem sie bey andern hat so vielmal wohl gegl\u00fccket?", "tokens": ["Dem", "sie", "bey", "an\u00b7dern", "hat", "so", "viel\u00b7mal", "wohl", "ge\u00b7gl\u00fc\u00b7cket", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PIS", "VAFIN", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So lacht man meinen arm, und heist mein wissen dunst.", "tokens": ["So", "lacht", "man", "mei\u00b7nen", "arm", ",", "und", "heist", "mein", "wis\u00b7sen", "dunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "VVFIN", "ADJD", "$,", "KON", "VVFIN", "PPOSAT", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie rieff: Zergliederung! la\u00df iedermann erkennen,", "tokens": ["Sie", "rieff", ":", "Zer\u00b7glie\u00b7de\u00b7rung", "!", "la\u00df", "ie\u00b7der\u00b7mann", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "$.", "VVIMP", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df man dich billig kan mein rechtes auge nennen;", "tokens": ["Da\u00df", "man", "dich", "bil\u00b7lig", "kan", "mein", "rech\u00b7tes", "au\u00b7ge", "nen\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADJD", "VMFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Entdecke, was da sey die qvelle dieser pein.", "tokens": ["Ent\u00b7de\u00b7cke", ",", "was", "da", "sey", "die", "qvel\u00b7le", "die\u00b7ser", "pein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "VAFIN", "ART", "ADJA", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du edle scheide-kunst! nicht spare deine s\u00e4ffte,", "tokens": ["Du", "ed\u00b7le", "schei\u00b7de\u00b7kunst", "!", "nicht", "spa\u00b7re", "dei\u00b7ne", "s\u00e4ff\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "PTKNEG", "VVFIN", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und zeuch den besten kern aus kr\u00e4utern, thier und stein,", "tokens": ["Und", "zeuch", "den", "bes\u00b7ten", "kern", "aus", "kr\u00e4u\u00b7tern", ",", "thier", "und", "stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "ADJA", "NN", "APPR", "VVINF", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Verneu durch trinckbar gold die schon verlohrne kr\u00e4ffte.", "tokens": ["Ver\u00b7neu", "durch", "trin\u00b7ck\u00b7bar", "gold", "die", "schon", "ver\u00b7lohr\u00b7ne", "kr\u00e4ff\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJD", "NN", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Sie sah hierauf erfreut, wie man ihr h\u00e4hne schlachte,", "tokens": ["Sie", "sah", "hier\u00b7auf", "er\u00b7freut", ",", "wie", "man", "ihr", "h\u00e4h\u00b7ne", "schlach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "VVFIN", "$,", "PWAV", "PIS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und stets ihr opffer-tisch von neuem weine flo\u00df,", "tokens": ["Und", "stets", "ihr", "opf\u00b7fer\u00b7tisch", "von", "neu\u00b7em", "wei\u00b7ne", "flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "APPR", "ADJA", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie man ihr t\u00e4glich mehr geweihte kertzen brachte,", "tokens": ["Wie", "man", "ihr", "t\u00e4g\u00b7lich", "mehr", "ge\u00b7weih\u00b7te", "kert\u00b7zen", "brach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "ADJD", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und machte selbsten sich in ihrem geiste gro\u00df.", "tokens": ["Und", "mach\u00b7te", "selbs\u00b7ten", "sich", "in", "ih\u00b7rem", "geis\u00b7te", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Allein es k\u00f6nnen sich auch g\u00f6tter nicht so freuen,", "tokens": ["Al\u00b7lein", "es", "k\u00f6n\u00b7nen", "sich", "auch", "g\u00f6t\u00b7ter", "nicht", "so", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PRF", "ADV", "ADJA", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df Eris nicht darein kan unlusts-\u00e4pffel streuen:", "tokens": ["Da\u00df", "E\u00b7ris", "nicht", "da\u00b7rein", "kan", "un\u00b7lust\u00b7s\u00b7\u00e4pf\u00b7fel", "streu\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PTKNEG", "PAV", "VMFIN", "NN", "VVINF", "$."], "meter": "-+---+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Denn Mortens mord-geschrey verst\u00f6rte dieses fest;", "tokens": ["Denn", "Mor\u00b7tens", "mord\u00b7ge\u00b7schrey", "ver\u00b7st\u00f6r\u00b7te", "die\u00b7ses", "fest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "VVFIN", "PDS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sie schrie: Wer will sich mehr auf Ph\u00f6bens macht verlassen?", "tokens": ["Sie", "schrie", ":", "Wer", "will", "sich", "mehr", "auf", "Ph\u00f6\u00b7bens", "macht", "ver\u00b7las\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWS", "VMFIN", "PRF", "ADV", "APPR", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Denn Luja, dem er sich sonst g\u00fctig finden l\u00e4st,", "tokens": ["Denn", "Lu\u00b7ja", ",", "dem", "er", "sich", "sonst", "g\u00fc\u00b7tig", "fin\u00b7den", "l\u00e4st", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "PRELS", "PPER", "PRF", "ADV", "ADJD", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sieht seiner seelen licht itzt j\u00e4mmerlich erblassen.", "tokens": ["Sieht", "sei\u00b7ner", "see\u00b7len", "licht", "itzt", "j\u00e4m\u00b7mer\u00b7lich", "er\u00b7blas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Hygea starrte drauf, als wie vom blitz ger\u00fchret,", "tokens": ["Hy\u00b7gea", "starr\u00b7te", "drauf", ",", "als", "wie", "vom", "blitz", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$,", "KOUS", "KOKOM", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Es fiel der scepter ihr vor schmertzen aus der hand,", "tokens": ["Es", "fiel", "der", "scep\u00b7ter", "ihr", "vor", "schmert\u00b7zen", "aus", "der", "hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "PPER", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jhr sitz, den die natur verwundert aufgef\u00fchret,", "tokens": ["Ihr", "sitz", ",", "den", "die", "na\u00b7tur", "ver\u00b7wun\u00b7dert", "auf\u00b7ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Woran die me\u00df-kunst auch hatt\u2019 allen witz gewandt,", "tokens": ["Wo\u00b7ran", "die", "me\u00df\u00b7kunst", "auch", "hatt'", "al\u00b7len", "witz", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Brach unversehns entzwey. Jhr auge ward verh\u00fcllet,", "tokens": ["Brach", "un\u00b7ver\u00b7sehns", "ent\u00b7zwey", ".", "Ihr", "au\u00b7ge", "ward", "ver\u00b7h\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKVZ", "$.", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der gantze tempel ward mit nebel angef\u00fcllet;", "tokens": ["Der", "gant\u00b7ze", "tem\u00b7pel", "ward", "mit", "ne\u00b7bel", "an\u00b7ge\u00b7f\u00fcl\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Die seuffzer schalleten aus der entz\u00fcndten brust;", "tokens": ["Die", "seuff\u00b7zer", "schal\u00b7le\u00b7ten", "aus", "der", "ent\u00b7z\u00fcnd\u00b7ten", "brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Der adern heisser qvell fieng hefftig an zu wallen,", "tokens": ["Der", "a\u00b7dern", "heis\u00b7ser", "qvell", "fi\u00b7eng", "heff\u00b7tig", "an", "zu", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Es war die \u00e4rmste kaum sich von sich selbst bewust,", "tokens": ["Es", "war", "die", "\u00e4rms\u00b7te", "kaum", "sich", "von", "sich", "selbst", "be\u00b7wust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADV", "PRF", "APPR", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df sie aus ungeduld lie\u00df diese w\u00f6rter fallen:", "tokens": ["Da\u00df", "sie", "aus", "un\u00b7ge\u00b7duld", "lie\u00df", "die\u00b7se", "w\u00f6r\u00b7ter", "fal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJD", "VVFIN", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "So windet Morta mir den scepter aus den h\u00e4nden?", "tokens": ["So", "win\u00b7det", "Mor\u00b7ta", "mir", "den", "scep\u00b7ter", "aus", "den", "h\u00e4n\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "PPER", "ART", "ADJA", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die sich so vielmal hat vor meinem thron geb\u00fcckt,", "tokens": ["Die", "sich", "so", "viel\u00b7mal", "hat", "vor", "mei\u00b7nem", "thron", "ge\u00b7b\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "ADV", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da Charon doch so offt hat m\u00fcssen wieder l\u00e4nden,", "tokens": ["Da", "Cha\u00b7ron", "doch", "so", "offt", "hat", "m\u00fcs\u00b7sen", "wie\u00b7der", "l\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "ADV", "ADV", "VAFIN", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und zwar durch meinen spruch, wenn er schon abger\u00fcckt.", "tokens": ["Und", "zwar", "durch", "mei\u00b7nen", "spruch", ",", "wenn", "er", "schon", "ab\u00b7ge\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die arge raubet nicht, die meinen kelch verachten,", "tokens": ["Die", "ar\u00b7ge", "rau\u00b7bet", "nicht", ",", "die", "mei\u00b7nen", "kelch", "ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nicht die aus \u00fcberflu\u00df der jahre nach ihr trachten;", "tokens": ["Nicht", "die", "aus", "\u00fc\u00b7berf\u00b7lu\u00df", "der", "jah\u00b7re", "nach", "ihr", "trach\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "APPR", "NN", "ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sie windt den lebens-drat auch in der helfft entzwey;", "tokens": ["Sie", "windt", "den", "le\u00b7bens\u00b7drat", "auch", "in", "der", "helfft", "ent\u00b7zwey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "VVFIN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sie f\u00fchrt den winter ein auch mitten in den lentzen;", "tokens": ["Sie", "f\u00fchrt", "den", "win\u00b7ter", "ein", "auch", "mit\u00b7ten", "in", "den", "lent\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADV", "ADV", "APPR", "ART", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Jhr frecher blitz tr\u00e4gt nicht vor meinen priestern schen;", "tokens": ["Ihr", "fre\u00b7cher", "blitz", "tr\u00e4gt", "nicht", "vor", "mei\u00b7nen", "pries\u00b7tern", "schen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Noch, wider die natur, vor ihren lorbeer-kr\u00e4ntzen.", "tokens": ["Noch", ",", "wi\u00b7der", "die", "na\u00b7tur", ",", "vor", "ih\u00b7ren", "lor\u00b7beer\u00b7kr\u00e4nt\u00b7zen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Soll ich den l\u00e4sterern ja recht zum spott gedeihen:", "tokens": ["Soll", "ich", "den", "l\u00e4s\u00b7te\u00b7rern", "ja", "recht", "zum", "spott", "ge\u00b7dei\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "ADV", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es sey Hygeens kunst ein bloses gauckel-spiel,", "tokens": ["Es", "sey", "Hy\u00b7ge\u00b7ens", "kunst", "ein", "blo\u00b7ses", "gau\u00b7ckel\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die krafft, so bezoar und ambra mir verleihen,", "tokens": ["Die", "krafft", ",", "so", "be\u00b7zoar", "und", "am\u00b7bra", "mir", "ver\u00b7lei\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "KON", "NE", "PPER", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Verl\u00e4ng\u2019 und k\u00fcrtze nicht das ausgesteckte ziel?", "tokens": ["Ver\u00b7l\u00e4ng'", "und", "k\u00fcrt\u00b7ze", "nicht", "das", "aus\u00b7ge\u00b7steck\u00b7te", "ziel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hilff himmel! la\u00df mich nicht in solcher schande leben!", "tokens": ["Hilff", "him\u00b7mel", "!", "la\u00df", "mich", "nicht", "in", "sol\u00b7cher", "schan\u00b7de", "le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "VVIMP", "PPER", "PTKNEG", "APPR", "PIAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Man schm\u00e4ht ja deine macht, so du mir hast gegeben.", "tokens": ["Man", "schm\u00e4ht", "ja", "dei\u00b7ne", "macht", ",", "so", "du", "mir", "hast", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PPOSAT", "VVFIN", "$,", "ADV", "PPER", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer mir die schuld beymi\u00dft, wenn ein verruchtes weib", "tokens": ["Wer", "mir", "die", "schuld", "bey\u00b7mi\u00dft", ",", "wenn", "ein", "ver\u00b7ruch\u00b7tes", "weib"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ART", "ADJD", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und unerfahrner tropff, so mich aus mi\u00dfbrauch nennen,", "tokens": ["Und", "un\u00b7er\u00b7fahr\u00b7ner", "tropff", ",", "so", "mich", "aus", "mi\u00df\u00b7brauch", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "ADV", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+--+-+--", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Aus geitz und aberwitz best\u00fcrmt des menschen leib,", "tokens": ["Aus", "geitz", "und", "a\u00b7ber\u00b7witz", "be\u00b7st\u00fcrmt", "des", "men\u00b7schen", "leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mu\u00df noch Hygeen recht, noch ihre s\u00f6hne kennen.", "tokens": ["Mu\u00df", "noch", "Hy\u00b7ge\u00b7en", "recht", ",", "noch", "ih\u00b7re", "s\u00f6h\u00b7ne", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NN", "ADJD", "$,", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "--+----+-+-+-", "measure": "anapaest.init"}}, "stanza.7": {"line.1": {"text": "Mehr lie\u00df die ungeduld nicht die erz\u00f6rnte sagen,", "tokens": ["Mehr", "lie\u00df", "die", "un\u00b7ge\u00b7duld", "nicht", "die", "er\u00b7z\u00f6rn\u00b7te", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "PTKNEG", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der eifer nahm die brust, und scham die wangen ein,", "tokens": ["Der", "ei\u00b7fer", "nahm", "die", "brust", ",", "und", "scham", "die", "wan\u00b7gen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie konte keinen dienst und keinen schmuck vertragen,", "tokens": ["Sie", "kon\u00b7te", "kei\u00b7nen", "dienst", "und", "kei\u00b7nen", "schmuck", "ver\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wolte diesen tag gantz ungest\u00f6ret seyn.", "tokens": ["Und", "wol\u00b7te", "die\u00b7sen", "tag", "gantz", "un\u00b7ge\u00b7st\u00f6\u00b7ret", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PDAT", "NN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Allein es zwang sie bald sich anders zu entschlieffen,", "tokens": ["Al\u00b7lein", "es", "zwang", "sie", "bald", "sich", "an\u00b7ders", "zu", "ent\u00b7schlief\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "ADV", "PRF", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df viel vor ihre noth begehrten rath zu wissen.", "tokens": ["Da\u00df", "viel", "vor", "ih\u00b7re", "noth", "be\u00b7gehr\u00b7ten", "rath", "zu", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So bald sie munter ward, und mich ersehen hat,", "tokens": ["So", "bald", "sie", "mun\u00b7ter", "ward", ",", "und", "mich", "er\u00b7se\u00b7hen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADJD", "VAFIN", "$,", "KON", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der ich nebst andern mehr im tempel war erschienen,", "tokens": ["Der", "ich", "nebst", "an\u00b7dern", "mehr", "im", "tem\u00b7pel", "war", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PIS", "ADV", "APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So sprach sie: Bringe doch dem Luja dieses blat,", "tokens": ["So", "sprach", "sie", ":", "Brin\u00b7ge", "doch", "dem", "Lu\u00b7ja", "die\u00b7ses", "blat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "NN", "ADV", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Es wird zu seinem trost und mir zum schutze dienen.", "tokens": ["Es", "wird", "zu", "sei\u00b7nem", "trost", "und", "mir", "zum", "schut\u00b7ze", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "KON", "PPER", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Der innhalt war, wie folgt, nach aufgeri\u00dfnem siegel:", "tokens": ["Der", "inn\u00b7halt", "war", ",", "wie", "folgt", ",", "nach", "auf\u00b7ge\u00b7ri\u00df\u00b7nem", "sie\u00b7gel", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$,", "PWAV", "VVFIN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Verzeih, Geliebter Freund! da\u00df meine macht gefehlt,", "tokens": ["Ver\u00b7zeih", ",", "Ge\u00b7lieb\u00b7ter", "Freund", "!", "da\u00df", "mei\u00b7ne", "macht", "ge\u00b7fehlt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$.", "KOUS", "PPOSAT", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und nun dein halbes hertz, ein heller tugend-spiegel,", "tokens": ["Und", "nun", "dein", "hal\u00b7bes", "hertz", ",", "ein", "hel\u00b7ler", "tu\u00b7gen\u00b7dspie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die werthe Liebste, wird den todten zugezehlt.", "tokens": ["Die", "wert\u00b7he", "Liebs\u00b7te", ",", "wird", "den", "tod\u00b7ten", "zu\u00b7ge\u00b7zehlt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "$,", "VAFIN", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du weist, es hat zwar GOtt mir ein solch pfand befohlen,", "tokens": ["Du", "weist", ",", "es", "hat", "zwar", "Gott", "mir", "ein", "solch", "pfand", "be\u00b7foh\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "NN", "PPER", "ART", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Von dem die sterblichen sich offtmal labsal holen,", "tokens": ["Von", "dem", "die", "sterb\u00b7li\u00b7chen", "sich", "offt\u00b7mal", "lab\u00b7sal", "ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "PRF", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wor\u00fcber dennoch er die oberhand beh\u00e4lt.", "tokens": ["Wo\u00b7r\u00fc\u00b7ber", "den\u00b7noch", "er", "die", "o\u00b7ber\u00b7hand", "be\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der sonder meine krafft l\u00e4st neue menschen werden,", "tokens": ["Der", "son\u00b7der", "mei\u00b7ne", "krafft", "l\u00e4st", "neu\u00b7e", "men\u00b7schen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KON", "PPOSAT", "NN", "VVFIN", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Schickt ihren untergang, auch wenn es ihm gef\u00e4llt,", "tokens": ["Schickt", "ih\u00b7ren", "un\u00b7ter\u00b7gang", ",", "auch", "wenn", "es", "ihm", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "ADV", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und lindert nur durch mich die menschlichen beschwerden.", "tokens": ["Und", "lin\u00b7dert", "nur", "durch", "mich", "die", "menschli\u00b7chen", "be\u00b7schwer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPER", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "Und dieser heist auch sie die erste schuld bezahlen,", "tokens": ["Und", "die\u00b7ser", "heist", "auch", "sie", "die", "ers\u00b7te", "schuld", "be\u00b7zah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "PPER", "ART", "ADJA", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum gieb ihm willig hin das dir geborgte pfand,", "tokens": ["Drum", "gieb", "ihm", "wil\u00b7lig", "hin", "das", "dir", "ge\u00b7borg\u00b7te", "pfand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "ADV", "ART", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der geist wird also los von seinen irrdschen schaalen,", "tokens": ["Der", "geist", "wird", "al\u00b7so", "los", "von", "sei\u00b7nen", "irrd\u00b7schen", "schaa\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "APPR", "PPOSAT", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "(die doch nicht gantz vergehn) und ruht ins H\u00f6chsten hand.", "tokens": ["(", "die", "doch", "nicht", "gantz", "ver\u00b7gehn", ")", "und", "ruht", "ins", "H\u00f6chs\u00b7ten", "hand", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "ADV", "PTKNEG", "ADV", "VVINF", "$(", "KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bringt sterben der natur und \u00e4rtzten nichts als fchrecken;", "tokens": ["Bringt", "ster\u00b7ben", "der", "na\u00b7tur", "und", "\u00e4rtz\u00b7ten", "nichts", "als", "fchre\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "KON", "VVFIN", "PIS", "KOKOM", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So kan es Christen doch nur eitel trost erwecken.", "tokens": ["So", "kan", "es", "Chris\u00b7ten", "doch", "nur", "ei\u00b7tel", "trost", "er\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "ADV", "ADV", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Di\u00df ist der weisen stein, der alle plagen hebt,", "tokens": ["Di\u00df", "ist", "der", "wei\u00b7sen", "stein", ",", "der", "al\u00b7le", "pla\u00b7gen", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PIS", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Drum will sie l\u00e4nger nicht in dieser welt verweilen,", "tokens": ["Drum", "will", "sie", "l\u00e4n\u00b7ger", "nicht", "in", "die\u00b7ser", "welt", "ver\u00b7wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADJD", "PTKNEG", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Weil hier nach ihm umsonst ein irrdscher Hermes strebt,", "tokens": ["Weil", "hier", "nach", "ihm", "um\u00b7sonst", "ein", "irrd\u00b7scher", "Her\u00b7mes", "strebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Soll sie der Trismegist des himmels ewig heilen.", "tokens": ["Soll", "sie", "der", "Tris\u00b7me\u00b7gist", "des", "him\u00b7mels", "e\u00b7wig", "hei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}