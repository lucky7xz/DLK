{"textgrid.poem.41440": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Hirsch und der Eber", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Eber fragt den Hirsch: was macht dich hundeschen?", "tokens": ["Ein", "E\u00b7ber", "fragt", "den", "Hirsch", ":", "was", "macht", "dich", "hun\u00b7de\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$.", "PWS", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr mich gesteh' ich gern, da\u00df ich es nicht begreife.", "tokens": ["F\u00fcr", "mich", "ge\u00b7steh'", "ich", "gern", ",", "da\u00df", "ich", "es", "nicht", "be\u00b7grei\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du h\u00f6rst so scharf, als sie: wie schnell sind deine L\u00e4ufe?", "tokens": ["Du", "h\u00f6rst", "so", "scharf", ",", "als", "sie", ":", "wie", "schnell", "sind", "dei\u00b7ne", "L\u00e4u\u00b7fe", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "KOUS", "PPER", "$.", "PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie f\u00fcrchterlich ist dein Geweih?", "tokens": ["Wie", "f\u00fcrch\u00b7ter\u00b7lich", "ist", "dein", "Ge\u00b7weih", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und da du gr\u00f6\u00dfer bist, so solltest du dich sch\u00e4men,", "tokens": ["Und", "da", "du", "gr\u00f6\u00b7\u00dfer", "bist", ",", "so", "soll\u00b7test", "du", "dich", "sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vor Kleinern stets die Flucht zu nehmen.", "tokens": ["Vor", "Klei\u00b7nern", "stets", "die", "Flucht", "zu", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Was ist es immermehr, das so dich schrecken kann?", "tokens": ["Was", "ist", "es", "im\u00b7mer\u00b7mehr", ",", "das", "so", "dich", "schre\u00b7cken", "kann", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "$,", "PRELS", "ADV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das will ich, spricht der Hirsch, dir im Vertrauen sagen:", "tokens": ["Das", "will", "ich", ",", "spricht", "der", "Hirsch", ",", "dir", "im", "Ver\u00b7trau\u00b7en", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$,", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Abscheu h\u00e4ngt mir noch von meinem Vater an;", "tokens": ["Der", "Ab\u00b7scheu", "h\u00e4ngt", "mir", "noch", "von", "mei\u00b7nem", "Va\u00b7ter", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich kann das Heulen nicht vertragen.", "tokens": ["Ich", "kann", "das", "Heu\u00b7len", "nicht", "ver\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}