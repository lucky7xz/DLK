{"dta.poem.2084": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Auerochs.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ob wir gleich von Ochs- und K\u00fchen etwas schon ge-", "tokens": ["Ob", "wir", "gleich", "von", "Ochs", "und", "K\u00fc\u00b7hen", "et\u00b7was", "schon", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "TRUNC", "KON", "NN", "ADV", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "meldet haben;", "tokens": ["mel\u00b7det", "ha\u00b7ben", ";"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VAINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Scheint der Auer doch besonders unserer Erw\u00e4hnung werth,", "tokens": ["Scheint", "der", "Au\u00b7er", "doch", "be\u00b7son\u00b7ders", "un\u00b7se\u00b7rer", "Er\u00b7w\u00e4h\u00b7nung", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADV", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Als worinn, obgleich er wild, mancher Nutzen uns beschert.", "tokens": ["Als", "wo\u00b7rinn", ",", "ob\u00b7gleich", "er", "wild", ",", "man\u00b7cher", "Nut\u00b7zen", "uns", "be\u00b7schert", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "KOUS", "PPER", "ADJD", "$,", "PIAT", "NN", "PPER", "VVPP", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Mit bewundrungswerther Gr\u00f6\u00dfe wollt\u2019 ihn die Natur be-", "tokens": ["Mit", "be\u00b7wund\u00b7rungs\u00b7wert\u00b7her", "Gr\u00f6\u00b7\u00dfe", "wollt'", "ihn", "die", "Na\u00b7tur", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PPER", "ART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "gaben", "tokens": ["ga\u00b7ben"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Und mit ungemeiner St\u00e4rke. Es geht fast kein ander", "tokens": ["Und", "mit", "un\u00b7ge\u00b7mei\u00b7ner", "St\u00e4r\u00b7ke", ".", "Es", "geht", "fast", "kein", "an\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "$.", "PPER", "VVFIN", "ADV", "PIAT", "ADJD"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Thier,", "tokens": ["Thier", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Au\u00dfer einem Elephanten, ihm an St\u00e4rk\u2019 und Kr\u00e4ften f\u00fcr", "tokens": ["Au\u00b7\u00dfer", "ei\u00b7nem", "E\u00b7le\u00b7phan\u00b7ten", ",", "ihm", "an", "St\u00e4rk'", "und", "Kr\u00e4f\u00b7ten", "f\u00fcr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PPER", "APPR", "NN", "KON", "NN", "APPR"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Zwischen seiner starken H\u00f6rner mehr als eisenharten Spitzen", "tokens": ["Zwi\u00b7schen", "sei\u00b7ner", "star\u00b7ken", "H\u00f6r\u00b7ner", "mehr", "als", "ei\u00b7sen\u00b7har\u00b7ten", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PIAT", "KOKOM", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.11": {"text": "K\u00f6nnen zwey ja gar drey M\u00e4nner f\u00fcglich bey einander", "tokens": ["K\u00f6n\u00b7nen", "zwey", "ja", "gar", "drey", "M\u00e4n\u00b7ner", "f\u00fcg\u00b7lich", "bey", "ein\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "CARD", "ADV", "ADV", "CARD", "NN", "ADJD", "APPR", "PRF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.12": {"text": "sitzen.", "tokens": ["sit\u00b7zen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Um den festen Unterkiefern ist er f\u00fcrchterlich behaart.", "tokens": ["Um", "den", "fes\u00b7ten", "Un\u00b7ter\u00b7kie\u00b7fern", "ist", "er", "f\u00fcrch\u00b7ter\u00b7lich", "be\u00b7haart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Jhm giebt ein verwildert Ansehn sein verworrner", "tokens": ["Jhm", "giebt", "ein", "ver\u00b7wil\u00b7dert", "An\u00b7sehn", "sein", "ver\u00b7worr\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "VVPP", "NN", "PPOSAT", "ADJA"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "schwarzer Bart.", "tokens": ["schwar\u00b7zer", "Bart", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.16": {"text": "Auf desselben rauhen Stirne wird ein B\u00fcschel Haar ge-", "tokens": ["Auf", "des\u00b7sel\u00b7ben", "rau\u00b7hen", "Stir\u00b7ne", "wird", "ein", "B\u00fc\u00b7schel", "Haar", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VAFIN", "ART", "NN", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.17": {"text": "funden,", "tokens": ["fun\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Welcher recht als Biesam riecht. Ob er nun gleich noch", "tokens": ["Wel\u00b7cher", "recht", "als", "Bie\u00b7sam", "riecht", ".", "Ob", "er", "nun", "gleich", "noch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAT", "ADJD", "KOKOM", "ADJD", "VVFIN", "$.", "KOUS", "PPER", "ADV", "ADV", "ADV"], "meter": "+-+-+-+--++-", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "so gro\u00df,", "tokens": ["so", "gro\u00df", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.20": {"text": "Und so stark, da\u00df er auch B\u00e4ume st\u00fcrzt mit einem einz\u2019gen", "tokens": ["Und", "so", "stark", ",", "da\u00df", "er", "auch", "B\u00e4u\u00b7me", "st\u00fcrzt", "mit", "ei\u00b7nem", "einz'\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "$,", "KOUS", "PPER", "ADV", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.21": {"text": "Sto\u00df;", "tokens": ["Sto\u00df", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.22": {"text": "Wird er doch von Menschenh\u00e4nden oft get\u00f6dtet, oft ge-", "tokens": ["Wird", "er", "doch", "von", "Men\u00b7schen\u00b7h\u00e4n\u00b7den", "oft", "ge\u00b7t\u00f6d\u00b7tet", ",", "oft", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "NN", "ADV", "VVPP", "$,", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.23": {"text": "bunden,", "tokens": ["bun\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["ADJA", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.24": {"text": "Und sein Fleisch, das sonders niedlich, wird von uns", "tokens": ["Und", "sein", "Fleisch", ",", "das", "son\u00b7ders", "nied\u00b7lich", ",", "wird", "von", "uns"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "ADV", "ADJD", "$,", "VAFIN", "APPR", "PPER"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.25": {"text": "mit Lust verzehrt.", "tokens": ["mit", "Lust", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.26": {"text": "Ist demnach auch dieses Thier dankens- und bewun-", "tokens": ["Ist", "dem\u00b7nach", "auch", "die\u00b7ses", "Thier", "dan\u00b7kens", "und", "be\u00b7wun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PAV", "ADV", "PDAT", "NN", "TRUNC", "KON", "TRUNC"], "meter": "+-+-+-++-+-+", "measure": "unknown.measure.septa"}, "line.27": {"text": "dernswerth.", "tokens": ["derns\u00b7werth", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-+", "measure": "iambic.single"}}}}}