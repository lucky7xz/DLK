{"textgrid.poem.29515": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ptochus lag in tausent N\u00f6then,", "tokens": ["Pto\u00b7chus", "lag", "in", "tau\u00b7sent", "N\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ihn dr\u00e4ngten bi\u00df auffs t\u00f6dten;", "tokens": ["Die", "ihn", "dr\u00e4ng\u00b7ten", "bi\u00df", "auffs", "t\u00f6d\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "APPRART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Solte Christen-Liebe haben,", "tokens": ["Sol\u00b7te", "Chris\u00b7ten\u00b7Lie\u00b7be", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich zu retten, sich zu laben;", "tokens": ["Sich", "zu", "ret\u00b7ten", ",", "sich", "zu", "la\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "PTKZU", "VVINF", "$,", "PRF", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lie\u00df sie hin und wieder suchen,", "tokens": ["Lie\u00df", "sie", "hin", "und", "wie\u00b7der", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weil sie sich ietzt sehr verkrochen;", "tokens": ["Weil", "sie", "sich", "ietzt", "sehr", "ver\u00b7kro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Lie\u00df sie suchen bey Gerichten,", "tokens": ["Lie\u00df", "sie", "su\u00b7chen", "bey", "Ge\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Fand sie aber da mit nichten;", "tokens": ["Fand", "sie", "a\u00b7ber", "da", "mit", "nich\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Muste h\u00f6ren, das man sagte:", "tokens": ["Mus\u00b7te", "h\u00f6\u00b7ren", ",", "das", "man", "sag\u00b7te", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Was das wer, wo nach er fragte?", "tokens": ["Was", "das", "wer", ",", "wo", "nach", "er", "frag\u00b7te", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "PWS", "$,", "PWAV", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}