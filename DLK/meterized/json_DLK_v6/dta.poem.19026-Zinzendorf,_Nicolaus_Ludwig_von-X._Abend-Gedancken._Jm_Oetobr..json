{"dta.poem.19026": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "X.   Abend-Gedancken.   Jm Oetobr.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Du Vater aller Geister,", "tokens": ["Du", "Va\u00b7ter", "al\u00b7ler", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du Strahl der Ewigkeit,", "tokens": ["Du", "Strahl", "der", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du wunderbahrer Meister,", "tokens": ["Du", "wun\u00b7der\u00b7bah\u00b7rer", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du Innbegriff der Zeit,", "tokens": ["Du", "Inn\u00b7be\u00b7griff", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du hast der Menschen Seelen", "tokens": ["Du", "hast", "der", "Men\u00b7schen", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In deine Hand gepr\u00e4gt:", "tokens": ["In", "dei\u00b7ne", "Hand", "ge\u00b7pr\u00e4gt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wem kans an Ruhe fehlen,", "tokens": ["Wem", "kans", "an", "Ru\u00b7he", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der hie sich schlafen legt.", "tokens": ["Der", "hie", "sich", "schla\u00b7fen", "legt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PRF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es ziehn der Sonnen Blicke,", "tokens": ["Es", "ziehn", "der", "Son\u00b7nen", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit ihrem hellen Strich", "tokens": ["Mit", "ih\u00b7rem", "hel\u00b7len", "Strich"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sich nach und nach zur\u00fccke,", "tokens": ["Sich", "nach", "und", "nach", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "KON", "APPR", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Lufft verfinstert sich,", "tokens": ["Die", "Lufft", "ver\u00b7fins\u00b7tert", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der dunckle Mond erleuchtet", "tokens": ["Der", "dunck\u00b7le", "Mond", "er\u00b7leuch\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Uns mit erborgtem Schein,", "tokens": ["Uns", "mit", "er\u00b7borg\u00b7tem", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dringt in die Erden ein.", "tokens": ["Dringt", "in", "die", "Er\u00b7den", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "++-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Das Wild in w\u00fcsten W\u00e4ldern", "tokens": ["Das", "Wild", "in", "w\u00fcs\u00b7ten", "W\u00e4l\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Geht hungrig auf den Raub;", "tokens": ["Geht", "hung\u00b7rig", "auf", "den", "Raub", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Das Vieh in stillen Feldern", "tokens": ["Das", "Vieh", "in", "stil\u00b7len", "Fel\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Sucht Ruh in Busch und Laub;", "tokens": ["Sucht", "Ruh", "in", "Busch", "und", "Laub", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Der Mensch von schweren Lasten", "tokens": ["Der", "Mensch", "von", "schwe\u00b7ren", "Las\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Der Arbeit unterdr\u00fcckt,", "tokens": ["Der", "Ar\u00b7beit", "un\u00b7ter\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Begehret auszurasten,", "tokens": ["Be\u00b7ge\u00b7hret", "aus\u00b7zu\u00b7ras\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Steht schl\u00e4ffrig und geb\u00fcckt.", "tokens": ["Steht", "schl\u00e4f\u00b7frig", "und", "ge\u00b7b\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Winde Ungeheuer", "tokens": ["Der", "Win\u00b7de", "Un\u00b7ge\u00b7heu\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "St\u00fcrmt auf die H\u00e4user an,", "tokens": ["St\u00fcrmt", "auf", "die", "H\u00e4u\u00b7ser", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo ein verschlo\u00dfnes Feuer", "tokens": ["Wo", "ein", "ver\u00b7schlo\u00df\u00b7nes", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich kaum erhalten kan:", "tokens": ["Sich", "kaum", "er\u00b7hal\u00b7ten", "kan", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn sich die Nebel sencken,", "tokens": ["Wenn", "sich", "die", "Ne\u00b7bel", "sen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verliehrt man alle Spuhr,", "tokens": ["Ver\u00b7liehrt", "man", "al\u00b7le", "Spuhr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Regen Str\u00f6hm\u2019 ertr\u00e4ncken", "tokens": ["Die", "Re\u00b7gen", "Str\u00f6hm'", "er\u00b7tr\u00e4n\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der flachen Felder Fluhr.", "tokens": ["Der", "fla\u00b7chen", "Fel\u00b7der", "Fluhr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da f\u00e4llt man billig nieder", "tokens": ["Da", "f\u00e4llt", "man", "bil\u00b7lig", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor GOttes Majest\u00e4t,", "tokens": ["Vor", "Got\u00b7tes", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und \u00fcbergibt ihm wieder,", "tokens": ["Und", "\u00fc\u00b7berg\u00b7ibt", "ihm", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was man von ihm empf\u00e4ht:", "tokens": ["Was", "man", "von", "ihm", "em\u00b7pf\u00e4ht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die gantze Krafft der Sinnen", "tokens": ["Die", "gant\u00b7ze", "Krafft", "der", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Senckt sich in den hinein,", "tokens": ["Senckt", "sich", "in", "den", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "PTKVZ", "$,"], "meter": "+-++-+", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Durch welchen sie beginnen,", "tokens": ["Durch", "wel\u00b7chen", "sie", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und dem sie eigen seyn.", "tokens": ["Und", "dem", "sie", "ei\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das heist den Tag vollenden,", "tokens": ["Das", "heist", "den", "Tag", "voll\u00b7en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das heist sich wohl gelegt:", "tokens": ["Das", "heist", "sich", "wohl", "ge\u00b7legt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man ruht in dessen H\u00e4nden,", "tokens": ["Man", "ruht", "in", "des\u00b7sen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PRELAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der alles hebt und tr\u00e4gt.", "tokens": ["Der", "al\u00b7les", "hebt", "und", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Himmel m\u00f6gen zittern,", "tokens": ["Die", "Him\u00b7mel", "m\u00f6\u00b7gen", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df unsre Veste kracht,", "tokens": ["Da\u00df", "uns\u00b7re", "Ves\u00b7te", "kracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Elemente wittern;", "tokens": ["Die", "E\u00b7le\u00b7men\u00b7te", "wit\u00b7tern", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "So sind wir wol bewacht.", "tokens": ["So", "sind", "wir", "wol", "be\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}