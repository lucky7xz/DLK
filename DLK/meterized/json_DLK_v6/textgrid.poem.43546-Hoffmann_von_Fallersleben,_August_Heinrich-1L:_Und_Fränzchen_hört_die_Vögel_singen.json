{"textgrid.poem.43546": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und Fr\u00e4nzchen h\u00f6rt die V\u00f6gel singen", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und Fr\u00e4nzchen h\u00f6rt die V\u00f6gel singen", "tokens": ["Und", "Fr\u00e4nz\u00b7chen", "h\u00f6rt", "die", "V\u00f6\u00b7gel", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und sieht sie von Zweigen zu Zweigen springen,", "tokens": ["Und", "sieht", "sie", "von", "Zwei\u00b7gen", "zu", "Zwei\u00b7gen", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und wie sie rufen: widewitt! widewitt!", "tokens": ["Und", "wie", "sie", "ru\u00b7fen", ":", "wi\u00b7de\u00b7witt", "!", "wi\u00b7de\u00b7witt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVINF", "$.", "VVFIN", "$.", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "So ruft er ihnen: komm mit! komm mit!", "tokens": ["So", "ruft", "er", "ih\u00b7nen", ":", "komm", "mit", "!", "komm", "mit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$.", "VVFIN", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und Fr\u00e4nzchen sieht die Blumen winken", "tokens": ["Und", "Fr\u00e4nz\u00b7chen", "sieht", "die", "Blu\u00b7men", "win\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und sieht die Steinchen im Wege blinken,", "tokens": ["Und", "sieht", "die", "Stein\u00b7chen", "im", "We\u00b7ge", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er b\u00fcckt sich und sammelt bei jedem Tritt", "tokens": ["Er", "b\u00fcckt", "sich", "und", "sam\u00b7melt", "bei", "je\u00b7dem", "Tritt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "KON", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und ruft bei allen: komm mit! komm mit!", "tokens": ["Und", "ruft", "bei", "al\u00b7len", ":", "komm", "mit", "!", "komm", "mit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIS", "$.", "VVFIN", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Und K\u00e4fer schwirren, und Falter flattern,", "tokens": ["Und", "K\u00e4\u00b7fer", "schwir\u00b7ren", ",", "und", "Fal\u00b7ter", "flat\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,", "KON", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er will sie fangen, er will sie ergattern,", "tokens": ["Er", "will", "sie", "fan\u00b7gen", ",", "er", "will", "sie", "er\u00b7gat\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er will hinterher mit verdoppeltem Schritt", "tokens": ["Er", "will", "hin\u00b7ter\u00b7her", "mit", "ver\u00b7dop\u00b7pel\u00b7tem", "Schritt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und ruft ihnen nach: komm mit! komm mit!", "tokens": ["Und", "ruft", "ih\u00b7nen", "nach", ":", "komm", "mit", "!", "komm", "mit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Und was er sieht von nah und von weiten,", "tokens": ["Und", "was", "er", "sieht", "von", "nah", "und", "von", "wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "APPR", "ADJD", "KON", "APPR", "ADJA", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Soll freu'n sich mit ihm und soll ihn begleiten,", "tokens": ["Soll", "freu'n", "sich", "mit", "ihm", "und", "soll", "ihn", "be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "PRF", "APPR", "PPER", "KON", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Komm mit! so ruft er bei jedem Tritt,", "tokens": ["Komm", "mit", "!", "so", "ruft", "er", "bei", "je\u00b7dem", "Tritt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die ganze Welt soll mit, soll mit!", "tokens": ["Die", "gan\u00b7ze", "Welt", "soll", "mit", ",", "soll", "mit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "APPR", "$,", "VMFIN", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}