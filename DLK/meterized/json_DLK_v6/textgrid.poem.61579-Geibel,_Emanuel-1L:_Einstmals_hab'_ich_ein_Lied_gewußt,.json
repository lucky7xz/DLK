{"textgrid.poem.61579": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einstmals hab' ich ein Lied gewu\u00dft,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einstmals hab' ich ein Lied gewu\u00dft,", "tokens": ["Einst\u00b7mals", "hab'", "ich", "ein", "Lied", "ge\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Einst in goldenen Stunden", "tokens": ["Einst", "in", "gol\u00b7de\u00b7nen", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Sang ich's, da ich ein Kind noch war;", "tokens": ["Sang", "ich's", ",", "da", "ich", "ein", "Kind", "noch", "war", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "PPER", "ART", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aber mir ist's entschwunden.", "tokens": ["A\u00b7ber", "mir", "ist's", "ent\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "VVPP", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Lieblich schwebte die Weise hin,", "tokens": ["Lieb\u00b7lich", "schweb\u00b7te", "die", "Wei\u00b7se", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Weich wie Schwanengefieder;", "tokens": ["Weich", "wie", "Schwa\u00b7nen\u00b7ge\u00b7fie\u00b7der", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Ach, wohl such' ich durch Feld und Wald,", "tokens": ["Ach", ",", "wohl", "such'", "ich", "durch", "Feld", "und", "Wald", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Finde nimmer sie wieder.", "tokens": ["Fin\u00b7de", "nim\u00b7mer", "sie", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "ADV", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Manchmal mein' ich, es wogt ihr Laut", "tokens": ["Manch\u00b7mal", "mein'", "ich", ",", "es", "wogt", "ihr", "Laut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "APPR"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "\u00dcber der Flur in den Winden,", "tokens": ["\u00dc\u00b7ber", "der", "Flur", "in", "den", "Win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Aber er ist verhallt im Nu,", "tokens": ["A\u00b7ber", "er", "ist", "ver\u00b7hallt", "im", "Nu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "VVPP", "APPRART", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Will ich ihn greifen und binden.", "tokens": ["Will", "ich", "ihn", "grei\u00b7fen", "und", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Oft auch, wenn ich bei Nacht entschlief,", "tokens": ["Oft", "auch", ",", "wenn", "ich", "bei", "Nacht", "ent\u00b7schlief", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Streift urpl\u00f6tzlich und leise", "tokens": ["Streift", "ur\u00b7pl\u00f6tz\u00b7lich", "und", "lei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "ADJD"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "\u00dcber mein Herz mit Traumeshand", "tokens": ["\u00dc\u00b7ber", "mein", "Herz", "mit", "Trau\u00b7mes\u00b7hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Die verlorene Weise.", "tokens": ["Die", "ver\u00b7lo\u00b7re\u00b7ne", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.5": {"line.1": {"text": "Aber fahr' ich vom Kissen auf,", "tokens": ["A\u00b7ber", "fahr'", "ich", "vom", "Kis\u00b7sen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Kann ich mich nimmer besinnen;", "tokens": ["Kann", "ich", "mich", "nim\u00b7mer", "be\u00b7sin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Nur vom Auge noch f\u00fchl' ich sacht", "tokens": ["Nur", "vom", "Au\u00b7ge", "noch", "f\u00fchl'", "ich", "sacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ADV", "VVFIN", "PPER", "VVFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Brennende Tr\u00e4nen rinnen.", "tokens": ["Bren\u00b7nen\u00b7de", "Tr\u00e4\u00b7nen", "rin\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Und doch mein' ich, f\u00e4nd' ich den Klang:", "tokens": ["Und", "doch", "mein'", "ich", ",", "f\u00e4nd'", "ich", "den", "Klang", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "All die heimlichen Schmerzen", "tokens": ["All", "die", "heim\u00b7li\u00b7chen", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "K\u00f6nnt' ich wieder, wie einst als Kind,", "tokens": ["K\u00f6nnt'", "ich", "wie\u00b7der", ",", "wie", "einst", "als", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "$,", "PWAV", "ADV", "KOUS", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Mir wegsingen vom Herzen.", "tokens": ["Mir", "weg\u00b7sin\u00b7gen", "vom", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}}}}