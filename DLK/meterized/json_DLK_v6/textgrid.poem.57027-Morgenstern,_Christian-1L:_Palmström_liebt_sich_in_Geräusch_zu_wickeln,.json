{"textgrid.poem.57027": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Palmstr\u00f6m liebt sich in Ger\u00e4usch zu wickeln,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Palmstr\u00f6m liebt sich in Ger\u00e4usch zu wickeln,", "tokens": ["Palm\u00b7str\u00f6m", "liebt", "sich", "in", "Ge\u00b7r\u00e4usch", "zu", "wi\u00b7ckeln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "teils zur Abwehr wider fremde L\u00e4rme,", "tokens": ["teils", "zur", "Ab\u00b7wehr", "wi\u00b7der", "frem\u00b7de", "L\u00e4r\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "teils um sich vor drittem Ohr zu schirmen.", "tokens": ["teils", "um", "sich", "vor", "drit\u00b7tem", "Ohr", "zu", "schir\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Und so l\u00e4\u00dft er sich um seine Zimmer", "tokens": ["Und", "so", "l\u00e4\u00dft", "er", "sich", "um", "sei\u00b7ne", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wasserr\u00f6hren legen, welche brausen.", "tokens": ["Was\u00b7ser\u00b7r\u00f6h\u00b7ren", "le\u00b7gen", ",", "wel\u00b7che", "brau\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "PRELS", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und ergeht sich, so beh\u00fctet, oft in", "tokens": ["Und", "er\u00b7geht", "sich", ",", "so", "be\u00b7h\u00fc\u00b7tet", ",", "oft", "in"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "$,", "ADV", "VVPP", "$,", "ADV", "APPR"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "stundenlangen Monologen, stunden\u2013", "tokens": ["stun\u00b7den\u00b7lan\u00b7gen", "Mo\u00b7no\u00b7lo\u00b7gen", ",", "stun\u00b7den", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "langen Monologen, gleich dem Redner", "tokens": ["lan\u00b7gen", "Mo\u00b7no\u00b7lo\u00b7gen", ",", "gleich", "dem", "Red\u00b7ner"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "von Athen, der in die Brandung br\u00fcllte,", "tokens": ["von", "A\u00b7then", ",", "der", "in", "die", "Bran\u00b7dung", "br\u00fcll\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "gleich Demosthenes am Strand des Meeres.", "tokens": ["gleich", "De\u00b7mos\u00b7the\u00b7nes", "am", "Strand", "des", "Mee\u00b7res", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}