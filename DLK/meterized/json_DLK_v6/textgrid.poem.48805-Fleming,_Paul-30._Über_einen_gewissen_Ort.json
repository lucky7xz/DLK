{"textgrid.poem.48805": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "30. \u00dcber einen gewissen Ort", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier, da der kalte Quell aus starken Adern dringet", "tokens": ["Hier", ",", "da", "der", "kal\u00b7te", "Quell", "aus", "star\u00b7ken", "A\u00b7dern", "drin\u00b7get"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und durch das stille Tal mit sanften Rauschen trillt,", "tokens": ["und", "durch", "das", "stil\u00b7le", "Tal", "mit", "sanf\u00b7ten", "Rau\u00b7schen", "trillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da beides Ufer ist in Blumen eingeh\u00fcllt,", "tokens": ["da", "bei\u00b7des", "U\u00b7fer", "ist", "in", "Blu\u00b7men", "ein\u00b7ge\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "da manch' Hamadryas mit mancher Orkas springet,", "tokens": ["da", "man\u00b7ch'", "Ha\u00b7mad\u00b7ry\u00b7as", "mit", "man\u00b7cher", "Or\u00b7kas", "sprin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.2": {"line.1": {"text": "hier, da die Nachtigal die s\u00fc\u00dfen Lieder singet", "tokens": ["hier", ",", "da", "die", "Nach\u00b7ti\u00b7gal", "die", "s\u00fc\u00b7\u00dfen", "Lie\u00b7der", "sin\u00b7get"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "durch diesen langen Pusch, der ein gesunder Schild", "tokens": ["durch", "die\u00b7sen", "lan\u00b7gen", "Pusch", ",", "der", "ein", "ge\u00b7sun\u00b7der", "Schild"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "f\u00fcr Ph\u00f6bus Eifer ist, da nichts als zahmes Wild", "tokens": ["f\u00fcr", "Ph\u00f6\u00b7bus", "Ei\u00b7fer", "ist", ",", "da", "nichts", "als", "zah\u00b7mes", "Wild"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "VAFIN", "$,", "KOUS", "PIS", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "in feister Weide scherzt und um die Bulschaft ringet,", "tokens": ["in", "feis\u00b7ter", "Wei\u00b7de", "scherzt", "und", "um", "die", "Bul\u00b7schaft", "rin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "hier, der Ort, dieser ists, da mich das erste Mal", "tokens": ["hier", ",", "der", "Ort", ",", "die\u00b7ser", "ists", ",", "da", "mich", "das", "ers\u00b7te", "Mal"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ART", "NN", "$,", "PDS", "VAFIN", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Hier dieser ists, der Ort, der mich so gl\u00fcckhaft machte.", "tokens": ["Hier", "die\u00b7ser", "ists", ",", "der", "Ort", ",", "der", "mich", "so", "gl\u00fcck\u00b7haft", "mach\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VAFIN", "$,", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "F\u00fcr dieses w\u00fcndsch' ich ihm, da\u00df diese seine Zier,", "tokens": ["F\u00fcr", "die\u00b7ses", "w\u00fcnd\u00b7sch'", "ich", "ihm", ",", "da\u00df", "die\u00b7se", "sei\u00b7ne", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PPER", "PPER", "$,", "KOUS", "PDS", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "in der ich meine Lust, die h\u00f6chste, noch betrachte,", "tokens": ["in", "der", "ich", "mei\u00b7ne", "Lust", ",", "die", "h\u00f6chs\u00b7te", ",", "noch", "be\u00b7trach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die in und um ihn ist, ihm bleibe f\u00fcr und f\u00fcr.", "tokens": ["die", "in", "und", "um", "ihn", "ist", ",", "ihm", "blei\u00b7be", "f\u00fcr", "und", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "KON", "APPR", "PPER", "VAFIN", "$,", "PPER", "VVFIN", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}