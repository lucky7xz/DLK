{"dta.poem.5433": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Quelle alles Guten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bewundre deine Wunder, und ich bet' in ihnen an", "tokens": ["Ich", "be\u00b7wund\u00b7re", "dei\u00b7ne", "Wun\u00b7der", ",", "und", "ich", "bet'", "in", "ih\u00b7nen", "an"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KON", "PPER", "VVFIN", "APPR", "PPER", "PTKVZ"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Dich, o wunderbarer Sch\u00f6pfer, Quell des Lichts", "tokens": ["Dich", ",", "o", "wun\u00b7der\u00b7ba\u00b7rer", "Sch\u00f6p\u00b7fer", ",", "Quell", "des", "Lichts"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "FM", "ADJA", "NN", "$,", "NN", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "und aller Dinge;", "tokens": ["und", "al\u00b7ler", "Din\u00b7ge", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ob ich gleich, wie dieses alles eigentlich aus dir entspringe,", "tokens": ["Ob", "ich", "gleich", ",", "wie", "die\u00b7ses", "al\u00b7les", "ei\u00b7gent\u00b7lich", "aus", "dir", "ent\u00b7sprin\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "PWAV", "PDS", "PIS", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Nicht zu fassen f\u00e4hig bin, weniger beschreiben kann.", "tokens": ["Nicht", "zu", "fas\u00b7sen", "f\u00e4\u00b7hig", "bin", ",", "we\u00b7ni\u00b7ger", "be\u00b7schrei\u00b7ben", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKZU", "VVINF", "ADJD", "VAFIN", "$,", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Eben, da\u00df die\u00df meine Seele nicht begreiffet, stellet mir", "tokens": ["E\u00b7ben", ",", "da\u00df", "die\u00df", "mei\u00b7ne", "See\u00b7le", "nicht", "be\u00b7greif\u00b7fet", ",", "stel\u00b7let", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PDS", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,", "VVFIN", "PPER"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Meinen Geist, so wie er ist,", "tokens": ["Mei\u00b7nen", "Geist", ",", "so", "wie", "er", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "KOKOM", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Klein; dich gleichfals, wie du bist,", "tokens": ["Klein", ";", "dich", "gleich\u00b7fals", ",", "wie", "du", "bist", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "ADV", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Unbegreiflich, weis\u2019, unendlich, liebreich und allm\u00e4chtig", "tokens": ["Un\u00b7be\u00b7greif\u00b7lich", ",", "weis'", ",", "un\u00b7end\u00b7lich", ",", "lieb\u00b7reich", "und", "all\u00b7m\u00e4ch\u00b7tig"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "VVFIN", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.10": {"text": "f\u00fcr.", "tokens": ["f\u00fcr", "."], "token_info": ["word", "punct"], "pos": ["APPR", "$."], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Die Erk\u00e4nntni\u00df dein- und meiner, so aus deinen Wercken", "tokens": ["Die", "Er\u00b7k\u00e4nnt\u00b7ni\u00df", "dein", "und", "mei\u00b7ner", ",", "so", "aus", "dei\u00b7nen", "Wer\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "TRUNC", "KON", "PPOSAT", "$,", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.12": {"text": "qvillt,", "tokens": ["qvillt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Womit deine Lieb\u2019 und Allmacht aller Himmel Himmel", "tokens": ["Wo\u00b7mit", "dei\u00b7ne", "Lieb'", "und", "All\u00b7macht", "al\u00b7ler", "Him\u00b7mel", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "KON", "NN", "PIAT", "NN", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.14": {"text": "f\u00fcllt,", "tokens": ["f\u00fcllt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Wirckt, zu deinen wahren Ehren, wahre Demuth, wahre", "tokens": ["Wirckt", ",", "zu", "dei\u00b7nen", "wah\u00b7ren", "Eh\u00b7ren", ",", "wah\u00b7re", "De\u00b7muth", ",", "wah\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,", "ADJA", "NN", "$,", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.16": {"text": "Liebe,", "tokens": ["Lie\u00b7be", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Wahre Sehnsucht, dir gef\u00e4llig, danckbar, froh und from", "tokens": ["Wah\u00b7re", "Sehn\u00b7sucht", ",", "dir", "ge\u00b7f\u00e4l\u00b7lig", ",", "dan\u00b7ck\u00b7bar", ",", "froh", "und", "from"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "NN"], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "zu werden;", "tokens": ["zu", "wer\u00b7den", ";"], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VAINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Sie erzeugt zugleich die dir angenehmen Liebes-Triebe", "tokens": ["Sie", "er\u00b7zeugt", "zu\u00b7gleich", "die", "dir", "an\u00b7ge\u00b7neh\u00b7men", "Lie\u00b7bes\u00b7Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "PPER", "ADJA", "NN"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.20": {"text": "Gegen unsern Neben-Menschen. Kann denn wol auf dieser", "tokens": ["Ge\u00b7gen", "un\u00b7sern", "Ne\u00b7ben\u00b7Men\u00b7schen", ".", "Kann", "denn", "wol", "auf", "die\u00b7ser"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "VMFIN", "ADV", "ADV", "APPR", "PDAT"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.21": {"text": "Erden", "tokens": ["Er\u00b7den"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.22": {"text": "Ein GOtt-liebers Opfer seyn, als ein Hertz, das seine", "tokens": ["Ein", "Got\u00b7tlie\u00b7bers", "Op\u00b7fer", "seyn", ",", "als", "ein", "Hertz", ",", "das", "sei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "VAINF", "$,", "KOUS", "ART", "NN", "$,", "PRELS", "PPOSAT"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "Macht,", "tokens": ["Macht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.24": {"text": "Seine Weisheit, seine Liebe, in der Creatur, erweget,", "tokens": ["Sei\u00b7ne", "Weis\u00b7heit", ",", "sei\u00b7ne", "Lie\u00b7be", ",", "in", "der", "Crea\u00b7tur", ",", "er\u00b7we\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "APPR", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.25": {"text": "Und, in ihr, die Herrlichkeit ihres Sch\u00f6pfers, mit Bedacht,", "tokens": ["Und", ",", "in", "ihr", ",", "die", "Herr\u00b7lich\u00b7keit", "ih\u00b7res", "Sch\u00f6p\u00b7fers", ",", "mit", "Be\u00b7dacht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "PPER", "$,", "ART", "NN", "PPOSAT", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.26": {"text": "Sieht, bewundert, ehrt, und fr\u00f6lich seine Wunder \u00fcberleget?", "tokens": ["Sieht", ",", "be\u00b7wun\u00b7dert", ",", "ehrt", ",", "und", "fr\u00f6\u00b7lich", "sei\u00b7ne", "Wun\u00b7der", "\u00fc\u00b7ber\u00b7le\u00b7get", "?"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "$,", "VVFIN", "$,", "KON", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}