{"textgrid.poem.66765": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Rote Rosen, die gl\u00fchen,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rote Rosen, die gl\u00fchen,", "tokens": ["Ro\u00b7te", "Ro\u00b7sen", ",", "die", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Zeugen gl\u00fccklicher Zeit,", "tokens": ["Zeu\u00b7gen", "gl\u00fcck\u00b7li\u00b7cher", "Zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Als von Sorgen und M\u00fchen", "tokens": ["Als", "von", "Sor\u00b7gen", "und", "M\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "KON", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Das Herz befreit!", "tokens": ["Das", "Herz", "be\u00b7freit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "\u00dcber Trauer und Tr\u00fcmmer,", "tokens": ["\u00dc\u00b7ber", "Trau\u00b7er", "und", "Tr\u00fcm\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "W\u00fcsten, h\u00e4\u00dflichen Graus,", "tokens": ["W\u00fcs\u00b7ten", ",", "h\u00e4\u00df\u00b7li\u00b7chen", "Graus", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Bl\u00fchenden Lebens Schimmer,", "tokens": ["Bl\u00fc\u00b7hen\u00b7den", "Le\u00b7bens", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Neu breite dich aus!", "tokens": ["Neu", "brei\u00b7te", "dich", "aus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Bl\u00fcten, lang nicht beschieden,", "tokens": ["Bl\u00fc\u00b7ten", ",", "lang", "nicht", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "PTKNEG", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Gru\u00df aus schenkender Hand,", "tokens": ["Gru\u00df", "aus", "schen\u00b7ken\u00b7der", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Boten der Sehnsucht nach Frieden,", "tokens": ["Bo\u00b7ten", "der", "Sehn\u00b7sucht", "nach", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Segnet, o segnet das freudlose Land!", "tokens": ["Seg\u00b7net", ",", "o", "seg\u00b7net", "das", "freud\u00b7lo\u00b7se", "Land", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}