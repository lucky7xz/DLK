{"textgrid.poem.33381": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Bittschrift der verwittweten Erzherzogin Austria an ihren neuen Gebieter Leopold II.", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sey mir willkommen, theuerster", "tokens": ["Sey", "mir", "will\u00b7kom\u00b7men", ",", "theu\u00b7ers\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "ADJA"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Gebieter, Gatte, Freund und Herr:", "tokens": ["Ge\u00b7bie\u00b7ter", ",", "Gat\u00b7te", ",", "Freund", "und", "Herr", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit kindlichem Vertrauen", "tokens": ["Mit", "kind\u00b7li\u00b7chem", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Harrt eine neue Gattin dein,", "tokens": ["Harrt", "ei\u00b7ne", "neu\u00b7e", "Gat\u00b7tin", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Um k\u00fcnftighin auf dich allein,", "tokens": ["Um", "k\u00fcnf\u00b7tig\u00b7hin", "auf", "dich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr Gl\u00fcck und Heil zu bauen.", "tokens": ["Ihr", "Gl\u00fcck", "und", "Heil", "zu", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du, den die Vorsicht mir erkohr", "tokens": ["Du", ",", "den", "die", "Vor\u00b7sicht", "mir", "er\u00b7kohr"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Zum Herrscher, \u00f6ffne Herz und Ohr", "tokens": ["Zum", "Herr\u00b7scher", ",", "\u00f6ff\u00b7ne", "Herz", "und", "Ohr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Anjetzo meinen Bitten:", "tokens": ["An\u00b7je\u00b7tzo", "mei\u00b7nen", "Bit\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich habe viel durch Mi\u00dfgeschick,", "tokens": ["Ich", "ha\u00b7be", "viel", "durch", "Mi\u00df\u00b7ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Durch Mi\u00dfverstand und Feindes T\u00fcck'", "tokens": ["Durch", "Mi\u00df\u00b7ver\u00b7stand", "und", "Fein\u00b7des", "T\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In kurzer Zeit gelitten.", "tokens": ["In", "kur\u00b7zer", "Zeit", "ge\u00b7lit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das Hemd ist zwar, wer Wirtschaft kennt", "tokens": ["Das", "Hemd", "ist", "zwar", ",", "wer", "Wirt\u00b7schaft", "kennt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "PWS", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sie versteht, das Fundament", "tokens": ["Und", "sie", "ver\u00b7steht", ",", "das", "Fun\u00b7da\u00b7ment"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von jeder Garderobe;", "tokens": ["Von", "je\u00b7der", "Gar\u00b7de\u00b7ro\u00b7be", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "D'rum zog man mir vor kurzer Frist,", "tokens": ["D'\u00b7rum", "zog", "man", "mir", "vor", "kur\u00b7zer", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "(weil's Hemdewechseln Mode ist)", "tokens": ["(", "weil's", "Hem\u00b7de\u00b7wech\u00b7seln", "Mo\u00b7de", "ist", ")"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "NN", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein neues an zur Probe;", "tokens": ["Ein", "neu\u00b7es", "an", "zur", "Pro\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch ist dies neue Unterkleid", "tokens": ["Doch", "ist", "dies", "neu\u00b7e", "Un\u00b7ter\u00b7kleid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PDS", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir hier zu eng, und da zu weit,", "tokens": ["Mir", "hier", "zu", "eng", ",", "und", "da", "zu", "weit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKA", "ADJD", "$,", "KON", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und will am Leib nicht passen:", "tokens": ["Und", "will", "am", "Leib", "nicht", "pas\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPRART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es halten weder Naht noch Stich,", "tokens": ["Es", "hal\u00b7ten", "we\u00b7der", "Naht", "noch", "Stich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und aller Orten, ach, mu\u00df ich", "tokens": ["Und", "al\u00b7ler", "Or\u00b7ten", ",", "ach", ",", "mu\u00df", "ich"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "PIAT", "NN", "$,", "ITJ", "$,", "VMFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es jetzt schon flicken lassen.", "tokens": ["Es", "jetzt", "schon", "fli\u00b7cken", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Mein Hauptschmuck, der so pr\u00e4chtig stand,", "tokens": ["Mein", "Haupt\u00b7schmuck", ",", "der", "so", "pr\u00e4ch\u00b7tig", "stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die kaiserliche Kron' entschwand", "tokens": ["Die", "kai\u00b7ser\u00b7li\u00b7che", "Kron'", "ent\u00b7schwand"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit meines Gatten Leben:", "tokens": ["Mit", "mei\u00b7nes", "Gat\u00b7ten", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dies Kleinod, ob es gleich anitzt", "tokens": ["Dies", "Klei\u00b7nod", ",", "ob", "es", "gleich", "a\u00b7nitzt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "NN", "$,", "KOUS", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein F\u00fcrstenhaupt mehr ziert als sch\u00fctzt,", "tokens": ["Ein", "F\u00fcrs\u00b7ten\u00b7haupt", "mehr", "ziert", "als", "sch\u00fctzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "KOKOM", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kannst du mir wiedergeben.", "tokens": ["Kannst", "du", "mir", "wie\u00b7der\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Allein der allersch\u00f6nste Schmuck,", "tokens": ["Al\u00b7lein", "der", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Schmuck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den jemals eine F\u00fcrstin trug,", "tokens": ["Den", "je\u00b7mals", "ei\u00b7ne", "F\u00fcrs\u00b7tin", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist der von Laudons H\u00e4nden", "tokens": ["Ist", "der", "von", "Lau\u00b7dons", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "APPR", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir neugewund'ne Lorberkranz:", "tokens": ["Mir", "neu\u00b7ge\u00b7wun\u00b7d'\u00b7ne", "Lor\u00b7ber\u00b7kranz", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "O la\u00df zu meines Hauptes Glanz", "tokens": ["O", "la\u00df", "zu", "mei\u00b7nes", "Haup\u00b7tes", "Glanz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur diesen mir vollenden!", "tokens": ["Nur", "die\u00b7sen", "mir", "voll\u00b7en\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Mein Zobelmantel, welcher gro\u00df", "tokens": ["Mein", "Zo\u00b7bel\u00b7man\u00b7tel", ",", "wel\u00b7cher", "gro\u00df"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und weit um meine Schultern flo\u00df,", "tokens": ["Und", "weit", "um", "mei\u00b7ne", "Schul\u00b7tern", "flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hing fest an einer Spange;", "tokens": ["Hing", "fest", "an", "ei\u00b7ner", "Span\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sieh, die feste Spange bricht,", "tokens": ["Und", "sieh", ",", "die", "fes\u00b7te", "Span\u00b7ge", "bricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zwar f\u00e4llt darum der Mantel nicht", "tokens": ["Zwar", "f\u00e4llt", "da\u00b7rum", "der", "Man\u00b7tel", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PAV", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und decket mich noch lange;", "tokens": ["Und", "de\u00b7cket", "mich", "noch", "lan\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Doch k\u00f6nntest du (wenn jetzo nicht", "tokens": ["Doch", "k\u00f6nn\u00b7test", "du", "(", "wenn", "jet\u00b7zo", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "$(", "KOUS", "ADV", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Mund vielleicht zur Unzeit spricht)", "tokens": ["Mein", "Mund", "viel\u00b7leicht", "zur", "Un\u00b7zeit", "spricht", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir f\u00fcglich noch daneben", "tokens": ["Mir", "f\u00fcg\u00b7lich", "noch", "da\u00b7ne\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "PAV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Wechsel f\u00fcr die Jahreszeit,", "tokens": ["Zum", "Wech\u00b7sel", "f\u00fcr", "die", "Jah\u00b7res\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein neues, zweites Mantelkleid", "tokens": ["Ein", "neu\u00b7es", ",", "zwei\u00b7tes", "Man\u00b7tel\u00b7kleid"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von Gros de Naples geben.", "tokens": ["Von", "Gros", "de", "Nap\u00b7les", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "NE", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Der Schmuck, der in mein fliegend Haar", "tokens": ["Der", "Schmuck", ",", "der", "in", "mein", "flie\u00b7gend", "Haar"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir r\u00fcckw\u00e4rts eingeflochten war", "tokens": ["Mir", "r\u00fcck\u00b7w\u00e4rts", "ein\u00b7ge\u00b7floch\u00b7ten", "war"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Gold- und Silberb\u00e4ndern,", "tokens": ["Mit", "Gold", "und", "Sil\u00b7ber\u00b7b\u00e4n\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bestand aus Steinen, gro\u00df und \u00e4cht,", "tokens": ["Be\u00b7stand", "aus", "Stei\u00b7nen", ",", "gro\u00df", "und", "\u00e4cht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur die Fa\u00e7on war alt und schlecht,", "tokens": ["Nur", "die", "Fa\u00e7on", "war", "alt", "und", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "D'rum wollte man sie \u00e4ndern.", "tokens": ["D'\u00b7rum", "woll\u00b7te", "man", "sie", "\u00e4n\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PIS", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Zwar Ungemach und Mi\u00dfverstand", "tokens": ["Zwar", "Un\u00b7ge\u00b7mach", "und", "Mi\u00df\u00b7ver\u00b7stand"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bestimmten meines Gatten Hand", "tokens": ["Be\u00b7stimm\u00b7ten", "mei\u00b7nes", "Gat\u00b7ten", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihn, wie er war, zu lassen:", "tokens": ["Ihn", ",", "wie", "er", "war", ",", "zu", "las\u00b7sen", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PWAV", "PPER", "VAFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch theilen selbst die Herr'n, die mir", "tokens": ["Doch", "thei\u00b7len", "selbst", "die", "Herr'n", ",", "die", "mir"], "token_info": ["word", "word", "word", "word", "word", 