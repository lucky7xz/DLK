{"dta.poem.12387": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Der ernsthafte J\u00e4ger .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es wollt ein J\u00e4ger jagen               ", "tokens": ["Es", "wollt", "ein", "J\u00e4\u00b7ger", "ja\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Hirschlein oder ein Reh,", "tokens": ["Ein", "Hirsc\u00b7hlein", "o\u00b7der", "ein", "Reh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Drei St\u00fcndlein vor dem Tagen,", "tokens": ["Drei", "St\u00fcnd\u00b7lein", "vor", "dem", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Hirschlein oder ein Reh.", "tokens": ["Ein", "Hirsc\u00b7hlein", "o\u00b7der", "ein", "Reh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "\u201each J\u00e4ger, du hast kein verschlafen,", "tokens": ["\u201e", "ach", "J\u00e4\u00b7ger", ",", "du", "hast", "kein", "ver\u00b7schla\u00b7fen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "XY", "NN", "$,", "PPER", "VAFIN", "PIAT", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u201elieber J\u00e4ger, jezt ist es Zeit;", "tokens": ["\u201e", "lie\u00b7ber", "J\u00e4\u00b7ger", ",", "jezt", "ist", "es", "Zeit", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NN", "$,", "ADV", "VAFIN", "PPER", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "\u201edein Schlaf thut mich erfreuen", "tokens": ["\u201e", "dein", "Schlaf", "thut", "mich", "er\u00b7freu\u00b7en"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201ein meiner stillen Einsamkeit.\u201c", "tokens": ["\u201e", "in", "mei\u00b7ner", "stil\u00b7len", "Ein\u00b7sam\u00b7keit", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das th\u00e4t den J\u00e4ger verdrie\u00dfen,", "tokens": ["Das", "th\u00e4t", "den", "J\u00e4\u00b7ger", "ver\u00b7drie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dieweil sie so reden th\u00e4t,", "tokens": ["Die\u00b7weil", "sie", "so", "re\u00b7den", "th\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Er wollt das Jungfr\u00e4ulein erschie\u00dfen,", "tokens": ["Er", "wollt", "das", "Jung\u00b7fr\u00e4u\u00b7lein", "er\u00b7schie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dieweil sie so reden th\u00e4t.", "tokens": ["Die\u00b7weil", "sie", "so", "re\u00b7den", "th\u00e4t", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Sie fiel dem J\u00e4ger zu F\u00fc\u00dfen,", "tokens": ["Sie", "fiel", "dem", "J\u00e4\u00b7ger", "zu", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Auf ihre schneeweisse Knie:", "tokens": ["Auf", "ih\u00b7re", "schnee\u00b7weis\u00b7se", "Knie", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "\u201each J\u00e4ger thu mich nicht erschie\u00dfen!\u201c", "tokens": ["\u201e", "ach", "J\u00e4\u00b7ger", "thu", "mich", "nicht", "er\u00b7schie\u00b7\u00dfen", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "XY", "NN", "VVFIN", "PPER", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem J\u00e4ger das Herze wohl brach.", "tokens": ["Dem", "J\u00e4\u00b7ger", "das", "Her\u00b7ze", "wohl", "brach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDS", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Sie th\u00e4t den J\u00e4ger wohl fragen:", "tokens": ["Sie", "th\u00e4t", "den", "J\u00e4\u00b7ger", "wohl", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "\u201each edler J\u00e4ger mein,", "tokens": ["\u201e", "ach", "ed\u00b7ler", "J\u00e4\u00b7ger", "mein", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJA", "NN", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201edarf ich ein gr\u00fcn Kranz fern tragen,", "tokens": ["\u201e", "darf", "ich", "ein", "gr\u00fcn", "Kranz", "fern", "tra\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "ART", "ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u201ein meinem goldfarbenen Haar?\u201c", "tokens": ["\u201e", "in", "mei\u00b7nem", "gold\u00b7far\u00b7be\u00b7nen", "Haar", "?", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+---+", "measure": "unknown.measure.tri"}}, "stanza.6": {"line.1": {"text": "\u201egr\u00fcn Kr\u00e4nzlein darfst du nicht tragen,", "tokens": ["\u201e", "gr\u00fcn", "Kr\u00e4nz\u00b7lein", "darfst", "du", "nicht", "tra\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NN", "VVFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "\u201ewie ein Jungfr\u00e4uelein tr\u00e4gt,", "tokens": ["\u201e", "wie", "ein", "Jung\u00b7fr\u00e4u\u00b7e\u00b7lein", "tr\u00e4gt", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201eein schneewei\u00df H\u00e4ublein sollst tragen,", "tokens": ["\u201e", "ein", "schnee\u00b7wei\u00df", "H\u00e4ub\u00b7lein", "sollst", "tra\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJD", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "\u201ewie ein jung J\u00e4gersfrau tr\u00e4gt.\u201c", "tokens": ["\u201e", "wie", "ein", "jung", "J\u00e4\u00b7gers\u00b7frau", "tr\u00e4gt", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "ART", "ADJD", "NN", "VVFIN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}