{"textgrid.poem.59177": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: O k\u00f6stlich's Kreuz, du st\u00e4rkst mich fein,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O k\u00f6stlich's Kreuz, du st\u00e4rkst mich fein,", "tokens": ["O", "k\u00f6st\u00b7lich's", "Kreuz", ",", "du", "st\u00e4rkst", "mich", "fein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du machst, da\u00df ich in dir mich kenne;", "tokens": ["Du", "machst", ",", "da\u00df", "ich", "in", "dir", "mich", "ken\u00b7ne", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "APPR", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du sollst mein Licht und F\u00fchrer sein", "tokens": ["Du", "sollst", "mein", "Licht", "und", "F\u00fch\u00b7rer", "sein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "KON", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei dunkler Nacht, wie ich dies Leben nenne.", "tokens": ["Bei", "dunk\u00b7ler", "Nacht", ",", "wie", "ich", "dies", "Le\u00b7ben", "nen\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV", "PPER", "PDS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}