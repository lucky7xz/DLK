{"dta.poem.1611": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Deutscher Ehren-Prei\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rt/ Helden-S\u00f6hne/ ", "tokens": ["H\u00f6rt", "/", "Hel\u00b7den\u00b7S\u00f6h\u00b7ne", "/"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVIMP", "$(", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Mein Barden-Geth\u00f6ne/", "tokens": ["Mein", "Bar\u00b7den\u00b7Get\u00b7h\u00f6\u00b7ne", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Gebt flei\u00dfig Acht/", "tokens": ["Gebt", "flei\u00b7\u00dfig", "Acht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "CARD", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Was aus dem H\u00e4yne", "tokens": ["Was", "aus", "dem", "H\u00e4y\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Darinn ich erscheine/", "tokens": ["Da\u00b7rinn", "ich", "er\u00b7schei\u00b7ne", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wird an euch bracht.", "tokens": ["Wird", "an", "euch", "bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ihr m\u00fcst euch fleissen/", "tokens": ["Ihr", "m\u00fcst", "euch", "fleis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Im Fall ihr wollt heissen", "tokens": ["Im", "Fall", "ihr", "wollt", "heis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPER", "VMFIN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ein Edles Blutt/", "tokens": ["Ein", "Ed\u00b7les", "Blutt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Bey Schwerdt und Eisen", "tokens": ["Bey", "Schwerdt", "und", "Ei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Im Felde zuweisen", "tokens": ["Im", "Fel\u00b7de", "zu\u00b7wei\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Den k\u00fchnen Mutt.", "tokens": ["Den", "k\u00fch\u00b7nen", "Mutt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Der Jugend Gaben", "tokens": ["Der", "Ju\u00b7gend", "Ga\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "In Ruhe vergraben", "tokens": ["In", "Ru\u00b7he", "ver\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Steht \u00fcbel an:", "tokens": ["Steht", "\u00fc\u00b7bel", "an", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Auff Kriegs-Gesch\u00e4ffte", "tokens": ["Auff", "Kriegs\u00b7Ge\u00b7sch\u00e4ff\u00b7te"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Verwenden die Kr\u00e4ffte", "tokens": ["Ver\u00b7wen\u00b7den", "die", "Kr\u00e4ff\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ist wohl gethan.", "tokens": ["Ist", "wohl", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Zu Hause liegen/", "tokens": ["Zu", "Hau\u00b7se", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die Gl\u00e4ser bekriegen", "tokens": ["Die", "Gl\u00e4\u00b7ser", "be\u00b7krie\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und stifften Zanck/", "tokens": ["Und", "stiff\u00b7ten", "Zanck", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Bringt euch ja Schande/", "tokens": ["Bringt", "euch", "ja", "Schan\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Vom F\u00fcrsten und Lande", "tokens": ["Vom", "F\u00fcrs\u00b7ten", "und", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Geringen Danck.", "tokens": ["Ge\u00b7rin\u00b7gen", "Danck", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Lasst euch doch tauren", "tokens": ["Lasst", "euch", "doch", "tau\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Im Miste zu sauren", "tokens": ["Im", "Mis\u00b7te", "zu", "sau\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKZU", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Auff fauler Haut:", "tokens": ["Auff", "fau\u00b7ler", "Haut", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Geht/ liebe Deutschen/", "tokens": ["Geht", "/", "lie\u00b7be", "Deut\u00b7schen", "/"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$(", "ADJA", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Die Feinde zu peitschen/", "tokens": ["Die", "Fein\u00b7de", "zu", "peit\u00b7schen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und fecht\u2019t ums Kraut.", "tokens": ["Und", "fecht't", "ums", "Kraut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Wo Waffen knallen", "tokens": ["Wo", "Waf\u00b7fen", "knal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Und H\u00f6rner erschallen/", "tokens": ["Und", "H\u00f6r\u00b7ner", "er\u00b7schal\u00b7len", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Das ist das Feld/", "tokens": ["Das", "ist", "das", "Feld", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Wo euren Tagen", "tokens": ["Wo", "eu\u00b7ren", "Ta\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Auff Ehre zu jagen", "tokens": ["Auff", "Eh\u00b7re", "zu", "ja\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PTKZU", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Der Raum bestellt.", "tokens": ["Der", "Raum", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Di\u00df hat der Alten", "tokens": ["Di\u00df", "hat", "der", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ged\u00e4chtnis erhalten/", "tokens": ["Ge\u00b7d\u00e4cht\u00b7nis", "er\u00b7hal\u00b7ten", "/"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Das man noch wei\u00df:", "tokens": ["Das", "man", "noch", "wei\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Mit Sturm und Ringen", "tokens": ["Mit", "Sturm", "und", "Rin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Die Feinde bezwingen", "tokens": ["Die", "Fein\u00b7de", "be\u00b7zwin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Gibt Ehr und Prei\u00df.", "tokens": ["Gibt", "Ehr", "und", "Prei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}