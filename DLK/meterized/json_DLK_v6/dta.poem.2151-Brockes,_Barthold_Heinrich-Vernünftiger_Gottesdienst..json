{"dta.poem.2151": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vern\u00fcnftiger Gottesdienst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bete dich, in deinen Wundern, o wunderbarer", "tokens": ["Ich", "be\u00b7te", "dich", ",", "in", "dei\u00b7nen", "Wun\u00b7dern", ",", "o", "wun\u00b7der\u00b7ba\u00b7rer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,", "FM", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sch\u00f6pfer an,", "tokens": ["Sch\u00f6p\u00b7fer", "an", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Weil ich, zu deinen heilgen Ehren, nichts W\u00fcrdigers er-", "tokens": ["Weil", "ich", ",", "zu", "dei\u00b7nen", "heil\u00b7gen", "Eh\u00b7ren", ",", "nichts", "W\u00fcr\u00b7di\u00b7gers", "er"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,", "PIS", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.4": {"text": "sinnen kann,", "tokens": ["sin\u00b7nen", "kann", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Als deine Liebe, Macht und Weisheit in den durch dich", "tokens": ["Als", "dei\u00b7ne", "Lie\u00b7be", ",", "Macht", "und", "Weis\u00b7heit", "in", "den", "durch", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "APPR", "ART", "APPR", "PPER"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "gewordnen Werken", "tokens": ["ge\u00b7word\u00b7nen", "Wer\u00b7ken"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Mit einem recht ger\u00fchrten Geist, mit Lust und Loben, zu", "tokens": ["Mit", "ei\u00b7nem", "recht", "ge\u00b7r\u00fchr\u00b7ten", "Geist", ",", "mit", "Lust", "und", "Lo\u00b7ben", ",", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "$,", "APPR"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "bemerken.", "tokens": ["be\u00b7mer\u00b7ken", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}