{"textgrid.poem.60575": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An das Gl\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Falsches Gl\u00fcck, das unsrem Arm entweichet,", "tokens": ["Fal\u00b7sches", "Gl\u00fcck", ",", "das", "uns\u00b7rem", "Arm", "ent\u00b7wei\u00b7chet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wann wir keichend dich bey nah erreichet!", "tokens": ["Wann", "wir", "kei\u00b7chend", "dich", "bey", "nah", "er\u00b7rei\u00b7chet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "PRF", "APPR", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Soll, o Abgott niedrer Seelen!", "tokens": ["Soll", ",", "o", "Ab\u00b7gott", "nie\u00b7drer", "See\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "FM", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll dein blinder Ha\u00df auch Weise qv\u00e4len?", "tokens": ["Soll", "dein", "blin\u00b7der", "Ha\u00df", "auch", "Wei\u00b7se", "qv\u00e4\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "ADV", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Manchem Thoren tr\u00e4gst du dich entgegen:", "tokens": ["Man\u00b7chem", "Tho\u00b7ren", "tr\u00e4gst", "du", "dich", "ent\u00b7ge\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Niemand sucht dich auf gebahnten Wegen:", "tokens": ["Nie\u00b7mand", "sucht", "dich", "auf", "ge\u00b7bahn\u00b7ten", "We\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Denn zu deinem steilen Reiche", "tokens": ["Denn", "zu", "dei\u00b7nem", "stei\u00b7len", "Rei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchrt ein k\u00fcrzrer Weg durch finstre Str\u00e4uche.", "tokens": ["F\u00fchrt", "ein", "k\u00fcrz\u00b7rer", "Weg", "durch", "finst\u00b7re", "Str\u00e4u\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Ich ergetze mich in stillen Gr\u00fcnden:", "tokens": ["Ich", "er\u00b7get\u00b7ze", "mich", "in", "stil\u00b7len", "Gr\u00fcn\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "All mein Unmuth flieht mit schnellen Winden,", "tokens": ["All", "mein", "Un\u00b7muth", "flieht", "mit", "schnel\u00b7len", "Win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn ich unter Freunden singe.", "tokens": ["Wenn", "ich", "un\u00b7ter", "Freun\u00b7den", "sin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6re selbst, wie meine Cyther klinge.", "tokens": ["H\u00f6\u00b7re", "selbst", ",", "wie", "mei\u00b7ne", "Cy\u00b7ther", "klin\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Wen besing ich, als den Gott der Reben?", "tokens": ["Wen", "be\u00b7sing", "ich", ",", "als", "den", "Gott", "der", "Re\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Denn die Rosen, die mein Haar beleben,", "tokens": ["Denn", "die", "Ro\u00b7sen", ",", "die", "mein", "Haar", "be\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und der vollen Gl\u00e4ser Menge", "tokens": ["Und", "der", "vol\u00b7len", "Gl\u00e4\u00b7ser", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind ihm heilig, und er liebt Ges\u00e4nge.", "tokens": ["Sind", "ihm", "hei\u00b7lig", ",", "und", "er", "liebt", "Ge\u00b7s\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "KON", "PPER", "VVFIN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Faunen! tanzt vor mir mit frohen Spr\u00fcngen:", "tokens": ["Fau\u00b7nen", "!", "tanzt", "vor", "mir", "mit", "fro\u00b7hen", "Spr\u00fcn\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Von Ly\u00e4ens Liebe will ich singen.", "tokens": ["Von", "Ly\u00b7\u00e4\u00b7ens", "Lie\u00b7be", "will", "ich", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Seine Sch\u00f6ne war noch bl\u00f6de", "tokens": ["Sei\u00b7ne", "Sch\u00f6\u00b7ne", "war", "noch", "bl\u00f6\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und aus unerfahrner Jugend spr\u00f6de.", "tokens": ["Und", "aus", "un\u00b7er\u00b7fahr\u00b7ner", "Ju\u00b7gend", "spr\u00f6\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Da verschlo\u00df er sich in einer Traube.", "tokens": ["Da", "ver\u00b7schlo\u00df", "er", "sich", "in", "ei\u00b7ner", "Trau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "O wie l\u00fcstern nahm sie ihn vom Laube!", "tokens": ["O", "wie", "l\u00fcs\u00b7tern", "nahm", "sie", "ihn", "vom", "Lau\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sie entbrannt' in fremde Triebe;", "tokens": ["Sie", "ent\u00b7brannt'", "in", "frem\u00b7de", "Trie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und noch itzo dient sein Wein der Liebe.", "tokens": ["Und", "noch", "it\u00b7zo", "dient", "sein", "Wein", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Wehrt so s\u00fcsser Ton nicht allem Leide?", "tokens": ["Wehrt", "so", "s\u00fcs\u00b7ser", "Ton", "nicht", "al\u00b7lem", "Lei\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "PTKNEG", "PIS", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Goldne Cyther, schalle stets von Freude,", "tokens": ["Gold\u00b7ne", "Cy\u00b7ther", ",", "schal\u00b7le", "stets", "von", "Freu\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wann ich hier am k\u00fchlen Bache,", "tokens": ["Wann", "ich", "hier", "am", "k\u00fch\u00b7len", "Ba\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hingestreckt auf weichen Blumen, lache:", "tokens": ["Hin\u00b7ge\u00b7streckt", "auf", "wei\u00b7chen", "Blu\u00b7men", ",", "la\u00b7che", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Hier, wo treue Schatten mich umschliessen,", "tokens": ["Hier", ",", "wo", "treu\u00b7e", "Schat\u00b7ten", "mich", "um\u00b7schlies\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und ich oft, berauscht von Wein und K\u00fcssen,", "tokens": ["Und", "ich", "oft", ",", "be\u00b7rauscht", "von", "Wein", "und", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "$,", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die ich um kein Gl\u00fcck vertausche,", "tokens": ["Die", "ich", "um", "kein", "Gl\u00fcck", "ver\u00b7tau\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "An der Phyllis vollem Busen lausche.", "tokens": ["An", "der", "Phyl\u00b7lis", "vol\u00b7lem", "Bu\u00b7sen", "lau\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Fahre hin, du sorgenreiches Gl\u00fccke!", "tokens": ["Fah\u00b7re", "hin", ",", "du", "sor\u00b7gen\u00b7rei\u00b7ches", "Gl\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Denn ich buhle nicht durch Bubenst\u00fccke", "tokens": ["Denn", "ich", "buh\u00b7le", "nicht", "durch", "Bu\u00b7ben\u00b7st\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Um das m\u00fchsame Vergn\u00fcgen,", "tokens": ["Um", "das", "m\u00fch\u00b7sa\u00b7me", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir im Schoo\u00df, verliebt in Rauch, zu liegen.", "tokens": ["Dir", "im", "Schoo\u00df", ",", "ver\u00b7liebt", "in", "Rauch", ",", "zu", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,", "VVFIN", "APPR", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Wenn kein Gold, noch Schimmer stolzer Ehren", "tokens": ["Wenn", "kein", "Gold", ",", "noch", "Schim\u00b7mer", "stol\u00b7zer", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "$,", "ADV", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nur um einen Tag mein Leben mehren;", "tokens": ["Nur", "um", "ei\u00b7nen", "Tag", "mein", "Le\u00b7ben", "meh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn ich nicht vergn\u00fcgter k\u00fcsse:", "tokens": ["Wenn", "ich", "nicht", "ver\u00b7gn\u00fcg\u00b7ter", "k\u00fcs\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was vermi\u00df ich, wenn ich dich vermisse?", "tokens": ["Was", "ver\u00b7mi\u00df", "ich", ",", "wenn", "ich", "dich", "ver\u00b7mis\u00b7se", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}