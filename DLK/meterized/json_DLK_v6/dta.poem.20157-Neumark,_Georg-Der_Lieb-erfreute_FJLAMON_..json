{"dta.poem.20157": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der Lieb-erfreute  \n  FJLAMON .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1666", "urn": "urn:nbn:de:kobv:b4-20424-1", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch-Historischer Lustgarten. Frankfurt (Main), 1666."}, "poem": {"stanza.1": {"line.1": {"text": "Zwey Ding\u2019 in dieser Welt mit Glantz f\u00fcr andern prahlen/", "tokens": ["Zwey", "Ding'", "in", "die\u00b7ser", "Welt", "mit", "Glantz", "f\u00fcr", "an\u00b7dern", "prah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "PDAT", "NN", "APPR", "NN", "APPR", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Liebsten Angesicht/ und ", "tokens": ["Der", "Liebs\u00b7ten", "An\u00b7ge\u00b7sicht", "/", "und"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$(", "KON"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das sch\u00f6nste Sternen-Heer/ so bey der Sonnen steht/", "tokens": ["Das", "sch\u00f6ns\u00b7te", "Ster\u00b7nen\u00b7Heer", "/", "so", "bey", "der", "Son\u00b7nen", "steht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verleuret seinen Schein/ die Klarheit ihm vergeht;", "tokens": ["Ver\u00b7leu\u00b7ret", "sei\u00b7nen", "Schein", "/", "die", "Klar\u00b7heit", "ihm", "ver\u00b7geht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So ist auch Belliflor; die andern m\u00fcssen weichen/", "tokens": ["So", "ist", "auch", "Bell\u00b7i\u00b7flor", ";", "die", "an\u00b7dern", "m\u00fcs\u00b7sen", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NE", "$.", "ART", "ADJA", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und wers auch Helena/ sie k\u00f6nte doch nicht gleichen", "tokens": ["Und", "wers", "auch", "He\u00b7le\u00b7na", "/", "sie", "k\u00f6n\u00b7te", "doch", "nicht", "glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "NE", "$(", "PPER", "VMFIN", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der G\u00f6ttlichen Gestalt: Ach m\u00f6chte sie auch seyn", "tokens": ["Der", "G\u00f6tt\u00b7li\u00b7chen", "Ge\u00b7stalt", ":", "Ach", "m\u00f6ch\u00b7te", "sie", "auch", "seyn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "VMFIN", "PPER", "ADV", "VAINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Barmhertzig und gelind\u2019! \u2012 \u2012 \u2012 \u2012 \u2012", "tokens": ["Barm\u00b7hert\u00b7zig", "und", "ge\u00b7lind'", "!", "\u2012", "\u2012", "\u2012", "\u2012", "\u2012"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "$.", "$(", "$(", "$(", "$(", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}