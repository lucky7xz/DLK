{"dta.poem.10354": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Xv.  \n  Das Liedchen von der Reue .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Herr Ulrich reitet im gr\u00fcnen Wald,", "tokens": ["Herr", "Ul\u00b7rich", "rei\u00b7tet", "im", "gr\u00fc\u00b7nen", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Die Bl\u00e4tter lustig rauschen.", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "lus\u00b7tig", "rau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er sieht eine holde M\u00e4dchengestalt", "tokens": ["Er", "sieht", "ei\u00b7ne", "hol\u00b7de", "M\u00e4d\u00b7chen\u00b7ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Durch Baumeszweige lauschen.", "tokens": ["Durch", "Bau\u00b7mes\u00b7zwei\u00b7ge", "lau\u00b7schen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Junker spricht: Wohl kenne ich", "tokens": ["Der", "Jun\u00b7ker", "spricht", ":", "Wohl", "ken\u00b7ne", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dies bl\u00fchende, gl\u00fchende Bildni\u00df,", "tokens": ["Dies", "bl\u00fc\u00b7hen\u00b7de", ",", "gl\u00fc\u00b7hen\u00b7de", "Bild\u00b7ni\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Verlockend stets umschwebt es mich", "tokens": ["Ver\u00b7lo\u00b7ckend", "stets", "um\u00b7schwebt", "es", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "ADV", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Volksgew\u00fchl und Wildni\u00df.", "tokens": ["In", "Volks\u00b7ge\u00b7w\u00fchl", "und", "Wild\u00b7ni\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zwei R\u00f6slein sind die Lippen dort,", "tokens": ["Zwei", "R\u00f6s\u00b7lein", "sind", "die", "Lip\u00b7pen", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die lieblichen, die frischen;", "tokens": ["Die", "lieb\u00b7li\u00b7chen", ",", "die", "fri\u00b7schen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch manches h\u00e4\u00dflich bitt're Wort", "tokens": ["Doch", "man\u00b7ches", "h\u00e4\u00df\u00b7lich", "bitt'\u00b7re", "Wort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schleicht t\u00fcckisch oft dazwischen.", "tokens": ["Schleicht", "t\u00fc\u00b7ckisch", "oft", "da\u00b7zwi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Drum gleicht dies M\u00fcndlein gar genau", "tokens": ["Drum", "gleicht", "dies", "M\u00fcnd\u00b7lein", "gar", "ge\u00b7nau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PDS", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den h\u00fcbschen Rosenb\u00fcschen,", "tokens": ["Den", "h\u00fcb\u00b7schen", "Ro\u00b7sen\u00b7b\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo gift'ge Schlangen wunderschlau", "tokens": ["Wo", "gift'\u00b7ge", "Schlan\u00b7gen", "wun\u00b7der\u00b7schlau"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im dunkeln Laube zischen.", "tokens": ["Im", "dun\u00b7keln", "Lau\u00b7be", "zi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dort jenes Gr\u00fcbchen wunderlieb", "tokens": ["Dort", "je\u00b7nes", "Gr\u00fcb\u00b7chen", "wun\u00b7der\u00b7lieb"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In wunderlieben Wangen,", "tokens": ["In", "wun\u00b7der\u00b7lie\u00b7ben", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das ist die Grube, worein mich trieb", "tokens": ["Das", "ist", "die", "Gru\u00b7be", ",", "wo\u00b7rein", "mich", "trieb"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wahnsinniges Verlangen.", "tokens": ["Wahn\u00b7sin\u00b7ni\u00b7ges", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.6": {"line.1": {"text": "Dort seh' ich ein sch\u00f6nes Lockenhaar", "tokens": ["Dort", "seh'", "ich", "ein", "sch\u00f6\u00b7nes", "Lo\u00b7cken\u00b7haar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vom sch\u00f6nsten K\u00f6pfchen hangen;", "tokens": ["Vom", "sch\u00f6ns\u00b7ten", "K\u00f6pf\u00b7chen", "han\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das sind die Netze wunderbar,", "tokens": ["Das", "sind", "die", "Net\u00b7ze", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Womit mich der B\u00f6se gefangen.", "tokens": ["Wo\u00b7mit", "mich", "der", "B\u00f6\u00b7se", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Und jenes blaue Auge dort,", "tokens": ["Und", "je\u00b7nes", "blau\u00b7e", "Au\u00b7ge", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So klar, wie stille Welle,", "tokens": ["So", "klar", ",", "wie", "stil\u00b7le", "Wel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das hielt ich f\u00fcr des Himmels Pfort',", "tokens": ["Das", "hielt", "ich", "f\u00fcr", "des", "Him\u00b7mels", "Pfort'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch war's die Pforte der H\u00f6lle. \u2014", "tokens": ["Doch", "wa\u00b7r's", "die", "Pfor\u00b7te", "der", "H\u00f6l\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "Herr Ulrich reitet weiter im Wald,", "tokens": ["Herr", "Ul\u00b7rich", "rei\u00b7tet", "wei\u00b7ter", "im", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die Bl\u00e4tter rauschen schaurig.", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "rau\u00b7schen", "schau\u00b7rig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da sieht er von fern eine zweite Gestalt,", "tokens": ["Da", "sieht", "er", "von", "fern", "ei\u00b7ne", "zwei\u00b7te", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die ist so bleich, so traurig.", "tokens": ["Die", "ist", "so", "bleich", ",", "so", "trau\u00b7rig", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Der Junker spricht: O Mutter dort,", "tokens": ["Der", "Jun\u00b7ker", "spricht", ":", "O", "Mut\u00b7ter", "dort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "NE", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die mich so m\u00fctterlich liebte,", "tokens": ["Die", "mich", "so", "m\u00fct\u00b7ter\u00b7lich", "lieb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der ich mit b\u00f6sem Thun und Wort", "tokens": ["Der", "ich", "mit", "b\u00f6\u00b7sem", "Thun", "und", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Leben bitterlich tr\u00fcbte!", "tokens": ["Das", "Le\u00b7ben", "bit\u00b7ter\u00b7lich", "tr\u00fcb\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "O, k\u00f6nnt ich dir trocknen die Augen na\u00df,", "tokens": ["O", ",", "k\u00f6nnt", "ich", "dir", "trock\u00b7nen", "die", "Au\u00b7gen", "na\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PPER", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mit der Gluth von meinen Schmerzen!", "tokens": ["Mit", "der", "Gluth", "von", "mei\u00b7nen", "Schmer\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "O, k\u00f6nnt ich dir r\u00f6then die Wangen bla\u00df", "tokens": ["O", ",", "k\u00f6nnt", "ich", "dir", "r\u00f6\u00b7then", "die", "Wan\u00b7gen", "bla\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPER", "PPER", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Mit dem Blut aus meinem Herzen!", "tokens": ["Mit", "dem", "Blut", "aus", "mei\u00b7nem", "Her\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Und weiter reitet Herr Ulerich,", "tokens": ["Und", "wei\u00b7ter", "rei\u00b7tet", "Herr", "U\u00b7le\u00b7rich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "NN", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Wald beginnt es zu d\u00fcstern,", "tokens": ["Im", "Wald", "be\u00b7ginnt", "es", "zu", "d\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Viel seltsame Stimmen regen sich,", "tokens": ["Viel", "selt\u00b7sa\u00b7me", "Stim\u00b7men", "re\u00b7gen", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADJA", "PRF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Abendwinde fl\u00fcstern.", "tokens": ["Die", "Ab\u00b7end\u00b7win\u00b7de", "fl\u00fcs\u00b7tern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Der Junker h\u00f6rt die Worte sein", "tokens": ["Der", "Jun\u00b7ker", "h\u00f6rt", "die", "Wor\u00b7te", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gar vielfach wiederklingen.", "tokens": ["Gar", "viel\u00b7fach", "wie\u00b7der\u00b7klin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das thaten die sp\u00f6ttischen Waldv\u00f6glein,", "tokens": ["Das", "tha\u00b7ten", "die", "sp\u00f6t\u00b7ti\u00b7schen", "Wald\u00b7v\u00f6\u00b7glein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die zwitschern laut und singen:", "tokens": ["Die", "zwit\u00b7schern", "laut", "und", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Herr Ulrich singt ein h\u00fcbsches Lied,", "tokens": ["Herr", "Ul\u00b7rich", "singt", "ein", "h\u00fcb\u00b7sches", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Liedchen von der Reue,", "tokens": ["Das", "Lied\u00b7chen", "von", "der", "Reu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und hat er zu Ende gesungen das Lied,", "tokens": ["Und", "hat", "er", "zu", "En\u00b7de", "ge\u00b7sun\u00b7gen", "das", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "NN", "VVPP", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "So singt er es wieder auf's Neue.", "tokens": ["So", "singt", "er", "es", "wie\u00b7der", "auf's", "Neu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "APPRART", "ADJA", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}