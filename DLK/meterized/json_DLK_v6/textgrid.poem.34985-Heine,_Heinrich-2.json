{"textgrid.poem.34985": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da sitzt er und schwatzt, mit lallender Zung',", "tokens": ["Da", "sitzt", "er", "und", "schwatzt", ",", "mit", "lal\u00b7len\u00b7der", "Zung'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der neue Alexander;", "tokens": ["Der", "neu\u00b7e", "A\u00b7lex\u00b7an\u00b7der", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Plan der Welteroberung,", "tokens": ["Den", "Plan", "der", "Wel\u00b7te\u00b7ro\u00b7be\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den setzt er auseinander:", "tokens": ["Den", "setzt", "er", "aus\u00b7ein\u00b7an\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bblothringen und Elsa\u00df, das wei\u00df ich l\u00e4ngst,", "tokens": ["\u00bb", "loth\u00b7rin\u00b7gen", "und", "El\u00b7sa\u00df", ",", "das", "wei\u00df", "ich", "l\u00e4ngst", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "KON", "NE", "$,", "PDS", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die fallen uns zu von selber;", "tokens": ["Die", "fal\u00b7len", "uns", "zu", "von", "sel\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKZU", "APPR", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Stute folgt am End' der Hengst,", "tokens": ["Der", "Stu\u00b7te", "folgt", "am", "End'", "der", "Hengst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es folgen der Kuh die K\u00e4lber.", "tokens": ["Es", "fol\u00b7gen", "der", "Kuh", "die", "K\u00e4l\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Mich lockt die Champagne, das be\u00dfre Land,", "tokens": ["Mich", "lockt", "die", "Cham\u00b7pag\u00b7ne", ",", "das", "be\u00df\u00b7re", "Land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.2": {"text": "Wo jene Reben sprie\u00dfen,", "tokens": ["Wo", "je\u00b7ne", "Re\u00b7ben", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die lieblich erleuchten unsern Verstand", "tokens": ["Die", "lieb\u00b7lich", "er\u00b7leuch\u00b7ten", "un\u00b7sern", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und uns das Leben vers\u00fc\u00dfen.", "tokens": ["Und", "uns", "das", "Le\u00b7ben", "ver\u00b7s\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Hier soll sich erproben mein Kriegesmut,", "tokens": ["Hier", "soll", "sich", "er\u00b7pro\u00b7ben", "mein", "Krie\u00b7ges\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Hier soll der Feldzug beginnen;", "tokens": ["Hier", "soll", "der", "Feld\u00b7zug", "be\u00b7gin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es knallen die Pfropfen, das wei\u00dfe Blut", "tokens": ["Es", "knal\u00b7len", "die", "Pfrop\u00b7fen", ",", "das", "wei\u00b7\u00dfe", "Blut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wird aus den Flaschen rinnen.", "tokens": ["Wird", "aus", "den", "Fla\u00b7schen", "rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Hier wird mein junges Heldentum", "tokens": ["Hier", "wird", "mein", "jun\u00b7ges", "Hel\u00b7den\u00b7tum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bis zu den Sternen moussieren!", "tokens": ["Bis", "zu", "den", "Ster\u00b7nen", "mous\u00b7sie\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Ich aber verfolge meinen Ruhm,", "tokens": ["Ich", "a\u00b7ber", "ver\u00b7fol\u00b7ge", "mei\u00b7nen", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich will auf Paris marschieren.", "tokens": ["Ich", "will", "auf", "Pa\u00b7ris", "mar\u00b7schie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NE", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Dort vor der Barriere mach ich halt,", "tokens": ["Dort", "vor", "der", "Bar\u00b7rie\u00b7re", "mach", "ich", "halt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Denn vor den Barrierepforten,", "tokens": ["Denn", "vor", "den", "Bar\u00b7rie\u00b7re\u00b7pf\u00b7or\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Da wird kein Oktroi bezahlt", "tokens": ["Da", "wird", "kein", "O\u00b7ktroi", "be\u00b7zahlt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "VVPP"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "F\u00fcr Wein von allen Sorten.\u00ab", "tokens": ["F\u00fcr", "Wein", "von", "al\u00b7len", "Sor\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "APPR", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}