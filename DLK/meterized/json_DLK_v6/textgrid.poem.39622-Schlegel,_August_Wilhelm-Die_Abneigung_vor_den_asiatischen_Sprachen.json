{"textgrid.poem.39622": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Abneigung vor den asiatischen Sprachen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei Algebristen", "tokens": ["Zwei", "Al\u00b7ge\u00b7bris\u00b7ten"], "token_info": ["word", "word"], "pos": ["CARD", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Und zwei Chimisten,", "tokens": ["Und", "zwei", "Chi\u00b7mis\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Die Viere wi\u00dfen nur die Sprachen,", "tokens": ["Die", "Vie\u00b7re", "wi\u00b7\u00dfen", "nur", "die", "Spra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die man h\u00f6rt zwischen Mainz und Aachen;", "tokens": ["Die", "man", "h\u00f6rt", "zwi\u00b7schen", "Mainz", "und", "Aa\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "NE", "KON", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Drum sehen sie es gar nicht gerne,", "tokens": ["Drum", "se\u00b7hen", "sie", "es", "gar", "nicht", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df man die Sprachen Asiens lerne,", "tokens": ["Da\u00df", "man", "die", "Spra\u00b7chen", "A\u00b7siens", "ler\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und in der R\u00e4um' und Zeiten Ferne", "tokens": ["Und", "in", "der", "R\u00e4um'", "und", "Zei\u00b7ten", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "KON", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Dichter und die Weisen kennen lerne.", "tokens": ["Die", "Dich\u00b7ter", "und", "die", "Wei\u00b7sen", "ken\u00b7nen", "ler\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Nichts Andres haben sie zu bei\u00dfen, noch zu kauen,", "tokens": ["Nichts", "And\u00b7res", "ha\u00b7ben", "sie", "zu", "bei\u00b7\u00dfen", ",", "noch", "zu", "kau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "PIS", "VAFIN", "PPER", "PTKZU", "VVINF", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Als hohles Rechnen, ekles Brauen;", "tokens": ["Als", "hoh\u00b7les", "Rech\u00b7nen", ",", "ek\u00b7les", "Brau\u00b7en", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Wobei sie so vergn\u00fcgt und selbstzufrieden sind,", "tokens": ["Wo\u00b7bei", "sie", "so", "ver\u00b7gn\u00fcgt", "und", "selbst\u00b7zu\u00b7frie\u00b7den", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVPP", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wie eine Milb' im K\u00e4s' und eine Laus im Grind.", "tokens": ["Wie", "ei\u00b7ne", "Milb'", "im", "K\u00e4s'", "und", "ei\u00b7ne", "Laus", "im", "Grind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "KON", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}