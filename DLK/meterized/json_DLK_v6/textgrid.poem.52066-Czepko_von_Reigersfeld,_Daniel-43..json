{"textgrid.poem.52066": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "43.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alvendus kaufft ein Gutt von funffzehn tausend Cronen", "tokens": ["Al\u00b7ven\u00b7dus", "kaufft", "ein", "Gutt", "von", "funff\u00b7zehn", "tau\u00b7send", "Cro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "CARD", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das wol ist angebaut, und da viel Leute wohnen:", "tokens": ["Das", "wol", "ist", "an\u00b7ge\u00b7baut", ",", "und", "da", "viel", "Leu\u00b7te", "woh\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "VVPP", "$,", "KON", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er hat es um das Geld gefunden, wie man spricht,", "tokens": ["Er", "hat", "es", "um", "das", "Geld", "ge\u00b7fun\u00b7den", ",", "wie", "man", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wol kaufft er. Warumb? Er zahlet keines nicht.", "tokens": ["So", "wol", "kaufft", "er", ".", "Wa\u00b7rumb", "?", "Er", "zah\u00b7let", "kei\u00b7nes", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$.", "PWAV", "$.", "PPER", "VVFIN", "PIS", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}