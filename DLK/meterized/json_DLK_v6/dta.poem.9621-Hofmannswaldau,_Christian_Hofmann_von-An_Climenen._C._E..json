{"dta.poem.9621": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An Climenen.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein urtheil ist: ich soll verbrennen/", "tokens": ["Mein", "ur\u00b7theil", "ist", ":", "ich", "soll", "ver\u00b7bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$.", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und doch darff ich die glut nicht nennen/", "tokens": ["Und", "doch", "darff", "ich", "die", "glut", "nicht", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Die meinen geist verzehrt.", "tokens": ["Die", "mei\u00b7nen", "geist", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die sinnen sind verkehrt/", "tokens": ["Die", "sin\u00b7nen", "sind", "ver\u00b7kehrt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Noch gleichwohl mu\u00df ich schweigen.", "tokens": ["Noch", "gleich\u00b7wohl", "mu\u00df", "ich", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mein hertz soll stahl und eisen seyn;", "tokens": ["Mein", "hertz", "soll", "stahl", "und", "ei\u00b7sen", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "KON", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wer aber hat doch fleisch und bein/", "tokens": ["Wer", "a\u00b7ber", "hat", "doch", "fleisch", "und", "bein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "ADV", "ADJD", "KON", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dem blut und regung nicht auch in die seele steigen?", "tokens": ["Dem", "blut", "und", "re\u00b7gung", "nicht", "auch", "in", "die", "see\u00b7le", "stei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PTKNEG", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Climene/ du kanst meine flammen", "tokens": ["Cli\u00b7me\u00b7ne", "/", "du", "kanst", "mei\u00b7ne", "flam\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$(", "PPER", "VMFIN", "PPOSAT", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcrwahr mit rechte nicht verdammen:", "tokens": ["F\u00fcr\u00b7wahr", "mit", "rech\u00b7te", "nicht", "ver\u00b7dam\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn dein verliebter blitz", "tokens": ["Denn", "dein", "ver\u00b7lieb\u00b7ter", "blitz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Hemmt selber meinen witz/", "tokens": ["Hemmt", "sel\u00b7ber", "mei\u00b7nen", "witz", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wilst du mehr noch wissen?", "tokens": ["Und", "wilst", "du", "mehr", "noch", "wis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dein heisser strahl hat mich entz\u00fcndt/", "tokens": ["Dein", "heis\u00b7ser", "strahl", "hat", "mich", "ent\u00b7z\u00fcndt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mein feu\u2019r ist deiner sonnen kind/", "tokens": ["Mein", "feu'r", "ist", "dei\u00b7ner", "son\u00b7nen", "kind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie soll ich \u00e4rmster denn nicht seine mutter k\u00fcssen?", "tokens": ["Wie", "soll", "ich", "\u00e4rms\u00b7ter", "denn", "nicht", "sei\u00b7ne", "mut\u00b7ter", "k\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADJD", "ADV", "PTKNEG", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ach! leugne nicht/ was du ver\u00fcbet/", "tokens": ["Ach", "!", "leug\u00b7ne", "nicht", "/", "was", "du", "ver\u00b7\u00fc\u00b7bet", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PTKNEG", "$(", "PWS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bin ein mensch/ und drum verliebet;", "tokens": ["Ich", "bin", "ein", "mensch", "/", "und", "drum", "ver\u00b7lie\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "KON", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df engel/ engel seyn/", "tokens": ["La\u00df", "en\u00b7gel", "/", "en\u00b7gel", "seyn", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$(", "ADJD", "VAINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sie f\u00fchlen keine pein;", "tokens": ["Sie", "f\u00fch\u00b7len", "kei\u00b7ne", "pein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wir aber haben glieder/", "tokens": ["Wir", "a\u00b7ber", "ha\u00b7ben", "glie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und sind aus fleisch und blut erbaut/", "tokens": ["Und", "sind", "aus", "fleisch", "und", "blut", "er\u00b7baut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ADJD", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wem vor des leibes bl\u00f6sse graut/", "tokens": ["Wem", "vor", "des", "lei\u00b7bes", "bl\u00f6s\u00b7se", "graut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der bring uns Adams stand und Evens unschuld wieder.", "tokens": ["Der", "bring", "uns", "A\u00b7dams", "stand", "und", "E\u00b7vens", "un\u00b7schuld", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NE", "VVFIN", "KON", "NE", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du stehest selbst/ ich bin getrieben/", "tokens": ["Du", "ste\u00b7hest", "selbst", "/", "ich", "bin", "ge\u00b7trie\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der himmel will/ ich soll dich lieben:", "tokens": ["Der", "him\u00b7mel", "will", "/", "ich", "soll", "dich", "lie\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$(", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn mein entbrandter schmertz", "tokens": ["Denn", "mein", "ent\u00b7brand\u00b7ter", "schmertz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Leidt ferner keinen schertz/", "tokens": ["Leidt", "fer\u00b7ner", "kei\u00b7nen", "schertz", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dein blick hat mich entz\u00fcndet/", "tokens": ["Dein", "blick", "hat", "mich", "ent\u00b7z\u00fcn\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich brenne nur/ dieweil ich mu\u00df/", "tokens": ["Ich", "bren\u00b7ne", "nur", "/", "die\u00b7weil", "ich", "mu\u00df", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "KOUS", "PPER", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So zeige nun durch einen ku\u00df/", "tokens": ["So", "zei\u00b7ge", "nun", "durch", "ei\u00b7nen", "ku\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df man zwar dort den todt/ hier aber leben findet.", "tokens": ["Da\u00df", "man", "zwar", "dort", "den", "todt", "/", "hier", "a\u00b7ber", "le\u00b7ben", "fin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "ART", "ADJD", "$(", "ADV", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}