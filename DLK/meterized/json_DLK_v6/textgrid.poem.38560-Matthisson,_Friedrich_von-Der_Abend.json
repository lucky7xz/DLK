{"textgrid.poem.38560": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Abend", "genre": "verse", "period": "N.A.", "pub_year": 1781, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie lieblich sinkt aus unbew\u00f6lktem Blau", "tokens": ["Wie", "lieb\u00b7lich", "sinkt", "aus", "un\u00b7be\u00b7w\u00f6lk\u00b7tem", "Blau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des goldnen Abends s\u00fcsse Ruh' herab!", "tokens": ["Des", "gold\u00b7nen", "A\u00b7bends", "s\u00fcs\u00b7se", "Ruh'", "her\u00b7ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein sanftes Rosenlicht umflie\u00dft den Hain,", "tokens": ["Ein", "sanf\u00b7tes", "Ro\u00b7sen\u00b7licht", "um\u00b7flie\u00dft", "den", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mischt mit des Baches Silberwelle sich,", "tokens": ["Mischt", "mit", "des", "Ba\u00b7ches", "Sil\u00b7ber\u00b7wel\u00b7le", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "PRF", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.5": {"text": "Bepurpurt Berg und Thal und Wiesenflur!", "tokens": ["Be\u00b7pur\u00b7purt", "Berg", "und", "Thal", "und", "Wie\u00b7sen\u00b7flur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie still ist Gottes Sch\u00f6pfung ringsumher!", "tokens": ["Wie", "still", "ist", "Got\u00b7tes", "Sch\u00f6p\u00b7fung", "rings\u00b7um\u00b7her", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nur dort im bl\u00fchenden Gestr\u00e4uche singt,", "tokens": ["Nur", "dort", "im", "bl\u00fc\u00b7hen\u00b7den", "Ge\u00b7str\u00e4u\u00b7che", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mit sanfter Klage, noch die Nachtigall", "tokens": ["Mit", "sanf\u00b7ter", "Kla\u00b7ge", ",", "noch", "die", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Dem hingeschiednen Tag' ein Sterbelied!", "tokens": ["Dem", "hin\u00b7ge\u00b7schied\u00b7nen", "Tag'", "ein", "Ster\u00b7be\u00b7lied", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich hebe freudig meine Augen auf,", "tokens": ["Ich", "he\u00b7be", "freu\u00b7dig", "mei\u00b7ne", "Au\u00b7gen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und siehe! du bist \u00fcberall, o Gott!", "tokens": ["Und", "sie\u00b7he", "!", "du", "bist", "\u00fc\u00b7be\u00b7rall", ",", "o", "Gott", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "PPER", "VAFIN", "ADV", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du bist es, Unerschaffner, der im Hauch", "tokens": ["Du", "bist", "es", ",", "Un\u00b7er\u00b7schaff\u00b7ner", ",", "der", "im", "Hauch"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "$,", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Abendwindes mir vor\u00fcberwallt,", "tokens": ["Des", "Ab\u00b7end\u00b7win\u00b7des", "mir", "vor\u00b7\u00fc\u00b7berw\u00b7allt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und frohen Schauer seiner Gegenwart", "tokens": ["Und", "fro\u00b7hen", "Schau\u00b7er", "sei\u00b7ner", "Ge\u00b7gen\u00b7wart"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In meine tiefger\u00fchrte Seele geu\u00dft!", "tokens": ["In", "mei\u00b7ne", "tief\u00b7ge\u00b7r\u00fchr\u00b7te", "See\u00b7le", "geu\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Du bist es, der dies Veilchen, welches hier,", "tokens": ["Du", "bist", "es", ",", "der", "dies", "Veil\u00b7chen", ",", "wel\u00b7ches", "hier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PRELS", "PDS", "NN", "$,", "PRELS", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Demuth Bild, im niedern Grase bl\u00fcht,", "tokens": ["Der", "De\u00b7muth", "Bild", ",", "im", "nie\u00b7dern", "Gra\u00b7se", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Aus m\u00fctterlichem Schoo\u00df der Erde rief,", "tokens": ["Aus", "m\u00fct\u00b7ter\u00b7li\u00b7chem", "Schoo\u00df", "der", "Er\u00b7de", "rief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ihm Farbenglanz und s\u00fcsse D\u00fcfte gab.", "tokens": ["Ihm", "Far\u00b7ben\u00b7glanz", "und", "s\u00fcs\u00b7se", "D\u00fcf\u00b7te", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Doch auch des Wurmes Vater bist du, Gott!", "tokens": ["Doch", "auch", "des", "Wur\u00b7mes", "Va\u00b7ter", "bist", "du", ",", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "NN", "VAFIN", "PPER", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Der dieses Veilchens, seiner Welt, sich freut!", "tokens": ["Der", "die\u00b7ses", "Veil\u00b7chens", ",", "sei\u00b7ner", "Welt", ",", "sich", "freut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "$,", "PPOSAT", "NN", "$,", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O wie sind deiner Wunder viel, o Herr!", "tokens": ["O", "wie", "sind", "dei\u00b7ner", "Wun\u00b7der", "viel", ",", "o", "Herr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VAFIN", "PPOSAT", "NN", "ADV", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein Geist, im Schranken seiner Endlichkeit,", "tokens": ["Mein", "Geist", ",", "im", "Schran\u00b7ken", "sei\u00b7ner", "End\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ermi\u00dft sie nicht! Wohin mein Auge schaut", "tokens": ["Er\u00b7mi\u00dft", "sie", "nicht", "!", "Wo\u00b7hin", "mein", "Au\u00b7ge", "schaut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$.", "PWAV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ist alles Kette, Ordnung, Harmonie,", "tokens": ["Ist", "al\u00b7les", "Ket\u00b7te", ",", "Ord\u00b7nung", ",", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und deiner Herlichkeiten Wiederglanz!", "tokens": ["Und", "dei\u00b7ner", "Her\u00b7lich\u00b7kei\u00b7ten", "Wie\u00b7der\u00b7glanz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "O du, der war und ist und seyn wird! Du,", "tokens": ["O", "du", ",", "der", "war", "und", "ist", "und", "seyn", "wird", "!", "Du", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "VAFIN", "KON", "VAFIN", "KON", "VAINF", "VAFIN", "$.", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Auf dessen Machtwink Welten untergehn", "tokens": ["Auf", "des\u00b7sen", "Macht\u00b7wink", "Wel\u00b7ten", "un\u00b7ter\u00b7gehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und Welten werden! Unbegreiflicher!", "tokens": ["Und", "Wel\u00b7ten", "wer\u00b7den", "!", "Un\u00b7be\u00b7greif\u00b7li\u00b7cher", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "VAINF", "$.", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Der Mensch, was ist er, da\u00df du sein gedenkst?", "tokens": ["Der", "Mensch", ",", "was", "ist", "er", ",", "da\u00df", "du", "sein", "ge\u00b7denkst", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWS", "VAFIN", "PPER", "$,", "KOUS", "PPER", "VAINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Anbetung dir und Preis und heisser Dank!", "tokens": ["An\u00b7be\u00b7tung", "dir", "und", "Preis", "und", "heis\u00b7ser", "Dank", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Im Tempel deiner herlichen Natur", "tokens": ["Im", "Tem\u00b7pel", "dei\u00b7ner", "her\u00b7li\u00b7chen", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Steigt mein Gebet, o Weltgeist, stillvereint", "tokens": ["Steigt", "mein", "Ge\u00b7bet", ",", "o", "Welt\u00b7geist", ",", "still\u00b7ver\u00b7eint"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "FM", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit dieser Wiesenblumen Opferduft,", "tokens": ["Mit", "die\u00b7ser", "Wie\u00b7sen\u00b7blu\u00b7men", "Op\u00b7fer\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zu dir, zu dir aus trunkner Seel' empor!", "tokens": ["Zu", "dir", ",", "zu", "dir", "aus", "trunk\u00b7ner", "Seel'", "em\u00b7por", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}