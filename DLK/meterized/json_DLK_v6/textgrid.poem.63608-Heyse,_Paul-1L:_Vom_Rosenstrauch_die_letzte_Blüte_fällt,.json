{"textgrid.poem.63608": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vom Rosenstrauch die letzte Bl\u00fcte f\u00e4llt,", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Rosenstrauch die letzte Bl\u00fcte f\u00e4llt,", "tokens": ["Vom", "Ro\u00b7sen\u00b7strauch", "die", "letz\u00b7te", "Bl\u00fc\u00b7te", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein b\u00f6ser Herbstwind schauert durch die Welt.", "tokens": ["Ein", "b\u00f6\u00b7ser", "Herbst\u00b7wind", "schau\u00b7ert", "durch", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wir pflegten Winters dies und das zu tun,", "tokens": ["Wir", "pfleg\u00b7ten", "Win\u00b7ters", "dies", "und", "das", "zu", "tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PDS", "KON", "PDS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das ward so m\u00fc\u00dfig, so entbehrlich nun!", "tokens": ["Das", "ward", "so", "m\u00fc\u00b7\u00dfig", ",", "so", "ent\u00b7behr\u00b7lich", "nun", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zu hoffen, harren, sorgen, uns zu freun \u2013", "tokens": ["Zu", "hof\u00b7fen", ",", "har\u00b7ren", ",", "sor\u00b7gen", ",", "uns", "zu", "freun", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "VVFIN", "$,", "NN", "$,", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das soll nun alles nimmer sich erneun.", "tokens": ["Das", "soll", "nun", "al\u00b7les", "nim\u00b7mer", "sich", "er\u00b7neun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "PIS", "ADV", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nicht sehn wir mehr der kleinen F\u00fc\u00dfe Spur", "tokens": ["Nicht", "sehn", "wir", "mehr", "der", "klei\u00b7nen", "F\u00fc\u00b7\u00dfe", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Leicht eingedr\u00fcckt der \u00fcberschneiten Flur.", "tokens": ["Leicht", "ein\u00b7ge\u00b7dr\u00fcckt", "der", "\u00fc\u00b7bersc\u00b7hnei\u00b7ten", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Nicht bei der fr\u00fchen Lampe goldnem Licht", "tokens": ["Nicht", "bei", "der", "fr\u00fc\u00b7hen", "Lam\u00b7pe", "gold\u00b7nem", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gl\u00fcht horchend auf ein kleines Angesicht.", "tokens": ["Gl\u00fcht", "hor\u00b7chend", "auf", "ein", "klei\u00b7nes", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Uns bringt der Winter nur mit Sturm und Graus", "tokens": ["Uns", "bringt", "der", "Win\u00b7ter", "nur", "mit", "Sturm", "und", "Graus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Melancholie ins ausgestorbne Haus.", "tokens": ["Me\u00b7lan\u00b7cho\u00b7lie", "ins", "aus\u00b7ge\u00b7storb\u00b7ne", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Das kl\u00fcgste w\u00e4r', sich einzuspinnen sacht,", "tokens": ["Das", "kl\u00fcgs\u00b7te", "w\u00e4r'", ",", "sich", "ein\u00b7zu\u00b7spin\u00b7nen", "sacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "$,", "PRF", "VVIZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie es zum Winterschlaf die Raupe macht.", "tokens": ["Wie", "es", "zum", "Win\u00b7ter\u00b7schlaf", "die", "Rau\u00b7pe", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Doch da ein Mensch soll wacker sein und wach,", "tokens": ["Doch", "da", "ein", "Mensch", "soll", "wa\u00b7cker", "sein", "und", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VMFIN", "ADJD", "VAINF", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Komm! fliehn wir sommerw\u00e4rts den Schwalben nach!", "tokens": ["Komm", "!", "fliehn", "wir", "som\u00b7mer\u00b7w\u00e4rts", "den", "Schwal\u00b7ben", "nach", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Vielleicht da\u00df zweier Wandrer tiefverarmt", "tokens": ["Viel\u00b7leicht", "da\u00df", "zwei\u00b7er", "Wand\u00b7rer", "tief\u00b7ver\u00b7armt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Bettlerfreundin Sonne sich erbarmt.", "tokens": ["Die", "Bett\u00b7ler\u00b7freun\u00b7din", "Son\u00b7ne", "sich", "er\u00b7barmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}