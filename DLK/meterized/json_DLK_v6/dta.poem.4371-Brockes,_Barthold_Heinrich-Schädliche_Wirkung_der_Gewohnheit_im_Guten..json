{"dta.poem.4371": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Sch\u00e4dliche Wirkung der Gewohnheit  \n im Guten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So lange die Gewohnheit uns unf\u00fchlbar macht, so", "tokens": ["So", "lan\u00b7ge", "die", "Ge\u00b7wohn\u00b7heit", "uns", "un\u00b7f\u00fchl\u00b7bar", "macht", ",", "so"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wie sie thut,", "tokens": ["wie", "sie", "thut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "So scheints, als ob, wenn wir verlangen ein uns noch", "tokens": ["So", "scheints", ",", "als", "ob", ",", "wenn", "wir", "ver\u00b7lan\u00b7gen", "ein", "uns", "noch"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KOKOM", "KOUS", "$,", "KOUS", "PPER", "VVFIN", "ART", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "nicht geh\u00f6rig Guht,", "tokens": ["nicht", "ge\u00b7h\u00f6\u00b7rig", "Guht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wir anders keinen Endzweck h\u00e4tten mit den von uns", "tokens": ["Wir", "an\u00b7ders", "kei\u00b7nen", "End\u00b7zweck", "h\u00e4t\u00b7ten", "mit", "den", "von", "uns"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "PIAT", "NN", "VAFIN", "APPR", "ART", "APPR", "PPER"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.6": {"text": "verlangten Gaben,", "tokens": ["ver\u00b7lang\u00b7ten", "Ga\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Als so, eh wir dieselben h\u00e4tten auch im Besitz, sie nicht", "tokens": ["Als", "so", ",", "eh", "wir", "die\u00b7sel\u00b7ben", "h\u00e4t\u00b7ten", "auch", "im", "Be\u00b7sitz", ",", "sie", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADV", "$,", "KOUS", "PPER", "PDS", "VAFIN", "ADV", "APPRART", "NN", "$,", "PPER", "PTKNEG"], "meter": "--+--+-+--+-+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "zu haben.", "tokens": ["zu", "ha\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VAINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}