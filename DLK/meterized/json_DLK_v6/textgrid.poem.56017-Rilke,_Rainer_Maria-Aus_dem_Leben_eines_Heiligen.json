{"textgrid.poem.56017": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Aus dem Leben eines Heiligen", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er kannte \u00c4ngste, deren Eingang schon", "tokens": ["Er", "kann\u00b7te", "\u00c4ngs\u00b7te", ",", "de\u00b7ren", "Ein\u00b7gang", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "PRELAT", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "wie Sterben war und nicht zu \u00fcberstehen.", "tokens": ["wie", "Ster\u00b7ben", "war", "und", "nicht", "zu", "\u00fc\u00b7bers\u00b7te\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VAFIN", "KON", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sein Herz erlernte, langsam durchzugehen;", "tokens": ["Sein", "Herz", "er\u00b7lern\u00b7te", ",", "lang\u00b7sam", "durch\u00b7zu\u00b7ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "er zog es gro\u00df wie einen Sohn.", "tokens": ["er", "zog", "es", "gro\u00df", "wie", "ei\u00b7nen", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und namenlose N\u00f6te kannte er,", "tokens": ["Und", "na\u00b7men\u00b7lo\u00b7se", "N\u00f6\u00b7te", "kann\u00b7te", "er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "finster und ohne Morgen wie Verschl\u00e4ge;", "tokens": ["fins\u00b7ter", "und", "oh\u00b7ne", "Mor\u00b7gen", "wie", "Ver\u00b7schl\u00e4\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "APPR", "NN", "KOKOM", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "und seine Seele gab er folgsam her,", "tokens": ["und", "sei\u00b7ne", "See\u00b7le", "gab", "er", "folg\u00b7sam", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "da sie erwachsen war, auf da\u00df sie l\u00e4ge", "tokens": ["da", "sie", "er\u00b7wach\u00b7sen", "war", ",", "auf", "da\u00df", "sie", "l\u00e4\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,", "APPR", "KOUS", "PPER", "VVFIN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "bei ihrem Br\u00e4utigam und Herrn; und blieb", "tokens": ["bei", "ih\u00b7rem", "Br\u00e4u\u00b7ti\u00b7gam", "und", "Herrn", ";", "und", "blieb"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$.", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "allein zur\u00fcck an einem solchen Orte,", "tokens": ["al\u00b7lein", "zu\u00b7r\u00fcck", "an", "ei\u00b7nem", "sol\u00b7chen", "Or\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "APPR", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "wo das Alleinsein alles \u00fcbertrieb,", "tokens": ["wo", "das", "Al\u00b7lein\u00b7sein", "al\u00b7les", "\u00fc\u00b7bert\u00b7rieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und wohnte weit und wollte niemals Worte.", "tokens": ["und", "wohn\u00b7te", "weit", "und", "woll\u00b7te", "nie\u00b7mals", "Wor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "VMFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Aber daf\u00fcr, nach Zeit und Zeit, erfuhr", "tokens": ["A\u00b7ber", "da\u00b7f\u00fcr", ",", "nach", "Zeit", "und", "Zeit", ",", "er\u00b7fuhr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PAV", "$,", "APPR", "NN", "KON", "NN", "$,", "VVFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "er auch das Gl\u00fcck, sich in die eignen H\u00e4nde,", "tokens": ["er", "auch", "das", "Gl\u00fcck", ",", "sich", "in", "die", "eig\u00b7nen", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "$,", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "damit er eine Z\u00e4rtlichkeit empf\u00e4nde,", "tokens": ["da\u00b7mit", "er", "ei\u00b7ne", "Z\u00e4rt\u00b7lich\u00b7keit", "em\u00b7pf\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "zu legen wie die ganze Kreatur.", "tokens": ["zu", "le\u00b7gen", "wie", "die", "gan\u00b7ze", "Kre\u00b7a\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}