{"textgrid.poem.45418": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[es steht nicht gut in unsrer lauten Welt]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es steht nicht gut in unsrer lauten Welt.", "tokens": ["Es", "steht", "nicht", "gut", "in", "uns\u00b7rer", "lau\u00b7ten", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Kr\u00e4fte, die im Ebenma\u00df verteilt,", "tokens": ["Die", "Kr\u00e4f\u00b7te", ",", "die", "im", "E\u00b7ben\u00b7ma\u00df", "ver\u00b7teilt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Ganze hielten streng im Gleichgewicht,", "tokens": ["Das", "Gan\u00b7ze", "hiel\u00b7ten", "streng", "im", "Gleich\u00b7ge\u00b7wicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie dr\u00e4ngen sich gesamt nach einem Punkt,", "tokens": ["Sie", "dr\u00e4n\u00b7gen", "sich", "ge\u00b7samt", "nach", "ei\u00b7nem", "Punkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der umschl\u00e4gt, \u00fcberf\u00fcllt, sieht man nicht vor.", "tokens": ["Der", "um\u00b7schl\u00e4gt", ",", "\u00fc\u00b7berf\u00b7\u00fcllt", ",", "sieht", "man", "nicht", "vor", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "PIS", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die Zeichen sind betr\u00fcbt. Wer angewohnt,", "tokens": ["Die", "Zei\u00b7chen", "sind", "be\u00b7tr\u00fcbt", ".", "Wer", "an\u00b7ge\u00b7wohnt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$.", "PWS", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nur schwarz zu sehn, verzweifelte wohl gar", "tokens": ["Nur", "schwarz", "zu", "sehn", ",", "ver\u00b7zwei\u00b7fel\u00b7te", "wohl", "gar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "$,", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und w\u00e4hnte sich am Rand der letzten Dinge.", "tokens": ["Und", "w\u00e4hn\u00b7te", "sich", "am", "Rand", "der", "letz\u00b7ten", "Din\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Doch ist ein Punkt, der Hoffnung gab und gibt:", "tokens": ["Doch", "ist", "ein", "Punkt", ",", "der", "Hoff\u00b7nung", "gab", "und", "gibt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der Mensch, er ist nicht schlimm. Denn w\u00e4r er es,", "tokens": ["Der", "Mensch", ",", "er", "ist", "nicht", "schlimm", ".", "Denn", "w\u00e4r", "er", "es", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "$.", "KON", "VAFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Dieselbe Welt best\u00fcnde nicht bis heut", "tokens": ["Die\u00b7sel\u00b7be", "Welt", "be\u00b7st\u00fcn\u00b7de", "nicht", "bis", "heut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "VVFIN", "PTKNEG", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und Untreu und Verrat und jedes Unrecht,", "tokens": ["Und", "Un\u00b7treu", "und", "Ver\u00b7rat", "und", "je\u00b7des", "Un\u00b7recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Sie h\u00e4tten l\u00e4ngst zerst\u00f6rt das sch\u00f6ne Ganze,", "tokens": ["Sie", "h\u00e4t\u00b7ten", "l\u00e4ngst", "zer\u00b7st\u00f6rt", "das", "sch\u00f6\u00b7ne", "Gan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Das, als bestanden, b\u00fcrgt f\u00fcr sein Bestehn.", "tokens": ["Das", ",", "als", "be\u00b7stan\u00b7den", ",", "b\u00fcrgt", "f\u00fcr", "sein", "Be\u00b7stehn", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "ADJA", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Der Mensch, er ist nicht schlimm", "tokens": ["Der", "Mensch", ",", "er", "ist", "nicht", "schlimm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}