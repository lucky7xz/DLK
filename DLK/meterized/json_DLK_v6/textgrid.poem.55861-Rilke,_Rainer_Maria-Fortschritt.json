{"textgrid.poem.55861": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Fortschritt", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wieder rauscht mein tiefes Leben lauter,", "tokens": ["Und", "wie\u00b7der", "rauscht", "mein", "tie\u00b7fes", "Le\u00b7ben", "lau\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "als ob es jetzt in breitern Ufern ginge.", "tokens": ["als", "ob", "es", "jetzt", "in", "brei\u00b7tern", "U\u00b7fern", "gin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Immer verwandter werden mir die Dinge", "tokens": ["Im\u00b7mer", "ver\u00b7wand\u00b7ter", "wer\u00b7den", "mir", "die", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "und alle Bilder immer angeschauter.", "tokens": ["und", "al\u00b7le", "Bil\u00b7der", "im\u00b7mer", "an\u00b7ge\u00b7schau\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dem Namenlosen f\u00fchl ich mich vertrauter:", "tokens": ["Dem", "Na\u00b7men\u00b7lo\u00b7sen", "f\u00fchl", "ich", "mich", "ver\u00b7trau\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit meinen Sinnen, wie mit V\u00f6geln, reiche", "tokens": ["Mit", "mei\u00b7nen", "Sin\u00b7nen", ",", "wie", "mit", "V\u00f6\u00b7geln", ",", "rei\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PWAV", "APPR", "NN", "$,", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "ich in die windigen Himmel aus der Eiche,", "tokens": ["ich", "in", "die", "win\u00b7di\u00b7gen", "Him\u00b7mel", "aus", "der", "Ei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "und in den abgebrochnen Tag der Teiche", "tokens": ["und", "in", "den", "ab\u00b7ge\u00b7broch\u00b7nen", "Tag", "der", "Tei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "sinkt, wie auf Fischen stehend, mein Gef\u00fchl.", "tokens": ["sinkt", ",", "wie", "auf", "Fi\u00b7schen", "ste\u00b7hend", ",", "mein", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "APPR", "NN", "VVPP", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}