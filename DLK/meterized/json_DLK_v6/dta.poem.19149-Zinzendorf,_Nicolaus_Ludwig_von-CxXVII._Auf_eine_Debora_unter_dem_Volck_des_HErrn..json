{"dta.poem.19149": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "CxXVII.  Auf eine Debora unter dem Volck  \n des HErrn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Du Oel Kind h\u00f6r, ich preise meine Liebe", "tokens": ["Du", "O\u00b7el", "Kind", "h\u00f6r", ",", "ich", "prei\u00b7se", "mei\u00b7ne", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Die meine, deine und der Br\u00fcderschafft,", "tokens": ["Die", "mei\u00b7ne", ",", "dei\u00b7ne", "und", "der", "Br\u00fc\u00b7der\u00b7schafft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PPOSAT", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vor ihre an dich angewandten Triebe,", "tokens": ["Vor", "ih\u00b7re", "an", "dich", "an\u00b7ge\u00b7wand\u00b7ten", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "APPR", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor ihren Blick der Gnade und der Krafft,", "tokens": ["Vor", "ih\u00b7ren", "Blick", "der", "Gna\u00b7de", "und", "der", "Krafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Gnade macht dich weinen,", "tokens": ["Die", "Gna\u00b7de", "macht", "dich", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Kr\u00e4ffte gl\u00fchn und scheinen,", "tokens": ["Die", "Kr\u00e4ff\u00b7te", "gl\u00fchn", "und", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nun kommt das Oel dazu,", "tokens": ["Nun", "kommt", "das", "O\u00b7el", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PAV", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Und will dein Ammt mit JEsu Christi seinen,", "tokens": ["Und", "will", "dein", "Ammt", "mit", "Je\u00b7su", "Chris\u00b7ti", "sei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "APPR", "NE", "NE", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Vereinigen, damit es Wunder thu.", "tokens": ["Ver\u00b7ei\u00b7ni\u00b7gen", ",", "da\u00b7mit", "es", "Wun\u00b7der", "thu", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Heer-F\u00fcrst ist dein Mann, du Tochter GOttes,", "tokens": ["Der", "Heer\u00b7F\u00fcrst", "ist", "dein", "Mann", ",", "du", "Toch\u00b7ter", "Got\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,", "PPER", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir haben billig Lieb und Furcht vor dich,", "tokens": ["Wir", "ha\u00b7ben", "bil\u00b7lig", "Lieb", "und", "Furcht", "vor", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "NN", "KON", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Streiter tragen seinen Theil des Spottes,", "tokens": ["Die", "Strei\u00b7ter", "tra\u00b7gen", "sei\u00b7nen", "Theil", "des", "Spot\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Braut zeigt ihren Blut- und Salbung-Strich,", "tokens": ["Die", "Braut", "zeigt", "ih\u00b7ren", "Blut", "und", "Sal\u00b7bung\u00b7Strich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "TRUNC", "KON", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Freund, der ihr gewogen,", "tokens": ["Der", "Freund", ",", "der", "ihr", "ge\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hat ihr den Strich gezogen,", "tokens": ["Hat", "ihr", "den", "Strich", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Indem er sie umfa\u00dft,", "tokens": ["In\u00b7dem", "er", "sie", "um\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Indem sie an der Mutter-Brust gesogen,", "tokens": ["In\u00b7dem", "sie", "an", "der", "Mut\u00b7ter\u00b7Brust", "ge\u00b7so\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "In dem ihr Hertz und Seines eingepa\u00dft.", "tokens": ["In", "dem", "ihr", "Hertz", "und", "Sei\u00b7nes", "ein\u00b7ge\u00b7pa\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "KON", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was soll ich dir zu deinem Tage sagen,", "tokens": ["Was", "soll", "ich", "dir", "zu", "dei\u00b7nem", "Ta\u00b7ge", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es wird mir schwer, die Worte fehlen mir,", "tokens": ["Es", "wird", "mir", "schwer", ",", "die", "Wor\u00b7te", "feh\u00b7len", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn erstlich wei\u00df ichs nicht so vorzutragen,", "tokens": ["Denn", "erst\u00b7lich", "wei\u00df", "ichs", "nicht", "so", "vor\u00b7zu\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PIS", "PTKNEG", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als ichs in meinem Hertzen drinne sp\u00fcr.", "tokens": ["Als", "ichs", "in", "mei\u00b7nem", "Hert\u00b7zen", "drin\u00b7ne", "sp\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zum andern, o du Dirne", "tokens": ["Zum", "an\u00b7dern", ",", "o", "du", "Dir\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "$,", "FM", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit der gesalbten Stirne!", "tokens": ["Mit", "der", "ge\u00b7salb\u00b7ten", "Stir\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wir sind bey nahe eins.", "tokens": ["Wir", "sind", "bey", "na\u00b7he", "eins", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJD", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Wir wachen beyde \u00fcber Zions Th\u00fcrne,", "tokens": ["Wir", "wa\u00b7chen", "bey\u00b7de", "\u00fc\u00b7ber", "Zi\u00b7ons", "Th\u00fcr\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Bald f\u00fchrest du mein Amt, bald f\u00fchr ich deins.", "tokens": ["Bald", "f\u00fch\u00b7rest", "du", "mein", "Amt", ",", "bald", "f\u00fchr", "ich", "deins", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nach dieser Pflicht und anbefohlnen Gnade,", "tokens": ["Nach", "die\u00b7ser", "Pflicht", "und", "an\u00b7be\u00b7fohl\u00b7nen", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geliebte Schwester, so ermahn ich dich,", "tokens": ["Ge\u00b7lieb\u00b7te", "Schwes\u00b7ter", ",", "so", "er\u00b7mahn", "ich", "dich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADV", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu wandeln auf dem gleichgemachten Pfade,", "tokens": ["Zu", "wan\u00b7deln", "auf", "dem", "gleich\u00b7ge\u00b7mach\u00b7ten", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem Haupt und der Gemeine w\u00fcrdiglich,", "tokens": ["Dem", "Haupt", "und", "der", "Ge\u00b7mei\u00b7ne", "w\u00fcr\u00b7dig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und keine Krafft zu spahren,", "tokens": ["Und", "kei\u00b7ne", "Krafft", "zu", "spah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ob du gleich nach den Jahren", "tokens": ["Ob", "du", "gleich", "nach", "den", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Kaum J\u00fcnglings-m\u00e4\u00dfig bist,", "tokens": ["Kaum", "J\u00fcng\u00b7lings\u00b7m\u00e4\u00b7\u00dfig", "bist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Da du gebohren warsdt ", "tokens": ["Da", "du", "ge\u00b7boh\u00b7ren", "warsdt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Was Fleisch und Blut des Menschen Sohnes ist.", "tokens": ["Was", "Fleisch", "und", "Blut", "des", "Men\u00b7schen", "Soh\u00b7nes", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Du k\u00f6ntest billig mehr von mir begehren,", "tokens": ["Du", "k\u00f6n\u00b7test", "bil\u00b7lig", "mehr", "von", "mir", "be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als du bi\u00df jetzo noch an mir gesehn,", "tokens": ["Als", "du", "bi\u00df", "jet\u00b7zo", "noch", "an", "mir", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Weil meine Tage schon so lange w\u00e4hren,", "tokens": ["Weil", "mei\u00b7ne", "Ta\u00b7ge", "schon", "so", "lan\u00b7ge", "w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mir so manche Gnade schon geschehn,", "tokens": ["Und", "mir", "so", "man\u00b7che", "Gna\u00b7de", "schon", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch der mein Hertze kennet,", "tokens": ["Doch", "der", "mein", "Hert\u00b7ze", "ken\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPOSAT", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und mich zum Knecht ernennet,", "tokens": ["Und", "mich", "zum", "Knecht", "er\u00b7nen\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der wei\u00df wie schwer es geht,", "tokens": ["Der", "wei\u00df", "wie", "schwer", "es", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und wie hingegen er mit dir gerennet,", "tokens": ["Und", "wie", "hin\u00b7ge\u00b7gen", "er", "mit", "dir", "ge\u00b7ren\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und dich schon lange an sein Creutz erh\u00f6ht.", "tokens": ["Und", "dich", "schon", "lan\u00b7ge", "an", "sein", "Creutz", "er\u00b7h\u00f6ht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "So gehe denn in dieser selgen F\u00fchrung", "tokens": ["So", "ge\u00b7he", "denn", "in", "die\u00b7ser", "sel\u00b7gen", "F\u00fch\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das k\u00fcnfftge Jahr mit grossen Schritten fort,", "tokens": ["Das", "k\u00fcnfft\u00b7ge", "Jahr", "mit", "gros\u00b7sen", "Schrit\u00b7ten", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erfahre seine wesentliche R\u00fchrung,", "tokens": ["Er\u00b7fah\u00b7re", "sei\u00b7ne", "we\u00b7sent\u00b7li\u00b7che", "R\u00fch\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und blicke manchmahl nach dem Ruhe-Port,", "tokens": ["Und", "bli\u00b7cke", "manch\u00b7mahl", "nach", "dem", "Ru\u00b7he\u00b7Port", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch la\u00df es bey den Blicken,", "tokens": ["Doch", "la\u00df", "es", "bey", "den", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und flei\u00dfigem Beschicken", "tokens": ["Und", "flei\u00b7\u00dfi\u00b7gem", "Be\u00b7schi\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der oberen Gemein,", "tokens": ["Der", "o\u00b7be\u00b7ren", "Ge\u00b7mein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Du aber must dich nicht vom Ort verr\u00fccken,", "tokens": ["Du", "a\u00b7ber", "must", "dich", "nicht", "vom", "Ort", "ver\u00b7r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und froh, und arbeitsam, und innig seyn.", "tokens": ["Und", "froh", ",", "und", "ar\u00b7beit\u00b7sam", ",", "und", "in\u00b7nig", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KON", "ADJD", "$,", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wenn du einmahl wirst ausgew\u00fcrcket haben,", "tokens": ["Wenn", "du", "ein\u00b7mahl", "wirst", "aus\u00b7ge\u00b7w\u00fcr\u00b7cket", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So wirst du zeit genug im Schoo\u00dfe ruhn,", "tokens": ["So", "wirst", "du", "zeit", "ge\u00b7nug", "im", "Schoo\u00b7\u00dfe", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das ist der Zweck von unsern Zeugen-Gaben,", "tokens": ["Das", "ist", "der", "Zweck", "von", "un\u00b7sern", "Zeu\u00b7gen\u00b7Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df wir, weils Tag ist, etwas sollen thun,", "tokens": ["Da\u00df", "wir", ",", "weils", "Tag", "ist", ",", "et\u00b7was", "sol\u00b7len", "thun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "NN", "VAFIN", "$,", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nun bete du: ", "tokens": ["Nun", "be\u00b7te", "du", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Hier ist mein Mardachai", "tokens": ["Hier", "ist", "mein", "Mar\u00b7dac\u00b7hai"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und ich bin deine Magd,", "tokens": ["Und", "ich", "bin", "dei\u00b7ne", "Magd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Wir wollens machen wie dein Knecht vor Ai,", "tokens": ["Wir", "wol\u00b7lens", "ma\u00b7chen", "wie", "dein", "Knecht", "vor", "Ai", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "KOKOM", "PPOSAT", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Mach du es wie dein Knecht zu Gilgal sagt.", "tokens": ["Mach", "du", "es", "wie", "dein", "Knecht", "zu", "Gil\u00b7gal", "sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "KOKOM", "PPOSAT", "NN", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}