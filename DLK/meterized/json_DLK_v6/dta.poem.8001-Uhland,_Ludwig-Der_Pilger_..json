{"dta.poem.8001": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Pilger .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Es wallt ein Pilger hohen Dranges,", "tokens": ["Es", "wallt", "ein", "Pil\u00b7ger", "ho\u00b7hen", "Dran\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er wallt zur sel\u2019gen Gottesstadt,", "tokens": ["Er", "wallt", "zur", "sel'\u00b7gen", "Got\u00b7tes\u00b7stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zur Stadt des himmlischen Gesanges,", "tokens": ["Zur", "Stadt", "des", "himm\u00b7li\u00b7schen", "Ge\u00b7san\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die ihm der Geist verhei\u00dfen hat.", "tokens": ["Die", "ihm", "der", "Geist", "ver\u00b7hei\u00b7\u00dfen", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u201edu klarer Strom! in deinem Spiegel", "tokens": ["\u201e", "du", "kla\u00b7rer", "Strom", "!", "in", "dei\u00b7nem", "Spie\u00b7gel"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPER", "ADJA", "NN", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wirst du die heil\u2019ge bald umfahn.", "tokens": ["Wirst", "du", "die", "heil'\u00b7ge", "bald", "um\u00b7fahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr sonnehellen Felsenh\u00fcgel!", "tokens": ["Ihr", "son\u00b7ne\u00b7hel\u00b7len", "Fel\u00b7sen\u00b7h\u00fc\u00b7gel", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr schaut sie schon von Weitem an.", "tokens": ["Ihr", "schaut", "sie", "schon", "von", "Wei\u00b7tem", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie ferne Glocken h\u00f6r\u2019 ich\u2019s klingen,", "tokens": ["Wie", "fer\u00b7ne", "Glo\u00b7cken", "h\u00f6r'", "ich's", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Abendroth durchbl\u00fcht den Hain.", "tokens": ["Das", "A\u00b7ben\u00b7droth", "durch\u00b7bl\u00fcht", "den", "Hain", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O h\u00e4tt\u2019 ich Fl\u00fcgel, mich zu schwingen", "tokens": ["O", "h\u00e4tt'", "ich", "Fl\u00fc\u00b7gel", ",", "mich", "zu", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "NN", "$,", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weit \u00fcber Thal und Felsenreihn!\u201c", "tokens": ["Weit", "\u00fc\u00b7ber", "Thal", "und", "Fel\u00b7sen\u00b7reihn", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er ist von hoher Wonne trunken,", "tokens": ["Er", "ist", "von", "ho\u00b7her", "Won\u00b7ne", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er ist von s\u00fc\u00dfen Schmerzen matt,", "tokens": ["Er", "ist", "von", "s\u00fc\u00b7\u00dfen", "Schmer\u00b7zen", "matt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, in die Blumen hingesunken,", "tokens": ["Und", ",", "in", "die", "Blu\u00b7men", "hin\u00b7ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gedenkt er seiner Gottesstadt.", "tokens": ["Ge\u00b7denkt", "er", "sei\u00b7ner", "Got\u00b7tes\u00b7stadt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u201esie sind zu gro\u00df noch, diese R\u00e4ume,", "tokens": ["\u201e", "sie", "sind", "zu", "gro\u00df", "noch", ",", "die\u00b7se", "R\u00e4u\u00b7me", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PTKA", "ADJD", "ADV", "$,", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr meiner Sehnsucht Flammenqual;", "tokens": ["F\u00fcr", "mei\u00b7ner", "Sehn\u00b7sucht", "Flam\u00b7men\u00b7qual", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Empfahet ihr mich, milde Tr\u00e4ume,", "tokens": ["E\u00b7mpfa\u00b7het", "ihr", "mich", ",", "mil\u00b7de", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und zeigt mir das ersehnte Thal!\u201c", "tokens": ["Und", "zeigt", "mir", "das", "er\u00b7sehn\u00b7te", "Thal", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Da ist der Himmel aufgeschlagen,", "tokens": ["Da", "ist", "der", "Him\u00b7mel", "auf\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sein lichter Engel schaut herab:", "tokens": ["Sein", "lich\u00b7ter", "En\u00b7gel", "schaut", "her\u00b7ab", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201ewie sollt\u2019 ich dir die Kraft versagen,", "tokens": ["\u201e", "wie", "sollt'", "ich", "dir", "die", "Kraft", "ver\u00b7sa\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VMFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem ich das hohe Sehnen gab!", "tokens": ["Dem", "ich", "das", "ho\u00b7he", "Seh\u00b7nen", "gab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die Sehnsucht und der Tr\u00e4ume Weben,", "tokens": ["Die", "Sehn\u00b7sucht", "und", "der", "Tr\u00e4u\u00b7me", "We\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie sind der weichen Seele s\u00fc\u00df,", "tokens": ["Sie", "sind", "der", "wei\u00b7chen", "See\u00b7le", "s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch edler ist ein starkes Streben", "tokens": ["Doch", "ed\u00b7ler", "ist", "ein", "star\u00b7kes", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und macht den sch\u00f6nen Traum gewi\u00df.\u201c", "tokens": ["Und", "macht", "den", "sch\u00f6\u00b7nen", "Traum", "ge\u00b7wi\u00df", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Er schwindet in die Morgend\u00fcfte;", "tokens": ["Er", "schwin\u00b7det", "in", "die", "Mor\u00b7gen\u00b7d\u00fcf\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Pilger springt gest\u00e4rkt empor,", "tokens": ["Der", "Pil\u00b7ger", "springt", "ge\u00b7st\u00e4rkt", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er strebet \u00fcber Berg\u2019 und Kl\u00fcfte,", "tokens": ["Er", "stre\u00b7bet", "\u00fc\u00b7ber", "Ber\u00b7g'", "und", "Kl\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er stehet schon am goldnen Thor.", "tokens": ["Er", "ste\u00b7het", "schon", "am", "gold\u00b7nen", "Thor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und sieh! gleich Mutterarmen schlie\u00dfet", "tokens": ["Und", "sieh", "!", "gleich", "Mut\u00b7ter\u00b7ar\u00b7men", "schlie\u00b7\u00dfet"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "$.", "ADV", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Stadt der Pforte Fl\u00fcgel auf;", "tokens": ["Die", "Stadt", "der", "Pfor\u00b7te", "Fl\u00fc\u00b7gel", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr himmlischer Gesang begr\u00fc\u00dfet", "tokens": ["Ihr", "himm\u00b7li\u00b7scher", "Ge\u00b7sang", "be\u00b7gr\u00fc\u00b7\u00dfet"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Sohn nach tapfrem Pilgerlauf.", "tokens": ["Den", "Sohn", "nach", "tapf\u00b7rem", "Pil\u00b7ger\u00b7lauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}