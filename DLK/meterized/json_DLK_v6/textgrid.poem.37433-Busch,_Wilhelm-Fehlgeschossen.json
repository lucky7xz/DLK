{"textgrid.poem.37433": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Fehlgeschossen", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fritz war ein kecker Junge", "tokens": ["Fritz", "war", "ein", "ke\u00b7cker", "Jun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und sehr gel\u00e4ufig mit der Zunge.", "tokens": ["Und", "sehr", "ge\u00b7l\u00e4u\u00b7fig", "mit", "der", "Zun\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Einstmals ist er beim \u00c4hrenlesen", "tokens": ["Einst\u00b7mals", "ist", "er", "beim", "\u00c4h\u00b7ren\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Drau\u00dfen im Felde gewesen,", "tokens": ["Drau\u00b7\u00dfen", "im", "Fel\u00b7de", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Wo die Weizengarben, je zu zehn,", "tokens": ["Wo", "die", "Wei\u00b7zen\u00b7gar\u00b7ben", ",", "je", "zu", "zehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ADV", "APPR", "CARD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Wie H\u00e4uslein in der Reihe stehn.", "tokens": ["Wie", "H\u00e4us\u00b7lein", "in", "der", "Rei\u00b7he", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Wetter zog herauf.", "tokens": ["Ein", "Wet\u00b7ter", "zog", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Da hei\u00dft es: Lauf!", "tokens": ["Da", "hei\u00dft", "es", ":", "Lauf", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Und flink, wie ein M\u00e4uslein", "tokens": ["Und", "flink", ",", "wie", "ein", "M\u00e4us\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ART", "NN"], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.10": {"text": "Schl\u00fcpft er ins n\u00e4chste Halmenh\u00e4uslein.", "tokens": ["Schl\u00fcpft", "er", "ins", "n\u00e4chs\u00b7te", "Hal\u00b7men\u00b7h\u00e4us\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Krach! \u2013 Potztausendnochmal!", "tokens": ["Krach", "!", "\u2013", "Potz\u00b7tau\u00b7send\u00b7noch\u00b7mal", "!"], "token_info": ["word", "punct", "punct", "word", "punct"], "pos": ["NN", "$.", "$(", "NE", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.12": {"text": "Dicht daneben z\u00fcndet der Wetterstrahl.", "tokens": ["Dicht", "da\u00b7ne\u00b7ben", "z\u00fcn\u00b7det", "der", "Wet\u00b7ter\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PAV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "\u00c4tsch! rief der Junge, der nicht bange,", "tokens": ["\u00c4tsch", "!", "rief", "der", "Jun\u00b7ge", ",", "der", "nicht", "ban\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "VVFIN", "ART", "NN", "$,", "PRELS", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und streckt die Zunge aus, die lange:", "tokens": ["Und", "streckt", "die", "Zun\u00b7ge", "aus", ",", "die", "lan\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Fehlgeschossen, Herr Blitz!", "tokens": ["Fehl\u00b7ge\u00b7schos\u00b7sen", ",", "Herr", "Blitz", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.16": {"text": "Hier sa\u00df der Fritz!", "tokens": ["Hier", "sa\u00df", "der", "Fritz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}