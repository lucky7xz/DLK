{"textgrid.poem.43119": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "Gefallene Engel", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist die alte finst're M\u00e4hr", "tokens": ["Es", "ist", "die", "al\u00b7te", "finst'\u00b7re", "M\u00e4hr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von zwei Vermaledeiten,", "tokens": ["Von", "zwei", "Ver\u00b7ma\u00b7le\u00b7dei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die ohne Rast und ohne Ruh", "tokens": ["Die", "oh\u00b7ne", "Rast", "und", "oh\u00b7ne", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fort durch die H\u00f6lle schreiten.", "tokens": ["Fort", "durch", "die", "H\u00f6l\u00b7le", "schrei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Von Zweien, die voll Hochmuth einst", "tokens": ["Von", "Zwei\u00b7en", ",", "die", "voll", "Hoch\u00b7muth", "einst"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "ADJD", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verschm\u00e4ht des Himmels Frieden,", "tokens": ["Ver\u00b7schm\u00e4ht", "des", "Him\u00b7mels", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und eine Seligkeit hindurch", "tokens": ["Und", "ei\u00b7ne", "Se\u00b7lig\u00b7keit", "hin\u00b7durch"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sich fremd und stolz gemieden;", "tokens": ["Sich", "fremd", "und", "stolz", "ge\u00b7mie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Von zwei Vermaledeiten, die", "tokens": ["Von", "zwei", "Ver\u00b7ma\u00b7le\u00b7dei\u00b7ten", ",", "die"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "CARD", "NN", "$,", "PRELS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So fern nun allem Reinen,", "tokens": ["So", "fern", "nun", "al\u00b7lem", "Rei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "PIS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Sich suchen, finden, halten, ach!", "tokens": ["Sich", "su\u00b7chen", ",", "fin\u00b7den", ",", "hal\u00b7ten", ",", "ach", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PRF", "VVINF", "$,", "VVINF", "$,", "VVFIN", "$,", "ITJ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und weinen \u2013 weinen \u2013 weinen!", "tokens": ["Und", "wei\u00b7nen", "\u2013", "wei\u00b7nen", "\u2013", "wei\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVINF", "$(", "VVINF", "$(", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}