{"textgrid.poem.26541": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das herbe Loos der Armen und der Schwachen,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das herbe Loos der Armen und der Schwachen,", "tokens": ["Das", "her\u00b7be", "Loos", "der", "Ar\u00b7men", "und", "der", "Schwa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es war auch ", "tokens": ["Es", "war", "auch"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Der eig'nen Meinung mu\u00dft' er sich entschlagen", "tokens": ["Der", "eig'\u00b7nen", "Mei\u00b7nung", "mu\u00dft'", "er", "sich", "ent\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und l\u00fcgen, wie gedruckt, in allen Sprachen.", "tokens": ["Und", "l\u00fc\u00b7gen", ",", "wie", "ge\u00b7druckt", ",", "in", "al\u00b7len", "Spra\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "VVPP", "$,", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dem Mammon mu\u00dft' er seinen B\u00fcckling machen,", "tokens": ["Dem", "Mam\u00b7mon", "mu\u00dft'", "er", "sei\u00b7nen", "B\u00fcck\u00b7ling", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Hoffahrt l\u00e4chelnd helfen in den Wagen,", "tokens": ["Der", "Hof\u00b7fahrt", "l\u00e4\u00b7chelnd", "hel\u00b7fen", "in", "den", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verbuhlten Weibern seid'ne Schleppen tragen \u2013", "tokens": ["Ver\u00b7buhl\u00b7ten", "Wei\u00b7bern", "sei\u00b7d'\u00b7ne", "Schlep\u00b7pen", "tra\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und ernst sein, wenn es ihn gereizt zum Lachen.", "tokens": ["Und", "ernst", "sein", ",", "wenn", "es", "ihn", "ge\u00b7reizt", "zum", "La\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAINF", "$,", "KOUS", "PPER", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie oft, bei diesem oder jenem Feste,", "tokens": ["Wie", "oft", ",", "bei", "die\u00b7sem", "o\u00b7der", "je\u00b7nem", "Fes\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "APPR", "PDAT", "KON", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als laut der Kork sprang von der dunklen Flasche,", "tokens": ["Als", "laut", "der", "Kork", "sprang", "von", "der", "dunk\u00b7len", "Fla\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hat er getoastet, schmeichelnd auf das beste.", "tokens": ["Hat", "er", "ge\u00b7to\u00b7as\u00b7tet", ",", "schmei\u00b7chelnd", "auf", "das", "bes\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "ADJD", "APPR", "ART", "ADJA", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Er streute auf sein Haupt der Demuth Asche,", "tokens": ["Er", "streu\u00b7te", "auf", "sein", "Haupt", "der", "De\u00b7muth", "A\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Witzworte nahm er hin erlauchter G\u00e4ste \u2013", "tokens": ["Witz\u00b7wor\u00b7te", "nahm", "er", "hin", "er\u00b7lauch\u00b7ter", "G\u00e4s\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ballte selbst die Faust nicht in der Tasche.", "tokens": ["Und", "ball\u00b7te", "selbst", "die", "Faust", "nicht", "in", "der", "Ta\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}