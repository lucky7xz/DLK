{"textgrid.poem.31416": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Grund guter Gott / wer kan zuviel doch von dir denken?", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Grund guter Gott / wer kan zuviel doch von dir denken?", "tokens": ["Grund", "gu\u00b7ter", "Gott", "/", "wer", "kan", "zu\u00b7viel", "doch", "von", "dir", "den\u00b7ken", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$(", "PWS", "VMFIN", "PIS", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "hat mehr denn / als das Meer / ein tr\u00f6pflein Feuchtigkeit?", "tokens": ["hat", "mehr", "denn", "/", "als", "das", "Meer", "/", "ein", "tr\u00f6pf\u00b7lein", "Feuch\u00b7tig\u00b7keit", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "$(", "KOUS", "ART", "NN", "$(", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "hab' ich mehr treue Sorg / als der / der alls bereit?", "tokens": ["hab'", "ich", "mehr", "treu\u00b7e", "Sorg", "/", "als", "der", "/", "der", "alls", "be\u00b7reit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "ADJA", "NN", "$(", "KOUS", "ART", "$(", "ART", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach nein! dein Gnaden-Meer kan alles \u00fcberschwenken!", "tokens": ["Ach", "nein", "!", "dein", "Gna\u00b7den\u00b7Meer", "kan", "al\u00b7les", "\u00fc\u00b7bersc\u00b7hwen\u00b7ken", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$.", "PPOSAT", "NN", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "in dein' unendlichkeit / will ich den Wunsch versenken:", "tokens": ["in", "dein'", "un\u00b7end\u00b7lich\u00b7keit", "/", "will", "ich", "den", "Wunsch", "ver\u00b7sen\u00b7ken", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du ewig-w\u00fcrkends Wort! w\u00fcrk' auch in dieser Zeit /", "tokens": ["Du", "e\u00b7wig\u00b7w\u00fcr\u00b7kends", "Wort", "!", "w\u00fcrk'", "auch", "in", "die\u00b7ser", "Zeit", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "VAFIN", "ADV", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "und deiner G\u00fcte Ruhm in alle Welt ausbreit!", "tokens": ["und", "dei\u00b7ner", "G\u00fc\u00b7te", "Ruhm", "in", "al\u00b7le", "Welt", "aus\u00b7breit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "zeig / da\u00df du kanst mehr guts / als wir erdenken / schenken.", "tokens": ["zeig", "/", "da\u00df", "du", "kanst", "mehr", "guts", "/", "als", "wir", "er\u00b7den\u00b7ken", "/", "schen\u00b7ken", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$(", "KOUS", "PPER", "VMFIN", "PIAT", "NN", "$(", "KOUS", "PPER", "VVINF", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach la\u00df / ach la\u00df' einmal / die starke Zuversicht", "tokens": ["Ach", "la\u00df", "/", "ach", "la\u00df'", "ein\u00b7mal", "/", "die", "star\u00b7ke", "Zu\u00b7ver\u00b7sicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "$(", "XY", "VVIMP", "ADV", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "zu deiner hohen Gnad' / am b\u00f6sen Welt-End siegen!", "tokens": ["zu", "dei\u00b7ner", "ho\u00b7hen", "Gnad'", "/", "am", "b\u00f6\u00b7sen", "Welt\u00b7End", "sie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$(", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "la\u00df' / eh sie untergeht / auf flammen noch dein Liecht:", "tokens": ["la\u00df'", "/", "eh", "sie", "un\u00b7ter\u00b7geht", "/", "auf", "flam\u00b7men", "noch", "dein", "Liecht", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "KOUS", "PPER", "VVFIN", "$(", "APPR", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "da\u00df die Angst-M\u00fcden noch neu Adler Fl\u00fcgel kriegen.", "tokens": ["da\u00df", "die", "Angst\u00b7M\u00fc\u00b7den", "noch", "neu", "Ad\u00b7ler", "Fl\u00fc\u00b7gel", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "+--+--++-+-+-", "measure": "dactylic.di.plus"}, "line.13": {"text": "Schick' eine Morgenr\u00f6t / eh dein Sohns-Sonn' anbricht.", "tokens": ["Schick'", "ei\u00b7ne", "Mor\u00b7gen\u00b7r\u00f6t", "/", "eh", "dein", "Sohns\u00b7Sonn'", "an\u00b7bricht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$(", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sing' uns ein Schwanen Lied / eh wir im Z\u00fcgen ligen!", "tokens": ["Sing'", "uns", "ein", "Schwa\u00b7nen", "Lied", "/", "eh", "wir", "im", "Z\u00fc\u00b7gen", "li\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$(", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}