{"textgrid.poem.61997": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[nur allein kann ich erstarken]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur allein kann ich erstarken,", "tokens": ["Nur", "al\u00b7lein", "kann", "ich", "er\u00b7star\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur allein sprie\u00dft mir die Kraft,", "tokens": ["Nur", "al\u00b7lein", "sprie\u00dft", "mir", "die", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Tret' ich in des Kampfes Marken,", "tokens": ["Tret'", "ich", "in", "des", "Kamp\u00b7fes", "Mar\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit des Mutes Eigenschaft.", "tokens": ["Mit", "des", "Mu\u00b7tes", "Ei\u00b7gen\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sag' ich los mich jenem Jammer,", "tokens": ["Sag'", "ich", "los", "mich", "je\u00b7nem", "Jam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "PPER", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jenem tiefen Seelenweh,", "tokens": ["Je\u00b7nem", "tie\u00b7fen", "See\u00b7len\u00b7weh", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "G\u00fcrte meine Lenden strammer", "tokens": ["G\u00fcr\u00b7te", "mei\u00b7ne", "Len\u00b7den", "stram\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und gepanzert fest ich steh'!", "tokens": ["Und", "ge\u00b7pan\u00b7zert", "fest", "ich", "steh'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Fest wie eine Memnons\u00e4ule,", "tokens": ["Fest", "wie", "ei\u00b7ne", "Mem\u00b7non\u00b7s\u00e4u\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter mir den Staub der Welt,", "tokens": ["Un\u00b7ter", "mir", "den", "Staub", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob mein Blick auch drauf verweile \u2013", "tokens": ["Ob", "mein", "Blick", "auch", "drauf", "ver\u00b7wei\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "PAV", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "'s ist der Blick von einem Held.", "tokens": ["'s", "ist", "der", "Blick", "von", "ei\u00b7nem", "Held", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Fest wie eine Memnons\u00e4ule,", "tokens": ["Fest", "wie", "ei\u00b7ne", "Mem\u00b7non\u00b7s\u00e4u\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwarzer Marmor ist mein Kleid,", "tokens": ["Schwar\u00b7zer", "Mar\u00b7mor", "ist", "mein", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch nicht m\u00fc\u00dfig ich verweile,", "tokens": ["Doch", "nicht", "m\u00fc\u00b7\u00dfig", "ich", "ver\u00b7wei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der W\u00fcsten Einsamkeit.", "tokens": ["In", "der", "W\u00fcs\u00b7ten", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Pflegend nicht wie jener K\u00f6nig", "tokens": ["Pfle\u00b7gend", "nicht", "wie", "je\u00b7ner", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "KOKOM", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von Aegypten, feiger Ruh,", "tokens": ["Von", "A\u00b7e\u00b7gyp\u00b7ten", ",", "fei\u00b7ger", "Ruh", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der ist gar nichts oder wenig,", "tokens": ["Der", "ist", "gar", "nichts", "o\u00b7der", "we\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PIS", "KON", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der dem B\u00f6sen siehet zu. \u2013", "tokens": ["Der", "dem", "B\u00f6\u00b7sen", "sie\u00b7het", "zu", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "In die Enge, in's Gedr\u00e4nge", "tokens": ["In", "die", "En\u00b7ge", ",", "in's", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00fcrz' ich mich mit lautem Klang,", "tokens": ["St\u00fcrz'", "ich", "mich", "mit", "lau\u00b7tem", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Singe vor der ganzen Menge", "tokens": ["Sin\u00b7ge", "vor", "der", "gan\u00b7zen", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ew'ger Wahrheit gro\u00dfen Sang.", "tokens": ["Ew'\u00b7ger", "Wahr\u00b7heit", "gro\u00b7\u00dfen", "Sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}