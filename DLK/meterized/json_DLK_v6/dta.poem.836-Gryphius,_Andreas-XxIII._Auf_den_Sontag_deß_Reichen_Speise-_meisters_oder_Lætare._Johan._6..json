{"dta.poem.836": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxIII.  Auf den Sontag/ de\u00df Reichen Speise-  \n meisters/ oder  L\u00e6tare.  Johan. 6.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Ach wilt du dich noch mehr betr\u00fcbtes Hertz/ betr\u00fcben? ", "tokens": ["Ach", "wilt", "du", "dich", "noch", "mehr", "be\u00b7tr\u00fcb\u00b7tes", "Hertz", "/", "be\u00b7tr\u00fc\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "ADV", "PIAT", "ADJA", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geht dir dein Aschenbrod zu raw vnd bitter eyn?", "tokens": ["Geht", "dir", "dein", "A\u00b7schen\u00b7brod", "zu", "raw", "vnd", "bit\u00b7ter", "eyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Vnd mu\u00df dein Tranck vermischt mit herben Threnen seyn?", "tokens": ["Vnd", "mu\u00df", "dein", "Tranck", "ver\u00b7mischt", "mit", "her\u00b7ben", "Thre\u00b7nen", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der kan nieht/ der dich auch bi\u00df in den Todt kan lieben/", "tokens": ["Der", "kan", "nieht", "/", "der", "dich", "auch", "bi\u00df", "in", "den", "Todt", "kan", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "VVFIN", "$(", "PRELS", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "(wen sonst nichts helffen mag) den milden Rath auffschieb\u1ebd.", "tokens": ["(", "wen", "sonst", "nichts", "helf\u00b7fen", "mag", ")", "den", "mil\u00b7den", "Rath", "auff\u00b7schie\u00b7b\u1ebd", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ADV", "PIS", "VVINF", "VMFIN", "$(", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Niemand hungern l\u00e4st/ wird dir in hungers pein", "tokens": ["Der", "Nie\u00b7mand", "hun\u00b7gern", "l\u00e4st", "/", "wird", "dir", "in", "hun\u00b7gers", "pein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVINF", "VVFIN", "$(", "VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bescheren was du wilt/ was erstlich nichts vnd klein", "tokens": ["Be\u00b7sche\u00b7ren", "was", "du", "wilt", "/", "was", "erst\u00b7lich", "nichts", "vnd", "klein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PWS", "PPER", "VMFIN", "$(", "PWS", "ADV", "PIS", "KON", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Macht bald sein Segen gro\u00df/ wenn Er ", "tokens": ["Macht", "bald", "sein", "Se\u00b7gen", "gro\u00df", "/", "wenn", "Er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "PPOSAT", "NN", "ADJD", "$(", "KOUS", "PPER"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Vnd and\u2019re speisen wil/ so mu\u00df verzug vnd noth", "tokens": ["Vnd", "an\u00b7d'\u00b7re", "spei\u00b7sen", "wil", "/", "so", "mu\u00df", "ver\u00b7zug", "vnd", "noth"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVINF", "VMFIN", "$(", "ADV", "VMFIN", "NN", "KON", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Vorher gehn/ wenn Er sich/ das ware Himmelbrodt", "tokens": ["Vor\u00b7her", "gehn", "/", "wenn", "Er", "sich", "/", "das", "wa\u00b7re", "Him\u00b7mel\u00b7brodt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVINF", "$(", "KOUS", "PPER", "PRF", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dir gibt im Abendmal/ must du vor hunger sp\u00fcreu.", "tokens": ["Dir", "gibt", "im", "A\u00b7bend\u00b7mal", "/", "must", "du", "vor", "hun\u00b7ger", "sp\u00fc\u00b7reu", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$(", "VMFIN", "PPER", "APPR", "ADJA", "ADJD", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Wenn kein Prophet mehr lehrt/ wenn ", "tokens": ["Wenn", "kein", "Pro\u00b7phet", "mehr", "lehrt", "/", "wenn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PIAT", "NN", "ADV", "VVFIN", "$(", "KOUS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "tand ", "tokens": ["tand"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Von Mosis Kantzel h\u00f6rt; denn lehrt Er Statt vnd Land", "tokens": ["Von", "Mo\u00b7sis", "Kant\u00b7zel", "h\u00f6rt", ";", "denn", "lehrt", "Er", "Statt", "vnd", "Land"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NE", "VVFIN", "$.", "KON", "VVFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd pflegt in sein Palast/ was hier verschmacht/ zu f\u00fchren.", "tokens": ["Vnd", "pflegt", "in", "sein", "Pa\u00b7last", "/", "was", "hier", "ver\u00b7schmacht", "/", "zu", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$(", "PWS", "ADV", "VVPP", "$(", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}