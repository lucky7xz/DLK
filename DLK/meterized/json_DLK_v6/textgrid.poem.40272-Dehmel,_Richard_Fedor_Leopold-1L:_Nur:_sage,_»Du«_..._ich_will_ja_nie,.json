{"textgrid.poem.40272": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nur: sage, \u00bbDu\u00ab ... ich will ja nie,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur: sage, \u00bbDu\u00ab ... ich will ja nie,", "tokens": ["Nur", ":", "sa\u00b7ge", ",", "\u00bb", "Du", "\u00ab", "...", "ich", "will", "ja", "nie", ","], "token_info": ["word", "punct", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "VVFIN", "$,", "$(", "PPER", "$(", "$(", "PPER", "VMFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "nie wieder deine Lippen k\u00fcssen,", "tokens": ["nie", "wie\u00b7der", "dei\u00b7ne", "Lip\u00b7pen", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "nun wir's gef\u00fchlt, so Knie an Knie", "tokens": ["nun", "wir's", "ge\u00b7f\u00fchlt", ",", "so", "Knie", "an", "Knie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "VVPP", "$,", "ADV", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "gef\u00fchlt, da\u00df wir uns lieben m\u00fcssen.", "tokens": ["ge\u00b7f\u00fchlt", ",", "da\u00df", "wir", "uns", "lie\u00b7ben", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPER", "PRF", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Abendrot umfing so brennend", "tokens": ["Das", "A\u00b7ben\u00b7drot", "um\u00b7fing", "so", "bren\u00b7nend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "der Eichen hohe Knospenkette;", "tokens": ["der", "Ei\u00b7chen", "ho\u00b7he", "Knos\u00b7pen\u00b7ket\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "wir aber sahen nur, uns trennend,", "tokens": ["wir", "a\u00b7ber", "sa\u00b7hen", "nur", ",", "uns", "tren\u00b7nend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "$,", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "die schwarzen, ragenden Skelette.", "tokens": ["die", "schwar\u00b7zen", ",", "ra\u00b7gen\u00b7den", "Ske\u00b7let\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und nickten doch von vielen B\u00e4umen", "tokens": ["Und", "nick\u00b7ten", "doch", "von", "vie\u00b7len", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "schon Bl\u00fcten unsrer Liebe zu", "tokens": ["schon", "Bl\u00fc\u00b7ten", "uns\u00b7rer", "Lie\u00b7be", "zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und erstes, keusches Gr\u00fcn; so tr\u00e4umen,", "tokens": ["und", "ers\u00b7tes", ",", "keu\u00b7sches", "Gr\u00fcn", ";", "so", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "$,", "ADJA", "NN", "$.", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "so nicken Kinder ... sage \u00bbDu\u00ab.", "tokens": ["so", "ni\u00b7cken", "Kin\u00b7der", "...", "sa\u00b7ge", "\u00bb", "Du", "\u00ab", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["ADV", "ADJA", "NN", "$(", "VVFIN", "$(", "PPER", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}