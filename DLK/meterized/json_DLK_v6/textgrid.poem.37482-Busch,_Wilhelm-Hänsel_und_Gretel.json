{"textgrid.poem.37482": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "H\u00e4nsel und Gretel", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbihr Kinder\u00ab, spricht das M\u00fctterlein,", "tokens": ["\u00bb", "ihr", "Kin\u00b7der", "\u00ab", ",", "spricht", "das", "M\u00fct\u00b7ter\u00b7lein", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$(", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbgeht ja nicht in den Wald hinein!\u00ab", "tokens": ["\u00bb", "geht", "ja", "nicht", "in", "den", "Wald", "hin\u00b7ein", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "ADV", "PTKNEG", "APPR", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Ja, prosit! wenn der Has' nicht w\u00e4r!", "tokens": ["Ja", ",", "pro\u00b7sit", "!", "wenn", "der", "Has'", "nicht", "w\u00e4r", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NE", "$.", "KOUS", "ART", "NN", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gleich m\u00fcssen sie dahinter her.", "tokens": ["Gleich", "m\u00fcs\u00b7sen", "sie", "da\u00b7hin\u00b7ter", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nicht lange, eh man's sich versah,", "tokens": ["Nicht", "lan\u00b7ge", ",", "eh", "man's", "sich", "ver\u00b7sah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "KOUS", "PIS", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Steht schon die Kinderfalle da.", "tokens": ["Steht", "schon", "die", "Kin\u00b7der\u00b7fal\u00b7le", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die b\u00f6se Hexe schreit: \u00bbNanu!\u00ab", "tokens": ["Die", "b\u00f6\u00b7se", "He\u00b7xe", "schreit", ":", "\u00bb", "Na\u00b7nu", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "$(", "ITJ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Perdatsch! da f\u00e4llt die Falle zu.", "tokens": ["Per\u00b7datsch", "!", "da", "f\u00e4llt", "die", "Fal\u00b7le", "zu", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und Hans und Gretel, ach, o Graus!", "tokens": ["Und", "Hans", "und", "Gre\u00b7tel", ",", "ach", ",", "o", "Graus", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "KON", "NE", "$,", "ITJ", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schleppt man bis in das Hexenhaus.", "tokens": ["Schleppt", "man", "bis", "in", "das", "He\u00b7xen\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Hexe macht das Feuer an,", "tokens": ["Die", "He\u00b7xe", "macht", "das", "Feu\u00b7er", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df sie die Kinder kochen kann.", "tokens": ["Da\u00df", "sie", "die", "Kin\u00b7der", "ko\u00b7chen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Am Tisch der dicke B\u00f6sewicht,", "tokens": ["Am", "Tisch", "der", "di\u00b7cke", "B\u00f6\u00b7se\u00b7wicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der pa\u00dft schon auf sein Leibgericht.", "tokens": ["Der", "pa\u00dft", "schon", "auf", "sein", "Leib\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch H\u00e4nsel fa\u00dft die Hex am Bein,", "tokens": ["Doch", "H\u00e4n\u00b7sel", "fa\u00dft", "die", "Hex", "am", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Plums! f\u00e4llt sie in den Topf hinein.", "tokens": ["Plums", "!", "f\u00e4llt", "sie", "in", "den", "Topf", "hin\u00b7ein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Die Hexe kriegte ihren Lohn,", "tokens": ["Die", "He\u00b7xe", "krieg\u00b7te", "ih\u00b7ren", "Lohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tot h\u00e4ngt sie an der Gabel schon.", "tokens": ["Tot", "h\u00e4ngt", "sie", "an", "der", "Ga\u00b7bel", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Der Menschenfresser, zornentbrannt,", "tokens": ["Der", "Men\u00b7schen\u00b7fres\u00b7ser", ",", "zor\u00b7nent\u00b7brannt", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kommt mit dem Messer angerannt.", "tokens": ["Kommt", "mit", "dem", "Mes\u00b7ser", "an\u00b7ge\u00b7rannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Im Kasten will er sie ertappen,", "tokens": ["Im", "Kas\u00b7ten", "will", "er", "sie", "er\u00b7tap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Kasten aber hat zwei Klappen.", "tokens": ["Der", "Kas\u00b7ten", "a\u00b7ber", "hat", "zwei", "Klap\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "O weh! Das hat er nicht bedacht,", "tokens": ["O", "weh", "!", "Das", "hat", "er", "nicht", "be\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "PDS", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun wird der K\u00e4fig zugemacht.", "tokens": ["Nun", "wird", "der", "K\u00e4\u00b7fig", "zu\u00b7ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Der Dicke wird gerollt \u2013 und plumpf!", "tokens": ["Der", "Di\u00b7cke", "wird", "ge\u00b7rollt", "\u2013", "und", "plumpf", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schmei\u00dft man ihn in den tiefen Sumpf.", "tokens": ["Schmei\u00dft", "man", "ihn", "in", "den", "tie\u00b7fen", "Sumpf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.14": {"line.1": {"text": "Jetzt gehn die zwei zum Wald hinaus,", "tokens": ["Jetzt", "gehn", "die", "zwei", "zum", "Wald", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "CARD", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Mutter schaut schon aus dem Haus;", "tokens": ["Die", "Mut\u00b7ter", "schaut", "schon", "aus", "dem", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Sie winkt und l\u00e4\u00dft die Rute sehn:", "tokens": ["Sie", "winkt", "und", "l\u00e4\u00dft", "die", "Ru\u00b7te", "sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Na, gute Nacht! da dank ich sch\u00f6n!", "tokens": ["Na", ",", "gu\u00b7te", "Nacht", "!", "da", "dank", "ich", "sch\u00f6n", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}