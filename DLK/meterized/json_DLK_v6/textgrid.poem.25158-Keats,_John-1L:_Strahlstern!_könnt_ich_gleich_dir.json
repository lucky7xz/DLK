{"textgrid.poem.25158": {"metadata": {"author": {"name": "Keats, John", "birth": "N.A.", "death": "N.A."}, "title": "1L: Strahlstern! k\u00f6nnt ich gleich dir ", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Strahlstern! k\u00f6nnt ich gleich dir ", "tokens": ["Strahl\u00b7stern", "!", "k\u00f6nnt", "ich", "gleich", "dir"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "PPER", "ADV", "PPER"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nicht einsam prangend in der n\u00e4chtigen Herde,", "tokens": ["Nicht", "ein\u00b7sam", "pran\u00b7gend", "in", "der", "n\u00e4ch\u00b7ti\u00b7gen", "Her\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nicht offnen Lides wandern im Verein", "tokens": ["Nicht", "off\u00b7nen", "Li\u00b7des", "wan\u00b7dern", "im", "Ver\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJA", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit dem geduldigen Eremit der Erde,", "tokens": ["Mit", "dem", "ge\u00b7dul\u00b7di\u00b7gen", "E\u00b7re\u00b7mit", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Dem Strom des Wassers, der mit Priesterhand", "tokens": ["Dem", "Strom", "des", "Was\u00b7sers", ",", "der", "mit", "Pries\u00b7ter\u00b7hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Menschen Lande w\u00e4scht in ewigem Wachen,", "tokens": ["Der", "Men\u00b7schen", "Lan\u00b7de", "w\u00e4scht", "in", "e\u00b7wi\u00b7gem", "Wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nicht starrend auf der Berge Schneegewand", "tokens": ["Nicht", "star\u00b7rend", "auf", "der", "Ber\u00b7ge", "Schnee\u00b7ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und dunkler Moore gr\u00fcn verschlossne Rachen \u2013", "tokens": ["Und", "dunk\u00b7ler", "Moo\u00b7re", "gr\u00fcn", "ver\u00b7schloss\u00b7ne", "Ra\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nein \u2013 doch best\u00e4ndig: immerdar gebettet", "tokens": ["Nein", "\u2013", "doch", "be\u00b7st\u00e4n\u00b7dig", ":", "im\u00b7mer\u00b7dar", "ge\u00b7bet\u00b7tet"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$(", "ADV", "ADJD", "$.", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf der Geliebten reifend wache Brust,", "tokens": ["Auf", "der", "Ge\u00b7lieb\u00b7ten", "rei\u00b7fend", "wa\u00b7che", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie Schwellen sich mit Sinken zart verkettet,", "tokens": ["Wie", "Schwel\u00b7len", "sich", "mit", "Sin\u00b7ken", "zart", "ver\u00b7ket\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PRF", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sanft f\u00fchlend, s\u00fc\u00dfer Unruh stets bewu\u00dft,", "tokens": ["Sanft", "f\u00fch\u00b7lend", ",", "s\u00fc\u00b7\u00dfer", "Un\u00b7ruh", "stets", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "ADJA", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Noch h\u00f6rend, noch, des Atems lindes Wehen \u2013", "tokens": ["Noch", "h\u00f6\u00b7rend", ",", "noch", ",", "des", "A\u00b7tems", "lin\u00b7des", "We\u00b7hen", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "$,", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So ewig leben \u2013 oder tot vergehen!", "tokens": ["So", "e\u00b7wig", "le\u00b7ben", "\u2013", "o\u00b7der", "tot", "ver\u00b7ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$(", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}