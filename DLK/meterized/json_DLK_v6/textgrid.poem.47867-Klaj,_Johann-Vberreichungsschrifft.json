{"textgrid.poem.47867": {"metadata": {"author": {"name": "Klaj, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Vberreichungsschrifft", "genre": "verse", "period": "N.A.", "pub_year": 1636, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr grossen V\u00e4tter nemt/ nemt hin di\u00df mein Gedichte/", "tokens": ["Ihr", "gros\u00b7sen", "V\u00e4t\u00b7ter", "nemt", "/", "nemt", "hin", "di\u00df", "mein", "Ge\u00b7dich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$(", "VVFIN", "ADV", "PDS", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ach nemet es doch an mit gn\u00e4digem Gesichte/", "tokens": ["Ach", "ne\u00b7met", "es", "doch", "an", "mit", "gn\u00e4\u00b7di\u00b7gem", "Ge\u00b7sich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "ADV", "APPR", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was meine Clio nechst gespielt zur Osterzeit/", "tokens": ["Was", "mei\u00b7ne", "Clio", "nechst", "ge\u00b7spielt", "zur", "Os\u00b7ter\u00b7zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADV", "VVPP", "APPRART", "NN", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "In eurer Gegenwart mit schlechter Liebligkeit.", "tokens": ["In", "eu\u00b7rer", "Ge\u00b7gen\u00b7wart", "mit", "schlech\u00b7ter", "Lieb\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nun leset/wenn ihr legt die grossen Sorgen nieder", "tokens": ["Nun", "le\u00b7set", "/", "wenn", "ihr", "legt", "die", "gros\u00b7sen", "Sor\u00b7gen", "nie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$(", "KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "F\u00fcr diese grosse Stadt/ ihr V\u00e4tter/ meine Lieder/", "tokens": ["F\u00fcr", "die\u00b7se", "gros\u00b7se", "Stadt", "/", "ihr", "V\u00e4t\u00b7ter", "/", "mei\u00b7ne", "Lie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$(", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die ich zu Gottes Lob und eurer Lust gemacht/", "tokens": ["Die", "ich", "zu", "Got\u00b7tes", "Lob", "und", "eu\u00b7rer", "Lust", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "NN", "KON", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ohn allen W\u00f6rterschein/ ohn alle Rednerpracht.", "tokens": ["Ohn", "al\u00b7len", "W\u00f6r\u00b7ter\u00b7schein", "/", "ohn", "al\u00b7le", "Red\u00b7ner\u00b7pracht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$(", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es hat euch ie und ie von Hertzen wolgefallen", "tokens": ["Es", "hat", "euch", "ie", "und", "ie", "von", "Hert\u00b7zen", "wol\u00b7ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "KON", "ADV", "APPR", "NN", "VMFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der edlen K\u00fcnste Kunst die Po\u00ebsi vor allen/", "tokens": ["Der", "ed\u00b7len", "K\u00fcns\u00b7te", "Kunst", "die", "Po\u00eb\u00b7si", "vor", "al\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "ART", "NN", "APPR", "PIAT", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Di\u00df weiset Celtes au\u00df", "tokens": ["Di\u00df", "wei\u00b7set", "Cel\u00b7tes", "au\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "NE", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "und wie sein Friederich in gr\u00fcnen Lorbeern geht/", "tokens": ["und", "wie", "sein", "Frie\u00b7de\u00b7rich", "in", "gr\u00fc\u00b7nen", "Lor\u00b7beern", "geht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Auch Eoban", "tokens": ["Auch", "E\u00b7o\u00b7ban"], "token_info": ["word", "word"], "pos": ["ADV", "NE"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Weil in der R\u00f6merzung er eure Stadt gepriesen.", "tokens": ["Weil", "in", "der", "R\u00f6\u00b7mer\u00b7zung", "er", "eu\u00b7re", "Stadt", "ge\u00b7prie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der R\u00f6mer/ der ihm hat bi\u00dfher sehr viel getraut/", "tokens": ["Der", "R\u00f6\u00b7mer", "/", "der", "ihm", "hat", "bi\u00df\u00b7her", "sehr", "viel", "ge\u00b7traut", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "PPER", "VAFIN", "ADV", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ist gegen Teutsch ein Wind und Klingel ohne Laut/", "tokens": ["Ist", "ge\u00b7gen", "Teutsch", "ein", "Wind", "und", "Klin\u00b7gel", "oh\u00b7ne", "Laut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "ART", "NN", "KON", "NN", "APPR", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Seid da\u00df wir aufgewacht/ der abgef\u00fchrten Grichen", "tokens": ["Seid", "da\u00df", "wir", "auf\u00b7ge\u00b7wacht", "/", "der", "ab\u00b7ge\u00b7f\u00fchr\u00b7ten", "Gri\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAIMP", "KOUS", "PPER", "VVPP", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Vers\u00fcster Verseklang gantz emsig nachgeschlichen/", "tokens": ["Ver\u00b7s\u00fcs\u00b7ter", "Ver\u00b7se\u00b7klang", "gantz", "em\u00b7sig", "nach\u00b7ge\u00b7schli\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "und auch der Mutter Rom den Handgrif abgemerkt/", "tokens": ["und", "auch", "der", "Mut\u00b7ter", "Rom", "den", "Hand\u00b7grif", "ab\u00b7ge\u00b7merkt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "NE", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der unsrer Sprachen Schmuk nun \u00fcm ein mercklichs st\u00e4rkt.", "tokens": ["Der", "uns\u00b7rer", "Spra\u00b7chen", "Schmuk", "nun", "\u00fcm", "ein", "merck\u00b7lichs", "st\u00e4rkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "ADV", "ADV", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Jetzt kan ein teutscher Mann sich rechtbehutsam sch\u00fctzen/", "tokens": ["Jetzt", "kan", "ein", "teut\u00b7scher", "Mann", "sich", "recht\u00b7be\u00b7hut\u00b7sam", "sch\u00fct\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "PRF", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Den Feind zu r\u00fckke haltn", "tokens": ["Den", "Feind", "zu", "r\u00fck\u00b7ke", "haltn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Er gehet der Natur in allen Dingen nach/", "tokens": ["Er", "ge\u00b7het", "der", "Na\u00b7tur", "in", "al\u00b7len", "Din\u00b7gen", "nach", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Er donnert/ sauset/ braust/ er rauschet gleich dem Bach/", "tokens": ["Er", "don\u00b7nert", "/", "sau\u00b7set", "/", "braust", "/", "er", "rau\u00b7schet", "gleich", "dem", "Bach", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "VVFIN", "$(", "VVFIN", "$(", "PPER", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der Lerchen h\u00f6rt er ab ihr Tiretireliren/", "tokens": ["Der", "Ler\u00b7chen", "h\u00f6rt", "er", "ab", "ihr", "Ti\u00b7re\u00b7ti\u00b7re\u00b7li\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+--+-+--", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Den Wassern ihr Ges\u00e4ust und murmelndes Spatziren/", "tokens": ["Den", "Was\u00b7sern", "ihr", "Ge\u00b7s\u00e4ust", "und", "mur\u00b7meln\u00b7des", "Spat\u00b7zi\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.27": {"text": "Er rasselt/ prasselt/ br\u00fclt/ wie wann der Donner brumt/", "tokens": ["Er", "ras\u00b7selt", "/", "pras\u00b7selt", "/", "br\u00fclt", "/", "wie", "wann", "der", "Don\u00b7ner", "brumt", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "VVFIN", "$(", "VVFIN", "$(", "KON", "PWAV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "und bey gew\u00f6lkter Nacht der Winde St\u00fcrmen sumt/", "tokens": ["und", "bey", "ge\u00b7w\u00f6lk\u00b7ter", "Nacht", "der", "Win\u00b7de", "St\u00fcr\u00b7men", "sumt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Da\u00df Wild und Wald erstaunt. Wie Berge Feuer streuen/", "tokens": ["Da\u00df", "Wild", "und", "Wald", "er\u00b7staunt", ".", "Wie", "Ber\u00b7ge", "Feu\u00b7er", "streu\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KON", "NN", "VVPP", "$.", "PWAV", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Bepichen Laub und Gras heissiedend Hartz au\u00dfspeien/", "tokens": ["Be\u00b7pi\u00b7chen", "Laub", "und", "Gras", "heis\u00b7sie\u00b7dend", "Hartz", "au\u00df\u00b7spei\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVPP", "NE", "NE", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Spricht ietzt ein teutscher Mund mit sch\u00f6nen Reden au\u00df/", "tokens": ["Spricht", "ietzt", "ein", "teut\u00b7scher", "Mund", "mit", "sch\u00f6\u00b7nen", "Re\u00b7den", "au\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Parnassus ist nun teutsch der Musen Sommerhau\u00df.", "tokens": ["Par\u00b7nas\u00b7sus", "ist", "nun", "teutsch", "der", "Mu\u00b7sen", "Som\u00b7mer\u00b7hau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Nun wol so lieber den/ ihr alten teutschen Helden/", "tokens": ["Nun", "wol", "so", "lie\u00b7ber", "den", "/", "ihr", "al\u00b7ten", "teut\u00b7schen", "Hel\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "ART", "$(", "PPOSAT", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Der in der Mutterzung wil euren Ruhm vermelden/", "tokens": ["Der", "in", "der", "Mut\u00b7ter\u00b7zung", "wil", "eu\u00b7ren", "Ruhm", "ver\u00b7mel\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "und euer N\u00fcrenberg/die Lust und Zier der Welt/", "tokens": ["und", "eu\u00b7er", "N\u00fc\u00b7ren\u00b7berg", "/", "die", "Lust", "und", "Zier", "der", "Welt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$(", "ART", "NN", "KON", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "In welcher Kunst und Witz/in welcher Gold und Geld/", "tokens": ["In", "wel\u00b7cher", "Kunst", "und", "Witz/in", "wel\u00b7cher", "Gold", "und", "Geld", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "KON", "NN", "PWAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "Wil in das Sternenbuch mit g\u00fcldner Dinte schreiben/", "tokens": ["Wil", "in", "das", "Ster\u00b7nen\u00b7buch", "mit", "g\u00fcld\u00b7ner", "Din\u00b7te", "schrei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.38": {"text": "Da\u00df es die Nachwelt liest und ewig mu\u00df bekleiben.", "tokens": ["Da\u00df", "es", "die", "Nach\u00b7welt", "liest", "und", "e\u00b7wig", "mu\u00df", "be\u00b7klei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "KON", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Den Marmel frist die Zeit/ Stahl rostet und wird alt/", "tokens": ["Den", "Mar\u00b7mel", "frist", "die", "Zeit", "/", "Stahl", "ros\u00b7tet", "und", "wird", "alt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$(", "NN", "VVFIN", "KON", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Das Gold wird wieder Koht/ Holtz brent und faulet bald/", "tokens": ["Das", "Gold", "wird", "wie\u00b7der", "Koht", "/", "Holtz", "brent", "und", "fau\u00b7let", "bald", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "NN", "$(", "NN", "VVFIN", "KON", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Nur ein Po\u00ebt verbleibt/ und seine Lust die B\u00fccher", "tokens": ["Nur", "ein", "Po\u00ebt", "ver\u00b7bleibt", "/", "und", "sei\u00b7ne", "Lust", "die", "B\u00fc\u00b7cher"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "$(", "KON", "PPOSAT", "NN", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.42": {"text": "Sind f\u00fcr dem Vntergang mit ihrem Vatter sicher/", "tokens": ["Sind", "f\u00fcr", "dem", "Vn\u00b7ter\u00b7gang", "mit", "ih\u00b7rem", "Vat\u00b7ter", "si\u00b7cher", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "und pochen Zeit und Tod. Gott kr\u00f6ne di