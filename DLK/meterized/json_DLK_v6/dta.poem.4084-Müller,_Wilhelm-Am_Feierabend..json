{"dta.poem.4084": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Am Feierabend.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Arme zu r\u00fchren!", "tokens": ["Ar\u00b7me", "zu", "r\u00fch\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "K\u00f6nnt' ich brausend", "tokens": ["K\u00f6nnt'", "ich", "brau\u00b7send"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Die R\u00e4der f\u00fchren!", "tokens": ["Die", "R\u00e4\u00b7der", "f\u00fch\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "K\u00f6nnt' ich wehen", "tokens": ["K\u00f6nnt'", "ich", "we\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Durch alle Haine,", "tokens": ["Durch", "al\u00b7le", "Hai\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "K\u00f6nnt' ich drehen", "tokens": ["K\u00f6nnt'", "ich", "dre\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Alle Steine!", "tokens": ["Al\u00b7le", "Stei\u00b7ne", "!"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "--+-", "measure": "anapaest.init"}, "line.8": {"text": "Da\u00df die sch\u00f6ne M\u00fcllerin", "tokens": ["Da\u00df", "die", "sch\u00f6\u00b7ne", "M\u00fcl\u00b7le\u00b7rin"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Merkte meinen treuen Sinn!", "tokens": ["Merk\u00b7te", "mei\u00b7nen", "treu\u00b7en", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, wie ist mein Arm so schwach!", "tokens": ["Ach", ",", "wie", "ist", "mein", "Arm", "so", "schwach", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was ich hebe, was ich trage,", "tokens": ["Was", "ich", "he\u00b7be", ",", "was", "ich", "tra\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Was ich schneide, was ich schlage,", "tokens": ["Was", "ich", "schnei\u00b7de", ",", "was", "ich", "schla\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jeder Knappe thut es nach.", "tokens": ["Je\u00b7der", "Knap\u00b7pe", "thut", "es", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und da sitz' ich in der gro\u00dfen Runde,", "tokens": ["Und", "da", "sitz'", "ich", "in", "der", "gro\u00b7\u00dfen", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "In der stillen k\u00fchlen Feierstunde,", "tokens": ["In", "der", "stil\u00b7len", "k\u00fch\u00b7len", "Fei\u00b7er\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Und der Meister spricht zu Allen:", "tokens": ["Und", "der", "Meis\u00b7ter", "spricht", "zu", "Al\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Euer Werk hat mir gefallen;", "tokens": ["Eu\u00b7er", "Werk", "hat", "mir", "ge\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.9": {"text": "Und das liebe M\u00e4dchen sagt", "tokens": ["Und", "das", "lie\u00b7be", "M\u00e4d\u00b7chen", "sagt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Allen eine gute Nacht.", "tokens": ["Al\u00b7len", "ei\u00b7ne", "gu\u00b7te", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}