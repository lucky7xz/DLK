{"textgrid.poem.58233": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "271. Vor sich sehen, macht sicher gehen", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was Gott von Ewigkeit und in die Ewigkeiten", "tokens": ["Was", "Gott", "von", "E\u00b7wig\u00b7keit", "und", "in", "die", "E\u00b7wig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "NN", "APPR", "NN", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Getan hat und wird tun, drum will ich nicht viel streiten.", "tokens": ["Ge\u00b7tan", "hat", "und", "wird", "tun", ",", "drum", "will", "ich", "nicht", "viel", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "KON", "VAFIN", "VVINF", "$,", "PAV", "VMFIN", "PPER", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Im Gegenw\u00e4rtigen bleib' ich nur auf der Wacht,", "tokens": ["Im", "Ge\u00b7gen\u00b7w\u00e4r\u00b7ti\u00b7gen", "bleib'", "ich", "nur", "auf", "der", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ich bin nur Kind im Haus, ich lass' den Vater machen,", "tokens": ["Ich", "bin", "nur", "Kind", "im", "Haus", ",", "ich", "lass'", "den", "Va\u00b7ter", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "APPRART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ihn lieben ist mein Werk, und er tut seine Sachen;", "tokens": ["Ihn", "lie\u00b7ben", "ist", "mein", "Werk", ",", "und", "er", "tut", "sei\u00b7ne", "Sa\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VAFIN", "PPOSAT", "NN", "$,", "KON", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Am Ende werd ich's sehn, da\u00df er es wohl gemacht.", "tokens": ["Am", "En\u00b7de", "werd", "ich's", "sehn", ",", "da\u00df", "er", "es", "wohl", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PIS", "VVINF", "$,", "KOUS", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}