{"textgrid.poem.38120": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der Staar und das Badw\u00e4nnelein", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Konrad war ein m\u00fcder Mann,", "tokens": ["Herr", "Kon\u00b7rad", "war", "ein", "m\u00fc\u00b7der", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er band sein Ro\u00df am Wirthshaus an.", "tokens": ["Er", "band", "sein", "Ro\u00df", "am", "Wirths\u00b7haus", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das M\u00e4gdlein sprach, steig ab, steig ab,", "tokens": ["Das", "M\u00e4gd\u00b7lein", "sprach", ",", "steig", "ab", ",", "steig", "ab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihre Aeuglein schwankten auf und ab.", "tokens": ["Ih\u00b7re", "A\u00b7e\u00b7u\u00b7glein", "schwank\u00b7ten", "auf", "und", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Ach Jungfer liebste Jungfrau mein,", "tokens": ["Ach", "Jung\u00b7fer", "liebs\u00b7te", "Jung\u00b7frau", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "ADJA", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schenk mir ein Becher k\u00fchlen Wein ein.", "tokens": ["Schenk", "mir", "ein", "Be\u00b7cher", "k\u00fch\u00b7len", "Wein", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach Herre, lieber Herre mein!", "tokens": ["Ach", "Her\u00b7re", ",", "lie\u00b7ber", "Her\u00b7re", "mein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "ADV", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bring ein Becher k\u00fchlen Wein.", "tokens": ["Ich", "bring", "ein", "Be\u00b7cher", "k\u00fch\u00b7len", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Trink ab, trink ab du rother Mund,", "tokens": ["Trink", "ab", ",", "trink", "ab", "du", "ro\u00b7ther", "Mund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "VVFIN", "PTKVZ", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Trink aus den Becher auf den Grund.", "tokens": ["Trink", "aus", "den", "Be\u00b7cher", "auf", "den", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Frau Wirthin, liebe Frau Wirthin mein,", "tokens": ["Frau", "Wirt\u00b7hin", ",", "lie\u00b7be", "Frau", "Wirt\u00b7hin", "mein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJA", "NN", "NN", "PPOSAT", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ist dies f\u00fcrwahr euer T\u00f6chterlein?", "tokens": ["Ist", "dies", "f\u00fcr\u00b7wahr", "eu\u00b7er", "T\u00f6ch\u00b7ter\u00b7lein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Mein T\u00f6chterlein ist sie nicht f\u00fcrwahr,", "tokens": ["Mein", "T\u00f6ch\u00b7ter\u00b7lein", "ist", "sie", "nicht", "f\u00fcr\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie ist mein Magd f\u00fcr immerdar.", "tokens": ["Sie", "ist", "mein", "Magd", "f\u00fcr", "im\u00b7mer\u00b7dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wollt ihr mir sie leihen auf eine Nacht?", "tokens": ["Wollt", "ihr", "mir", "sie", "lei\u00b7hen", "auf", "ei\u00b7ne", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PPER", "VVINF", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "So will ich euch geben des Goldes Macht.", "tokens": ["So", "will", "ich", "euch", "ge\u00b7ben", "des", "Gol\u00b7des", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.9": {"line.1": {"text": "Wollt ihr mir geben des Goldes Macht,", "tokens": ["Wollt", "ihr", "mir", "ge\u00b7ben", "des", "Gol\u00b7des", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Will ich sie euch leihen auf eine Nacht.", "tokens": ["Will", "ich", "sie", "euch", "lei\u00b7hen", "auf", "ei\u00b7ne", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PPER", "VVINF", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.10": {"line.1": {"text": "Nun richt dem Herrn ein Fu\u00dfbad an,", "tokens": ["Nun", "richt", "dem", "Herrn", "ein", "Fu\u00df\u00b7bad", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Rosmarin und Majoran.", "tokens": ["Mit", "Ros\u00b7ma\u00b7rin", "und", "Ma\u00b7jo\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Sie ging in Garten und brach das Kraut,", "tokens": ["Sie", "ging", "in", "Gar\u00b7ten", "und", "brach", "das", "Kraut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da sprach der Staar, \u00bbo weh du Braut,", "tokens": ["Da", "sprach", "der", "Staar", ",", "\u00bb", "o", "weh", "du", "Braut", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "$(", "FM", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "In dem Badw\u00e4nnelein ist sie hergetragen,", "tokens": ["In", "dem", "Bad\u00b7w\u00e4n\u00b7nel\u00b7ein", "ist", "sie", "her\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Darin mu\u00df sie ihm die F\u00fc\u00dfe zwagen,", "tokens": ["Da\u00b7rin", "mu\u00df", "sie", "ihm", "die", "F\u00fc\u00b7\u00dfe", "zwa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.13": {"line.1": {"text": "Der Vater starb in Leid und Noth,", "tokens": ["Der", "Va\u00b7ter", "starb", "in", "Leid", "und", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Mutter gr\u00e4mt sich schier zu todt.", "tokens": ["Die", "Mut\u00b7ter", "gr\u00e4mt", "sich", "schier", "zu", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "O weh du Braut! du Findelkind,", "tokens": ["O", "weh", "du", "Braut", "!", "du", "Fin\u00b7del\u00b7kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "NN", "$.", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wei\u00dft nicht wo 