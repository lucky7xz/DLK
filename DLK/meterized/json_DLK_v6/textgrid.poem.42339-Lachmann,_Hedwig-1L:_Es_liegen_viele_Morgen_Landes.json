{"textgrid.poem.42339": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es liegen viele Morgen Landes", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es liegen viele Morgen Landes", "tokens": ["Es", "lie\u00b7gen", "vie\u00b7le", "Mor\u00b7gen", "Lan\u00b7des"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Seit Ewigkeiten unbestellt.", "tokens": ["Seit", "E\u00b7wig\u00b7kei\u00b7ten", "un\u00b7be\u00b7stellt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit H\u00fcgeln sturmverwehten Sandes", "tokens": ["Mit", "H\u00fc\u00b7geln", "sturm\u00b7ver\u00b7weh\u00b7ten", "San\u00b7des"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beschwert von Anbeginn der Welt.", "tokens": ["Be\u00b7schwert", "von", "An\u00b7be\u00b7ginn", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kein S\u00e4mann hat in ihre Poren", "tokens": ["Kein", "S\u00e4\u00b7mann", "hat", "in", "ih\u00b7re", "Po\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Saat des Lebens je gesenkt.", "tokens": ["Die", "Saat", "des", "Le\u00b7bens", "je", "ge\u00b7senkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In der Unendlichkeit verloren", "tokens": ["In", "der", "Un\u00b7end\u00b7lich\u00b7keit", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verf\u00e4llt ihr Staub, dem Staub vermengt.", "tokens": ["Ver\u00b7f\u00e4llt", "ihr", "Staub", ",", "dem", "Staub", "ver\u00b7mengt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}