{"dta.poem.19571": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die sterbende Meduse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Ein kurzes Schwert gez\u00fcckt in nerv'ger Rechten,", "tokens": ["Ein", "kur\u00b7zes", "Schwert", "ge\u00b7z\u00fcckt", "in", "ner\u00b7v'\u00b7ger", "Rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Belauert Perseus bang in seinem Schild", "tokens": ["Be\u00b7lau\u00b7ert", "Per\u00b7seus", "bang", "in", "sei\u00b7nem", "Schild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der schlummernden Meduse Spiegelbild,", "tokens": ["Der", "schlum\u00b7mern\u00b7den", "Me\u00b7du\u00b7se", "Spie\u00b7gel\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das s\u00fc\u00dfe Haupt mit m\u00fcden Schlangenflechten.", "tokens": ["Das", "s\u00fc\u00b7\u00dfe", "Haupt", "mit", "m\u00fc\u00b7den", "Schlan\u00b7gen\u00b7flech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zur H\u00e4lfte zeigt der Spiegel l\u00e4ngs der Erde", "tokens": ["Zur", "H\u00e4lf\u00b7te", "zeigt", "der", "Spie\u00b7gel", "l\u00e4ngs", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des jungen Wuchses athmende Geberde \u2014", "tokens": ["Des", "jun\u00b7gen", "Wuch\u00b7ses", "ath\u00b7men\u00b7de", "Ge\u00b7ber\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "\u201eraub' ich das arge Haupt mit raschem Hiebe,", "tokens": ["\u201e", "raub'", "ich", "das", "ar\u00b7ge", "Haupt", "mit", "ra\u00b7schem", "Hie\u00b7be", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Verderblich der Verderberin genaht?", "tokens": ["Ver\u00b7derb\u00b7lich", "der", "Ver\u00b7der\u00b7be\u00b7rin", "ge\u00b7naht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wenn nur die blonde Wimper schlummernd bliebe!", "tokens": ["Wenn", "nur", "die", "blon\u00b7de", "Wim\u00b7per", "schlum\u00b7mernd", "blie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der Blick versteint! Gef\u00e4hrlich ist die That.", "tokens": ["Der", "Blick", "ver\u00b7steint", "!", "Ge\u00b7f\u00e4hr\u00b7lich", "ist", "die", "That", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$.", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die M\u00f6rderin! Sie schlie\u00dft vielleicht aus List", "tokens": ["Die", "M\u00f6r\u00b7de\u00b7rin", "!", "Sie", "schlie\u00dft", "viel\u00b7leicht", "aus", "List"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "PPER", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Die wachen Augen! Sie die grausam ist!", "tokens": ["Die", "wa\u00b7chen", "Au\u00b7gen", "!", "Sie", "die", "grau\u00b7sam", "ist", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "ART", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Durch wei\u00dfe Lider schimmert blaues Licht", "tokens": ["Durch", "wei\u00b7\u00dfe", "Li\u00b7der", "schim\u00b7mert", "blau\u00b7es", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und \u2014 zischte dort der Kopf der Natter nicht?", "tokens": ["Und", "zischte", "dort", "der", "Kopf", "der", "Nat\u00b7ter", "nicht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Medusen tr\u00e4umt da\u00df einen Kranz sie winde,", "tokens": ["Me\u00b7du\u00b7sen", "tr\u00e4umt", "da\u00df", "ei\u00b7nen", "Kranz", "sie", "win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Menschen sch\u00f6ner Liebling der sie war,", "tokens": ["Der", "Men\u00b7schen", "sch\u00f6\u00b7ner", "Lieb\u00b7ling", "der", "sie", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ART", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bevor die Stirn der G\u00f6ttin Angebinde", "tokens": ["Be\u00b7vor", "die", "Stirn", "der", "G\u00f6t\u00b7tin", "An\u00b7ge\u00b7bin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verschattet ihr mit wirrem Schlangenhaar.", "tokens": ["Ver\u00b7schat\u00b7tet", "ihr", "mit", "wir\u00b7rem", "Schlan\u00b7gen\u00b7haar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit den Gespielen glaubt sie noch zu wandern", "tokens": ["Mit", "den", "Ge\u00b7spie\u00b7len", "glaubt", "sie", "noch", "zu", "wan\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und spendet ihnen lockensch\u00fcttelnd Gr\u00fc\u00dfe,", "tokens": ["Und", "spen\u00b7det", "ih\u00b7nen", "lo\u00b7cken\u00b7sch\u00fct\u00b7telnd", "Gr\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "In bl\u00fch'ndem Reigen regt sie mit den Andern", "tokens": ["In", "bl\u00fch'n\u00b7dem", "Rei\u00b7gen", "regt", "sie", "mit", "den", "An\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die freudehellen, die beschwingten F\u00fc\u00dfe,", "tokens": ["Die", "freu\u00b7de\u00b7hel\u00b7len", ",", "die", "be\u00b7schwing\u00b7ten", "F\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ihr Antlitz hat vergessen, da\u00df es t\u00f6dte,", "tokens": ["Ihr", "Ant\u00b7litz", "hat", "ver\u00b7ges\u00b7sen", ",", "da\u00df", "es", "t\u00f6d\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Es glaubt, es glaubt an die barmherz'ge L\u00fcge", "tokens": ["Es", "glaubt", ",", "es", "glaubt", "an", "die", "barm\u00b7her\u00b7z'\u00b7ge", "L\u00fc\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Des Traums. Es lauscht dem Hauch der Hirtenfl\u00f6te,", "tokens": ["Des", "Traums", ".", "Es", "lauscht", "dem", "Hauch", "der", "Hir\u00b7ten\u00b7fl\u00f6\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Der weich melodisch zieht durch seine Z\u00fcge.", "tokens": ["Der", "weich", "me\u00b7lo\u00b7disch", "zieht", "durch", "sei\u00b7ne", "Z\u00fc\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Es l\u00e4chelt still, von schwerem Bann befreit,", "tokens": ["Es", "l\u00e4\u00b7chelt", "still", ",", "von", "schwe\u00b7rem", "Bann", "be\u00b7freit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "In unverlorner erster Lieblichkeit.", "tokens": ["In", "un\u00b7ver\u00b7lor\u00b7ner", "ers\u00b7ter", "Lieb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der M\u00f6rder tritt an ihre Seite dicht", "tokens": ["Der", "M\u00f6r\u00b7der", "tritt", "an", "ih\u00b7re", "Sei\u00b7te", "dicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und dunkler tr\u00e4umt Medusens Angesicht.", "tokens": ["Und", "dunk\u00b7ler", "tr\u00e4umt", "Me\u00b7du\u00b7sens", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VVFIN", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr ist, sie habe Ha\u00df empfunden schon,", "tokens": ["Ihr", "ist", ",", "sie", "ha\u00b7be", "Ha\u00df", "emp\u00b7fun\u00b7den", "schon", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VAFIN", "NN", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vor sich geschaudert, dumpf und bang gelitten,", "tokens": ["Vor", "sich", "ge\u00b7schau\u00b7dert", ",", "dumpf", "und", "bang", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVPP", "$,", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Die Menschen habe scheu sie erst geflohn,", "tokens": ["Die", "Men\u00b7schen", "ha\u00b7be", "scheu", "sie", "erst", "ge\u00b7flohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dann ihnen nachgestellt mit Meuchlerschritten \u2014", "tokens": ["Dann", "ih\u00b7nen", "nach\u00b7ge\u00b7stellt", "mit", "Meuch\u00b7ler\u00b7schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sie sinnt was Unheilbares sie gequ\u00e4lt,", "tokens": ["Sie", "sinnt", "was", "Un\u00b7heil\u00b7ba\u00b7res", "sie", "ge\u00b7qu\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADJA", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df sie dem eignen Leben feind geworden", "tokens": ["Da\u00df", "sie", "dem", "eig\u00b7nen", "Le\u00b7ben", "feind", "ge\u00b7wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADJD", "VAPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und andres Leben sich erg\u00f6tzt zu morden \u2014", "tokens": ["Und", "and\u00b7res", "Le\u00b7ben", "sich", "er\u00b7g\u00f6tzt", "zu", "mor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PRF", "VVPP", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Sie sinnt umsonst. Ihr h\u00e4lt's der Traum verhehlt.", "tokens": ["Sie", "sinnt", "um\u00b7sonst", ".", "Ihr", "h\u00e4lt's", "der", "Traum", "ver\u00b7hehlt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "PPER", "VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die grause Larve, die sie lang geschreckt,", "tokens": ["Die", "grau\u00b7se", "Lar\u00b7ve", ",", "die", "sie", "lang", "ge\u00b7schreckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ist wie mit einem Purpurtuch bedeckt.", "tokens": ["Ist", "wie", "mit", "ei\u00b7nem", "Pur\u00b7pur\u00b7tuch", "be\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Das Graun ist aufgel\u00f6st in Seligkeit,", "tokens": ["Das", "Graun", "ist", "auf\u00b7ge\u00b7l\u00f6st", "in", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Begonnen hat der Seele Feierzeit.", "tokens": ["Be\u00b7gon\u00b7nen", "hat", "der", "See\u00b7le", "Fei\u00b7er\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Der D\u00e4mmer herrscht. Das harte Licht verblich.", "tokens": ["Der", "D\u00e4m\u00b7mer", "herrscht", ".", "Das", "har\u00b7te", "Licht", "ver\u00b7blich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$.", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Als eine der Erl\u00f6sten f\u00fchlt sie sich.", "tokens": ["Als", "ei\u00b7ne", "der", "Er\u00b7l\u00f6s\u00b7ten", "f\u00fchlt", "sie", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "NN", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Sie f\u00fcrchtet keines Schreckens Wiederkehr,", "tokens": ["Sie", "f\u00fcrch\u00b7tet", "kei\u00b7nes", "Schre\u00b7ckens", "Wie\u00b7der\u00b7kehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Sie wei\u00df, die Qualen kommen nimmermehr,", "tokens": ["Sie", "wei\u00df", ",", "die", "Qua\u00b7len", "kom\u00b7men", "nim\u00b7mer\u00b7mehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Nein, nimmermehr, und nun ist Alles gut!", "tokens": ["Nein", ",", "nim\u00b7mer\u00b7mehr", ",", "und", "nun", "ist", "Al\u00b7les", "gut", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "$,", "KON", "ADV", "VAFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Sie liegt, den Hals gebogen, auf dem Rasen,", "tokens": ["Sie", "liegt", ",", "den", "Hals", "ge\u00b7bo\u00b7gen", ",", "auf", "dem", "Ra\u00b7sen", ","], "token_info": ["word", "word", "