{"dta.poem.12936": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie in gro\u00dfen schmertzen da-  \n nieder lag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jndem ich einen blick nach deinen sternen sende,", "tokens": ["In\u00b7dem", "ich", "ei\u00b7nen", "blick", "nach", "dei\u00b7nen", "ster\u00b7nen", "sen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schaue, wie die qval dir fast die augen bricht,", "tokens": ["Und", "schau\u00b7e", ",", "wie", "die", "qval", "dir", "fast", "die", "au\u00b7gen", "bricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ART", "NN", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So denck ich fernerweit an meine wunden nicht.", "tokens": ["So", "denck", "ich", "fer\u00b7ner\u00b7weit", "an", "mei\u00b7ne", "wun\u00b7den", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df sich dein jammer nur zu meinem hertzen wende!", "tokens": ["Da\u00df", "sich", "dein", "jam\u00b7mer", "nur", "zu", "mei\u00b7nem", "hert\u00b7zen", "wen\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So seuffz\u2019 ich, Flavia! blos um dich zu befreyn:", "tokens": ["So", "seuff\u00b7z'", "ich", ",", "Fla\u00b7via", "!", "blos", "um", "dich", "zu", "be\u00b7freyn", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "$,", "NE", "$.", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Ach wolte mich, o innigstes ergetzen!", "tokens": ["Ach", "wol\u00b7te", "mich", ",", "o", "in\u00b7nigs\u00b7tes", "er\u00b7get\u00b7zen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "FM", "FM", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Der himmel doch so w\u00fcrdig sch\u00e4tzen,", "tokens": ["Der", "him\u00b7mel", "doch", "so", "w\u00fcr\u00b7dig", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und lie\u00df in deiner angst mich gleich des todes seyn!", "tokens": ["Und", "lie\u00df", "in", "dei\u00b7ner", "angst", "mich", "gleich", "des", "to\u00b7des", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PPER", "ADV", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}