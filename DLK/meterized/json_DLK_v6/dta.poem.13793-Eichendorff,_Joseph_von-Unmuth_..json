{"dta.poem.13793": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Unmuth .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Auch mich f\u00fcllt' m\u00e4nnlich Trauern,", "tokens": ["Auch", "mich", "f\u00fcllt'", "m\u00e4nn\u00b7lich", "Trau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie Euch, bei Deutschlands Weh'n \u2014", "tokens": ["Wie", "Euch", ",", "bei", "Deutschlands", "Weh'n"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPR", "NE", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und mu\u00df in Sehnsuchts-Schauern", "tokens": ["Und", "mu\u00df", "in", "Sehn\u00b7suchts\u00b7Schau\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hier ruhmlos untergehn!", "tokens": ["Hier", "ruh\u00b7mlos", "un\u00b7ter\u00b7gehn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}