{"textgrid.poem.56836": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich ging an tr\u00e4umenden Teichen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ging an tr\u00e4umenden Teichen", "tokens": ["Ich", "ging", "an", "tr\u00e4u\u00b7men\u00b7den", "Tei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "vor\u00fcber in mondiger Nacht;", "tokens": ["vor\u00b7\u00fc\u00b7ber", "in", "mon\u00b7di\u00b7ger", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "in den fl\u00fcsternden Kronen der Eichen", "tokens": ["in", "den", "fl\u00fcs\u00b7tern\u00b7den", "Kro\u00b7nen", "der", "Ei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "spielten die Winde so sacht ...", "tokens": ["spiel\u00b7ten", "die", "Win\u00b7de", "so", "sacht", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Da umspann mich der Zauber der Stunde,", "tokens": ["Da", "um\u00b7spann", "mich", "der", "Zau\u00b7ber", "der", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "da\u00df ich hemmte den einsamen Gang \u2013", "tokens": ["da\u00df", "ich", "hemm\u00b7te", "den", "ein\u00b7sa\u00b7men", "Gang", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "nur die Nixen sangen im Grunde,", "tokens": ["nur", "die", "Ni\u00b7xen", "san\u00b7gen", "im", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "tief im Grunde,", "tokens": ["tief", "im", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "ihren leisen, dunklen Gesang.", "tokens": ["ih\u00b7ren", "lei\u00b7sen", ",", "dunk\u00b7len", "Ge\u00b7sang", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Ihr Antlitz tauchten die Sterne", "tokens": ["Ihr", "Ant\u00b7litz", "tauch\u00b7ten", "die", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "ins schauernde Wellenmeer,", "tokens": ["ins", "schau\u00b7ern\u00b7de", "Wel\u00b7len\u00b7meer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "aus duftverschleierter Ferne", "tokens": ["aus", "duft\u00b7ver\u00b7schlei\u00b7er\u00b7ter", "Fer\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.4": {"text": "gr\u00fc\u00dften die Berge her.", "tokens": ["gr\u00fc\u00df\u00b7ten", "die", "Ber\u00b7ge", "her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Kein Laut in schweigender Runde \u2013", "tokens": ["Kein", "Laut", "in", "schwei\u00b7gen\u00b7der", "Run\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "APPR", "APPR", "ADJA", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "keines V\u00f6gleins versp\u00e4teter Klang \u2013", "tokens": ["kei\u00b7nes", "V\u00f6\u00b7gleins", "ver\u00b7sp\u00e4\u00b7te\u00b7ter", "Klang", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "NN", "$("], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "nur die Nixen sangen im Grunde,", "tokens": ["nur", "die", "Ni\u00b7xen", "san\u00b7gen", "im", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "tief im Grunde,", "tokens": ["tief", "im", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "ihren leisen, dunklen Gesang.", "tokens": ["ih\u00b7ren", "lei\u00b7sen", ",", "dunk\u00b7len", "Ge\u00b7sang", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Da war mir, es k\u00e4me gezogen", "tokens": ["Da", "war", "mir", ",", "es", "k\u00e4\u00b7me", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PPER", "VVFIN", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "ein Nachen im leichten Wind", "tokens": ["ein", "Na\u00b7chen", "im", "leich\u00b7ten", "Wind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "und tr\u00fcge \u00fcber die Wogen", "tokens": ["und", "tr\u00fc\u00b7ge", "\u00fc\u00b7ber", "die", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "ein strahlendes K\u00f6nigskind ...", "tokens": ["ein", "strah\u00b7len\u00b7des", "K\u00f6\u00b7nigs\u00b7kind", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und ich rief mit bittendem Munde \u2013", "tokens": ["Und", "ich", "rief", "mit", "bit\u00b7ten\u00b7dem", "Mun\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "doch keine Antwort klang \u2013", "tokens": ["doch", "kei\u00b7ne", "Ant\u00b7wort", "klang", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "nur die Nixen sangen im Grunde,", "tokens": ["nur", "die", "Ni\u00b7xen", "san\u00b7gen", "im", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "tief im Grunde,", "tokens": ["tief", "im", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "ihren leisen, dunklen Gesang.", "tokens": ["ih\u00b7ren", "lei\u00b7sen", ",", "dunk\u00b7len", "Ge\u00b7sang", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}