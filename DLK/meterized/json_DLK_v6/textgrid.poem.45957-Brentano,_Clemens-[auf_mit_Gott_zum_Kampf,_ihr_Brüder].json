{"textgrid.poem.45957": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[auf mit Gott zum Kampf, ihr Br\u00fcder]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf mit Gott zum Kampf, ihr Br\u00fcder,", "tokens": ["Auf", "mit", "Gott", "zum", "Kampf", ",", "ihr", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "APPR", "NN", "APPRART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem Schwert und dem Gebete,", "tokens": ["Mit", "dem", "Schwert", "und", "dem", "Ge\u00b7be\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Rei\u00df den Sieg vom Himmel nieder,", "tokens": ["Rei\u00df", "den", "Sieg", "vom", "Him\u00b7mel", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deutscher, Russe, Britte, Schwede!", "tokens": ["Deut\u00b7scher", ",", "Rus\u00b7se", ",", "Brit\u00b7te", ",", "Schwe\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Helf' uns Gott, der Herr, der Hohe,", "tokens": ["Hel\u00b7f'", "uns", "Gott", ",", "der", "Herr", ",", "der", "Ho\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Der auf uns herniederschauet,", "tokens": ["Der", "auf", "uns", "her\u00b7nie\u00b7der\u00b7schau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seht schon lodern lichterlohe", "tokens": ["Seht", "schon", "lo\u00b7dern", "lich\u00b7ter\u00b7lo\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Scheiterhaufen rings erbauet.", "tokens": ["Schei\u00b7ter\u00b7hau\u00b7fen", "rings", "er\u00b7bau\u00b7et", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "In den Flammen heil'gen Zornes,", "tokens": ["In", "den", "Flam\u00b7men", "heil'\u00b7gen", "Zor\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In gerechter Rache Gluten", "tokens": ["In", "ge\u00b7rech\u00b7ter", "Ra\u00b7che", "Glu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Brennt der Busch des b\u00f6sen Dornes,", "tokens": ["Brennt", "der", "Busch", "des", "b\u00f6\u00b7sen", "Dor\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der die ganze Welt lie\u00df bluten.", "tokens": ["Der", "die", "gan\u00b7ze", "Welt", "lie\u00df", "blu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Selig, wer von ganzem Herzen", "tokens": ["Se\u00b7lig", ",", "wer", "von", "gan\u00b7zem", "Her\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles, was ihn tief verletzet,", "tokens": ["Al\u00b7les", ",", "was", "ihn", "tief", "ver\u00b7let\u00b7zet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Trauer, alle Schmerzen,", "tokens": ["Al\u00b7le", "Trau\u00b7er", ",", "al\u00b7le", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An dies heil'ge Opfer setzet.", "tokens": ["An", "dies", "heil'\u00b7ge", "Op\u00b7fer", "set\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Denn wir wollen das verbrennen,", "tokens": ["Denn", "wir", "wol\u00b7len", "das", "ver\u00b7bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PDS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was in Leib und Seel uns st\u00f6ret,", "tokens": ["Was", "in", "Leib", "und", "Seel", "uns", "st\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer kann das mit Worten nennen,", "tokens": ["Wer", "kann", "das", "mit", "Wor\u00b7ten", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PDS", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Was ihn in dem Geist emp\u00f6ret.", "tokens": ["Was", "ihn", "in", "dem", "Geist", "em\u00b7p\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Elend, Qual und Not und Frevel", "tokens": ["E\u00b7lend", ",", "Qual", "und", "Not", "und", "Fre\u00b7vel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trug und List und Hohn und L\u00fcge,", "tokens": ["Trug", "und", "List", "und", "Hohn", "und", "L\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schmolz der Feind zu gl\u00fchem Schwefel,", "tokens": ["Schmolz", "der", "Feind", "zu", "gl\u00fc\u00b7hem", "Schwe\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df die Flamme h\u00f6her schl\u00fcge.", "tokens": ["Da\u00df", "die", "Flam\u00b7me", "h\u00f6\u00b7her", "schl\u00fc\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Freudig drum ihr Kampfesbr\u00fcder,", "tokens": ["Freu\u00b7dig", "drum", "ihr", "Kamp\u00b7fes\u00b7br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PAV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlie\u00dft euch treulich um die Flammen,", "tokens": ["Schlie\u00dft", "euch", "treu\u00b7lich", "um", "die", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Brennt den Dorn zur Asche nieder,", "tokens": ["Brennt", "den", "Dorn", "zur", "A\u00b7sche", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der ein \u00d6lbaum soll entstammen.", "tokens": ["Der", "ein", "\u00d6l\u00b7baum", "soll", "ent\u00b7stam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Eine Taube soll sich schwingen", "tokens": ["Ei\u00b7ne", "Tau\u00b7be", "soll", "sich", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der Glut, soll Friedenszweige", "tokens": ["Aus", "der", "Glut", ",", "soll", "Frie\u00b7dens\u00b7zwei\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "VMFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der emp\u00f6rten Erde bringen,", "tokens": ["Der", "em\u00b7p\u00f6r\u00b7ten", "Er\u00b7de", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie aus der Zornflut steige.", "tokens": ["Da\u00df", "sie", "aus", "der", "Zorn\u00b7flut", "stei\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Friede ward umsonst verlanget,", "tokens": ["Frie\u00b7de", "ward", "um\u00b7sonst", "ver\u00b7lan\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsrer Ehr' und Freiheit Friede,", "tokens": ["Uns\u00b7rer", "Ehr'", "und", "Frei\u00b7heit", "Frie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf zum Kampf nun, wer nicht banget,", "tokens": ["Auf", "zum", "Kampf", "nun", ",", "wer", "nicht", "ban\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "ADV", "$,", "PWS", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und vor keinem G\u00f6tzen kniete.", "tokens": ["Und", "vor", "kei\u00b7nem", "G\u00f6t\u00b7zen", "knie\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Vivat alle mit einander,", "tokens": ["Vi\u00b7vat", "al\u00b7le", "mit", "ein\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "APPR", "PRF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vivat Georg und Alexander,", "tokens": ["Vi\u00b7vat", "Ge\u00b7org", "und", "A\u00b7lex\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vivat Friedrich, Vivat Franz!", "tokens": ["Vi\u00b7vat", "Fried\u00b7rich", ",", "Vi\u00b7vat", "Franz", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vivat hoch der Waffentanz!", "tokens": ["Vi\u00b7vat", "hoch", "der", "Waf\u00b7fen\u00b7tanz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Brautkranz!", "tokens": ["Braut\u00b7kranz", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Viktoria!", "tokens": ["Vik\u00b7to\u00b7ria", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Gloria!", "tokens": ["Glo\u00b7ria", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "--", "measure": "unknown.measure.zero"}}}}}