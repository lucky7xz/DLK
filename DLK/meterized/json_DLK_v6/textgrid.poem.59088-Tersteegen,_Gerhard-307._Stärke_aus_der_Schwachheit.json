{"textgrid.poem.59088": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "307. St\u00e4rke aus der Schwachheit", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "L\u00e4\u00dft Gott dich deine Ohnmacht finden,", "tokens": ["L\u00e4\u00dft", "Gott", "dich", "dei\u00b7ne", "Ohn\u00b7macht", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sollt' dein Vertrauen drum verschwinden?", "tokens": ["Sollt'", "dein", "Ver\u00b7trau\u00b7en", "drum", "ver\u00b7schwin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du h\u00e4ltst dich noch; ei, la\u00df dich fallen,", "tokens": ["Du", "h\u00e4ltst", "dich", "noch", ";", "ei", ",", "la\u00df", "dich", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "NE", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dann tr\u00e4gt und h\u00e4lt dich Gott in allen!", "tokens": ["Dann", "tr\u00e4gt", "und", "h\u00e4lt", "dich", "Gott", "in", "al\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "NN", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}