{"textgrid.poem.38509": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Schlummernde", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tausend r\u00f6thlichbes\u00e4umte Apfelbl\u00fcten", "tokens": ["Tau\u00b7send", "r\u00f6th\u00b7lich\u00b7be\u00b7s\u00e4um\u00b7te", "Ap\u00b7fel\u00b7bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["CARD", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Auf ihr blendendes Nachtkleid hingeschneiet,", "tokens": ["Auf", "ihr", "blen\u00b7den\u00b7des", "Nacht\u00b7kleid", "hin\u00b7ge\u00b7schnei\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Schlummert Minia unter Maienschatten,", "tokens": ["Schlum\u00b7mert", "Mi\u00b7nia", "un\u00b7ter", "Mai\u00b7en\u00b7schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Von der Nachtigall traulich eingefl\u00f6tet!", "tokens": ["Von", "der", "Nach\u00b7ti\u00b7gall", "trau\u00b7lich", "ein\u00b7ge\u00b7fl\u00f6\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Hingefesselten Blikes trink' ich Wonne,", "tokens": ["Hin\u00b7ge\u00b7fes\u00b7sel\u00b7ten", "Bli\u00b7kes", "trink'", "ich", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Ach! in geizigen, honigs\u00fcssen Z\u00fcgen,", "tokens": ["Ach", "!", "in", "gei\u00b7zi\u00b7gen", ",", "ho\u00b7ni\u00b7gs\u00fcs\u00b7sen", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Aus des schlafenden M\u00e4dchens holdem Antliz!", "tokens": ["Aus", "des", "schla\u00b7fen\u00b7den", "M\u00e4d\u00b7chens", "hol\u00b7dem", "Ant\u00b7liz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Horche jeglichem leisen Odemlispel,", "tokens": ["Hor\u00b7che", "jeg\u00b7li\u00b7chem", "lei\u00b7sen", "O\u00b7dem\u00b7lis\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Sp\u00e4h' des athmenden Busens s\u00fcsses Beben! \u2013", "tokens": ["Sp\u00e4h'", "des", "ath\u00b7men\u00b7den", "Bu\u00b7sens", "s\u00fcs\u00b7ses", "Be\u00b7ben", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADJA", "NN", "$.", "$("], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Sagt, ihr sch\u00fczenden Engel meines M\u00e4dchens,", "tokens": ["Sagt", ",", "ihr", "sch\u00fc\u00b7zen\u00b7den", "En\u00b7gel", "mei\u00b7nes", "M\u00e4d\u00b7chens", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Saht ihr reizender einst, in Edens Garten,", "tokens": ["Saht", "ihr", "rei\u00b7zen\u00b7der", "einst", ",", "in", "E\u00b7dens", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "$,", "APPR", "NE", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Unter bl\u00fchenden Lauben Eva schlummern?", "tokens": ["Un\u00b7ter", "bl\u00fc\u00b7hen\u00b7den", "Lau\u00b7ben", "E\u00b7va", "schlum\u00b7mern", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}}}}