{"dta.poem.7487": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "XlIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich stand auf einem Berg, da h\u00f6rt' ich singen", "tokens": ["Ich", "stand", "auf", "ei\u00b7nem", "Berg", ",", "da", "h\u00f6rt'", "ich", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur Linken pl\u00f6tzlich ernste, tr\u00fcbe Lieder;", "tokens": ["Zur", "Lin\u00b7ken", "pl\u00f6tz\u00b7lich", "erns\u00b7te", ",", "tr\u00fc\u00b7be", "Lie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Opfer war es f\u00fcr die Erde wieder,", "tokens": ["Ein", "Op\u00b7fer", "war", "es", "f\u00fcr", "die", "Er\u00b7de", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich kannte wohl der Glocken dumpfes Klingen.", "tokens": ["Ich", "kann\u00b7te", "wohl", "der", "Glo\u00b7cken", "dum\u00b7pfes", "Klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zur Rechten sah ich einen S\u00e4ugling bringen;", "tokens": ["Zur", "Rech\u00b7ten", "sah", "ich", "ei\u00b7nen", "S\u00e4ug\u00b7ling", "brin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie eines Schmetterlinges bunt Gefieder,", "tokens": ["Wie", "ei\u00b7nes", "Schmet\u00b7ter\u00b7lin\u00b7ges", "bunt", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Viel lust'ge B\u00e4nder wehten auf und nieder,", "tokens": ["Viel", "lust'\u00b7ge", "B\u00e4n\u00b7der", "weh\u00b7ten", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Gl\u00f6ckchen wollt' vor Freude schier zerspringen.", "tokens": ["Ein", "Gl\u00f6ck\u00b7chen", "wollt'", "vor", "Freu\u00b7de", "schier", "zer\u00b7sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Andacht wagt' kein Wesen rings zu st\u00f6ren:", "tokens": ["Die", "An\u00b7dacht", "wagt'", "kein", "We\u00b7sen", "rings", "zu", "st\u00f6\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Herden hielten still auf ihren Weiden,", "tokens": ["Die", "Her\u00b7den", "hiel\u00b7ten", "still", "auf", "ih\u00b7ren", "Wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie fromme Beter fl\u00fcsterten die F\u00f6hren.", "tokens": ["Wie", "from\u00b7me", "Be\u00b7ter", "fl\u00fcs\u00b7ter\u00b7ten", "die", "F\u00f6h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Als ob die Glocken sich umarmt, die Beiden,", "tokens": ["Als", "ob", "die", "Glo\u00b7cken", "sich", "um\u00b7armt", ",", "die", "Bei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "PRF", "VVPP", "$,", "PRELS", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Konnt' ich bald Einen s\u00fcssen Klang nur h\u00f6ren", "tokens": ["Konnt'", "ich", "bald", "Ei\u00b7nen", "s\u00fcs\u00b7sen", "Klang", "nur", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Tod und Leben nicht mehr unterscheiden.", "tokens": ["Und", "Tod", "und", "Le\u00b7ben", "nicht", "mehr", "un\u00b7ter\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}