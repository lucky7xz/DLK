{"textgrid.poem.55097": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Der Gott und die Bajadere", "genre": "verse", "period": "N.A.", "pub_year": 1797, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mahad\u00f6h, der Herr der Erde,", "tokens": ["Ma\u00b7ha\u00b7d\u00f6h", ",", "der", "Herr", "der", "Er\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommt herab zum sechsten Mal,", "tokens": ["Kommt", "her\u00b7ab", "zum", "sechs\u00b7ten", "Mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df er unsersgleichen werde,", "tokens": ["Da\u00df", "er", "un\u00b7sers\u00b7glei\u00b7chen", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mitzuf\u00fchlen Freud und Qual.", "tokens": ["Mit\u00b7zu\u00b7f\u00fch\u00b7len", "Freud", "und", "Qual", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Er bequemt sich, hier zu wohnen,", "tokens": ["Er", "be\u00b7quemt", "sich", ",", "hier", "zu", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "L\u00e4\u00dft sich alles selbst geschehn.", "tokens": ["L\u00e4\u00dft", "sich", "al\u00b7les", "selbst", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIS", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Soll er strafen oder schonen,", "tokens": ["Soll", "er", "stra\u00b7fen", "o\u00b7der", "scho\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mu\u00df er Menschen menschlich sehn.", "tokens": ["Mu\u00df", "er", "Men\u00b7schen", "menschlich", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "ADJD", "VVINF", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Und hat er die Stadt sich als Wandrer betrachtet,", "tokens": ["Und", "hat", "er", "die", "Stadt", "sich", "als", "Wand\u00b7rer", "be\u00b7trach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "PRF", "KOUS", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "Die Gro\u00dfen belauert, auf Kleine geachtet,", "tokens": ["Die", "Gro\u00b7\u00dfen", "be\u00b7lau\u00b7ert", ",", "auf", "Klei\u00b7ne", "ge\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "APPR", "ADJA", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.11": {"text": "Verl\u00e4\u00dft er sie abends, um weiterzugehn.", "tokens": ["Ver\u00b7l\u00e4\u00dft", "er", "sie", "a\u00b7bends", ",", "um", "wei\u00b7ter\u00b7zu\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$,", "KOUI", "VVINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Als er nun hinausgegangen,", "tokens": ["Als", "er", "nun", "hin\u00b7aus\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die letzten H\u00e4user sind,", "tokens": ["Wo", "die", "letz\u00b7ten", "H\u00e4u\u00b7ser", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieht er, mit gemalten Wangen,", "tokens": ["Sieht", "er", ",", "mit", "ge\u00b7mal\u00b7ten", "Wan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein verlornes sch\u00f6nes Kind.", "tokens": ["Ein", "ver\u00b7lor\u00b7nes", "sch\u00f6\u00b7nes", "Kind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbgr\u00fc\u00df dich, Jungfrau!\u00ab \u2013 \u00bbDank der Ehre!", "tokens": ["\u00bb", "gr\u00fc\u00df", "dich", ",", "Jung\u00b7frau", "!", "\u00ab", "\u2013", "\u00bb", "Dank", "der", "Eh\u00b7re", "!"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "$,", "NN", "$.", "$(", "$(", "$(", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wart, ich komme gleich hinaus.\u00ab", "tokens": ["Wart", ",", "ich", "kom\u00b7me", "gleich", "hin\u00b7aus", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "$,", "PPER", "VVFIN", "ADV", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "\u00bbund wer bist du?\u00ab \u2013 \u00bbBajadere,", "tokens": ["\u00bb", "und", "wer", "bist", "du", "?", "\u00ab", "\u2013", "\u00bb", "Ba\u00b7ja\u00b7de\u00b7re", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "punct"], "pos": ["$(", "KON", "PWS", "VAFIN", "PPER", "$.", "$(", "$(", "$(", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und dies ist der Liebe Haus.\u00ab", "tokens": ["Und", "dies", "ist", "der", "Lie\u00b7be", "Haus", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PDS", "VAFIN", "ART", "NN", "NN", "$.", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.9": {"text": "Sie r\u00fchrt sich, die Cymbeln zum Tanze zu schlagen;", "tokens": ["Sie", "r\u00fchrt", "sich", ",", "die", "Cym\u00b7beln", "zum", "Tan\u00b7ze", "zu", "schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "Sie wei\u00df sich so lieblich im Kreise zu tragen,", "tokens": ["Sie", "wei\u00df", "sich", "so", "lieb\u00b7lich", "im", "Krei\u00b7se", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADJD", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.11": {"text": "Sie neigt sich und biegt sich und reicht ihm den Strau\u00df.", "tokens": ["Sie", "neigt", "sich", "und", "biegt", "sich", "und", "reicht", "ihm", "den", "Strau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "KON", "VVFIN", "PRF", "KON", "VVFIN", "PPER", "ART", "NE", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Schmeichelnd zieht sie ihn zur Schwelle,", "tokens": ["Schmei\u00b7chelnd", "zieht", "sie", "ihn", "zur", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lebhaft ihn ins Haus hinein.", "tokens": ["Leb\u00b7haft", "ihn", "ins", "Haus", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbsch\u00f6ner Fremdling, lampenhelle", "tokens": ["\u00bb", "sch\u00f6\u00b7ner", "Fremd\u00b7ling", ",", "lam\u00b7pen\u00b7hel\u00b7le"], "token_info": ["punct", "word", "word", "punct", "word"], "pos": ["$(", "ADJA", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll sogleich die H\u00fctte sein.", "tokens": ["Soll", "sog\u00b7leich", "die", "H\u00fct\u00b7te", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bist du m\u00fcd, ich will dich laben,", "tokens": ["Bist", "du", "m\u00fcd", ",", "ich", "will", "dich", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lindern deiner F\u00fc\u00dfe Schmerz.", "tokens": ["Lin\u00b7dern", "dei\u00b7ner", "F\u00fc\u00b7\u00dfe", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Was du willst, das sollst du haben,", "tokens": ["Was", "du", "willst", ",", "das", "sollst", "du", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "$,", "PDS", "VMFIN", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ruhe, Freuden oder Scherz.\u00ab", "tokens": ["Ru\u00b7he", ",", "Freu\u00b7den", "o\u00b7der", "Scherz", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Sie lindert gesch\u00e4ftig geheuchelte Leiden.", "tokens": ["Sie", "lin\u00b7dert", "ge\u00b7sch\u00e4f\u00b7tig", "ge\u00b7heu\u00b7chel\u00b7te", "Lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "Der G\u00f6ttliche l\u00e4chelt; er siehet mit Freuden", "tokens": ["Der", "G\u00f6tt\u00b7li\u00b7che", "l\u00e4\u00b7chelt", ";", "er", "sie\u00b7het", "mit", "Freu\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.11": {"text": "Durch tiefes Verderben ein menschliches Herz.", "tokens": ["Durch", "tie\u00b7fes", "Ver\u00b7der\u00b7ben", "ein", "menschli\u00b7ches", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Und er fordert Sklavendienste;", "tokens": ["Und", "er", "for\u00b7dert", "Skla\u00b7ven\u00b7diens\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Immer heitrer wird sie nur,", "tokens": ["Im\u00b7mer", "hei\u00b7trer", "wird", "sie", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und des M\u00e4dchens fr\u00fche K\u00fcnste", "tokens": ["Und", "des", "M\u00e4d\u00b7chens", "fr\u00fc\u00b7he", "K\u00fcns\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werden nach und nach Natur.", "tokens": ["Wer\u00b7den", "nach", "und", "nach", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und so stellet auf die Bl\u00fcte", "tokens": ["Und", "so", "stel\u00b7let", "auf", "die", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bald und bald die Frucht sich ein;", "tokens": ["Bald", "und", "bald", "die", "Frucht", "sich", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ist Gehorsam im Gem\u00fcte,", "tokens": ["Ist", "Ge\u00b7hor\u00b7sam", "im", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wird nicht fern die Liebe sein.", "tokens": ["Wird", "nicht", "fern", "die", "Lie\u00b7be", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Aber sie sch\u00e4rfer und sch\u00e4rfer zu pr\u00fcfen,", "tokens": ["A\u00b7ber", "sie", "sch\u00e4r\u00b7fer", "und", "sch\u00e4r\u00b7fer", "zu", "pr\u00fc\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "KON", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.10": {"text": "W\u00e4hlet der Kenner der H\u00f6hen und Tiefen", "tokens": ["W\u00e4h\u00b7let", "der", "Ken\u00b7ner", "der", "H\u00f6\u00b7hen", "und", "Tie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "KON", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.11": {"text": "Lust und Entsetzen und grimmige Pein.", "tokens": ["Lust", "und", "Ent\u00b7set\u00b7zen", "und", "grim\u00b7mi\u00b7ge", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Und er k\u00fc\u00dft die bunten Wangen,", "tokens": ["Und", "er", "k\u00fc\u00dft", "die", "bun\u00b7ten", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sie f\u00fchlt der Liebe Qual,", "tokens": ["Und", "sie", "f\u00fchlt", "der", "Lie\u00b7be", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das M\u00e4dchen steht gefangen,", "tokens": ["Und", "das", "M\u00e4d\u00b7chen", "steht", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sie weint zum erstenmal;", "tokens": ["Und", "sie", "weint", "zum", "ers\u00b7ten\u00b7mal", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sinkt zu seinen F\u00fc\u00dfen nieder,", "tokens": ["Sinkt", "zu", "sei\u00b7nen", "F\u00fc\u00b7\u00dfen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht um Wollust noch Gewinst,", "tokens": ["Nicht", "um", "Wol\u00b7lust", "noch", "Ge\u00b7winst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach! und die gelenken Glieder,", "tokens": ["Ach", "!", "und", "die", "ge\u00b7len\u00b7ken", "Glie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sie versagen allen Dienst.", "tokens": ["Sie", "ver\u00b7sa\u00b7gen", "al\u00b7len", "Dienst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Und so zu des Lagers vergn\u00fcglicher Feier", "tokens": ["Und", "so", "zu", "des", "La\u00b7gers", "ver\u00b7gn\u00fcg\u00b7li\u00b7cher", "Fei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "Bereiten den dunklen, behaglichen Schleier", "tokens": ["Be\u00b7rei\u00b7ten", "den", "dunk\u00b7len", ",", "be\u00b7hag\u00b7li\u00b7chen", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.11": {"text": "Die n\u00e4chtlichen Stunden, das sch\u00f6ne Gespinst.", "tokens": ["Die", "n\u00e4cht\u00b7li\u00b7chen", "Stun\u00b7den", ",", "das", "sch\u00f6\u00b7ne", "Ge\u00b7spinst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Sp\u00e4t entschlummert unter Scherzen,", "tokens": ["Sp\u00e4t", "ent\u00b7schlum\u00b7mert", "un\u00b7ter", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fr\u00fch erwacht nach kurzer Rast,", "tokens": ["Fr\u00fch", "er\u00b7wacht", "nach", "kur\u00b7zer", "Rast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Findet sie an ihrem Herzen", "tokens": ["Fin\u00b7det", "sie", "an", "ih\u00b7rem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tot den vielgeliebten Gast.", "tokens": ["Tot", "den", "viel\u00b7ge\u00b7lieb\u00b7ten", "Gast", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schreiend st\u00fcrzt sie auf ihn nieder;", "tokens": ["Schrei\u00b7end", "st\u00fcrzt", "sie", "auf", "ihn", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber nicht erweckt sie ihn,", "tokens": ["A\u00b7ber", "nicht", "er\u00b7weckt", "sie", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und man tr\u00e4gt die starren Glieder", "tokens": ["Und", "man", "tr\u00e4gt", "die", "star\u00b7ren", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bald zur Flammengrube hin.", "tokens": ["Bald", "zur", "Flam\u00b7men\u00b7gru\u00b7be", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Sie h\u00f6ret die Priester, die Totenges\u00e4nge,", "tokens": ["Sie", "h\u00f6\u00b7ret", "die", "Pries\u00b7ter", ",", "die", "To\u00b7ten\u00b7ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "Sie raset und rennet und teilet die Menge.", "tokens": ["Sie", "ra\u00b7set", "und", "ren\u00b7net", "und", "tei\u00b7let", "die", "Men\u00b7ge", "."], "to