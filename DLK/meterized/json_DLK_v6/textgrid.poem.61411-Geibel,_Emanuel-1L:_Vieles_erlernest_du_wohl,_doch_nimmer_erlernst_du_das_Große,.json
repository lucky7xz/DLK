{"textgrid.poem.61411": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vieles erlernest du wohl, doch nimmer erlernst du das Gro\u00dfe,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vieles erlernest du wohl, doch nimmer erlernst du das Gro\u00dfe,", "tokens": ["Vie\u00b7les", "er\u00b7ler\u00b7nest", "du", "wohl", ",", "doch", "nim\u00b7mer", "er\u00b7lernst", "du", "das", "Gro\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "$,", "ADV", "ADV", "VVFIN", "PPER", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und das Gewaltige gibt einzig der Strahl der Geburt.", "tokens": ["Und", "das", "Ge\u00b7wal\u00b7ti\u00b7ge", "gibt", "ein\u00b7zig", "der", "Strahl", "der", "Ge\u00b7burt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Wem an die Wiege der Gott nicht trat mit segnender Lippe,", "tokens": ["Wem", "an", "die", "Wie\u00b7ge", "der", "Gott", "nicht", "trat", "mit", "seg\u00b7nen\u00b7der", "Lip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ART", "NN", "PTKNEG", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Nach der ", "tokens": ["Nach", "der"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "M\u00e4nnliche Tugend erringst du dir selbst, unendliches Wissen", "tokens": ["M\u00e4nn\u00b7li\u00b7che", "Tu\u00b7gend", "er\u00b7ringst", "du", "dir", "selbst", ",", "un\u00b7end\u00b7li\u00b7ches", "Wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PPER", "ADV", "$,", "ADJA", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Kaufst du mit Schwei\u00df, es gehorcht deiner Bem\u00fchung der Stoff;", "tokens": ["Kaufst", "du", "mit", "Schwei\u00df", ",", "es", "ge\u00b7horcht", "dei\u00b7ner", "Be\u00b7m\u00fc\u00b7hung", "der", "Stoff", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Aber die Bl\u00fcte des Seins \u2013 nenn's Sch\u00f6nheit, Genius, Liebe,", "tokens": ["A\u00b7ber", "die", "Bl\u00fc\u00b7te", "des", "Seins", "\u2013", "nenn's", "Sch\u00f6n\u00b7heit", ",", "Ge\u00b7nius", ",", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$(", "APPRART", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.8": {"text": "Nenn' es Begnadung \u2013 umsonst wie der ambrosische Tau,", "tokens": ["Nenn'", "es", "Be\u00b7gna\u00b7dung", "\u2013", "um\u00b7sonst", "wie", "der", "am\u00b7bro\u00b7si\u00b7sche", "Tau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$(", "ADV", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Unerbeten f\u00e4llt es herab auf die Stirn des Erw\u00e4hlten,", "tokens": ["Un\u00b7er\u00b7be\u00b7ten", "f\u00e4llt", "es", "her\u00b7ab", "auf", "die", "Stirn", "des", "Er\u00b7w\u00e4hl\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Da\u00df sie in seliger Scham unter dem Lorbeer ergl\u00fcht.", "tokens": ["Da\u00df", "sie", "in", "se\u00b7li\u00b7ger", "Scham", "un\u00b7ter", "dem", "Lor\u00b7beer", "er\u00b7gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}}}}}