{"textgrid.poem.58461": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "499. Jesus zu der Seele", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Still, liebes Kind, das Haus ist mein,", "tokens": ["Still", ",", "lie\u00b7bes", "Kind", ",", "das", "Haus", "ist", "mein", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJA", "NN", "$,", "ART", "NN", "VAFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "R\u00e4um mir dein Herz und alles ein,", "tokens": ["R\u00e4um", "mir", "dein", "Herz", "und", "al\u00b7les", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "KON", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Setz dich nur hin, schau s\u00fc\u00dfe zu,", "tokens": ["Setz", "dich", "nur", "hin", ",", "schau", "s\u00fc\u00b7\u00dfe", "zu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "$,", "ADJD", "VVFIN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Was ich im Innern bin und tu!", "tokens": ["Was", "ich", "im", "In\u00b7nern", "bin", "und", "tu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VAFIN", "KON", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}