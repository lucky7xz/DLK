{"textgrid.poem.26535": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was in der Kunst mich staunen l\u00e4\u00dft seit Jahren,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was in der Kunst mich staunen l\u00e4\u00dft seit Jahren,", "tokens": ["Was", "in", "der", "Kunst", "mich", "stau\u00b7nen", "l\u00e4\u00dft", "seit", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "PPER", "VVINF", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das ist: da\u00df an dem Edlen sie und \u00c4chten", "tokens": ["Das", "ist", ":", "da\u00df", "an", "dem", "Ed\u00b7len", "sie", "und", "\u00c4ch\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "$.", "KOUS", "APPR", "ART", "NN", "PPER", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sogleich die M\u00e4ngel seh'n, ob vor dem Schlechten", "tokens": ["Sog\u00b7leich", "die", "M\u00e4n\u00b7gel", "seh'n", ",", "ob", "vor", "dem", "Schlech\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVINF", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie stets sich auch zu vollem Beifall schaaren.", "tokens": ["Sie", "stets", "sich", "auch", "zu", "vol\u00b7lem", "Bei\u00b7fall", "schaa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wer hat es nicht zu seinem Schmerz erfahren,", "tokens": ["Wer", "hat", "es", "nicht", "zu", "sei\u00b7nem", "Schmerz", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df Thorheit ganz wie Weisheit k\u00f6nne rechten,", "tokens": ["Da\u00df", "Thor\u00b7heit", "ganz", "wie", "Weis\u00b7heit", "k\u00f6n\u00b7ne", "rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "KOKOM", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Stumpfsinn siegreich oft ein Wort verfechten,", "tokens": ["Und", "Stumpf\u00b7sinn", "sieg\u00b7reich", "oft", "ein", "Wort", "ver\u00b7fech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wor\u00fcber bei der Einsicht Zweifel waren.", "tokens": ["Wo\u00b7r\u00fc\u00b7ber", "bei", "der", "Ein\u00b7sicht", "Zwei\u00b7fel", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Stell' vor dein Bild nur immerhin den Blinden,", "tokens": ["Stell'", "vor", "dein", "Bild", "nur", "im\u00b7mer\u00b7hin", "den", "Blin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er darf getrost die Farbe dir bestreiten;", "tokens": ["Er", "darf", "ge\u00b7trost", "die", "Far\u00b7be", "dir", "be\u00b7strei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Falsch darf der Taube deine T\u00f6ne finden.", "tokens": ["Falsch", "darf", "der", "Tau\u00b7be", "dei\u00b7ne", "T\u00f6\u00b7ne", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und einer Dichtung h\u00f6chste Herrlichkeiten", "tokens": ["Und", "ei\u00b7ner", "Dich\u00b7tung", "h\u00f6chs\u00b7te", "Herr\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als baren \u00dcberschwang der Narr empfinden,", "tokens": ["Als", "ba\u00b7ren", "\u00dc\u00b7bersc\u00b7hwang", "der", "Narr", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der sich im Tollhaus K\u00f6nig d\u00fcnkt zu Zeiten.", "tokens": ["Der", "sich", "im", "Toll\u00b7haus", "K\u00f6\u00b7nig", "d\u00fcnkt", "zu", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "NN", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}