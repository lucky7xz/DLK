{"textgrid.poem.60432": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schautet ihr das Br\u00e4utchen schw\u00e4rmen", "tokens": ["Schau\u00b7tet", "ihr", "das", "Br\u00e4ut\u00b7chen", "schw\u00e4r\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf der Heid im Mondenstrahl,", "tokens": ["Auf", "der", "Heid", "im", "Mon\u00b7dens\u00b7trahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fcrdet ihr im Schlo\u00df nicht l\u00e4rmen,", "tokens": ["W\u00fcr\u00b7det", "ihr", "im", "Schlo\u00df", "nicht", "l\u00e4r\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "R\u00fcsten nicht das Hochzeitsmahl.", "tokens": ["R\u00fcs\u00b7ten", "nicht", "das", "Hoch\u00b7zeits\u00b7mahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dreier Tage galts ein Jagen,", "tokens": ["Drei\u00b7er", "Ta\u00b7ge", "galts", "ein", "Ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Scholl das Horn in Wald und Kluft,", "tokens": ["Scholl", "das", "Horn", "in", "Wald", "und", "Kluft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mancher Keuler ward erschlagen,", "tokens": ["Man\u00b7cher", "Keu\u00b7ler", "ward", "er\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "V\u00f6gel st\u00fcrzten aus der Luft.", "tokens": ["V\u00f6\u00b7gel", "st\u00fcrz\u00b7ten", "aus", "der", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und der Hirsch, der Stolz der Schluchten,", "tokens": ["Und", "der", "Hirsch", ",", "der", "Stolz", "der", "Schluch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Liegt mit zwanzig Enden kalt,", "tokens": ["Liegt", "mit", "zwan\u00b7zig", "En\u00b7den", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "CARD", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liegt, als h\u00e4tt er auf den Fluchten", "tokens": ["Liegt", ",", "als", "h\u00e4tt", "er", "auf", "den", "Fluch\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOKOM", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mitgerissen ein St\u00fcck Wald.", "tokens": ["Mit\u00b7ge\u00b7ris\u00b7sen", "ein", "St\u00fcck", "Wald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Denn zur Ehre seines Festes", "tokens": ["Denn", "zur", "Eh\u00b7re", "sei\u00b7nes", "Fes\u00b7tes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rief der Ritter in den Forst:", "tokens": ["Rief", "der", "Rit\u00b7ter", "in", "den", "Forst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bblieber Wald! heraus dein Bestes,", "tokens": ["\u00bb", "lie\u00b7ber", "Wald", "!", "he\u00b7raus", "dein", "Bes\u00b7tes", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NN", "$.", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00f6nstes an Geweih und Borst!\u00ab", "tokens": ["Sch\u00f6ns\u00b7tes", "an", "Ge\u00b7weih", "und", "Borst", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Fr\u00fch am Morgen in dem Schlosse", "tokens": ["Fr\u00fch", "am", "Mor\u00b7gen", "in", "dem", "Schlos\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Werden hundert G\u00e4ste laut,", "tokens": ["Wer\u00b7den", "hun\u00b7dert", "G\u00e4s\u00b7te", "laut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit dem Ritter, hoch zu Rosse,", "tokens": ["Mit", "dem", "Rit\u00b7ter", ",", "hoch", "zu", "Ros\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Holen sie die sch\u00f6ne Braut.", "tokens": ["Ho\u00b7len", "sie", "die", "sch\u00f6\u00b7ne", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Anna gl\u00e4nzt im Brautgeschmeide,", "tokens": ["An\u00b7na", "gl\u00e4nzt", "im", "Braut\u00b7ge\u00b7schmei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Strahlt in Sch\u00f6nheit wunderbar,", "tokens": ["Strahlt", "in", "Sch\u00f6n\u00b7heit", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df das Volk aufschreit vor Freude,", "tokens": ["Da\u00df", "das", "Volk", "auf\u00b7schreit", "vor", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo vor\u00fcberzieht die Schar.", "tokens": ["Wo", "vor\u00b7\u00fc\u00b7berz\u00b7ieht", "die", "Schar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Kein so sch\u00f6nes Weib begegnet", "tokens": ["Kein", "so", "sch\u00f6\u00b7nes", "Weib", "be\u00b7geg\u00b7net"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ADV", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heut der Sonne auf der Welt;", "tokens": ["Heut", "der", "Son\u00b7ne", "auf", "der", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Priester, wie er segnet,", "tokens": ["Und", "der", "Pries\u00b7ter", ",", "wie", "er", "seg\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Vor Erstaunen inneh\u00e4lt.", "tokens": ["Vor", "Er\u00b7stau\u00b7nen", "in\u00b7ne\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Erich, dem zur Pflicht des Weibes", "tokens": ["E\u00b7rich", ",", "dem", "zur", "Pflicht", "des", "Wei\u00b7bes"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie der Priester angetraut,", "tokens": ["Sie", "der", "Pries\u00b7ter", "an\u00b7ge\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In die Sch\u00f6nheit ihres Leibes,", "tokens": ["In", "die", "Sch\u00f6n\u00b7heit", "ih\u00b7res", "Lei\u00b7bes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seinen offnen Himmel, schaut.", "tokens": ["Sei\u00b7nen", "off\u00b7nen", "Him\u00b7mel", ",", "schaut", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Anna freut sich all des Glanzes,", "tokens": ["An\u00b7na", "freut", "sich", "all", "des", "Glan\u00b7zes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "PIAT", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihres Ritters freut sie sich,", "tokens": ["Ih\u00b7res", "Rit\u00b7ters", "freut", "sie", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihres gr\u00fcnen Myrtenkranzes,", "tokens": ["Ih\u00b7res", "gr\u00fc\u00b7nen", "Myr\u00b7ten\u00b7kran\u00b7zes", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Bald beginnt ein festlich Schmausen,", "tokens": ["Bald", "be\u00b7ginnt", "ein", "fest\u00b7lich", "Schmau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geigenschall und H\u00f6rnerklang,", "tokens": ["Gei\u00b7gen\u00b7schall", "und", "H\u00f6r\u00b7ner\u00b7klang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lebehoch! und Tanzesbrausen,", "tokens": ["Le\u00b7be\u00b7hoch", "!", "und", "Tan\u00b7zes\u00b7brau\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$.", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Becherklirren, Spiel und Sang.", "tokens": ["Be\u00b7cher\u00b7klir\u00b7ren", ",", "Spiel", "und", "Sang", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Aber als die Nacht gekommen:", "tokens": ["A\u00b7ber", "als", "die", "Nacht", "ge\u00b7kom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dicht in ihres Ohres N\u00e4h", "tokens": ["Dicht", "in", "ih\u00b7res", "Oh\u00b7res", "N\u00e4h"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6rt die sch\u00f6ne Braut, beklommen,", "tokens": ["H\u00f6rt", "die", "sch\u00f6\u00b7ne", "Braut", ",", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rauschen den bekannten See.", "tokens": ["Rau\u00b7schen", "den", "be\u00b7kann\u00b7ten", "See", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Tr\u00fcb ihr alle Kerzen flimmern,", "tokens": ["Tr\u00fcb", "ihr", "al\u00b7le", "Ker\u00b7zen", "flim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Luft wird ihr so schw\u00fcl,", "tokens": ["Und", "die", "Luft", "wird", "ihr", "so", "schw\u00fcl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Durchs Get\u00f6s das leise Wimmern", "tokens": ["Durchs", "Ge\u00b7t\u00f6s", "das", "lei\u00b7se", "Wim\u00b7mern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6rt sie von der Heidem\u00fchl.", "tokens": ["H\u00f6rt", "sie", "von", "der", "Hei\u00b7de\u00b7m\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}