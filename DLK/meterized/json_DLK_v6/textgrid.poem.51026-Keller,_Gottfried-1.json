{"textgrid.poem.51026": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es schneit und eist den ganzen Tag,", "tokens": ["Es", "schneit", "und", "eist", "den", "gan\u00b7zen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Frost erklirret scharf und blank,", "tokens": ["Der", "Frost", "er\u00b7klir\u00b7ret", "scharf", "und", "blank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie ich mich geb\u00e4rden mag \u2013", "tokens": ["Und", "wie", "ich", "mich", "ge\u00b7b\u00e4r\u00b7den", "mag", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es liegt ein M\u00e4gdlein ernstlich krank.", "tokens": ["Es", "liegt", "ein", "M\u00e4gd\u00b7lein", "ernst\u00b7lich", "krank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Roseng\u00e4rtlein ist verschneit,", "tokens": ["Das", "Ro\u00b7sen\u00b7g\u00e4rt\u00b7lein", "ist", "ver\u00b7schneit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das bl\u00fchte als ihr Angesicht,", "tokens": ["Das", "bl\u00fch\u00b7te", "als", "ihr", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch glimmt, wie aus der Ferne weit,", "tokens": ["Noch", "glimmt", ",", "wie", "aus", "der", "Fer\u00b7ne", "weit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Augen mildes Sternenlicht.", "tokens": ["Der", "Au\u00b7gen", "mil\u00b7des", "Ster\u00b7nen\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Noch ziert den Mund ein blasses Rot", "tokens": ["Noch", "ziert", "den", "Mund", "ein", "blas\u00b7ses", "Rot"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und immer eines Kusses wert;", "tokens": ["Und", "im\u00b7mer", "ei\u00b7nes", "Kus\u00b7ses", "wert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie l\u00e4\u00dft's geschehen, weil die Not", "tokens": ["Sie", "l\u00e4\u00dft's", "ge\u00b7sche\u00b7hen", ",", "weil", "die", "Not"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "VVPP", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Menschenkinder beten lehrt.", "tokens": ["Die", "Men\u00b7schen\u00b7kin\u00b7der", "be\u00b7ten", "lehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbich lieb auch deinen lieben Mund,", "tokens": ["\u00bb", "ich", "lieb", "auch", "dei\u00b7nen", "lie\u00b7ben", "Mund", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJD", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lieb deine Seele nicht allein \u2013", "tokens": ["Lieb", "dei\u00b7ne", "See\u00b7le", "nicht", "al\u00b7lein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKNEG", "ADV", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Im Fr\u00fchling wollen wir gesund", "tokens": ["Im", "Fr\u00fch\u00b7ling", "wol\u00b7len", "wir", "ge\u00b7sund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und beide wieder fr\u00f6hlich sein!", "tokens": ["Und", "bei\u00b7de", "wie\u00b7der", "fr\u00f6h\u00b7lich", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich lieb auch deiner F\u00fc\u00dfe Paar,", "tokens": ["Ich", "lieb", "auch", "dei\u00b7ner", "F\u00fc\u00b7\u00dfe", "Paar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn sie in Gras und Blumen gehn;", "tokens": ["Wenn", "sie", "in", "Gras", "und", "Blu\u00b7men", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In einem B\u00e4chlein sommerklar", "tokens": ["In", "ei\u00b7nem", "B\u00e4ch\u00b7lein", "som\u00b7mer\u00b7klar"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Will ich sie wieder baden sehn!", "tokens": ["Will", "ich", "sie", "wie\u00b7der", "ba\u00b7den", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Auf dem besonnten Kieselgrund", "tokens": ["Auf", "dem", "be\u00b7sonn\u00b7ten", "Kie\u00b7sel\u00b7grund"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stehn sie wahrhaftig wie ein Turm,", "tokens": ["Stehn", "sie", "wahr\u00b7haf\u00b7tig", "wie", "ein", "Turm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Obgleich der Kn\u00f6chel zartes Rund", "tokens": ["Ob\u00b7gleich", "der", "Kn\u00f6\u00b7chel", "zar\u00b7tes", "Rund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bedroht ein kleiner Wellensturm!\u00ab", "tokens": ["Be\u00b7droht", "ein", "klei\u00b7ner", "Wel\u00b7len\u00b7sturm", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Da scheint die Wintersonne bleich", "tokens": ["Da", "scheint", "die", "Win\u00b7ter\u00b7son\u00b7ne", "bleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durchs Fenster in den stillen Raum,", "tokens": ["Durchs", "Fens\u00b7ter", "in", "den", "stil\u00b7len", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und auf dem Glase, Zweig an Zweig,", "tokens": ["Und", "auf", "dem", "Gla\u00b7se", ",", "Zweig", "an", "Zweig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ergl\u00e4nzt ein Trauerweidenbaum!", "tokens": ["Er\u00b7gl\u00e4nzt", "ein", "Trau\u00b7er\u00b7wei\u00b7den\u00b7baum", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}