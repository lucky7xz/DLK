{"textgrid.poem.44433": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Der Selbstm\u00f6rder", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er kam zur\u00fcck nach Hause vom langgedehnten Gang,", "tokens": ["Er", "kam", "zu\u00b7r\u00fcck", "nach", "Hau\u00b7se", "vom", "lang\u00b7ge\u00b7dehn\u00b7ten", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mit Absicht ausgedehnet bis Sonnenuntergang.", "tokens": ["Mit", "Ab\u00b7sicht", "aus\u00b7ge\u00b7deh\u00b7net", "bis", "Son\u00b7nen\u00b7un\u00b7ter\u00b7gang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Geb\u00fcsch hatt er durchdrungen, wo rings kein Mensch sich zeigt,", "tokens": ["Ge\u00b7b\u00fcsch", "hatt", "er", "durch\u00b7drun\u00b7gen", ",", "wo", "rings", "kein", "Mensch", "sich", "zeigt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "$,", "PWAV", "ADV", "PIAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Am Flu\u00df war er gestanden, das Haupt hinabgeneigt.", "tokens": ["Am", "Flu\u00df", "war", "er", "ge\u00b7stan\u00b7den", ",", "das", "Haupt", "hin\u00b7ab\u00b7ge\u00b7neigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VVPP", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Und wie die Wellen liefen, so lief sein Auge mit,", "tokens": ["Und", "wie", "die", "Wel\u00b7len", "lie\u00b7fen", ",", "so", "lief", "sein", "Au\u00b7ge", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Des Menschen Los begr\u00fcbelnd, des Gl\u00fcckes fl\u00fcchtgen Schritt.", "tokens": ["Des", "Men\u00b7schen", "Los", "be\u00b7gr\u00fc\u00b7belnd", ",", "des", "Gl\u00fc\u00b7ckes", "fl\u00fccht\u00b7gen", "Schritt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVPP", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Ha, dacht er, k\u00e4m doch einer und stie\u00dfe mich hinein,", "tokens": ["Ha", ",", "dacht", "er", ",", "k\u00e4m", "doch", "ei\u00b7ner", "und", "stie\u00b7\u00dfe", "mich", "hin\u00b7ein", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "$,", "VVFIN", "ADV", "ART", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "---+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Rasch, unversehn von r\u00fcckw\u00e4rts, mir sollte wohler sein.", "tokens": ["Rasch", ",", "un\u00b7ver\u00b7sehn", "von", "r\u00fcck\u00b7w\u00e4rts", ",", "mir", "soll\u00b7te", "woh\u00b7ler", "sein", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "APPR", "ADV", "$,", "PPER", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Ja, einmal hob sein Fu\u00df sich, doch trat er schnell zur\u00fcck,", "tokens": ["Ja", ",", "ein\u00b7mal", "hob", "sein", "Fu\u00df", "sich", ",", "doch", "trat", "er", "schnell", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "PRF", "$,", "ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Denn er, der Allbelaurer, sah \u00fcbrall Lauscherblick.", "tokens": ["Denn", "er", ",", "der", "All\u00b7be\u00b7lau\u00b7rer", ",", "sah", "\u00fcb\u00b7rall", "Lau\u00b7scher\u00b7blick", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "NN", "$,", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Auch schien das All zu rufen, der Grund, der Baum, der Wald:", "tokens": ["Auch", "schien", "das", "All", "zu", "ru\u00b7fen", ",", "der", "Grund", ",", "der", "Baum", ",", "der", "Wald", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Wo wir stehn, da f\u00e4llt niemand, als zwingender Gewalt.", "tokens": ["Wo", "wir", "stehn", ",", "da", "f\u00e4llt", "nie\u00b7mand", ",", "als", "zwin\u00b7gen\u00b7der", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PIS", "$,", "KOUS", "APPR", "NN", "$."], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "So war er lang gewandelt, durch Staub, im Sonnegl\u00fchn,", "tokens": ["So", "war", "er", "lang", "ge\u00b7wan\u00b7delt", ",", "durch", "Staub", ",", "im", "Son\u00b7ne\u00b7gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$,", "APPR", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Der Mittag ohne Nahrung ging unbeachtet hin;", "tokens": ["Der", "Mit\u00b7tag", "oh\u00b7ne", "Nah\u00b7rung", "ging", "un\u00b7be\u00b7ach\u00b7tet", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Erst als die Nacht gekommen mit ihren Tropfen Taus,", "tokens": ["Erst", "als", "die", "Nacht", "ge\u00b7kom\u00b7men", "mit", "ih\u00b7ren", "Trop\u00b7fen", "Taus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VVPP", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Da dr\u00fcckt durch enge Gassen er schlotternd sich nach Haus.", "tokens": ["Da", "dr\u00fcckt", "durch", "en\u00b7ge", "Gas\u00b7sen", "er", "schlot\u00b7ternd", "sich", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "PPER", "ADJD", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Die Stiege ist erklommen, sein alter Diener tritt,", "tokens": ["Die", "Stie\u00b7ge", "ist", "er\u00b7klom\u00b7men", ",", "sein", "al\u00b7ter", "Die\u00b7ner", "tritt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Ihm \u00f6ffnend, auf die Schwelle, er kennt des Herren Schritt,", "tokens": ["Ihm", "\u00f6ff\u00b7nend", ",", "auf", "die", "Schwel\u00b7le", ",", "er", "kennt", "des", "Her\u00b7ren", "Schritt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Ein Licht h\u00e4lt er erhoben, das bebt ihm in der Hand \u2013", "tokens": ["Ein", "Licht", "h\u00e4lt", "er", "er\u00b7ho\u00b7ben", ",", "das", "bebt", "ihm", "in", "der", "Hand", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVPP", "$,", "PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Sieht er den Eingetretnen, bleich wie die blasse Wand.", "tokens": ["Sieht", "er", "den", "Ein\u00b7ge\u00b7tret\u00b7nen", ",", "bleich", "wie", "die", "blas\u00b7se", "Wand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Der auch vertr\u00e4gt den Blick nicht, der Kerze Strahlenstich,", "tokens": ["Der", "auch", "ver\u00b7tr\u00e4gt", "den", "Blick", "nicht", ",", "der", "Ker\u00b7ze", "Strah\u00b7len\u00b7stich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ART", "NN", "PTKNEG", "$,", "ART", "NN", "NE", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Macht auf die Zimmert\u00fcre und schlie\u00dft sie hinter sich.", "tokens": ["Macht", "auf", "die", "Zim\u00b7mer\u00b7t\u00fc\u00b7re", "und", "schlie\u00dft", "sie", "hin\u00b7ter", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "KON", "VVFIN", "PPER", "APPR", "PRF", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.23": {"text": "Da steht er im Gemache von Abendd\u00e4mmrung tr\u00fcb", "tokens": ["Da", "steht", "er", "im", "Ge\u00b7ma\u00b7che", "von", "A\u00b7bend\u00b7d\u00e4mm\u00b7rung", "tr\u00fcb"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Und findet alles wieder, wies fr\u00fch beim Scheiden blieb.", "tokens": ["Und", "fin\u00b7det", "al\u00b7les", "wie\u00b7der", ",", "wies", "fr\u00fch", "beim", "Schei\u00b7den", "blieb", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "$,", "VVFIN", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "Ein Stuhl liegt hingeworfen, ein Kissen auf dem Grund,", "tokens": ["Ein", "Stuhl", "liegt", "hin\u00b7ge\u00b7wor\u00b7fen", ",", "ein", "Kis\u00b7sen", "auf", "dem", "Grund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Der H\u00e4ngeleuchter schadhaft gibt eitles W\u00fcten kund,", "tokens": ["Der", "H\u00e4n\u00b7ge\u00b7leuch\u00b7ter", "schad\u00b7haft", "gibt", "eit\u00b7les", "W\u00fc\u00b7ten", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Ein kostbares Gem\u00e4lde, von Alters Rost bedampft,", "tokens": ["Ein", "kost\u00b7ba\u00b7res", "Ge\u00b7m\u00e4l\u00b7de", ",", "von", "Al\u00b7ters", "Rost", "be\u00b7dampft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Der reichen Wand entrissen und L\u00f6cher eingestampft,", "tokens": ["Der", "rei\u00b7chen", "Wand", "ent\u00b7ris\u00b7sen", "und", "L\u00f6\u00b7cher", "ein\u00b7ge\u00b7stampft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "Das Schreibpult unverschlossen. Doch schlo\u00df ers gleich nicht ab,", "tokens": ["Das", "Schreib\u00b7pult", "un\u00b7ver\u00b7schlos\u00b7sen", ".", "Doch", "schlo\u00df", "ers", "gleich", "nicht", "ab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$.", "KON", "VVFIN", "PIS", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "Es h\u00e4lt noch, was er morgens ihm zum Bewachen gab:", "tokens": ["Es", "h\u00e4lt", "noch", ",", "was", "er", "mor\u00b7gens", "ihm", "zum", "Be\u00b7wa\u00b7chen", "gab", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWS", "PPER", "ADV", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "Ein Dolch und zwei Pistolen, ein kleines Fl\u00e4schchen Gift,", "tokens": ["Ein", "Dolch", "und", "zwei", "Pis\u00b7to\u00b7len", ",", "ein", "klei\u00b7nes", "Fl\u00e4\u00b7schchen", "Gift", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "CARD", "NN", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Und dann ein Blatt, bekritzelt, er nennts Verteid'gungsschrift,", "tokens": ["Und", "dann", "ein", "Blatt", ",", "be\u00b7krit\u00b7zelt", ",", "er", "nennts", "Ver\u00b7tei\u00b7d'\u00b7gungs\u00b7schrift", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "VVFIN", "$,", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.33": {"text": "An der er lang geschrieben und mag noch schreiben leicht,", "tokens": ["An", "der", "er", "lang", "ge\u00b7schrie\u00b7ben", "und", "mag", "noch", "schrei\u00b7ben", "leicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "VVPP", "KON", "VMFIN", "ADV", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "Weil, was er heut gebilligt, er morgen fr\u00fch durchstreicht.", "tokens": ["Weil", ",", "was", "er", "heut", "ge\u00b7bil\u00b7ligt", ",", "er", "mor\u00b7gen", "fr\u00fch", "durch\u00b7streicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWS", "PPER", "ADV", "VVPP", "$,", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.35": {"text": "...", "tokens": ["..."], "token_info": ["punct"], "pos": ["$("]}}}}}