{"dta.poem.5630": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vom Geist und C\u00f6rper", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Allein,", "tokens": ["Al\u00b7lein", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Kan Zweifeln ohne Dencken seyn?", "tokens": ["Kan", "Zwei\u00b7feln", "oh\u00b7ne", "Den\u00b7cken", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich zweifle nun: so denck ich sicherlich.", "tokens": ["Ich", "zweif\u00b7le", "nun", ":", "so", "denck", "ich", "si\u00b7cher\u00b7lich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "ADV", "VVIMP", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich denck, ob ich auch sey? Dies \u00fcberf\u00fchret mich;", "tokens": ["Ich", "denck", ",", "ob", "ich", "auch", "sey", "?", "Dies", "\u00fc\u00b7berf\u00b7\u00fch\u00b7ret", "mich", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VAFIN", "$.", "PDS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich mu\u00df ja seyn, indem ich dencken kan.", "tokens": ["Ich", "mu\u00df", "ja", "seyn", ",", "in\u00b7dem", "ich", "den\u00b7cken", "kan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VAINF", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nun denck ich ja; so bin ich dann.", "tokens": ["Nun", "denck", "ich", "ja", ";", "so", "bin", "ich", "dann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "$.", "ADV", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}