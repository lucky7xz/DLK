{"dta.poem.2052": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Elephant.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Diesen macht er lang und kurz, kann ihn mannichfaltig", "tokens": ["Die\u00b7sen", "macht", "er", "lang", "und", "kurz", ",", "kann", "ihn", "man\u00b7nich\u00b7fal\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,", "VMFIN", "PPER", "ADJD"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "biegen,", "tokens": ["bie\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und auf ungez\u00e4hlte Weise von sich strecken, drehen, schmie-", "tokens": ["Und", "auf", "un\u00b7ge\u00b7z\u00e4hl\u00b7te", "Wei\u00b7se", "von", "sich", "stre\u00b7cken", ",", "dre\u00b7hen", ",", "schmie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "APPR", "PRF", "VVFIN", "$,", "VVINF", "$,", "TRUNC"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "gen,", "tokens": ["gen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Durch ihn trinken, Essen nehmen, riechen, greifen, werfen,", "tokens": ["Durch", "ihn", "trin\u00b7ken", ",", "Es\u00b7sen", "neh\u00b7men", ",", "rie\u00b7chen", ",", "grei\u00b7fen", ",", "wer\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$,", "NN", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "f\u00fchlen,", "tokens": ["f\u00fch\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Mit ihm fechten, dr\u00fccken, spr\u00fctzen, ja sogar den Ballen", "tokens": ["Mit", "ihm", "fech\u00b7ten", ",", "dr\u00fc\u00b7cken", ",", "spr\u00fct\u00b7zen", ",", "ja", "so\u00b7gar", "den", "Bal\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVINF", "$,", "VVINF", "$,", "VVFIN", "$,", "ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.8": {"text": "spielen.", "tokens": ["spie\u00b7len", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Dieses wunderbare Werkzeug, da er keinen Hals nicht", "tokens": ["Die\u00b7ses", "wun\u00b7der\u00b7ba\u00b7re", "Werk\u00b7zeug", ",", "da", "er", "kei\u00b7nen", "Hals", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "$,", "KOUS", "PPER", "PIAT", "NN", "PTKNEG"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.10": {"text": "hat,", "tokens": ["hat", ","], "token_info": ["word", "punct"], "pos": ["VAFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Und sich sonst nicht wehren k\u00f6nnte, dient ihm an des", "tokens": ["Und", "sich", "sonst", "nicht", "weh\u00b7ren", "k\u00f6nn\u00b7te", ",", "dient", "ihm", "an", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PRF", "ADV", "PTKNEG", "VVINF", "VMFIN", "$,", "VVFIN", "PPER", "APPR", "ART"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.12": {"text": "Halses Statt.", "tokens": ["Hal\u00b7ses", "Statt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Seine Haut soll so verh\u00e4rtet, und so stark fast, wie", "tokens": ["Sei\u00b7ne", "Haut", "soll", "so", "ver\u00b7h\u00e4r\u00b7tet", ",", "und", "so", "stark", "fast", ",", "wie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "VVPP", "$,", "KON", "ADV", "ADJD", "ADV", "$,", "PWAV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.14": {"text": "ein Bein,", "tokens": ["ein", "Bein", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.15": {"text": "Folglich, auch von einer Kugel nicht einmal durchdringlich", "tokens": ["Folg\u00b7lich", ",", "auch", "von", "ei\u00b7ner", "Ku\u00b7gel", "nicht", "ein\u00b7mal", "durch\u00b7dring\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "APPR", "ART", "NN", "PTKNEG", "ADV", "ADJD"], "meter": "+-+-+-+-++--+-", "measure": "trochaic.septa.relaxed"}, "line.16": {"text": "seyn.", "tokens": ["seyn", "."], "token_info": ["word", "punct"], "pos": ["VAINF", "$."], "meter": "+", "measure": "single.up"}, "line.17": {"text": "In dem warmen Indien, wo sie meist gefunden werden,", "tokens": ["In", "dem", "war\u00b7men", "In\u00b7di\u00b7en", ",", "wo", "sie", "meist", "ge\u00b7fun\u00b7den", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PWAV", "PPER", "ADV", "VVPP", "VAINF", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Weiden sie in Br\u00fcchen, W\u00e4ldern, auch auf H\u00f6h\u2019n, bey", "tokens": ["Wei\u00b7den", "sie", "in", "Br\u00fc\u00b7chen", ",", "W\u00e4l\u00b7dern", ",", "auch", "auf", "H\u00f6h'n", ",", "bey"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "PPER", "APPR", "NN", "$,", "NN", "$,", "ADV", "APPR", "NN", "$,", "APPR"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.19": {"text": "ganzen Heerden.", "tokens": ["gan\u00b7zen", "Heer\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.20": {"text": "Mit dem wei\u00dfen Elfenbein, welches sie so sch\u00fctzt, als ziert,", "tokens": ["Mit", "dem", "wei\u00b7\u00dfen", "El\u00b7fen\u00b7bein", ",", "wel\u00b7ches", "sie", "so", "sch\u00fctzt", ",", "als", "ziert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,", "KOUS", "VVFIN", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.21": {"text": "Wird ein vortheilhafter Handel in der ganzen Welt ge-", "tokens": ["Wird", "ein", "vor\u00b7theil\u00b7haf\u00b7ter", "Han\u00b7del", "in", "der", "gan\u00b7zen", "Welt", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.22": {"text": "f\u00fchrt.", "tokens": ["f\u00fchrt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.23": {"text": "Wenn wir denn von diesem Thier etwas lesen oder h\u00f6ren,", "tokens": ["Wenn", "wir", "denn", "von", "die\u00b7sem", "Thier", "et\u00b7was", "le\u00b7sen", "o\u00b7der", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PDAT", "NN", "ADV", "VVINF", "KON", "VVINF", "$,"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "La\u00dft uns, unsern Pflichten nach, seinen gro\u00dfen Sch\u00f6pfer", "tokens": ["La\u00dft", "uns", ",", "un\u00b7sern", "Pflich\u00b7ten", "nach", ",", "sei\u00b7nen", "gro\u00b7\u00dfen", "Sch\u00f6p\u00b7fer"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "PPOSAT", "NN", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.25": {"text": "ehren!", "tokens": ["eh\u00b7ren", "!"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}