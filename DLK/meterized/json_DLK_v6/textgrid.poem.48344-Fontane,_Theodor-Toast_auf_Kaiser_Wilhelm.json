{"textgrid.poem.48344": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Toast auf Kaiser Wilhelm", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An uns vor\u00fcber zog ein festlich Spiel,", "tokens": ["An", "uns", "vor\u00b7\u00fc\u00b7ber", "zog", "ein", "fest\u00b7lich", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wir sahen Freundschaft, Liebe hilfreich walten", "tokens": ["Wir", "sa\u00b7hen", "Freund\u00b7schaft", ",", "Lie\u00b7be", "hilf\u00b7reich", "wal\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und folgten gern der Dichtung sch\u00f6nem Ziel:", "tokens": ["Und", "folg\u00b7ten", "gern", "der", "Dich\u00b7tung", "sch\u00f6\u00b7nem", "Ziel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Uns der Erkenntnis Spiegel vorzuhalten;", "tokens": ["Uns", "der", "Er\u00b7kennt\u00b7nis", "Spie\u00b7gel", "vor\u00b7zu\u00b7hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein Mahnwort war's; und eh' der Vorhang fiel,", "tokens": ["Ein", "Mahn\u00b7wort", "wa\u00b7r's", ";", "und", "eh'", "der", "Vor\u00b7hang", "fiel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$.", "KON", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Erschien im Kranz erst werdender Gestalten", "tokens": ["Er\u00b7schien", "im", "Kranz", "erst", "wer\u00b7den\u00b7der", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Der erste Trinkspruch aber unsrem ", "tokens": ["Der", "ers\u00b7te", "Trinks\u00b7pruch", "a\u00b7ber", "uns\u00b7rem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dem Kaiser, ", "tokens": ["Dem", "Kai\u00b7ser", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Am Fenster steht er, gr\u00fc\u00dft uns freundlich mild,", "tokens": ["Am", "Fens\u00b7ter", "steht", "er", ",", "gr\u00fc\u00dft", "uns", "freund\u00b7lich", "mild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und jeden trifft's, als tr\u00e4f' ihn Heil und Segen;", "tokens": ["Und", "je\u00b7den", "trifft's", ",", "als", "tr\u00e4f'", "ihn", "Heil", "und", "Se\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "KOUS", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Talisman wird uns sein hehres Bild,", "tokens": ["Ein", "Ta\u00b7lis\u00b7man", "wird", "uns", "sein", "heh\u00b7res", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Ein Hoffnungskeim, den wir im Herzen hegen,", "tokens": ["Ein", "Hoff\u00b7nungs\u00b7keim", ",", "den", "wir", "im", "Her\u00b7zen", "he\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als m\u00fcsse fortan auf uns niedersteigen", "tokens": ["Als", "m\u00fcs\u00b7se", "for\u00b7tan", "auf", "uns", "nie\u00b7ders\u00b7tei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "ADV", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ein Teil der G\u00f6ttergnade, die sein eigen.", "tokens": ["Ein", "Teil", "der", "G\u00f6t\u00b7terg\u00b7na\u00b7de", ",", "die", "sein", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ja, in der Horen wechselvollem Tanz \u2013", "tokens": ["Ja", ",", "in", "der", "Ho\u00b7ren", "wech\u00b7sel\u00b7vol\u00b7lem", "Tanz", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPR", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er sprach es selbst \u2013 erfuhr er viel der Gnaden,", "tokens": ["Er", "sprach", "es", "selbst", "\u2013", "er\u00b7fuhr", "er", "viel", "der", "Gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$(", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch der ihm vorbestimmte Siegeskranz", "tokens": ["Doch", "der", "ihm", "vor\u00b7be\u00b7stimm\u00b7te", "Sie\u00b7ges\u00b7kranz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ward auch ", "tokens": ["Ward", "auch"], "token_info": ["word", "word"], "pos": ["VAFIN", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Mit Zwangeskraft, weil Pflicht sein Leben ganz,", "tokens": ["Mit", "Zwan\u00b7ges\u00b7kraft", ",", "weil", "Pflicht", "sein", "Le\u00b7ben", "ganz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUS", "NN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Hat er das Gl\u00fcck an seinen Tisch geladen,", "tokens": ["Hat", "er", "das", "Gl\u00fcck", "an", "sei\u00b7nen", "Tisch", "ge\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das Gl\u00fcck auch ", "tokens": ["Das", "Gl\u00fcck", "auch"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Er war zu gro\u00df, um Gr\u00f6\u00dfe zu beneiden.", "tokens": ["Er", "war", "zu", "gro\u00df", ",", "um", "Gr\u00f6\u00b7\u00dfe", "zu", "be\u00b7nei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "$,", "KOUI", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "An Siegen fast noch reicher als an Jahren,", "tokens": ["An", "Sie\u00b7gen", "fast", "noch", "rei\u00b7cher", "als", "an", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADV", "ADJD", "KOKOM", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kr\u00f6nt Demut ihn als seine hellste Zier,", "tokens": ["Kr\u00f6nt", "De\u00b7mut", "ihn", "als", "sei\u00b7ne", "hells\u00b7te", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "KOUS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ob H\u00f6chstes oder Tiefstes er erfahren,", "tokens": ["Ob", "H\u00f6chs\u00b7tes", "o\u00b7der", "Tiefs\u00b7tes", "er", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "KON", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er wei\u00df es wohl, der Urquell quillt nicht hier;", "tokens": ["Er", "wei\u00df", "es", "wohl", ",", "der", "Ur\u00b7quell", "quillt", "nicht", "hier", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie ruhmbegleitet seine Wege waren,", "tokens": ["Wie", "ruhm\u00b7be\u00b7glei\u00b7tet", "sei\u00b7ne", "We\u00b7ge", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Er weist hinauf: \u203a", "tokens": ["Er", "weist", "hin\u00b7auf", ":", "\u203a"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}}}}