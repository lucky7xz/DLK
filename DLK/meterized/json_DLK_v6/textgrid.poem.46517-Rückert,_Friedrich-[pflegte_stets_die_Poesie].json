{"textgrid.poem.46517": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[pflegte stets die Poesie]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Pflegte stets die Poesie", "tokens": ["Pfleg\u00b7te", "stets", "die", "Poe\u00b7sie"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Mir den Zustand zu begleiten,", "tokens": ["Mir", "den", "Zu\u00b7stand", "zu", "be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch im Anfang pflegte sie", "tokens": ["Doch", "im", "An\u00b7fang", "pfleg\u00b7te", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu begleiten ihn vom Weiten.", "tokens": ["Zu", "be\u00b7glei\u00b7ten", "ihn", "vom", "Wei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Immer n\u00e4her ist ger\u00fcckt", "tokens": ["Im\u00b7mer", "n\u00e4\u00b7her", "ist", "ge\u00b7r\u00fcckt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dann dem Zustand sein Erkennen;", "tokens": ["Dann", "dem", "Zu\u00b7stand", "sein", "Er\u00b7ken\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und nun ist es so gegl\u00fcckt,", "tokens": ["Und", "nun", "ist", "es", "so", "ge\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df nicht beide sind zu trennen.", "tokens": ["Da\u00df", "nicht", "bei\u00b7de", "sind", "zu", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PIS", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Freilich bist du selber krank,", "tokens": ["Frei\u00b7lich", "bist", "du", "sel\u00b7ber", "krank", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn du singst, wo Kinder sterben;", "tokens": ["Wenn", "du", "singst", ",", "wo", "Kin\u00b7der", "ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PWAV", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch der Krankheit sage Dank,", "tokens": ["Doch", "der", "Krank\u00b7heit", "sa\u00b7ge", "Dank", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die dir bricht des Todes Herben.", "tokens": ["Die", "dir", "bricht", "des", "To\u00b7des", "Her\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}