{"textgrid.poem.24502": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auff einen andern Beamten", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein frembder Schiffbruch hat osst andre reich gemacht/", "tokens": ["Ein", "fremb\u00b7der", "Schiff\u00b7bruch", "hat", "osst", "and\u00b7re", "reich", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "PIS", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Perlen hat ein Sturm auch an das Land gebracht.", "tokens": ["Und", "Per\u00b7len", "hat", "ein", "Sturm", "auch", "an", "das", "Land", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was wunders/ wenn dein Kopff des Landes Ruhe st\u00f6ret/", "tokens": ["Was", "wun\u00b7ders", "/", "wenn", "dein", "Kopff", "des", "Lan\u00b7des", "Ru\u00b7he", "st\u00f6\u00b7ret", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "$(", "KOUS", "PPOSAT", "NN", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil/ was sein Ungl\u00fcck macht/ dein eigen Gl\u00fcck vermehret.", "tokens": ["Weil", "/", "was", "sein", "Un\u00b7gl\u00fcck", "macht", "/", "dein", "ei\u00b7gen", "Gl\u00fcck", "ver\u00b7meh\u00b7ret", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "PWS", "PPOSAT", "NN", "VVFIN", "$(", "PPOSAT", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}