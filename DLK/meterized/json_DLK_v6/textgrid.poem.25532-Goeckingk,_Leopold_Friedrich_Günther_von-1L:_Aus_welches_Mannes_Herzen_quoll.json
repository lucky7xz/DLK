{"textgrid.poem.25532": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus welches Mannes Herzen quoll", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus welches Mannes Herzen quoll", "tokens": ["Aus", "wel\u00b7ches", "Man\u00b7nes", "Her\u00b7zen", "quoll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mehr Freundschaft in die Welt, mehr Liebe?", "tokens": ["Mehr", "Freund\u00b7schaft", "in", "die", "Welt", ",", "mehr", "Lie\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So voll des Guten ist's, so voll,", "tokens": ["So", "voll", "des", "Gu\u00b7ten", "ist's", ",", "so", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VAFIN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df mit der H\u00e4lft' es edel bliebe.", "tokens": ["Da\u00df", "mit", "der", "H\u00e4lft'", "es", "e\u00b7del", "blie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du kommst in einen fremden Kreis,", "tokens": ["Du", "kommst", "in", "ei\u00b7nen", "frem\u00b7den", "Kreis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und alle Augen sind gefangen,", "tokens": ["Und", "al\u00b7le", "Au\u00b7gen", "sind", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Du gehst, (wie gut mein Herz das wei\u00df!)", "tokens": ["Du", "gehst", ",", "(", "wie", "gut", "mein", "Herz", "das", "wei\u00df", "!", ")"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "$(", "PWAV", "ADJD", "PPOSAT", "NN", "PDS", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und jedes Herz ist mit gegangen.", "tokens": ["Und", "je\u00b7des", "Herz", "ist", "mit", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "APPR", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Doch, da\u00df der Spott, der leis' und laut", "tokens": ["Doch", ",", "da\u00df", "der", "Spott", ",", "der", "leis'", "und", "laut"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Nicht Ordensband, nicht Zepter schonet,", "tokens": ["Nicht", "Or\u00b7dens\u00b7band", ",", "nicht", "Zep\u00b7ter", "scho\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PTKNEG", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Sobald ein Thor sie tr\u00e4gt, vertraut", "tokens": ["So\u00b7bald", "ein", "Thor", "sie", "tr\u00e4gt", ",", "ver\u00b7traut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Bei so viel Tugenden noch wohnet:", "tokens": ["Bei", "so", "viel", "Tu\u00b7gen\u00b7den", "noch", "woh\u00b7net", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Das \u2013 bist du nicht zu k\u00fchn, mein Geist?", "tokens": ["Das", "\u2013", "bist", "du", "nicht", "zu", "k\u00fchn", ",", "mein", "Geist", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$(", "VAFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Vielleicht auch wohl zu weich geschaffen?", "tokens": ["Viel\u00b7leicht", "auch", "wohl", "zu", "weich", "ge\u00b7schaf\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Allein es sey! die Lieb' ist dreist;", "tokens": ["Al\u00b7lein", "es", "sey", "!", "die", "Lieb'", "ist", "dreist", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Hervor denn, Spott! mit deinen Waffen!", "tokens": ["Her\u00b7vor", "denn", ",", "Spott", "!", "mit", "dei\u00b7nen", "Waf\u00b7fen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "NN", "$.", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Hervor! du sollst mir dieses Herz", "tokens": ["Her\u00b7vor", "!", "du", "sollst", "mir", "die\u00b7ses", "Herz"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKVZ", "$.", "PPER", "VMFIN", "PPER", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Verlassen, weil es mir geh\u00f6ret;", "tokens": ["Ver\u00b7las\u00b7sen", ",", "weil", "es", "mir", "ge\u00b7h\u00f6\u00b7ret", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Zu lange hab' ich meinen Schmerz,", "tokens": ["Zu", "lan\u00b7ge", "hab'", "ich", "mei\u00b7nen", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Feind meiner Ruhe, schon gen\u00e4hret.", "tokens": ["Feind", "mei\u00b7ner", "Ru\u00b7he", ",", "schon", "ge\u00b7n\u00e4h\u00b7ret", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "ADV", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.21": {"text": "Wie? schwurest du an ", "tokens": ["Wie", "?", "schwu\u00b7rest", "du", "an"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.22": {"text": "Ha\u00df und Erniedrigung den Thoren,", "tokens": ["Ha\u00df", "und", "Er\u00b7nied\u00b7ri\u00b7gung", "den", "Tho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.23": {"text": "Wie ", "tokens": ["Wie"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.24": {"text": "Dem\u00fcthigung und Tod geschworen?", "tokens": ["De\u00b7m\u00fct\u00b7hi\u00b7gung", "und", "Tod", "ge\u00b7schwo\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Erschufst, aus einer Million", "tokens": ["Er\u00b7schufst", ",", "aus", "ei\u00b7ner", "Mil\u00b7li\u00b7on"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Von Narren, du nur einen Weisen?", "tokens": ["Von", "Nar\u00b7ren", ",", "du", "nur", "ei\u00b7nen", "Wei\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Erwarbst du einen Freund dir schon?", "tokens": ["Er\u00b7warbst", "du", "ei\u00b7nen", "Freund", "dir", "schon", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Wohlan! so will ich selbst dich preisen.", "tokens": ["Wo\u00b7hlan", "!", "so", "will", "ich", "selbst", "dich", "prei\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "VMFIN", "PPER", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Nein! Beifall l\u00e4chelt dir die Welt,", "tokens": ["Nein", "!", "Bei\u00b7fall", "l\u00e4\u00b7chelt", "dir", "die", "Welt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Doch Rache knirschen dir die Thoren,", "tokens": ["Doch", "Ra\u00b7che", "knir\u00b7schen", "dir", "die", "Tho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Und eh' dein Witz ", "tokens": ["Und", "eh'", "dein", "Witz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.32": {"text": "Sind hundert Herzen schon verloren.", "tokens": ["Sind", "hun\u00b7dert", "Her\u00b7zen", "schon", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Unwiderstehlich bist du, Spott!", "tokens": ["Un\u00b7wi\u00b7der\u00b7steh\u00b7lich", "bist", "du", ",", "Spott", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Mich, deinen Feind, kannst du bezwingen,", "tokens": ["Mich", ",", "dei\u00b7nen", "Feind", ",", "kannst", "du", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Wenn Steckenpferd und Donquixott,", "tokens": ["Wenn", "Ste\u00b7cken\u00b7pferd", "und", "Don\u00b7quix\u00b7ott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Von dir gespornt, durch Reife springen.", "tokens": ["Von", "dir", "ge\u00b7spornt", ",", "durch", "Rei\u00b7fe", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Unwiderstehlich! Dennoch spricht", "tokens": ["Un\u00b7wi\u00b7der\u00b7steh\u00b7lich", "!", "Den\u00b7noch", "spricht"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADJD", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Mein Herz zu laut: du habest M\u00e4ngel;", "tokens": ["Mein", "Herz", "zu", "laut", ":", "du", "ha\u00b7best", "M\u00e4n\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKA", "ADJD", "$.", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Nicht wahr, die Engel spotten nicht?", "tokens": ["Nicht", "wahr", ",", "die", "En\u00b7gel", "spot\u00b7ten", "nicht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "ART", "NN", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Flieh denn! so ist mein Freund ein Engel!", "tokens": ["Flieh", "denn", "!", "so", "ist", "mein", "Freund", "ein", "En\u00b7gel", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "ADV", "VAFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}