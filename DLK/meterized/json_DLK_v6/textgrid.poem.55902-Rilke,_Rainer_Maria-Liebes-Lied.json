{"textgrid.poem.55902": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Liebes-Lied", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie soll ich meine Seele halten, da\u00df", "tokens": ["Wie", "soll", "ich", "mei\u00b7ne", "See\u00b7le", "hal\u00b7ten", ",", "da\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,", "KOUS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "sie nicht an deine r\u00fchrt? Wie soll ich sie", "tokens": ["sie", "nicht", "an", "dei\u00b7ne", "r\u00fchrt", "?", "Wie", "soll", "ich", "sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PTKNEG", "APPR", "PPOSAT", "VVFIN", "$.", "PWAV", "VMFIN", "PPER", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "hinheben \u00fcber dich zu andern Dingen?", "tokens": ["hin\u00b7he\u00b7ben", "\u00fc\u00b7ber", "dich", "zu", "an\u00b7dern", "Din\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Ach gerne m\u00f6cht ich sie bei irgendwas", "tokens": ["Ach", "ger\u00b7ne", "m\u00f6cht", "ich", "sie", "bei", "ir\u00b7gend\u00b7was"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VMFIN", "PPER", "PPER", "APPR", "PIAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Verlorenem im Dunkel unterbringen", "tokens": ["Ver\u00b7lo\u00b7re\u00b7nem", "im", "Dun\u00b7kel", "un\u00b7ter\u00b7brin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "an einer fremden stillen Stelle, die", "tokens": ["an", "ei\u00b7ner", "frem\u00b7den", "stil\u00b7len", "Stel\u00b7le", ",", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,", "PRELS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "nicht weiterschwingt, wenn deine Tiefen schwingen.", "tokens": ["nicht", "wei\u00b7ter\u00b7schwingt", ",", "wenn", "dei\u00b7ne", "Tie\u00b7fen", "schwin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Doch alles, was uns anr\u00fchrt, dich und mich,", "tokens": ["Doch", "al\u00b7les", ",", "was", "uns", "an\u00b7r\u00fchrt", ",", "dich", "und", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,", "PPER", "KON", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "nimmt uns zusammen wie ein Bogenstrich,", "tokens": ["nimmt", "uns", "zu\u00b7sam\u00b7men", "wie", "ein", "Bo\u00b7gen\u00b7strich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.10": {"text": "der aus zwei Saiten ", "tokens": ["der", "aus", "zwei", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "CARD", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Auf welches Instrument sind wir gespannt?", "tokens": ["Auf", "wel\u00b7ches", "Inst\u00b7ru\u00b7ment", "sind", "wir", "ge\u00b7spannt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und welcher Geiger hat uns in der Hand?", "tokens": ["Und", "wel\u00b7cher", "Gei\u00b7ger", "hat", "uns", "in", "der", "Hand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "O s\u00fc\u00dfes Lied.", "tokens": ["O", "s\u00fc\u00b7\u00dfes", "Lied", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}