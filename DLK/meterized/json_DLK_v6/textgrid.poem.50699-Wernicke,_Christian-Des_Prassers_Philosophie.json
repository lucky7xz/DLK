{"textgrid.poem.50699": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Des Prassers Philosophie", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Klar ist die Weltweisheit, die er als neu uns schenkt,", "tokens": ["Klar", "ist", "die", "Welt\u00b7weis\u00b7heit", ",", "die", "er", "als", "neu", "uns", "schenkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "KOUS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+---+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Die er mit vollem Mund' uns aus der Sch\u00fcssel liest;", "tokens": ["Die", "er", "mit", "vol\u00b7lem", "Mund'", "uns", "aus", "der", "Sch\u00fcs\u00b7sel", "liest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es sagt ", "tokens": ["Es", "sagt"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}}}}