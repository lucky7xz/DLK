{"dta.poem.18441": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Uber der Belisse uhrgen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du weist dein uhrgen nur/ und steckst es wieder ein/", "tokens": ["Du", "weist", "dein", "uhr\u00b7gen", "nur", "/", "und", "steckst", "es", "wie\u00b7der", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "$(", "KON", "VVFIN", "PPER", "ADV", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht wissend/ da\u00df du drau\u00df die unruh hast verlohren/", "tokens": ["Nicht", "wis\u00b7send", "/", "da\u00df", "du", "drau\u00df", "die", "un\u00b7ruh", "hast", "ver\u00b7loh\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$(", "KOUS", "PPER", "PAV", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn wa\u00df es treiben soll/ darzu bin ich erkohren/", "tokens": ["Denn", "wa\u00df", "es", "trei\u00b7ben", "soll", "/", "dar\u00b7zu", "bin", "ich", "er\u00b7koh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVINF", "VMFIN", "$(", "PAV", "VAFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die r\u00e4der sind mein hertz/ die unruh meine pein.", "tokens": ["Die", "r\u00e4\u00b7der", "sind", "mein", "hertz", "/", "die", "un\u00b7ruh", "mei\u00b7ne", "pein", "."], "token_info": ["word", "word", "word", "word", "word", "