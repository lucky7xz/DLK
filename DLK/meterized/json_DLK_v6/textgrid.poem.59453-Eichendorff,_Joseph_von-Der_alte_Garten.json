{"textgrid.poem.59453": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der alte Garten", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kaiserkron und P\u00e4onien rot,", "tokens": ["Kai\u00b7ser\u00b7kron", "und", "P\u00e4o\u00b7ni\u00b7en", "rot", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die m\u00fcssen verzaubert sein,", "tokens": ["Die", "m\u00fcs\u00b7sen", "ver\u00b7zau\u00b7bert", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "VVPP", "VAINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Denn Vater und Mutter sind lange tot,", "tokens": ["Denn", "Va\u00b7ter", "und", "Mut\u00b7ter", "sind", "lan\u00b7ge", "tot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Was bl\u00fchn sie hier so allein?", "tokens": ["Was", "bl\u00fchn", "sie", "hier", "so", "al\u00b7lein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Springbrunnen plaudert noch immerfort", "tokens": ["Der", "Spring\u00b7brun\u00b7nen", "plau\u00b7dert", "noch", "im\u00b7mer\u00b7fort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Von der alten sch\u00f6nen Zeit,", "tokens": ["Von", "der", "al\u00b7ten", "sch\u00f6\u00b7nen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine Frau sitzt eingeschlafen dort,", "tokens": ["Ei\u00b7ne", "Frau", "sitzt", "ein\u00b7ge\u00b7schla\u00b7fen", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ihre Locken bedecken ihr Kleid.", "tokens": ["Ih\u00b7re", "Lo\u00b7cken", "be\u00b7de\u00b7cken", "ihr", "Kleid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Sie hat eine Laute in der Hand,", "tokens": ["Sie", "hat", "ei\u00b7ne", "Lau\u00b7te", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Als ob sie im Schlafe spricht,", "tokens": ["Als", "ob", "sie", "im", "Schla\u00b7fe", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mir ist, als h\u00e4tt ich sie sonst gekannt \u2013", "tokens": ["Mir", "ist", ",", "als", "h\u00e4tt", "ich", "sie", "sonst", "ge\u00b7kannt", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Still, geh vorbei und weck sie nicht!", "tokens": ["Still", ",", "geh", "vor\u00b7bei", "und", "weck", "sie", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "PTKVZ", "KON", "VVIMP", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und wenn es dunkelt das Tal entlang,", "tokens": ["Und", "wenn", "es", "dun\u00b7kelt", "das", "Tal", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Streift sie die Saiten sacht,", "tokens": ["Streift", "sie", "die", "Sai\u00b7ten", "sacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da gibt's einen wunderbaren Klang", "tokens": ["Da", "gibt's", "ei\u00b7nen", "wun\u00b7der\u00b7ba\u00b7ren", "Klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Durch den Garten die ganze Nacht.", "tokens": ["Durch", "den", "Gar\u00b7ten", "die", "gan\u00b7ze", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}