{"textgrid.poem.46964": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Spekbacher", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Spekbacher, der Spekbacher!", "tokens": ["Der", "Spek\u00b7ba\u00b7cher", ",", "der", "Spek\u00b7ba\u00b7cher", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Wenn der die Sch\u00fctzen rief;", "tokens": ["Wenn", "der", "die", "Sch\u00fct\u00b7zen", "rief", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Tag und Nacht und Nacht und Tag", "tokens": ["Der", "Tag", "und", "Nacht", "und", "Nacht", "und", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Feinden auf der F\u00e4hrte lag", "tokens": ["Den", "Fein\u00b7den", "auf", "der", "F\u00e4hr\u00b7te", "lag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und gar des Nachts nicht schlief.", "tokens": ["Und", "gar", "des", "Nachts", "nicht", "schlief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zum Schlafen nahm er nie sich Zeit,", "tokens": ["Zum", "Schla\u00b7fen", "nahm", "er", "nie", "sich", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "PRF", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als wenn er nachts wo ritt;", "tokens": ["Als", "wenn", "er", "nachts", "wo", "ritt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "PWAV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn dann das Pferd des Wegs fort lief,", "tokens": ["Wenn", "dann", "das", "Pferd", "des", "Wegs", "fort", "lief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "NN", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So sa\u00df der Held darauf und schlief", "tokens": ["So", "sa\u00df", "der", "Held", "da\u00b7rauf", "und", "schlief"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PAV", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und kam vom Fleck damit.", "tokens": ["Und", "kam", "vom", "Fleck", "da\u00b7mit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wenn wo kam ein Scheideweg,", "tokens": ["Und", "wenn", "wo", "kam", "ein", "Schei\u00b7de\u00b7weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PWAV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So stand der kluge Gaul;", "tokens": ["So", "stand", "der", "klu\u00b7ge", "Gaul", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aufwacht der Held, und wohlgemut,", "tokens": ["Auf\u00b7wacht", "der", "Held", ",", "und", "wohl\u00b7ge\u00b7mut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als h\u00e4tt' er recht die Nacht geruht,", "tokens": ["Als", "h\u00e4tt'", "er", "recht", "die", "Nacht", "ge\u00b7ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "War er den Tag nicht faul.", "tokens": ["War", "er", "den", "Tag", "nicht", "faul", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Spekbacher, der Spekbacher!", "tokens": ["Der", "Spek\u00b7ba\u00b7cher", ",", "der", "Spek\u00b7ba\u00b7cher", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Als er vor Kufstein lag,", "tokens": ["Als", "er", "vor", "Kufs\u00b7tein", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ging er auf Kundschaft selbst zur Stadt,", "tokens": ["Ging", "er", "auf", "Kund\u00b7schaft", "selbst", "zur", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu sehn, ob sie noch Vorrat hat", "tokens": ["Zu", "sehn", ",", "ob", "sie", "noch", "Vor\u00b7rat", "hat"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "PPER", "ADV", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sich noch halten mag.", "tokens": ["Und", "sich", "noch", "hal\u00b7ten", "mag."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["KON", "PRF", "ADV", "VVINF", "VMFIN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Und als auf ihn Verdacht gefa\u00dft", "tokens": ["Und", "als", "auf", "ihn", "Ver\u00b7dacht", "ge\u00b7fa\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "PPER", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Festung Kommandant,", "tokens": ["Der", "Fes\u00b7tung", "Kom\u00b7man\u00b7dant", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Lie\u00df er ihn hin ins Zimmer stehn,", "tokens": ["Lie\u00df", "er", "ihn", "hin", "ins", "Zim\u00b7mer", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Leuten ihn beim Licht besehn,", "tokens": ["Von", "Leu\u00b7ten", "ihn", "beim", "Licht", "be\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die ihn sonst wohl gekannt.", "tokens": ["Die", "ihn", "sonst", "wohl", "ge\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da sah der Held so mutig drein,", "tokens": ["Da", "sah", "der", "Held", "so", "mu\u00b7tig", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So feltsam ganz und gar,", "tokens": ["So", "felt\u00b7sam", "ganz", "und", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df er von keinem ward erkannt,", "tokens": ["Da\u00df", "er", "von", "kei\u00b7nem", "ward", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihn entlie\u00df der Kommandant", "tokens": ["Und", "ihn", "ent\u00b7lie\u00df", "der", "Kom\u00b7man\u00b7dant"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hinaus zu seiner Schar.", "tokens": ["Hin\u00b7aus", "zu", "sei\u00b7ner", "Schar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der Spekbacher, der Spekbacher!", "tokens": ["Der", "Spek\u00b7ba\u00b7cher", ",", "der", "Spek\u00b7ba\u00b7cher", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Wenn er zum Kampf zog aus,", "tokens": ["Wenn", "er", "zum", "Kampf", "zog", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da lief sein kleiner Bub' ihm nach,", "tokens": ["Da", "lief", "sein", "klei\u00b7ner", "Bub'", "ihm", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und was der Vater droht' und sprach,", "tokens": ["Und", "was", "der", "Va\u00b7ter", "droht'", "und", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er blieb doch nicht zu Haus.", "tokens": ["Er", "blieb", "doch", "nicht", "zu", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "In das Gewehrfeu'r lief er 'nein,", "tokens": ["In", "das", "Ge\u00b7wehr\u00b7feu'r", "lief", "er", "'n\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Da wies man ihn hinaus;", "tokens": ["Da", "wies", "man", "ihn", "hin\u00b7aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da macht' sich seitw\u00e4rts hin der Bub',", "tokens": ["Da", "macht'", "sich", "seit\u00b7w\u00e4rts", "hin", "der", "Bub'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo Kugeln schlugen ein, die grub", "tokens": ["Wo", "Ku\u00b7geln", "schlu\u00b7gen", "ein", ",", "die", "grub"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "PTKVZ", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er mit dem Messer aus.", "tokens": ["Er", "mit", "dem", "Mes\u00b7ser", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und wie er sieht, den Sch\u00fctzen fehlt", "tokens": ["Und", "wie", "er", "sieht", ",", "den", "Sch\u00fct\u00b7zen", "fehlt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es an Munition,", "tokens": ["Es", "an", "Mu\u00b7ni\u00b7tion", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "L\u00e4uft er damit hinein ins Glied", "tokens": ["L\u00e4uft", "er", "da\u00b7mit", "hin\u00b7ein", "ins", "Glied"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PAV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bringt, da\u00df es sein Vater sieht,", "tokens": ["Und", "bringt", ",", "da\u00df", "es", "sein", "Va\u00b7ter", "sieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sein H\u00fctlein voll davon.", "tokens": ["Sein", "H\u00fct\u00b7lein", "voll", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Der Spekbacher, der Spekbacher!", "tokens": ["Der", "Spek\u00b7ba\u00b7cher", ",", "der", "Spek\u00b7ba\u00b7cher", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Als es nun lang' gew\u00e4hrt,", "tokens": ["Als", "es", "nun", "lang'", "ge\u00b7w\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Held nun gehn mu\u00dft' auf die Flucht,", "tokens": ["Der", "Held", "nun", "gehn", "mu\u00dft'", "auf", "die", "Flucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "VMFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ward er von Reitern aufgesucht,", "tokens": ["Ward", "er", "von", "Rei\u00b7tern", "auf\u00b7ge\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcr vogelfrei erkl\u00e4rt.", "tokens": ["F\u00fcr", "vo\u00b7gel\u00b7frei", "er\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Im Winter tief im Schneegebirg'", "tokens": ["Im", "Win\u00b7ter", "tief", "im", "Schnee\u00b7ge\u00b7bir\u00b7g'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00dft' er umirren gehn;", "tokens": ["Mu\u00dft'", "er", "um\u00b7ir\u00b7ren", "gehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als er sich in das Wetterloch", "tokens": ["Als", "er", "sich", "in", "das", "Wet\u00b7ter\u00b7loch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In seiner h\u00f6chsten Not verkroch,", "tokens": ["In", "sei\u00b7ner", "h\u00f6chs\u00b7ten", "Not", "ver\u00b7kroch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hatt' er viel auszustehn.", "tokens": ["Hatt'", "er", "viel", "aus\u00b7zu\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.12": {"line.1": {"text": "Im Mute der Verzweifelung", "tokens": ["Im", "Mu\u00b7te", "der", "Ver\u00b7zwei\u00b7fe\u00b7lung"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Trieb's ihn zuletzt heraus;", "tokens": ["Trie\u00b7b's", "ihn", "zu\u00b7letzt", "he\u00b7raus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Er wagt's, ins Thal hinabzugehn,", "tokens": ["Er", "wagt's", ",", "ins", "Thal", "hin\u00b7ab\u00b7zu\u00b7gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPRART", "NN", "VVINF",