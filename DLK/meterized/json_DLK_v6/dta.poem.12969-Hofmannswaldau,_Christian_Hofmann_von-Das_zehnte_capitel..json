{"dta.poem.12969": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Das zehnte capitel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Entfleuch der eitelkeit des irrdischen get\u00fcmmels,", "tokens": ["Ent\u00b7fleuch", "der", "ei\u00b7tel\u00b7keit", "des", "irr\u00b7di\u00b7schen", "ge\u00b7t\u00fcm\u00b7mels", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und meide, wie du kanst, die h\u00e4ndel dieser welt!", "tokens": ["Und", "mei\u00b7de", ",", "wie", "du", "kanst", ",", "die", "h\u00e4n\u00b7del", "die\u00b7ser", "welt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "VMFIN", "$,", "PRELS", "ADJD", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn man vergi\u00dft dabey gemeiniglich des himmels,", "tokens": ["Denn", "man", "ver\u00b7gi\u00dft", "da\u00b7bey", "ge\u00b7mei\u00b7nig\u00b7lich", "des", "him\u00b7mels", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PAV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ob man gleich sein gespr\u00e4ch in lautrer einfalt h\u00e4lt.", "tokens": ["Ob", "man", "gleich", "sein", "ge\u00b7spr\u00e4ch", "in", "laut\u00b7rer", "ein\u00b7falt", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PPOSAT", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn wir der eitelkeit schon unsre worte schencken,", "tokens": ["Wenn", "wir", "der", "ei\u00b7tel\u00b7keit", "schon", "uns\u00b7re", "wor\u00b7te", "schen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So spielet sie sich leicht auch in die thaten ein,", "tokens": ["So", "spie\u00b7let", "sie", "sich", "leicht", "auch", "in", "die", "tha\u00b7ten", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "ADV", "APPR", "ART", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und also k\u00f6nnen wir, eh\u2019 wir daran gedencken,", "tokens": ["Und", "al\u00b7so", "k\u00f6n\u00b7nen", "wir", ",", "eh'", "wir", "da\u00b7ran", "ge\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "$,", "KOUS", "PPER", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Des Satans und der welt leib-eigne sclaven seyn.", "tokens": ["Des", "Sa\u00b7tans", "und", "der", "welt", "leib\u00b7eig\u00b7ne", "scla\u00b7ven", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich sprach offt bey mir selbst: Ach h\u00e4ttest du geschwiegen,", "tokens": ["Ich", "sprach", "offt", "bey", "mir", "selbst", ":", "Ach", "h\u00e4t\u00b7test", "du", "ge\u00b7schwie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "ADV", "$.", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und von den menschen dich in eine w\u00fcst\u2019 entfernt!", "tokens": ["Und", "von", "den", "men\u00b7schen", "dich", "in", "ei\u00b7ne", "w\u00fcst'", "ent\u00b7fernt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PRF", "APPR", "ART", "VVFIN", "ADJD", "$."], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Was findet doch ein mensch im schwatzen vor vergn\u00fcgen,", "tokens": ["Was", "fin\u00b7det", "doch", "ein", "mensch", "im", "schwat\u00b7zen", "vor", "ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN", "APPRART", "VVFIN", "APPR", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Das man mit so viel angst, m\u00fch und gefahr erlernt.", "tokens": ["Das", "man", "mit", "so", "viel", "angst", ",", "m\u00fch", "und", "ge\u00b7fahr", "er\u00b7lernt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "APPR", "ADV", "ADV", "VVPP", "$,", "ADJD", "KON", "NN", "VVPP", "$."], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Das macht es, da\u00df wir uns so gerne tr\u00f6sten lassen,", "tokens": ["Das", "macht", "es", ",", "da\u00df", "wir", "uns", "so", "ger\u00b7ne", "tr\u00f6s\u00b7ten", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "ADV", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und unsrer creutzigung bald \u00fcberdr\u00fc\u00dfig sind.", "tokens": ["Und", "uns\u00b7rer", "creut\u00b7zi\u00b7gung", "bald", "\u00fc\u00b7berd\u00b7r\u00fc\u00b7\u00dfig", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es meynt der schwache geist sich wiederum zu fassen,", "tokens": ["Es", "meynt", "der", "schwa\u00b7che", "geist", "sich", "wie\u00b7de\u00b7rum", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PRF", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So bald ein lieber mund sich aufzuthun beginnt.", "tokens": ["So", "bald", "ein", "lie\u00b7ber", "mund", "sich", "auf\u00b7zu\u00b7thun", "be\u00b7ginnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADV", "VVFIN", "PRF", "VVIZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wir reden allzugern von dingen, die wir lieben,", "tokens": ["Wir", "re\u00b7den", "all\u00b7zu\u00b7gern", "von", "din\u00b7gen", ",", "die", "wir", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und klagen, da\u00df wir nur auf dorn und disteln ruhn,", "tokens": ["Und", "kla\u00b7gen", ",", "da\u00df", "wir", "nur", "auf", "dorn", "und", "dis\u00b7teln", "ruhn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "KOUS", "PPER", "ADV", "APPR", "NE", "KON", "NN", "VVINF", "$,"], "meter": "-+-++-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Damit wir unsern freund durch unser ach betr\u00fcben.", "tokens": ["Da\u00b7mit", "wir", "un\u00b7sern", "freund", "durch", "un\u00b7ser", "ach", "be\u00b7tr\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ach da\u00df wir menschen doch so unvern\u00fcnfftig thun!", "tokens": ["Ach", "da\u00df", "wir", "men\u00b7schen", "doch", "so", "un\u00b7ver\u00b7n\u00fcnff\u00b7tig", "thun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPER", "VVFIN", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der \u00e4userliche trost ist uns gewi\u00df nichts n\u00fctze,", "tokens": ["Der", "\u00e4u\u00b7ser\u00b7li\u00b7che", "trost", "ist", "uns", "ge\u00b7wi\u00df", "nichts", "n\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Er h\u00e4lt vielmehr die krafft des innern trostes auf:", "tokens": ["Er", "h\u00e4lt", "viel\u00b7mehr", "die", "krafft", "des", "in\u00b7nern", "tros\u00b7tes", "auf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Drum, seele! wache, bet\u2019 und d\u00e4mpffe deine hitze;", "tokens": ["Drum", ",", "see\u00b7le", "!", "wa\u00b7che", ",", "bet'", "und", "d\u00e4mpf\u00b7fe", "dei\u00b7ne", "hit\u00b7ze", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "VVFIN", "$.", "ADJA", "$,", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Du weist, die theure zeit hat einen schnellen lauff.", "tokens": ["Du", "weist", ",", "die", "theu\u00b7re", "zeit", "hat", "ei\u00b7nen", "schnel\u00b7len", "lauff", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Befiehlt dir deine pflicht, das schweigen aufzuheben,", "tokens": ["Be\u00b7fiehlt", "dir", "dei\u00b7ne", "pflicht", ",", "das", "schwei\u00b7gen", "auf\u00b7zu\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,", "ART", "ADJA", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "So siehe, da\u00df dein mund klug und behutsam sey:", "tokens": ["So", "sie\u00b7he", ",", "da\u00df", "dein", "mund", "klug", "und", "be\u00b7hut\u00b7sam", "sey", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ein wohlerwogner spruch kan gro\u00dfen nutzen geben,", "tokens": ["Ein", "woh\u00b7ler\u00b7wog\u00b7ner", "spruch", "kan", "gro\u00b7\u00dfen", "nut\u00b7zen", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und ein gelindes wort legt manches \u00fcbel bey.", "tokens": ["Und", "ein", "ge\u00b7lin\u00b7des", "wort", "legt", "man\u00b7ches", "\u00fc\u00b7bel", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PIS", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Da\u00df man die zunge nicht so, wie man soll, bewahret,", "tokens": ["Da\u00df", "man", "die", "zun\u00b7ge", "nicht", "so", ",", "wie", "man", "soll", ",", "be\u00b7wah\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "PTKNEG", "ADV", "$,", "PWAV", "PIS", "VMFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und manchmal \u00fcber sie bis in die h\u00f6lle f\u00e4llt;", "tokens": ["Und", "manch\u00b7mal", "\u00fc\u00b7ber", "sie", "bis", "in", "die", "h\u00f6l\u00b7le", "f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "ADV", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Das thut die sicherheit, da\u00df man sich nichts befahret,", "tokens": ["Das", "thut", "die", "si\u00b7cher\u00b7heit", ",", "da\u00df", "man", "sich", "nichts", "be\u00b7fah\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,", "KOUS", "PIS", "PRF", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ob gleich der feind sein garn uns vor den augen stellt.", "tokens": ["Ob", "gleich", "der", "feind", "sein", "garn", "uns", "vor", "den", "au\u00b7gen", "stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPOSAT", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Drum siehe dich wohl vor! und schweigst du schon zu zeiten,", "tokens": ["Drum", "sie\u00b7he", "dich", "wohl", "vor", "!", "und", "schweigst", "du", "schon", "zu", "zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "KON", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "So la\u00df doch deinen mund nicht unaufh\u00f6rlich ruhn;", "tokens": ["So", "la\u00df", "doch", "dei\u00b7nen", "mund", "nicht", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "ruhn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "PPOSAT", "NN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Weil geistliche gespr\u00e4ch und reden unter leuten,", "tokens": ["Weil", "geist\u00b7li\u00b7che", "ge\u00b7spr\u00e4ch", "und", "re\u00b7den", "un\u00b7ter", "leu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Die gleiches sinnes sind, viel zur erbauung thun.", "tokens": ["Die", "glei\u00b7ches", "sin\u00b7nes", "sind", ",", "viel", "zur", "er\u00b7bau\u00b7ung", "thun", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "VAFIN", "$,", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}