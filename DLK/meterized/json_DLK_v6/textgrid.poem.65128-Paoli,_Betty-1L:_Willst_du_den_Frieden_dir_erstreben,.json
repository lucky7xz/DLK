{"textgrid.poem.65128": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Willst du den Frieden dir erstreben,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du den Frieden dir erstreben,", "tokens": ["Willst", "du", "den", "Frie\u00b7den", "dir", "er\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der aller G\u00fcter h\u00f6chstes hei\u00dft,", "tokens": ["Der", "al\u00b7ler", "G\u00fc\u00b7ter", "h\u00f6chs\u00b7tes", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So sieh in Welt, Natur und Leben", "tokens": ["So", "sieh", "in", "Welt", ",", "Na\u00b7tur", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur einzig Stoff f\u00fcr deinen Geist!", "tokens": ["Nur", "ein\u00b7zig", "Stoff", "f\u00fcr", "dei\u00b7nen", "Geist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Bew\u00e4lt'ge alle Schickssalsspenden,", "tokens": ["Be\u00b7w\u00e4lt'\u00b7ge", "al\u00b7le", "Schicks\u00b7sals\u00b7spen\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Gl\u00fcck, das Leid, mit jener Macht,", "tokens": ["Das", "Gl\u00fcck", ",", "das", "Leid", ",", "mit", "je\u00b7ner", "Macht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die in des Bildners weisen H\u00e4nden", "tokens": ["Die", "in", "des", "Bild\u00b7ners", "wei\u00b7sen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus rohem Marmor G\u00f6tter schafft!", "tokens": ["Aus", "ro\u00b7hem", "Mar\u00b7mor", "G\u00f6t\u00b7ter", "schafft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O schwinge die gefeite Wehre,", "tokens": ["O", "schwin\u00b7ge", "die", "ge\u00b7fei\u00b7te", "Weh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die huldvoll dir ein Gott geschenkt,", "tokens": ["Die", "huld\u00b7voll", "dir", "ein", "Gott", "ge\u00b7schenkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df rein zum Kunstwerk sich verkl\u00e4re,", "tokens": ["Da\u00df", "rein", "zum", "Kunst\u00b7werk", "sich", "ver\u00b7kl\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was in dir athmet, f\u00fchlt und denkt!", "tokens": ["Was", "in", "dir", "ath\u00b7met", ",", "f\u00fchlt", "und", "denkt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und strahlt das Werk voll Gr\u00f6\u00df' und Milde", "tokens": ["Und", "strahlt", "das", "Werk", "voll", "Gr\u00f6\u00df'", "und", "Mil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In der Vollendung heiterm Licht,", "tokens": ["In", "der", "Vol\u00b7len\u00b7dung", "hei\u00b7term", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Was thut es, wenn vor seinem Bilde", "tokens": ["Was", "thut", "es", ",", "wenn", "vor", "sei\u00b7nem", "Bil\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der K\u00fcnstler todt zusammenbricht?! \u2013", "tokens": ["Der", "K\u00fcnst\u00b7ler", "todt", "zu\u00b7sam\u00b7men\u00b7bricht", "?!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}