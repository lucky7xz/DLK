{"dta.poem.23314": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Bauernglaube.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr g\u00f6nnt uns wohl das Himmelsheil,", "tokens": ["Ihr", "g\u00f6nnt", "uns", "wohl", "das", "Him\u00b7mels\u00b7heil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "G\u00f6nnt jedem daran gleichen Theil:", "tokens": ["G\u00f6nnt", "je\u00b7dem", "da\u00b7ran", "glei\u00b7chen", "Theil", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PAV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Heil der Erde ward Regal,", "tokens": ["Das", "Heil", "der", "Er\u00b7de", "ward", "Re\u00b7gal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns blieb allein der Erde Qual.", "tokens": ["Uns", "blieb", "al\u00b7lein", "der", "Er\u00b7de", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was baut ihr neue Kirchen doch!", "tokens": ["Was", "baut", "ihr", "neu\u00b7e", "Kir\u00b7chen", "doch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir finden unsern Herrgott noch.", "tokens": ["Wir", "fin\u00b7den", "un\u00b7sern", "Herr\u00b7gott", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O baut ein einzig Haus einmal,", "tokens": ["O", "baut", "ein", "ein\u00b7zig", "Haus", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJD", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Drin wir vergessen unsre Qual.", "tokens": ["Drin", "wir", "ver\u00b7ges\u00b7sen", "uns\u00b7re", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}