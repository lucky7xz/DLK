{"textgrid.poem.30707": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, wann wir solten sehen,", "tokens": ["Ach", ",", "wann", "wir", "sol\u00b7ten", "se\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was uns k\u00fcnt offt geschehen,", "tokens": ["Was", "uns", "k\u00fcnt", "offt", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wann nicht de\u00df Himmels W\u00e4chter", "tokens": ["Wann", "nicht", "de\u00df", "Him\u00b7mels", "W\u00e4ch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PTKNEG", "ART", "NN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Uns st\u00fcnden f\u00fcr Verfechter,", "tokens": ["Uns", "st\u00fcn\u00b7den", "f\u00fcr", "Ver\u00b7fech\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wir w\u00fcrden uns entsetzen,", "tokens": ["Wir", "w\u00fcr\u00b7den", "uns", "ent\u00b7set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und f\u00fcr gar bl\u00f6de sch\u00e4tzen!", "tokens": ["Und", "f\u00fcr", "gar", "bl\u00f6\u00b7de", "sch\u00e4t\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "ADJA", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Hingegen wann von jenen", "tokens": ["Hin\u00b7ge\u00b7gen", "wann", "von", "je\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PWAV", "APPR", "PDAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wir h\u00f6rten auch erwehnen,", "tokens": ["Wir", "h\u00f6r\u00b7ten", "auch", "er\u00b7weh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Mit was f\u00fcr Thurst und R\u00e4ncken", "tokens": ["Mit", "was", "f\u00fcr", "Thurst", "und", "R\u00e4n\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Wir Gott und sie so kr\u00e4ncken,", "tokens": ["Wir", "Gott", "und", "sie", "so", "kr\u00e4n\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Wie w\u00fcrden wir erzittern", "tokens": ["Wie", "w\u00fcr\u00b7den", "wir", "er\u00b7zit\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Und f\u00fcr der Straff uns sch\u00fcttern!", "tokens": ["Und", "f\u00fcr", "der", "Straff", "uns", "sch\u00fct\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Wer keines wil bewegen,", "tokens": ["Wer", "kei\u00b7nes", "wil", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Der wird sich letzlich legen", "tokens": ["Der", "wird", "sich", "letz\u00b7lich", "le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PRF", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Ins Bette, wo die Flammen", "tokens": ["Ins", "Bet\u00b7te", ",", "wo", "die", "Flam\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Gehn \u00fcber ihn zusammen.", "tokens": ["Gehn", "\u00fc\u00b7ber", "ihn", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}