{"textgrid.poem.29663": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "78.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Jungfern, seht euch f\u00fcr; habt Achtung auff das schreiben!", "tokens": ["Ihr", "Jung\u00b7fern", ",", "seht", "euch", "f\u00fcr", ";", "habt", "Ach\u00b7tung", "auff", "das", "schrei\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "APPR", "$.", "VAFIN", "NN", "APPR", "ART", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Zeit ist wunderbar, pflegt List und Kunst zu treiben!", "tokens": ["Die", "Zeit", "ist", "wun\u00b7der\u00b7bar", ",", "pflegt", "List", "und", "Kunst", "zu", "trei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "VVFIN", "NE", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Cornelius war weg und kunte doch zwey Erben", "tokens": ["Cor\u00b7ne\u00b7li\u00b7us", "war", "weg", "und", "kun\u00b7te", "doch", "zwey", "Er\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADV", "KON", "VMFIN", "ADV", "CARD", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Vom Weibe durch drey Jahr mit Briefen nur erwerben.", "tokens": ["Vom", "Wei\u00b7be", "durch", "drey", "Jahr", "mit", "Brie\u00b7fen", "nur", "er\u00b7wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "CARD", "NN", "APPR", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}