{"textgrid.poem.67178": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hinunter sinket der Wald,", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hinunter sinket der Wald,", "tokens": ["Hin\u00b7un\u00b7ter", "sin\u00b7ket", "der", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Und Knospen \u00e4hnlich, h\u00e4ngen", "tokens": ["Und", "Knos\u00b7pen", "\u00e4hn\u00b7lich", ",", "h\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KON", "NN", "ADJD", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Einw\u00e4rts die Bl\u00e4tter, denen", "tokens": ["Ein\u00b7w\u00e4rts", "die", "Bl\u00e4t\u00b7ter", ",", "de\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "ART", "NN", "$,", "PRELS"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Bl\u00fcht unten auf ein Grund,", "tokens": ["Bl\u00fcht", "un\u00b7ten", "auf", "ein", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nicht gar unm\u00fcndig.", "tokens": ["Nicht", "gar", "un\u00b7m\u00fcn\u00b7dig", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Da n\u00e4mlich ist Ulrich", "tokens": ["Da", "n\u00e4m\u00b7lich", "ist", "Ul\u00b7rich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gegangen; oft sinnt, \u00fcber den Fu\u00dftritt,", "tokens": ["Ge\u00b7gan\u00b7gen", ";", "oft", "sinnt", ",", "\u00fc\u00b7ber", "den", "Fu\u00df\u00b7tritt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Ein gro\u00df Schicksal", "tokens": ["Ein", "gro\u00df", "Schick\u00b7sal"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Bereit, an \u00fcbrigem Orte.", "tokens": ["Be\u00b7reit", ",", "an", "\u00fcb\u00b7ri\u00b7gem", "Or\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}