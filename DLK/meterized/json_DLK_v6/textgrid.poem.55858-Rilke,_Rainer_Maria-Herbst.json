{"textgrid.poem.55858": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Herbst", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Bl\u00e4tter fallen, fallen wie von weit,", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "fal\u00b7len", ",", "fal\u00b7len", "wie", "von", "weit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "VVFIN", "KOKOM", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "als welkten in den Himmeln ferne G\u00e4rten;", "tokens": ["als", "welk\u00b7ten", "in", "den", "Him\u00b7meln", "fer\u00b7ne", "G\u00e4r\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "sie fallen mit verneinender Geb\u00e4rde.", "tokens": ["sie", "fal\u00b7len", "mit", "ver\u00b7nei\u00b7nen\u00b7der", "Ge\u00b7b\u00e4r\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und in den N\u00e4chten f\u00e4llt die schwere Erde", "tokens": ["Und", "in", "den", "N\u00e4ch\u00b7ten", "f\u00e4llt", "die", "schwe\u00b7re", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "aus allen Sternen in die Einsamkeit.", "tokens": ["aus", "al\u00b7len", "Ster\u00b7nen", "in", "die", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wir alle fallen. Diese Hand da f\u00e4llt.", "tokens": ["Wir", "al\u00b7le", "fal\u00b7len", ".", "Die\u00b7se", "Hand", "da", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVINF", "$.", "PDAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und sieh dir andre an: es ist in allen.", "tokens": ["Und", "sieh", "dir", "and\u00b7re", "an", ":", "es", "ist", "in", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PIS", "PTKVZ", "$.", "PPER", "VAFIN", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und doch ist Einer, welcher dieses Fallen", "tokens": ["Und", "doch", "ist", "Ei\u00b7ner", ",", "wel\u00b7cher", "die\u00b7ses", "Fal\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PIS", "$,", "PRELS", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "unendlich sanft in seinen H\u00e4nden h\u00e4lt.", "tokens": ["un\u00b7end\u00b7lich", "sanft", "in", "sei\u00b7nen", "H\u00e4n\u00b7den", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}