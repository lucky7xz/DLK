{"textgrid.poem.36085": {"metadata": {"author": {"name": "Bechstein, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "XlVI. Abschied.", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbund heute soll ich sterben! Soll! Muss! Die Zeit ist aus!", "tokens": ["\u00bb", "und", "heu\u00b7te", "soll", "ich", "ster\u00b7ben", "!", "Soll", "!", "Muss", "!", "Die", "Zeit", "ist", "aus", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADV", "VMFIN", "PPER", "VVINF", "$.", "VMFIN", "$.", "NN", "$.", "ART", "NN", "VAFIN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "So brich herein, Verderben, im Donnersturmgebraus!", "tokens": ["So", "brich", "her\u00b7ein", ",", "Ver\u00b7der\u00b7ben", ",", "im", "Don\u00b7ner\u00b7sturm\u00b7ge\u00b7braus", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$,", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "St\u00fcrz' \u00fcber mich zusammen, Geb\u00e4u! Brich, Mauerwand!", "tokens": ["St\u00fcrz'", "\u00fc\u00b7ber", "mich", "zu\u00b7sam\u00b7men", ",", "Ge\u00b7b\u00e4u", "!", "Brich", ",", "Mau\u00b7er\u00b7wand", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PTKVZ", "$,", "NN", "$.", "NE", "$,", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Loh' auf in H\u00f6llenflammen! Begrabt mich, Schutt und Brand!\u00ab", "tokens": ["Loh'", "auf", "in", "H\u00f6l\u00b7len\u00b7flam\u00b7men", "!", "Be\u00b7grabt", "mich", ",", "Schutt", "und", "Brand", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "APPR", "APPR", "NN", "$.", "VVFIN", "PPER", "$,", "ADJD", "KON", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbdoch still \u2013 ganz still \u2013 erwarte die Stunde zagenfrei!", "tokens": ["\u00bb", "doch", "still", "\u2013", "ganz", "still", "\u2013", "er\u00b7war\u00b7te", "die", "Stun\u00b7de", "za\u00b7gen\u00b7frei", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "$(", "ADV", "ADJD", "$(", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Man sagt, dass Toben kindisch, dass Dulden m\u00e4nnlich sei.", "tokens": ["Man", "sagt", ",", "dass", "To\u00b7ben", "kin\u00b7disch", ",", "dass", "Dul\u00b7den", "m\u00e4nn\u00b7lich", "sei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "NN", "ADJD", "$,", "KOUS", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ob der Verzweiflung Dolchstoss das Herz mir blutig sticht,", "tokens": ["Ob", "der", "Ver\u00b7zwei\u00b7flung", "Dolchs\u00b7toss", "das", "Herz", "mir", "blu\u00b7tig", "sticht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NE", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+--+-++-+-+-+", "measure": "iambic.septa.invert"}, "line.4": {"text": "Gescheben ist geschehen \u2013 mein Jammer \u2013 \u00e4ndert's nicht.\u00ab", "tokens": ["Ge\u00b7sche\u00b7ben", "ist", "ge\u00b7sche\u00b7hen", "\u2013", "mein", "Jam\u00b7mer", "\u2013", "\u00e4n\u00b7dert's", "nicht", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVPP", "VAFIN", "VVPP", "$(", "PPOSAT", "NN", "$(", "VVFIN", "PTKNEG", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00bbmein Wagner, komm! Noch einmal vernimm des Freundes Wort;", "tokens": ["\u00bb", "mein", "Wag\u00b7ner", ",", "komm", "!", "Noch", "ein\u00b7mal", "ver\u00b7nimm", "des", "Freun\u00b7des", "Wort", ";"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "VVFIN", "$.", "ADV", "ADV", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Nicht lang' mehr wirst Du's h\u00f6ren, dann geht er eilig fort.", "tokens": ["Nicht", "lang'", "mehr", "wirst", "Du's", "h\u00f6\u00b7ren", ",", "dann", "geht", "er", "ei\u00b7lig", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "VAFIN", "PIS", "VVINF", "$,", "ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Er geht, und kommt nicht wieder, er reiset weit, gar weit,", "tokens": ["Er", "geht", ",", "und", "kommt", "nicht", "wie\u00b7der", ",", "er", "rei\u00b7set", "weit", ",", "gar", "weit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "PTKNEG", "ADV", "$,", "PPER", "VVFIN", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und Keiner folgt dem Wandrer, und giebt ihm das Geleit.\u00ab", "tokens": ["Und", "Kei\u00b7ner", "folgt", "dem", "Wand\u00b7rer", ",", "und", "giebt", "ihm", "das", "Ge\u00b7leit", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Der wird wohl nie vergessen, der t\u00f6nt wohl \u00fcberall!", "tokens": ["Der", "wird", "wohl", "nie", "ver\u00b7ges\u00b7sen", ",", "der", "t\u00f6nt", "wohl", "\u00fc\u00b7be\u00b7rall", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "VVPP", "$,", "PRELS", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "So glich ich jener Nymphe, der einst Narziss so lieb,", "tokens": ["So", "glich", "ich", "je\u00b7ner", "Nym\u00b7phe", ",", "der", "einst", "Nar\u00b7ziss", "so", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN", "$,", "PRELS", "ADV", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Bis sie voll Kummer hinschwand, und nur ihr Ruf noch blieb?\u00ab", "tokens": ["Bis", "sie", "voll", "Kum\u00b7mer", "hin\u00b7schwand", ",", "und", "nur", "ihr", "Ruf", "noch", "blieb", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADJD", "NN", "VVFIN", "$,", "KON", "ADV", "PPOSAT", "NN", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbsoll ich auch klagend scheiden? Nein! Bei mir sei's gelobt:", "tokens": ["\u00bb", "soll", "ich", "auch", "kla\u00b7gend", "schei\u00b7den", "?", "Nein", "!", "Bei", "mir", "sei's", "ge\u00b7lobt", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$.", "PTKANT", "$.", "APPR", "PPER", "VAFIN", "VVPP", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Mein Gram ist \u00fcberwunden, mein Schmerz hat ausgetobt.", "tokens": ["Mein", "Gram", "ist", "\u00fc\u00b7berw\u00b7un\u00b7den", ",", "mein", "Schmerz", "hat", "aus\u00b7ge\u00b7tobt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ich will so ruhig still sein, wie fern das todte Meer.", "tokens": ["Ich", "will", "so", "ru\u00b7hig", "still", "sein", ",", "wie", "fern", "das", "tod\u00b7te", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "ADJD", "VAINF", "$,", "PWAV", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Auch f\u00fchl' ich eine Ruhe, wie wenn ich todt schon w\u00e4r'.\u00ab", "tokens": ["Auch", "f\u00fchl'", "ich", "ei\u00b7ne", "Ru\u00b7he", ",", "wie", "wenn", "ich", "todt", "schon", "w\u00e4r'", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "KOKOM", "KOUS", "PPER", "ADJD", "ADV", "VAFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbo Ruhe! H\u00e4tte fr\u00fcher mich Deine Brust gestillt!", "tokens": ["\u00bb", "o", "Ru\u00b7he", "!", "H\u00e4t\u00b7te", "fr\u00fc\u00b7her", "mich", "Dei\u00b7ne", "Brust", "ge\u00b7stillt", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$.", "VVFIN", "ADJD", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "O h\u00e4tten Grabesschleier als Kind mich eingeh\u00fcllt!", "tokens": ["O", "h\u00e4t\u00b7ten", "Gra\u00b7bes\u00b7schlei\u00b7er", "als", "Kind", "mich", "ein\u00b7ge\u00b7h\u00fcllt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NN", "KOUS", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie sanft w\u00e4r' ich geschieden, h\u00e4tt' nichts vom Schmerz gewusst,", "tokens": ["Wie", "sanft", "w\u00e4r'", "ich", "ge\u00b7schie\u00b7den", ",", "h\u00e4tt'", "nichts", "vom", "Schmerz", "ge\u00b7wusst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "VVPP", "$,", "VAFIN", "PIS", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Schlief' eingelullt in Frieden still an der Mutterbrust!\u00ab", "tokens": ["Schlie\u00b7f'", "ein\u00b7ge\u00b7lullt", "in", "Frie\u00b7den", "still", "an", "der", "Mut\u00b7ter\u00b7brust", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "ADJD", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bbgeh, Wagner, zu den Freunden; will sie noch einmal sehn!", "tokens": ["\u00bb", "geh", ",", "Wag\u00b7ner", ",", "zu", "den", "Freun\u00b7den", ";", "will", "sie", "noch", "ein\u00b7mal", "sehn", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "$,", "APPR", "ART", "NN", "$.", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Noch einmal lasst den Odem der Mutter mich umwehn!", "tokens": ["Noch", "ein\u00b7mal", "lasst", "den", "O\u00b7dem", "der", "Mut\u00b7ter", "mich", "um\u00b7wehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "F\u00fchrt mich hinaus in's Freie, dass ich sie sehen kann;", "tokens": ["F\u00fchrt", "mich", "hin\u00b7aus", "in's", "Frei\u00b7e", ",", "dass", "ich", "sie", "se\u00b7hen", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APZR", "APPRART", "NN", "$,", "KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und sehn die Sonne sinken. \u2013 Ich bin ein kranker Mann.\u00ab", "tokens": ["Und", "sehn", "die", "Son\u00b7ne", "sin\u00b7ken", ".", "\u2013", "Ich", "bin", "ein", "kran\u00b7ker", "Mann", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVINF", "$.", "$(", "PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "\u00bbsehr krank \u2013 schwer krank \u2013 zum Tod krank! \u2013 Geh, guter Arzt, nach Haus,", "tokens": ["\u00bb", "sehr", "krank", "\u2013", "schwer", "krank", "\u2013", "zum", "Tod", "krank", "!", "\u2013", "Geh", ",", "gu\u00b7ter", "Arzt", ",", "nach", "Haus", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "$(", "ADJD", "ADJD", "$(", "APPRART", "NN", "ADJD", "$.", "$(", "NE", "$,", "ADJA", "NN", "$,", "APPR", "NN", "$,"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Gieb Andern Deine Mittel, nicht mir, mit mir ist's aus!", "tokens": ["Gieb", "An\u00b7dern", "Dei\u00b7ne", "Mit\u00b7tel", ",", "nicht", "mir", ",", "mit", "mir", "ist's", "aus", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "PPOSAT", "NN", "$,", "PTKNEG", "PPER", "$,", "APPR", "PPER", "NE", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Schwer liegt mir's in den Gliedern, schwer auf dem Herzen \u2013 oh!", "tokens": ["Schwer", "liegt", "mir's", "in", "den", "Glie\u00b7dern", ",", "schwer", "auf", "dem", "Her\u00b7zen", "\u2013", "oh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVFIN", "NE", "APPR", "ART", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$(", "FM", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "F\u00fchrt langsam, liebe Freunde, langsam, was eilt Ihr so?\u00ab \u2013", "tokens": ["F\u00fchrt", "lang\u00b7sam", ",", "lie\u00b7be", "Freun\u00b7de", ",", "lang\u00b7sam", ",", "was", "eilt", "Ihr", "so", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "ADJD", "$,", "ADJA", "NN", "$,", "ADJD", "$,", "PWS", "VVFIN", "PPER", "ADV", "$.", "$(", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "\u00bbmit Weile eile! Kennt Ihr das alte Spr\u00fcchlein nicht?", "tokens": ["\u00bb", "mit", "Wei\u00b7le", "ei\u00b7le", "!", "Kennt", "Ihr", "das", "al\u00b7te", "Spr\u00fcch\u00b7lein", "nicht", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "VVFIN", "$.", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "O weilt noch eine Weile, eilt mit der Eile nicht!", "tokens": ["O", "weilt", "noch", "ei\u00b7ne", "Wei\u00b7le", ",", "eilt", "mit", "der", "Ei\u00b7le", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "O h\u00e4tt' ich einst gez\u00fcgelt mein Eilen und geweilt!", "tokens": ["O", "h\u00e4tt'", "ich", "einst", "ge\u00b7z\u00fc\u00b7gelt", "mein", "Ei\u00b7len", "und", "ge\u00b7weilt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "VVPP", "PPOSAT", "NN", "KON", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Vom wilden Drang befl\u00fcgelt hab' ich mich ", "tokens": ["Vom", "wil\u00b7den", "Drang", "be\u00b7fl\u00fc\u00b7gelt", "hab'", "ich", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "VAFIN", "PPER", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Mir d\u00fcnket Eure Freundschaft ein Abendsonnenstrahl.", "tokens": ["M