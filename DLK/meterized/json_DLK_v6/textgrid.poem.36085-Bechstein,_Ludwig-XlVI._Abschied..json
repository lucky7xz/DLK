er": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mein nahes Ende f\u00fchl' ich \u2013 bin ein verlorner Mann!", "tokens": ["Mein", "na\u00b7hes", "En\u00b7de", "f\u00fchl'", "ich", "\u2013", "bin", "ein", "ver\u00b7lor\u00b7ner", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "$(", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Verloren, ganz verloren! Wisst Ihr, wie schwer das wiegt?", "tokens": ["Ver\u00b7lo\u00b7ren", ",", "ganz", "ver\u00b7lo\u00b7ren", "!", "Wisst", "Ihr", ",", "wie", "schwer", "das", "wiegt", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVPP", "$.", "VVFIN", "PPER", "$,", "PWAV", "ADJD", "PDS", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Was legt Ihr in die Waagschaal', das dieses \u00fcberwiegt?\u00ab \u2013", "tokens": ["Was", "legt", "Ihr", "in", "die", "Waag\u00b7schaal'", ",", "das", "die\u00b7ses", "\u00fc\u00b7berw\u00b7iegt", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "PDS", "VVFIN", "$.", "$(", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.13": {"line.1": {"text": "\u00bbverloren, ganz verloren! Sucht doch nach mir, Ihr Herr'n!", "tokens": ["\u00bb", "ver\u00b7lo\u00b7ren", ",", "ganz", "ver\u00b7lo\u00b7ren", "!", "Sucht", "doch", "nach", "mir", ",", "Ihr", "Herr'n", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVPP", "$,", "ADV", "VVPP", "$.",