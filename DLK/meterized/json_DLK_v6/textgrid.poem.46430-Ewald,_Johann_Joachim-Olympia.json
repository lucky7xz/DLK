{"textgrid.poem.46430": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Olympia", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Ungewitter tobt bey stiller Mitternacht,", "tokens": ["Ein", "Un\u00b7ge\u00b7wit\u00b7ter", "tobt", "bey", "stil\u00b7ler", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der nahe Himmel z\u00fcrnt, Olympia erwacht,", "tokens": ["Der", "na\u00b7he", "Him\u00b7mel", "z\u00fcrnt", ",", "O\u00b7lym\u00b7pia", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "$,", "NE", "VVPP", "$,"], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Sie flieht in Damons Bett, bewundert ihren Witz!", "tokens": ["Sie", "flieht", "in", "Da\u00b7mons", "Bett", ",", "be\u00b7wun\u00b7dert", "ih\u00b7ren", "Witz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hier h\u00f6rt sie keinen Schlag, hier sieht sie keinen Blitz.", "tokens": ["Hier", "h\u00f6rt", "sie", "kei\u00b7nen", "Schlag", ",", "hier", "sieht", "sie", "kei\u00b7nen", "Blitz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$,", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}