{"textgrid.poem.33896": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "T\u00e4ndelei", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich schaute dir in's Auge schnell,", "tokens": ["Ich", "schau\u00b7te", "dir", "in's", "Au\u00b7ge", "schnell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du blicktest gar zu mild,", "tokens": ["Du", "blick\u00b7test", "gar", "zu", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und lieblich sah ich, klar und hell,", "tokens": ["Und", "lieb\u00b7lich", "sah", "ich", ",", "klar", "und", "hell", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Darin mein eig'nes Bild.", "tokens": ["Da\u00b7rin", "mein", "eig'\u00b7nes", "Bild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "In eine wunderbare Flut", "tokens": ["In", "ei\u00b7ne", "wun\u00b7der\u00b7ba\u00b7re", "Flut"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Farben war's getaucht,", "tokens": ["Von", "Far\u00b7ben", "wa\u00b7r's", "ge\u00b7taucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Von Licht und Glanz die Zauberglut", "tokens": ["Von", "Licht", "und", "Glanz", "die", "Zau\u00b7berg\u00b7lut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dar\u00fcber hingehaucht.", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "hin\u00b7ge\u00b7haucht", "."], "token_info": ["word", "word", "punct"], "pos": ["PAV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da wurde dir das Auge feucht,", "tokens": ["Da", "wur\u00b7de", "dir", "das", "Au\u00b7ge", "feucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und perlenklar und rein", "tokens": ["Und", "per\u00b7len\u00b7klar", "und", "rein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Trat eine Thr\u00e4ne, schnell erzeugt,", "tokens": ["Trat", "ei\u00b7ne", "Thr\u00e4\u00b7ne", ",", "schnell", "er\u00b7zeugt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Licht in das Licht hinein.", "tokens": ["Licht", "in", "das", "Licht", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Mein Bild, als w\u00e4r's mit Flut und Wind,", "tokens": ["Mein", "Bild", ",", "als", "w\u00e4r's", "mit", "Flut", "und", "Wind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOKOM", "VAFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es k\u00e4mpfte frei und frank", "tokens": ["Es", "k\u00e4mpf\u00b7te", "frei", "und", "frank"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit deiner Thr\u00e4ne, bis es lind", "tokens": ["Mit", "dei\u00b7ner", "Thr\u00e4\u00b7ne", ",", "bis", "es", "lind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In ihrem Schoo\u00df versank.", "tokens": ["In", "ih\u00b7rem", "Schoo\u00df", "ver\u00b7sank", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So dir im Auge, wundersam", "tokens": ["So", "dir", "im", "Au\u00b7ge", ",", "wun\u00b7der\u00b7sam"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "PPER", "APPRART", "NN", "$,", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sah ich mich selbst entsteh'n,", "tokens": ["Sah", "ich", "mich", "selbst", "ent\u00b7steh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und, als die stille Thr\u00e4ne kam,", "tokens": ["Und", ",", "als", "die", "stil\u00b7le", "Thr\u00e4\u00b7ne", "kam", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch sch\u00f6ner mich vergeh'n.", "tokens": ["Noch", "sch\u00f6\u00b7ner", "mich", "ver\u00b7geh'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "ADJA", "PPER", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}