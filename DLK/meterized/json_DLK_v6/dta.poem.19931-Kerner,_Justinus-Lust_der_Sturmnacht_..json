{"dta.poem.19931": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Lust der Sturmnacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann durch Berg und Thale drau\u00dfen", "tokens": ["Wann", "durch", "Berg", "und", "Tha\u00b7le", "drau\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "KON", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Regen schauert, St\u00fcrme brausen,", "tokens": ["Re\u00b7gen", "schau\u00b7ert", ",", "St\u00fcr\u00b7me", "brau\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schild und Fenster hell erklirren", "tokens": ["Schild", "und", "Fens\u00b7ter", "hell", "er\u00b7klir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und in Nacht die Wandrer irren.", "tokens": ["Und", "in", "Nacht", "die", "Wand\u00b7rer", "ir\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ruht es sich so s\u00fc\u00df hier innen,", "tokens": ["Ruht", "es", "sich", "so", "s\u00fc\u00df", "hier", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADJD", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aufgel\u00f6st in sel'ges Minnen,", "tokens": ["Auf\u00b7ge\u00b7l\u00f6st", "in", "sel'\u00b7ges", "Min\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Blau und Gold, all' Himmelsschimmer,", "tokens": ["Blau", "und", "Gold", ",", "all'", "Him\u00b7mels\u00b7schim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flieht herein in's stille Zimmer.", "tokens": ["Flieht", "her\u00b7ein", "in's", "stil\u00b7le", "Zim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Reiches Leben! hab' Erbarmen!", "tokens": ["Rei\u00b7ches", "Le\u00b7ben", "!", "hab'", "Er\u00b7bar\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VAFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Halt mich fest in linden Armen!", "tokens": ["Halt", "mich", "fest", "in", "lin\u00b7den", "Ar\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lenzesblumen aufw\u00e4rts dringen,", "tokens": ["Len\u00b7zes\u00b7blu\u00b7men", "auf\u00b7w\u00e4rts", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00f6lklein ziehen, V\u00f6gel singen.", "tokens": ["W\u00f6lk\u00b7lein", "zie\u00b7hen", ",", "V\u00f6\u00b7gel", "sin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ende wie du Sturmnacht wilde!", "tokens": ["En\u00b7de", "wie", "du", "Sturm\u00b7nacht", "wil\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPER", "NN", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Klirrt ihr Fenster! schwankt ihr Schilde!", "tokens": ["Klirrt", "ihr", "Fens\u00b7ter", "!", "schwankt", "ihr", "Schil\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "B\u00e4umt euch, W\u00e4lder! braus', o Welle!", "tokens": ["B\u00e4umt", "euch", ",", "W\u00e4l\u00b7der", "!", "braus'", ",", "o", "Wel\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$.", "VVFIN", "$,", "FM", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich umf\u00e4ngt des Himmels Helle.", "tokens": ["Mich", "um\u00b7f\u00e4ngt", "des", "Him\u00b7mels", "Hel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}