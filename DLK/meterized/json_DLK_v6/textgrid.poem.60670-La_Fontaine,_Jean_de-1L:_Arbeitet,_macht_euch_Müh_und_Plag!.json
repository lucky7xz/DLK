{"textgrid.poem.60670": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Arbeitet, macht euch M\u00fch und Plag!", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Arbeitet, macht euch M\u00fch und Plag!", "tokens": ["Ar\u00b7bei\u00b7tet", ",", "macht", "euch", "M\u00fch", "und", "Plag", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist ein Tun, das immer Fr\u00fcchte bringt.", "tokens": ["Das", "ist", "ein", "Tun", ",", "das", "im\u00b7mer", "Fr\u00fcch\u00b7te", "bringt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Landmann, der im Sterben lag,", "tokens": ["Ein", "Land\u00b7mann", ",", "der", "im", "Ster\u00b7ben", "lag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sah sich von seinen Kindern treu umringt.", "tokens": ["Sah", "sich", "von", "sei\u00b7nen", "Kin\u00b7dern", "treu", "um\u00b7ringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbo h\u00fctet euch,\u00ab sprach er, \u00bbdas Erbe loszuschlagen,", "tokens": ["\u00bb", "o", "h\u00fc\u00b7tet", "euch", ",", "\u00ab", "sprach", "er", ",", "\u00bb", "das", "Er\u00b7be", "los\u00b7zu\u00b7schla\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "VVFIN", "PPER", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Das unsre Vordern uns gelassen.", "tokens": ["Das", "uns\u00b7re", "Vor\u00b7dern", "uns", "ge\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein Schatz liegt drin versteckt, nur wei\u00df ich nicht zu sagen,", "tokens": ["Ein", "Schatz", "liegt", "drin", "ver\u00b7steckt", ",", "nur", "wei\u00df", "ich", "nicht", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "VVPP", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "An welchem Ort, doch werdet ihr ihn fassen!", "tokens": ["An", "wel\u00b7chem", "Ort", ",", "doch", "wer\u00b7det", "ihr", "ihn", "fas\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "$,", "ADV", "VAFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Habt Eifer nur und Mut, so hebt ihr ihn am Ende.", "tokens": ["Habt", "Ei\u00b7fer", "nur", "und", "Mut", ",", "so", "hebt", "ihr", "ihn", "am", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "KON", "NN", "$,", "ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Beackert euer Feld; sobald geerntet ist,", "tokens": ["Be\u00b7ac\u00b7kert", "eu\u00b7er", "Feld", ";", "so\u00b7bald", "ge\u00b7ern\u00b7tet", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hackt, w\u00fchlt und grabt und g\u00f6nnt euch keine Frist,", "tokens": ["Hackt", ",", "w\u00fchlt", "und", "grabt", "und", "g\u00f6nnt", "euch", "kei\u00b7ne", "Frist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "KON", "ADJD", "KON", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und r\u00fchrt von fr\u00fch bis sp\u00e4t die H\u00e4nde.\u00ab", "tokens": ["Und", "r\u00fchrt", "von", "fr\u00fch", "bis", "sp\u00e4t", "die", "H\u00e4n\u00b7de", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJD", "APPR", "ADJD", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Vater stirbt; und unverweilt", "tokens": ["Der", "Va\u00b7ter", "stirbt", ";", "und", "un\u00b7ver\u00b7weilt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sind seine S\u00f6hne auf das Feld geeilt", "tokens": ["Sind", "sei\u00b7ne", "S\u00f6h\u00b7ne", "auf", "das", "Feld", "ge\u00b7eilt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und w\u00fchlen, ackern \u00fcberall so gut,", "tokens": ["Und", "w\u00fch\u00b7len", ",", "ac\u00b7kern", "\u00fc\u00b7be\u00b7rall", "so", "gut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Da\u00df dieses doppelten Tribut", "tokens": ["Da\u00df", "die\u00b7ses", "dop\u00b7pel\u00b7ten", "Tri\u00b7but"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "Bescherte in den Erntestunden.", "tokens": ["Be\u00b7scher\u00b7te", "in", "den", "Ern\u00b7tes\u00b7tun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Zwar hat man kein vergrabnes Geld gefunden;", "tokens": ["Zwar", "hat", "man", "kein", "ver\u00b7grab\u00b7nes", "Geld", "ge\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Doch lohnte sich gar sehr des Vaters letzte Spende,", "tokens": ["Doch", "lohn\u00b7te", "sich", "gar", "sehr", "des", "Va\u00b7ters", "letz\u00b7te", "Spen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die er den jungen Erben gab vor seinem Ende:", "tokens": ["Die", "er", "den", "jun\u00b7gen", "Er\u00b7ben", "gab", "vor", "sei\u00b7nem", "En\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Mahnung, da\u00df der gr\u00f6\u00dfte Schatz", "tokens": ["Die", "Mah\u00b7nung", ",", "da\u00df", "der", "gr\u00f6\u00df\u00b7te", "Schatz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Die Arbeit ist, wo sie am Platz.", "tokens": ["Die", "Ar\u00b7beit", "ist", ",", "wo", "sie", "am", "Platz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PWAV", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}