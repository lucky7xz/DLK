{"textgrid.poem.36074": {"metadata": {"author": {"name": "Bechstein, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "XxXV. Liebeswunder.", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob Faustus Haupte strahlet ein neuer Stern des Gl\u00fccks,", "tokens": ["Ob", "Faus\u00b7tus", "Haup\u00b7te", "strah\u00b7let", "ein", "neu\u00b7er", "Stern", "des", "Gl\u00fccks", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NN", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der sich im Glanze spiegelt des frohen Flammenblicks.", "tokens": ["Der", "sich", "im", "Glan\u00b7ze", "spie\u00b7gelt", "des", "fro\u00b7hen", "Flam\u00b7men\u00b7blicks", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und w\u00e4r' es ein Kometstern, w\u00e4r's nur ein Meteor,", "tokens": ["Und", "w\u00e4r'", "es", "ein", "Ko\u00b7mets\u00b7tern", ",", "w\u00e4r's", "nur", "ein", "Me\u00b7te\u00b7or", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "$,", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Nie war der Magus reicher, nie fr\u00f6hlicher zuvor.", "tokens": ["Nie", "war", "der", "Ma\u00b7gus", "rei\u00b7cher", ",", "nie", "fr\u00f6h\u00b7li\u00b7cher", "zu\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$,", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Wer ist die Wundersch\u00f6nheit, die bei dem Zaubrer wohnt?", "tokens": ["Wer", "ist", "die", "Wun\u00b7der\u00b7sch\u00f6n\u00b7heit", ",", "die", "bei", "dem", "Zaub\u00b7rer", "wohnt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Auf deren Rosenantlitz ein ganzer Himmel thront?", "tokens": ["Auf", "de\u00b7ren", "Ro\u00b7sen\u00b7ant\u00b7litz", "ein", "gan\u00b7zer", "Him\u00b7mel", "thront", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ein Himmel voller Liebreiz, und eine Welt voll Lust,", "tokens": ["Ein", "Him\u00b7mel", "vol\u00b7ler", "Lieb\u00b7reiz", ",", "und", "ei\u00b7ne", "Welt", "voll", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "KON", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der Liebesflammen lodern aus eisumstarrter Brust?", "tokens": ["Der", "Lie\u00b7bes\u00b7flam\u00b7men", "lo\u00b7dern", "aus", "ei\u00b7sum\u00b7starr\u00b7ter", "Brust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "In Greisenbusen werden entschlafne W\u00fcnsche wach,", "tokens": ["In", "Grei\u00b7sen\u00b7bu\u00b7sen", "wer\u00b7den", "ent\u00b7schlaf\u00b7ne", "W\u00fcn\u00b7sche", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der stillen Luft vertrauen sie manch geheimes Ach.", "tokens": ["Der", "stil\u00b7len", "Luft", "ver\u00b7trau\u00b7en", "sie", "manch", "ge\u00b7hei\u00b7mes", "Ach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "In M\u00e4nnerblicken malt sich, was kaum der Mund vorschweigt;", "tokens": ["In", "M\u00e4n\u00b7ner\u00b7bli\u00b7cken", "malt", "sich", ",", "was", "kaum", "der", "Mund", "vor\u00b7schweigt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PRF", "$,", "PRELS", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der J\u00fcngling preist sich selig, dem sie der Traumgott zeigt.", "tokens": ["Der", "J\u00fcng\u00b7ling", "preist", "sich", "se\u00b7lig", ",", "dem", "sie", "der", "Traum\u00b7gott", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Und dieses Weib, o Faustus, dess Sch\u00f6nheit allentz\u00fcckt,", "tokens": ["Und", "die\u00b7ses", "Weib", ",", "o", "Faus\u00b7tus", ",", "dess", "Sch\u00f6n\u00b7heit", "al\u00b7lent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "FM", "FM", "$,", "PRELAT", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Hat nur das ", "tokens": ["Hat", "nur", "das"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADV", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "Von ihren K\u00fcssen trunken schl\u00e4fst ihr am Busen ein,", "tokens": ["Von", "ih\u00b7ren", "K\u00fcs\u00b7sen", "trun\u00b7ken", "schl\u00e4fst", "ihr", "am", "Bu\u00b7sen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und nennst sie wonneschwelgend voll frohen Stolzes ", "tokens": ["Und", "nennst", "sie", "won\u00b7ne\u00b7schwel\u00b7gend", "voll", "fro\u00b7hen", "Stol\u00b7zes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADJD", "ADJA", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Jetzt magst Du Deines Namens recht fr\u00f6hlich sein in Ruh,", "tokens": ["Jetzt", "magst", "Du", "Dei\u00b7nes", "Na\u00b7mens", "recht", "fr\u00f6h\u00b7lich", "sein", "in", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "VAINF", "APPR", "NN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sie liebt Dich heiss die Sch\u00f6ne, die Dir am Herzen liegt,", "tokens": ["Sie", "liebt", "Dich", "heiss", "die", "Sch\u00f6\u00b7ne", ",", "die", "Dir", "am", "Her\u00b7zen", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie Anadyomene, von Mavors Arm gewiegt.", "tokens": ["Wie", "A\u00b7nad\u00b7yo\u00b7me\u00b7ne", ",", "von", "Ma\u00b7vors", "Arm", "ge\u00b7wiegt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "APPR", "NE", "NN", "VVPP", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Es quillt von ihren Lippen klangreich manch s\u00fcsses Wort,", "tokens": ["Es", "quillt", "von", "ih\u00b7ren", "Lip\u00b7pen", "klan\u00b7greich", "manch", "s\u00fcs\u00b7ses", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ADJD", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "In Hellas sch\u00f6ner Sprache rauscht es melodisch fort.", "tokens": ["In", "Hel\u00b7las", "sch\u00f6\u00b7ner", "Spra\u00b7che", "rauscht", "es", "me\u00b7lo\u00b7disch", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie weiss sie hold zu schmeicheln; das Zauberwort ", "tokens": ["Wie", "weiss", "sie", "hold", "zu", "schmei\u00b7cheln", ";", "das", "Zau\u00b7ber\u00b7wort"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$.", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Macht Dich wie Liebesjubel der Philomelen froh.", "tokens": ["Macht", "Dich", "wie", "Lie\u00b7bes\u00b7ju\u00b7bel", "der", "Phi\u00b7lo\u00b7me\u00b7len", "froh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KOKOM", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "O Liebeslust, wie gleichst Du dem Nachtigallgesang!", "tokens": ["O", "Lie\u00b7bes\u00b7lust", ",", "wie", "gleichst", "Du", "dem", "Nach\u00b7ti\u00b7gall\u00b7ge\u00b7sang", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+----+-+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "In s\u00fcsser Mainachtd\u00e4mmrung ein sanfter Fl\u00f6tenklang.", "tokens": ["In", "s\u00fcs\u00b7ser", "Mai\u00b7nacht\u00b7d\u00e4mm\u00b7rung", "ein", "sanf\u00b7ter", "Fl\u00f6\u00b7ten\u00b7klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ein Brautbett, das mit Rosen bestreuet und bekr\u00e4nzt,", "tokens": ["Ein", "Braut\u00b7bett", ",", "das", "mit", "Ro\u00b7sen", "be\u00b7streu\u00b7et", "und", "be\u00b7kr\u00e4nzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ein Lustpokal, den Freude dem Gl\u00fccklichen kredenzt.", "tokens": ["Ein", "Lust\u00b7po\u00b7kal", ",", "den", "Freu\u00b7de", "dem", "Gl\u00fcck\u00b7li\u00b7chen", "kre\u00b7denzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Du bist ein heilger Lotus, der aus den Fluthen taucht,", "tokens": ["Du", "bist", "ein", "heil\u00b7ger", "Lo\u00b7tus", ",", "der", "aus", "den", "Flut\u00b7hen", "taucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dess stille Blumensehnsucht statt Klagen, Duft verhaucht.", "tokens": ["Dess", "stil\u00b7le", "Blu\u00b7men\u00b7sehn\u00b7sucht", "statt", "Kla\u00b7gen", ",", "Duft", "ver\u00b7haucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Doch auch die Blume findet ihr Liebeseigenthum,", "tokens": ["Doch", "auch", "die", "Blu\u00b7me", "fin\u00b7det", "ihr", "Lie\u00b7be\u00b7sei\u00b7gen\u00b7thum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und im Entz\u00fccken sterbend sinkt das Nelumbium.", "tokens": ["Und", "im", "Ent\u00b7z\u00fc\u00b7cken", "ster\u00b7bend", "sinkt", "das", "Ne\u00b7lum\u00b7bi\u00b7um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.9": {"line.1": {"text": "O Liebeslust, wie flammst Du, hell wie der Asgardbrand,", "tokens": ["O", "Lie\u00b7bes\u00b7lust", ",", "wie", "flammst", "Du", ",", "hell", "wie", "der", "As\u00b7gard\u00b7brand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VVFIN", "PPER", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Darin ein G\u00f6tterhimmel einst sein Verderben fand!", "tokens": ["Da\u00b7rin", "ein", "G\u00f6t\u00b7ter\u00b7him\u00b7mel", "einst", "sein", "Ver\u00b7der\u00b7ben", "fand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ja, wenn die Feuerriesen der Sinnlichkeit befreit,", "tokens": ["Ja", ",", "wenn", "die", "Feu\u00b7er\u00b7rie\u00b7sen", "der", "Sinn\u00b7lich\u00b7keit", "be\u00b7freit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Dann heilge, reine Disen, naht Eure letzte Zeit!", "tokens": ["Dann", "heil\u00b7ge", ",", "rei\u00b7ne", "Di\u00b7sen", ",", "naht", "Eu\u00b7re", "letz\u00b7te", "Zeit", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJA", "NN", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+----+-+", "measure": "unknown.measure.penta"}}, "stanza.10": {"line.1": {"text": "Wo Lust und Liebe buhlen, da zeugen sie geschwind", "tokens": ["Wo", "Lust", "und", "Lie\u00b7be", "buh\u00b7len", ",", "da", "zeu\u00b7gen", "sie", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN", "VVINF", "$,", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die Tochter heissen Blutes, und ", "tokens": ["Die", "Toch\u00b7ter", "heis\u00b7sen", "Blu\u00b7tes", ",", "und"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "KON"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf w\u00e4chst sie zur Virago, die Riesen niederringt;", "tokens": ["Auf", "w\u00e4chst", "sie", "zur", "Vi\u00b7ra\u00b7go", ",", "die", "Rie\u00b7sen", "nie\u00b7der\u00b7ringt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "APPRART", "NE", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der Eumeniden Sch\u00f6nste sie Flammenfackeln schwingt.", "tokens": ["Der", "Eu\u00b7me\u00b7ni\u00b7den", "Sch\u00f6ns\u00b7te", "sie", "Flam\u00b7men\u00b7fa\u00b7ckeln", "schwingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "So nennt auch Faustus Liebe, was Liebe nimmer ist,", "tokens": ["So", "nennt", "auch", "Faus\u00b7tus", "Lie\u00b7be", ",", "was", "Lie\u00b7be", "nim\u00b7mer", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NE", "NN", "$,", "PWS", "NN", "ADV", "VAFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ihm scheint der Wollust Urbild die Huldin die er k\u00fcsst.", "tokens": ["Ihm", "scheint", "der", "Wol\u00b7lust", "Ur\u00b7bild", "die", "Hul\u00b7din", "die", "er", "k\u00fcsst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "ART", "NN", "ART", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und als er sich ein Reicher, ein Hochbegl\u00fcckter glaubt,", "tokens": ["Und", "als", "er", "sich", "ein", "Rei\u00b7cher", ",", "ein", "Hoch\u00b7be\u00b7gl\u00fcck\u00b7ter", "glaubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "War er nie mehr des Reichthums, nie mehr des Gl\u00fccks beraubt. \u2013", "tokens": ["War", "er", "nie", "mehr", "des", "Reicht\u00b7hums", ",", "nie", "mehr", "des", "Gl\u00fccks", "be\u00b7raubt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "$,", "ADV", "ADV", "ART", "NN", "VVPP", "$.", "$("], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.12": {"line.1": {"text": "In Lieb' und Lust der Liebe verrauscht ihm fast ein Jahr,", "tokens": ["In", "Lieb'", "und", "Lust", "der", "Lie\u00b7be", "ver\u00b7rauscht", "ihm", "fast", "ein", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und Helena wird Mutter, das d\u00e4ucht ihm wunderbar.", "tokens": ["Und", "He\u00b7le\u00b7na", "wird", "Mut\u00b7ter", ",", "das", "d\u00e4ucht", "ihm", "wun\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "NN", "$,", "PDS", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u00bbhab' ich nicht von der H\u00f6lle nur ein Phantom begehrt,", "tokens": ["\u00bb", "hab'", "ich", "nicht", "von", "der", "H\u00f6l\u00b7le", "nur", "ein", "Phan\u00b7tom", "be\u00b7gehrt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Um meiner Lust zu fr\u00f6hnen, und hat sie mehr gew\u00e4hrt?\u00ab", "tokens": ["Um", "mei\u00b7ner", "Lust", "zu", "fr\u00f6h\u00b7nen", ",", "und", "hat", "sie", "mehr", "ge\u00b7w\u00e4hrt", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$,", "KON", "VAFIN", "PPER", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.13": {"line.1": {"text": "\u00bbund wirklich Mutter? Wirklich ich Vater?\u00ab \u2013 Freudig schl\u00e4gt", "tokens": ["\u00bb", "und", "wirk\u00b7lich", "Mut\u00b7ter", "?", "Wirk\u00b7lich", "ich", "Va\u00b7ter", "?", "\u00ab", "\u2013", "Freu\u00b7dig", "schl\u00e4gt"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct", "word", "word"], "pos": ["$(", "KON", "ADJD", "NN", "$.", "ADJD", "PPER", "NN", "$.", "$(", "$(", "ADJD", "VVFIN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sein Herz, als ihm die Amme sein Kind entgegen tr\u00e4gt.", "tokens": ["Sein", "Herz", ",", "als", "ihm", "die", "Am\u00b7me", "sein", "Kind", "ent\u00b7ge\u00b7gen", "tr\u00e4gt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "ART", "NN", "PPOSAT", "NN", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Das bl\u00fchnde Kind, die W\u00e4nglein wie rosen\u00fcbermalt,", "tokens": ["Das", "bl\u00fchn\u00b7de", "Kind", ",", "die", "W\u00e4ng\u00b7lein", "wie", "ro\u00b7sen\u00b7\u00fc\u00b7ber\u00b7malt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "KOKOM", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und wie der Mutter Liebreiz aus seinen Augen strahlt!", "tokens": ["Und", "wie", "der", "Mut\u00b7ter", "Lieb\u00b7reiz", "aus", "sei\u00b7nen", "Au\u00b7gen", "strahlt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.14": {"line.1": {"text": "O Vatergl\u00fcck, wie f\u00fcllst Du mit Lust des Mannes Herz!", "tokens": ["O", "Va\u00b7ter\u00b7gl\u00fcck", ",", "wie", "f\u00fcllst", "Du", "mit", "Lust", "des", "Man\u00b7nes", "Herz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VVFIN", "PPER", "APPR", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Eine neue Welt enth\u00fcllst Du, tr\u00e4gst W\u00fcnsche himmelw\u00e4rts!", "tokens": ["Ei\u00b7ne", "neu\u00b7e", "Welt", "ent\u00b7h\u00fcllst", "Du", ",", "tr\u00e4gst", "W\u00fcn\u00b7sche", "him\u00b7mel\u00b7w\u00e4rts", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$,", "VVFIN", "NN", "ADV", "$."], "meter": "+-+-+-+--+-+--", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Faust h\u00e4tte gern gebetet, so fr\u00f6hlich durft' er sein;", "tok