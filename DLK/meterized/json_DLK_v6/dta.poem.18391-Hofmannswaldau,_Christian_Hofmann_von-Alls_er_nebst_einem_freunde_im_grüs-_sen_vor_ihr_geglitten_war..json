{"dta.poem.18391": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Alls er nebst einem freunde im gr\u00fcs-  \n sen vor ihr geglitten war.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du kennst noch/ werther freund/ das liebens-werthe kind/", "tokens": ["Du", "kennst", "noch", "/", "wert\u00b7her", "freund", "/", "das", "lie\u00b7bens\u00b7wert\u00b7he", "kind", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das kind/ dem iedes glied von anmuths-rosen bl\u00fchet/", "tokens": ["Das", "kind", "/", "dem", "ie\u00b7des", "glied", "von", "an\u00b7muths\u00b7ro\u00b7sen", "bl\u00fc\u00b7het", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "PIAT", "NN", "APPR", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vor dem wir ehemals so offt geglitten sind/", "tokens": ["Vor", "dem", "wir", "e\u00b7he\u00b7mals", "so", "offt", "ge\u00b7glit\u00b7ten", "sind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADV", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So offt wir es im gehn zu gr\u00fcssen uns bem\u00fchet:", "tokens": ["So", "offt", "wir", "es", "im", "gehn", "zu", "gr\u00fcs\u00b7sen", "uns", "be\u00b7m\u00fc\u00b7het", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPER", "APPRART", "VVINF", "PTKZU", "VVINF", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Di\u00df hat/ mein hertzens-freund/ uns deutlich kund gethan/", "tokens": ["Di\u00df", "hat", "/", "mein", "hert\u00b7zens\u00b7freund", "/", "uns", "deut\u00b7lich", "kund", "ge\u00b7than", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$(", "PPOSAT", "NN", "$(", "PPER", "ADJD", "PTKVZ", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der m\u00e4nner falle sey das frauen-volck zu nennen.", "tokens": ["Der", "m\u00e4n\u00b7ner", "fal\u00b7le", "sey", "das", "frau\u00b7en\u00b7volck", "zu", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn weil ihr gru\u00df so weit zum gleiten dienen kan/", "tokens": ["Denn", "weil", "ihr", "gru\u00df", "so", "weit", "zum", "glei\u00b7ten", "die\u00b7nen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "ADV", "ADJD", "APPRART", "ADJA", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie mu\u00df man nahe nicht von k\u00fcssen fallen k\u00f6nnen?", "tokens": ["Wie", "mu\u00df", "man", "na\u00b7he", "nicht", "von", "k\u00fcs\u00b7sen", "fal\u00b7len", "k\u00f6n\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PIS", "ADJD", "PTKNEG", "APPR", "VVINF", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}