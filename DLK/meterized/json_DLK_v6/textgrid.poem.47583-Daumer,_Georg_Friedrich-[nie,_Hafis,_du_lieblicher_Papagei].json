{"textgrid.poem.47583": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[nie, Hafis, du lieblicher Papagei]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nie, Hafis, du lieblicher Papagei", "tokens": ["Nie", ",", "Ha\u00b7fis", ",", "du", "lieb\u00b7li\u00b7cher", "Pa\u00b7pa\u00b7gei"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "NE", "$,", "PPER", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Aus der Liebe goldenem Dschinnistane,", "tokens": ["Aus", "der", "Lie\u00b7be", "gol\u00b7de\u00b7nem", "Dschin\u00b7nis\u00b7ta\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Fehl' es deinem Schnabel an Kosegl\u00fccks,", "tokens": ["Fehl'", "es", "dei\u00b7nem", "Schna\u00b7bel", "an", "Ko\u00b7se\u00b7gl\u00fccks", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "N\u00e4scherei und s\u00fc\u00dfem Marzipane. \u2013", "tokens": ["N\u00e4\u00b7sche\u00b7rei", "und", "s\u00fc\u00b7\u00dfem", "Mar\u00b7zi\u00b7pa\u00b7ne", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wehe, weh uns Armen! Ach, es geht", "tokens": ["We\u00b7he", ",", "weh", "uns", "Ar\u00b7men", "!", "Ach", ",", "es", "geht"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "NN", "$.", "ITJ", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Mit dem Turban auch der Kopf verloren!", "tokens": ["Mit", "dem", "Tur\u00b7ban", "auch", "der", "Kopf", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Doch, so wie er uns vom Rumpfe rollt,", "tokens": ["Doch", ",", "so", "wie", "er", "uns", "vom", "Rump\u00b7fe", "rollt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "KOKOM", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Gegen uns, was sind die Tamerlane? \u2013", "tokens": ["Ge\u00b7gen", "uns", ",", "was", "sind", "die", "Ta\u00b7mer\u00b7la\u00b7ne", "?", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "$,", "PWS", "VAFIN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Fernehin aufsuchte des Lebens Quell", "tokens": ["Fer\u00b7ne\u00b7hin", "auf\u00b7such\u00b7te", "des", "Le\u00b7bens", "Quell"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Alexander \u2013 er hat ihn nicht gekostet;", "tokens": ["A\u00b7lex\u00b7an\u00b7der", "\u2013", "er", "hat", "ihn", "nicht", "ge\u00b7kos\u00b7tet", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Wir, wir kosten ihn im Vaterland", "tokens": ["Wir", ",", "wir", "kos\u00b7ten", "ihn", "im", "Va\u00b7ter\u00b7land"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Bei der Schenke grauem Guardiane. \u2013", "tokens": ["Bei", "der", "Schen\u00b7ke", "grau\u00b7em", "Gu\u00b7ar\u00b7di\u00b7a\u00b7ne", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "L\u00fcstet dich zu w\u00fchlen im Lockenhaar", "tokens": ["L\u00fcs\u00b7tet", "dich", "zu", "w\u00fch\u00b7len", "im", "Lo\u00b7cken\u00b7haar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKZU", "VVFIN", "APPRART", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Eines holden, jugendlichen Hauptes,", "tokens": ["Ei\u00b7nes", "hol\u00b7den", ",", "ju\u00b7gend\u00b7li\u00b7chen", "Haup\u00b7tes", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Nicht um Urlaub flehe die Vernunft!", "tokens": ["Nicht", "um", "Ur\u00b7laub", "fle\u00b7he", "die", "Ver\u00b7nunft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Nichts erflehst du von dem Paviane. \u2013", "tokens": ["Nichts", "er\u00b7flehst", "du", "von", "dem", "Pa\u00b7vi\u00b7a\u00b7ne", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "D\u00fcnken ein allzu leichtes, luftiges,", "tokens": ["D\u00fcn\u00b7ken", "ein", "all\u00b7zu", "leich\u00b7tes", ",", "luf\u00b7ti\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "PTKA", "ADJA", "$,", "ADJA", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Lustiges Gesindel dir Poeten \u2013", "tokens": ["Lus\u00b7ti\u00b7ges", "Ge\u00b7sin\u00b7del", "dir", "Po\u00b7et\u00b7en", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Mit dem Blute des Herzens f\u00fcttern sie", "tokens": ["Mit", "dem", "Blu\u00b7te", "des", "Her\u00b7zens", "f\u00fct\u00b7tern", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "PPER"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ihre Versebrut, die Pelikane. \u2013", "tokens": ["Ih\u00b7re", "Ver\u00b7seb\u00b7rut", ",", "die", "Pe\u00b7li\u00b7ka\u00b7ne", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "$.", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Singt Hafis sein zauberisches Lied,", "tokens": ["Singt", "Ha\u00b7fis", "sein", "zau\u00b7be\u00b7ri\u00b7sches", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "N\u00fcchterne taumeln ihm und Trunkne tanzen;", "tokens": ["N\u00fcch\u00b7ter\u00b7ne", "tau\u00b7meln", "ihm", "und", "Trunk\u00b7ne", "tan\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Auf dem Reichspallaste der Poesie", "tokens": ["Auf", "dem", "Reichs\u00b7pal\u00b7las\u00b7te", "der", "Poe\u00b7sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wehet er als Pracht- und Ehrenfahne.", "tokens": ["We\u00b7het", "er", "als", "Pracht", "und", "Eh\u00b7ren\u00b7fah\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "TRUNC", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}