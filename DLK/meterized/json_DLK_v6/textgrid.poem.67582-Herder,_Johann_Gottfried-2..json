{"textgrid.poem.67582": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der L\u00f6we schlief und fuhr", "tokens": ["Der", "L\u00f6\u00b7we", "schlief", "und", "fuhr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Im Schlaf auf ob dem gr\u00e4\u00dflichen Geschrei.", "tokens": ["Im", "Schlaf", "auf", "ob", "dem", "gr\u00e4\u00df\u00b7li\u00b7chen", "Ge\u00b7schrei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbist Aufruhr", "tokens": ["\u00bb", "ist", "Auf\u00b7ruhr"], "token_info": ["punct", "word", "word"], "pos": ["$(", "VAFIN", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "In meinem Reich?\u00ab Er eilt herbei;", "tokens": ["In", "mei\u00b7nem", "Reich", "?", "\u00ab", "Er", "eilt", "her\u00b7bei", ";"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "$(", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "'s war Froschgeschrei.", "tokens": ["'s", "war", "Froschge\u00b7schrei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "\u00bbwas lohnte mir's, die Kreischer zu zerbaxen?", "tokens": ["\u00bb", "was", "lohn\u00b7te", "mir's", ",", "die", "Krei\u00b7scher", "zu", "zer\u00b7ba\u00b7xen", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "NE", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Es ist so sch\u00f6ner Tag; la\u00df sie coaxen!\u00ab", "tokens": ["Es", "ist", "so", "sch\u00f6\u00b7ner", "Tag", ";", "la\u00df", "sie", "coa\u00b7xen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "$.", "VVIMP", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Kunstrichter! Ist im w\u00e4rmsten Sommerpfuhl", "tokens": ["Kuns\u00b7trich\u00b7ter", "!", "Ist", "im", "w\u00e4rms\u00b7ten", "Som\u00b7mer\u00b7pfuhl"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Geschrei wie dort Coax auf Euerm Richterstuhl?", "tokens": ["Ge\u00b7schrei", "wie", "dort", "Coax", "auf", "Eu\u00b7erm", "Rich\u00b7ter\u00b7stuhl", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADV", "NE", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "\u00bbder K\u00f6nig L\u00f6we selbst soll's h\u00f6ren!\u00ab", "tokens": ["\u00bb", "der", "K\u00f6\u00b7nig", "L\u00f6\u00b7we", "selbst", "soll's", "h\u00f6\u00b7ren", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "NE", "ADV", "PIS", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr k\u00f6nnt ihn auch einmal im Mittagsschlafe st\u00f6ren;", "tokens": ["Ihr", "k\u00f6nnt", "ihn", "auch", "ein\u00b7mal", "im", "Mit\u00b7tags\u00b7schla\u00b7fe", "st\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch seid getrost! er g\u00f6nnt Euch Euern Pfuhl,", "tokens": ["Doch", "seid", "ge\u00b7trost", "!", "er", "g\u00f6nnt", "Euch", "Eu\u00b7ern", "Pfuhl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "$.", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}