{"textgrid.poem.61416": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie der purpurne Wein, wenn die blinkende Schale zersprungen,", "tokens": ["Wie", "der", "pur\u00b7pur\u00b7ne", "Wein", ",", "wenn", "die", "blin\u00b7ken\u00b7de", "Scha\u00b7le", "zer\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "KOUS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Also zerflie\u00dfet der Geist ohne des Wortes Gef\u00e4\u00df,", "tokens": ["Al\u00b7so", "zer\u00b7flie\u00b7\u00dfet", "der", "Geist", "oh\u00b7ne", "des", "Wor\u00b7tes", "Ge\u00b7f\u00e4\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Und nicht h\u00e4lt er dir stand. Doch bildet' er still sich der Rede", "tokens": ["Und", "nicht", "h\u00e4lt", "er", "dir", "stand", ".", "Doch", "bil\u00b7det'", "er", "still", "sich", "der", "Re\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "VVFIN", "PPER", "PPER", "VVFIN", "$.", "KON", "VVFIN", "PPER", "VMFIN", "PRF", "ART", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "K\u00f6stlichen Leib: wie ein Freund spricht er vertraulich dich an.", "tokens": ["K\u00f6st\u00b7li\u00b7chen", "Leib", ":", "wie", "ein", "Freund", "spricht", "er", "ver\u00b7trau\u00b7lich", "dich", "an", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PWAV", "ART", "NN", "VVFIN", "PPER", "ADJD", "PPER", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Durch ein Wunder erschlie\u00dft sich das unsichtbare Geheimnis,", "tokens": ["Durch", "ein", "Wun\u00b7der", "er\u00b7schlie\u00dft", "sich", "das", "un\u00b7sicht\u00b7ba\u00b7re", "Ge\u00b7heim\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Und das lebendige Wort zeuget lebendige Tat. \u2013", "tokens": ["Und", "das", "le\u00b7ben\u00b7di\u00b7ge", "Wort", "zeu\u00b7get", "le\u00b7ben\u00b7di\u00b7ge", "Tat", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$.", "$("], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "\u00dcber den Wassern schwebte der Geist, doch als er das Wort ward,", "tokens": ["\u00dc\u00b7ber", "den", "Was\u00b7sern", "schweb\u00b7te", "der", "Geist", ",", "doch", "als", "er", "das", "Wort", "ward", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,", "ADV", "KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Stieg aus dem Chaos der Nacht herrlich die Sch\u00f6pfung empor.", "tokens": ["Stieg", "aus", "dem", "Chaos", "der", "Nacht", "herr\u00b7lich", "die", "Sch\u00f6p\u00b7fung", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-++--+--+", "measure": "iambic.hexa.invert"}}}}}