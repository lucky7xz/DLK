{"textgrid.poem.59858": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer nicht vermag seine Lieder zu sch\u00f6pfen", "tokens": ["Wer", "nicht", "ver\u00b7mag", "sei\u00b7ne", "Lie\u00b7der", "zu", "sch\u00f6p\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PTKNEG", "VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aus der eignen Brust und der wirklichen Welt,", "tokens": ["Aus", "der", "eig\u00b7nen", "Brust", "und", "der", "wirk\u00b7li\u00b7chen", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der geh\u00f6rt selbst zu den hirnlosen K\u00f6pfen,", "tokens": ["Der", "ge\u00b7h\u00f6rt", "selbst", "zu", "den", "hirn\u00b7lo\u00b7sen", "K\u00f6p\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+---+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Denen sein hirnloses Lied gef\u00e4llt.", "tokens": ["De\u00b7nen", "sein", "hirn\u00b7lo\u00b7ses", "Lied", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}