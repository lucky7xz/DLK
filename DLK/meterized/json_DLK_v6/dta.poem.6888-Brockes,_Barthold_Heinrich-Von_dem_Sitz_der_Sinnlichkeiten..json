{"dta.poem.6888": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So aber f\u00fchlt sie es, und bringt sofort", "tokens": ["So", "a\u00b7ber", "f\u00fchlt", "sie", "es", ",", "und", "bringt", "so\u00b7fort"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "$,", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "An selben Ort,", "tokens": ["An", "sel\u00b7ben", "Ort", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Den Druck zur\u00fcck, wo er erst angefangen,", "tokens": ["Den", "Druck", "zu\u00b7r\u00fcck", ",", "wo", "er", "erst", "an\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der ins Gehirn schnell durch die Nerven gangen.", "tokens": ["Der", "ins", "Ge\u00b7hirn", "schnell", "durch", "die", "Ner\u00b7ven", "gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Man schlag uns an ein Bein, so wird man wissen,", "tokens": ["Man", "schlag", "uns", "an", "ein", "Bein", ",", "so", "wird", "man", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "ADV", "VAFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Durch das Gesetz; so uns zum Heil gemacht,", "tokens": ["Durch", "das", "Ge\u00b7setz", ";", "so", "uns", "zum", "Heil", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "ADV", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Da\u00df solches unser Bein,", "tokens": ["Da\u00df", "sol\u00b7ches", "un\u00b7ser", "Bein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und da\u00df wir ihm daselbst zu H\u00fclffe kommen m\u00fcssen.", "tokens": ["Und", "da\u00df", "wir", "ihm", "da\u00b7selbst", "zu", "H\u00fclf\u00b7fe", "kom\u00b7men", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "PAV", "APPR", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}