{"dta.poem.8057": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Harald .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Vor seinem Heergefolge ritt", "tokens": ["Vor", "sei\u00b7nem", "Heer\u00b7ge\u00b7fol\u00b7ge", "ritt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der k\u00fchne Held Harald.", "tokens": ["Der", "k\u00fch\u00b7ne", "Held", "Ha\u00b7rald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie zogen in des Mondes Schein", "tokens": ["Sie", "zo\u00b7gen", "in", "des", "Mon\u00b7des", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch einen wilden Wald.", "tokens": ["Durch", "ei\u00b7nen", "wil\u00b7den", "Wald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie tragen manch\u2019 erk\u00e4mpfte Fahn\u2019,", "tokens": ["Sie", "tra\u00b7gen", "man\u00b7ch'", "er\u00b7k\u00e4mpf\u00b7te", "Fahn'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die hoch im Winde wallt,", "tokens": ["Die", "hoch", "im", "Win\u00b7de", "wallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie singen manches Siegeslied,", "tokens": ["Sie", "sin\u00b7gen", "man\u00b7ches", "Sie\u00b7ges\u00b7lied", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das durch die Berge hallt.", "tokens": ["Das", "durch", "die", "Ber\u00b7ge", "hallt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was rauschet, lauschet im Geb\u00fcsch?", "tokens": ["Was", "rau\u00b7schet", ",", "lau\u00b7schet", "im", "Ge\u00b7b\u00fcsch", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was wiegt sich auf dem Baum?", "tokens": ["Was", "wiegt", "sich", "auf", "dem", "Baum", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was senket aus den Wolken sich,", "tokens": ["Was", "sen\u00b7ket", "aus", "den", "Wol\u00b7ken", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN", "PRF", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Und taucht aus Stromes Schaum?", "tokens": ["Und", "taucht", "aus", "Stro\u00b7mes", "Schaum", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Was wirft mit Blumen um und um?", "tokens": ["Was", "wirft", "mit", "Blu\u00b7men", "um", "und", "um", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "NN", "PTKVZ", "KON", "KOUI", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was singt so wonniglich?", "tokens": ["Was", "singt", "so", "won\u00b7nig\u00b7lich", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was tanzet durch der Krieger Reihn?", "tokens": ["Was", "tan\u00b7zet", "durch", "der", "Krie\u00b7ger", "Reihn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schwingt auf die Rosse sich?", "tokens": ["Schwingt", "auf", "die", "Ros\u00b7se", "sich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was kost so sanft und k\u00fc\u00dft so s\u00fc\u00df?", "tokens": ["Was", "kost", "so", "sanft", "und", "k\u00fc\u00dft", "so", "s\u00fc\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADJD", "KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und h\u00e4lt so lind umfa\u00dft?", "tokens": ["Und", "h\u00e4lt", "so", "lind", "um\u00b7fa\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und nimmt das Schwerdt, und zieht vom Ro\u00df,", "tokens": ["Und", "nimmt", "das", "Schwerdt", ",", "und", "zieht", "vom", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und l\u00e4\u00dft nicht Ruh noch Rast?", "tokens": ["Und", "l\u00e4\u00dft", "nicht", "Ruh", "noch", "Rast", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "NN", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es ist der Elfen leichte Schaar;", "tokens": ["Es", "ist", "der", "El\u00b7fen", "leich\u00b7te", "Schaar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hier hilft kein Widerstand.", "tokens": ["Hier", "hilft", "kein", "Wi\u00b7der\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schon sind die Krieger all dahin,", "tokens": ["Schon", "sind", "die", "Krie\u00b7ger", "all", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PIAT", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sind all im Feenland.", "tokens": ["Sind", "all", "im", "Fe\u00b7en\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Nur er, der Beste, blieb zur\u00fcck,", "tokens": ["Nur", "er", ",", "der", "Bes\u00b7te", ",", "blieb", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "ART", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der k\u00fchne Held Harald.", "tokens": ["Der", "k\u00fch\u00b7ne", "Held", "Ha\u00b7rald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er ist vom Wirbel bis zur Sohl\u2019", "tokens": ["Er", "ist", "vom", "Wir\u00b7bel", "bis", "zur", "Sohl'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In harten Stahl geschnallt.", "tokens": ["In", "har\u00b7ten", "Stahl", "ge\u00b7schnallt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "All seine Krieger sind entr\u00fcckt,", "tokens": ["All", "sei\u00b7ne", "Krie\u00b7ger", "sind", "ent\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da liegen Schwerdt und Schild,", "tokens": ["Da", "lie\u00b7gen", "Schwerdt", "und", "Schild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Rosse, ledig ihrer Herrn,", "tokens": ["Die", "Ros\u00b7se", ",", "le\u00b7dig", "ih\u00b7rer", "Herrn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie gehn im Walde wild.", "tokens": ["Sie", "gehn", "im", "Wal\u00b7de", "wild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "In gro\u00dfer Trauer ritt von dann", "tokens": ["In", "gro\u00b7\u00dfer", "Trau\u00b7er", "ritt", "von", "dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "APPR", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der stolze Held Harald,", "tokens": ["Der", "stol\u00b7ze", "Held", "Ha\u00b7rald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "Er ritt allein im Mondenschein", "tokens": ["Er", "ritt", "al\u00b7lein", "im", "Mon\u00b7den\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl durch den weiten Wald.", "tokens": ["Wohl", "durch", "den", "wei\u00b7ten", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Vom Felsen rauscht es frisch und klar,", "tokens": ["Vom", "Fel\u00b7sen", "rauscht", "es", "frisch", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er springt vom Rosse schnell,", "tokens": ["Er", "springt", "vom", "Ros\u00b7se", "schnell", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er schnallt vom Haupte sich den Helm", "tokens": ["Er", "schnallt", "vom", "Haup\u00b7te", "sich", "den", "Helm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und trinkt vom k\u00fchlen Quell.", "tokens": ["Und", "trinkt", "vom", "k\u00fch\u00b7len", "Quell", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Doch wie er kaum den Durst gestillt,", "tokens": ["Doch", "wie", "er", "kaum", "den", "Durst", "ge\u00b7stillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Versagt ihm Arm und Bein;", "tokens": ["Ver\u00b7sagt", "ihm", "Arm", "und", "Bein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er mu\u00df sich setzen auf den Fels,", "tokens": ["Er", "mu\u00df", "sich", "set\u00b7zen", "auf", "den", "Fels", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er nickt und schlummert ein.", "tokens": ["Er", "nickt", "und", "schlum\u00b7mert", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Er schlummert auf demselben Stein", "tokens": ["Er", "schlum\u00b7mert", "auf", "dem\u00b7sel\u00b7ben", "Stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schon manche hundert Jahr\u2019,", "tokens": ["Schon", "man\u00b7che", "hun\u00b7dert", "Jahr'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "CARD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Haupt gesenket auf die Brust,", "tokens": ["Das", "Haupt", "ge\u00b7sen\u00b7ket", "auf", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit grauem Bart und Haar.", "tokens": ["Mit", "grau\u00b7em", "Bart", "und", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Wann Blitze zucken, Donner rollt,", "tokens": ["Wann", "Blit\u00b7ze", "zu\u00b7cken", ",", "Don\u00b7ner", "rollt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wann Sturm erbraust im Wald,", "tokens": ["Wann", "Sturm", "er\u00b7braust", "im", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann greift er tr\u00e4umend nach dem Schwerdt,", "tokens": ["Dann", "greift", "er", "tr\u00e4u\u00b7mend", "nach", "dem", "Schwerdt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der alte Held Harald.", "tokens": ["Der", "al\u00b7te", "Held", "Ha\u00b7rald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}