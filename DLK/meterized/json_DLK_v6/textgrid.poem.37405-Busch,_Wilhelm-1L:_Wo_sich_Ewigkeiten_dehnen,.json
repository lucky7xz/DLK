{"textgrid.poem.37405": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo sich Ewigkeiten dehnen,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo sich Ewigkeiten dehnen,", "tokens": ["Wo", "sich", "E\u00b7wig\u00b7kei\u00b7ten", "deh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6ren die Gedanken auf,", "tokens": ["H\u00f6\u00b7ren", "die", "Ge\u00b7dan\u00b7ken", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur der Herzen frommes Sehnen", "tokens": ["Nur", "der", "Her\u00b7zen", "from\u00b7mes", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ahnt, was ohne Zeitenlauf.", "tokens": ["Ahnt", ",", "was", "oh\u00b7ne", "Zei\u00b7ten\u00b7lauf", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo wir waren, wo wir bleiben,", "tokens": ["Wo", "wir", "wa\u00b7ren", ",", "wo", "wir", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sagt kein kluges Menschenwort;", "tokens": ["Sagt", "kein", "klu\u00b7ges", "Men\u00b7schen\u00b7wort", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch die Gr\u00fcbelgeister schreiben:", "tokens": ["Doch", "die", "Gr\u00fc\u00b7bel\u00b7geis\u00b7ter", "schrei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bist du weg, so bleibe fort.", "tokens": ["Bist", "du", "weg", ",", "so", "blei\u00b7be", "fort", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "La\u00df dich nicht aufs neu gel\u00fcsten.", "tokens": ["La\u00df", "dich", "nicht", "aufs", "neu", "ge\u00b7l\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPRART", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was geschah, es wird geschehn.", "tokens": ["Was", "ge\u00b7schah", ",", "es", "wird", "ge\u00b7schehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ewig an des Lebens K\u00fcsten", "tokens": ["E\u00b7wig", "an", "des", "Le\u00b7bens", "K\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wirst du scheiternd untergehn.", "tokens": ["Wirst", "du", "schei\u00b7ternd", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}