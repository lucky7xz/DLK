{"textgrid.poem.55533": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Annette an ihren Geliebten", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah, wie Doris bei Dam\u00f6ten stand,", "tokens": ["Ich", "sah", ",", "wie", "Do\u00b7ris", "bei", "Da\u00b7m\u00f6\u00b7ten", "stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "NE", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er nahm sie z\u00e4rtlich bei der Hand;", "tokens": ["Er", "nahm", "sie", "z\u00e4rt\u00b7lich", "bei", "der", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lang sahen sie einander an,", "tokens": ["Lang", "sa\u00b7hen", "sie", "ein\u00b7an\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sahn sich um, ob nicht die Eltern wachen,", "tokens": ["Und", "sahn", "sich", "um", ",", "ob", "nicht", "die", "El\u00b7tern", "wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKVZ", "$,", "KOUS", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und da sie niemand sahn,", "tokens": ["Und", "da", "sie", "nie\u00b7mand", "sahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Geschwind \u2013 Genug, sie machten's, wie wir's machen.", "tokens": ["Ge\u00b7schwind", "\u2013", "Ge\u00b7nug", ",", "sie", "mach\u00b7ten's", ",", "wie", "wir's", "ma\u00b7chen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADV", "$,", "PPER", "VVFIN", "$,", "PWAV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}