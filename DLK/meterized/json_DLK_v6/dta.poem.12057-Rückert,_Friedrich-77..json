{"dta.poem.12057": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "77.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Reichthums Vermehrung kann die Armuth nicht vermindern,", "tokens": ["Reicht\u00b7hums", "Ver\u00b7meh\u00b7rung", "kann", "die", "Ar\u00b7muth", "nicht", "ver\u00b7min\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Solang das Recht nicht wird ungleiche Theilung hindern.", "tokens": ["So\u00b7lang", "das", "Recht", "nicht", "wird", "un\u00b7glei\u00b7che", "Thei\u00b7lung", "hin\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "VAFIN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "In einem Land, wo reich die Reichen sind allein,", "tokens": ["In", "ei\u00b7nem", "Land", ",", "wo", "reich", "die", "Rei\u00b7chen", "sind", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "ADJD", "ART", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Werden die Armen nur um desto \u00e4rmer seyn.", "tokens": ["Wer\u00b7den", "die", "Ar\u00b7men", "nur", "um", "des\u00b7to", "\u00e4r\u00b7mer", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "APPR", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandr