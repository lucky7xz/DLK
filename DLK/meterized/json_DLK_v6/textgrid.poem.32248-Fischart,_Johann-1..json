{"textgrid.poem.32248": {"metadata": {"author": {"name": "Fischart, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1568, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dem Hau\u00df, spricht man, stehts nicht wol", "tokens": ["In", "dem", "Hau\u00df", ",", "spricht", "man", ",", "stehts", "nicht", "wol"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "VVFIN", "PIS", "$,", "VVFIN", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd mu\u00df gewi\u00df was b\u00f6\u00df gemanen,", "tokens": ["Vnd", "mu\u00df", "ge\u00b7wi\u00df", "was", "b\u00f6\u00df", "ge\u00b7ma\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PWS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wann die Henn kr\u00e4ht vber den Hanen,", "tokens": ["Wann", "die", "Henn", "kr\u00e4ht", "vber", "den", "Ha\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da sie doch darf\u00fcr gachssen soll,", "tokens": ["Da", "sie", "doch", "dar\u00b7f\u00fcr", "gachs\u00b7sen", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PAV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu leuchtern jren Eyerstoll.", "tokens": ["Zu", "leuch\u00b7tern", "jren", "Ey\u00b7ers\u00b7toll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Also wie viel mehr mu\u00df es h\u00f6n", "tokens": ["Al\u00b7so", "wie", "viel", "mehr", "mu\u00df", "es", "h\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ADV", "ADV", "VMFIN", "PPER", "ADJD"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.7": {"text": "In einem Regiment dann stehn,", "tokens": ["In", "ei\u00b7nem", "Re\u00b7gi\u00b7ment", "dann", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Welchs gr\u00f6sser ist vnd sorgen voll:", "tokens": ["Welchs", "gr\u00f6s\u00b7ser", "ist", "vnd", "sor\u00b7gen", "voll", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wann die Henn wil die Hanen f\u00fchren,", "tokens": ["Wann", "die", "Henn", "wil", "die", "Ha\u00b7nen", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+++-+-+-", "measure": "unknown.measure.penta"}, "line.10": {"text": "Da mu\u00df sie die gewi\u00df verf\u00fchren,", "tokens": ["Da", "mu\u00df", "sie", "die", "ge\u00b7wi\u00df", "ver\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Dann es ist wider die Natur,", "tokens": ["Dann", "es", "ist", "wi\u00b7der", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Da\u00df das schw\u00e4cher das st\u00e4rcker f\u00fchrt,", "tokens": ["Da\u00df", "das", "schw\u00e4\u00b7cher", "das", "st\u00e4r\u00b7cker", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "ART", "ADJA", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.13": {"text": "Das vnzierlichst das zierlichst ziert,", "tokens": ["Das", "vn\u00b7zier\u00b7lichst", "das", "zier\u00b7lichst", "ziert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PDS", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Welch vngleicheit dient zur auffrur.", "tokens": ["Welch", "vn\u00b7glei\u00b7cheit", "dient", "zur", "auf\u00b7frur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}