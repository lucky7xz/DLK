{"textgrid.poem.26570": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Noch ist dein Antlitz hell und mild", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch ist dein Antlitz hell und mild", "tokens": ["Noch", "ist", "dein", "Ant\u00b7litz", "hell", "und", "mild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sanft sind deine Augen;", "tokens": ["Und", "sanft", "sind", "dei\u00b7ne", "Au\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du k\u00f6nntest zum Madonnenbild,", "tokens": ["Du", "k\u00f6nn\u00b7test", "zum", "Ma\u00b7don\u00b7nen\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit himmlischem Gen\u00fcgen", "tokens": ["Mit", "himm\u00b7li\u00b7schem", "Ge\u00b7n\u00fc\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In jungfr\u00e4ulichen Z\u00fcgen,", "tokens": ["In", "jung\u00b7fr\u00e4u\u00b7li\u00b7chen", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Dem fr\u00f6mmsten Maler taugen.", "tokens": ["Dem", "fr\u00f6mms\u00b7ten", "Ma\u00b7ler", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Noch k\u00f6nnt' ein starkes, schlichtes Herz,", "tokens": ["Noch", "k\u00f6nnt'", "ein", "star\u00b7kes", ",", "schlich\u00b7tes", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht achtend deines Falles,", "tokens": ["Nicht", "ach\u00b7tend", "dei\u00b7nes", "Fal\u00b7les", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit stumm zur\u00fcckgewies'nem Schmerz", "tokens": ["Mit", "stumm", "zu\u00b7r\u00fcck\u00b7ge\u00b7wies'\u00b7nem", "Schmerz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bekr\u00e4nzen, fr\u00fch Verirrte,", "tokens": ["Be\u00b7kr\u00e4n\u00b7zen", ",", "fr\u00fch", "Ver\u00b7irr\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dein Haupt mit wei\u00dfer Myrte \u2013", "tokens": ["Dein", "Haupt", "mit", "wei\u00b7\u00dfer", "Myr\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verzeiht doch Liebe Alles!", "tokens": ["Ver\u00b7zeiht", "doch", "Lie\u00b7be", "Al\u00b7les", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Noch k\u00f6nntest du so treu, so gut \u2013", "tokens": ["Noch", "k\u00f6nn\u00b7test", "du", "so", "treu", ",", "so", "gut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn du mit reu'ger Thr\u00e4ne", "tokens": ["Wenn", "du", "mit", "reu'\u00b7ger", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In jenes Herzens milder Hut", "tokens": ["In", "je\u00b7nes", "Her\u00b7zens", "mil\u00b7der", "Hut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geb\u00fc\u00dft die Schuld der Erden \u2013", "tokens": ["Ge\u00b7b\u00fc\u00dft", "die", "Schuld", "der", "Er\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zum reinsten Weibe werden,", "tokens": ["Zum", "reins\u00b7ten", "Wei\u00b7be", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie einstens Magdalene.", "tokens": ["Wie", "eins\u00b7tens", "Mag\u00b7da\u00b7le\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das k\u00f6nntest du! \u2013 Doch b\u00fc\u00dfen bleibt", "tokens": ["Das", "k\u00f6nn\u00b7test", "du", "!", "\u2013", "Doch", "b\u00fc\u00b7\u00dfen", "bleibt"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "$.", "$(", "KON", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ja fremd der raschen Jugend;", "tokens": ["Ja", "fremd", "der", "ra\u00b7schen", "Ju\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Leben zum Genusse treibt \u2013", "tokens": ["Das", "Le\u00b7ben", "zum", "Ge\u00b7nus\u00b7se", "treibt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer m\u00f6cht' es ihr verargen,", "tokens": ["Wer", "m\u00f6cht'", "es", "ihr", "ver\u00b7ar\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df sie verlacht den kargen", "tokens": ["Da\u00df", "sie", "ver\u00b7lacht", "den", "kar\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und matten Lohn der Tugend?", "tokens": ["Und", "mat\u00b7ten", "Lohn", "der", "Tu\u00b7gend", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wohlan denn \u2013 so genie\u00dfe, Kind!", "tokens": ["Wo\u00b7hlan", "denn", "\u2013", "so", "ge\u00b7nie\u00b7\u00dfe", ",", "Kind", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "$(", "ADV", "VVFIN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df deine jungen Sinne \u2013", "tokens": ["La\u00df", "dei\u00b7ne", "jun\u00b7gen", "Sin\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie W\u00f6lkchen oft vom Fr\u00fchlingswind", "tokens": ["Wie", "W\u00f6lk\u00b7chen", "oft", "vom", "Fr\u00fch\u00b7lings\u00b7wind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu heimlichen Gewittern", "tokens": ["Zu", "heim\u00b7li\u00b7chen", "Ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Herangef\u00e4chelt \u2013 zittern", "tokens": ["Her\u00b7an\u00b7ge\u00b7f\u00e4\u00b7chelt", "\u2013", "zit\u00b7tern"], "token_info": ["word", "punct", "word"], "pos": ["VVFIN", "$(", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im hei\u00dfen Strahl der Minne.", "tokens": ["Im", "hei\u00b7\u00dfen", "Strahl", "der", "Min\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Doch wenn die Stunde kommen mu\u00df \u2013", "tokens": ["Doch", "wenn", "die", "Stun\u00b7de", "kom\u00b7men", "mu\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O dann begl\u00fccke Jenen,", "tokens": ["O", "dann", "be\u00b7gl\u00fc\u00b7cke", "Je\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "PDS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der l\u00e4ngst nach deinem Feuerku\u00df,", "tokens": ["Der", "l\u00e4ngst", "nach", "dei\u00b7nem", "Feu\u00b7er\u00b7ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach deines G\u00fcrtels Sinken", "tokens": ["Nach", "dei\u00b7nes", "G\u00fcr\u00b7tels", "Sin\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und deiner Glieder Blinken", "tokens": ["Und", "dei\u00b7ner", "Glie\u00b7der", "Blin\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gelechzt mit trunk'nem Sehnen.", "tokens": ["Ge\u00b7lechzt", "mit", "trunk'\u00b7nem", "Seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der l\u00e4ngst erkannt, da\u00df deinem Haupt,", "tokens": ["Der", "l\u00e4ngst", "er\u00b7kannt", ",", "da\u00df", "dei\u00b7nem", "Haupt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "$,", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem schwer zur\u00fcckgebog'nen,", "tokens": ["Dem", "schwer", "zu\u00b7r\u00fcck\u00b7ge\u00b7bo\u00b7g'\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Unschuld erster Kranz geraubt \u2013", "tokens": ["Der", "Un\u00b7schuld", "ers\u00b7ter", "Kranz", "ge\u00b7raubt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df mit bewu\u00dftem Trachten", "tokens": ["Da\u00df", "mit", "be\u00b7wu\u00df\u00b7tem", "Trach\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Schon diese Augen schmachten,", "tokens": ["Schon", "die\u00b7se", "Au\u00b7gen", "schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die bl\u00e4ulich leicht umzog'nen.", "tokens": ["Die", "bl\u00e4u\u00b7lich", "leicht", "um\u00b7zo\u00b7g'\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "VVIZU", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Und was du hast an Gluth und Blut,", "tokens": ["Und", "was", "du", "hast", "an", "Gluth", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VAFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das lasse gl\u00fch'n und wallen \u2013", "tokens": ["Das", "las\u00b7se", "gl\u00fch'n", "und", "wal\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "KON", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und la\u00df, umwogt von hoher Fluth,", "tokens": ["Und", "la\u00df", ",", "um\u00b7wogt", "von", "ho\u00b7her", "Fluth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn sich die Lippen pressen", "tokens": ["Wenn", "sich", "die", "Lip\u00b7pen", "pres\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In seligem Vergessen", "tokens": ["In", "se\u00b7li\u00b7gem", "Ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den letzten Schleier fallen!", "tokens": ["Den", "letz\u00b7ten", "Schlei\u00b7er", "fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Das k\u00f6nntest du. \u2013 Doch matt und schwach", "tokens": ["Das", "k\u00f6nn\u00b7test", "du", ".", "\u2013", "Doch", "matt", "und", "schwach"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "$.", "$(", "KON", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schl\u00e4gt in der Brust das Herz dir \u2013", "tokens": ["Schl\u00e4gt", "in", "der", "Brust", "das", "Herz", "dir", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "PPER", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und sorglos tr\u00e4gst du deine Schmach:", "tokens": ["Und", "sorg\u00b7los", "tr\u00e4gst", "du", "dei\u00b7ne", "Schmach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn jener Tag vor allen,", "tokens": ["Denn", "je\u00b7ner", "Tag", "vor", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "APPR", "PIAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "An welchem du gefallen,", "tokens": ["An", "wel\u00b7chem", "du", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bracht' weder Lust noch Schmerz dir.", "tokens": ["Bracht'", "we\u00b7der", "Lust", "noch", "Schmerz", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "NN", "ADV", "NN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Nicht einmal z\u00fcrnen kannst du, Weib,", "tokens": ["Nicht", "ein\u00b7mal", "z\u00fcr\u00b7nen", "kannst", "du", ",", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "VMFIN", "PPER", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sch\u00f6n es dir auch st\u00fcnde;", "tokens": ["Wie", "sch\u00f6n", "es", "dir", "auch", "st\u00fcn\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du schm\u00fcckst nur l\u00e4chelnd deinen Leib,", "tokens": ["Du", "schm\u00fcckst", "nur", "l\u00e4\u00b7chelnd", "dei\u00b7nen", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So schwach im Widerstreben,", "tokens": ["So", "schwach", "im", "Wi\u00b7der\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So treulos ohne Beben \u2013", "tokens": ["So", "treu\u00b7los", "oh\u00b7ne", "Be\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", 