{"textgrid.poem.65011": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "100.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als H\u00f6chstes w\u00fcnsch' ich dir, will scheidend ich dich segnen:", "tokens": ["Als", "H\u00f6chs\u00b7tes", "w\u00fcn\u00b7sch'", "ich", "dir", ",", "will", "schei\u00b7dend", "ich", "dich", "seg\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "PPER", "PPER", "$,", "VMFIN", "ADJD", "PPER", "PRF", "VVINF", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da\u00df dir ein zweites Herz wie meines m\u00f6g' begegnen.", "tokens": ["Da\u00df", "dir", "ein", "zwei\u00b7tes", "Herz", "wie", "mei\u00b7nes", "m\u00f6g'", "be\u00b7geg\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "KOKOM", "PPOSAT", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und nun nimm hin die\u00df Lied, wie einfach schlicht es t\u00f6nt!", "tokens": ["Und", "nun", "nimm", "hin", "die\u00df", "Lied", ",", "wie", "ein\u00b7fach", "schlicht", "es", "t\u00f6nt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVIMP", "ADV", "PDS", "NN", "$,", "PWAV", "ADJD", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mich hats mit Gott und Welt und dir und mir vers\u00f6hnt.", "tokens": ["Mich", "hats", "mit", "Gott", "und", "Welt", "und", "dir", "und", "mir", "ver\u00b7s\u00f6hnt", "."], "token_info": ["word", "wo