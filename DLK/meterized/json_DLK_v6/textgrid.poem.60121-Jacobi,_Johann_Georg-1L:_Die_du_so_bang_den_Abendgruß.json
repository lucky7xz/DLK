{"textgrid.poem.60121": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die du so bang den Abendgru\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die du so bang den Abendgru\u00df", "tokens": ["Die", "du", "so", "bang", "den", "A\u00b7bend\u00b7gru\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf mich herunter wehest,", "tokens": ["Auf", "mich", "her\u00b7un\u00b7ter", "we\u00b7hest", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APZR", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zur Wolke schwebst, und mit dem Fu\u00df", "tokens": ["Zur", "Wol\u00b7ke", "schwebst", ",", "und", "mit", "dem", "Fu\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Todtenh\u00fcgeln stehest,", "tokens": ["Auf", "Tod\u00b7ten\u00b7h\u00fc\u00b7geln", "ste\u00b7hest", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O Linde! manche Thr\u00e4ne hat", "tokens": ["O", "Lin\u00b7de", "!", "man\u00b7che", "Thr\u00e4\u00b7ne", "hat"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$.", "PIAT", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Boden hier benetzet,", "tokens": ["Den", "Bo\u00b7den", "hier", "be\u00b7net\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und Menschenjammer, bla\u00df und matt,", "tokens": ["Und", "Men\u00b7schen\u00b7jam\u00b7mer", ",", "bla\u00df", "und", "matt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf ihn sein Kreuz gesetzet.", "tokens": ["Auf", "ihn", "sein", "Kreuz", "ge\u00b7set\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die auf dem einen H\u00fcgel hier,", "tokens": ["Die", "auf", "dem", "ei\u00b7nen", "H\u00fc\u00b7gel", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geweint um ihre Lieben,", "tokens": ["Ge\u00b7weint", "um", "ih\u00b7re", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die birgt ein andrer neben dir;", "tokens": ["Die", "birgt", "ein", "an\u00b7drer", "ne\u00b7ben", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "APPR", "PPER", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Und ihrer wenig blieben.", "tokens": ["Und", "ih\u00b7rer", "we\u00b7nig", "blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie schlafen. Ach! um ihr Gebein", "tokens": ["Sie", "schla\u00b7fen", ".", "Ach", "!", "um", "ihr", "Ge\u00b7bein"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ITJ", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verhallte schon die Trauer;", "tokens": ["Ver\u00b7hall\u00b7te", "schon", "die", "Trau\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Du Linde rauschest ganz allein", "tokens": ["Du", "Lin\u00b7de", "rau\u00b7schest", "ganz", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "NE", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In athemlose Schauer.", "tokens": ["In", "at\u00b7hem\u00b7lo\u00b7se", "Schau\u00b7er", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Vergebens l\u00e4\u00dft auf k\u00fchles Grab", "tokens": ["Ver\u00b7ge\u00b7bens", "l\u00e4\u00dft", "auf", "k\u00fch\u00b7les", "Grab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Zweig die Bl\u00fcthe fallen,", "tokens": ["Dein", "Zweig", "die", "Bl\u00fc\u00b7the", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vergebens t\u00f6nt von dir herab", "tokens": ["Ver\u00b7ge\u00b7bens", "t\u00f6nt", "von", "dir", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Lied der Nachtigallen;", "tokens": ["Das", "Lied", "der", "Nach\u00b7ti\u00b7gal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie schlummern fort. Du aber schl\u00e4gst", "tokens": ["Sie", "schlum\u00b7mern", "fort", ".", "Du", "a\u00b7ber", "schl\u00e4gst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In modervolle Gr\u00fcfte", "tokens": ["In", "mo\u00b7der\u00b7vol\u00b7le", "Gr\u00fcf\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Wurzel, schm\u00fcckest dich, und tr\u00e4gst", "tokens": ["Die", "Wur\u00b7zel", ",", "schm\u00fc\u00b7ckest", "dich", ",", "und", "tr\u00e4gst"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Empor die Bl\u00fcthend\u00fcfte.", "tokens": ["Em\u00b7por", "die", "Bl\u00fc\u00b7then\u00b7d\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Auf Erden sieht man immer so", "tokens": ["Auf", "Er\u00b7den", "sieht", "man", "im\u00b7mer", "so"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PIS", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Tod ans Leben gr\u00e4nzen;", "tokens": ["Den", "Tod", "ans", "Le\u00b7ben", "gr\u00e4n\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch ewig kannst du, stolz und froh,", "tokens": ["Doch", "e\u00b7wig", "kannst", "du", ",", "stolz", "und", "froh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Aeste nicht bekr\u00e4nzen.", "tokens": ["Die", "A\u00b7es\u00b7te", "nicht", "be\u00b7kr\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Es trocknet schon der Jugend Saft", "tokens": ["Es", "trock\u00b7net", "schon", "der", "Ju\u00b7gend", "Saft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In dir; Verwesung winket,", "tokens": ["In", "dir", ";", "Ver\u00b7we\u00b7sung", "win\u00b7ket", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$.", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bis endlich deine letzte Kraft", "tokens": ["Bis", "end\u00b7lich", "dei\u00b7ne", "letz\u00b7te", "Kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dahin auf Gr\u00e4ber sinket.", "tokens": ["Da\u00b7hin", "auf", "Gr\u00e4\u00b7ber", "sin\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wenn aber dein Gefl\u00fcster auch", "tokens": ["Wenn", "a\u00b7ber", "dein", "Ge\u00b7fl\u00fcs\u00b7ter", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verstummt an diesen H\u00fcgeln,", "tokens": ["Ver\u00b7stummt", "an", "die\u00b7sen", "H\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So bringet neuen Fr\u00fchlingshauch", "tokens": ["So", "brin\u00b7get", "neu\u00b7en", "Fr\u00fch\u00b7lings\u00b7hauch"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der West auf Rosenfl\u00fcgeln.", "tokens": ["Der", "West", "auf", "Ro\u00b7sen\u00b7fl\u00fc\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Damit die Felder wieder bl\u00fchn,", "tokens": ["Da\u00b7mit", "die", "Fel\u00b7der", "wie\u00b7der", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Umwallt er Berg' und Gr\u00fcnde;", "tokens": ["Um\u00b7wallt", "er", "Ber\u00b7g'", "und", "Gr\u00fcn\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Will deinen Spr\u00f6\u00dfling auferziehn,", "tokens": ["Will", "dei\u00b7nen", "Spr\u00f6\u00df\u00b7ling", "auf\u00b7er\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und kr\u00f6nt die junge Linde.", "tokens": ["Und", "kr\u00f6nt", "die", "jun\u00b7ge", "Lin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wohl uns! Der gro\u00dfe Lebensquell", "tokens": ["Wohl", "uns", "!", "Der", "gro\u00b7\u00dfe", "Le\u00b7bens\u00b7quell"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Versiegt dem Geiste nimmer.", "tokens": ["Ver\u00b7siegt", "dem", "Geis\u00b7te", "nim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Kreuz auf Gr\u00e4bern, wie so hell", "tokens": ["Das", "Kreuz", "auf", "Gr\u00e4\u00b7bern", ",", "wie", "so", "hell"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "$,", "PWAV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In dieser Hoffnung Schimmer!", "tokens": ["In", "die\u00b7ser", "Hoff\u00b7nung", "Schim\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O Linde! gern an deinem Fu\u00df", "tokens": ["O", "Lin\u00b7de", "!", "gern", "an", "dei\u00b7nem", "Fu\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$.", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "H\u00f6r' ich des Wipfels Wehen;", "tokens": ["H\u00f6r'", "ich", "des", "Wip\u00b7fels", "We\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dein feyerlicher Abendgru\u00df", "tokens": ["Dein", "fey\u00b7er\u00b7li\u00b7cher", "A\u00b7bend\u00b7gru\u00df"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Verk\u00fcndet Auferstehen.", "tokens": ["Ver\u00b7k\u00fcn\u00b7det", "Auf\u00b7er\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}