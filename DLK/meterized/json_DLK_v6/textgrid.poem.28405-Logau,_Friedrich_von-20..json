{"textgrid.poem.28405": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was frag ich nach der Welt? Sie winckt, flucht oder dr\u00e4ut,", "tokens": ["Was", "frag", "ich", "nach", "der", "Welt", "?", "Sie", "winckt", ",", "flucht", "o\u00b7der", "dr\u00e4ut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann mein Mund Gottes Sohn rufft an und sie nicht scheut.", "tokens": ["Wann", "mein", "Mund", "Got\u00b7tes", "Sohn", "rufft", "an", "und", "sie", "nicht", "scheut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "NN", "VVFIN", "PTKVZ", "KON", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Welt ist willig blind; drum hilfft sie keinem Blinden;", "tokens": ["Die", "Welt", "ist", "wil\u00b7lig", "blind", ";", "drum", "hilfft", "sie", "kei\u00b7nem", "Blin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADJD", "$.", "PAV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich aber suche Rath, wo Rath in Noth zu finden.", "tokens": ["Ich", "a\u00b7ber", "su\u00b7che", "Rath", ",", "wo", "Rath", "in", "Noth", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "$,", "PWAV", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}