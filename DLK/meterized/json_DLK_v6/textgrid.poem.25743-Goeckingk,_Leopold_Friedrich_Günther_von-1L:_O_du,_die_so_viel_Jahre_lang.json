{"textgrid.poem.25743": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: O du, die so viel Jahre lang", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du, die so viel Jahre lang", "tokens": ["O", "du", ",", "die", "so", "viel", "Jah\u00b7re", "lang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PRELS", "ADV", "PIAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Nachbar war im Schlaf' und Essen,", "tokens": ["Mein", "Nach\u00b7bar", "war", "im", "Schlaf'", "und", "Es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit mir den Rhein befuhr, und zu den Feueressen", "tokens": ["Mit", "mir", "den", "Rhein", "be\u00b7fuhr", ",", "und", "zu", "den", "Feu\u00b7e\u00b7res\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NE", "VVFIN", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Donners, auf den ", "tokens": ["Des", "Don\u00b7ners", ",", "auf", "den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Verzeihe, da\u00df dich mein Gesang", "tokens": ["Ver\u00b7zei\u00b7he", ",", "da\u00df", "dich", "mein", "Ge\u00b7sang"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hat undankbar bis jetzt vergessen.", "tokens": ["Hat", "un\u00b7dank\u00b7bar", "bis", "jetzt", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ist je aus meinem Kopf' ein Lied,", "tokens": ["Ist", "je", "aus", "mei\u00b7nem", "Kopf'", "ein", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Werth der Unsterblichkeit, gekommen,", "tokens": ["Werth", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", ",", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "So dank' ich's dir allein! Denn die Ideen schwommen", "tokens": ["So", "dank'", "ich's", "dir", "al\u00b7lein", "!", "Denn", "die", "I\u00b7deen", "schwom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "ADV", "$.", "KON", "ART", "NN", "VVINF"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Verwirrt umher, die Stirne gl\u00fcht'", "tokens": ["Ver\u00b7wirrt", "um\u00b7her", ",", "die", "Stir\u00b7ne", "gl\u00fcht'"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und dampfte, aber nichts gerieth,", "tokens": ["Und", "dampf\u00b7te", ",", "a\u00b7ber", "nichts", "ge\u00b7rieth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Bis ich aus dir Rappee genommen.", "tokens": ["Bis", "ich", "aus", "dir", "Rap\u00b7pee", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "NN", "VVPP", "$."], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.13": {"text": "Ich mi\u00dfte, wenn's der Arzt gebot,", "tokens": ["Ich", "mi\u00df\u00b7te", ",", "wenn's", "der", "Arzt", "ge\u00b7bot", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Drei Monat' allen Saft der Reben;", "tokens": ["Drei", "Mo\u00b7nat'", "al\u00b7len", "Saft", "der", "Re\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Wohl k\u00f6nnt' ich ohne ihn, drei ganze Jahre leben,", "tokens": ["Wohl", "k\u00f6nnt'", "ich", "oh\u00b7ne", "ihn", ",", "drei", "gan\u00b7ze", "Jah\u00b7re", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "$,", "CARD", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Vielleicht drei Tag' auch ohne Brod;", "tokens": ["Viel\u00b7leicht", "drei", "Tag'", "auch", "oh\u00b7ne", "Brod", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Doch, ohne dich! \u2013 Ich wollt', o Tod!", "tokens": ["Doch", ",", "oh\u00b7ne", "dich", "!", "\u2013", "Ich", "wollt'", ",", "o", "Tod", "!"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "KOUI", "PPER", "$.", "$(", "PPER", "VMFIN", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Dir lieber beide H\u00e4nde geben.", "tokens": ["Dir", "lie\u00b7ber", "bei\u00b7de", "H\u00e4n\u00b7de", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Du, gute Dose, lehrtest mich", "tokens": ["Du", ",", "gu\u00b7te", "Do\u00b7se", ",", "lehr\u00b7test", "mich"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Die Einsamkeit als J\u00fcngling lieben,", "tokens": ["Die", "Ein\u00b7sam\u00b7keit", "als", "J\u00fcng\u00b7ling", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Bei Schw\u00e4tzern oft Geduld, bei Narren Langmuth \u00fcben,", "tokens": ["Bei", "Schw\u00e4t\u00b7zern", "oft", "Ge\u00b7duld", ",", "bei", "Nar\u00b7ren", "Lang\u00b7muth", "\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "NN", "$,", "APPR", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Mit Fr\u00f6hlichen mich inniglich", "tokens": ["Mit", "Fr\u00f6h\u00b7li\u00b7chen", "mich", "in\u00b7nig\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Erfreuen, mit Betr\u00fcbten mich", "tokens": ["Er\u00b7freu\u00b7en", ",", "mit", "Be\u00b7tr\u00fcb\u00b7ten", "mich"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Noch herzlicher als sonst betr\u00fcben.", "tokens": ["Noch", "herz\u00b7li\u00b7cher", "als", "sonst", "be\u00b7tr\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Was kaum ein Freund dem andern that,", "tokens": ["Was", "kaum", "ein", "Freund", "dem", "an\u00b7dern", "that", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Das thatest du. Du hingst wie eine Klette", "tokens": ["Das", "tha\u00b7test", "du", ".", "Du", "hingst", "wie", "ei\u00b7ne", "Klet\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "An mir; du warst mein Trost, wenn ich im \u00f6den Bette", "tokens": ["An", "mir", ";", "du", "warst", "mein", "Trost", ",", "wenn", "ich", "im", "\u00f6\u00b7den", "Bet\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Den Schlaf vergebens zu mir bat,", "tokens": ["Den", "Schlaf", "ver\u00b7ge\u00b7bens", "zu", "mir", "bat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Mein Trost, wenn ich bei Akten spat", "tokens": ["Mein", "Trost", ",", "wenn", "ich", "bei", "Ak\u00b7ten", "spat"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Noch g\u00e4hnt' und gern geschlummert h\u00e4tte.", "tokens": ["Noch", "g\u00e4hnt'", "und", "gern", "ge\u00b7schlum\u00b7mert", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Werd' ich dereinst zum letztenmal", "tokens": ["Werd'", "ich", "de\u00b7reinst", "zum", "letz\u00b7ten\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPRART", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Die d\u00fcrren kalten H\u00e4nde falten,", "tokens": ["Die", "d\u00fcr\u00b7ren", "kal\u00b7ten", "H\u00e4n\u00b7de", "fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Und \u00fcber meine Brust in beiden fest dich halten,", "tokens": ["Und", "\u00fc\u00b7ber", "mei\u00b7ne", "Brust", "in", "bei\u00b7den", "fest", "dich", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "APPR", "PIAT", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "So wird mit Schluchzen mein Gemahl", "tokens": ["So", "wird", "mit", "Schluch\u00b7zen", "mein", "Ge\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Heraus dich brechen, und so kahl", "tokens": ["He\u00b7raus", "dich", "bre\u00b7chen", ",", "und", "so", "kahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "VVINF", "$,", "KON", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Dein Rock auch ist, doch gern behalten.", "tokens": ["Dein", "Rock", "auch", "ist", ",", "doch", "gern", "be\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "$,", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}