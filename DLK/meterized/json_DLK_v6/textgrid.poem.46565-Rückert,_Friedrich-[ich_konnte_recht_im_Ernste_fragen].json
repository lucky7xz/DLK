{"textgrid.poem.46565": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ich konnte recht im Ernste fragen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich konnte recht im Ernste fragen:", "tokens": ["Ich", "konn\u00b7te", "recht", "im", "Erns\u00b7te", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo ist mein j\u00fcngstes Kn\u00e4bchen?", "tokens": ["Wo", "ist", "mein", "j\u00fcngs\u00b7tes", "Kn\u00e4b\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Um dann mit Lachen mir zu sagen:", "tokens": ["Um", "dann", "mit", "La\u00b7chen", "mir", "zu", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "APPR", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gott Lob, es ist ein M\u00e4dchen", "tokens": ["Gott", "Lob", ",", "es", "ist", "ein", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und soll kein H\u00f6schen tragen!", "tokens": ["Und", "soll", "kein", "H\u00f6\u00b7schen", "tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich konnt' es wirklich wei\u00df mir machen:", "tokens": ["Ich", "konnt'", "es", "wirk\u00b7lich", "wei\u00df", "mir", "ma\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es ist nur eins der Kn\u00e4bchen!", "tokens": ["Es", "ist", "nur", "eins", "der", "Kn\u00e4b\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und dann wie aus dem Traum erwachen:", "tokens": ["Und", "dann", "wie", "aus", "dem", "Traum", "er\u00b7wa\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es ist mein einziges M\u00e4dchen!", "tokens": ["Es", "ist", "mein", "ein\u00b7zi\u00b7ges", "M\u00e4d\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und \u00fcber mich selber lachen.", "tokens": ["Und", "\u00fc\u00b7ber", "mich", "sel\u00b7ber", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Tod versteht nicht Scherz und Lachen,", "tokens": ["Der", "Tod", "ver\u00b7steht", "nicht", "Scherz", "und", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er nahm mein einziges M\u00e4dchen,", "tokens": ["Er", "nahm", "mein", "ein\u00b7zi\u00b7ges", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und als die Zugab in den Rachen", "tokens": ["Und", "als", "die", "Zu\u00b7gab", "in", "den", "Ra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mein wirklich j\u00fcngstes Kn\u00e4bchen;", "tokens": ["Mein", "wirk\u00b7lich", "j\u00fcngs\u00b7tes", "Kn\u00e4b\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Pfui \u00fcber den garstigen Drachen!", "tokens": ["Pfui", "\u00fc\u00b7ber", "den", "gars\u00b7ti\u00b7gen", "Dra\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}