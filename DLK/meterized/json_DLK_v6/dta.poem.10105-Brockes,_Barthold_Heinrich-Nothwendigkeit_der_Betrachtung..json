{"dta.poem.10105": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Nothwendigkeit der Betrachtung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geliebter Mensch, da dich das sch\u00f6ne Bluhmen-Reich,", "tokens": ["Ge\u00b7lieb\u00b7ter", "Mensch", ",", "da", "dich", "das", "sch\u00f6\u00b7ne", "Bluh\u00b7men\u00b7Reich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und alles was demselben gleich,", "tokens": ["Und", "al\u00b7les", "was", "dem\u00b7sel\u00b7ben", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PWS", "PDAT", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So wenig, ja fast gar nicht r\u00fchret,", "tokens": ["So", "we\u00b7nig", ",", "ja", "fast", "gar", "nicht", "r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "ADV", "ADV", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und, wie doch billig w\u00e4r, zu GOttes Lob nicht f\u00fchret,", "tokens": ["Und", ",", "wie", "doch", "bil\u00b7lig", "w\u00e4r", ",", "zu", "Got\u00b7tes", "Lob", "nicht", "f\u00fch\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ADV", "ADJD", "VAFIN", "$,", "APPR", "NN", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ob es gleich noch so sch\u00f6n, so lieblich zugericht;", "tokens": ["Ob", "es", "gleich", "noch", "so", "sch\u00f6n", ",", "so", "lieb\u00b7lich", "zu\u00b7ge\u00b7richt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "ADJD", "$,", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So handelst du ja wider deine Pflicht.", "tokens": ["So", "han\u00b7delst", "du", "ja", "wi\u00b7der", "dei\u00b7ne", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es w\u00e4re denn, (da nichts \u00fcmsonst hervor gebracht)", "tokens": ["Es", "w\u00e4\u00b7re", "denn", ",", "(", "da", "nichts", "\u00fcm\u00b7sonst", "her\u00b7vor", "ge\u00b7bracht", ")"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "$(", "ADV", "PIS", "ADV", "PTKVZ", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df alle diese Pracht", "tokens": ["Da\u00df", "al\u00b7le", "die\u00b7se", "Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr dich so wenig sey, als f\u00fcr das Vieh, gemacht,", "tokens": ["F\u00fcr", "dich", "so", "we\u00b7nig", "sey", ",", "als", "f\u00fcr", "das", "Vieh", ",", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "PIS", "VAFIN", "$,", "KOUS", "APPR", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und da\u00df vielleicht, dem grossen All zum Preise,", "tokens": ["Und", "da\u00df", "viel\u00b7leicht", ",", "dem", "gros\u00b7sen", "All", "zum", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "$,", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In unsers Lufft-Raums weitem Kreise", "tokens": ["In", "un\u00b7sers", "Lufft\u00b7Raums", "wei\u00b7tem", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gantz andre Creaturen lebten,", "tokens": ["Gantz", "and\u00b7re", "Crea\u00b7tu\u00b7ren", "leb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Die GOttes Wercke h\u00f6her sch\u00e4tzten,", "tokens": ["Die", "Got\u00b7tes", "Wer\u00b7cke", "h\u00f6\u00b7her", "sch\u00e4tz\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sich, Jhm zum Ruhm, daran ergetzten,", "tokens": ["Sich", ",", "Jhm", "zum", "Ruhm", ",", "da\u00b7ran", "er\u00b7getz\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "$,", "PPER", "APPRART", "NN", "$,", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und die, den Sch\u00f6pffer zu erheben,", "tokens": ["Und", "die", ",", "den", "Sch\u00f6pf\u00b7fer", "zu", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "In froher Andacht sich bestreben;", "tokens": ["In", "fro\u00b7her", "An\u00b7dacht", "sich", "be\u00b7stre\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und da\u00df denn eigentlich", "tokens": ["Und", "da\u00df", "denn", "ei\u00b7gent\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "F\u00fcr die, und nicht f\u00fcr dich,", "tokens": ["F\u00fcr", "die", ",", "und", "nicht", "f\u00fcr", "dich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "KON", "PTKNEG", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Die Wunder der Natur, und ihre Wunder-Pracht,", "tokens": ["Die", "Wun\u00b7der", "der", "Na\u00b7tur", ",", "und", "ih\u00b7re", "Wun\u00b7der\u00b7Pracht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Erschaffen worden und gemacht.", "tokens": ["Er\u00b7schaf\u00b7fen", "wor\u00b7den", "und", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Mir schauert fast die Haut, wann ich die\u00df \u00fcberlege,", "tokens": ["Mir", "schau\u00b7ert", "fast", "die", "Haut", ",", "wann", "ich", "die\u00df", "\u00fc\u00b7berl\u00b7e\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "PWAV", "PPER", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da\u00df unser Geist hiedurch fast aus der Menschen Orden", "tokens": ["Da\u00df", "un\u00b7ser", "Geist", "hie\u00b7durch", "fast", "aus", "der", "Men\u00b7schen", "Or\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PAV", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Herausgerissen sey, und gantz zum Vieh-Geist worden.", "tokens": ["Her\u00b7aus\u00b7ge\u00b7ris\u00b7sen", "sey", ",", "und", "gantz", "zum", "Vieh\u00b7Geist", "wor\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "KON", "ADV", "APPRART", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ach! lasst uns denn doch k\u00fcnftig uns bem\u00fchn,", "tokens": ["Ach", "!", "lasst", "uns", "denn", "doch", "k\u00fcnf\u00b7tig", "uns", "be\u00b7m\u00fchn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Uns diesem Ungl\u00fcck zu entziehn.", "tokens": ["Uns", "die\u00b7sem", "Un\u00b7gl\u00fcck", "zu", "ent\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lasst uns die F\u00e4higkeit, die uns von GOTT gegeben,", "tokens": ["Lasst", "uns", "die", "F\u00e4\u00b7hig\u00b7keit", ",", "die", "uns", "von", "GoTT", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In Seiner Creatur uns Seiner zu erfreun,", "tokens": ["In", "Sei\u00b7ner", "Crea\u00b7tur", "uns", "Sei\u00b7ner", "zu", "er\u00b7freun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "PPOSAT", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und Seinen Ruhm dadurch best\u00e4ndig zu erneun,", "tokens": ["Und", "Sei\u00b7nen", "Ruhm", "da\u00b7durch", "be\u00b7st\u00e4n\u00b7dig", "zu", "er\u00b7neun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PAV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch besser, als bisher, wol anzuwenden streben!", "tokens": ["Doch", "bes\u00b7ser", ",", "als", "bis\u00b7her", ",", "wol", "an\u00b7zu\u00b7wen\u00b7den", "stre\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "ADV", "$,", "ADV", "VVIZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}