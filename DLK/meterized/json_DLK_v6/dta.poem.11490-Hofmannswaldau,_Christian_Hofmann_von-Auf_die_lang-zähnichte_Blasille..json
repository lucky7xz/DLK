{"dta.poem.11490": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die lang-z\u00e4hnichte Blasille.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man wundert sich, warum Blasille keinen mann,", "tokens": ["Man", "wun\u00b7dert", "sich", ",", "wa\u00b7rum", "Bla\u00b7sil\u00b7le", "kei\u00b7nen", "mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "$,", "PWAV", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den sie doch gerne h\u00e4tt\u2019, annoch bekommen kan;", "tokens": ["Den", "sie", "doch", "ger\u00b7ne", "h\u00e4tt'", ",", "an\u00b7noch", "be\u00b7kom\u00b7men", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VAFIN", "$,", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die ursach ist verhasst, doch will ich sie erwehnen:", "tokens": ["Die", "ur\u00b7sach", "ist", "ver\u00b7hasst", ",", "doch", "will", "ich", "sie", "er\u00b7weh\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "ADV", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein jeder f\u00fcrchtet sich vor ihren langen z\u00e4hnen.", "tokens": ["Ein", "je\u00b7der", "f\u00fcrch\u00b7tet", "sich", "vor", "ih\u00b7ren", "lan\u00b7gen", "z\u00e4h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}