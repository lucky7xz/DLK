{"textgrid.poem.51130": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fcr ein Gesangfest im Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1878, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jetzt ist des Winters grimmer Frost", "tokens": ["Jetzt", "ist", "des", "Win\u00b7ters", "grim\u00b7mer", "Frost"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entflohen aus den Landen", "tokens": ["Ent\u00b7flo\u00b7hen", "aus", "den", "Lan\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und rings der reiche Blumentrost", "tokens": ["Und", "rings", "der", "rei\u00b7che", "Blu\u00b7men\u00b7trost"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Feld und Hag erstanden;", "tokens": ["In", "Feld", "und", "Hag", "er\u00b7stan\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und singt auch keine Nachtigall", "tokens": ["Und", "singt", "auch", "kei\u00b7ne", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im weiten Tal mit s\u00fc\u00dfem Schall,", "tokens": ["Im", "wei\u00b7ten", "Tal", "mit", "s\u00fc\u00b7\u00dfem", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So gehn wir Leute selber dran", "tokens": ["So", "gehn", "wir", "Leu\u00b7te", "sel\u00b7ber", "dran"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ADV", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und stimmen hell das Lenzlied an!", "tokens": ["Und", "stim\u00b7men", "hell", "das", "Lenz\u00b7lied", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Zeit ist rauh und schwer der Tag,", "tokens": ["Die", "Zeit", "ist", "rauh", "und", "schwer", "der", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An Not und Neid kein Mangel;", "tokens": ["An", "Not", "und", "Neid", "kein", "Man\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es zuckt das Herz mit bangem Schlag", "tokens": ["Es", "zuckt", "das", "Herz", "mit", "ban\u00b7gem", "Schlag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie 's Fischlein an der Angel;", "tokens": ["Wie", "'s", "Fisc\u00b7hlein", "an", "der", "An\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch steht die Welt in Sorgen still,", "tokens": ["Doch", "steht", "die", "Welt", "in", "Sor\u00b7gen", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wenn sich keiner fassen will,", "tokens": ["Und", "wenn", "sich", "kei\u00b7ner", "fas\u00b7sen", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PRF", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So gehn wir Leute dennoch dran", "tokens": ["So", "gehn", "wir", "Leu\u00b7te", "den\u00b7noch", "dran"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ADV", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und heben laut das Lenzlied an!", "tokens": ["Und", "he\u00b7ben", "laut", "das", "Lenz\u00b7lied", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Verschlie\u00dft des Kummers dunkle Gruft", "tokens": ["Ver\u00b7schlie\u00dft", "des", "Kum\u00b7mers", "dunk\u00b7le", "Gruft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und stellet ein das Klagen!", "tokens": ["Und", "stel\u00b7let", "ein", "das", "Kla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00dft lieber uns die Maienluft", "tokens": ["La\u00dft", "lie\u00b7ber", "uns", "die", "Mai\u00b7en\u00b7luft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit seidnen Fahnen schlagen!", "tokens": ["Mit", "seid\u00b7nen", "Fah\u00b7nen", "schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So treiben wir den Teufel aus,", "tokens": ["So", "trei\u00b7ben", "wir", "den", "Teu\u00b7fel", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schon wird es frei und licht im Haus!", "tokens": ["Schon", "wird", "es", "frei", "und", "licht", "im", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wir aber reihn uns Mann zu Mann", "tokens": ["Wir", "a\u00b7ber", "reihn", "uns", "Mann", "zu", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und heben froh das Lenzlied an!", "tokens": ["Und", "he\u00b7ben", "froh", "das", "Lenz\u00b7lied", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}