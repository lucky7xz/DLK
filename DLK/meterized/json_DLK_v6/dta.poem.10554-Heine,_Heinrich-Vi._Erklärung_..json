{"dta.poem.10554": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vi.  \n  Erkl\u00e4rung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.57", "da:0.42"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Heranged\u00e4mmert kam der Abend,", "tokens": ["Her\u00b7an\u00b7ge\u00b7d\u00e4m\u00b7mert", "kam", "der", "A\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wilder tos'te die Fluth,", "tokens": ["Wil\u00b7der", "to\u00b7s'\u00b7te", "die", "Fluth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Und ich sa\u00df am Strand, und schaute zu", "tokens": ["Und", "ich", "sa\u00df", "am", "Strand", ",", "und", "schau\u00b7te", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "$,", "KON", "VVFIN", "APPR"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Dem wei\u00dfen Tanz der Wellen,", "tokens": ["Dem", "wei\u00b7\u00dfen", "Tanz", "der", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und meine Brust schwoll auf wie das Meer,", "tokens": ["Und", "mei\u00b7ne", "Brust", "schwoll", "auf", "wie", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "PTKVZ", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und sehnend ergriff mich ein tiefes Heimweh", "tokens": ["Und", "seh\u00b7nend", "er\u00b7griff", "mich", "ein", "tie\u00b7fes", "Heim\u00b7weh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.7": {"text": "Nach dir, du holdes Bild,", "tokens": ["Nach", "dir", ",", "du", "hol\u00b7des", "Bild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Das \u00fcberall mich umschwebt,", "tokens": ["Das", "\u00fc\u00b7be\u00b7rall", "mich", "um\u00b7schwebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Und \u00fcberall mich ruft,", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", "mich", "ruft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Ueberall, \u00fcberall,", "tokens": ["Ue\u00b7be\u00b7rall", ",", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.11": {"text": "Im Sausen des Windes, im Brausen des Meers,", "tokens": ["Im", "Sau\u00b7sen", "des", "Win\u00b7des", ",", "im", "Brau\u00b7sen", "des", "Meers", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.12": {"text": "Und im Seufzen der eigenen Brust.", "tokens": ["Und", "im", "Seuf\u00b7zen", "der", "ei\u00b7ge\u00b7nen", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Mit leichtem Rohr schrieb ich in den Sand:", "tokens": ["Mit", "leich\u00b7tem", "Rohr", "schrieb", "ich", "in", "den", "Sand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "\u201eagnes, ich liebe Dich!\u201c", "tokens": ["\u201e", "ag\u00b7nes", ",", "ich", "lie\u00b7be", "Dich", "!", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "$,", "PPER", "VVFIN", "PPER", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Doch b\u00f6se Wellen ergossen sich", "tokens": ["Doch", "b\u00f6\u00b7se", "Wel\u00b7len", "er\u00b7gos\u00b7sen", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PRF"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ueber das s\u00fc\u00dfe Bekenntni\u00df,", "tokens": ["Ue\u00b7ber", "das", "s\u00fc\u00b7\u00dfe", "Be\u00b7kennt\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Und l\u00f6schten es aus.", "tokens": ["Und", "l\u00f6schten", "es", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Zerbrechliches Rohr, zerstiebender Sand,", "tokens": ["Zer\u00b7brech\u00b7li\u00b7ches", "Rohr", ",", "zer\u00b7stie\u00b7ben\u00b7der", "Sand", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zerflie\u00dfende Wellen, Euch trau' ich nicht mehr!", "tokens": ["Zer\u00b7flie\u00b7\u00dfen\u00b7de", "Wel\u00b7len", ",", "Euch", "trau'", "ich", "nicht", "mehr", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+--+-++--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Himmel wird dunkler, mein Herz wird wilder,", "tokens": ["Der", "Him\u00b7mel", "wird", "dunk\u00b7ler", ",", "mein", "Herz", "wird", "wil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "$,", "PPOSAT", "NN", "VAFIN", "ADJA", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und mit starker Hand, aus Norwegs W\u00e4ldern", "tokens": ["Und", "mit", "star\u00b7ker", "Hand", ",", "aus", "Nor\u00b7wegs", "W\u00e4l\u00b7dern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "$,", "APPR", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Rei\u00df ich die h\u00f6chste Tanne,", "tokens": ["Rei\u00df", "ich", "die", "h\u00f6chs\u00b7te", "Tan\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und tauche sie ein", "tokens": ["Und", "tau\u00b7che", "sie", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "In des Aetnas gl\u00fchenden Schlund, und mit solcher", "tokens": ["In", "des", "A\u00b7et\u00b7nas", "gl\u00fc\u00b7hen\u00b7den", "Schlund", ",", "und", "mit", "sol\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,", "KON", "APPR", "PIAT"], "meter": "---+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Feuergetr\u00e4nkten Riesenfeder", "tokens": ["Feu\u00b7er\u00b7ge\u00b7tr\u00e4nk\u00b7ten", "Rie\u00b7sen\u00b7fe\u00b7der"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Schreib' ich an die dunkle Himmelsdecke:", "tokens": ["Schreib'", "ich", "an", "die", "dunk\u00b7le", "Him\u00b7mels\u00b7de\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "\u201eagnes, ich liebe Dich!\u201c", "tokens": ["\u201e", "ag\u00b7nes", ",", "ich", "lie\u00b7be", "Dich", "!", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "$,", "PPER", "VVFIN", "PPER", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Jedwede Nacht lodert alsdann", "tokens": ["Jed\u00b7we\u00b7de", "Nacht", "lo\u00b7dert", "als\u00b7dann"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Dort oben die ewige Flammenschrift,", "tokens": ["Dort", "o\u00b7ben", "die", "e\u00b7wi\u00b7ge", "Flam\u00b7men\u00b7schrift", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und alle nachwachsende Enkelgeschlechter", "tokens": ["Und", "al\u00b7le", "nach\u00b7wach\u00b7sen\u00b7de", "En\u00b7kel\u00b7ge\u00b7schlech\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Lesen jauchzend die Himmelsworte:", "tokens": ["Le\u00b7sen", "jauch\u00b7zend", "die", "Him\u00b7mels\u00b7wor\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "\u201eagnes, ich liebe Dich!\u201c", "tokens": ["\u201e", "ag\u00b7nes", ",", "ich", "lie\u00b7be", "Dich", "!", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "$,", "PPER", "VVFIN", "PPER", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}