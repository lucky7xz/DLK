{"textgrid.poem.55881": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Der S\u00e4nger singt vor einem", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du blasses Kind, an jedem Abend soll", "tokens": ["Du", "blas\u00b7ses", "Kind", ",", "an", "je\u00b7dem", "A\u00b7bend", "soll"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "APPR", "PIAT", "NN", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "der S\u00e4nger dunkel stehn bei deinen Dingen", "tokens": ["der", "S\u00e4n\u00b7ger", "dun\u00b7kel", "stehn", "bei", "dei\u00b7nen", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und soll dir Sagen, die im Blute klingen,", "tokens": ["und", "soll", "dir", "Sa\u00b7gen", ",", "die", "im", "Blu\u00b7te", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00fcber die Br\u00fccke seiner Stimme bringen", "tokens": ["\u00fc\u00b7ber", "die", "Br\u00fc\u00b7cke", "sei\u00b7ner", "Stim\u00b7me", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "und eine Harfe, seiner H\u00e4nde voll.", "tokens": ["und", "ei\u00b7ne", "Har\u00b7fe", ",", "sei\u00b7ner", "H\u00e4n\u00b7de", "voll", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nicht aus der Zeit ist, was er dir erz\u00e4hlt,", "tokens": ["Nicht", "aus", "der", "Zeit", "ist", ",", "was", "er", "dir", "er\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VAFIN", "$,", "PWS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "gehoben ist es wie aus Wandgeweben;", "tokens": ["ge\u00b7ho\u00b7ben", "ist", "es", "wie", "aus", "Wand\u00b7ge\u00b7we\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "KOKOM", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "solche Gestalten hat es nie gegeben, \u2013", "tokens": ["sol\u00b7che", "Ge\u00b7stal\u00b7ten", "hat", "es", "nie", "ge\u00b7ge\u00b7ben", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "und Niegewesenes nennt er das Leben.", "tokens": ["und", "Nie\u00b7ge\u00b7we\u00b7se\u00b7nes", "nennt", "er", "das", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und heute hat er diesen Sang erw\u00e4hlt:", "tokens": ["Und", "heu\u00b7te", "hat", "er", "die\u00b7sen", "Sang", "er\u00b7w\u00e4hlt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du blondes Kind von F\u00fcrsten und aus Frauen,", "tokens": ["Du", "blon\u00b7des", "Kind", "von", "F\u00fcrs\u00b7ten", "und", "aus", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die einsam warteten im wei\u00dfen Saal, \u2013", "tokens": ["die", "ein\u00b7sam", "war\u00b7te\u00b7ten", "im", "wei\u00b7\u00dfen", "Saal", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPRART", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "fast alle waren bang, dich aufzubauen,", "tokens": ["fast", "al\u00b7le", "wa\u00b7ren", "bang", ",", "dich", "auf\u00b7zu\u00b7bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "ADJD", "$,", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "um aus den Bildern einst auf dich zu schauen:", "tokens": ["um", "aus", "den", "Bil\u00b7dern", "einst", "auf", "dich", "zu", "schau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "NN", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "auf deine Augen mit den ernsten Brauen,", "tokens": ["auf", "dei\u00b7ne", "Au\u00b7gen", "mit", "den", "erns\u00b7ten", "Brau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "auf deine H\u00e4nde, hell und schmal.", "tokens": ["auf", "dei\u00b7ne", "H\u00e4n\u00b7de", ",", "hell", "und", "schmal", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du hast von ihnen Perlen und T\u00fcrkisen,", "tokens": ["Du", "hast", "von", "ih\u00b7nen", "Per\u00b7len", "und", "T\u00fcr\u00b7ki\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "von diesen Frauen, die in Bildern stehn", "tokens": ["von", "die\u00b7sen", "Frau\u00b7en", ",", "die", "in", "Bil\u00b7dern", "stehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "als st\u00fcnden sie allein in Abendwiesen, \u2013", "tokens": ["als", "st\u00fcn\u00b7den", "sie", "al\u00b7lein", "in", "Ab\u00b7end\u00b7wie\u00b7sen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "du hast von ihnen Perlen und T\u00fcrkisen", "tokens": ["du", "hast", "von", "ih\u00b7nen", "Per\u00b7len", "und", "T\u00fcr\u00b7ki\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "und Ringe mit verdunkelten Devisen", "tokens": ["und", "Rin\u00b7ge", "mit", "ver\u00b7dun\u00b7kel\u00b7ten", "De\u00b7vi\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "und Seiden, welche welke D\u00fcfte wehn.", "tokens": ["und", "Sei\u00b7den", ",", "wel\u00b7che", "wel\u00b7ke", "D\u00fcf\u00b7te", "wehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "PWAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Du tr\u00e4gst die Gemmen ihrer G\u00fcrtelb\u00e4nder", "tokens": ["Du", "tr\u00e4gst", "die", "Gem\u00b7men", "ih\u00b7rer", "G\u00fcr\u00b7tel\u00b7b\u00e4n\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "ans hohe Fenster in den Glanz der Stunden,", "tokens": ["ans", "ho\u00b7he", "Fens\u00b7ter", "in", "den", "Glanz", "der", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und in die Seide sanfter Brautgew\u00e4nder", "tokens": ["und", "in", "die", "Sei\u00b7de", "sanf\u00b7ter", "Braut\u00b7ge\u00b7w\u00e4n\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "sind deine kleinen B\u00fccher eingebunden,", "tokens": ["sind", "dei\u00b7ne", "klei\u00b7nen", "B\u00fc\u00b7cher", "ein\u00b7ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "und drinnen hast du, m\u00e4chtig \u00fcber L\u00e4nder,", "tokens": ["und", "drin\u00b7nen", "hast", "du", ",", "m\u00e4ch\u00b7tig", "\u00fc\u00b7ber", "L\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "ganz gro\u00df geschrieben und mit reichen, runden", "tokens": ["ganz", "gro\u00df", "ge\u00b7schrie\u00b7ben", "und", "mit", "rei\u00b7chen", ",", "run\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADJD", "VVPP", "KON", "APPR", "ADJA", "$,", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Buchstaben deinen Namen vorgefunden.", "tokens": ["Buch\u00b7sta\u00b7ben", "dei\u00b7nen", "Na\u00b7men", "vor\u00b7ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und alles ist, als w\u00e4r es schon geschehn.", "tokens": ["Und", "al\u00b7les", "ist", ",", "als", "w\u00e4r", "es", "schon", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "$,", "KOKOM", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Sie haben so, als ob du nicht mehr k\u00e4mst,", "tokens": ["Sie", "ha\u00b7ben", "so", ",", "als", "ob", "du", "nicht", "mehr", "k\u00e4mst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "KOKOM", "KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "an alle Becher ihren Mund gesetzt,", "tokens": ["an", "al\u00b7le", "Be\u00b7cher", "ih\u00b7ren", "Mund", "ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "zu allen Freuden ihr Gef\u00fchl gehetzt", "tokens": ["zu", "al\u00b7len", "Freu\u00b7den", "ihr", "Ge\u00b7f\u00fchl", "ge\u00b7hetzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und keinem Leide leidlos zugesehn;", "tokens": ["und", "kei\u00b7nem", "Lei\u00b7de", "leid\u00b7los", "zu\u00b7ge\u00b7sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "so da\u00df du jetzt", "tokens": ["so", "da\u00df", "du", "jetzt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "stehst und dich sch\u00e4mst.", "tokens": ["stehst", "und", "dich", "sch\u00e4mst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "PPER", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "... Du blasses Kind, dein Leben ist auch eines, \u2013", "tokens": ["...", "Du", "blas\u00b7ses", "Kind", ",", "dein", "Le\u00b7ben", "ist", "auch", "ei\u00b7nes", ",", "\u2013"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "PIS", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "der S\u00e4nger kommt dir sagen, da\u00df du bist.", "tokens": ["der", "S\u00e4n\u00b7ger", "kommt", "dir", "sa\u00b7gen", ",", "da\u00df", "du", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und da\u00df du mehr bist als ein Traum des Haines,", "tokens": ["Und", "da\u00df", "du", "mehr", "bist", "als", "ein", "Traum", "des", "Hai\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VAFIN", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "mehr als die Seligkeit des Sonnenscheines,", "tokens": ["mehr", "als", "die", "Se\u00b7lig\u00b7keit", "des", "Son\u00b7nen\u00b7schei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "den mancher graue Tag vergi\u00dft.", "tokens": ["den", "man\u00b7cher", "grau\u00b7e", "Tag", "ver\u00b7gi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Leben ist so unaussprechlich Deines,", "tokens": ["Dein", "Le\u00b7ben", "ist", "so", "un\u00b7aus\u00b7sprech\u00b7lich", "Dei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "weil es von vielen \u00fcberladen ist.", "tokens": ["weil", "es", "von", "vie\u00b7len", "\u00fc\u00b7berl\u00b7a\u00b7den", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Empfindest du, wie die Vergangenheiten", "tokens": ["Emp\u00b7fin\u00b7dest", "du", ",", "wie", "die", "Ver\u00b7gan\u00b7gen\u00b7hei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "leicht werden, wenn du eine Weile lebst,", "tokens": ["leicht", "wer\u00b7den", ",", "wenn", "du", "ei\u00b7ne", "Wei\u00b7le", "lebst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "wie sie dich sanft auf Wunder vorbereiten,", "tokens": ["wie", "sie", "dich", "sanft", "auf", "Wun\u00b7der", "vor\u00b7be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADJD", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "jedes Gef\u00fchl mit Bildern dir begleiten, \u2013", "tokens": ["je\u00b7des", "Ge\u00b7f\u00fchl", "mit", "Bil\u00b7dern", "dir", "be\u00b7glei\u00b7ten", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "PPER", "VVINF", "$,", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "und nur ein Zeichen scheinen ganze Zeiten", "tokens": ["und", "nur", "ein", "Zei\u00b7chen", "schei\u00b7nen", "gan\u00b7ze", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "f\u00fcr eine Geste, die du sch\u00f6n erhebst. \u2013", "tokens": ["f\u00fcr", "ei\u00b7ne", "Ges\u00b7te", ",", "die", "du", "sch\u00f6n", "er\u00b7hebst", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Das ist der Sinn von allem, was einst war,", "tokens": ["Das", "ist", "der", "Sinn", "von", "al\u00b7lem", ",", "was", "einst", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "PIS", "$,", "PRELS", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "da\u00df es nicht bleibt mit seiner ganzen Schwere,", "tokens": ["da\u00df", "es", "nicht", "bleibt", "mit", "sei\u00b7ner", "gan\u00b7zen", "Schwe\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "da\u00df es zu unserm Wesen wiederkehre,", "tokens": ["da\u00df", "es", "zu", "un\u00b7serm", "We\u00b7sen", "wie\u00b7der\u00b7keh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "in uns verwoben, tief und wunderbar:", "tokens": ["in", "uns", "ver\u00b7wo\u00b7ben", ",", "tief", "und", "wun\u00b7der\u00b7bar", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "So waren diese Frauen elfenbeinern,", "tokens": ["So", "wa\u00b7ren", "die\u00b7se", "Frau\