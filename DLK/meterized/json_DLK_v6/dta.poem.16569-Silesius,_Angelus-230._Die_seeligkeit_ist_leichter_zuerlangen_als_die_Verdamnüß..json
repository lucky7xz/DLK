{"dta.poem.16569": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "230.  Die seeligkeit ist leichter zuerlangen  \n als die Verdamn\u00fc\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Es dunkt mich leichter seyn in Himmel sich zuschwingen;", "tokens": ["Es", "dunkt", "mich", "leich\u00b7ter", "seyn", "in", "Him\u00b7mel", "sich", "zu\u00b7schwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "VAINF", "APPR", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als mit der S\u00fcnden m\u00fch in Abgrund ein zu dringen.", "tokens": ["Als", "mit", "der", "S\u00fcn\u00b7den", "m\u00fch", "in", "Ab\u00b7grund", "ein", "zu", "drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJD", "APPR", "NN", "ART", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}