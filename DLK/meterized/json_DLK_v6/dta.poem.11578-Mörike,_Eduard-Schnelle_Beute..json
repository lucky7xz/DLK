{"dta.poem.11578": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Schnelle Beute.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Hat der Dichter im Geist ein k\u00f6stliches Liedchen empfangen,", "tokens": ["Hat", "der", "Dich\u00b7ter", "im", "Geist", "ein", "k\u00f6st\u00b7li\u00b7ches", "Lied\u00b7chen", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPRART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ruht und rastet er nicht, bis es vollendet ihn gr\u00fc\u00dft:", "tokens": ["Ruht", "und", "ras\u00b7tet", "er", "nicht", ",", "bis", "es", "voll\u00b7en\u00b7det", "ihn", "gr\u00fc\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Sch\u00f6nste! so sah ich dich neulich zum ersten Mal fl\u00fcchtig", "tokens": ["Sch\u00f6ns\u00b7te", "!", "so", "sah", "ich", "dich", "neu\u00b7lich", "zum", "ers\u00b7ten", "Mal", "fl\u00fcch\u00b7tig"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "VVFIN", "PPER", "PRF", "ADV", "APPRART", "ADJA", "NN", "ADJD"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.4": {"text": "Und ich brannte; nun liegst heute du mir schon im", "tokens": ["Und", "ich", "brann\u00b7te", ";", "nun", "liegst", "heu\u00b7te", "du", "mir", "schon", "im"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "ADV", "VVFIN", "ADV", "PPER", "PPER", "ADV", "APPRART"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}}}}}