{"dta.poem.12803": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als  \n Herr  D.  Joh. Franciscus Buddeus  \n das Academische Regiment wieder  \n niederlegte.  \n Jm namen Herr  M.  Uhles.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das macht, da\u00df GOttes Geist,", "tokens": ["Das", "macht", ",", "da\u00df", "Got\u00b7tes", "Geist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dem sich ein kluger sinn zu dienen stets beflei\u00dft,", "tokens": ["Dem", "sich", "ein", "klu\u00b7ger", "sinn", "zu", "die\u00b7nen", "stets", "be\u00b7flei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ART", "ADJA", "NN", "PTKZU", "VVINF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit seiner lieb und furcht sein hertze zubereitet.", "tokens": ["Mit", "sei\u00b7ner", "lieb", "und", "furcht", "sein", "hert\u00b7ze", "zu\u00b7be\u00b7rei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "KON", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}