{"textgrid.poem.63294": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Berliner Mittelstandsbegr\u00e4bnis", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einer Margarinekiste habe ich sie begraben.", "tokens": ["In", "ei\u00b7ner", "Mar\u00b7ga\u00b7ri\u00b7ne\u00b7kis\u00b7te", "ha\u00b7be", "ich", "sie", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Ein Leihsarg war nicht mehr zu haben.", "tokens": ["Ein", "Leihs\u00b7arg", "war", "nicht", "mehr", "zu", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Kosten f\u00fcr einen Begr\u00e4bnisplatz konnt ich nicht erschwingen:", "tokens": ["Die", "Kos\u00b7ten", "f\u00fcr", "ei\u00b7nen", "Be\u00b7gr\u00e4b\u00b7nis\u00b7platz", "konnt", "ich", "nicht", "er\u00b7schwin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+--+-+-+-", "measure": "amphibrach.tetra.plus"}, "line.4": {"text": "Ich mu\u00dfte die Margarinekiste mit der teueren Entschlafenen", "tokens": ["Ich", "mu\u00df\u00b7te", "die", "Mar\u00b7ga\u00b7ri\u00b7ne\u00b7kis\u00b7te", "mit", "der", "teu\u00b7e\u00b7ren", "Ent\u00b7schla\u00b7fe\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+---+--+-+--+--+--", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "auf einem Handwagen in die Laubenkolonie", "tokens": ["auf", "ei\u00b7nem", "Hand\u00b7wa\u00b7gen", "in", "die", "Lau\u00b7ben\u00b7ko\u00b7lo\u00b7nie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "am schlesischen Bahnhof bringen.", "tokens": ["am", "schle\u00b7si\u00b7schen", "Bahn\u00b7hof", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Dort habe ich sie in stockfinsterer Nacht", "tokens": ["Dort", "ha\u00b7be", "ich", "sie", "in", "stock\u00b7fins\u00b7te\u00b7rer", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Unter Kohlr\u00fcben zur ewigen Ruhe gebracht.", "tokens": ["Un\u00b7ter", "Kohl\u00b7r\u00fc\u00b7ben", "zur", "e\u00b7wi\u00b7gen", "Ru\u00b7he", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "Aber im Fr\u00fchling werden aus der Erde Kohlr\u00fcben,", "tokens": ["A\u00b7ber", "im", "Fr\u00fch\u00b7ling", "wer\u00b7den", "aus", "der", "Er\u00b7de", "Kohl\u00b7r\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-+-++-", "measure": "iambic.septa.invert"}}, "stanza.4": {"line.1": {"text": "die sie mit ihrem Leibe ged\u00fcngt,", "tokens": ["die", "sie", "mit", "ih\u00b7rem", "Lei\u00b7be", "ge\u00b7d\u00fcngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "zum himmlischen Lichte sprie\u00dfen,", "tokens": ["zum", "himm\u00b7li\u00b7schen", "Lich\u00b7te", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Und der Hilfsweichensteller Kraschunke wird sie zum Nachtmahl genie\u00dfen.", "tokens": ["Und", "der", "Hilfs\u00b7wei\u00b7chen\u00b7stel\u00b7ler", "Kra\u00b7schun\u00b7ke", "wird", "sie", "zum", "Nacht\u00b7mahl", "ge\u00b7nie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VAFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "---+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "W\u00e4hrend sie noch in der Pfanne (in Margarine-Ersatz) schmoren und braten,", "tokens": ["W\u00e4h\u00b7rend", "sie", "noch", "in", "der", "Pfan\u00b7ne", "(", "in", "Mar\u00b7ga\u00b7ri\u00b7ne\u00b7Er\u00b7satz", ")", "schmo\u00b7ren", "und", "bra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "$(", "APPR", "NE", "$(", "VVINF", "KON", "VVFIN", "$,"], "meter": "+--+--+--+-+-+-+--+-", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Bemerkt Frau Kraschunke erfreut: \u00bbDie Kohlr\u00fcben sind dieses Jahr", "tokens": ["Be\u00b7merkt", "Frau", "Kra\u00b7schun\u00b7ke", "er\u00b7freut", ":", "\u00bb", "Die", "Kohl\u00b7r\u00fc\u00b7ben", "sind", "die\u00b7ses", "Jahr"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "NN", "VVFIN", "$.", "$(", "ART", "NN", "VAFIN", "PDAT", "NN"], "meter": "-+--+--+-+-+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "aber ungew\u00f6hnlich gro\u00df geraten...\u00ab", "tokens": ["a\u00b7ber", "un\u00b7ge\u00b7w\u00f6hn\u00b7lich", "gro\u00df", "ge\u00b7ra\u00b7ten", "...", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "ADJD", "VVPP", "$(", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}