{"textgrid.poem.37917": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der Pfalzgraf am Rhein", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wohnt' ein Pfalzgraf an dem Rhein,", "tokens": ["Es", "wohnt'", "ein", "Pfalz\u00b7graf", "an", "dem", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der lie\u00df verjagen sein Schwesterlein,", "tokens": ["Der", "lie\u00df", "ver\u00b7ja\u00b7gen", "sein", "Schwes\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da kam der K\u00fcchenjung zu ihm:", "tokens": ["Da", "kam", "der", "K\u00fc\u00b7chen\u00b7jung", "zu", "ihm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbwillkommen! Willkommen, Pfalzgraf am Rhein!", "tokens": ["\u00bb", "will\u00b7kom\u00b7men", "!", "Will\u00b7kom\u00b7men", ",", "Pfalz\u00b7graf", "am", "Rhein", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$.", "NN", "$,", "NN", "APPRART", "NE", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Wo ist dein sch\u00f6nes Schwesterlein?\u00ab", "tokens": ["Wo", "ist", "dein", "sch\u00f6\u00b7nes", "Schwes\u00b7ter\u00b7lein", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbmein Schwesterlein die kriegst du nicht,", "tokens": ["\u00bb", "mein", "Schwes\u00b7ter\u00b7lein", "die", "kriegst", "du", "nicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "ART", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie ist dir viel zu adelich,", "tokens": ["Sie", "ist", "dir", "viel", "zu", "a\u00b7de\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und du geh\u00f6rst zur K\u00fcch hinein.\u00ab", "tokens": ["Und", "du", "ge\u00b7h\u00f6rst", "zur", "K\u00fcch", "hin\u00b7ein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbwarum sollt ich sie kriegen nicht,", "tokens": ["\u00bb", "wa\u00b7rum", "sollt", "ich", "sie", "krie\u00b7gen", "nicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VMFIN", "PPER", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie hat von mir ein Kindelein.\u00ab", "tokens": ["Sie", "hat", "von", "mir", "ein", "Kin\u00b7de\u00b7lein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbhat sie von dir ein Kindelein,", "tokens": ["\u00bb", "hat", "sie", "von", "dir", "ein", "Kin\u00b7de\u00b7lein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "APPR", "PPER", "ART", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Soll sie nicht mehr mein Schwester seyn.\u00ab", "tokens": ["Soll", "sie", "nicht", "mehr", "mein", "Schwes\u00b7ter", "seyn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "PPOSAT", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er lie\u00df sie gei\u00dfeln drei ganzer Tag,", "tokens": ["Er", "lie\u00df", "sie", "gei\u00b7\u00dfeln", "drei", "gan\u00b7zer", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Bis man ihr Lung und Leber sah:", "tokens": ["Bis", "man", "ihr", "Lung", "und", "Le\u00b7ber", "sah", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbh\u00f6r auf, h\u00f6r auf, es ist genug,", "tokens": ["\u00bb", "h\u00f6r", "auf", ",", "h\u00f6r", "auf", ",", "es", "ist", "ge\u00b7nug", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es geh\u00f6rt dem K\u00f6nig aus Engelland.\u00ab", "tokens": ["Es", "ge\u00b7h\u00f6rt", "dem", "K\u00f6\u00b7nig", "aus", "En\u00b7gel\u00b7land", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NE", "$.", "$("], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbgeh\u00f6rt es dem K\u00f6nig von Engelland,", "tokens": ["\u00bb", "ge\u00b7h\u00f6rt", "es", "dem", "K\u00f6\u00b7nig", "von", "En\u00b7gel\u00b7land", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "So kostet mich's mein ganzes Land,", "tokens": ["So", "kos\u00b7tet", "mich's", "mein", "gan\u00b7zes", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein ganzes Land ist nicht genug,", "tokens": ["Mein", "gan\u00b7zes", "Land", "ist", "nicht", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Leben mu\u00df auch noch darzu.\u00ab", "tokens": ["Mein", "Le\u00b7ben", "mu\u00df", "auch", "noch", "dar\u00b7zu", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "ADV", "PAV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Es stund nicht l\u00e4nger als drei Tag' an,", "tokens": ["Es", "stund", "nicht", "l\u00e4n\u00b7ger", "als", "drei", "Tag'", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJD", "KOKOM", "CARD", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da kam der K\u00f6nig aus Engelland:", "tokens": ["Da", "kam", "der", "K\u00f6\u00b7nig", "aus", "En\u00b7gel\u00b7land", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bbwillkommen, willkommen Pfalzgraf am Rhein,", "tokens": ["\u00bb", "will\u00b7kom\u00b7men", ",", "will\u00b7kom\u00b7men", "Pfalz\u00b7graf", "am", "Rhein", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "ADJA", "NN", "APPRART", "NE", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wo ist, wo ist dein Schwesterlein?\u00ab", "tokens": ["Wo", "ist", ",", "wo", "ist", "dein", "Schwes\u00b7ter\u00b7lein", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "$,", "PWAV", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbmein Schwesterlein, die ist schon todt,", "tokens": ["\u00bb", "mein", "Schwes\u00b7ter\u00b7lein", ",", "die", "ist", "schon", "todt", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PRELS", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie liegt begraben r\u00f6slinroth.\u00ab", "tokens": ["Sie", "liegt", "be\u00b7gra\u00b7ben", "r\u00f6s\u00b7lin\u00b7roth", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbliegt sie begraben r\u00f6slinroth,", "tokens": ["\u00bb", "liegt", "sie", "be\u00b7gra\u00b7ben", "r\u00f6s\u00b7lin\u00b7roth", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "VVPP", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "So mu\u00dft du leiden den bittern Tod.\u00ab", "tokens": ["So", "mu\u00dft", "du", "lei\u00b7den", "den", "bit\u00b7tern", "Tod", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Selbst zog er sein schweres goldnes Schwerdt,", "tokens": ["Selbst", "zog", "er", "sein", "schwe\u00b7res", "gold\u00b7nes", "Schwerdt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und stach es dem Pfalzgrafen durch sein Herz:", "tokens": ["Und", "stach", "es", "dem", "Pfalz\u00b7gra\u00b7fen", "durch", "sein", "Herz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "\u00bbhat sie m\u00fcssen leiden den bittern Tod,", "tokens": ["\u00bb", "hat", "sie", "m\u00fcs\u00b7sen", "lei\u00b7den", "den", "bit\u00b7tern", "Tod", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "VMFIN", "VVINF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "So mu\u00dft du leiden den Schmerz.\u00ab", "tokens": ["So", "mu\u00dft", "du", "lei\u00b7den", "den", "Schmerz", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}