{"textgrid.poem.27949": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "64.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kumm in diese Welt, hindurch dort nauff zu reisen;", "tokens": ["Ich", "kumm", "in", "die\u00b7se", "Welt", ",", "hin\u00b7durch", "dort", "nauff", "zu", "rei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PDAT", "NN", "$,", "PAV", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil Christus ist der Weg, so wird er mich wol weisen.", "tokens": ["Weil", "Chris\u00b7tus", "ist", "der", "Weg", ",", "so", "wird", "er", "mich", "wol", "wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VAFIN", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich kan in dieser Welt viel Redligkeit nicht schauen;", "tokens": ["Ich", "kan", "in", "die\u00b7ser", "Welt", "viel", "Red\u00b7lig\u00b7keit", "nicht", "schau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PDAT", "NN", "PIAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil er die Wahrheit ist, mag ihm ich wol vertrauen.", "tokens": ["Weil", "er", "die", "Wahr\u00b7heit", "ist", ",", "mag", "ihm", "ich", "wol", "ver\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hier mu\u00df ich zwischen Tod und N\u00f6then st\u00fcndlich schweben;", "tokens": ["Hier", "mu\u00df", "ich", "zwi\u00b7schen", "Tod", "und", "N\u00f6\u00b7then", "st\u00fcnd\u00b7lich", "schwe\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "KON", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Weil er das Leben ist, so kan durch ihn ich leben.", "tokens": ["Weil", "er", "das", "Le\u00b7ben", "ist", ",", "so", "kan", "durch", "ihn", "ich", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,", "ADV", "VMFIN", "APPR", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was wil ich weiter mehr? La\u00df, Herr, nur dich mich haben,", "tokens": ["Was", "wil", "ich", "wei\u00b7ter", "mehr", "?", "La\u00df", ",", "Herr", ",", "nur", "dich", "mich", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ADV", "$.", "VVIMP", "$,", "NN", "$,", "ADV", "PPER", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So acht ich keine Welt mit allen ihren Gaben.", "tokens": ["So", "acht", "ich", "kei\u00b7ne", "Welt", "mit", "al\u00b7len", "ih\u00b7ren", "Ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "PPER", "PIAT", "NN", "APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}