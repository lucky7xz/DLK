{"textgrid.poem.51298": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mensch, spannt die Folter dich so hefftig, als die Gicht,", "tokens": ["Mensch", ",", "spannt", "die", "Fol\u00b7ter", "dich", "so", "heff\u00b7tig", ",", "als", "die", "Gicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "PPER", "ADV", "ADJD", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Brennt dich der kalte Brand gelinder, als ein Licht:", "tokens": ["Brennt", "dich", "der", "kal\u00b7te", "Brand", "ge\u00b7lin\u00b7der", ",", "als", "ein", "Licht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ADJA", "$,", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bricht sachter dich der Stein, als feuervolle Zangen,", "tokens": ["Bricht", "sach\u00b7ter", "dich", "der", "Stein", ",", "als", "feu\u00b7er\u00b7vol\u00b7le", "Zan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "ART", "NN", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "F\u00e4llt leichter dich der Schlag, als Pf\u00e4hle, Spie\u00df und Stangen.", "tokens": ["F\u00e4llt", "leich\u00b7ter", "dich", "der", "Schlag", ",", "als", "Pf\u00e4h\u00b7le", ",", "Spie\u00df", "und", "Stan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "ART", "NN", "$,", "KOUS", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mensch t\u00f6dtet dich die Pest so schleunig als das Schwerd,", "tokens": ["Mensch", "t\u00f6d\u00b7tet", "dich", "die", "Pest", "so", "schleu\u00b7nig", "als", "das", "Schwerd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "ADV", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verzehrt das Fieber dich gehaster, als ein Heerd.", "tokens": ["Ver\u00b7zehrt", "das", "Fie\u00b7ber", "dich", "ge\u00b7has\u00b7ter", ",", "als", "ein", "Heerd", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADJD", "$,", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Qu\u00e4lt l\u00e4nger dich das Beil als Hertz und Seitenstechen,", "tokens": ["Qu\u00e4lt", "l\u00e4n\u00b7ger", "dich", "das", "Beil", "als", "Hertz", "und", "Sei\u00b7ten\u00b7ste\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "ART", "NN", "KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "St\u00fcrtzt dich das schwere Weh viel eh: als Radebrechen,", "tokens": ["St\u00fcrtzt", "dich", "das", "schwe\u00b7re", "Weh", "viel", "eh", ":", "als", "Ra\u00b7de\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ADV", "KOUS", "$.", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mensch w\u00fcrgt ein Steckflu\u00df dich so schmertzlich, als ein Strang,", "tokens": ["Mensch", "w\u00fcrgt", "ein", "Steck\u00b7flu\u00df", "dich", "so", "schmertz\u00b7lich", ",", "als", "ein", "Strang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPER", "ADV", "ADJD", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Quetscht seichter dich die Schraub, als Hertz Gespann und Zwang.", "tokens": ["Quetscht", "seich\u00b7ter", "dich", "die", "Schraub", ",", "als", "Hertz", "Ge\u00b7spann", "und", "Zwang", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "$,", "KOUS", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verfault der Scharbock dich so bald, als schwartze Stuben,", "tokens": ["Ver\u00b7fault", "der", "Schar\u00b7bock", "dich", "so", "bald", ",", "als", "schwart\u00b7ze", "Stu\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "ADV", "ADV", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ersa\u00fcfft die Wa\u00dfersucht dich sicherer, als Gruben.", "tokens": ["Er\u00b7sa\u00fcfft", "die", "Wa\u00b7\u00dfer\u00b7sucht", "dich", "si\u00b7che\u00b7rer", ",", "als", "Gru\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADJD", "$,", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Mensch, so bringt Kranckheit dich, so Marter in den Tod:", "tokens": ["Mensch", ",", "so", "bringt", "Kran\u00b7ck\u00b7heit", "dich", ",", "so", "Mar\u00b7ter", "in", "den", "Tod", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "NN", "PPER", "$,", "ADV", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Mit dieser Obrigkeit, mit jener straffet Gott.", "tokens": ["Mit", "die\u00b7ser", "Ob\u00b7rig\u00b7keit", ",", "mit", "je\u00b7ner", "straf\u00b7fet", "Gott", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "APPR", "PDAT", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Geht es die Warheit an? la\u00df nicht den Hencker schonen,", "tokens": ["Geht", "es", "die", "War\u00b7heit", "an", "?", "la\u00df", "nicht", "den", "Hen\u00b7cker", "scho\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$.", "VVIMP", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer gla\u00fcbts? aus solcher Hand giebt es die sch\u00f6nsten Kronen.", "tokens": ["Wer", "gla\u00fcbts", "?", "aus", "sol\u00b7cher", "Hand", "giebt", "es", "die", "sch\u00f6ns\u00b7ten", "Kro\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$.", "APPR", "PIAT", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}}}}}