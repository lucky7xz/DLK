{"textgrid.poem.54231": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Galante G-- wirst du das Siegel brechen,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Galante G-- wirst du das Siegel brechen,", "tokens": ["Ga\u00b7lan\u00b7te", "G\u00b7", "wirst", "du", "das", "Sie\u00b7gel", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "TRUNC", "VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "meter.parsing.error", "measure": "measure.parsing.error"}, "line.2": {"text": "So seh ich zu voraus, du lachst in deinen Sinn,", "tokens": ["So", "seh", "ich", "zu", "vo\u00b7raus", ",", "du", "lachst", "in", "dei\u00b7nen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKZU", "PTKVZ", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wirst, ich wei\u00df es schon, in den Gedancken sprechen,", "tokens": ["Und", "wirst", ",", "ich", "wei\u00df", "es", "schon", ",", "in", "den", "Ge\u00b7dan\u00b7cken", "spre\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df ich von Worten reich, und arm in halten, bin.", "tokens": ["Da\u00df", "ich", "von", "Wor\u00b7ten", "reich", ",", "und", "arm", "in", "hal\u00b7ten", ",", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADJD", "$,", "KON", "ADJD", "APPR", "VVINF", "$,", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Di\u00df glaube nicht von mir, mein Engel, soltstu wissen,", "tokens": ["Di\u00df", "glau\u00b7be", "nicht", "von", "mir", ",", "mein", "En\u00b7gel", ",", "solt\u00b7stu", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "APPR", "PPER", "$,", "PPOSAT", "NN", "$,", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie gro\u00df die Sehnsucht stets nach deinen Umgang ist,", "tokens": ["Wie", "gro\u00df", "die", "Sehn\u00b7sucht", "stets", "nach", "dei\u00b7nen", "Um\u00b7gang", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So w\u00fcrdest du gewi\u00df mehr als zu kl\u00e4rlich schliessen,", "tokens": ["So", "w\u00fcr\u00b7dest", "du", "ge\u00b7wi\u00df", "mehr", "als", "zu", "kl\u00e4r\u00b7lich", "schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PIAT", "KOKOM", "PTKA", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df du mir allerdings gantz unentbehrlich bist.", "tokens": ["Da\u00df", "du", "mir", "al\u00b7ler\u00b7dings", "gantz", "un\u00b7ent\u00b7behr\u00b7lich", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dein Englischer Verstand und die besondern Gaben,", "tokens": ["Dein", "Eng\u00b7li\u00b7scher", "Ver\u00b7stand", "und", "die", "be\u00b7son\u00b7dern", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Verdienen, da\u00df die Welt sie weit und breit verehrt.", "tokens": ["Ver\u00b7die\u00b7nen", ",", "da\u00df", "die", "Welt", "sie", "weit", "und", "breit", "ver\u00b7ehrt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "PPER", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nicht meyne, da\u00df sich nur an dir die M\u00e4nner laben,", "tokens": ["Nicht", "mey\u00b7ne", ",", "da\u00df", "sich", "nur", "an", "dir", "die", "M\u00e4n\u00b7ner", "la\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "KOUS", "PRF", "ADV", "APPR", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-----+-+-", "measure": "unknown.measure.tetra"}, "line.12": {"text": "Weil di\u00df Vergn\u00fcgen uns zugleich auch mit geh\u00f6rt.", "tokens": ["Weil", "di\u00df", "Ver\u00b7gn\u00fc\u00b7gen", "uns", "zu\u00b7gleich", "auch", "mit", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "NN", "PPER", "ADV", "ADV", "APPR", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Denn auch das weibliche Geschlechte mu\u00df bekennen,", "tokens": ["Denn", "auch", "das", "weib\u00b7li\u00b7che", "Ge\u00b7schlech\u00b7te", "mu\u00df", "be\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df ihre Hertzen dir best\u00e4ndge Fr\u00f6hner seyn,", "tokens": ["Da\u00df", "ih\u00b7re", "Hert\u00b7zen", "dir", "be\u00b7st\u00e4nd\u00b7ge", "Fr\u00f6h\u00b7ner", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ein eintzger Anblick macht, da\u00df wir vor Liebe brennen,", "tokens": ["Ein", "eintz\u00b7ger", "An\u00b7blick", "macht", ",", "da\u00df", "wir", "vor", "Lie\u00b7be", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "KOUS", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wir gehen sonder Zwang die sch\u00f6ne Dienstschafft ein.", "tokens": ["Wir", "ge\u00b7hen", "son\u00b7der", "Zwang", "die", "sch\u00f6\u00b7ne", "Dienst\u00b7schafft", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wirst du mich in die Zahl von deinen Freunden schreiben,", "tokens": ["Wirst", "du", "mich", "in", "die", "Zahl", "von", "dei\u00b7nen", "Freun\u00b7den", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So schw\u00f6r ich heilig dir, ich bin vor Lust entz\u00fcckt,", "tokens": ["So", "schw\u00f6r", "ich", "hei\u00b7lig", "dir", ",", "ich", "bin", "vor", "Lust", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPER", "$,", "PPER", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und werde dir davor recht sehr verbunden bleiben,", "tokens": ["Und", "wer\u00b7de", "dir", "da\u00b7vor", "recht", "sehr", "ver\u00b7bun\u00b7den", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PAV", "ADV", "ADV", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So lange bi\u00df der Tod mich in den Bogen r\u00fcckt.", "tokens": ["So", "lan\u00b7ge", "bi\u00df", "der", "Tod", "mich", "in", "den", "Bo\u00b7gen", "r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Indessen will ich mich an dein Versprechen halten,", "tokens": ["In\u00b7des\u00b7sen", "will", "ich", "mich", "an", "dein", "Ver\u00b7spre\u00b7chen", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Galante G-- gedencke stets an mich,", "tokens": ["Ga\u00b7lan\u00b7te", "G\u00b7", "ge\u00b7den\u00b7cke", "stets", "an", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "TRUNC", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "meter.parsing.error", "measure": "measure.parsing.error"}, "line.23": {"text": "La\u00df deine Gegen-Gunst zu keiner Zeit erkalten.", "tokens": ["La\u00df", "dei\u00b7ne", "Ge\u00b7gen\u00b7Gunst", "zu", "kei\u00b7ner", "Zeit", "er\u00b7kal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Mein dir geweyhtes Hertz verehrt dich inniglich.", "tokens": ["Mein", "dir", "ge\u00b7weyh\u00b7tes", "Hertz", "ver\u00b7ehrt", "dich", "in\u00b7nig\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PPER", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Du wirst dar\u00fcber wohl nicht eifers\u00fcchtig heissen,", "tokens": ["Du", "wirst", "da\u00b7r\u00fc\u00b7ber", "wohl", "nicht", "ei\u00b7fer\u00b7s\u00fcch\u00b7tig", "heis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "ADV", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Da\u00df ich dein liebstes Blat so starck als dich gek\u00fc\u00dft,", "tokens": ["Da\u00df", "ich", "dein", "liebs\u00b7tes", "Blat", "so", "starck", "als", "dich", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "ADV", "ADJD", "KOKOM", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Denn deine Hand mu\u00df mir die beste Linderung weissen,", "tokens": ["Denn", "dei\u00b7ne", "Hand", "mu\u00df", "mir", "die", "bes\u00b7te", "Lin\u00b7de\u00b7rung", "weis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Wenn meine Sehnsucht Dich, mein Element vermi\u00dft.", "tokens": ["Wenn", "mei\u00b7ne", "Sehn\u00b7sucht", "Dich", ",", "mein", "E\u00b7le\u00b7ment", "ver\u00b7mi\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "$,", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}