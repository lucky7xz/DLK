{"textgrid.poem.32280": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Lob der Faulheit", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Faulheit, jetzo will ich dir", "tokens": ["Faul\u00b7heit", ",", "jet\u00b7zo", "will", "ich", "dir"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VMFIN", "PPER", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch ein kleines Loblied bringen. \u2013", "tokens": ["Auch", "ein", "klei\u00b7nes", "Lob\u00b7lied", "brin\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "O \u2013 \u2013 wie \u2013 \u2013 sau \u2013 \u2013 er \u2013 \u2013 wird es mir, \u2013 \u2013", "tokens": ["O", "\u2013", "\u2013", "wie", "\u2013", "\u2013", "sau", "\u2013", "\u2013", "er", "\u2013", "\u2013", "wird", "es", "mir", ",", "\u2013", "\u2013"], "token_info": ["word", "punct", "punct", "word", "punct", "punct", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "$(", "$(", "PWAV", "$(", "$(", "ADJD", "$(", "$(", "PPER", "$(", "$(", "VAFIN", "PPER", "PPER", "$,", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich \u2013 \u2013 nach W\u00fcrden \u2013 \u2013 zu besingen!", "tokens": ["Dich", "\u2013", "\u2013", "nach", "W\u00fcr\u00b7den", "\u2013", "\u2013", "zu", "be\u00b7sin\u00b7gen", "!"], "token_info": ["word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PPER", "$(", "$(", "APPR", "NN", "$(", "$(", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch, ich will mein Bestes tun,", "tokens": ["Doch", ",", "ich", "will", "mein", "Bes\u00b7tes", "tun", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nach der Arbeit ist gut ruhn.", "tokens": ["Nach", "der", "Ar\u00b7beit", "ist", "gut", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "H\u00f6chstes Gut! wer dich nur hat,", "tokens": ["H\u00f6chs\u00b7tes", "Gut", "!", "wer", "dich", "nur", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PWS", "PPER", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dessen ungest\u00f6rtes Leben \u2013 \u2013", "tokens": ["Des\u00b7sen", "un\u00b7ge\u00b7st\u00f6r\u00b7tes", "Le\u00b7ben", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PDAT", "ADJA", "NN", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! \u2013 \u2013 ich \u2013 \u2013 g\u00e4hn' \u2013 \u2013 ich \u2013 \u2013 werde matt \u2013 \u2013", "tokens": ["Ach", "!", "\u2013", "\u2013", "ich", "\u2013", "\u2013", "g\u00e4hn'", "\u2013", "\u2013", "ich", "\u2013", "\u2013", "wer\u00b7de", "matt", "\u2013", "\u2013"], "token_info": ["word", "punct", "punct", "punct", "word", "punct", "punct", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["ITJ", "$.", "$(", "$(", "PPER", "$(", "$(", "VVIMP", "$(", "$(", "PPER", "$(", "$(", "VAFIN", "ADJD", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun \u2013 \u2013 so \u2013 \u2013 magst du \u2013 \u2013 mirs vergeben,", "tokens": ["Nun", "\u2013", "\u2013", "so", "\u2013", "\u2013", "magst", "du", "\u2013", "\u2013", "mirs", "ver\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct", "punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ADV", "$(", "$(", "ADV", "$(", "$(", "VMFIN", "PPER", "$(", "$(", "NE", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df ich dich nicht singen kann;", "tokens": ["Da\u00df", "ich", "dich", "nicht", "sin\u00b7gen", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Du verhinderst mich ja dran.", "tokens": ["Du", "ver\u00b7hin\u00b7derst", "mich", "ja", "dran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}