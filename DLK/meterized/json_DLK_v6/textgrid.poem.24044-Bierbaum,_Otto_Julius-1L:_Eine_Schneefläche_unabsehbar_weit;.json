{"textgrid.poem.24044": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Eine Schneefl\u00e4che unabsehbar weit;", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine Schneefl\u00e4che unabsehbar weit;", "tokens": ["Ei\u00b7ne", "Schnee\u00b7fl\u00e4\u00b7che", "un\u00b7ab\u00b7seh\u00b7bar", "weit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Der graue Nebel dar\u00fcber wie eine Last", "tokens": ["Der", "grau\u00b7e", "Ne\u00b7bel", "da\u00b7r\u00fc\u00b7ber", "wie", "ei\u00b7ne", "Last"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PAV", "KOKOM", "ART", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Von dumpfem Ha\u00df.", "tokens": ["Von", "dum\u00b7pfem", "Ha\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ists Tag? Ists Abend?", "tokens": ["Ists", "Tag", "?", "Ists", "A\u00b7bend", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "NE", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ich sehe kein Gestirn.", "tokens": ["Ich", "se\u00b7he", "kein", "Ge\u00b7stirn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ob die Sonne noch lebt?", "tokens": ["Ob", "die", "Son\u00b7ne", "noch", "lebt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Ueber die eisige Fl\u00e4che schleppt sich m\u00fcde mein Schritt.", "tokens": ["Ue\u00b7ber", "die", "ei\u00b7si\u00b7ge", "Fl\u00e4\u00b7che", "schleppt", "sich", "m\u00fc\u00b7de", "mein", "Schritt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PRF", "ADJD", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Mir ist, als s\u00f6ge der giftige Nebel aus allen meinen Poren", "tokens": ["Mir", "ist", ",", "als", "s\u00f6\u00b7ge", "der", "gif\u00b7ti\u00b7ge", "Ne\u00b7bel", "aus", "al\u00b7len", "mei\u00b7nen", "Po\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "NN", "ART", "ADJA", "NN", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "+--+--+--+--+-+-+-", "measure": "dactylic.tetra.plus"}, "line.3": {"text": "Das Leben und z\u00f6ge mich fort", "tokens": ["Das", "Le\u00b7ben", "und", "z\u00f6\u00b7ge", "mich", "fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "VVFIN", "PPER", "PTKVZ"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "In ein langsames Sterben.", "tokens": ["In", "ein", "lang\u00b7sa\u00b7mes", "Ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Seine Finger sind na\u00df, schlaff, kalt.", "tokens": ["Sei\u00b7ne", "Fin\u00b7ger", "sind", "na\u00df", ",", "schlaff", ",", "kalt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "VVFIN", "$,", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Oh, ihr rosig sonnendurchgl\u00fchten Finger", "tokens": ["Oh", ",", "ihr", "ro\u00b7sig", "son\u00b7nen\u00b7durch\u00b7gl\u00fch\u00b7ten", "Fin\u00b7ger"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "ADJD", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Des Fr\u00fchlingsmorgens, die ihr ins Leben weckt,", "tokens": ["Des", "Fr\u00fch\u00b7lings\u00b7mor\u00b7gens", ",", "die", "ihr", "ins", "Le\u00b7ben", "weckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wo seid ihr?", "tokens": ["Wo", "seid", "ihr", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.7": {"line.1": {"text": "Und ein h\u00fcpfender Wind der Erinnerung", "tokens": ["Und", "ein", "h\u00fcp\u00b7fen\u00b7der", "Wind", "der", "E\u00b7rin\u00b7ne\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN"], "meter": "--+--+--+--", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Geht durch mein Herz hin wie ein leiser Tanz", "tokens": ["Geht", "durch", "mein", "Herz", "hin", "wie", "ein", "lei\u00b7ser", "Tanz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Voll seidenem Rauschen.", "tokens": ["Voll", "sei\u00b7de\u00b7nem", "Rau\u00b7schen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Da", "tokens": ["Da"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Eine Stimme hinter mir.", "tokens": ["Ei\u00b7ne", "Stim\u00b7me", "hin\u00b7ter", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hart wie frostberstendes Eis.", "tokens": ["Hart", "wie", "frost\u00b7bers\u00b7ten\u00b7des", "Eis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "\u00bbdu da!\u00ab", "tokens": ["\u00bb", "du", "da", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "ADV", "$.", "$("], "meter": "-+", "measure": "iambic.single"}}, "stanza.9": {"line.1": {"text": "Wie in den Boden gerammt, steh ich erschrocken.", "tokens": ["Wie", "in", "den", "Bo\u00b7den", "ge\u00b7rammt", ",", "steh", "ich", "er\u00b7schro\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "VVPP", "$,", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.10": {"line.1": {"text": "\u00bbwas erschrickst du! Ich bin nicht der Tod.", "tokens": ["\u00bb", "was", "er\u00b7schrickst", "du", "!", "Ich", "bin", "nicht", "der", "Tod", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "PTKNEG", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ich bin nicht der Tod ....", "tokens": ["Ich", "bin", "nicht", "der", "Tod", "...."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ach!\u00ab", "tokens": ["Ach", "!", "\u00ab"], "token_info": ["word", "punct", "punct"], "pos": ["ITJ", "$.", "$("], "meter": "+", "measure": "single.up"}}, "stanza.11": {"line.1": {"text": "Eine Wolke umballt meine Sinne. In kalte", "tokens": ["Ei\u00b7ne", "Wol\u00b7ke", "um\u00b7ballt", "mei\u00b7ne", "Sin\u00b7ne", ".", "In", "kal\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$.", "APPR", "ADJA"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Leichenkammern entflieht meine Seele.", "tokens": ["Lei\u00b7chen\u00b7kam\u00b7mern", "ent\u00b7flieht", "mei\u00b7ne", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Dann taucht sie heraus", "tokens": ["Dann", "taucht", "sie", "he\u00b7raus"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "In eine gro\u00dfe Helligkeit,", "tokens": ["In", "ei\u00b7ne", "gro\u00b7\u00dfe", "Hel\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und neben einem greisen Manne schreit ich", "tokens": ["Und", "ne\u00b7ben", "ei\u00b7nem", "grei\u00b7sen", "Man\u00b7ne", "schreit", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Durch ein sonnenhei\u00dfes Land.", "tokens": ["Durch", "ein", "son\u00b7nen\u00b7hei\u00b7\u00dfes", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Grellwei\u00dfe Felsen und d\u00fcrres Gelb", "tokens": ["Grell\u00b7wei\u00b7\u00dfe", "Fel\u00b7sen", "und", "d\u00fcr\u00b7res", "Gelb"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sterbender Reife rechts und links.", "tokens": ["Ster\u00b7ben\u00b7der", "Rei\u00b7fe", "rechts", "und", "links", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "KON", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.13": {"line.1": {"text": "\u00bbhebe dein Haupt!", "tokens": ["\u00bb", "he\u00b7be", "dein", "Haupt", "!"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Sieh! Da ist Golgatha!\u00ab", "tokens": ["Sieh", "!", "Da", "ist", "Gol\u00b7ga\u00b7tha", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$.", "ADV", "VAFIN", "NE", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Christus!", "tokens": ["Chris\u00b7tus", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.15": {"line.1": {"text": "Im gl\u00fchenden Sonnenbrand,", "tokens": ["Im", "gl\u00fc\u00b7hen\u00b7den", "Son\u00b7nen\u00b7brand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Tief niedergesunken das Haupt,", "tokens": ["Tief", "nie\u00b7der\u00b7ge\u00b7sun\u00b7ken", "das", "Haupt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Am Kreuz.", "tokens": ["Am", "Kreuz", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Ich sehe in seinem blonden Haar", "tokens": ["Ich", "se\u00b7he", "in", "sei\u00b7nem", "blon\u00b7den", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Den Dornenkranz, die Schmerzensgloriole.", "tokens": ["Den", "Dor\u00b7nen\u00b7kranz", ",", "die", "Schmer\u00b7zens\u00b7glo\u00b7ri\u00b7o\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sein Leib ist d\u00fcrr und voller Blutrunst.", "tokens": ["Sein", "Leib", "ist", "d\u00fcrr", "und", "vol\u00b7ler", "Blut\u00b7runst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Oh, Christus!", "tokens": ["Oh", ",", "Chris\u00b7tus", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "NE", "$."], "meter": "+--", "measure": "dactylic.init"}}, "stanza.17": {"line.1": {"text": "\u00bbkomm!\u00ab", "tokens": ["\u00bb", "komm", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$.", "$("], "meter": "+", "measure": "single.up"}}, "stanza.18": {"line.1": {"text": "Oh, la\u00df mich beten am heiligen Marterstamm!", "tokens": ["Oh", ",", "la\u00df", "mich", "be\u00b7ten", "am", "hei\u00b7li\u00b7gen", "Mar\u00b7ter\u00b7stamm", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVIMP", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Hier la\u00df mich beten lernen!", "tokens": ["Hier", "la\u00df", "mich", "be\u00b7ten", "ler\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "\u00bbkomm! Siehe die Leute an, die beten.\u00ab", "tokens": ["\u00bb", "komm", "!", "Sie\u00b7he", "die", "Leu\u00b7te", "an", ",", "die", "be\u00b7ten", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$.", "VVIMP", "ART", "NN", "PTKVZ", "$,", "PRELS", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.20": {"line.1": {"text": "Er f\u00fchrt mich fort.", "tokens": ["Er", "f\u00fchrt", "mich", "fort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und wieder flieht meine Seele.", "tokens": ["Und", "wie\u00b7der", "flieht", "mei\u00b7ne", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Durch wetternden Sturm flieht sie und Waffengeklirr", "tokens": ["Durch", "wet\u00b7tern\u00b7den", "Sturm", "flieht", "sie", "und", "Waf\u00b7fen\u00b7ge\u00b7klirr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "KON", "NN"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und Feuersbrunst", "tokens": ["Und", "Feu\u00b7ers\u00b7brunst"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und Sterbeklagen.", "tokens": ["Und", "Ster\u00b7be\u00b7kla\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Und in ein mittleres Licht taucht sie auf.", "tokens": ["Und", "in", "ein", "mitt\u00b7le\u00b7res", "Licht", "taucht", "sie", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.21": {"line.1": {"text": "Auf glattem Asphalte schreiten wir", "tokens": ["Auf", "glat\u00b7tem", "As\u00b7phal\u00b7te", "schrei\u00b7ten", "wir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Durch eine gro\u00dfe Stadt,", "tokens": ["Durch", "ei\u00b7ne", "gro\u00b7\u00dfe", "Stadt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.22": {"line.1": {"text": "\u00bbhebe dein Haupt! Sieh, da ist Golgatha!\u00ab", "tokens": ["\u00bb", "he\u00b7be", "dein", "Haupt", "!", "Sieh", ",", "da", "ist", "Gol\u00b7ga\u00b7tha", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPOSAT", "NN", "$.", "NE", "$,", "ADV", "VAFIN", "NE", "$.", "$("], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}}, "stanza.23": {"line.1": {"text": "Gott! Gott! Entsetzlich, da \u2013:", "tokens": ["Gott", "!", "Gott", "!", "Ent\u00b7setz\u00b7lich", ",", "da", "\u2013", ":"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "wor