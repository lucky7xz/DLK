{"textgrid.poem.24074": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Jenseits von Gut und B\u00f6se", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schw\u00fcl war der Tag ...", "tokens": ["Schw\u00fcl", "war", "der", "Tag", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Auch das Gewitter, das aus schwarzer Wolke", "tokens": ["Auch", "das", "Ge\u00b7wit\u00b7ter", ",", "das", "aus", "schwar\u00b7zer", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fegenden Regen in das Seegrau go\u00df,", "tokens": ["Fe\u00b7gen\u00b7den", "Re\u00b7gen", "in", "das", "See\u00b7grau", "go\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Gab keine K\u00fchlung.", "tokens": ["Gab", "kei\u00b7ne", "K\u00fch\u00b7lung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Unbewegt das Laub,", "tokens": ["Un\u00b7be\u00b7wegt", "das", "Laub", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Durstd\u00fcrr.", "tokens": ["Dur\u00b7std\u00fcrr", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Und auch mein Herz war schw\u00fcl,", "tokens": ["Und", "auch", "mein", "Herz", "war", "schw\u00fcl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Von Sehnsucht schw\u00fcl nach dir", "tokens": ["Von", "Sehn\u00b7sucht", "schw\u00fcl", "nach", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Und deinem Hei\u00dfesten.", "tokens": ["Und", "dei\u00b7nem", "Hei\u00b7\u00dfes\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.10": {"text": "Und dir auch d\u00fcrstete das Herz", "tokens": ["Und", "dir", "auch", "d\u00fcrs\u00b7te\u00b7te", "das", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und alle Sinne,", "tokens": ["Und", "al\u00b7le", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Und deinen schmiegeweichen Leib an mich", "tokens": ["Und", "dei\u00b7nen", "schmie\u00b7ge\u00b7wei\u00b7chen", "Leib", "an", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Hast du gedr\u00e4ngt,", "tokens": ["Hast", "du", "ge\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Bittend aus Sehnsuchtsschw\u00fcle.", "tokens": ["Bit\u00b7tend", "aus", "Sehn\u00b7suchts\u00b7schw\u00fc\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Da sahen unsere Seelen sich nackt in Liebe,", "tokens": ["Da", "sa\u00b7hen", "un\u00b7se\u00b7re", "See\u00b7len", "sich", "nackt", "in", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PRF", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und segenfeierlich vereinte uns Natur.", "tokens": ["Und", "se\u00b7gen\u00b7fei\u00b7er\u00b7lich", "ver\u00b7ein\u00b7te", "uns", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie im Garten", "tokens": ["Wie", "im", "Gar\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPRART", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.4": {"text": "Des Paradieses, ehe die Schlange sprach,", "tokens": ["Des", "Pa\u00b7ra\u00b7die\u00b7ses", ",", "e\u00b7he", "die", "Schlan\u00b7ge", "sprach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Also erkannten wir uns wie im Traum", "tokens": ["Al\u00b7so", "er\u00b7kann\u00b7ten", "wir", "uns", "wie", "im", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und waren selig.", "tokens": ["Und", "wa\u00b7ren", "se\u00b7lig", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Wortelos", "tokens": ["Wor\u00b7te\u00b7los"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Im Arm uns lagen wir und kosteten", "tokens": ["Im", "Arm", "uns", "la\u00b7gen", "wir", "und", "kos\u00b7te\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Vom Baume holdester Erkenntnis.", "tokens": ["Vom", "Bau\u00b7me", "hol\u00b7des\u00b7ter", "Er\u00b7kennt\u00b7nis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Schw\u00fcl war der Tag,", "tokens": ["Schw\u00fcl", "war", "der", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "In Schw\u00fcle ging die Nacht.", "tokens": ["In", "Schw\u00fc\u00b7le", "ging", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In segenschwangerer Wolke schwebten wir", "tokens": ["In", "se\u00b7gen\u00b7schwan\u00b7ge\u00b7rer", "Wol\u00b7ke", "schweb\u00b7ten", "wir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Jenseits von Gut und B\u00f6se.", "tokens": ["Jen\u00b7seits", "von", "Gut", "und", "B\u00f6\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}