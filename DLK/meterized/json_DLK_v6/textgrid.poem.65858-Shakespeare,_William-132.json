{"textgrid.poem.65858": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "132", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich liebe deine Augen: ein Bedauern", "tokens": ["Ich", "lie\u00b7be", "dei\u00b7ne", "Au\u00b7gen", ":", "ein", "Be\u00b7dau\u00b7ern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Les' ich Verschm\u00e4hter drin: wohl kennen sie dein Herz", "tokens": ["Les'", "ich", "Ver\u00b7schm\u00e4h\u00b7ter", "drin", ":", "wohl", "ken\u00b7nen", "sie", "dein", "Herz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "NN", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und tragen Schwarz, und scheinen mild zu trauern,", "tokens": ["Und", "tra\u00b7gen", "Schwarz", ",", "und", "schei\u00b7nen", "mild", "zu", "trau\u00b7ern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "$,", "KON", "VVFIN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit holdem Gram betrachtend meinen Schmerz.", "tokens": ["Mit", "hol\u00b7dem", "Gram", "be\u00b7trach\u00b7tend", "mei\u00b7nen", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und wahrlich, nicht des Ostens grauen Wangen", "tokens": ["Und", "wahr\u00b7lich", ",", "nicht", "des", "Os\u00b7tens", "grau\u00b7en", "Wan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "PTKNEG", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Steht Himmels Morgensonne reizender,", "tokens": ["Steht", "Him\u00b7mels", "Mor\u00b7gen\u00b7son\u00b7ne", "rei\u00b7zen\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Noch blinkt der volle Stern mit abendlichem Prangen", "tokens": ["Noch", "blinkt", "der", "vol\u00b7le", "Stern", "mit", "a\u00b7bend\u00b7li\u00b7chem", "Pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vom kalten Westen halb so stolz daher,", "tokens": ["Vom", "kal\u00b7ten", "Wes\u00b7ten", "halb", "so", "stolz", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "ADV", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wie jenes Traueraugenp\u00e4rlein blickt.", "tokens": ["Wie", "je\u00b7nes", "Trau\u00b7er\u00b7au\u00b7gen\u00b7p\u00e4r\u00b7lein", "blickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "O dann la\u00df Trauer auch dein Herz versch\u00f6nen", "tokens": ["O", "dann", "la\u00df", "Trau\u00b7er", "auch", "dein", "Herz", "ver\u00b7sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VVIMP", "NN", "ADV", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Um mich, wenn Trauer dich so reizend schm\u00fcckt!", "tokens": ["Um", "mich", ",", "wenn", "Trau\u00b7er", "dich", "so", "rei\u00b7zend", "schm\u00fcckt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "$,", "KOUS", "NN", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "La\u00df ", "tokens": ["La\u00df"], "token_info": ["word"], "pos": ["VVIMP"], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Dann schw\u00f6r' ich, Sch\u00f6nheit selbst ist schwarz; und was", "tokens": ["Dann", "schw\u00f6r'", "ich", ",", "Sch\u00f6n\u00b7heit", "selbst", "ist", "schwarz", ";", "und", "was"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "ADV", "VAFIN", "ADJD", "$.", "KON", "PWS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Nicht deine Farbe tr\u00e4gt, verfolgt mein Ha\u00df.", "tokens": ["Nicht", "dei\u00b7ne", "Far\u00b7be", "tr\u00e4gt", ",", "ver\u00b7folgt", "mein", "Ha\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}