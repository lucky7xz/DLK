{"textgrid.poem.52685": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Epigonenthum", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was schleppet ihr in m\u00fc\u00dfgen Frohnen", "tokens": ["Was", "schlep\u00b7pet", "ihr", "in", "m\u00fc\u00df\u00b7gen", "Froh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Pf\u00fcndchen zu dem Haufen Gold?", "tokens": ["Ein", "Pf\u00fcnd\u00b7chen", "zu", "dem", "Hau\u00b7fen", "Gold", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr seid und bleibet Epigonen,", "tokens": ["Ihr", "seid", "und", "blei\u00b7bet", "E\u00b7pi\u00b7go\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr m\u00f6gt euch stellen, wie ihr wollt!", "tokens": ["Ihr", "m\u00f6gt", "euch", "stel\u00b7len", ",", "wie", "ihr", "wollt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So h\u00f6r ich unsre Weisen sagen,", "tokens": ["So", "h\u00f6r", "ich", "uns\u00b7re", "Wei\u00b7sen", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und f\u00fchle fast, sie haben Recht,", "tokens": ["Und", "f\u00fch\u00b7le", "fast", ",", "sie", "ha\u00b7ben", "Recht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch nicht sogleich will ich verzagen,", "tokens": ["Doch", "nicht", "sog\u00b7leich", "will", "ich", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn auch ein Pf\u00fcndchen Gold ist \u00e4cht.", "tokens": ["Denn", "auch", "ein", "Pf\u00fcnd\u00b7chen", "Gold", "ist", "\u00e4cht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch sei es Gold! Heraufgegraben", "tokens": ["Doch", "sei", "es", "Gold", "!", "Her\u00b7auf\u00b7ge\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VAFIN", "PPER", "NN", "$.", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Tief aus den Schachten des Gem\u00fcths", "tokens": ["Tief", "aus", "den", "Schach\u00b7ten", "des", "Ge\u00b7m\u00fcths"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Doch goldne Tugend soll er haben", "tokens": ["Doch", "gold\u00b7ne", "Tu\u00b7gend", "soll", "er", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VMFIN", "PPER", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Klang und Schimmer eures Lieds!", "tokens": ["Der", "Klang", "und", "Schim\u00b7mer", "eu\u00b7res", "Lieds", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O suchet mehr, als nur zu gl\u00e4nzen,", "tokens": ["O", "su\u00b7chet", "mehr", ",", "als", "nur", "zu", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,", "KOUS", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und denket an ein altes Wort,", "tokens": ["Und", "den\u00b7ket", "an", "ein", "al\u00b7tes", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wollt ihr bereichern und erg\u00e4nzen", "tokens": ["Wollt", "ihr", "be\u00b7rei\u00b7chern", "und", "er\u00b7g\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Dichtung Nibelungenhort!", "tokens": ["Der", "Dich\u00b7tung", "Ni\u00b7be\u00b7lun\u00b7gen\u00b7hort", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ihr sollt nicht k\u00fcnsteln, nie erheucheln", "tokens": ["Ihr", "sollt", "nicht", "k\u00fcns\u00b7teln", ",", "nie", "er\u00b7heu\u00b7cheln"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Selbstgenu\u00df ein Herzensfest,", "tokens": ["Zum", "Selbst\u00b7ge\u00b7nu\u00df", "ein", "Her\u00b7zens\u00b7fest", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr sollt nicht falschen G\u00f6ttern schmeicheln,", "tokens": ["Ihr", "sollt", "nicht", "fal\u00b7schen", "G\u00f6t\u00b7tern", "schmei\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn euch die innre Macht verl\u00e4\u00dft.", "tokens": ["Wenn", "euch", "die", "inn\u00b7re", "Macht", "ver\u00b7l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Gebt Leidenschaft! euch selbst! so findet", "tokens": ["Gebt", "Lei\u00b7den\u00b7schaft", "!", "euch", "selbst", "!", "so", "fin\u00b7det"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "NN", "$.", "PPER", "ADV", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr ohne K\u00fcnste Form und Ton,", "tokens": ["Ihr", "oh\u00b7ne", "K\u00fcns\u00b7te", "Form", "und", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Glaubt an die Welt, die ihr verk\u00fcndet!", "tokens": ["Glaubt", "an", "die", "Welt", ",", "die", "ihr", "ver\u00b7k\u00fcn\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der L\u00fcgner nur ist Epigon.", "tokens": ["Der", "L\u00fcg\u00b7ner", "nur", "ist", "E\u00b7pi\u00b7gon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}