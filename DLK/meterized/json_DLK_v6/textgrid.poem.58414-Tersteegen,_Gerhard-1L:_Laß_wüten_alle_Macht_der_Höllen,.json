{"textgrid.poem.58414": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: La\u00df w\u00fcten alle Macht der H\u00f6llen,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df w\u00fcten alle Macht der H\u00f6llen,", "tokens": ["La\u00df", "w\u00fc\u00b7ten", "al\u00b7le", "Macht", "der", "H\u00f6l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df st\u00fcrmen Wind' und Meereswellen,", "tokens": ["La\u00df", "st\u00fcr\u00b7men", "Wind'", "und", "Mee\u00b7res\u00b7wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nimm du dich keiner Unruh an,", "tokens": ["Nimm", "du", "dich", "kei\u00b7ner", "Un\u00b7ruh", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PRF", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es bleibe nur dein tiefster Wille", "tokens": ["Es", "blei\u00b7be", "nur", "dein", "tiefs\u00b7ter", "Wil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bei Gott in innig sanfter Stille,", "tokens": ["Bei", "Gott", "in", "in\u00b7nig", "sanf\u00b7ter", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da dich der Feind nicht finden kann!", "tokens": ["Da", "dich", "der", "Feind", "nicht", "fin\u00b7den", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}