{"textgrid.poem.44026": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du Abgott niedertr\u00e4chtger Sinnen,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Abgott niedertr\u00e4chtger Sinnen,", "tokens": ["Du", "Ab\u00b7gott", "nie\u00b7der\u00b7tr\u00e4cht\u00b7ger", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich, falsches Gl\u00fccke, red ich an,", "tokens": ["Dich", ",", "fal\u00b7sches", "Gl\u00fc\u00b7cke", ",", "red", "ich", "an", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was wilstu noch von dem gewinnen,", "tokens": ["Was", "wils\u00b7tu", "noch", "von", "dem", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "APPR", "ART", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der nun nichts mehr verlieren kan?", "tokens": ["Der", "nun", "nichts", "mehr", "ver\u00b7lie\u00b7ren", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du st\u00e4upst nur einen tauben R\u00fccken,", "tokens": ["Du", "st\u00e4u\u00b7pst", "nur", "ei\u00b7nen", "tau\u00b7ben", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Der Draht und Gei\u00dfel m\u00fcde macht", "tokens": ["Der", "Draht", "und", "Gei\u00b7\u00dfel", "m\u00fc\u00b7de", "macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und, ohne sich vor dir zu b\u00fccken,", "tokens": ["Und", ",", "oh\u00b7ne", "sich", "vor", "dir", "zu", "b\u00fc\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUI", "PRF", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Den aufgefangnen Streich verlacht.", "tokens": ["Den", "auf\u00b7ge\u00b7fang\u00b7nen", "Streich", "ver\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O spare die zerschmi\u00dfne Ruthe", "tokens": ["O", "spa\u00b7re", "die", "zer\u00b7schmi\u00df\u00b7ne", "Ru\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf einen, welcher be\u00dfer f\u00fchlt!", "tokens": ["Auf", "ei\u00b7nen", ",", "wel\u00b7cher", "be\u00b7\u00dfer", "f\u00fchlt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich troze dich mit diesem Blute,", "tokens": ["Ich", "tro\u00b7ze", "dich", "mit", "die\u00b7sem", "Blu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In welchem sich dein Jachzorn k\u00fchlt.", "tokens": ["In", "wel\u00b7chem", "sich", "dein", "Jach\u00b7zorn", "k\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mir jagt der Bliz von deinem Keile", "tokens": ["Mir", "jagt", "der", "Bliz", "von", "dei\u00b7nem", "Kei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kein blind und t\u00f6dtlich Schr\u00f6cken ein,", "tokens": ["Kein", "blind", "und", "t\u00f6dt\u00b7lich", "Schr\u00f6\u00b7cken", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "KON", "ADJD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und eh ich kl\u00e4glich fleh und heule,", "tokens": ["Und", "eh", "ich", "kl\u00e4g\u00b7lich", "fleh", "und", "heu\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Eh soll mein Fleisch zerri\u00dfen seyn.", "tokens": ["Eh", "soll", "mein", "Fleisch", "zer\u00b7ri\u00b7\u00dfen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "NN", "VVPP", "VAINF", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "So, so, verdopple Schlag und Eifer!", "tokens": ["So", ",", "so", ",", "ver\u00b7dopp\u00b7le", "Schlag", "und", "Ei\u00b7fer", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schlag sch\u00e4rfer und begreif dich nicht!", "tokens": ["Schlag", "sch\u00e4r\u00b7fer", "und", "be\u00b7greif", "dich", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Schmerz erregt mir J\u00e4scht und Geifer,", "tokens": ["Der", "Schmerz", "er\u00b7regt", "mir", "J\u00e4scht", "und", "Gei\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den spei ich dir ins Angesicht.", "tokens": ["Den", "spei", "ich", "dir", "ins", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein Rasen dient mir zum Gesp\u00f6tte,", "tokens": ["Dein", "Ra\u00b7sen", "dient", "mir", "zum", "Ge\u00b7sp\u00f6t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und k\u00f6nte mir mein Wuntsch geschehn,", "tokens": ["Und", "k\u00f6n\u00b7te", "mir", "mein", "Wunt\u00b7sch", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Da\u00df jede Wunde Lippen h\u00e4tte,", "tokens": ["Da\u00df", "je\u00b7de", "Wun\u00b7de", "Lip\u00b7pen", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So wollt ich dich recht grausam schm\u00e4hn.", "tokens": ["So", "wollt", "ich", "dich", "recht", "grau\u00b7sam", "schm\u00e4hn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADJD", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Was ist das Absehn deiner T\u00fccke?", "tokens": ["Was", "ist", "das", "Ab\u00b7sehn", "dei\u00b7ner", "T\u00fc\u00b7cke", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie, soll ich vor dein Altar knien?", "tokens": ["Wie", ",", "soll", "ich", "vor", "dein", "Al\u00b7tar", "kni\u00b7en", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie, soll ich mit bethr\u00e4ntem Blicke", "tokens": ["Wie", ",", "soll", "ich", "mit", "be\u00b7thr\u00e4n\u00b7tem", "Bli\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein schnelles Seegel an mich ziehn?", "tokens": ["Dein", "schnel\u00b7les", "See\u00b7gel", "an", "mich", "ziehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie, soll ich, o du stummer G\u00f6ze,", "tokens": ["Wie", ",", "soll", "ich", ",", "o", "du", "stum\u00b7mer", "G\u00f6\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "VMFIN", "PPER", "$,", "FM", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der wahren Wei\u00dfheit untreu seyn", "tokens": ["Der", "wah\u00b7ren", "Wei\u00df\u00b7heit", "un\u00b7treu", "seyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und vor den Koth geringer Sch\u00e4ze", "tokens": ["Und", "vor", "den", "Koth", "ge\u00b7rin\u00b7ger", "Sch\u00e4\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dir Herz und Ruh und Andacht weihn?", "tokens": ["Dir", "Herz", "und", "Ruh", "und", "An\u00b7dacht", "weihn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du wilt vielleicht, ich soll dich ehren?", "tokens": ["Du", "wilt", "viel\u00b7leicht", ",", "ich", "soll", "dich", "eh\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ach las dir nur die Lust vergehn.", "tokens": ["Ach", "las", "dir", "nur", "die", "Lust", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dies soll man wohl nicht eher h\u00f6ren,", "tokens": ["Dies", "soll", "man", "wohl", "nicht", "e\u00b7her", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIS", "ADV", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als bis die B\u00e4um am Himmel stehn.", "tokens": ["Als", "bis", "die", "B\u00e4um", "am", "Him\u00b7mel", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie, soll ich mich zu Tode gr\u00e4men?", "tokens": ["Wie", ",", "soll", "ich", "mich", "zu", "To\u00b7de", "gr\u00e4\u00b7men", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "VMFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nein, dir zu Liebe sterb ich nicht.", "tokens": ["Nein", ",", "dir", "zu", "Lie\u00b7be", "sterb", "ich", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "APPR", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wiltu mir die Zunge l\u00e4hmen,", "tokens": ["Und", "wil\u00b7tu", "mir", "die", "Zun\u00b7ge", "l\u00e4h\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So flucht der Augen finstres Licht.", "tokens": ["So", "flucht", "der", "Au\u00b7gen", "finst\u00b7res", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich leb, und das nur dir zu Truze;", "tokens": ["Ich", "leb", ",", "und", "das", "nur", "dir", "zu", "Tru\u00b7ze", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PDS", "ADV", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du kriegst kein gutes Wort von mir.", "tokens": ["Du", "kriegst", "kein", "gu\u00b7tes", "Wort", "von", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Narr verlangt nach deinem Schuze", "tokens": ["Ein", "Narr", "ver\u00b7langt", "nach", "dei\u00b7nem", "Schu\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und klopft an deine Gnadenth\u00fcr;", "tokens": ["Und", "klopft", "an", "dei\u00b7ne", "Gna\u00b7dent\u00b7h\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Eh ich dein Tischgast hei\u00dfen sollte,", "tokens": ["Eh", "ich", "dein", "Tischgast", "hei\u00b7\u00dfen", "soll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eh fr\u00e4\u00df ich Kleyen, Leim und Stroh,", "tokens": ["Eh", "fr\u00e4\u00df", "ich", "Kle\u00b7yen", ",", "Leim", "und", "Stroh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und eh ich dir gehorchen wollte,", "tokens": ["Und", "eh", "ich", "dir", "ge\u00b7hor\u00b7chen", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Eh dient ich gar dem Pharao.", "tokens": ["Eh", "dient", "ich", "gar", "dem", "Pha\u00b7rao", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Versuche mich mit sch\u00e4rferm Grimme,", "tokens": ["Ver\u00b7su\u00b7che", "mich", "mit", "sch\u00e4r\u00b7ferm", "Grim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Versalze mir die junge Zeit,", "tokens": ["Ver\u00b7sal\u00b7ze", "mir", "die", "jun\u00b7ge", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ver\u00e4ndre meine Menschenstimme", "tokens": ["Ver\u00b7\u00e4nd\u00b7re", "mei\u00b7ne", "Men\u00b7schen\u00b7stim\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mach ein gl\u00fchend Rind bereit,", "tokens": ["Und", "mach", "ein", "gl\u00fc\u00b7hend", "Rind", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJD", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn will ich dir zur Schande br\u00fcllen", "tokens": ["Denn", "will", "ich", "dir", "zur", "Schan\u00b7de", "br\u00fcl\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PPER", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und mehr verstockt als jammervoll", "tokens": ["Und", "mehr", "ver\u00b7stockt", "als", "jam\u00b7mer\u00b7voll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVPP", "KOKOM", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das Ohr mit solchen Fl\u00fcchen f\u00fcllen,", "tokens": ["Das", "Ohr", "mit", "sol\u00b7chen", "Fl\u00fc\u00b7chen", "f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df Tag und Licht verschwarzen soll.", "tokens": ["Da\u00df", "Tag", "und", "Licht", "ver\u00b7schwar\u00b7zen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Komm, las mich in dem M\u00f6rser sto\u00dfen,", "tokens": ["Komm", ",", "las", "mich", "in", "dem", "M\u00f6r\u00b7ser", "sto\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Komm, flicht die Glieder um dein Rad:", "tokens": ["Komm", ",", "flicht", "die", "Glie\u00b7der", "um", "dein", "Rad", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was gilt's? Du solt dich mehr erbo\u00dfen,", "tokens": ["Was", "gilt's", "?", "Du", "solt", "dich", "mehr", "er\u00b7bo\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$.", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn keine Marter W\u00fcrckung hat.", "tokens": ["Wenn", "kei\u00b7ne", "Mar\u00b7ter", "W\u00fcr\u00b7ckung", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und w\u00fcrde mir der Hals gebrochen,", "tokens": ["Und", "w\u00fcr\u00b7de", "mir", "der", "Hals", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dies kanst und schafstu nicht so leicht,", "tokens": ["Dies", "kanst", "und", "schafs\u00b7tu", "nicht", "so", "leicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "KON", "VVFIN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So soll dich auch mein Aa\u00df noch pochen,", "tokens": ["So", "soll", "dich", "auch", "mein", "Aa\u00df", "noch", "po\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das manchmahl desto schwerer weicht.", "tokens": ["Das", "manch\u00b7mahl", "des\u00b7to", "schwe\u00b7rer", "weicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Man seh nur deine gro\u00dfen Knechte,", "tokens": ["Man", "seh", "nur", "dei\u00b7ne", "gro\u00b7\u00dfen", "Knech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Gold und Ehr und Stand erdr\u00fcckt,", "tokens": ["Die", "Gold", "und", "Ehr", "und", "Stand", "er\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie machen die Gewalt zum Rechte", "tokens": ["Sie", "ma\u00b7chen", "die", "Ge\u00b7walt", "zum", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und werden nimmermehr erquickt;", "tokens": ["Und", "wer\u00b7den", "nim\u00b7mer\u00b7mehr", "er\u00b7quickt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie hungern bey den vollen Sch\u00fc\u00dfeln", "tokens": ["Sie", "hun\u00b7gern", "bey", "den", "vol\u00b7len", "Sch\u00fc\u00b7\u00dfeln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nach ruhiger Zufriedenheit,", "tokens": ["Nach", "ru\u00b7hi\u00b7ger", "Zu\u00b7frie\u00b7den\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Der Geiz beschwert sie nebst den Schl\u00fc\u00dfeln", "tokens": ["Der", "Geiz", "be\u00b7schwert", "sie", "nebst", "den", "Schl\u00fc\u00b7\u00dfeln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit Argwohn, M\u00fch und Furchtsamkeit.", "tokens": ["Mit", "Arg\u00b7wohn", ",", "M\u00fch", "und", "Furcht\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Vor diesem war man bey der Eichel", "tokens": ["Vor", "die\u00b7sem", "war", "man", "bey", "der", "Ei\u00b7chel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "VAFIN", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In Fried und Sicherheit vergn\u00fcgt,", "tokens": ["In", "Fried", "und", "Si\u00b7cher\u00b7heit", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jezt lechst man nach der F\u00fcrsten Speichel", "tokens": ["Jezt", "lechst", "man", "nach", "der", "F\u00fcrs\u00b7ten", "Spei\u00b7chel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sucht Gefahr, wo Purpur liegt.", "tokens": ["Und", "sucht", "Ge\u00b7fahr", ",", "wo", "Pur\u00b7pur", "liegt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$,", "PWAV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die meisten ringen nach den Sorgen,", "tokens": ["Die", "meis\u00b7ten", "rin\u00b7gen", "nach", "den", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erwerben, was den Kummer n\u00e4hrt,", "tokens": ["Er\u00b7wer\u00b7ben", ",", "was", "den", "Kum\u00b7mer", "n\u00e4hrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und rechnen bis an lichten Morgen", "tokens": ["Und", "rech\u00b7nen", "bis", "an", "lich\u00b7ten", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "KON", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Und w\u00fcntschen, bis die Seel ausf\u00e4hrt.", "tokens": ["Und", "w\u00fcnt\u00b7schen", ",", "bis", "die", "Seel", "aus\u00b7f\u00e4hrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Mein Herz ist viel zu hoch gebohren,", "tokens": ["Mein", "Herz", "ist", "viel", "zu", "hoch", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als da\u00df ich in der Sclaverey,", "tokens": ["Als", "da\u00df", "ich", "in", "der", "Scla\u00b7ve\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu der des P\u00f6bels Mund geschworen,", "tokens": ["Zu", "der", "des", "P\u00f6\u00b7bels", "Mund", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Schimpf vor seinen Adel sey.", "tokens": ["Ein", "Schimpf", "vor", "sei\u00b7nen", "A\u00b7del", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Wei\u00dfheit ist mein Leibgedinge,", "tokens": ["Die", "Wei\u00df\u00b7heit", "ist", "mein", "Leib\u00b7ge\u00b7din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit dieser reis ich durch die Welt,", "tokens": ["Mit", "die\u00b7ser", "reis", "ich", "durch", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und machen wir gleich kurze Spr\u00fcnge,", "tokens": ["Und", "ma\u00b7chen", "wir", "gleich", "kur\u00b7ze", "Spr\u00fcn\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So glaub ich, da\u00df man seltner f\u00e4llt.", "tokens": ["So", "glaub", "ich", ",", "da\u00df", "man", "selt\u00b7ner", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Es mag mich Neid und Feind verlezen,", "tokens": ["Es", "mag", "mich", "Neid", "und", "Feind", "ver\u00b7le\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es mag sich mir der Heuchler Heer,", "tokens": ["Es", "mag", "sich", "mir", "der", "Heuch\u00b7ler", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ja selbst der Tod entgegensezen,", "tokens": ["Ja", "selbst", "der", "Tod", "ent\u00b7ge\u00b7gen\u00b7se\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es komme Schmach und Unrecht her,", "tokens": ["Es", "kom\u00b7me", "Schmach", "und", "Un\u00b7recht", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie sollen doch nichts mehr erhalten,", "tokens": ["Sie", "sol\u00b7len", "doch", "nichts", "mehr", "er\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIS", "ADV", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Als da\u00df mein unger\u00e4chter Geist,", "tokens": ["Als", "da\u00df", "mein", "un\u00b7ge\u00b7r\u00e4ch\u00b7ter", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So scharf sie mir die Brust zerspalten,", "tokens": ["So", "scharf", "sie", "mir", "die", "Brust", "zer\u00b7spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Herz voll kluger Hochmuth weist.", "tokens": ["Ein", "Herz", "voll", "klu\u00b7ger", "Hoch\u00b7muth", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Da hastu deine sch\u00f6ne Tittel:", "tokens": ["Da", "has\u00b7tu", "dei\u00b7ne", "sch\u00f6\u00b7ne", "Tit\u00b7tel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du Wetterhahn, du blindes Weib,", "tokens": ["Du", "Wet\u00b7ter\u00b7hahn", ",", "du", "blin\u00b7des", "Weib", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du Blaustrumpf, du Verderbungsmittel,", "tokens": ["Du", "Blau\u00b7strumpf", ",", "du", "Ver\u00b7der\u00b7bungs\u00b7mit\u00b7tel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du Hure vor des P\u00f6bels Leib,", "tokens": ["Du", "Hu\u00b7re", "vor", "des", "P\u00f6\u00b7bels", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du Zauberbalg, du Thorheitsschwester,", "tokens": ["Du", "Zau\u00b7ber\u00b7balg"