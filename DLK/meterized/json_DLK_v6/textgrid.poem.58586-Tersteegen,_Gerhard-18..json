{"textgrid.poem.58586": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rt, wie lieblich will der Herr sein Jerusalem bedecken,", "tokens": ["H\u00f6rt", ",", "wie", "lieb\u00b7lich", "will", "der", "Herr", "sein", "Je\u00b7ru\u00b7sa\u00b7lem", "be\u00b7de\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ADJD", "VMFIN", "ART", "NN", "VAINF", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Wie der V\u00f6glein Fl\u00fcgel sich \u00fcber ihre Jungen strecken;", "tokens": ["Wie", "der", "V\u00f6\u00b7glein", "Fl\u00fc\u00b7gel", "sich", "\u00fc\u00b7ber", "ih\u00b7re", "Jun\u00b7gen", "stre\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Abgewichne Kinder, h\u00f6rt, kehret um und kehret ein,", "tokens": ["Ab\u00b7ge\u00b7wich\u00b7ne", "Kin\u00b7der", ",", "h\u00f6rt", ",", "keh\u00b7ret", "um", "und", "keh\u00b7ret", "ein", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,", "VVFIN", "PTKVZ", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Wenn ihr mit Jerusalem wollet so bewahret sein!", "tokens": ["Wenn", "ihr", "mit", "Je\u00b7ru\u00b7sa\u00b7lem", "wol\u00b7let", "so", "be\u00b7wah\u00b7ret", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "VMFIN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Herr, du rufst, ich komme schon; la\u00df mein Herz denn auch auf Erden", "tokens": ["Herr", ",", "du", "rufst", ",", "ich", "kom\u00b7me", "schon", ";", "la\u00df", "mein", "Herz", "denn", "auch", "auf", "Er\u00b7den"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$.", "VVIMP", "PPOSAT", "NN", "KON", "ADV", "APPR", "NN"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Deine werte Friedensstadt und also beschirmet werden,", "tokens": ["Dei\u00b7ne", "wer\u00b7te", "Frie\u00b7dens\u00b7stadt", "und", "al\u00b7so", "be\u00b7schir\u00b7met", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "ADV", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Rett mich aus der Feinde Macht, in mein Herze wohnen komm,", "tokens": ["Rett", "mich", "aus", "der", "Fein\u00b7de", "Macht", ",", "in", "mein", "Her\u00b7ze", "woh\u00b7nen", "komm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ART", "NN", "NN", "$,", "APPR", "PPOSAT", "NN", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Wandle Tag und Nacht in mir, als in deinem Eigentum!", "tokens": ["Wand\u00b7le", "Tag", "und", "Nacht", "in", "mir", ",", "als", "in", "dei\u00b7nem", "Ei\u00b7gen\u00b7tum", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "APPR", "PPER", "$,", "KOUS", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}}}}}