{"textgrid.poem.33671": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "In d\u00fcstrer Zeit", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Boden sinkt von meinen Tagen", "tokens": ["Zu", "Bo\u00b7den", "sinkt", "von", "mei\u00b7nen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Lust an allem, Blatt um Blatt.", "tokens": ["Die", "Lust", "an", "al\u00b7lem", ",", "Blatt", "um", "Blatt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIS", "$,", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich f\u00fchl's mit Schmerz und mag nicht klagen,", "tokens": ["Ich", "f\u00fchl's", "mit", "Schmerz", "und", "mag", "nicht", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "L\u00e4ngst bin ich auch der Klage satt.", "tokens": ["L\u00e4ngst", "bin", "ich", "auch", "der", "Kla\u00b7ge", "satt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Verh\u00fcllt nur rollt ein innres Dr\u00e4ngen,", "tokens": ["Ver\u00b7h\u00fcllt", "nur", "rollt", "ein", "inn\u00b7res", "Dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein unerf\u00fclltes Zukunftwort,", "tokens": ["Ein", "un\u00b7er\u00b7f\u00fcll\u00b7tes", "Zu\u00b7kunft\u00b7wort", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Strom von hei\u00dfen Glutges\u00e4ngen", "tokens": ["Ein", "Strom", "von", "hei\u00b7\u00dfen", "Glut\u00b7ge\u00b7s\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In meiner Brust ungl\u00fccklich fort;", "tokens": ["In", "mei\u00b7ner", "Brust", "un\u00b7gl\u00fcck\u00b7lich", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ungl\u00fccklich, denn es blieb kein Streben,", "tokens": ["Un\u00b7gl\u00fcck\u00b7lich", ",", "denn", "es", "blieb", "kein", "Stre\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KON", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Selbst meine Seele nicht mehr mein.", "tokens": ["Selbst", "mei\u00b7ne", "See\u00b7le", "nicht", "mehr", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKNEG", "ADV", "PPOSAT", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem sp\u00e4ten Herbsttag gleicht mein Leben,", "tokens": ["Dem", "sp\u00e4\u00b7ten", "Herbst\u00b7tag", "gleicht", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Herbsttag ohne Sonnenschein.", "tokens": ["Dem", "Herbst\u00b7tag", "oh\u00b7ne", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Vielleicht nur kurz, bevor es dunkelt,", "tokens": ["Viel\u00b7leicht", "nur", "kurz", ",", "be\u00b7vor", "es", "dun\u00b7kelt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df auch noch mir ein Abend gl\u00fcht,", "tokens": ["Da\u00df", "auch", "noch", "mir", "ein", "A\u00b7bend", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein m\u00fcder letzter Strahl, und funkelt", "tokens": ["Ein", "m\u00fc\u00b7der", "letz\u00b7ter", "Strahl", ",", "und", "fun\u00b7kelt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Tage, denen nichts mehr bl\u00fcht.", "tokens": ["Auf", "Ta\u00b7ge", ",", "de\u00b7nen", "nichts", "mehr", "bl\u00fcht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}