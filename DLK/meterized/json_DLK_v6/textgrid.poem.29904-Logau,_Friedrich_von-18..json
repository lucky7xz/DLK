{"textgrid.poem.29904": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Christus ist mir alles worden, ist mir so auch worden Ich;", "tokens": ["Chris\u00b7tus", "ist", "mir", "al\u00b7les", "wor\u00b7den", ",", "ist", "mir", "so", "auch", "wor\u00b7den", "Ich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PIS", "VAPP", "$,", "VAFIN", "PPER", "ADV", "ADV", "VAPP", "PPER", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Ist er Ich nun worden mir, so wird Ich nicht lassen mich.", "tokens": ["Ist", "er", "Ich", "nun", "wor\u00b7den", "mir", ",", "so", "wird", "Ich", "nicht", "las\u00b7sen", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "VAPP", "PPER", "$,", "ADV", "VAFIN", "PPER", "PTKNEG", "VVINF", "PPER", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}