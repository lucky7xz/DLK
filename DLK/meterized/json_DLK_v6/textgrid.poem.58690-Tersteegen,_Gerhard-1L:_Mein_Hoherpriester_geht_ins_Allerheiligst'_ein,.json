{"textgrid.poem.58690": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Hoherpriester geht ins Allerheiligst' ein,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Hoherpriester geht ins Allerheiligst' ein,", "tokens": ["Mein", "Ho\u00b7her\u00b7pries\u00b7ter", "geht", "ins", "Al\u00b7ler\u00b7hei\u00b7ligst'", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf ewig er vers\u00f6hnt die Menge meiner S\u00fcnden;", "tokens": ["Auf", "e\u00b7wig", "er", "ver\u00b7s\u00f6hnt", "die", "Men\u00b7ge", "mei\u00b7ner", "S\u00fcn\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kommt, S\u00fcnder, liebet Gott, ihr k\u00f6nnt Erl\u00f6sung finden,", "tokens": ["Kommt", ",", "S\u00fcn\u00b7der", ",", "lie\u00b7bet", "Gott", ",", "ihr", "k\u00f6nnt", "Er\u00b7l\u00f6\u00b7sung", "fin\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "NN", "$,", "PPER", "VVFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch soll in Jesu Blut die S\u00fcnd' ers\u00e4ufet sein!", "tokens": ["Doch", "soll", "in", "Je\u00b7su", "Blut", "die", "S\u00fcnd'", "er\u00b7s\u00e4u\u00b7fet", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NE", "NN", "ART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}