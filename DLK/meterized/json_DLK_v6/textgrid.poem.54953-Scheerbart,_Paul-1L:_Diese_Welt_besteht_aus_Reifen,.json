{"textgrid.poem.54953": {"metadata": {"author": {"name": "Scheerbart, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Diese Welt besteht aus Reifen,", "genre": "verse", "period": "N.A.", "pub_year": 1889, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Diese Welt besteht aus Reifen,", "tokens": ["Die\u00b7se", "Welt", "be\u00b7steht", "aus", "Rei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die voll \u00c4rger immer pfeifen,", "tokens": ["Die", "voll", "\u00c4r\u00b7ger", "im\u00b7mer", "pfei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sie Garnichts mehr begreifen!", "tokens": ["Da\u00df", "sie", "Gar\u00b7nichts", "mehr", "be\u00b7grei\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sollen sie sich weiter schleifen,", "tokens": ["Sol\u00b7len", "sie", "sich", "wei\u00b7ter", "schlei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "D\u00fcrfen sie sich nicht versteifen", "tokens": ["D\u00fcr\u00b7fen", "sie", "sich", "nicht", "ver\u00b7stei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf das ewig dumme Keifen!", "tokens": ["Auf", "das", "e\u00b7wig", "dum\u00b7me", "Kei\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "La\u00dft sie t\u00e4glich anders pfeifen \u2013", "tokens": ["La\u00dft", "sie", "t\u00e4g\u00b7lich", "an\u00b7ders", "pfei\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "ADV", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sonst geh\u00f6ren diese Reifen,", "tokens": ["Sonst", "ge\u00b7h\u00f6\u00b7ren", "die\u00b7se", "Rei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Die uns immer wieder kneifen,", "tokens": ["Die", "uns", "im\u00b7mer", "wie\u00b7der", "knei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Nicht zu jenen guten Pfeifen,", "tokens": ["Nicht", "zu", "je\u00b7nen", "gu\u00b7ten", "Pfei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Deren Wohlklang wir begreifen.", "tokens": ["De\u00b7ren", "Wohl\u00b7klang", "wir", "be\u00b7grei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}