{"dta.poem.11481": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die bey dem  mausol\u00e6o  redende Artemisia.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus der ", "tokens": ["Aus", "der"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Schaut dieses denckmahl an, wo ehr und traurigkeit", "tokens": ["Schaut", "die\u00b7ses", "denck\u00b7mahl", "an", ",", "wo", "ehr", "und", "trau\u00b7rig\u00b7keit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PDAT", "NN", "PTKVZ", "$,", "PWAV", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf gleichen thronen herrscht! Seht, was noch keine zeit", "tokens": ["Auf", "glei\u00b7chen", "thro\u00b7nen", "herrscht", "!", "Seht", ",", "was", "noch", "kei\u00b7ne", "zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$.", "VVFIN", "$,", "PRELS", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und kunst gewiesen hat! Das Asien gebaut,", "tokens": ["Und", "kunst", "ge\u00b7wie\u00b7sen", "hat", "!", "Das", "A\u00b7sien", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "$.", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Wodurch sich die natur selbst \u00fcberwunden schaut!", "tokens": ["Wo\u00b7durch", "sich", "die", "na\u00b7tur", "selbst", "\u00fc\u00b7berw\u00b7un\u00b7den", "schaut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "ADV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der liebe griffel hat die grabschrifft einge\u00e4tzt,", "tokens": ["Der", "lie\u00b7be", "grif\u00b7fel", "hat", "die", "grab\u00b7schrifft", "ein\u00b7ge\u00b7\u00e4tzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und statt der farbe sie mit thr\u00e4nen voll genetzt:", "tokens": ["Und", "statt", "der", "far\u00b7be", "sie", "mit", "thr\u00e4\u00b7nen", "voll", "ge\u00b7netzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sie baut in dieser grufft vor meines f\u00fcrsten ruhm,", "tokens": ["Sie", "baut", "in", "die\u00b7ser", "grufft", "vor", "mei\u00b7nes", "f\u00fcrs\u00b7ten", "ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dem tode zum verdru\u00df ein ewig heiligthum.", "tokens": ["Dem", "to\u00b7de", "zum", "ver\u00b7dru\u00df", "ein", "e\u00b7wig", "hei\u00b7lig\u00b7thum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ART", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Doch liebe! welch ein fleck beschm\u00fctzt dein ehren-kleid,", "tokens": ["Doch", "lie\u00b7be", "!", "welch", "ein", "fleck", "be\u00b7schm\u00fctzt", "dein", "eh\u00b7ren\u00b7kleid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PWAT", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wenn ich die marmel hier zu neben-buhlern leid\u2019?", "tokens": ["Wenn", "ich", "die", "mar\u00b7mel", "hier", "zu", "ne\u00b7ben\u00b7buh\u00b7lern", "leid'", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.12": {"text": "Und meiner seele gluth, die keiner thr\u00e4nen gu\u00df", "tokens": ["Und", "mei\u00b7ner", "see\u00b7le", "gluth", ",", "die", "kei\u00b7ner", "thr\u00e4\u00b7nen", "gu\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Zu leschen f\u00e4hig ist, mit ihnen theilen mu\u00df?", "tokens": ["Zu", "le\u00b7schen", "f\u00e4\u00b7hig", "ist", ",", "mit", "ih\u00b7nen", "thei\u00b7len", "mu\u00df", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "VAFIN", "$,", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Nein, Artemisia! nachdem Mausolens geist", "tokens": ["Nein", ",", "Ar\u00b7te\u00b7mi\u00b7sia", "!", "nach\u00b7dem", "Mau\u00b7so\u00b7lens", "geist"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "NE", "$.", "KOUS", "NN", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.15": {"text": "Jtzt in das blasse feld der todten abgereist,", "tokens": ["Jtzt", "in", "das", "blas\u00b7se", "feld", "der", "tod\u00b7ten", "ab\u00b7ge\u00b7reist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So mu\u00df die liebe doch die strahlen von sich streun,", "tokens": ["So", "mu\u00df", "die", "lie\u00b7be", "doch", "die", "strah\u00b7len", "von", "sich", "streun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "VVFIN", "ADV", "ART", "ADJA", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und auch die asche selbst in dir lebendig seyn.", "tokens": ["Und", "auch", "die", "asc\u00b7he", "selbst", "in", "dir", "le\u00b7ben\u00b7dig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "ADV", "APPR", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}