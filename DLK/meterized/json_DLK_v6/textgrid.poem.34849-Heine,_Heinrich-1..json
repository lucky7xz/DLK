{"textgrid.poem.34849": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbblieb' ich doch ein Junggeselle!\u00ab \u2013", "tokens": ["\u00bb", "blieb'", "ich", "doch", "ein", "Jung\u00b7ge\u00b7sel\u00b7le", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADV", "ART", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seufzet Pluto tausendmal \u2013", "tokens": ["Seuf\u00b7zet", "Plu\u00b7to", "tau\u00b7send\u00b7mal", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbjetzt, in meiner Eh'standsqual,", "tokens": ["\u00bb", "jetzt", ",", "in", "mei\u00b7ner", "Eh'\u00b7stands\u00b7qual", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Merk ich, fr\u00fcher ohne Weib", "tokens": ["Merk", "ich", ",", "fr\u00fc\u00b7her", "oh\u00b7ne", "Weib"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "ADJD", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "War die H\u00f6lle keine H\u00f6lle.", "tokens": ["War", "die", "H\u00f6l\u00b7le", "kei\u00b7ne", "H\u00f6l\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Blieb' ich doch ein Junggeselle!", "tokens": ["Blieb'", "ich", "doch", "ein", "Jung\u00b7ge\u00b7sel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seit ich Proserpinen hab,", "tokens": ["Seit", "ich", "Pro\u00b7ser\u00b7pi\u00b7nen", "hab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fcnsch ich t\u00e4glich mich ins Grab!", "tokens": ["W\u00fcnsch", "ich", "t\u00e4g\u00b7lich", "mich", "ins", "Grab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn sie keift, so h\u00f6r ich kaum", "tokens": ["Wenn", "sie", "keift", ",", "so", "h\u00f6r", "ich", "kaum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Meines Zerberus Gebelle.", "tokens": ["Mei\u00b7nes", "Zer\u00b7be\u00b7rus", "Ge\u00b7bel\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Stets vergeblich, stets nach Frieden", "tokens": ["Stets", "ver\u00b7geb\u00b7lich", ",", "stets", "nach", "Frie\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ring ich. Hier im Schattenreich", "tokens": ["Ring", "ich", ".", "Hier", "im", "Schat\u00b7ten\u00b7reich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kein Verdammter ist mir gleich!", "tokens": ["Kein", "Ver\u00b7damm\u00b7ter", "ist", "mir", "gleich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich beneide Sisyphus", "tokens": ["Ich", "be\u00b7nei\u00b7de", "Si\u00b7syp\u00b7hus"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die edlen Danaiden.\u00ab", "tokens": ["Und", "die", "ed\u00b7len", "Da\u00b7na\u00b7i\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}