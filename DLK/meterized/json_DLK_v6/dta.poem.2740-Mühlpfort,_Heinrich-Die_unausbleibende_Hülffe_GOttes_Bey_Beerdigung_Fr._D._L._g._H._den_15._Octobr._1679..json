{"dta.poem.2740": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die unausbleibende H\u00fclffe GOttes/  \n Bey Beerdigung Fr. D. L. g. H. den 15.  \n Octobr. 1679.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist des Menschen Hertz? ein Meer voll von Gedanck&#275;/", "tokens": ["Was", "ist", "des", "Men\u00b7schen", "Hertz", "?", "ein", "Meer", "voll", "von", "Ge\u00b7danck", "&#275;", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "XML_entity", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "NN", "$.", "ART", "NN", "ADJD", "APPR", "NN", "$(", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie jenes von dem Schaum erz\u00fcrnter Wellen braust/", "tokens": ["Wie", "je\u00b7nes", "von", "dem", "Schaum", "er\u00b7z\u00fcrn\u00b7ter", "Wel\u00b7len", "braust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So m\u00fcssen sich allhier stets die Begierden zancken/", "tokens": ["So", "m\u00fcs\u00b7sen", "sich", "all\u00b7hier", "stets", "die", "Be\u00b7gier\u00b7den", "zan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "ADV", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-++-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wenn schon der Regungen erboster Nordwind saust.", "tokens": ["Wenn", "schon", "der", "Re\u00b7gun\u00b7gen", "er\u00b7bos\u00b7ter", "Nord\u00b7wind", "saust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Die See und auch das Hertz wird wol niemand ergr\u00fcnden/", "tokens": ["Die", "See", "und", "auch", "das", "Hertz", "wird", "wol", "nie\u00b7mand", "er\u00b7gr\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADV", "ART", "NN", "VAFIN", "ADV", "PIS", "VVINF", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Sie blehen sich zugleich vom stoltzen Hochmuth auff:", "tokens": ["Sie", "ble\u00b7hen", "sich", "zu\u00b7gleich", "vom", "stolt\u00b7zen", "Hoch\u00b7muth", "auff", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So ist bey Beyden nicht Best\u00e4ndigkeit zu finden/", "tokens": ["So", "ist", "bey", "Bey\u00b7den", "nicht", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "zu", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "PTKNEG", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es \u00e4ndert Well und Hertz im Augenblick den Lauff.", "tokens": ["Es", "\u00e4n\u00b7dert", "Well", "und", "Hertz", "im", "Au\u00b7gen\u00b7blick", "den", "Lauff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "KON", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Bald scheinen sie so klar als ein polirter Spiegel/", "tokens": ["Bald", "schei\u00b7nen", "sie", "so", "klar", "als", "ein", "po\u00b7lir\u00b7ter", "Spie\u00b7gel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und kurtz verstellet sie ein ungeheure Nacht/", "tokens": ["Und", "kurtz", "ver\u00b7stel\u00b7let", "sie", "ein", "un\u00b7ge\u00b7heu\u00b7re", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Bald halten sie das Ziel/ und lieben Schlo\u00df und Riegel/", "tokens": ["Bald", "hal\u00b7ten", "sie", "das", "Ziel", "/", "und", "lie\u00b7ben", "Schlo\u00df", "und", "Rie\u00b7gel", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$(", "KON", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bi\u00df sie ein neuer Sturm eydbr\u00fcchig wieder macht.", "tokens": ["Bi\u00df", "sie", "ein", "neu\u00b7er", "Sturm", "eyd\u00b7br\u00fc\u00b7chig", "wie\u00b7der", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So viel h\u00e4gt nicht das Meer liebkosende Sirenen/", "tokens": ["So", "viel", "h\u00e4gt", "nicht", "das", "Meer", "lieb\u00b7ko\u00b7sen\u00b7de", "Si\u00b7re\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PTKNEG", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.14": {"text": "Als mit viel L\u00fcsten ist verzaubert unser Hertz;", "tokens": ["Als", "mit", "viel", "L\u00fcs\u00b7ten", "ist", "ver\u00b7zau\u00b7bert", "un\u00b7ser", "Hertz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PIAT", "NN", "VAFIN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So kan sich Triton nicht mit Perl und Muscheln kr\u00f6nen/", "tokens": ["So", "kan", "sich", "Tri\u00b7ton", "nicht", "mit", "Perl", "und", "Mu\u00b7scheln", "kr\u00f6\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "NE", "PTKNEG", "APPR", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Als dieses sich bekr\u00e4ntzt mit Wollust/ Spiel und Schertz.", "tokens": ["Als", "die\u00b7ses", "sich", "be\u00b7kr\u00b7\u00e4ntzt", "mit", "Wol\u00b7lust", "/", "Spiel", "und", "Schertz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "PRF", "VVFIN", "APPR", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Hingegen schl\u00e4gt auch nicht mit so viel Donner-Keulen", "tokens": ["Hin\u00b7ge\u00b7gen", "schl\u00e4gt", "auch", "nicht", "mit", "so", "viel", "Don\u00b7ner\u00b7Keu\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PTKNEG", "APPR", "ADV", "PIAT", "NN"], "meter": "+--+-++--+-+-", "measure": "iambic.hexa.invert"}, "line.18": {"text": "Des Himmels starcker Arm in Amphitritens Schaum;", "tokens": ["Des", "Him\u00b7mels", "star\u00b7cker", "Arm", "in", "Am\u00b7phit\u00b7ri\u00b7tens", "Schaum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Als unser Hertze wird durchbohrt von Kummer-Pfeilen/", "tokens": ["Als", "un\u00b7ser", "Hert\u00b7ze", "wird", "durch\u00b7bohrt", "von", "Kum\u00b7mer\u00b7Pfei\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "VVFIN", "VAFIN", "VVPP", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So da\u00df es brechen mu\u00df und reget sich noch kaum.", "tokens": ["So", "da\u00df", "es", "bre\u00b7chen", "mu\u00df", "und", "re\u00b7get", "sich", "noch", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVINF", "VMFIN", "KON", "VVFIN", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die See ist niemals treu: das Hertze/ weil wir leben/", "tokens": ["Die", "See", "ist", "nie\u00b7mals", "treu", ":", "das", "Hert\u00b7ze", "/", "weil", "wir", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$.", "PDS", "VVFIN", "$(", "KOUS", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Bleibt immer t\u00fcckisch arg mit Tollheit angef\u00fcllt/", "tokens": ["Bleibt", "im\u00b7mer", "t\u00fc\u00b7ckisch", "arg", "mit", "Toll\u00b7heit", "an\u00b7ge\u00b7f\u00fcllt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "ADJD", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und wenn es trotzig sich wird an die Sterne heben/", "tokens": ["Und", "wenn", "es", "trot\u00b7zig", "sich", "wird", "an", "die", "Ster\u00b7ne", "he\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "PRF", "VAFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So hat Kleinm\u00fcthigkeit bald die Geschwulst gestillt.", "tokens": ["So", "hat", "Klein\u00b7m\u00fct\u00b7hig\u00b7keit", "bald", "die", "Ge\u00b7schwulst", "ge\u00b7stillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+++-+-+", "measure": "unknown.measure.septa"}, "line.25": {"text": "Drumb ist von Anbegin all unser Thun und Tichten", "tokens": ["Drumb", "ist", "von", "An\u00b7be\u00b7gin", "all", "un\u00b7ser", "Thun", "und", "Tich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "APPR", "NN", "PIAT", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Verkehrt/ und irriger als wol ein Labyrinth.", "tokens": ["Ver\u00b7kehrt", "/", "und", "ir\u00b7ri\u00b7ger", "als", "wol", "ein", "La\u00b7by\u00b7rinth", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$(", "KON", "ADJD", "KOKOM", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wie klug wir auff der Welt auch unsre Wege richten/", "tokens": ["Wie", "klug", "wir", "auff", "der", "Welt", "auch", "uns\u00b7re", "We\u00b7ge", "rich\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "APPR", "ART", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So glaubt doch/ da\u00df es nicht des HErren Wege sind.", "tokens": ["So", "glaubt", "doch", "/", "da\u00df", "es", "nicht", "des", "Her\u00b7ren", "We\u00b7ge", "sind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$(", "KOUS", "PPER", "PTKNEG", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wir lachen zwar den Wahn der unbesonn\u2019 nen Heyden/", "tokens": ["Wir", "la\u00b7chen", "zwar", "den", "Wahn", "der", "un\u00b7be\u00b7sonn'", "nen", "Hey\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ART", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Da\u00df sie aus Holtz und Stein viel G\u00f6tter ausgedacht/", "tokens": ["Da\u00df", "sie", "aus", "Holtz", "und", "Stein", "viel", "G\u00f6t\u00b7ter", "aus\u00b7ge\u00b7dacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Zu ihnen sich genaht in Kummer/ Angst und Leiden/", "tokens": ["Zu", "ih\u00b7nen", "sich", "ge\u00b7naht", "in", "Kum\u00b7mer", "/", "Angst", "und", "Lei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "VVPP", "APPR", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Rauchkertzen angesteckt/ und Opffer abgeschlacht;", "tokens": ["Rauch\u00b7kert\u00b7zen", "an\u00b7ge\u00b7steckt", "/", "und", "Opf\u00b7fer", "ab\u00b7ge\u00b7schlacht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$(", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Alleine sind uns nicht wol die Begierden G\u00f6tter?", "tokens": ["Al\u00b7lei\u00b7ne", "sind", "uns", "nicht", "wol", "die", "Be\u00b7gier\u00b7den", "G\u00f6t\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.34": {"text": "Verl\u00e4st sich unser Hertz nicht auch auf Stock und Stein?", "tokens": ["Ver\u00b7l\u00e4st", "sich", "un\u00b7ser", "Hertz", "nicht", "auch", "auf", "Stock", "und", "Stein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "PTKNEG", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und soll das Reichthum nicht aus aller Noth Erretter/", "tokens": ["Und", "soll", "das", "Reicht\u00b7hum", "nicht", "aus", "al\u00b7ler", "Noth", "Er\u00b7ret\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PTKNEG", "APPR", "PIAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der Grossen hohe Macht die beste H\u00fclffe seyn?", "tokens": ["Der", "Gros\u00b7sen", "ho\u00b7he", "Macht", "die", "bes\u00b7te", "H\u00fclf\u00b7fe", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Man wird den Purpur eh\u2019 als Gottes Thron anbeten/", "tokens": ["Man", "wird", "den", "Pur\u00b7pur", "eh'", "als", "Got\u00b7tes", "Thron", "an\u00b7be\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "NE", "KOUS", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und den Gewaltigen noch mehr zu Dienste stehn:", "tokens": ["Und", "den", "Ge\u00b7wal\u00b7ti\u00b7gen", "noch", "mehr", "zu", "Diens\u00b7te", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ja wer unangesagt darff zu den F\u00fcrsten treten/", "tokens": ["Ja", "wer", "un\u00b7an\u00b7ge\u00b7sagt", "darff", "zu", "den", "F\u00fcrs\u00b7ten", "tre\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PWS", "ADJD", "VMFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Scheint unter Sternenschon noch auf der Welt zu gehn.", "tokens": ["Scheint", "un\u00b7ter", "Ster\u00b7nen\u00b7schon", "noch", "auf", "der", "Welt", "zu", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wer trotzt nicht zu der Zeit mit g\u00fcltigen Patronen?", "tokens": ["Wer", "trotzt", "nicht", "zu", "der", "Zeit", "mit", "g\u00f