{"textgrid.poem.47245": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "10. Waldandacht", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Orgelt\u00f6ne brausen", "tokens": ["Or\u00b7gel\u00b7t\u00f6\u00b7ne", "brau\u00b7sen"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Durch der Tannen Haar,", "tokens": ["Durch", "der", "Tan\u00b7nen", "Haar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und mit stillem Grausen", "tokens": ["Und", "mit", "stil\u00b7lem", "Grau\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Knie' ich am Altar,", "tokens": ["Knie'", "ich", "am", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Den in Waldeshallen", "tokens": ["Den", "in", "Wal\u00b7des\u00b7hal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Mir der Fr\u00fchling baut,", "tokens": ["Mir", "der", "Fr\u00fch\u00b7ling", "baut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und des Herzens Wallen", "tokens": ["Und", "des", "Her\u00b7zens", "Wal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Wird im Liede laut.", "tokens": ["Wird", "im", "Lie\u00b7de", "laut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Seinen Sabbat feiert,", "tokens": ["Sei\u00b7nen", "Sab\u00b7bat", "fei\u00b7ert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sch\u00f6pfung, der dich schuf,", "tokens": ["Sch\u00f6p\u00b7fung", ",", "der", "dich", "schuf", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und die Seel' umschleiert", "tokens": ["Und", "die", "Seel'", "um\u00b7schlei\u00b7ert"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Seines Friedens Ruf.", "tokens": ["Sei\u00b7nes", "Frie\u00b7dens", "Ruf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wenn du Vaterstrafen,", "tokens": ["Wenn", "du", "Va\u00b7ter\u00b7stra\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Kind, nicht f\u00fcrchten mu\u00dft,", "tokens": ["Kind", ",", "nicht", "f\u00fcrch\u00b7ten", "mu\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Kannst du ruhig schlafen", "tokens": ["Kannst", "du", "ru\u00b7hig", "schla\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "An der Mutter Brust.", "tokens": ["An", "der", "Mut\u00b7ter", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}