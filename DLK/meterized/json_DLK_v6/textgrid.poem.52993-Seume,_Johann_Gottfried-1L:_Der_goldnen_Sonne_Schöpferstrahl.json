{"textgrid.poem.52993": {"metadata": {"author": {"name": "Seume, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der goldnen Sonne Sch\u00f6pferstrahl", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der goldnen Sonne Sch\u00f6pferstrahl", "tokens": ["Der", "gold\u00b7nen", "Son\u00b7ne", "Sch\u00f6p\u00b7fer\u00b7strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Belebet wieder Berg und Thal,", "tokens": ["Be\u00b7le\u00b7bet", "wie\u00b7der", "Berg", "und", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ihre neue W\u00e4rme schafft", "tokens": ["Und", "ih\u00b7re", "neu\u00b7e", "W\u00e4r\u00b7me", "schafft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Erde wieder Jugendkraft.", "tokens": ["Der", "Er\u00b7de", "wie\u00b7der", "Ju\u00b7gend\u00b7kraft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Laut pocht das Herz, hoch wallt das Blut;", "tokens": ["Laut", "pocht", "das", "Herz", ",", "hoch", "wallt", "das", "Blut", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "$,", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Frey fliegt der Blick, k\u00fchn steigt der Muth;", "tokens": ["Frey", "fliegt", "der", "Blick", ",", "k\u00fchn", "steigt", "der", "Muth", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Geist schwingt sich mit Seraphsfu\u00df", "tokens": ["Der", "Geist", "schwingt", "sich", "mit", "Se\u00b7raphs\u00b7fu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Lunens Horn zum Sirius.", "tokens": ["Von", "Lu\u00b7nens", "Horn", "zum", "Si\u00b7rius", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Gott, dessen Hauch die R\u00e4ume f\u00fcllt,", "tokens": ["Gott", ",", "des\u00b7sen", "Hauch", "die", "R\u00e4u\u00b7me", "f\u00fcllt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df Leben durch das Weltall quillt,", "tokens": ["Da\u00df", "Le\u00b7ben", "durch", "das", "Wel\u00b7tall", "quillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch den der Sterne Jubel schwebt,", "tokens": ["Durch", "den", "der", "Ster\u00b7ne", "Ju\u00b7bel", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und die Ephemeride lebt:", "tokens": ["Und", "die", "E\u00b7phe\u00b7me\u00b7ri\u00b7de", "lebt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Ich jauchze dir, ich jauchze dir;", "tokens": ["Ich", "jauch\u00b7ze", "dir", ",", "ich", "jauch\u00b7ze", "dir", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Odem wehet \u00fcber mir:", "tokens": ["Dein", "O\u00b7dem", "we\u00b7het", "\u00fc\u00b7ber", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du h\u00f6rest, Vater, meinen Dank", "tokens": ["Du", "h\u00f6\u00b7rest", ",", "Va\u00b7ter", ",", "mei\u00b7nen", "Dank"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch deiner Sph\u00e4ren Lobgesang.", "tokens": ["Durch", "dei\u00b7ner", "Sph\u00e4\u00b7ren", "Lob\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}