{"textgrid.poem.35249": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie ist ihr holder Feind, der sich am kleinsten achtet,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie ist ihr holder Feind, der sich am kleinsten achtet,", "tokens": ["Sie", "ist", "ihr", "hol\u00b7der", "Feind", ",", "der", "sich", "am", "kleins\u00b7ten", "ach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "PRF", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und seinen N\u00e4chsten gro\u00df; der nicht nach Ehren trachtet,", "tokens": ["Und", "sei\u00b7nen", "N\u00e4chs\u00b7ten", "gro\u00df", ";", "der", "nicht", "nach", "Eh\u00b7ren", "trach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "$.", "ART", "PTKNEG", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und z\u00fcrnet ob dem Lob; Verachtung, Spott und Schand'", "tokens": ["Und", "z\u00fcr\u00b7net", "ob", "dem", "Lob", ";", "Ver\u00b7ach\u00b7tung", ",", "Spott", "und", "Schand'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOUS", "ART", "NN", "$.", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist ihr als eine Straf' der S\u00fcnden wohl bekannt.", "tokens": ["Ist", "ihr", "als", "ei\u00b7ne", "Straf'", "der", "S\u00fcn\u00b7den", "wohl", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KOUS", "ART", "NN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie pfleget nimmermehr nach gro\u00dfem Gut zu streben,", "tokens": ["Sie", "pfle\u00b7get", "nim\u00b7mer\u00b7mehr", "nach", "gro\u00b7\u00dfem", "Gut", "zu", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und w\u00fcnscht in ihrem Haus der Schnecke gleich zu leben", "tokens": ["Und", "w\u00fcnscht", "in", "ih\u00b7rem", "Haus", "der", "Schne\u00b7cke", "gleich", "zu", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In stiller Schattenruh'; sie tr\u00e4gt ein schlechtes Kleid,", "tokens": ["In", "stil\u00b7ler", "Schat\u00b7ten\u00b7ruh'", ";", "sie", "tr\u00e4gt", "ein", "schlech\u00b7tes", "Kleid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wei\u00df nicht, was man nennt Ha\u00df, Meuchellist und Neid.", "tokens": ["Und", "wei\u00df", "nicht", ",", "was", "man", "nennt", "Ha\u00df", ",", "Meu\u00b7chel\u00b7list", "und", "Neid", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PRELS", "PIS", "VVFIN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ihr Wort ist ja und nein, begierig, viel zu h\u00f6ren,", "tokens": ["Ihr", "Wort", "ist", "ja", "und", "nein", ",", "be\u00b7gie\u00b7rig", ",", "viel", "zu", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "KON", "PTKANT", "$,", "ADJD", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie ehret Jedermann und l\u00e4\u00dft sich t\u00e4glich lehren,", "tokens": ["Sie", "eh\u00b7ret", "Je\u00b7der\u00b7mann", "und", "l\u00e4\u00dft", "sich", "t\u00e4g\u00b7lich", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "KON", "VVFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bestrafet ihren Sinn, der sich nicht niederneigt", "tokens": ["Be\u00b7stra\u00b7fet", "ih\u00b7ren", "Sinn", ",", "der", "sich", "nicht", "nie\u00b7der\u00b7neigt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PRELS", "PRF", "PTKNEG", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und vor dem h\u00f6chsten Gott bis zu der Erden beugt.", "tokens": ["Und", "vor", "dem", "h\u00f6chs\u00b7ten", "Gott", "bis", "zu", "der", "Er\u00b7den", "beugt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "APPR", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}