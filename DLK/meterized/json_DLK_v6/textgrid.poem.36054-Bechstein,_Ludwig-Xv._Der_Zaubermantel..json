{"textgrid.poem.36054": {"metadata": {"author": {"name": "Bechstein, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Xv. Der Zaubermantel.", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwas frommt nun dumpfes Br\u00fcten bei Dingen, die geschah'n?", "tokens": ["\u00bb", "was", "frommt", "nun", "dum\u00b7pfes", "Br\u00fc\u00b7ten", "bei", "Din\u00b7gen", ",", "die", "ge\u00b7scha\u00b7h'n", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "ADV", "ADJA", "NN", "APPR", "NN", "$,", "PRELS", "VVPP", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das ist ein Basilisk-Ei, gelegt von einem Hahn.", "tokens": ["Das", "ist", "ein", "Ba\u00b7si\u00b7lis\u00b7k\u00b7Ei", ",", "ge\u00b7legt", "von", "ei\u00b7nem", "Hahn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Der Hahn, der heisst Gewissen, ist W\u00e4chteramts bewusst,", "tokens": ["Der", "Hahn", ",", "der", "heisst", "Ge\u00b7wis\u00b7sen", ",", "ist", "W\u00e4ch\u00b7ter\u00b7amts", "be\u00b7wusst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "NN", "$,", "VAFIN", "NE", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der Basilisk ist Tr\u00fcbsinn, vergiftend jede Lust.\u00ab", "tokens": ["Der", "Ba\u00b7si\u00b7lisk", "ist", "Tr\u00fcb\u00b7sinn", ",", "ver\u00b7gif\u00b7tend", "je\u00b7de", "Lust", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$,", "VVPP", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbauf, auf, zu frischem Leben! Fort in die weite Welt,", "tokens": ["\u00bb", "auf", ",", "auf", ",", "zu", "fri\u00b7schem", "Le\u00b7ben", "!", "Fort", "in", "die", "wei\u00b7te", "Welt", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "PTKVZ", "$,", "APPR", "ADJA", "NN", "$.", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wo nicht solch bittrer Unmuth die Freuden Dir verg\u00fcllt!", "tokens": ["Wo", "nicht", "solch", "bit\u00b7trer", "Un\u00b7muth", "die", "Freu\u00b7den", "Dir", "ver\u00b7g\u00fcllt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "PIAT", "ADJA", "NN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Zu frohen Hochzeitfesten, zu Reigen und Turnier!", "tokens": ["Zu", "fro\u00b7hen", "Hoch\u00b7zeit\u00b7fes\u00b7ten", ",", "zu", "Rei\u00b7gen", "und", "Tur\u00b7nier", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Von allen schlimmen G\u00e4sten ist Gram der schlimmste mir.\u00ab", "tokens": ["Von", "al\u00b7len", "schlim\u00b7men", "G\u00e4s\u00b7ten", "ist", "Gram", "der", "schlimms\u00b7te", "mir", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VAFIN", "NN", "ART", "ADJA", "PPER", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "So spricht zu Faust Mephisto, zu dem gebeugten Mann,", "tokens": ["So", "spricht", "zu", "Faust", "Me\u00b7phis\u00b7to", ",", "zu", "dem", "ge\u00b7beug\u00b7ten", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "NE", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der seines Namens Makel noch nicht vergessen kann.", "tokens": ["Der", "sei\u00b7nes", "Na\u00b7mens", "Ma\u00b7kel", "noch", "nicht", "ver\u00b7ges\u00b7sen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NE", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der seines Stolzes Glanzschild entehrt sieht und befleckt,", "tokens": ["Der", "sei\u00b7nes", "Stol\u00b7zes", "Glanz\u00b7schild", "ent\u00b7ehrt", "sieht", "und", "be\u00b7fleckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVPP", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und sich so weit vom Ruhmziel, das er sich ausgesteckt.", "tokens": ["Und", "sich", "so", "weit", "vom", "Ruhm\u00b7ziel", ",", "das", "er", "sich", "aus\u00b7ge\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADV", "ADJD", "APPRART", "NN", "$,", "PRELS", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "\u00bbwohlan denn, fort! Und trage mich nun auf sichrer Bahn!", "tokens": ["\u00bb", "wo\u00b7hlan", "denn", ",", "fort", "!", "Und", "tra\u00b7ge", "mich", "nun", "auf", "sich\u00b7rer", "Bahn", "!"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "$,", "PTKVZ", "$.", "KON", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+----+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Ein irrender Odysseus im Lebensozean,", "tokens": ["Ein", "ir\u00b7ren\u00b7der", "O\u00b7dys\u00b7seus", "im", "Le\u00b7ben\u00b7so\u00b7ze\u00b7an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "APPRART", "NN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Bin bald ich der Charybdis, bald Soylla's Heuleu nah,", "tokens": ["Bin", "bald", "ich", "der", "Cha\u00b7ryb\u00b7dis", ",", "bald", "Soyl\u00b7la's", "Heu\u00b7leu", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "ART", "NN", "$,", "ADV", "NE", "NE", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "Doch nimmer werd' ich finden der Sehnsucht Ithaka!\u00ab", "tokens": ["Doch", "nim\u00b7mer", "werd'", "ich", "fin\u00b7den", "der", "Sehn\u00b7sucht", "I\u00b7tha\u00b7ka", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVFIN", "ART", "NN", "NE", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbund nun wohin Mephisto? Den Ort bestimme gleich!\u00ab \u2013", "tokens": ["\u00bb", "und", "nun", "wo\u00b7hin", "Me\u00b7phis\u00b7to", "?", "Den", "Ort", "be\u00b7stim\u00b7me", "gleich", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "KON", "ADV", "PWAV", "NE", "$.", "ART", "NN", "VVFIN", "ADV", "$.", "$(", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da kommen drei Barone, jung, freudes\u00fcchtig, reich.", "tokens": ["Da", "kom\u00b7men", "drei", "Ba\u00b7ro\u00b7ne", ",", "jung", ",", "freu\u00b7de\u00b7s\u00fcch\u00b7tig", ",", "reich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "NN", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u00bbwir tragen grosse Bitten Euch vor, erhabner Mann,", "tokens": ["\u00bb", "wir", "tra\u00b7gen", "gros\u00b7se", "Bit\u00b7ten", "Euch", "vor", ",", "er\u00b7hab\u00b7ner", "Mann", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADJA", "NN", "PPER", "PTKVZ", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der, was unm\u00f6glich scheine, leicht m\u00f6glich machen kann!\u00ab", "tokens": ["Der", ",", "was", "un\u00b7m\u00f6g\u00b7lich", "schei\u00b7ne", ",", "leicht", "m\u00f6g\u00b7lich", "ma\u00b7chen", "kann", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "$,", "PRELS", "ADJD", "VVFIN", "$,", "ADJD", "ADJD", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Und Faustus: \u00bbRedet Freunde, sagt an, was Ihr begehrt!", "tokens": ["Und", "Faus\u00b7tus", ":", "\u00bb", "Re\u00b7det", "Freun\u00b7de", ",", "sagt", "an", ",", "was", "Ihr", "be\u00b7gehrt", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$.", "$(", "VVFIN", "NN", "$,", "VVFIN", "PTKVZ", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es hat Euch meine Liebe schon Euern Wunsch gew\u00e4hrt.", "tokens": ["Es", "hat", "Euch", "mei\u00b7ne", "Lie\u00b7be", "schon", "Eu\u00b7ern", "Wunsch", "ge\u00b7w\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ihr scheint mir reiselustig, und reisefertig gleich,", "tokens": ["Ihr", "scheint", "mir", "rei\u00b7se\u00b7lus\u00b7tig", ",", "und", "rei\u00b7se\u00b7fer\u00b7tig", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "KON", "ADJD", "ADV", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Verlangt Ihr eine Lustfahrt, so theil' ich sie mit Euch!\u00ab", "tokens": ["Ver\u00b7langt", "Ihr", "ei\u00b7ne", "Lust\u00b7fahrt", ",", "so", "theil'", "ich", "sie", "mit", "Euch", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "PPER", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "\u203aihr wisst, der Baierherzog verm\u00e4hlt der Tochter Hand;", "tokens": ["\u203a", "ihr", "wisst", ",", "der", "Bai\u00b7er\u00b7her\u00b7zog", "ver\u00b7m\u00e4hlt", "der", "Toch\u00b7ter", "Hand", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "ART", "NE", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ganz M\u00fcnchen gl\u00e4nzt im Festprunk, zu weit nur liegt das Land.", "tokens": ["Ganz", "M\u00fcn\u00b7chen", "gl\u00e4nzt", "im", "Fest\u00b7prunk", ",", "zu", "weit", "nur", "liegt", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "APPRART", "NN", "$,", "PTKA", "ADJD", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wir m\u00f6chten gern die Feier und gern das Brautpaar sehn,", "tokens": ["Wir", "m\u00f6ch\u00b7ten", "gern", "die", "Fei\u00b7er", "und", "gern", "das", "Braut\u00b7paar", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "KON", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Doch nur, wenn Ihr uns h\u00fclfreich, o Faustus, kann's geschehn.\u2039", "tokens": ["Doch", "nur", ",", "wenn", "Ihr", "uns", "h\u00fclf\u00b7reich", ",", "o", "Faus\u00b7tus", ",", "kann's", "ge\u00b7schehn", ".", "\u2039"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "PRF", "ADJD", "$,", "FM", "FM", "$,", "VMFIN", "VVINF", "$.", "$("], "meter": "--+-+-+-+-+-+", "measure": "anapaest.init"}}, "stanza.8": {"line.1": {"text": "Eine glatte Schlange giebt es, die leicht in Herzen schleicht,", "tokens": ["Ei\u00b7ne", "glat\u00b7te", "Schlan\u00b7ge", "giebt", "es", ",", "die", "leicht", "in", "Her\u00b7zen", "schleicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$,", "PRELS", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Und einen s\u00fcssen Honig, der Macht als Kost gereicht.", "tokens": ["Und", "ei\u00b7nen", "s\u00fcs\u00b7sen", "Ho\u00b7nig", ",", "der", "Macht", "als", "Kost", "ge\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "ART", "NN", "KOUS", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und eine Blume, giftig, voll Balsamdufts dabei:", "tokens": ["Und", "ei\u00b7ne", "Blu\u00b7me", ",", "gif\u00b7tig", ",", "voll", "Bal\u00b7sam\u00b7dufts", "da\u00b7bei", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADJD", "$,", "ADJD", "NN", "PAV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sieh, Schlange, Honig, Blume, das ist die ", "tokens": ["Sieh", ",", "Schlan\u00b7ge", ",", "Ho\u00b7nig", ",", "Blu\u00b7me", ",", "das", "ist", "die"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "PDS", "VAFIN", "ART"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "\u00bbwir schaun das F\u00fcrstenbrautpaar, eh' wieder Abend naht,", "tokens": ["\u00bb", "wir", "schaun", "das", "F\u00fcrs\u00b7ten\u00b7braut\u00b7paar", ",", "eh'", "wie\u00b7der", "A\u00b7bend", "naht", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Nur sorget, dass nicht lautbar und stadtkund wird die That.", "tokens": ["Nur", "sor\u00b7get", ",", "dass", "nicht", "laut\u00b7bar", "und", "stadt\u00b7kund", "wird", "die", "That", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PTKNEG", "ADJD", "KON", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wir wollen schneller reiten, wie Perseus einstens ritt,", "tokens": ["Wir", "wol\u00b7len", "schnel\u00b7ler", "rei\u00b7ten", ",", "wie", "Per\u00b7seus", "eins\u00b7tens", "ritt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VVFIN", "$,", "PWAV", "NE", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wir nehmen nur das H\u00fctlein des Fortunatus mit.\u00ab", "tokens": ["Wir", "neh\u00b7men", "nur", "das", "H\u00fct\u00b7lein", "des", "For\u00b7tu\u00b7na\u00b7tus", "mit", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "\u00bbich, Faustus-Fortunatus, f\u00fchr' Euch auf luft'ger Bahn;", "tokens": ["\u00bb", "ich", ",", "Faus\u00b7tus\u00b7For\u00b7tu\u00b7na\u00b7tus", ",", "f\u00fchr'", "Euch", "auf", "luft'\u00b7ger", "Bahn", ";"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "NE", "$,", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Es wird im Meer der Wolken mein Mantel unser Kahn.", "tokens": ["Es", "wird", "im", "Meer", "der", "Wol\u00b7ken", "mein", "Man\u00b7tel", "un\u00b7ser", "Kahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ART", "NN", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wir segeln hoch und schweigend, vom Aetherduft umhaucht,", "tokens": ["Wir", "se\u00b7geln", "hoch", "und", "schwei\u00b7gend", ",", "vom", "A\u00b7e\u00b7ther\u00b7duft", "um\u00b7haucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ins Bad der Morgenr\u00f6the das k\u00fchne Haupt getaucht!\u00ab", "tokens": ["Ins", "Bad", "der", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "das", "k\u00fch\u00b7ne", "Haupt", "ge\u00b7taucht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NE", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "So redet Faustus heiter, er winkt dem Diener zu;", "tokens": ["So", "re\u00b7det", "Faus\u00b7tus", "hei\u00b7ter", ",", "er", "winkt", "dem", "Die\u00b7ner", "zu", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADJD", "$,", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Aus breitet der den Mantel \u2013 sie schwinden hin im Nu.", "tokens": ["Aus", "brei\u00b7tet", "der", "den", "Man\u00b7tel", "\u2013", "sie", "schwin\u00b7den", "hin", "im", "Nu", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "ART", "NN", "$(", "PPER", "VVFIN", "ADV", "APPRART", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Hoch \u00fcber Berg' und W\u00e4lder, gleich einer Wolke Zug,", "tokens": ["Hoch", "\u00fc\u00b7ber", "Ber\u00b7g'", "und", "W\u00e4l\u00b7der", ",", "gleich", "ei\u00b7ner", "Wol\u00b7ke", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$,", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die Sturmwind fortpeitscht, richten sie s\u00fcdw\u00e4rts ihren Flug.", "tokens": ["Die", "Sturm\u00b7wind", "fort\u00b7peitscht", ",", "rich\u00b7ten", "sie", "s\u00fcd\u00b7w\u00e4rts", "ih\u00b7ren", "Flug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.12": {"line.1": {"text": "Und eh' der Alpen Kronen ergl\u00fchn im Morgenschein,", "tokens": ["Und", "eh'", "der", "Al\u00b7pen", "Kro\u00b7nen", "er\u00b7gl\u00fchn", "im", "Mor\u00b7gen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die fern her\u00e4bergr\u00fcssen ins ebne Land herein,", "tokens": ["Die", "fern", "he\u00b7r\u00e4\u00b7ber\u00b7gr\u00fcs\u00b7sen", "ins", "eb\u00b7ne", "Land", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Stellt schon auf festen Boden der Zaubrer seine Last,", "tokens": ["Stellt", "schon", "auf", "fes\u00b7ten", "Bo\u00b7den", "der", "Zaub\u00b7rer", "sei\u00b7ne", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "---+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die, schwindelnd von der Luftfahrt, noch kaum das Wunder fasst.", "tokens": ["Die", ",", "schwin\u00b7delnd", "von", "der", "Luft\u00b7fahrt", ",", "noch", "kaum", "das", "Wun\u00b7der", "fasst", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "APPR", "ART", "NN", "$,", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.13": {"line.1": {"text": "Und wo mit hellen Zinnen aufragt die Residenz,", "tokens": ["Und", "wo", "mit", "hel\u00b7len", "Zin\u00b7nen", "auf\u00b7ragt", "die", "Re\u00b7si\u00b7denz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Gr\u00fcsst man die reichen Fremden mit tiefer Reverenz.", "tokens": ["Gr\u00fcs\u00b7st", "man", "die", "rei\u00b7chen", "Frem\u00b7den", "mit", "tie\u00b7fer", "Re\u00b7ve\u00b7renz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sie schreiten, wie geladen, durch das Gew\u00fchl voll Pracht,", "tokens": ["Sie", "schrei\u00b7ten", ",", "wie", "ge\u00b7la\u00b7den", ",", "durch", "da