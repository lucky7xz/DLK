{"textgrid.poem.40860": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Aufruf", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Inde\u00df wir beim Turniere und Tanze ", "tokens": ["In\u00b7de\u00df", "wir", "beim", "Tur\u00b7nie\u00b7re", "und", "Tan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "KON", "NN"], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Trinkt in ", "tokens": ["Trinkt", "in"], "token_info": ["word", "word"], "pos": ["VVFIN", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Wir kitzeln ", "tokens": ["Wir", "kit\u00b7zeln"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "\u00bbein Narr, wer auf dem Todbett sich Hochzeitskr\u00e4nze flicht!", "tokens": ["\u00bb", "ein", "Narr", ",", "wer", "auf", "dem", "Tod\u00b7bett", "sich", "Hoch\u00b7zeits\u00b7kr\u00e4n\u00b7ze", "flicht", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PWS", "APPR", "ART", "NN", "PRF", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wer riss' ihm aus den H\u00e4nden das eitle Spielwerk nicht?", "tokens": ["Wer", "riss'", "ihm", "aus", "den", "H\u00e4n\u00b7den", "das", "eit\u00b7le", "Spiel\u00b7werk", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Es schreit um H\u00fclf' und Retter der Feuerglocken Gedr\u00f6hn,", "tokens": ["Es", "schreit", "um", "H\u00fcl\u00b7f'", "und", "Ret\u00b7ter", "der", "Feu\u00b7er\u00b7glo\u00b7cken", "Ge\u00b7dr\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+--+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Beim Brand des eignen Hauses schw\u00e4rmt nur ein Thor: ei wie sch\u00f6n!", "tokens": ["Beim", "Brand", "des", "eig\u00b7nen", "Hau\u00b7ses", "schw\u00e4rmt", "nur", "ein", "Thor", ":", "ei", "wie", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "$.", "ADJD", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Drum auf, ihr Herrn und Edlen, wohlauf zum Schwertertanz!", "tokens": ["Drum", "auf", ",", "ihr", "Herrn", "und", "Ed\u00b7len", ",", "wohl\u00b7auf", "zum", "Schwer\u00b7ter\u00b7tanz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "$,", "PPOSAT", "NN", "KON", "NN", "$,", "PWAV", "APPRART", "NN", "$."], "meter": "-+-+-+----+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Vom Haupt den welken Festkranz! Erringt euch neuen Kranz!", "tokens": ["Vom", "Haupt", "den", "wel\u00b7ken", "Fest\u00b7kranz", "!", "Er\u00b7ringt", "euch", "neu\u00b7en", "Kranz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$.", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Auf, sammelt eure Schaaren, dann an die Marken frisch,", "tokens": ["Auf", ",", "sam\u00b7melt", "eu\u00b7re", "Schaa\u00b7ren", ",", "dann", "an", "die", "Mar\u00b7ken", "frisch", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Dort laden wir uns wieder bei fr\u00e4nk'schen Wirthen zu Tisch!\u00ab", "tokens": ["Dort", "la\u00b7den", "wir", "uns", "wie\u00b7der", "bei", "fr\u00e4nk'\u00b7schen", "Wirt\u00b7hen", "zu", "Tisch", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "ADJA", "NN", "APPR", "NN", "$.", "$("], "meter": "-+---+--+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "So t\u00f6nte Maxens Rede hell durch den hohen Saal,", "tokens": ["So", "t\u00f6n\u00b7te", "Ma\u00b7xens", "Re\u00b7de", "hell", "durch", "den", "ho\u00b7hen", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "NN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Rings jauchzten lautauf Beifall die Edlen allzumal,", "tokens": ["Rings", "jauchz\u00b7ten", "laut\u00b7auf", "Bei\u00b7fall", "die", "Ed\u00b7len", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und horch! herauf aus dem Hofe, als stimmt' es froh mit ein,", "tokens": ["Und", "horch", "!", "her\u00b7auf", "aus", "dem", "Ho\u00b7fe", ",", "als", "stimmt'", "es", "froh", "mit", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "ADV", "APPR", "ART", "NN", "$,", "KOUS", "VVFIN", "PPER", "ADJD", "APPR", "ART", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Scholl muthig Ro\u00dfgewieher in das Gejubel drein.", "tokens": ["Scholl", "mut\u00b7hig", "Ro\u00df\u00b7ge\u00b7wie\u00b7her", "in", "das", "Ge\u00b7ju\u00b7bel", "drein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.5": {"line.1": {"text": "Da hatten Aller Augen zum Fenster sich gekehrt,", "tokens": ["Da", "hat\u00b7ten", "Al\u00b7ler", "Au\u00b7gen", "zum", "Fens\u00b7ter", "sich", "ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJA", "NN", "APPRART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "An einer S\u00e4ule gebunden stand unten ein wei\u00dfes Pferd.", "tokens": ["An", "ei\u00b7ner", "S\u00e4u\u00b7le", "ge\u00b7bun\u00b7den", "stand", "un\u00b7ten", "ein", "wei\u00b7\u00dfes", "Pferd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u00bbein herrlich Thier, beim Himmel! Ihr Herrn, aus wessen Stall?\u00ab", "tokens": ["\u00bb", "ein", "herr\u00b7lich", "Thier", ",", "beim", "Him\u00b7mel", "!", "Ihr", "Herrn", ",", "aus", "wes\u00b7sen", "Stall", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJD", "NN", "$,", "APPRART", "NN", "$.", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da sch\u00fcttelten die K\u00f6pfe und zuckten die Achseln All'.", "tokens": ["Da", "sch\u00fct\u00b7tel\u00b7ten", "die", "K\u00f6p\u00b7fe", "und", "zuck\u00b7ten", "die", "Ach\u00b7seln", "All'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "NE", "$."], "meter": "-+---+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbseht nur den stolzen Nacken, das Auge muthighell,", "tokens": ["\u00bb", "seht", "nur", "den", "stol\u00b7zen", "Na\u00b7cken", ",", "das", "Au\u00b7ge", "mut\u00b7hig\u00b7hell", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "---+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bunt und doch ohne Makel, wie Fr\u00fchlingswolken das Fell!", "tokens": ["Bunt", "und", "doch", "oh\u00b7ne", "Ma\u00b7kel", ",", "wie", "Fr\u00fch\u00b7lings\u00b7wol\u00b7ken", "das", "Fell", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "APPR", "NN", "$,", "PWAV", "NN", "ART", "NN", "$."], "meter": "+--+-+--+-+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Goldquasten rieseln klingend, wirr durch einander bewegt,", "tokens": ["Gold\u00b7quas\u00b7ten", "rie\u00b7seln", "klin\u00b7gend", ",", "wirr", "durch", "ein\u00b7an\u00b7der", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVPP", "$,", "ADJD", "APPR", "PRF", "VVFIN", "$,"], "meter": "++-+-+-+--+--+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Wenn mit dem Hufe stampfend es k\u00fchn den Boden schl\u00e4gt.", "tokens": ["Wenn", "mit", "dem", "Hu\u00b7fe", "stamp\u00b7fend", "es", "k\u00fchn", "den", "Bo\u00b7den", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VVPP", "PPER", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Stolz tr\u00e4gt die Purpurdecke, wie'n K\u00f6nig, das edle Thier;", "tokens": ["Stolz", "tr\u00e4gt", "die", "Pur\u00b7pur\u00b7de\u00b7cke", ",", "wie'n", "K\u00f6\u00b7nig", ",", "das", "ed\u00b7le", "Thier", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--+--+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Doch ha ha, ein Liebesbrieflein! \u2013 seht, 's ist zum Bersten schier!", "tokens": ["Doch", "ha", "ha", ",", "ein", "Lie\u00b7bes\u00b7brie\u00b7flein", "!", "\u2013", "seht", ",", "'s", "ist", "zum", "Bers\u00b7ten", "schier", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "$,", "ART", "NN", "$.", "$(", "VVFIN", "$,", "PPER", "VAFIN", "APPRART", "NN", "ADJD", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Geheftet unter dem Schweife tr\u00e4gt es ein wei\u00df Paket,", "tokens": ["Ge\u00b7hef\u00b7tet", "un\u00b7ter", "dem", "Schwei\u00b7fe", "tr\u00e4gt", "es", "ein", "wei\u00df", "Pa\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "VVFIN", "NE", "$,"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ei geh' doch Einer hinunter und seh', was drinnen steht?\u00ab", "tokens": ["Ei", "geh'", "doch", "Ei\u00b7ner", "hin\u00b7un\u00b7ter", "und", "seh'", ",", "was", "drin\u00b7nen", "steht", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "ADV", "PIS", "PTKVZ", "KON", "VVFIN", "$,", "PRELS", "ADV", "VVFIN", "$.", "$("], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}}, "stanza.8": {"line.1": {"text": "Da machte Einer unten das Blatt behutsam los,", "tokens": ["Da", "mach\u00b7te", "Ei\u00b7ner", "un\u00b7ten", "das", "Blatt", "be\u00b7hut\u00b7sam", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das in sich solche Worte und solchen Sinn verschlo\u00df:", "tokens": ["Das", "in", "sich", "sol\u00b7che", "Wor\u00b7te", "und", "sol\u00b7chen", "Sinn", "ver\u00b7schlo\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PRF", "PIAT", "NN", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u00bbwir Louis der Eilft' in Frankreich, Navarr' et c\u00e4tera", "tokens": ["\u00bb", "wir", "Lou\u00b7is", "der", "Eilft'", "in", "Fran\u00b7kreich", ",", "Na\u00b7varr'", "et", "c\u00e4\u00b7te\u00b7ra"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPER", "NE", "ART", "NN", "APPR", "NE", "$,", "NE", "NE", "NE"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Durch Gottes Gnaden K\u00f6nig, Herzog in Burgundia.\u00ab", "tokens": ["Durch", "Got\u00b7tes", "Gna\u00b7den", "K\u00f6\u00b7nig", ",", "Her\u00b7zog", "in", "Bur\u00b7gun\u00b7dia", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "NN", "NN", "$,", "NE", "APPR", "NE", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "\u00bbei, heftet unser Vetter an solchen Ort sein Mandat?", "tokens": ["\u00bb", "ei", ",", "hef\u00b7tet", "un\u00b7ser", "Vet\u00b7ter", "an", "sol\u00b7chen", "Ort", "sein", "Man\u00b7dat", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Doch fahre fort zu lesen \u2013 's ist lustig in der That!\u00ab", "tokens": ["Doch", "fah\u00b7re", "fort", "zu", "le\u00b7sen", "\u2013", "'s", "ist", "lus\u00b7tig", "in", "der", "That", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$(", "PPER", "VAFIN", "ADJD", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "So scherzet Max und l\u00e4chelt, doch glimmt sein Blick voll Gluth,", "tokens": ["So", "scher\u00b7zet", "Max", "und", "l\u00e4\u00b7chelt", ",", "doch", "glimmt", "sein", "Blick", "voll", "Gluth", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "KON", "VVFIN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So l\u00e4chelt fern das W\u00f6lkchen, in dem der Brandkeil ruht.", "tokens": ["So", "l\u00e4\u00b7chelt", "fern", "das", "W\u00f6lk\u00b7chen", ",", "in", "dem", "der", "Brand\u00b7keil", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "\u00bban unsern lieben Vetter, Erzherzog von Oesterreich!", "tokens": ["\u00bb", "an", "un\u00b7sern", "lie\u00b7ben", "Vet\u00b7ter", ",", "Erz\u00b7her\u00b7zog", "von", "O\u00b7es\u00b7ter\u00b7reich", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "ADJA", "NN", "$,", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.2": {"text": "Man spricht, Ihr wollt uns besuchen, der Wunsch k\u00f6mmt unserm gleich;", "tokens": ["Man", "spricht", ",", "Ihr", "wollt", "uns", "be\u00b7su\u00b7chen", ",", "der", "Wunsch", "k\u00f6mmt", "un\u00b7serm", "gleich", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,", "ART", "NN", "VVFIN", "PPOSAT", "ADV", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Drum senden wir die\u00df R\u00f6\u00dflein, der Weg ist etwas weit,", "tokens": ["Drum", "sen\u00b7den", "wir", "die\u00df", "R\u00f6\u00df\u00b7lein", ",", "der", "Weg", "ist", "et\u00b7was", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PDS", "NN", "$,", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.in