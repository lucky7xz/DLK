{"textgrid.poem.36009": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "Dem lieben Vater zum neuen Jahr 1825", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Vater! nimm voll Liebe", "tokens": ["O", "Va\u00b7ter", "!", "nimm", "voll", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "VVIMP", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Heut meine W\u00fcnsche hin:", "tokens": ["Heut", "mei\u00b7ne", "W\u00fcn\u00b7sche", "hin", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nie sei dein Leben tr\u00fcbe,", "tokens": ["Nie", "sei", "dein", "Le\u00b7ben", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zufrieden stets dein Sinn.", "tokens": ["Zu\u00b7frie\u00b7den", "stets", "dein", "Sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du f\u00fchrtest mich zur Tugend,", "tokens": ["Du", "f\u00fchr\u00b7test", "mich", "zur", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Vater! Dank sei dir.", "tokens": ["O", "Va\u00b7ter", "!", "Dank", "sei", "dir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "NN", "VAFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du warntest meine Jugend,", "tokens": ["Du", "warn\u00b7test", "mei\u00b7ne", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gabst fromme Lehren mir.", "tokens": ["Gabst", "from\u00b7me", "Leh\u00b7ren", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da\u00df Gott voll Vatertriebe", "tokens": ["Da\u00df", "Gott", "voll", "Va\u00b7ter\u00b7trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Uns f\u00fchrt an milder Hand,", "tokens": ["Uns", "f\u00fchrt", "an", "mil\u00b7der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df all sein Tun nur Liebe,", "tokens": ["Da\u00df", "all", "sein", "Tun", "nur", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "PPOSAT", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ward mir durch dich bekannt.", "tokens": ["Ward", "mir", "durch", "dich", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nun soll mich nicht bet\u00f6ren", "tokens": ["Nun", "soll", "mich", "nicht", "be\u00b7t\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Eitelkeiten Lust,", "tokens": ["Der", "Ei\u00b7tel\u00b7kei\u00b7ten", "Lust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Treu will ich deine Lehren", "tokens": ["Treu", "will", "ich", "dei\u00b7ne", "Leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bewahren in der Brust.", "tokens": ["Be\u00b7wah\u00b7ren", "in", "der", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und gl\u00e4ubig will ich schauen", "tokens": ["Und", "gl\u00e4u\u00b7big", "will", "ich", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Zuversicht zu Gott,", "tokens": ["Mit", "Zu\u00b7ver\u00b7sicht", "zu", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und immer ihm vertrauen", "tokens": ["Und", "im\u00b7mer", "ihm", "ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Leben und im Tod.", "tokens": ["Im", "Le\u00b7ben", "und", "im", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und dir dank' ich die Freude,", "tokens": ["Und", "dir", "dank'", "ich", "die", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die hohe Seligkeit,", "tokens": ["Die", "ho\u00b7he", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die nach dem Erdenleide", "tokens": ["Die", "nach", "dem", "Er\u00b7den\u00b7lei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dort ewig mich erfreut.", "tokens": ["Dort", "e\u00b7wig", "mich", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}