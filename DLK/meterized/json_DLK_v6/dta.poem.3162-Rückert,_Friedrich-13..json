{"dta.poem.3162": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Mutter hast du mir, den Vater noch vorab,", "tokens": ["Die", "Mut\u00b7ter", "hast", "du", "mir", ",", "den", "Va\u00b7ter", "noch", "vor\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PPER", "$,", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Schwester zwischenein, geleitet all' zu Grab.", "tokens": ["Die", "Schwes\u00b7ter", "zwi\u00b7schen\u00b7ein", ",", "ge\u00b7lei\u00b7tet", "all'", "zu", "Grab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "VVFIN", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Den allen warest du nicht Arzt allein des Leibes,", "tokens": ["Den", "al\u00b7len", "wa\u00b7rest", "du", "nicht", "Arzt", "al\u00b7lein", "des", "Lei\u00b7bes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "PPER", "PTKNEG", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Seelentrost und Freund; das sei auch mir und bleib' es.", "tokens": ["Ein", "See\u00b7lent\u00b7rost", "und", "Freund", ";", "das", "sei", "auch", "mir", "und", "bleib'", "es", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$.", "PDS", "VAFIN", "ADV", "PPER", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nenn' ich dich Aeskulap? ich nenne r\u00fcstig heiter", "tokens": ["Nenn'", "ich", "dich", "A\u00b7es\u00b7ku\u00b7lap", "?", "ich", "nen\u00b7ne", "r\u00fcs\u00b7tig", "hei\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "NE", "$.", "PPER", "VVFIN", "ADJD", "ADJD"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dich Hermes mit dem Stab, den Seelenheimgeleiter.", "tokens": ["Dich", "Her\u00b7mes", "mit", "dem", "Stab", ",", "den", "See\u00b7len\u00b7heim\u00b7ge\u00b7lei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NE", "APPR", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du legest ja nicht auf, erhebest nur den Zoll,", "tokens": ["Du", "le\u00b7gest", "ja", "nicht", "auf", ",", "er\u00b7he\u00b7best", "nur", "den", "Zoll", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "PTKVZ", "$,", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und hilfst gewissenhaft sterben, was sterben soll.", "tokens": ["Und", "hilfst", "ge\u00b7wis\u00b7sen\u00b7haft", "ster\u00b7ben", ",", "was", "ster\u00b7ben", "soll", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "VVINF", "$,", "PWS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ihr Aerzte seid einmal verordnet uns zu M\u00f6rdern;", "tokens": ["Ihr", "A\u00b7erz\u00b7te", "seid", "ein\u00b7mal", "ver\u00b7ord\u00b7net", "uns", "zu", "M\u00f6r\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Heil denen, die geschickt und freundlich uns bef\u00f6rdern.", "tokens": ["Heil", "de\u00b7nen", ",", "die", "ge\u00b7schickt", "und", "freund\u00b7lich", "uns", "be\u00b7f\u00f6r\u00b7dern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDS", "$,", "PRELS", "VVPP", "KON", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}