{"textgrid.poem.40693": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "[auf stillem Teich wird leicht dich tragen]", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf stillem Teich wird leicht dich tragen", "tokens": ["Auf", "stil\u00b7lem", "Teich", "wird", "leicht", "dich", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ADJD", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den einzlen Mann, der schmale Kahn;", "tokens": ["Den", "einz\u00b7len", "Mann", ",", "der", "schma\u00b7le", "Kahn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch durch den st\u00fcrm'schen Ozean", "tokens": ["Doch", "durch", "den", "st\u00fcr\u00b7m'\u00b7schen", "O\u00b7ze\u00b7an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mu\u00dft du nach m\u00e4cht'germ Fahrzeug fragen", "tokens": ["Mu\u00dft", "du", "nach", "m\u00e4cht'\u00b7germ", "Fahr\u00b7zeug", "fra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und mit Genossen dich vertragen.", "tokens": ["Und", "mit", "Ge\u00b7nos\u00b7sen", "dich", "ver\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}