{"dta.poem.19378": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "111.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vier K\u00f6nigst\u00f6chter sind auf einem rings von Wogen", "tokens": ["Vier", "K\u00f6\u00b7nig\u00b7st\u00f6ch\u00b7ter", "sind", "auf", "ei\u00b7nem", "rings", "von", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VAFIN", "APPR", "ART", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Umsp\u00fclten Lenzeiland von einer Fee erzogen.", "tokens": ["Um\u00b7sp\u00fcl\u00b7ten", "Len\u00b7zei\u00b7land", "von", "ei\u00b7ner", "Fee", "er\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und morgen sollen sie zur\u00fcck zur Heimath ziehn,", "tokens": ["Und", "mor\u00b7gen", "sol\u00b7len", "sie", "zu\u00b7r\u00fcck", "zur", "Hei\u00b7math", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PTKVZ", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil ihnen aller Schmuck der Bildung ist verliehn.", "tokens": ["Weil", "ih\u00b7nen", "al\u00b7ler", "Schmuck", "der", "Bil\u00b7dung", "ist", "ver\u00b7liehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Da sprach die Fee: Ich bin mit jeder wohl zufrieden,", "tokens": ["Da", "sprach", "die", "Fee", ":", "Ich", "bin", "mit", "je\u00b7der", "wohl", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "PPER", "VAFIN", "APPR", "PIS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch einer mu\u00df zuletzt der Vorzug seyn beschieden.", "tokens": ["Doch", "ei\u00b7ner", "mu\u00df", "zu\u00b7letzt", "der", "Vor\u00b7zug", "seyn", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "ADV", "ART", "NN", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nun geht zur Ruh, und wann euch weckt des Morgens Glanz,", "tokens": ["Nun", "geht", "zur", "Ruh", ",", "und", "wann", "euch", "weckt", "des", "Mor\u00b7gens", "Glanz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$,", "KON", "PWAV", "PPER", "VVFIN", "ART", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist einer unter euch beschert ein Perlenkranz.", "tokens": ["Ist", "ei\u00b7ner", "un\u00b7ter", "euch", "be\u00b7schert", "ein", "Per\u00b7len\u00b7kranz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Dieselbe findet ihn am Grund des K\u00f6rbchens liegen;", "tokens": ["Die\u00b7sel\u00b7be", "fin\u00b7det", "ihn", "am", "Grund", "des", "K\u00f6rb\u00b7chens", "lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den soll die Finderin bewahren hold verschwiegen. \u2014", "tokens": ["Den", "soll", "die", "Fin\u00b7de\u00b7rin", "be\u00b7wah\u00b7ren", "hold", "ver\u00b7schwie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "VVINF", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Da blickten alle vier einander l\u00e4chelnd an,", "tokens": ["Da", "blick\u00b7ten", "al\u00b7le", "vier", "ein\u00b7an\u00b7der", "l\u00e4\u00b7chelnd", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "CARD", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und jede dachte: die wird wol den Preis empfahn.", "tokens": ["Und", "je\u00b7de", "dach\u00b7te", ":", "die", "wird", "wol", "den", "Preis", "em\u00b7pfahn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$.", "PDS", "VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Nicht eine dachte, da\u00df sie selber siegen sollte,", "tokens": ["Nicht", "ei\u00b7ne", "dach\u00b7te", ",", "da\u00df", "sie", "sel\u00b7ber", "sie\u00b7gen", "soll\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur, wie sie sich des Siegs der andern freuen wollte.", "tokens": ["Nur", ",", "wie", "sie", "sich", "des", "Siegs", "der", "an\u00b7dern", "freu\u00b7en", "woll\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "PRF", "ART", "NN", "ART", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "So tr\u00e4umten sie die Nacht bis zu des Morgens Glanz,", "tokens": ["So", "tr\u00e4um\u00b7ten", "sie", "die", "Nacht", "bis", "zu", "des", "Mor\u00b7gens", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "APPR", "ART", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und an des K\u00f6rbchens Grund fand jede einen Kranz.", "tokens": ["Und", "an", "des", "K\u00f6rb\u00b7chens", "Grund", "fand", "je\u00b7de", "ei\u00b7nen", "Kranz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Err\u00f6thend lie\u00dfen sie den Kranz im K\u00f6rbchen liegen,", "tokens": ["Er\u00b7r\u00f6\u00b7thend", "lie\u00b7\u00dfen", "sie", "den", "Kranz", "im", "K\u00f6rb\u00b7chen", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und jede h\u00e4tte gern sich selbst den Fund verschwiegen.", "tokens": ["Und", "je\u00b7de", "h\u00e4t\u00b7te", "gern", "sich", "selbst", "den", "Fund", "ver\u00b7schwie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "VAFIN", "ADV", "PRF", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Doch als der Abschied kam, verrieth die holde Scham", "tokens": ["Doch", "als", "der", "Ab\u00b7schied", "kam", ",", "ver\u00b7rieth", "die", "hol\u00b7de", "Scham"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von jeder jeder wohl, was jede mit sich nahm.", "tokens": ["Von", "je\u00b7der", "je\u00b7der", "wohl", ",", "was", "je\u00b7de", "mit", "sich", "nahm", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PIS", "ADV", "$,", "PRELS", "PIAT", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Sie brauchten sich es nicht zu fragen noch zu sagen,", "tokens": ["Sie", "brauch\u00b7ten", "sich", "es", "nicht", "zu", "fra\u00b7gen", "noch", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PPER", "PTKNEG", "PTKZU", "VVINF", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und f\u00fchlten sich begl\u00fcckt all' einen Kranz zu tragen.", "tokens": ["Und", "f\u00fchl\u00b7ten", "sich", "be\u00b7gl\u00fcckt", "all'", "ei\u00b7nen", "Kranz", "zu", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "VVFIN", "PIS", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}