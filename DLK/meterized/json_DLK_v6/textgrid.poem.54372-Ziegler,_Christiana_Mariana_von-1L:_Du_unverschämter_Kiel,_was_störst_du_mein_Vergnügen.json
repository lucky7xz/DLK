{"textgrid.poem.54372": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du unversch\u00e4mter Kiel, was st\u00f6rst du mein Vergn\u00fcgen", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du unversch\u00e4mter Kiel, was st\u00f6rst du mein Vergn\u00fcgen", "tokens": ["Du", "un\u00b7ver\u00b7sch\u00e4m\u00b7ter", "Kiel", ",", "was", "st\u00f6rst", "du", "mein", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "PWS", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das mir der Himmel g\u00f6nnt, das Gl\u00fccke beygelegt?", "tokens": ["Das", "mir", "der", "Him\u00b7mel", "g\u00f6nnt", ",", "das", "Gl\u00fc\u00b7cke", "bey\u00b7ge\u00b7legt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "VVFIN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer l\u00e4\u00dft ein freches Blat, zu meinem Throne fliegen,", "tokens": ["Wer", "l\u00e4\u00dft", "ein", "fre\u00b7ches", "Blat", ",", "zu", "mei\u00b7nem", "Thro\u00b7ne", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das voller L\u00e4strung ist, statt Dinte Galle hegt?", "tokens": ["Das", "vol\u00b7ler", "L\u00e4st\u00b7rung", "ist", ",", "statt", "Din\u00b7te", "Gal\u00b7le", "hegt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "KOUI", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie? schickt es nicht Alphons? der l\u00e4ngst verjagte K\u00f6nig?", "tokens": ["Wie", "?", "schickt", "es", "nicht", "Al\u00b7phons", "?", "der", "l\u00e4ngst", "ver\u00b7jag\u00b7te", "K\u00f6\u00b7nig", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "PTKNEG", "NN", "$.", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Tollk\u00fchner! wagst du dich die Majestet zu schm\u00e4hn?", "tokens": ["Toll\u00b7k\u00fch\u00b7ner", "!", "wagst", "du", "dich", "die", "Ma\u00b7jes\u00b7tet", "zu", "schm\u00e4hn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "PRF", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Reizt dich die Raserey? ach du bist viel zu wenig,", "tokens": ["Reizt", "dich", "die", "Ra\u00b7se\u00b7rey", "?", "ach", "du", "bist", "viel", "zu", "we\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "XY", "PPER", "VAFIN", "ADV", "PTKA", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nach einer K\u00f6niginn voll Rach und Zorn zu sehn.", "tokens": ["Nach", "ei\u00b7ner", "K\u00f6\u00b7ni\u00b7ginn", "voll", "Rach", "und", "Zorn", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Unterthan will noch im tiefsten Kerker fluchen;", "tokens": ["Der", "Un\u00b7ter\u00b7than", "will", "noch", "im", "tiefs\u00b7ten", "Ker\u00b7ker", "flu\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein ganz versto\u00dfner Knecht, und halb zertretner Wurm,", "tokens": ["Ein", "ganz", "ver\u00b7sto\u00df\u00b7ner", "Knecht", ",", "und", "halb", "zer\u00b7tret\u00b7ner", "Wurm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$,", "KON", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der billig tief geb\u00fcckt, die Gnade sollte suchen,", "tokens": ["Der", "bil\u00b7lig", "tief", "ge\u00b7b\u00fcckt", ",", "die", "Gna\u00b7de", "soll\u00b7te", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "VVPP", "$,", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Erhebt, o Frevelthat! sein Haupt mit Wuth und Sturm.", "tokens": ["Er\u00b7hebt", ",", "o", "Fre\u00b7vel\u00b7that", "!", "sein", "Haupt", "mit", "Wuth", "und", "Sturm", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "NN", "$.", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Was willst du \u00fcber mich so unbesonnen schreyen?", "tokens": ["Was", "willst", "du", "\u00fc\u00b7ber", "mich", "so", "un\u00b7be\u00b7son\u00b7nen", "schre\u00b7yen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.14": {"text": "Ach w\u00fcte wider dich, du bist selbst Schuld daran,", "tokens": ["Ach", "w\u00fc\u00b7te", "wi\u00b7der", "dich", ",", "du", "bist", "selbst", "Schuld", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "APPR", "PPER", "$,", "PPER", "VAFIN", "ADV", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und h\u00e4ttest dir vorl\u00e4ngst das k\u00f6nnen prophezeyen,", "tokens": ["Und", "h\u00e4t\u00b7test", "dir", "vor\u00b7l\u00e4ngst", "das", "k\u00f6n\u00b7nen", "pro\u00b7phe\u00b7ze\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVFIN", "PDS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Was billig Reich und Land zu letzt an dir gethan.", "tokens": ["Was", "bil\u00b7lig", "Reich", "und", "Land", "zu", "letzt", "an", "dir", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "NN", "KON", "NN", "APPR", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wenn ein Regente nicht das Scepter weis zu tragen,", "tokens": ["Wenn", "ein", "Re\u00b7gen\u00b7te", "nicht", "das", "Scep\u00b7ter", "weis", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "ART", "NN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "Und dazu ungeschickt in aller Augen ist;", "tokens": ["Und", "da\u00b7zu", "un\u00b7ge\u00b7schickt", "in", "al\u00b7ler", "Au\u00b7gen", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ADJD", "APPR", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So wird so leicht kein Mensch zu deinem Vortheil sagen,", "tokens": ["So", "wird", "so", "leicht", "kein", "Mensch", "zu", "dei\u00b7nem", "Vor\u00b7theil", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da\u00df man dasselbige in deiner Hand vermi\u00dft.", "tokens": ["Da\u00df", "man", "das\u00b7sel\u00b7bi\u00b7ge", "in", "dei\u00b7ner", "Hand", "ver\u00b7mi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PDAT", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Dein Schreiben l\u00e4sset mich viel tolle Tr\u00e4ume lesen,", "tokens": ["Dein", "Schrei\u00b7ben", "l\u00e4s\u00b7set", "mich", "viel", "tol\u00b7le", "Tr\u00e4u\u00b7me", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wie! schl\u00e4fst du? weil du mich dein Weib so oft genennt?", "tokens": ["Wie", "!", "schl\u00e4fst", "du", "?", "weil", "du", "mich", "dein", "Weib", "so", "oft", "ge\u00b7nennt", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "$.", "KOUS", "PPER", "PRF", "PPOSAT", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ich bin wohl K\u00f6niginn, nicht dein Gemal gewesen;", "tokens": ["Ich", "bin", "wohl", "K\u00f6\u00b7ni\u00b7ginn", ",", "nicht", "dein", "Ge\u00b7mal", "ge\u00b7we\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$,", "PTKNEG", "PPOSAT", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Schau wie der falsche Wahn dir Aug und Sinn verblendt.", "tokens": ["Schau", "wie", "der", "fal\u00b7sche", "Wahn", "dir", "Aug", "und", "Sinn", "ver\u00b7blendt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "ADJA", "NN", "PPER", "NN", "KON", "NN", "VVPP", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.25": {"text": "Willst du vielleicht daraus ein Eheband erzwingen,", "tokens": ["Willst", "du", "viel\u00b7leicht", "da\u00b7raus", "ein", "E\u00b7he\u00b7band", "er\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PAV", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.26": {"text": "Da\u00df man mich \u00f6ffentlich an deine Hand gesellt,", "tokens": ["Da\u00df", "man", "mich", "\u00f6f\u00b7fent\u00b7lich", "an", "dei\u00b7ne", "Hand", "ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+---+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.27": {"text": "Und weil ich w\u00fcrdig war mich auf den Thron zu bringen,", "tokens": ["Und", "weil", "ich", "w\u00fcr\u00b7dig", "war", "mich", "auf", "den", "Thron", "zu", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VAFIN", "PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die Krone die du tr\u00e4gst, zugleich mir zugestellt;", "tokens": ["Die", "Kro\u00b7ne", "die", "du", "tr\u00e4gst", ",", "zu\u00b7gleich", "mir", "zu\u00b7ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "VVFIN", "$,", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "O! so betr\u00fcgst du dich, und wirst am besten wissen,", "tokens": ["O", "!", "so", "be\u00b7tr\u00fcgst", "du", "dich", ",", "und", "wirst", "am", "bes\u00b7ten", "wis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPER", "PRF", "$,", "KON", "VAFIN", "PTKA", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Da\u00df doch mein Stand dabey stets unver\u00e4ndert blieb;", "tokens": ["Da\u00df", "doch", "mein", "Stand", "da\u00b7bey", "stets", "un\u00b7ver\u00b7\u00e4n\u00b7dert", "blieb", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "PAV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Weil mich, ob gleich mein Geist der Freyheit ward entrissen,", "tokens": ["Weil", "mich", ",", "ob", "gleich", "mein", "Geist", "der", "Frey\u00b7heit", "ward", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "ADV", "PPOSAT", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die Unschuld immer noch in ihre Rolle schrieb.", "tokens": ["Die", "Un\u00b7schuld", "im\u00b7mer", "noch", "in", "ih\u00b7re", "Rol\u00b7le", "schrieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wie magst du, sch\u00e4me dich, Unsinniger, wohl sprechen?", "tokens": ["Wie", "magst", "du", ",", "sch\u00e4\u00b7me", "dich", ",", "Un\u00b7sin\u00b7ni\u00b7ger", ",", "wohl", "spre\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "NN", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ob w\u00fcrde dermaleinst der Himmel ganz gewi\u00df", "tokens": ["Ob", "w\u00fcr\u00b7de", "der\u00b7ma\u00b7leinst", "der", "Him\u00b7mel", "ganz", "ge\u00b7wi\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "ADV", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die Schande, so ich trieb, mit gr\u00f6stem Nachdruck r\u00e4chen,", "tokens": ["Die", "Schan\u00b7de", ",", "so", "ich", "trieb", ",", "mit", "gr\u00f6s\u00b7tem", "Nach\u00b7druck", "r\u00e4\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Dies wahrlich macht mir nicht die mindeste K\u00fcmmerni\u00df.", "tokens": ["Dies", "wahr\u00b7lich", "macht", "mir", "nicht", "die", "min\u00b7des\u00b7te", "K\u00fcm\u00b7mer\u00b7ni\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.37": {"text": "Don Pedro konnte sich mit mir gar wohl verm\u00e4hlen,", "tokens": ["Don", "Ped\u00b7ro", "konn\u00b7te", "sich", "mit", "mir", "gar", "wohl", "ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VMFIN", "PRF", "APPR", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "An dem das Reich nunmehr viel Lust und Freude schaut.", "tokens": ["An", "dem", "das", "Reich", "nun\u00b7mehr", "viel", "Lust", "und", "Freu\u00b7de", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ADV", "PIAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Der R\u00f6msche Stuhl, der nicht verm\u00f6gend ist zu fehlen,", "tokens": ["Der", "R\u00f6m\u00b7sche", "Stuhl", ",", "der", "nicht", "ver\u00b7m\u00f6\u00b7gend", "ist", "zu", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PTKNEG", "VVPP", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "lin