 "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Dein Ach und Weh, das du mir suchest nach zurufen,", "tokens": ["Dein", "Ach", "und", "Weh", ",", "das", "du", "mir", "su\u00b7chest", "nach", "zu\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "APPR", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Mu\u00df eben, wie dein Geist, so schwach und kraftlos seyn,", "tokens": ["Mu\u00df", "e\u00b7ben", ",", "wie", "dein", "Geist", ",", "so", "schwach", "und", "kraft\u00b7los", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "$,", "PWAV", "PPOSAT", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Ich h\u00f6re nichts davon auf meines Thrones Stuffen,", "tokens": ["Ich", "h\u00f6\u00b7re", "nichts", "da\u00b7von", "auf", "mei\u00b7nes", "Thro\u00b7nes", "Stuf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PAV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Den deiner Thr\u00e4nen Na\u00df noch minder kann entweyhn.", "tokens": ["Den", "dei\u00b7ner", "Thr\u00e4\u00b7nen", "Na\u00df", "noch", "min\u00b7der", "kann", "ent\u00b7weyhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "ADV", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Dein frecher Fluch, der dir, du Schaum der L\u00e4sterzungen,", "tokens": ["Dein", "fre\u00b7cher", "Fluch", ",", "der", "dir", ",", "du", "Schaum", "der", "L\u00e4s\u00b7ter\u00b7zun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "PPER", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Fast ieden Augenblick vor Raserey entf\u00e4hrt,", "tokens": ["Fast", "ie\u00b7den", "Au\u00b7gen\u00b7blick", "vor", "Ra\u00b7se\u00b7rey", "ent\u00b7f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Ist bis anher noch nicht ins Schlafgemach gedrungen,", "tokens": ["Ist", "bis", "an\u00b7her", "noch", "nicht", "ins", "Schlaf\u00b7ge\u00b7mach", "ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "PTKNEG", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Weil man darinnen nichts, als K\u00fcsse schallen h\u00f6rt.", "tokens": ["Weil", "man", "da\u00b7rin\u00b7nen", "nichts", ",", "als", "K\u00fcs\u00b7se", "schal\u00b7len", "h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PIS", "$,", "KOUS", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Die Liebe bettet mich nunmehr auf eitel Rosen,", "tokens": ["Die", "Lie\u00b7be", "bet\u00b7tet", "mich", "nun\u00b7mehr", "auf", "ei\u00b7tel", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Mich, die ich ehemals auf Dorn und Ketten schlief.", "tokens": ["Mich", ",", "die", "ich", "e\u00b7he\u00b7mals", "auf", "Dorn", "und", "Ket\u00b7ten", "schlief", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Das Gl\u00fcck vermochte mir nicht sch\u00f6ner lieb zu kosen,", "tokens": ["Das", "Gl\u00fcck", "ver\u00b7moch\u00b7te", "mir", "nicht", "sch\u00f6\u00b7ner", "lieb", "zu", "ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "ADJD", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Al\u00df da\u00df es mich zum Schoo\u00df von dem Don Pedro rief.", "tokens": ["Al\u00df", "da\u00df", "es", "mich", "zum", "Schoo\u00df", "von", "dem", "Don", "Ped\u00b7ro", "rief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "PRF", "APPRART", "NN", "APPR", "ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.57": {"text": "Mit ihm ist Reich und Land, und ich auch sehr zu frieden;", "tokens": ["Mit", "ihm", "ist", "Reich", "und", "Land", ",", "und", "ich", "auch", "sehr", "zu", "frie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "NN", "KON", "NN", "$,", "KON", "PPER", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Das Schicksal, das mich sonst mit H\u00fclsen nur gespeist,", "tokens": ["Das", "Schick\u00b7sal", ",", "das", "mich", "sonst", "mit", "H\u00fcl\u00b7sen", "nur", "ge\u00b7speist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Hat mir nunmehr daf\u00fcr die sch\u00f6nste Kost beschieden,", "tokens": ["Hat", "mir", "nun\u00b7mehr", "da\u00b7f\u00fcr", "die", "sch\u00f6ns\u00b7te", "Kost", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PAV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Die noch weit schmackbarer, als s\u00fcsse Datteln hei\u00dft.", "tokens": ["Die", "noch", "weit", "schmack\u00b7ba\u00b7rer", ",", "als", "s\u00fcs\u00b7se", "Dat\u00b7teln", "hei\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ADJA", "$,", "KOUS", "ADJA", "NN", "VVFIN", "$."], "meter": "-+++-+-+-+-+", "measure": "unknown.measure.septa"}, "line.61": {"text": "Algarbien, das uns vertraut sieht scherzen, lachen,", "tokens": ["Al\u00b7gar\u00b7bi\u00b7en", ",", "das", "uns", "ver\u00b7traut", "sieht", "scher\u00b7zen", ",", "la\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "VVINF", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Will sich schon zum voraus was gutes prophezeyn.", "tokens": ["Will", "sich", "schon", "zum", "vo\u00b7raus", "was", "gu\u00b7tes", "pro\u00b7phe\u00b7zeyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADV", "APPRART", "ADV", "PWS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Und dieses kann sich auch die sichre Rechnung machen,", "tokens": ["Und", "die\u00b7ses", "kann", "sich", "auch", "die", "sich\u00b7re", "Rech\u00b7nung", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "PRF", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Da\u00df seine Sehnsucht nicht vergebens werde seyn.", "tokens": ["Da\u00df", "sei\u00b7ne", "Sehn\u00b7sucht", "nicht", "ver\u00b7ge\u00b7bens", "wer\u00b7de", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "ADV", "VAFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Wo Lieb und Einigkeit um Thron und Lager schweben,", "tokens": ["Wo", "Lieb", "und", "Ei\u00b7nig\u00b7keit", "um", "Thron", "und", "La\u00b7ger", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Speist sich die Hoffnung nie mit lerer Phantasey,", "tokens": ["Speist", "sich", "die", "Hoff\u00b7nung", "nie", "mit", "le\u00b7rer", "Phan\u00b7ta\u00b7sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Gnug, da\u00df ich nun der Welt und dir kann Nachricht geben,", "tokens": ["Gnug", ",", "da\u00df", "ich", "nun", "der", "Welt", "und", "dir", "kann", "Nach\u00b7richt", "ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "KON", "PPER", "VMFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Was f\u00fcr ein Unterschied oft zwischen Br\u00fcdern sey.", "tokens": ["Was", "f\u00fcr", "ein", "Un\u00b7ter\u00b7schied", "oft", "zwi\u00b7schen", "Br\u00fc\u00b7dern", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ADV", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}