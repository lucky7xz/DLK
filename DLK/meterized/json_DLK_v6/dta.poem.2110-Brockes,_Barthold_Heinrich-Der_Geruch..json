{"dta.poem.2110": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Geruch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Blumen Balsam zu genie\u00dfen,", "tokens": ["Der", "Blu\u00b7men", "Bal\u00b7sam", "zu", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Riecht unsre Seele kr\u00e4ftiger, wenn wir im Ruch", "tokens": ["Riecht", "uns\u00b7re", "See\u00b7le", "kr\u00e4f\u00b7ti\u00b7ger", ",", "wenn", "wir", "im", "Ruch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die Augen schlie\u00dfen.", "tokens": ["die", "Au\u00b7gen", "schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Sie schlie\u00dfen sich auch von sich selbst, vermuthlich da\u00df", "tokens": ["Sie", "schlie\u00b7\u00dfen", "sich", "auch", "von", "sich", "selbst", ",", "ver\u00b7muth\u00b7lich", "da\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "APPR", "PRF", "ADV", "$,", "ADV", "KOUS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "wir unser Denken,", "tokens": ["wir", "un\u00b7ser", "Den\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Von Gegenw\u00fcrfen nicht zerstreut, mehr auf die Lust im", "tokens": ["Von", "Ge\u00b7gen\u00b7w\u00fcr\u00b7fen", "nicht", "zer\u00b7streut", ",", "mehr", "auf", "die", "Lust", "im"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PTKNEG", "VVPP", "$,", "ADV", "APPR", "ART", "NN", "APPRART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Riechen lenken,", "tokens": ["Rie\u00b7chen", "len\u00b7ken", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Uns mehr daran ergetzen sollen. Ich seh die\u00df, als ein", "tokens": ["Uns", "mehr", "da\u00b7ran", "er\u00b7get\u00b7zen", "sol\u00b7len", ".", "Ich", "seh", "die\u00df", ",", "als", "ein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "PAV", "VVINF", "VMFIN", "$.", "PPER", "VVFIN", "PDS", "$,", "KOUS", "ART"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Wunder, an,", "tokens": ["Wun\u00b7der", ",", "an", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "So sonder Absicht nicht gewirkt, und danke Gott, da\u00df ich", "tokens": ["So", "son\u00b7der", "Ab\u00b7sicht", "nicht", "ge\u00b7wirkt", ",", "und", "dan\u00b7ke", "Gott", ",", "da\u00df", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJA", "NN", "PTKNEG", "VVPP", "$,", "KON", "VVFIN", "NN", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.11": {"text": "die Gabe", "tokens": ["die", "Ga\u00b7be"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Bedachtsamlich zu riechen habe,", "tokens": ["Be\u00b7dacht\u00b7sam\u00b7lich", "zu", "rie\u00b7chen", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Da\u00df ich so dann die Augen schlie\u00dfen, und sie schnell wieder", "tokens": ["Da\u00df", "ich", "so", "dann", "die", "Au\u00b7gen", "schlie\u00b7\u00dfen", ",", "und", "sie", "schnell", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ART", "NN", "VVINF", "$,", "KON", "PPER", "ADJD", "ADV"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "\u00f6ffnen kann.", "tokens": ["\u00f6ff\u00b7nen", "kann", "."], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}