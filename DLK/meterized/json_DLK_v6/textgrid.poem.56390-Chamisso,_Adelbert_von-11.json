{"textgrid.poem.56390": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "11", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbhab das Gesch\u00e4ft wohl", "tokens": ["\u00bb", "hab", "das", "Ge\u00b7sch\u00e4ft", "wohl"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "VAFIN", "ART", "NN", "ADV"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Geschafft mit der Arbeit.", "tokens": ["Ge\u00b7schafft", "mit", "der", "Ar\u00b7beit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Thrym hat den Hammer,", "tokens": ["Th\u00b7rym", "hat", "den", "Ham\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Der Herrscher der Riesen,", "tokens": ["Der", "Herr\u00b7scher", "der", "Rie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und wieder erwerben,", "tokens": ["Und", "wie\u00b7der", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "F\u00fcrwahr, soll ihn keiner,", "tokens": ["F\u00fcr\u00b7wahr", ",", "soll", "ihn", "kei\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPER", "PIS", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Er f\u00fchre denn Fraya", "tokens": ["Er", "f\u00fch\u00b7re", "denn", "Fray\u00b7a"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "NE"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Zur Frau ihm heim.\u00ab", "tokens": ["Zur", "Frau", "ihm", "heim", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}}}}