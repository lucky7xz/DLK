{"dta.poem.8198": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Waldweg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Wo blaue Schleh'n im tiefen Grase standen;", "tokens": ["Wo", "blau\u00b7e", "Schleh'n", "im", "tie\u00b7fen", "Gra\u00b7se", "stan\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann durch die Hecke \u00fcber schmalen Steg", "tokens": ["Dann", "durch", "die", "He\u00b7cke", "\u00fc\u00b7ber", "schma\u00b7len", "Steg"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf eine Wiese, die an allen Randen", "tokens": ["Auf", "ei\u00b7ne", "Wie\u00b7se", ",", "die", "an", "al\u00b7len", "Ran\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein hoher Zaun vielfarbgen Laub's umzog;", "tokens": ["Ein", "ho\u00b7her", "Zaun", "viel\u00b7farb\u00b7gen", "Laub's", "um\u00b7zog", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Buscheichen unter wilden Rosenb\u00fcschen,", "tokens": ["Bu\u00b7schei\u00b7chen", "un\u00b7ter", "wil\u00b7den", "Ro\u00b7sen\u00b7b\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Um die sich frei die Gei\u00dfblattranke bog,", "tokens": ["Um", "die", "sich", "frei", "die", "Gei\u00df\u00b7blatt\u00b7ran\u00b7ke", "bog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "PRF", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Brombeergewirr und H\u00fclsendorn dazwischen;", "tokens": ["Brom\u00b7beer\u00b7ge\u00b7wirr", "und", "H\u00fcl\u00b7sen\u00b7dorn", "da\u00b7zwi\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PAV", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "Vorbei an Farrenkr\u00e4utern wob der Eppich", "tokens": ["Vor\u00b7bei", "an", "Far\u00b7ren\u00b7kr\u00e4u\u00b7tern", "wob", "der", "Ep\u00b7pich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "Entlang des Walles seinen dunklen Teppich.", "tokens": ["Ent\u00b7lang", "des", "Wal\u00b7les", "sei\u00b7nen", "dunk\u00b7len", "Tep\u00b7pich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und vorw\u00e4rts schreitend st\u00f6rte bald mein Tritt", "tokens": ["Und", "vor\u00b7w\u00e4rts", "schrei\u00b7tend", "st\u00f6r\u00b7te", "bald", "mein", "Tritt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die Biene auf, die um die Distel schw\u00e4rmte,", "tokens": ["Die", "Bie\u00b7ne", "auf", ",", "die", "um", "die", "Dis\u00b7tel", "schw\u00e4rm\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Bald h\u00f6rte ich, wie durch die Gr\u00e4ser glitt", "tokens": ["Bald", "h\u00f6r\u00b7te", "ich", ",", "wie", "durch", "die", "Gr\u00e4\u00b7ser", "glitt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Die Schlange, die am Sonnenstrahl sich w\u00e4rmte.", "tokens": ["Die", "Schlan\u00b7ge", ",", "die", "am", "Son\u00b7nen\u00b7strahl", "sich", "w\u00e4rm\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Sonst war es kirchenstill in alle Weite,", "tokens": ["Sonst", "war", "es", "kir\u00b7chen\u00b7still", "in", "al\u00b7le", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Kein Vogel h\u00f6rbar; nur an meiner Seite", "tokens": ["Kein", "Vo\u00b7gel", "h\u00f6r\u00b7bar", ";", "nur", "an", "mei\u00b7ner", "Sei\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADJD", "$.", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Sprang schnaufend ab und zu des Oheims Hund;", "tokens": ["Sprang", "schnau\u00b7fend", "ab", "und", "zu", "des", "O\u00b7heims", "Hund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "KON", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Denn nicht allein w\u00e4r' ich um solche Zeit", "tokens": ["Denn", "nicht", "al\u00b7lein", "w\u00e4r'", "ich", "um", "sol\u00b7che", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ADV", "VAFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Gegangen zum entlegnen Waldesgrund;", "tokens": ["Ge\u00b7gan\u00b7gen", "zum", "ent\u00b7leg\u00b7nen", "Wal\u00b7des\u00b7grund", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Mir graute vor der Mittagseinsamkeit. \u2014", "tokens": ["Mir", "grau\u00b7te", "vor", "der", "Mit\u00b7ta\u00b7gsein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Hei\u00df war die Luft, und alle Winde schliefen;", "tokens": ["Hei\u00df", "war", "die", "Luft", ",", "und", "al\u00b7le", "Win\u00b7de", "schlie\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Und vor mir lag ein sonnig offner Raum,", "tokens": ["Und", "vor", "mir", "lag", "ein", "son\u00b7nig", "off\u00b7ner", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Wo quer hindurch schutzlos die Steige liefen.", "tokens": ["Wo", "quer", "hin\u00b7durch", "schutz\u00b7los", "die", "Stei\u00b7ge", "lie\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PAV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Wohl hatt' ich's sauer und ertrug es kaum;", "tokens": ["Wohl", "hatt'", "ich's", "sau\u00b7er", "und", "er\u00b7trug", "es", "kaum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Doch rascher schreitend \u00fcberwand ich's bald.", "tokens": ["Doch", "ra\u00b7scher", "schrei\u00b7tend", "\u00fc\u00b7ber\u00b7wand", "ich's", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Dann war ein Bach, ein Wall zu \u00fcberspringen,", "tokens": ["Dann", "war", "ein", "Bach", ",", "ein", "Wall", "zu", "\u00fc\u00b7bers\u00b7prin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Dann noch ein Steg, und vor mir lag der Wald,", "tokens": ["Dann", "noch", "ein", "Steg", ",", "und", "vor", "mir", "lag", "der", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "KON", "APPR", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "In dem schon herbstlich roth die Bl\u00e4tter hingen.", "tokens": ["In", "dem", "schon", "herbst\u00b7lich", "roth", "die", "Bl\u00e4t\u00b7ter", "hin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJD", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Und dr\u00fcber her, hoch in der blauen Luft,", "tokens": ["Und", "dr\u00fc\u00b7ber", "her", ",", "hoch", "in", "der", "blau\u00b7en", "Luft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "PTKVZ", "$,", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.29": {"text": "Stand beutes\u00fcchtig ein gewaltger Weih',", "tokens": ["Stand", "beu\u00b7te\u00b7s\u00fcch\u00b7tig", "ein", "ge\u00b7walt\u00b7ger", "Weih'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Die Fl\u00fcgel schlagend durch den Sonnenduft;", "tokens": ["Die", "Fl\u00fc\u00b7gel", "schla\u00b7gend", "durch", "den", "Son\u00b7nen\u00b7duft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Tief aus der Holzung scholl des H\u00e4hers Schrei.", "tokens": ["Tief", "aus", "der", "Hol\u00b7zung", "scholl", "des", "H\u00e4\u00b7hers", "Schrei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJD", "ART", "NN", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.32": {"text": "Himbeerenduft und Tannenharzgeruch", "tokens": ["Him\u00b7bee\u00b7ren\u00b7duft", "und", "Tan\u00b7nen\u00b7harz\u00b7ge\u00b7ruch"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.33": {"text": "Quoll mir entgegen schon auf meinem Wege,", "tokens": ["Quoll", "mir", "ent\u00b7ge\u00b7gen", "schon", "auf", "mei\u00b7nem", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.34": {"text": "Und dort im Walle schimmerte der Bruch,", "tokens": ["Und", "dort", "im", "Wal\u00b7le", "schim\u00b7mer\u00b7te", "der", "Bruch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Durch den ich meinen Pfad nahm in's Gehege.", "tokens": ["Durch", "den", "ich", "mei\u00b7nen", "Pfad", "nahm", "in's", "Ge\u00b7he\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.36": {"text": "Schon streckten dort gleich S\u00e4ulen der Kapelle", "tokens": ["Schon", "streck\u00b7ten", "dort", "gleich", "S\u00e4u\u00b7len", "der", "Ka\u00b7pel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "An's Laubgew\u00f6lb' die Tannenst\u00e4mme sich;", "tokens": ["An's", "Laub\u00b7ge\u00b7w\u00f6lb'", "die", "Tan\u00b7nen\u00b7st\u00e4m\u00b7me", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Dann war's erreicht, und wie an Kirchenschwelle", "tokens": ["Dann", "wa\u00b7r's", "er\u00b7reicht", ",", "und", "wie", "an", "Kir\u00b7chen\u00b7schwel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "VVPP", "$,", "KON", "PWAV", "APPR", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.39": {"text": "Umschauerte die Schattenk\u00fchle mich.", "tokens": ["Um\u00b7schau\u00b7er\u00b7te", "die", "Schat\u00b7ten\u00b7k\u00fch\u00b7le", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}