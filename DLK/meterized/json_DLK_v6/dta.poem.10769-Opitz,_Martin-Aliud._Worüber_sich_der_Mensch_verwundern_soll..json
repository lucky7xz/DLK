{"dta.poem.10769": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Aliud.  \n  Wor\u00fcber sich der Mensch verwundern soll.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Was bistu doch/ o Mensch/ gegen der gr\u00f6\u00df der Erden?", "tokens": ["Was", "bis\u00b7tu", "doch", "/", "o", "Mensch", "/", "ge\u00b7gen", "der", "gr\u00f6\u00df", "der", "Er\u00b7den", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "$(", "FM", "NN", "$(", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ist der Erden gr\u00f6\u00df gegen de\u00df Himmels pracht?", "tokens": ["Was", "ist", "der", "Er\u00b7den", "gr\u00f6\u00df", "ge\u00b7gen", "de\u00df", "Him\u00b7mels", "pracht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was ist de\u00df Himmels pracht gegen de\u00df Sch\u00f6pffers Macht:", "tokens": ["Was", "ist", "de\u00df", "Him\u00b7mels", "pracht", "ge\u00b7gen", "de\u00df", "Sch\u00f6pf\u00b7fers", "Macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch welche du vnd sie vnd er regieret werden?", "tokens": ["Durch", "wel\u00b7che", "du", "vnd", "sie", "vnd", "er", "re\u00b7gie\u00b7ret", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "KON", "PPER", "KON", "PPER", "VVFIN", "VAINF", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Darumb soltu/ o Mensch/ dich vber dich selbst schwingen/", "tokens": ["Da\u00b7rumb", "sol\u00b7tu", "/", "o", "Mensch", "/", "dich", "vber", "dich", "selbst", "schwin\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "$(", "FM", "NN", "$(", "PPER", "APPR", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Dir lassen an dich selbst kein wunder wunder sein/", "tokens": ["Dir", "las\u00b7sen", "an", "dich", "selbst", "kein", "wun\u00b7der", "wun\u00b7der", "sein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "APPR", "PPER", "ADV", "PIAT", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Verwundern soltu dich vber die ding allein", "tokens": ["Ver\u00b7wun\u00b7dern", "sol\u00b7tu", "dich", "vber", "die", "ding", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PRF", "APPR", "ART", "NN", "ADV"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "In Gott vnd vber. Gott allein in allen dingen.", "tokens": ["In", "Gott", "vnd", "vber", ".", "Gott", "al\u00b7lein", "in", "al\u00b7len", "din\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "$.", "NN", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}