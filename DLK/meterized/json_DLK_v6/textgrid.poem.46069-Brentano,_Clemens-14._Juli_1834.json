{"textgrid.poem.46069": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "14. Juli 1834", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df wohl, was dich bannt in mir,", "tokens": ["Ich", "wei\u00df", "wohl", ",", "was", "dich", "bannt", "in", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWS", "PPER", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Lebensglut in meiner Brust,", "tokens": ["Die", "Le\u00b7bens\u00b7glut", "in", "mei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die s\u00fc\u00dfe zauberhafte Zier,", "tokens": ["Die", "s\u00fc\u00b7\u00dfe", "zau\u00b7ber\u00b7haf\u00b7te", "Zier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der bangen tiefgeheimen Lust,", "tokens": ["Der", "ban\u00b7gen", "tief\u00b7ge\u00b7hei\u00b7men", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die aus mir strahlet, ruft zu dir,", "tokens": ["Die", "aus", "mir", "strah\u00b7let", ",", "ruft", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$,", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schlie\u00df mich in einen Felsen ein,", "tokens": ["Schlie\u00df", "mich", "in", "ei\u00b7nen", "Fel\u00b7sen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ruft doch arm Lind durch Mark und Bein:", "tokens": ["Ruft", "doch", "arm", "Lind", "durch", "Mark", "und", "Bein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Komm, lebe, liebe, stirb an mir,", "tokens": ["Komm", ",", "le\u00b7be", ",", "lie\u00b7be", ",", "stirb", "an", "mir", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Leg' dir diesen Fels auf deine Brust,", "tokens": ["Leg'", "dir", "die\u00b7sen", "Fels", "auf", "dei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PDAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Du mu\u00dft, mu\u00dft.", "tokens": ["Du", "mu\u00dft", ",", "mu\u00dft", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "VMFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}