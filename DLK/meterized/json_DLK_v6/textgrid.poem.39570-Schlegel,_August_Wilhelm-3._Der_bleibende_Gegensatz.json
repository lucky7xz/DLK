{"textgrid.poem.39570": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "3. Der bleibende Gegensatz", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie dachten die Naturen auszuwechseln,", "tokens": ["Sie", "dach\u00b7ten", "die", "Na\u00b7tu\u00b7ren", "aus\u00b7zu\u00b7wech\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wechselten nur fruchtlos manchen Brief.", "tokens": ["Und", "wech\u00b7sel\u00b7ten", "nur", "frucht\u00b7los", "man\u00b7chen", "Brief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Originales will der Eine k\u00fcnstlich drechseln;", "tokens": ["O\u00b7rig\u00b7i\u00b7na\u00b7les", "will", "der", "Ei\u00b7ne", "k\u00fcnst\u00b7lich", "drech\u00b7seln", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "ART", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Andre spinnewebt speculativ.", "tokens": ["Der", "And\u00b7re", "spin\u00b7ne\u00b7webt", "spe\u00b7cu\u00b7la\u00b7tiv", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Kaum kennt man noch den Zauberer der Geister,", "tokens": ["Kaum", "kennt", "man", "noch", "den", "Zau\u00b7be\u00b7rer", "der", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn er bei'm Gr\u00fcbler dort in dumpfer Kammer haust.", "tokens": ["Wenn", "er", "bei'm", "Gr\u00fcb\u00b7ler", "dort", "in", "dum\u00b7pfer", "Kam\u00b7mer", "haust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch jeder bleibt er selbst: der Famulus, der Meister,", "tokens": ["Doch", "je\u00b7der", "bleibt", "er", "selbst", ":", "der", "Fa\u00b7mu\u00b7lus", ",", "der", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "ADV", "$.", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der bla\u00dfe Wagner und der kr\u00e4ft'ge Faust.", "tokens": ["Der", "bla\u00b7\u00dfe", "Wag\u00b7ner", "und", "der", "kr\u00e4ft'\u00b7ge", "Faust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}