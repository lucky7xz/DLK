{"dta.poem.11923": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "84.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dort in der Sonne steht, dir ungesehn, ein Geist,", "tokens": ["Dort", "in", "der", "Son\u00b7ne", "steht", ",", "dir", "un\u00b7ge\u00b7sehn", ",", "ein", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,", "PPER", "ADJD", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von dessen Blick gelenkt, um ihn die Sch\u00f6pfung kreist.", "tokens": ["Von", "des\u00b7sen", "Blick", "ge\u00b7lenkt", ",", "um", "ihn", "die", "Sch\u00f6p\u00b7fung", "kreist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "VVPP", "$,", "KOUI", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du f\u00fchlest seinen Blick, der dir das Auge f\u00fcllt!", "tokens": ["Du", "f\u00fch\u00b7lest", "sei\u00b7nen", "Blick", ",", "der", "dir", "das", "Au\u00b7ge", "f\u00fcllt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihn siehst du nicht, den dir sein eigner Glanz verh\u00fcllt.", "tokens": ["Ihn", "siehst", "du", "nicht", ",", "den", "dir", "sein", "eig\u00b7ner", "Glanz", "ver\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PRELS", "PPER", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du sehnest dich empor in seinem Glanz zu gehn,", "tokens": ["Du", "seh\u00b7nest", "dich", "em\u00b7por", "in", "sei\u00b7nem", "Glanz", "zu", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit ihm vereinigt dort im Mittelpunkt zu stehn.", "tokens": ["Mit", "ihm", "ver\u00b7ei\u00b7nigt", "dort", "im", "Mit\u00b7tel\u00b7punkt", "zu", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADV", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Vom Mittelpunkte dort zu schauen frohbewu\u00dft", "tokens": ["Vom", "Mit\u00b7tel\u00b7punk\u00b7te", "dort", "zu", "schau\u00b7en", "froh\u00b7be\u00b7wu\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "PTKZU", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit gradem Blick, was hier du schaun mit schiefem mu\u00dft.", "tokens": ["Mit", "gra\u00b7dem", "Blick", ",", "was", "hier", "du", "schaun", "mit", "schie\u00b7fem", "mu\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "ADV", "PPER", "VVFIN", "APPR", "ADJA", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Des wohlgeordneten Planetentanzes Spiel,", "tokens": ["Des", "wohl\u00b7ge\u00b7ord\u00b7ne\u00b7ten", "Pla\u00b7ne\u00b7ten\u00b7tan\u00b7zes", "Spiel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In dem der Sonnengeist wirkt und erkennt sein Ziel.", "tokens": ["In", "dem", "der", "Son\u00b7nen\u00b7geist", "wirkt", "und", "er\u00b7kennt", "sein", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Er stralt von Wonn' und ist von Sch\u00f6pferlust bewegt,", "tokens": ["Er", "stralt", "von", "Wonn'", "und", "ist", "von", "Sch\u00f6p\u00b7fer\u00b7lust", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "VAFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie er mit seinem Blick sein Weltgetrieb erregt.", "tokens": ["Wie", "er", "mit", "sei\u00b7nem", "Blick", "sein", "Welt\u00b7ge\u00b7trieb", "er\u00b7regt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Doch sieh, nun blicket er aus seinem Dienerchor", "tokens": ["Doch", "sieh", ",", "nun", "bli\u00b7cket", "er", "aus", "sei\u00b7nem", "Die\u00b7ner\u00b7chor"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Umkreis h\u00f6her auf, wie du zu ihm empor.", "tokens": ["Vom", "Um\u00b7kreis", "h\u00f6\u00b7her", "auf", ",", "wie", "du", "zu", "ihm", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "PTKVZ", "$,", "PWAV", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Und selber sieht er sich an h\u00f6herm Sonnenband,", "tokens": ["Und", "sel\u00b7ber", "sieht", "er", "sich", "an", "h\u00f6\u00b7herm", "Son\u00b7nen\u00b7band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fchlt sich, dem Mittelpunkt entr\u00fcckt, wie du, am Rand.", "tokens": ["F\u00fchlt", "sich", ",", "dem", "Mit\u00b7tel\u00b7punkt", "ent\u00b7r\u00fcckt", ",", "wie", "du", ",", "am", "Rand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "ART", "NN", "VVPP", "$,", "PWAV", "PPER", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Das aber l\u00e4hmt ihn nicht, und tr\u00fcbt nicht seinen Glanz;", "tokens": ["Das", "a\u00b7ber", "l\u00e4hmt", "ihn", "nicht", ",", "und", "tr\u00fcbt", "nicht", "sei\u00b7nen", "Glanz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "PPER", "PTKNEG", "$,", "KON", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erst als des Ganzen Glied f\u00fchlt er sich selber ganz.", "tokens": ["Erst", "als", "des", "Gan\u00b7zen", "Glied", "f\u00fchlt", "er", "sich", "sel\u00b7ber", "ganz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "NN", "VVFIN", "PPER", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}}, "stanza.10": {"line.1": {"text": "In seinem Kreis mit Lust wirkt er durch h\u00f6h're Kraft;", "tokens": ["In", "sei\u00b7nem", "Kreis", "mit", "Lust", "wirkt", "er", "durch", "h\u00f6h'\u00b7re", "Kraft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und also wirke du in deinem sonnenhaft.", "tokens": ["Und", "al\u00b7so", "wir\u00b7ke", "du", "in", "dei\u00b7nem", "son\u00b7nen\u00b7haft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Wo du in Gott dich f\u00fchlst, stehst du im Mittelpunkt;", "tokens": ["Wo", "du", "in", "Gott", "dich", "f\u00fchlst", ",", "stehst", "du", "im", "Mit\u00b7tel\u00b7punkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wo du ihn verlierst, bist du ins All zerfunkt.", "tokens": ["Und", "wo", "du", "ihn", "ver\u00b7lierst", ",", "bist", "du", "ins", "All", "zer\u00b7funkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}