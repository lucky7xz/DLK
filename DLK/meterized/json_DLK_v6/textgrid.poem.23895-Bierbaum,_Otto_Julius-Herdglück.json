{"textgrid.poem.23895": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Herdgl\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Gl\u00fcck weht zwei zusammen,", "tokens": ["Das", "Gl\u00fcck", "weht", "zwei", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "CARD", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der weiche, warme Wind,", "tokens": ["Der", "wei\u00b7che", ",", "war\u00b7me", "Wind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df wie des Herdes Flammen", "tokens": ["Da\u00df", "wie", "des", "Her\u00b7des", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie gluteneinig sind.", "tokens": ["Sie", "glu\u00b7ten\u00b7ei\u00b7nig", "sind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die St\u00fcrme heulen im Schlote;", "tokens": ["Die", "St\u00fcr\u00b7me", "heu\u00b7len", "im", "Schlo\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "La\u00df heulen den gieren Braus!", "tokens": ["La\u00df", "heu\u00b7len", "den", "gie\u00b7ren", "Braus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Herdfeuer glimmt, das rote,", "tokens": ["Herd\u00b7feu\u00b7er", "glimmt", ",", "das", "ro\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und nimmer l\u00f6scht es aus.", "tokens": ["Und", "nim\u00b7mer", "l\u00f6scht", "es", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}