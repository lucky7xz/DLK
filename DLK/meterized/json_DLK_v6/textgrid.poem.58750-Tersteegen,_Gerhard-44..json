{"textgrid.poem.58750": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "44.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df sie vollkommen seien in eins. Joh. 17, 23", "tokens": ["Da\u00df", "sie", "voll\u00b7kom\u00b7men", "sei\u00b7en", "in", "eins", ".", "Joh", ".", "17", ",", "23"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "number", "punct", "number"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "APPR", "PIS", "$.", "NE", "$.", "CARD", "$,", "CARD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der reinen Liebe Ziel ist, mit Gott eines werden,", "tokens": ["Der", "rei\u00b7nen", "Lie\u00b7be", "Ziel", "ist", ",", "mit", "Gott", "ei\u00b7nes", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "$,", "APPR", "NN", "ART", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Liebe f\u00fchrt dahin durch Wege voll Beschwerden,", "tokens": ["Die", "Lie\u00b7be", "f\u00fchrt", "da\u00b7hin", "durch", "We\u00b7ge", "voll", "Be\u00b7schwer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "APPR", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch Leiden, Kreuz und Schand', gar nicht durch Ruhm und Ehr':", "tokens": ["Durch", "Lei\u00b7den", ",", "Kreuz", "und", "Schand'", ",", "gar", "nicht", "durch", "Ruhm", "und", "Ehr'", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,", "ADV", "PTKNEG", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dies ist der heil'ge Weg, was will man denn noch mehr?", "tokens": ["Dies", "ist", "der", "heil'\u00b7ge", "Weg", ",", "was", "will", "man", "denn", "noch", "mehr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "PWS", "VMFIN", "PIS", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Allein Gott! Eine Lieb' macht alles eines sein;", "tokens": ["Al\u00b7lein", "Gott", "!", "Ei\u00b7ne", "Lieb'", "macht", "al\u00b7les", "ei\u00b7nes", "sein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$.", "ART", "NN", "VVFIN", "PIS", "PIS", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das h\u00f6chste Gut ist in den einen Punkt geleget.", "tokens": ["Das", "h\u00f6chs\u00b7te", "Gut", "ist", "in", "den", "ei\u00b7nen", "Punkt", "ge\u00b7le\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "ART", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Liebe f\u00fchrt uns in den eignen Ursprung ein,", "tokens": ["Die", "Lie\u00b7be", "f\u00fchrt", "uns", "in", "den", "eig\u00b7nen", "Ur\u00b7sprung", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da unser Anfang, Gott, uns tr\u00e4get und beweget.", "tokens": ["Da", "un\u00b7ser", "An\u00b7fang", ",", "Gott", ",", "uns", "tr\u00e4\u00b7get", "und", "be\u00b7we\u00b7get", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "NN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Erhab'ne Einheit Gott's, worin die Braut dann stehet!", "tokens": ["Er\u00b7hab'\u00b7ne", "Ein\u00b7heit", "Gott's", ",", "wo\u00b7rin", "die", "Braut", "dann", "ste\u00b7het", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "$,", "PWAV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zuletzt ein g\u00f6ttliches Vermengen vor sich gehet.", "tokens": ["Zu\u00b7letzt", "ein", "g\u00f6tt\u00b7li\u00b7ches", "Ver\u00b7men\u00b7gen", "vor", "sich", "ge\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Seel' hat nichts und ist vergn\u00fcgt und fest im Mut,", "tokens": ["Die", "Seel'", "hat", "nichts", "und", "ist", "ver\u00b7gn\u00fcgt", "und", "fest", "im", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "KON", "VAFIN", "VVPP", "KON", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verwandelt durch die Lieb' in ihrem h\u00f6chsten Gut.", "tokens": ["Ver\u00b7wan\u00b7delt", "durch", "die", "Lieb'", "in", "ih\u00b7rem", "h\u00f6chs\u00b7ten", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Man sieht nicht, da\u00df die Braut, die liebste, mehr erscheine,", "tokens": ["Man", "sieht", "nicht", ",", "da\u00df", "die", "Braut", ",", "die", "liebs\u00b7te", ",", "mehr", "er\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "$,", "KOUS", "ART", "NN", "$,", "ART", "ADJA", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es wirket in ihr Gott alleine,", "tokens": ["Es", "wir\u00b7ket", "in", "ihr", "Gott", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In ihrer Einheit sie vernichtigt ist so sehr,", "tokens": ["In", "ih\u00b7rer", "Ein\u00b7heit", "sie", "ver\u00b7nich\u00b7tigt", "ist", "so", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVPP", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Lieb', Wahrheit sieht man noch und weiter sonst nicht mehr.", "tokens": ["Lieb'", ",", "Wahr\u00b7heit", "sieht", "man", "noch", "und", "wei\u00b7ter", "sonst", "nicht", "mehr", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVFIN", "PIS", "ADV", "KON", "ADV", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}