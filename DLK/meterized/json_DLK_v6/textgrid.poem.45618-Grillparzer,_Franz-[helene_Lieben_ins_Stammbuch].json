{"textgrid.poem.45618": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[helene Lieben ins Stammbuch]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast mich gemalt,", "tokens": ["Du", "hast", "mich", "ge\u00b7malt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Ich habe bezahlt", "tokens": ["Ich", "ha\u00b7be", "be\u00b7zahlt"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Durch Sitzen still und geduldig.", "tokens": ["Durch", "Sit\u00b7zen", "still", "und", "ge\u00b7dul\u00b7dig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "KON", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Doch rechnet man zu und ab,", "tokens": ["Doch", "rech\u00b7net", "man", "zu", "und", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Was eins und das andre gab,", "tokens": ["Was", "eins", "und", "das", "and\u00b7re", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "KON", "ART", "PIS", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "So bleib ich betr\u00e4chtlich schuldig.", "tokens": ["So", "bleib", "ich", "be\u00b7tr\u00e4cht\u00b7lich", "schul\u00b7dig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}