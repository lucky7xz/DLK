{"dta.poem.9875": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf den nahmens-tag herrn J. W.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geehrter/ dieser schein/ der heute dich begl\u00fccket/", "tokens": ["Ge\u00b7ehr\u00b7ter", "/", "die\u00b7ser", "schein", "/", "der", "heu\u00b7te", "dich", "be\u00b7gl\u00fc\u00b7cket", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PDAT", "NN", "$(", "ART", "ADV", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Strahlt unser hertzen auch mit frischen blicken an/", "tokens": ["Strahlt", "un\u00b7ser", "hert\u00b7zen", "auch", "mit", "fri\u00b7schen", "bli\u00b7cken", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ADJA", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und zeuget/ wie die lust/ die deine brust entz\u00fccket/", "tokens": ["Und", "zeu\u00b7get", "/", "wie", "die", "lust", "/", "die", "dei\u00b7ne", "brust", "ent\u00b7z\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOKOM", "ART", "NN", "$(", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auch freunde durch den glantz zugleich entflammen kan", "tokens": ["Auch", "freun\u00b7de", "durch", "den", "glantz", "zu\u00b7gleich", "ent\u00b7flam\u00b7men", "kan"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "APPR", "ART", "NN", "ADV", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wir f\u00fchlen jeden ku\u00df/ den dir der himmel reichet/", "tokens": ["Wir", "f\u00fch\u00b7len", "je\u00b7den", "ku\u00df", "/", "den", "dir", "der", "him\u00b7mel", "rei\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$(", "ART", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wir schmecken auch den safft beperlter liebligkeit/", "tokens": ["Wir", "schme\u00b7cken", "auch", "den", "safft", "be\u00b7perl\u00b7ter", "lieb\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und lernen/ da er dich mit bisam \u00fcberstreichet/", "tokens": ["Und", "ler\u00b7nen", "/", "da", "er", "dich", "mit", "bi\u00b7sam", "\u00fc\u00b7bers\u00b7trei\u00b7chet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$(", "KOUS", "PPER", "PRF", "APPR", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df seine gunst auf uns canarizucker streut.", "tokens": ["Da\u00df", "sei\u00b7ne", "gunst", "auf", "uns", "ca\u00b7na\u00b7ri\u00b7zu\u00b7cker", "streut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So nim denn/ werther/ hin/ was unsre freude zollet/", "tokens": ["So", "nim", "denn", "/", "wert\u00b7her", "/", "hin", "/", "was", "uns\u00b7re", "freu\u00b7de", "zol\u00b7let", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "$(", "PWS", "$(", "PTKVZ", "$(", "PWS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein farben-armes blat/ das kein zinober deckt/", "tokens": ["Ein", "fa\u00b7rben\u00b7ar\u00b7mes", "blat", "/", "das", "kein", "zi\u00b7no\u00b7ber", "deckt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das unser aller hertz in einen zedel rollet/", "tokens": ["Das", "un\u00b7ser", "al\u00b7ler", "hertz", "in", "ei\u00b7nen", "ze\u00b7del", "rol\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "PIAT", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und mehr nach redligkeit/ als bunten pinseln schmeckt.", "tokens": ["Und", "mehr", "nach", "red\u00b7lig\u00b7keit", "/", "als", "bun\u00b7ten", "pin\u00b7seln", "schmeckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "$(", "KOUS", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der himmel kr\u00e4ntze dich mit gr\u00fcnen anmuths-zweigen/", "tokens": ["Der", "him\u00b7mel", "kr\u00e4nt\u00b7ze", "dich", "mit", "gr\u00fc\u00b7nen", "an\u00b7muths\u00b7zwei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und schliesse deine lust begn\u00fcgten armen ein;", "tokens": ["Und", "schlies\u00b7se", "dei\u00b7ne", "lust", "be\u00b7gn\u00fcg\u00b7ten", "ar\u00b7men", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADJA", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Er lasse steten ruhm aus deiner feder steigen/", "tokens": ["Er", "las\u00b7se", "ste\u00b7ten", "ruhm", "aus", "dei\u00b7ner", "fe\u00b7der", "stei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und dein bepalmtes lob der ehren wurtzel seyn.", "tokens": ["Und", "dein", "be\u00b7palm\u00b7tes", "lob", "der", "eh\u00b7ren", "wurt\u00b7zel", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Bi\u00df/ wenn dir alles wird mit vollen str\u00f6men fliessen/", "tokens": ["Bi\u00df", "/", "wenn", "dir", "al\u00b7les", "wird", "mit", "vol\u00b7len", "str\u00f6\u00b7men", "flies\u00b7sen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$(", "KOUS", "PPER", "PIS", "VAFIN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und s\u00fcden/ west und nord in tuberrosen stehn/", "tokens": ["Und", "s\u00fc\u00b7den", "/", "west", "und", "nord", "in", "tu\u00b7ber\u00b7ro\u00b7sen", "stehn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "VVFIN", "KON", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wir endlich deinen flei\u00df durch quellendes vers\u00fcssen", "tokens": ["Wir", "end\u00b7lich", "dei\u00b7nen", "flei\u00df", "durch", "quel\u00b7len\u00b7des", "ver\u00b7s\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "PPOSAT", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "In lorbeer-reicher pracht und tausend knospen sehn.", "tokens": ["In", "lor\u00b7beer\u00b7rei\u00b7cher", "pracht", "und", "tau\u00b7send", "knos\u00b7pen", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}