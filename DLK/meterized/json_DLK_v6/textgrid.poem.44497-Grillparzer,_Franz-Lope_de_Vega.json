{"textgrid.poem.44497": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Lope de Vega", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du reicher Geist mit unbekannten Sch\u00e4tzen,", "tokens": ["Du", "rei\u00b7cher", "Geist", "mit", "un\u00b7be\u00b7kann\u00b7ten", "Sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dir selber mehr als andern unbekannt,", "tokens": ["Dir", "sel\u00b7ber", "mehr", "als", "an\u00b7dern", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PIAT", "KOKOM", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Weil du nicht liebst, an Zahlen Zahl zu setzen,", "tokens": ["Weil", "du", "nicht", "liebst", ",", "an", "Zah\u00b7len", "Zahl", "zu", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "$,", "APPR", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nein, einzeln sie verschenkst mit voller Hand.", "tokens": ["Nein", ",", "ein\u00b7zeln", "sie", "ver\u00b7schenkst", "mit", "vol\u00b7ler", "Hand", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wo irgend Gold in unerforschten Kl\u00fcften,", "tokens": ["Wo", "ir\u00b7gend", "Gold", "in", "un\u00b7er\u00b7forschten", "Kl\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die W\u00fcnschelrute zeigt dir seine Spur;", "tokens": ["Die", "W\u00fcn\u00b7schel\u00b7ru\u00b7te", "zeigt", "dir", "sei\u00b7ne", "Spur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie deine Spanier, die gen Abend schifften,", "tokens": ["Wie", "dei\u00b7ne", "Spa\u00b7nier", ",", "die", "gen", "A\u00b7bend", "schiff\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Befuhrst du alle K\u00fcsten der Natur.", "tokens": ["Be\u00b7fuhrst", "du", "al\u00b7le", "K\u00fcs\u00b7ten", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und was an Menschen, Pflanzen, Blumen, Tieren", "tokens": ["Und", "was", "an", "Men\u00b7schen", ",", "Pflan\u00b7zen", ",", "Blu\u00b7men", ",", "Tie\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["KON", "PWS", "APPR", "NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur irgend da und sich des Daseins freut,", "tokens": ["Nur", "ir\u00b7gend", "da", "und", "sich", "des", "Da\u00b7seins", "freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "KON", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das wobst du ein, der G\u00f6ttin Bild zu zieren,", "tokens": ["Das", "wobst", "du", "ein", ",", "der", "G\u00f6t\u00b7tin", "Bild", "zu", "zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die, t\u00e4glich sterbend, st\u00fcndlich sich erneut.", "tokens": ["Die", ",", "t\u00e4g\u00b7lich", "ster\u00b7bend", ",", "st\u00fcnd\u00b7lich", "sich", "er\u00b7neut", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "VVPP", "$,", "ADJD", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Mutter alles Wesenhaften, Guten,", "tokens": ["Die", "Mut\u00b7ter", "al\u00b7les", "We\u00b7sen\u00b7haf\u00b7ten", ",", "Gu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie sitzt an deinem Born, der str\u00f6mend quillt,", "tokens": ["Sie", "sitzt", "an", "dei\u00b7nem", "Born", ",", "der", "str\u00f6\u00b7mend", "quillt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und spiegelt sich in den kristallnen Fluten,", "tokens": ["Und", "spie\u00b7gelt", "sich", "in", "den", "kris\u00b7tall\u00b7nen", "Flu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ihr Selbst verwechselnd tr\u00e4umrisch mit dem Bild.", "tokens": ["Ihr", "Selbst", "ver\u00b7wech\u00b7selnd", "tr\u00e4um\u00b7risch", "mit", "dem", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und l\u00e4chelt sie, so l\u00e4chelst du ihr wieder,", "tokens": ["Und", "l\u00e4\u00b7chelt", "sie", ",", "so", "l\u00e4\u00b7chelst", "du", "ihr", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und grollt sie, gibst du ihr den Trotz zur\u00fcck,", "tokens": ["Und", "grollt", "sie", ",", "gibst", "du", "ihr", "den", "Trotz", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durchsichtig, gleich der Wahrheit, deine Lieder,", "tokens": ["Durch\u00b7sich\u00b7tig", ",", "gleich", "der", "Wahr\u00b7heit", ",", "dei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und t\u00e4uschend nur, wie T\u00e4uschung auch das Gl\u00fcck.", "tokens": ["Und", "t\u00e4u\u00b7schend", "nur", ",", "wie", "T\u00e4u\u00b7schung", "auch", "das", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$,", "PWAV", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und so ein Kind, noch bei ergrauten Haaren,", "tokens": ["Und", "so", "ein", "Kind", ",", "noch", "bei", "er\u00b7grau\u00b7ten", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und auch ein Greis beim fr\u00fchsten Kinderspiel,", "tokens": ["Und", "auch", "ein", "Greis", "beim", "fr\u00fchs\u00b7ten", "Kin\u00b7der\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hast du f\u00fcr all, was Menschheit je erfahren,", "tokens": ["Hast", "du", "f\u00fcr", "all", ",", "was", "Menschheit", "je", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PIAT", "$,", "PWS", "NN", "ADV", "VVINF", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ein Bild, ein Wort, den Pfad und auch das Ziel.", "tokens": ["Ein", "Bild", ",", "ein", "Wort", ",", "den", "Pfad", "und", "auch", "das", "Ziel", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}