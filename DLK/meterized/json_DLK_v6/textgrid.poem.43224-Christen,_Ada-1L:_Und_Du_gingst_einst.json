{"textgrid.poem.43224": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und Du gingst einst", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und Du gingst einst", "tokens": ["Und", "Du", "gingst", "einst"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "In d\u00fcnnem Kleid", "tokens": ["In", "d\u00fcn\u00b7nem", "Kleid"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und n\u00e4htest die Finger Dir wund,", "tokens": ["Und", "n\u00e4h\u00b7test", "die", "Fin\u00b7ger", "Dir", "wund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Du theiltest einst", "tokens": ["Du", "theil\u00b7test", "einst"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Freude und Leid", "tokens": ["Freu\u00b7de", "und", "Leid"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Mit mir \u2013 und den Bissen vom Mund.", "tokens": ["Mit", "mir", "\u2013", "und", "den", "Bis\u00b7sen", "vom", "Mund", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$(", "KON", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Und Du warst einst", "tokens": ["Und", "Du", "warst", "einst"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV"], "meter": "--+-", "measure": "anapaest.init"}, "line.8": {"text": "So treu und rein,", "tokens": ["So", "treu", "und", "rein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Du eitle, herzlose Frau,", "tokens": ["Du", "eit\u00b7le", ",", "herz\u00b7lo\u00b7se", "Frau", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Du wolltest einst", "tokens": ["Du", "woll\u00b7test", "einst"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Mein eigen sein", "tokens": ["Mein", "ei\u00b7gen", "sein"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJD", "VAINF"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Und treu \u2013 wie der Himmel blau. \u2013", "tokens": ["Und", "treu", "\u2013", "wie", "der", "Him\u00b7mel", "blau", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "$(", "KOKOM", "ART", "NN", "ADJD", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Du warst es einst", "tokens": ["Du", "warst", "es", "einst"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Und hast gefreit", "tokens": ["Und", "hast", "ge\u00b7freit"], "token_info": ["word", "word", "word"], "pos": ["KON", "VAFIN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Mit hostienreinem Leib,", "tokens": ["Mit", "hos\u00b7ti\u00b7en\u00b7rei\u00b7nem", "Leib", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Dein Herz befleckt,", "tokens": ["Dein", "Herz", "be\u00b7fleckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Die Seele entweiht", "tokens": ["Die", "See\u00b7le", "ent\u00b7weiht"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.18": {"text": "Hat nur deine Ehe \u2013 Weib! ...", "tokens": ["Hat", "nur", "dei\u00b7ne", "E\u00b7he", "\u2013", "Weib", "!", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "$(", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}