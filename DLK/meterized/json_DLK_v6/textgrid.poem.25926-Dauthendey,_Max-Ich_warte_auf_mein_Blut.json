{"textgrid.poem.25926": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Ich warte auf mein Blut", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie um Kartoffeln, die dumpfen,", "tokens": ["Wie", "um", "Kar\u00b7tof\u00b7feln", ",", "die", "dum\u00b7pfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Keller mit bleichem Keim", "tokens": ["Im", "Kel\u00b7ler", "mit", "blei\u00b7chem", "Keim"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Werden die Tage nicht heller,", "tokens": ["Wer\u00b7den", "die", "Ta\u00b7ge", "nicht", "hel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Kommt mir mein Blut nicht heim.", "tokens": ["Kommt", "mir", "mein", "Blut", "nicht", "heim", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mein Blut lief auf die Gassen,", "tokens": ["Mein", "Blut", "lief", "auf", "die", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Leib war's zu allein,", "tokens": ["Im", "Leib", "wa\u00b7r's", "zu", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "APPR", "ADV", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Es wollte zu seinem Weib", "tokens": ["Es", "woll\u00b7te", "zu", "sei\u00b7nem", "Weib"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und in ihr Herz hinein.", "tokens": ["Und", "in", "ihr", "Herz", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich liege wie bei Leichen,", "tokens": ["Ich", "lie\u00b7ge", "wie", "bei", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich warte auf mein Blut,", "tokens": ["Ich", "war\u00b7te", "auf", "mein", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das rennt auf Steine sich hart,", "tokens": ["Das", "rennt", "auf", "Stei\u00b7ne", "sich", "hart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Weil in ihm die Sehnsucht nicht ruht.", "tokens": ["Weil", "in", "ihm", "die", "Sehn\u00b7sucht", "nicht", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}