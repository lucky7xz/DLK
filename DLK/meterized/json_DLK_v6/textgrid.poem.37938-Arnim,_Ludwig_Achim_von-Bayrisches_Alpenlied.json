{"textgrid.poem.37938": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Bayrisches Alpenlied", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Franz l\u00e4\u00dft dich gr\u00fc\u00dfen", "tokens": ["Der", "Franz", "l\u00e4\u00dft", "dich", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "PRF", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Gar hoch und gar fest,", "tokens": ["Gar", "hoch", "und", "gar", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Vom Palmbaum hoch sprie\u00dfen", "tokens": ["Vom", "Palm\u00b7baum", "hoch", "sprie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJD", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Gar vielerley Aest.", "tokens": ["Gar", "vie\u00b7ler\u00b7ley", "A\u00b7est", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mit gr\u00fcnblauer Seiden", "tokens": ["Mit", "gr\u00fcn\u00b7blau\u00b7er", "Sei\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ein Kr\u00e4nzlein h\u00e4ngt dran,", "tokens": ["Ein", "Kr\u00e4nz\u00b7lein", "h\u00e4ngt", "dran", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Drum sollt du wohl meiden", "tokens": ["Drum", "sollt", "du", "wohl", "mei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ein anderen Mann.", "tokens": ["Ein", "an\u00b7de\u00b7ren", "Mann", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Ja M\u00e4del, sein Lieben", "tokens": ["Ja", "M\u00e4\u00b7del", ",", "sein", "Lie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PTKANT", "NN", "$,", "PPOSAT", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Nimmt sonst mal ein End,", "tokens": ["Nimmt", "sonst", "mal", "ein", "End", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wie R\u00f6slein da dr\u00fcben,", "tokens": ["Wie", "R\u00f6s\u00b7lein", "da", "dr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ADV", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Reif hat verbrennt.", "tokens": ["Die", "Reif", "hat", "ver\u00b7brennt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Im Thal liegt noch Nebel,", "tokens": ["Im", "Thal", "liegt", "noch", "Ne\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Alpen sind klar,", "tokens": ["Die", "Al\u00b7pen", "sind", "klar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Doch wird er bald sehen,", "tokens": ["Doch", "wird", "er", "bald", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Was unten ist wahr.", "tokens": ["Was", "un\u00b7ten", "ist", "wahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Er sieht wohl die Schwalben,", "tokens": ["Er", "sieht", "wohl", "die", "Schwal\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sie ziehen dann nieder", "tokens": ["Sie", "zie\u00b7hen", "dann", "nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die K\u00fch von den Alpen,", "tokens": ["Die", "K\u00fch", "von", "den", "Al\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sie kommen auch wieder.", "tokens": ["Sie", "kom\u00b7men", "auch", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Jezt klingeln sie, gr\u00fc\u00dfen,", "tokens": ["Jezt", "klin\u00b7geln", "sie", ",", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sie haben gut Haus,", "tokens": ["Sie", "ha\u00b7ben", "gut", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Viel Br\u00fcnnlein drin flie\u00dfen,", "tokens": ["Viel", "Br\u00fcnn\u00b7lein", "drin", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ein Golddach ist drauf.", "tokens": ["Ein", "Gold\u00b7dach", "ist", "drauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Das Haus ist ganz offen,", "tokens": ["Das", "Haus", "ist", "ganz", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Kein Ringel daf\u00fcr,", "tokens": ["Kein", "Rin\u00b7gel", "da\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PAV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Der Franz thut wohl hoffen,", "tokens": ["Der", "Franz", "thut", "wohl", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Du klopfst an die Th\u00fcr.", "tokens": ["Du", "klopfst", "an", "die", "Th\u00fcr."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--", "measure": "dactylic.init"}}, "stanza.8": {"line.1": {"text": "Am buxbaumern Tischlein,", "tokens": ["Am", "bux\u00b7bau\u00b7mern", "Tisc\u00b7hlein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Drauf stehn zwey Glas Wein;", "tokens": ["Drauf", "stehn", "zwey", "Glas", "Wein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "CARD", "NN", "NN", "$."], "meter": "-+-++", "measure": "unknown.measure.tri"}, "line.3": {"text": "Er schenkt klaren Wein ein,", "tokens": ["Er", "schenkt", "kla\u00b7ren", "Wein", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Er saget was fein.", "tokens": ["Er", "sa\u00b7get", "was", "fein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Er redet was wahr ist,", "tokens": ["Er", "re\u00b7det", "was", "wahr", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "ADJD", "VAFIN", "$,"], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Er trinket was klar ist,", "tokens": ["Er", "trin\u00b7ket", "was", "klar", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "ADJD", "VAFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Er liebet was fein ist:", "tokens": ["Er", "lie\u00b7bet", "was", "fein", "ist", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADJD", "VAFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Lieb M\u00e4del er gr\u00fc\u00dft dich.", "tokens": ["Lieb", "M\u00e4\u00b7del", "er", "gr\u00fc\u00dft", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-++-", "measure": "unknown.measure.tri"}}}}}