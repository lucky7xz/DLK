{"textgrid.poem.57263": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es t\u00f6net sein Lob Feld, und Wald, Thal, und Gebirg,", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es t\u00f6net sein Lob Feld, und Wald, Thal, und Gebirg,", "tokens": ["Es", "t\u00f6\u00b7net", "sein", "Lob", "Feld", ",", "und", "Wald", ",", "Thal", ",", "und", "Ge\u00b7birg", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$,", "KON", "NN", "$,", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.2": {"text": "Das Gestad' hallet, es donnert das Meer dumpfbrausend", "tokens": ["Das", "Ge\u00b7stad'", "hal\u00b7let", ",", "es", "don\u00b7nert", "das", "Meer", "dumpf\u00b7brau\u00b7send"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "VVPP"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des Unendlichen Lob, siehe des Herlichen,", "tokens": ["Des", "Un\u00b7end\u00b7li\u00b7chen", "Lob", ",", "sie\u00b7he", "des", "Her\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVIMP", "ART", "NN", "$,"], "meter": "--+--+-+-+--", "measure": "anapaest.di.plus"}, "line.4": {"text": "Unerreichten von dem Danklied der Natur!", "tokens": ["Un\u00b7er\u00b7reich\u00b7ten", "von", "dem", "Dank\u00b7lied", "der", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+---+", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "Es singt die Natur dennoch dem, welcher sie schuf,", "tokens": ["Es", "singt", "die", "Na\u00b7tur", "den\u00b7noch", "dem", ",", "wel\u00b7cher", "sie", "schuf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ART", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ihr Get\u00f6n schallet vom Himmel herab, lautpreisend", "tokens": ["Ihr", "Ge\u00b7t\u00f6n", "schal\u00b7let", "vom", "Him\u00b7mel", "her\u00b7ab", ",", "laut\u00b7prei\u00b7send"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "ADV", "$,", "ADJD"], "meter": "+-++--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "In umw\u00f6lkender Nacht rufet des Strahls Gef\u00e4hrt", "tokens": ["In", "um\u00b7w\u00f6l\u00b7ken\u00b7der", "Nacht", "ru\u00b7fet", "des", "Strahls", "Ge\u00b7f\u00e4hrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Von den Wipfeln, und der Berg' Haupt es herab!", "tokens": ["Von", "den", "Wip\u00b7feln", ",", "und", "der", "Ber\u00b7g'", "Haupt", "es", "her\u00b7ab", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KON", "ART", "NN", "NN", "PPER", "ADV", "$."], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "Es rauschet der Hain, und sein Bach lispelt es auch", "tokens": ["Es", "rau\u00b7schet", "der", "Hain", ",", "und", "sein", "Bach", "lis\u00b7pelt", "es", "auch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mit empor, preisend, ein Feyrer, wie er! die Luft wehts", "tokens": ["Mit", "em\u00b7por", ",", "prei\u00b7send", ",", "ein", "Fey\u00b7rer", ",", "wie", "er", "!", "die", "Luft", "wehts"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PTKVZ", "$,", "VVPP", "$,", "ART", "NN", "$,", "PWAV", "PPER", "$.", "ART", "NN", "VVFIN"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Zu dem Bogen mit auf! Hoch in der Wolke ward", "tokens": ["Zu", "dem", "Bo\u00b7gen", "mit", "auf", "!", "Hoch", "in", "der", "Wol\u00b7ke", "ward"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "PTKVZ", "$.", "ADJD", "APPR", "ART", "NN", "VAFIN"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der Erhaltung und der Huld Bogen gesetzt.", "tokens": ["Der", "Er\u00b7hal\u00b7tung", "und", "der", "Huld", "Bo\u00b7gen", "ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "VVPP", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Und schweigest denn du, welchen Gott ewig erschuf?", "tokens": ["Und", "schwei\u00b7gest", "denn", "du", ",", "wel\u00b7chen", "Gott", "e\u00b7wig", "er\u00b7schuf", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "PPER", "$,", "PWAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und verstumst mitten im Preis' um dich her? Gott hauchte", "tokens": ["Und", "ver\u00b7stumst", "mit\u00b7ten", "im", "Preis'", "um", "dich", "her", "?", "Gott", "hauch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "APPR", "PPER", "PTKVZ", "$.", "NN", "VVFIN"], "meter": "--+---+-++-+-", "measure": "anapaest.init"}, "line.3": {"text": "Dir Unsterblichkeit ein! Danke dem Herlichen!", "tokens": ["Dir", "U\u00b7nsterb\u00b7lich\u00b7keit", "ein", "!", "Dan\u00b7ke", "dem", "Her\u00b7li\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PTKVZ", "$.", "PTKANT", "ART", "NN", "$."], "meter": "-+--+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Unerreicht bleibt von dem Aufschwung des Gesangs", "tokens": ["Un\u00b7er\u00b7reicht", "bleibt", "von", "dem", "Auf\u00b7schwung", "des", "Ge\u00b7sangs"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+---+", "measure": "unknown.measure.penta"}}, "stanza.5": {"line.1": {"text": "Der Geber, allein dennoch sing, preis' ihn, o du,", "tokens": ["Der", "Ge\u00b7ber", ",", "al\u00b7lein", "den\u00b7noch", "sing", ",", "preis'", "ihn", ",", "o", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "VVFIN", "$,", "VVFIN", "PPER", "$,", "FM", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der empfing! Leuchtendes Chor um mich her, ernstfreudig,", "tokens": ["Der", "emp\u00b7fing", "!", "Leuch\u00b7ten\u00b7des", "Chor", "um", "mich", "her", ",", "ernst\u00b7freu\u00b7dig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "NN", "NN", "APPR", "PPER", "PTKVZ", "$,", "ADJD", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Du Erheber des Herrn, tret' ich herzu, und sing'", "tokens": ["Du", "Er\u00b7he\u00b7ber", "des", "Herrn", ",", "tret'", "ich", "her\u00b7zu", ",", "und", "sing'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "ADV", "$,", "KON", "NE"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "In Entz\u00fcckung, o du Chor, Psalme mit dir!", "tokens": ["In", "Ent\u00b7z\u00fc\u00b7ckung", ",", "o", "du", "Chor", ",", "Psal\u00b7me", "mit", "dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "FM", "PPER", "NN", "$,", "NN", "APPR", "PPER", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Der Welten erschuf, dort des Tags sinkendes Gold,", "tokens": ["Der", "Wel\u00b7ten", "er\u00b7schuf", ",", "dort", "des", "Tags", "sin\u00b7ken\u00b7des", "Gold", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--++--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und den Staub hier voll Gew\u00fcrmegedr\u00e4ng, wer ist der?", "tokens": ["Und", "den", "Staub", "hier", "voll", "Ge\u00b7w\u00fcr\u00b7me\u00b7ge\u00b7dr\u00e4ng", ",", "wer", "ist", "der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADJD", "NN", "$,", "PWS", "VAFIN", "ART", "$."], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Es ist Gott! es ist Gott! Vater! so rufen wir;", "tokens": ["Es", "ist", "Gott", "!", "es", "ist", "Gott", "!", "Va\u00b7ter", "!", "so", "ru\u00b7fen", "wir", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$.", "PPER", "VAFIN", "NN", "$.", "NN", "$.", "ADV", "VVFIN", "PPER", "$."], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und unz\u00e4hlbar, die mit uns rufen, seyd ihr!", "tokens": ["Und", "un\u00b7z\u00e4hl\u00b7bar", ",", "die", "mit", "uns", "ru\u00b7fen", ",", "seyd", "ihr", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Der Welten erschuf, dort den Leun! Heisser ergiesst", "tokens": ["Der", "Wel\u00b7ten", "er\u00b7schuf", ",", "dort", "den", "Leun", "!", "Heis\u00b7ser", "er\u00b7giesst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "ART", "NN", "$.", "NN", "VVFIN"], "meter": "-+--+---+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sich sein Herz! Widder, und dich Kaprikorn, Pleionen,", "tokens": ["Sich", "sein", "Herz", "!", "Wid\u00b7der", ",", "und", "dich", "Ka\u00b7pri\u00b7korn", ",", "Plei\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "$.", "NN", "$,", "KON", "PPER", "NN", "$,", "NN", "$,"], "meter": "---+-++-++-+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Skorpion, und den Krebs. Steigender w\u00e4gt sie dort", "tokens": ["Skor\u00b7pi\u00b7on", ",", "und", "den", "Krebs", ".", "Stei\u00b7gen\u00b7der", "w\u00e4gt", "sie", "dort"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "ART", "NN", "$.", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.4": {"text": "Den Begleiter. Mit dem Pfeil zielet, und blitzt", "tokens": ["Den", "Be\u00b7glei\u00b7ter", ".", "Mit", "dem", "Pfeil", "zie\u00b7let", ",", "und", "blitzt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "APPR", "ART", "NN", "VVFIN", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.8": {"line.1": {"text": "Der Sch\u00fctze! Wie t\u00f6nt, dreht, er sich, K\u00f6cher, und Pfeil!", "tokens": ["Der", "Sch\u00fct\u00b7ze", "!", "Wie", "t\u00f6nt", ",", "dreht", ",", "er", "sich", ",", "K\u00f6\u00b7cher", ",", "und", "Pfeil", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PWAV", "VVFIN", "$,", "VVFIN", "$,", "PPER", "PRF", "$,", "NN", "$,", "KON", "NN", "$."], "meter": "-+--++--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie vereint leuchtet ihr, Zwilling', herab! Sie heben", "tokens": ["Wie", "ver\u00b7eint", "leuch\u00b7tet", "ihr", ",", "Zwil\u00b7ling'", ",", "her\u00b7ab", "!", "Sie", "he\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWAV", "VVPP", "VVFIN", "PPER", "$,", "NN", "$,", "ADV", "$.", "PPER", "VVFIN"], "meter": "--++-+++-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Im Triumphe des Gangs freudig den Strahlenfuss!", "tokens": ["Im", "Tri\u00b7um\u00b7phe", "des", "Gangs", "freu\u00b7dig", "den", "Strah\u00b7len\u00b7fuss", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD", "ART", "NN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Und der Fisch spielet, und bl\u00e4st Str\u00f6me der Glut.", "tokens": ["Und", "der", "Fisch", "spie\u00b7let", ",", "und", "bl\u00e4st", "Str\u00f6\u00b7me", "der", "Glut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "KON", "VVFIN", "NN", "ART", "NN", "$."], "meter": "--++-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.9": {"line.1": {"text": "Die Ros' in dem Kranz duftet Licht! K\u00f6niglich schwebt,", "tokens": ["Die", "Ros'", "in", "dem", "Kranz", "duf\u00b7tet", "Licht", "!", "K\u00f6\u00b7nig\u00b7lich", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "NN", "$.", "ADJD", "VVFIN", "$,"], "meter": "-+--++-++--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "In dem Blick Flamme, der Adler, gebeut Gehorsam", "tokens": ["In", "dem", "Blick", "Flam\u00b7me", ",", "der", "Ad\u00b7ler", ",", "ge\u00b7beut", "Ge\u00b7hor\u00b7sam"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "$,", "ART", "NN", "$,", "VVFIN", "NN"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Den Gef\u00e4hrten um sich! Stolz, den gebognen Hals,", "tokens": ["Den", "Ge\u00b7f\u00e4hr\u00b7ten", "um", "sich", "!", "Stolz", ",", "den", "ge\u00b7bog\u00b7nen", "Hals", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "$.", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und den Fittig in die H\u00f6h, schwimmet der Schwan!", "tokens": ["Und", "den", "Fit\u00b7tig", "in", "die", "H\u00f6h", ",", "schwim\u00b7met", "der", "Schwan", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}}, "stanza.10": {"line.1": {"text": "Wer gab Melodie, Leyer, dir? zog das Get\u00f6n", "tokens": ["Wer", "gab", "Me\u00b7lo\u00b7die", ",", "Le\u00b7yer", ",", "dir", "?", "zog", "das", "Ge\u00b7t\u00f6n"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "NE", "$,", "NE", "$,", "PPER", "$.", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und das Gold himlischer Saiten dir auf? Du schallest", "tokens": ["Und", "das", "Gold", "him\u00b7li\u00b7scher", "Sai\u00b7ten", "dir", "auf", "?", "Du", "schal\u00b7lest"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN"], "meter": "--++--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zu dem kreisenden Tanz, welchen, beseelt von dir,", "tokens": ["Zu", "dem", "krei\u00b7sen\u00b7den", "Tanz", ",", "wel\u00b7chen", ",", "be\u00b7seelt", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PWAT", "$,", "VVFIN", "APPR", "PPER", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Der Planet h\u00e4lt in der Laufbahn um dich her.", "tokens": ["Der", "Pla\u00b7net", "h\u00e4lt", "in", "der", "Lauf\u00b7bahn", "um", "dich", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.re