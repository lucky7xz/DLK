{"textgrid.poem.26029": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Mein Lieb steht unter seiner T\u00fcr", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Glocken wandern von Haus zu Haus", "tokens": ["Glo\u00b7cken", "wan\u00b7dern", "von", "Haus", "zu", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "NN", "APPR", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Und teilen Freudenkleider aus.", "tokens": ["Und", "tei\u00b7len", "Freu\u00b7den\u00b7klei\u00b7der", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Lieb steht unter seiner T\u00fcr,", "tokens": ["Mein", "Lieb", "steht", "un\u00b7ter", "sei\u00b7ner", "T\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Seligkeit schm\u00fcckt sie wie Sommerzier.", "tokens": ["Se\u00b7lig\u00b7keit", "schm\u00fcckt", "sie", "wie", "Som\u00b7mer\u00b7zier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KOKOM", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Die Glocken haben Wege gebaut,", "tokens": ["Die", "Glo\u00b7cken", "ha\u00b7ben", "We\u00b7ge", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und alle Wege frohlocken laut.", "tokens": ["Und", "al\u00b7le", "We\u00b7ge", "froh\u00b7lo\u0