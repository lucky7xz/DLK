{"textgrid.poem.25008": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Novemberabend", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Hauch des Grabes schien von Blatt zu Blatt,", "tokens": ["Ein", "Hauch", "des", "Gra\u00b7bes", "schien", "von", "Blatt", "zu", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von Ast zu Aesten tr\u00e4g zu wallen;", "tokens": ["Von", "Ast", "zu", "A\u00b7es\u00b7ten", "tr\u00e4g", "zu", "wal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das letzte Laub nur klammerte noch matt", "tokens": ["Das", "letz\u00b7te", "Laub", "nur", "klam\u00b7mer\u00b7te", "noch", "matt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sich an die Zweige vor dem Fallen.", "tokens": ["Sich", "an", "die", "Zwei\u00b7ge", "vor", "dem", "Fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vom Nebel des Novembers kalt umtrieft,", "tokens": ["Vom", "Ne\u00b7bel", "des", "No\u00b7vem\u00b7bers", "kalt", "um\u00b7trieft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der rings auf H\u00fcgeln lag und Mooren,", "tokens": ["Der", "rings", "auf", "H\u00fc\u00b7geln", "lag", "und", "Moo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hin schritt ich, in Erinnerung vertieft,", "tokens": ["Hin", "schritt", "ich", ",", "in", "E\u00b7rin\u00b7ne\u00b7rung", "ver\u00b7tieft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "An all das Gl\u00fcck, das ich verloren.", "tokens": ["An", "all", "das", "Gl\u00fcck", ",", "das", "ich", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Jugend Hoffnungen und Tr\u00e4ume deckt", "tokens": ["Der", "Ju\u00b7gend", "Hoff\u00b7nun\u00b7gen", "und", "Tr\u00e4u\u00b7me", "deckt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr immerdar die Nacht der Gr\u00fcfte,", "tokens": ["F\u00fcr", "im\u00b7mer\u00b7dar", "die", "Nacht", "der", "Gr\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und meine Seele bebt zur\u00fcck erschreckt,", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "bebt", "zu\u00b7r\u00fcck", "er\u00b7schreckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wenn ich den Leichenschleier l\u00fcfte.", "tokens": ["Wenn", "ich", "den", "Lei\u00b7chen\u00b7schlei\u00b7er", "l\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dahin, wie meines Geistes k\u00fchner Flug,", "tokens": ["Da\u00b7hin", ",", "wie", "mei\u00b7nes", "Geis\u00b7tes", "k\u00fch\u00b7ner", "Flug", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PWAV", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr, die im Arm ihr einst mir ruhtet!", "tokens": ["Ihr", ",", "die", "im", "Arm", "ihr", "einst", "mir", "ruh\u00b7tet", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPRART", "NN", "PPER", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "An Wunden, die euch fr\u00fch das Schicksal schlug,", "tokens": ["An", "Wun\u00b7den", ",", "die", "euch", "fr\u00fch", "das", "Schick\u00b7sal", "schlug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Um mich, vor mir seid ihr verblutet!", "tokens": ["Um", "mich", ",", "vor", "mir", "seid", "ihr", "ver\u00b7blu\u00b7tet", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "$,", "APPR", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Der einsam ich zur\u00fcckgeblieben bin,", "tokens": ["Der", "ein\u00b7sam", "ich", "zu\u00b7r\u00fcck\u00b7ge\u00b7blie\u00b7ben", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nun st\u00fcrmen f\u00fchl' ich's rauh und rauher,", "tokens": ["Nun", "st\u00fcr\u00b7men", "f\u00fchl'", "ich's", "rauh", "und", "rau\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und meines Lebens Bl\u00e4tter sinken hin,", "tokens": ["Und", "mei\u00b7nes", "Le\u00b7bens", "Bl\u00e4t\u00b7ter", "sin\u00b7ken", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die letzten in des Herbstes Schauer.", "tokens": ["Die", "letz\u00b7ten", "in", "des", "Herbs\u00b7tes", "Schau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich dacht' es; hinter Wolken, tr\u00fcb' und schwer,", "tokens": ["Ich", "dacht'", "es", ";", "hin\u00b7ter", "Wol\u00b7ken", ",", "tr\u00fcb'", "und", "schwer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "APPR", "NN", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sah ich das Abendlicht verglimmen,", "tokens": ["Sah", "ich", "das", "A\u00b7bend\u00b7licht", "ver\u00b7glim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und leise trug der Wind vom Friedhof her", "tokens": ["Und", "lei\u00b7se", "trug", "der", "Wind", "vom", "Fried\u00b7hof", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mir an das Ohr der Toten Stimmen.", "tokens": ["Mir", "an", "das", "Ohr", "der", "To\u00b7ten", "Stim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}