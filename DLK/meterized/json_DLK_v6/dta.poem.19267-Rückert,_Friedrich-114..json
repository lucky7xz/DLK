{"dta.poem.19267": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "114.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Am Fenster t\u00e4glich siehst du an dein Blumenst\u00f6ckchen,", "tokens": ["Am", "Fens\u00b7ter", "t\u00e4g\u00b7lich", "siehst", "du", "an", "dein", "Blu\u00b7men\u00b7st\u00f6ck\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch scheinst du anzusehn nur die gewelkten Gl\u00f6ckchen,", "tokens": ["Doch", "scheinst", "du", "an\u00b7zu\u00b7sehn", "nur", "die", "ge\u00b7welk\u00b7ten", "Gl\u00f6ck\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVIZU", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht die noch bl\u00fchenden, und die erst bl\u00fchen sollen,", "tokens": ["Nicht", "die", "noch", "bl\u00fc\u00b7hen\u00b7den", ",", "und", "die", "erst", "bl\u00fc\u00b7hen", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADV", "VVINF", "$,", "KON", "ART", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die an die Stelle der gewelkten treten wollen.", "tokens": ["Die", "an", "die", "Stel\u00b7le", "der", "ge\u00b7welk\u00b7ten", "tre\u00b7ten", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "O Unzufriedenheit, die ihre Sch\u00e4tze z\u00e4hlt,", "tokens": ["O", "Un\u00b7zu\u00b7frie\u00b7den\u00b7heit", ",", "die", "ih\u00b7re", "Sch\u00e4t\u00b7ze", "z\u00e4hlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu sehn nicht was sie hat, zu sehn nur was ihr fehlt.", "tokens": ["Zu", "sehn", "nicht", "was", "sie", "hat", ",", "zu", "sehn", "nur", "was", "ihr", "fehlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PTKNEG", "PWS", "PPER", "VAFIN", "$,", "PTKZU", "VVINF", "ADV", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}