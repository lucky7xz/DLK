{"textgrid.poem.32904": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier nimm die sanfte Leyer wieder,", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.57", "af:0.42"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier nimm die sanfte Leyer wieder,", "tokens": ["Hier", "nimm", "die", "sanf\u00b7te", "Le\u00b7yer", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O Muse, die du mir geliehn:", "tokens": ["O", "Mu\u00b7se", ",", "die", "du", "mir", "ge\u00b7liehn", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun sing ich weiter keine Lieder,", "tokens": ["Nun", "sing", "ich", "wei\u00b7ter", "kei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die von der Jugend Freuden gl\u00fchn.", "tokens": ["Die", "von", "der", "Ju\u00b7gend", "Freu\u00b7den", "gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Verzeih, wenn ich zu schwach gespielet:", "tokens": ["Ver\u00b7zeih", ",", "wenn", "ich", "zu", "schwach", "ge\u00b7spie\u00b7let", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Liebe fodert unser Herz:", "tokens": ["Die", "Lie\u00b7be", "fo\u00b7dert", "un\u00b7ser", "Herz", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das wenigste hab ich gef\u00fchlet;", "tokens": ["Das", "we\u00b7nigs\u00b7te", "hab", "ich", "ge\u00b7f\u00fch\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Das meiste sang ich blos aus Scherz.", "tokens": ["Das", "meis\u00b7te", "sang", "ich", "blos", "aus", "Scherz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Von Waffen und vom Ha\u00df umgeben,", "tokens": ["Von", "Waf\u00b7fen", "und", "vom", "Ha\u00df", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sang ich von Z\u00e4rtlichkeit und Ruh:", "tokens": ["Sang", "ich", "von", "Z\u00e4rt\u00b7lich\u00b7keit", "und", "Ruh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sang vom s\u00fc\u00dfen Saft der Reben,", "tokens": ["Ich", "sang", "vom", "s\u00fc\u00b7\u00dfen", "Saft", "der", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Wasser trank ich oft darzu.", "tokens": ["Und", "Was\u00b7ser", "trank", "ich", "oft", "dar\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADV", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "K\u00f6mmt einst der goldne Friede wieder,", "tokens": ["K\u00f6mmt", "einst", "der", "gold\u00b7ne", "Frie\u00b7de", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fchl ich einst gar der Liebe Gl\u00fcck:", "tokens": ["F\u00fchl", "ich", "einst", "gar", "der", "Lie\u00b7be", "Gl\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vielleicht wag ich dann sch\u00f6nre Lieder:", "tokens": ["Viel\u00b7leicht", "wag", "ich", "dann", "sch\u00f6n\u00b7re", "Lie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dann, Muse, gieb mir sie zur\u00fcck!", "tokens": ["Dann", ",", "Mu\u00b7se", ",", "gieb", "mir", "sie", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VVIMP", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}