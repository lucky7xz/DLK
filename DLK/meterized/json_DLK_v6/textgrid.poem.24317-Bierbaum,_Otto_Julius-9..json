{"textgrid.poem.24317": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier ist das edelste Werk getan", "tokens": ["Hier", "ist", "das", "e\u00b7dels\u00b7te", "Werk", "ge\u00b7tan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Allerlebendigster Kunst: hier ist", "tokens": ["Al\u00b7ler\u00b7le\u00b7ben\u00b7digs\u00b7ter", "Kunst", ":", "hier", "ist"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "ADV", "VAFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Kunst und Natur ganz eins.", "tokens": ["Kunst", "und", "Na\u00b7tur", "ganz", "eins", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "PIS", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Nichts verlor die Natur an die Kunst auf diesen Terrassen,", "tokens": ["Nichts", "ver\u00b7lor", "die", "Na\u00b7tur", "an", "die", "Kunst", "auf", "die\u00b7sen", "Ter\u00b7ras\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+--+--+-+-++-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Die sich ihr f\u00fcgten, indem sie sie edel", "tokens": ["Die", "sich", "ihr", "f\u00fcg\u00b7ten", ",", "in\u00b7dem", "sie", "sie", "e\u00b7del"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PRF", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fa\u00dften: Steine aus deinem Kern,", "tokens": ["Fa\u00df\u00b7ten", ":", "Stei\u00b7ne", "aus", "dei\u00b7nem", "Kern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Fels von Fiesole.", "tokens": ["Fels", "von", "Fie\u00b7so\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}}, "stanza.3": {"line.1": {"text": "Feld und Garten ist eins: es schlingt,", "tokens": ["Feld", "und", "Gar\u00b7ten", "ist", "eins", ":", "es", "schlingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PIS", "$.", "PPER", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Wachsend aus gleicher Furche mit ihm", "tokens": ["Wach\u00b7send", "aus", "glei\u00b7cher", "Fur\u00b7che", "mit", "ihm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN", "APPR", "PPER"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Zwischen den \u00fcppigsten Halmen des Korns,", "tokens": ["Zwi\u00b7schen", "den", "\u00fcp\u00b7pigs\u00b7ten", "Hal\u00b7men", "des", "Korns", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "----+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Wolluststark sich die Rebe empor,", "tokens": ["Wol\u00b7lust\u00b7stark", "sich", "die", "Re\u00b7be", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Keine R\u00e4uberin: Geliebte,", "tokens": ["Kei\u00b7ne", "R\u00e4u\u00b7be\u00b7rin", ":", "Ge\u00b7lieb\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$.", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hoch in den \u00d6lbaum.", "tokens": ["Hoch", "in", "den", "\u00d6l\u00b7baum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Alles umarmt sich hier: Rose den Lorbeerbaum,", "tokens": ["Al\u00b7les", "um\u00b7armt", "sich", "hier", ":", "Ro\u00b7se", "den", "Lor\u00b7beer\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ADV", "$.", "NE", "ART", "NN", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Efeu die Eiche, die", "tokens": ["E\u00b7feu", "die", "Ei\u00b7che", ",", "die"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NE", "ART", "NN", "$,", "PRELS"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nie ihr Blatt verliert.", "tokens": ["Nie", "ihr", "Blatt", "ver\u00b7liert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Engelwurz flicht sich sanft,", "tokens": ["En\u00b7gel\u00b7wurz", "flicht", "sich", "sanft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Liebevoll, Schmuck, ins Gr\u00fcn", "tokens": ["Lie\u00b7be\u00b7voll", ",", "Schmuck", ",", "ins", "Gr\u00fcn"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["ADJD", "$,", "NN", "$,", "APPRART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Steiler, schwarzer Zypressen. Es h\u00e4ngt,", "tokens": ["Stei\u00b7ler", ",", "schwar\u00b7zer", "Zyp\u00b7res\u00b7sen", ".", "Es", "h\u00e4ngt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$.", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Gleich einem riesigen Bacchusgelock,", "tokens": ["Gleich", "ei\u00b7nem", "rie\u00b7si\u00b7gen", "Bac\u00b7chus\u00b7ge\u00b7lock", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Blau der Glyzine Bl\u00fctentraube", "tokens": ["Blau", "der", "Gly\u00b7zi\u00b7ne", "Bl\u00fc\u00b7ten\u00b7trau\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Schwer vom S\u00e4ulengeb\u00e4lk der Villa.", "tokens": ["Schwer", "vom", "S\u00e4u\u00b7len\u00b7ge\u00b7b\u00e4lk", "der", "Vil\u00b7la", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Iris und Tulpen s\u00e4umen das Garten-Feld;", "tokens": ["I\u00b7ris", "und", "Tul\u00b7pen", "s\u00e4u\u00b7men", "das", "Gar\u00b7ten\u00b7Feld", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "\u00dcberall Sterne und Glocken im Gras,", "tokens": ["\u00dc\u00b7be\u00b7rall", "Ster\u00b7ne", "und", "Glo\u00b7cken", "im", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Seltsame, feurige: namenlos", "tokens": ["Selt\u00b7sa\u00b7me", ",", "feu\u00b7ri\u00b7ge", ":", "na\u00b7men\u00b7los"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "ADJA", "$.", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Nordischer Zunge.", "tokens": ["Nor\u00b7di\u00b7scher", "Zun\u00b7ge", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Nichts scheint wild hier; alles ist Zucht;", "tokens": ["Nichts", "scheint", "wild", "hier", ";", "al\u00b7les", "ist", "Zucht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJD", "ADV", "$.", "PIS", "VAFIN", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Aber es ist die edelste Freiheit.", "tokens": ["A\u00b7ber", "es", "ist", "die", "e\u00b7dels\u00b7te", "Frei\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Dienerin wurde Natur dem Geiste,", "tokens": ["Die\u00b7ne\u00b7rin", "wur\u00b7de", "Na\u00b7tur", "dem", "Geis\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Der aus ihrem Geist regiert.", "tokens": ["Der", "aus", "ih\u00b7rem", "Geist", "re\u00b7giert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Hier erkannt ich die Kraft", "tokens": ["Hier", "er\u00b7kannt", "ich", "die", "Kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Und die herrliche Ewigkeit,", "tokens": ["Und", "die", "herr\u00b7li\u00b7che", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Hellas und Rom, des Sinns", "tokens": ["Hel\u00b7las", "und", "Rom", ",", "des", "Sinns"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "KON", "NE", "$,", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bindet und hebt und verkl\u00e4rt und den Menschen", "tokens": ["Bin\u00b7det", "und", "hebt", "und", "ver\u00b7kl\u00e4rt", "und", "den", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "KON", "VVFIN", "KON", "VVPP", "KON", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Wirklich zum Herren der Erde macht.", "tokens": ["Wirk\u00b7lich", "zum", "Her\u00b7ren", "der", "Er\u00b7de", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}