{"textgrid.poem.26603": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie auch der Tag sich dehne,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie auch der Tag sich dehne,", "tokens": ["Wie", "auch", "der", "Tag", "sich", "deh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "PRF", "PDS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf \u00f6der H\u00fcgellehne", "tokens": ["Auf", "\u00f6\u00b7der", "H\u00fc\u00b7gel\u00b7leh\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Steht er und blickt in's Land;", "tokens": ["Steht", "er", "und", "blickt", "in's", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Er sieht die Sonne steigen", "tokens": ["Er", "sieht", "die", "Son\u00b7ne", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und still sich wieder neigen", "tokens": ["Und", "still", "sich", "wie\u00b7der", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PRF", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bis zu des Himmels Rand.", "tokens": ["Bis", "zu", "des", "Him\u00b7mels", "Rand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er sieht den Lenz auf Erden,", "tokens": ["Er", "sieht", "den", "Lenz", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er sieht es Sommer werden", "tokens": ["Er", "sieht", "es", "Som\u00b7mer", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "NN", "VAINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Herbst und Winter auch;", "tokens": ["Und", "Herbst", "und", "Win\u00b7ter", "auch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nicht Mittagsgluth, nicht Regen,", "tokens": ["Nicht", "Mit\u00b7tags\u00b7gluth", ",", "nicht", "Re\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PTKNEG", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Nicht Schnee kann ihn bewegen,", "tokens": ["Nicht", "Schnee", "kann", "ihn", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Noch eis'ger Sturmeshauch.", "tokens": ["Noch", "eis'\u00b7ger", "Stur\u00b7mes\u00b7hauch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Hund an seiner Seite,", "tokens": ["Der", "Hund", "an", "sei\u00b7ner", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df er die Schafe leite,", "tokens": ["Da\u00df", "er", "die", "Scha\u00b7fe", "lei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kennt seines Rufes Ton;", "tokens": ["Kennt", "sei\u00b7nes", "Ru\u00b7fes", "Ton", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sonst will kein Wort verlieren,", "tokens": ["Sonst", "will", "kein", "Wort", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Allein mit seinen Thieren,", "tokens": ["Al\u00b7lein", "mit", "sei\u00b7nen", "Thie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der dumpfe Menschensohn.", "tokens": ["Der", "dump\u00b7fe", "Men\u00b7schen\u00b7sohn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}