{"textgrid.poem.38639": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Erinnerung am Genfersee", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne sinkt; ein purpurfarbner Duft", "tokens": ["Die", "Son\u00b7ne", "sinkt", ";", "ein", "pur\u00b7purf\u00b7arb\u00b7ner", "Duft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schwimmt um Savoyens dunkle Tannenh\u00fcgel;", "tokens": ["Schwimmt", "um", "Sa\u00b7vo\u00b7yens", "dunk\u00b7le", "Tan\u00b7nen\u00b7h\u00fc\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Der Alpen Schnee entgl\u00fcht in hoher Luft;", "tokens": ["Der", "Al\u00b7pen", "Schnee", "ent\u00b7gl\u00fcht", "in", "ho\u00b7her", "Luft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Geneva malt sich in der Fluthen Spiegel.", "tokens": ["Ge\u00b7ne\u00b7va", "malt", "sich", "in", "der", "Flut\u00b7hen", "Spie\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In Gold verflie\u00dft der Berggeh\u00f6lze Saum;", "tokens": ["In", "Gold", "ver\u00b7flie\u00dft", "der", "Berg\u00b7ge\u00b7h\u00f6l\u00b7ze", "Saum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Wiesenflur, beschneit von Bl\u00fctenflocken,", "tokens": ["Die", "Wie\u00b7sen\u00b7flur", ",", "be\u00b7schneit", "von", "Bl\u00fc\u00b7ten\u00b7flo\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Haucht Wohlger\u00fcche; Zephyr athmet kaum;", "tokens": ["Haucht", "Wohl\u00b7ge\u00b7r\u00fc\u00b7che", ";", "Ze\u00b7phyr", "ath\u00b7met", "kaum", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vom Jura schallt der Klang der Heerdenglocken.", "tokens": ["Vom", "Ju\u00b7ra", "schallt", "der", "Klang", "der", "Heer\u00b7den\u00b7glo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Fischer singt im Kahne, der gemach,", "tokens": ["Der", "Fi\u00b7scher", "singt", "im", "Kah\u00b7ne", ",", "der", "ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,", "PRELS", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im rothen Wiederschein, zum Ufer gleitet,", "tokens": ["Im", "ro\u00b7then", "Wie\u00b7der\u00b7schein", ",", "zum", "U\u00b7fer", "glei\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo der bemoosten Eiche Schattendach", "tokens": ["Wo", "der", "be\u00b7moos\u00b7ten", "Ei\u00b7che", "Schat\u00b7ten\u00b7dach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die nezumhang'ne Wohnung \u00fcberbreitet.", "tokens": ["Die", "ne\u00b7zum\u00b7hang'\u00b7ne", "Woh\u00b7nung", "\u00fc\u00b7berb\u00b7rei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Am H\u00fcgel, der die Fluthen weit umschaut,", "tokens": ["Am", "H\u00fc\u00b7gel", ",", "der", "die", "Flut\u00b7hen", "weit", "um\u00b7schaut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schwebt die Erinn'rung l\u00e4chelnd zu mir nieder,", "tokens": ["Schwebt", "die", "Er\u00b7inn'\u00b7rung", "l\u00e4\u00b7chelnd", "zu", "mir", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und, gleich des Waldes erstem Fr\u00fchlingslaut,", "tokens": ["Und", ",", "gleich", "des", "Wal\u00b7des", "ers\u00b7tem", "Fr\u00fch\u00b7lings\u00b7laut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ert\u00f6nt die lang' vergess'ne Leier wieder.", "tokens": ["Er\u00b7t\u00f6nt", "die", "lang'", "ver\u00b7ge\u00b7ss'\u00b7ne", "Lei\u00b7er", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADV", "ADJA", "NN", "ADV", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "So gl\u00e4nzte der Gefilde Maigewand,", "tokens": ["So", "gl\u00e4nz\u00b7te", "der", "Ge\u00b7fil\u00b7de", "Mai\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So gl\u00fchte fern der Schnee, so friedlich hallte", "tokens": ["So", "gl\u00fch\u00b7te", "fern", "der", "Schnee", ",", "so", "fried\u00b7lich", "hall\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "$,", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Heerde L\u00e4uten, als an Salis Hand", "tokens": ["Der", "Heer\u00b7de", "L\u00e4u\u00b7ten", ",", "als", "an", "Sa\u00b7lis", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "KOUS", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich dort am Weidenbusch auf Blumen wallte.", "tokens": ["Ich", "dort", "am", "Wei\u00b7den\u00b7busch", "auf", "Blu\u00b7men", "wall\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "So l\u00e4chelte die Fluth, so rosig schien", "tokens": ["So", "l\u00e4\u00b7chel\u00b7te", "die", "Fluth", ",", "so", "ro\u00b7sig", "schien"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Abendhimmel durch bewegte Zweige,", "tokens": ["Der", "A\u00b7bend\u00b7him\u00b7mel", "durch", "be\u00b7weg\u00b7te", "Zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So freundlich stralte durch Platanengr\u00fcn", "tokens": ["So", "freund\u00b7lich", "stral\u00b7te", "durch", "Pla\u00b7ta\u00b7nen\u00b7gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Stern der D\u00e4mmrung, unsres Bundes Zeuge.", "tokens": ["Der", "Stern", "der", "D\u00e4mm\u00b7rung", ",", "uns\u00b7res", "Bun\u00b7des", "Zeu\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Sein Lied erklang, die Wipfel neigten sich!", "tokens": ["Sein", "Lied", "er\u00b7klang", ",", "die", "Wip\u00b7fel", "neig\u00b7ten", "sich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Uferschilf sah' man den Seegott lauschen:", "tokens": ["Im", "U\u00b7fer\u00b7schilf", "sah'", "man", "den", "See\u00b7gott", "lau\u00b7schen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da schlug die Stunde! Trennung fernte mich,", "tokens": ["Da", "schlug", "die", "Stun\u00b7de", "!", "Tren\u00b7nung", "fern\u00b7te", "mich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und nur Zypressen h\u00f6rt' ich einsam rauschen.", "tokens": ["Und", "nur", "Zyp\u00b7res\u00b7sen", "h\u00f6rt'", "ich", "ein\u00b7sam", "rau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NE", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.8": {"line.1": {"text": "So weht den Schmetterling, der, kaum enth\u00fcllt,", "tokens": ["So", "weht", "den", "Schmet\u00b7ter\u00b7ling", ",", "der", ",", "kaum", "ent\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Am Halm der Klippe festgeklammert bebte,", "tokens": ["Am", "Halm", "der", "Klip\u00b7pe", "fest\u00b7ge\u00b7klam\u00b7mert", "beb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Sturm ins Meer, eh' noch im Lenzgefild", "tokens": ["Der", "Sturm", "ins", "Meer", ",", "eh'", "noch", "im", "Lenz\u00b7ge\u00b7fild"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "KOUS", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zum Rosenhain der Blumensylphe schwebte.", "tokens": ["Zum", "Ro\u00b7sen\u00b7hain", "der", "Blu\u00b7men\u00b7syl\u00b7phe", "schweb\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}