{"textgrid.poem.34427": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: In Rhythmen lebend und in Reimen schwelgend,", "genre": "verse", "period": "N.A.", "pub_year": 1885, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Rhythmen lebend und in Reimen schwelgend,", "tokens": ["In", "Rhyth\u00b7men", "le\u00b7bend", "und", "in", "Rei\u00b7men", "schwel\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "hinwandl ich trunken meine Jugendpfade:", "tokens": ["hin\u00b7wandl", "ich", "trun\u00b7ken", "mei\u00b7ne", "Ju\u00b7gend\u00b7pfa\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "nach jedem Sch\u00f6nen greif ich an dem Wege,", "tokens": ["nach", "je\u00b7dem", "Sch\u00f6\u00b7nen", "greif", "ich", "an", "dem", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und von den G\u00f6ttern fordr ich jede Gnade!", "tokens": ["und", "von", "den", "G\u00f6t\u00b7tern", "fordr", "ich", "je\u00b7de", "Gna\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Drum, wenn den ruhelosen Sohn der Freude", "tokens": ["Drum", ",", "wenn", "den", "ru\u00b7he\u00b7lo\u00b7sen", "Sohn", "der", "Freu\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "$,", "KOUS", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die blonden Locken bald und bald die braunen", "tokens": ["die", "blon\u00b7den", "Lo\u00b7cken", "bald", "und", "bald", "die", "brau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "KON", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "unwiderstehlich, neu und ewig fesseln,", "tokens": ["un\u00b7wi\u00b7der\u00b7steh\u00b7lich", ",", "neu", "und", "e\u00b7wig", "fes\u00b7seln", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "so dulde l\u00e4chelnd meiner Liebe Launen.", "tokens": ["so", "dul\u00b7de", "l\u00e4\u00b7chelnd", "mei\u00b7ner", "Lie\u00b7be", "Lau\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch schilt mich nicht, noch f\u00fcrchte, dass die Treue", "tokens": ["Doch", "schilt", "mich", "nicht", ",", "noch", "f\u00fcrch\u00b7te", ",", "dass", "die", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "ADV", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "zum Freunde wie zur Liebsten k\u00f6nne schwinden:", "tokens": ["zum", "Freun\u00b7de", "wie", "zur", "Liebs\u00b7ten", "k\u00f6n\u00b7ne", "schwin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KOKOM", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "sie steht auf andrem Grunde, sie wird dauern,", "tokens": ["sie", "steht", "auf", "an\u00b7drem", "Grun\u00b7de", ",", "sie", "wird", "dau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,", "PPER", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "ein fester Sonnenstrahl in Fr\u00fchlingswinden!", "tokens": ["ein", "fes\u00b7ter", "Son\u00b7nen\u00b7strahl", "in", "Fr\u00fch\u00b7lings\u00b7win\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}