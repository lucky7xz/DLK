{"textgrid.poem.39289": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr wi\u00dft vom Blitze eine graue M\u00e4re,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr wi\u00dft vom Blitze eine graue M\u00e4re,", "tokens": ["Ihr", "wi\u00dft", "vom", "Blit\u00b7ze", "ei\u00b7ne", "grau\u00b7e", "M\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der im granitnen Leibe des Giganten,", "tokens": ["Der", "im", "gra\u00b7nit\u00b7nen", "Lei\u00b7be", "des", "Gi\u00b7gan\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Herabgeschleudert aus azurner Sph\u00e4re,", "tokens": ["Her\u00b7ab\u00b7ge\u00b7schleu\u00b7dert", "aus", "a\u00b7zur\u00b7ner", "Sph\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zum Strahl verk\u00f6rpert ward des Diamanten.", "tokens": ["Zum", "Strahl", "ver\u00b7k\u00f6r\u00b7pert", "ward", "des", "Di\u00b7a\u00b7man\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch wie entsprungen in des Berges Minen", "tokens": ["Doch", "wie", "ent\u00b7sprun\u00b7gen", "in", "des", "Ber\u00b7ges", "Mi\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Granaten, Amethyst' und Crysolithe,", "tokens": ["Gra\u00b7na\u00b7ten", ",", "A\u00b7me\u00b7thyst'", "und", "Cry\u00b7so\u00b7li\u00b7the", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Agath', Saphir', Topasen und Rubinen,", "tokens": ["A\u00b7ga\u00b7th'", ",", "Sa\u00b7phir'", ",", "To\u00b7pa\u00b7sen", "und", "Ru\u00b7bi\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Des will ich k\u00fcnden euch die andre Mythe.", "tokens": ["Des", "will", "ich", "k\u00fcn\u00b7den", "euch", "die", "and\u00b7re", "My\u00b7the", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VMFIN", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Als einst der Herr gemalt den Regenbogen", "tokens": ["Als", "einst", "der", "Herr", "ge\u00b7malt", "den", "Re\u00b7gen\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "VVPP", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit buntem Schmelze aller Trikoloren,", "tokens": ["Mit", "bun\u00b7tem", "Schmel\u00b7ze", "al\u00b7ler", "Tri\u00b7ko\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als Riesenbuchstab im Azur gezogen,", "tokens": ["Als", "Rie\u00b7sen\u00b7buch\u00b7stab", "im", "A\u00b7zur", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gewoben aus der Liebe Meteoren,", "tokens": ["Ge\u00b7wo\u00b7ben", "aus", "der", "Lie\u00b7be", "Me\u00b7te\u00b7o\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da sprach ein Cherub zu dem Herrn der Erde:", "tokens": ["Da", "sprach", "ein", "Che\u00b7rub", "zu", "dem", "Herrn", "der", "Er\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbwas frommt Jehova dieser Farben Blendung,", "tokens": ["\u00bb", "was", "frommt", "Je\u00b7ho\u00b7va", "die\u00b7ser", "Far\u00b7ben", "Blen\u00b7dung", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "NE", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn auf des Bergs porphyrnem Feuerherde", "tokens": ["Wenn", "auf", "des", "Bergs", "por\u00b7phyr\u00b7nem", "Feu\u00b7er\u00b7her\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Strahl verfl\u00fcchtet deiner Gnadensendung?\u00ab", "tokens": ["Der", "Strahl", "ver\u00b7fl\u00fcch\u00b7tet", "dei\u00b7ner", "Gna\u00b7den\u00b7sen\u00b7dung", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "\u00bbwillst du der Welt ein ewig' Zeichen stiften,", "tokens": ["\u00bb", "willst", "du", "der", "Welt", "ein", "e\u00b7wig'", "Zei\u00b7chen", "stif\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So la\u00df die Glut zum K\u00f6rper sich versteinen,", "tokens": ["So", "la\u00df", "die", "Glut", "zum", "K\u00f6r\u00b7per", "sich", "ver\u00b7stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "APPRART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df aus der Erde mittern\u00e4cht'gen Triften", "tokens": ["Da\u00df", "aus", "der", "Er\u00b7de", "mit\u00b7ter\u00b7n\u00e4cht'\u00b7gen", "Trif\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Sonnenfarben deiner Liebe scheinen.\u00ab", "tokens": ["Die", "Son\u00b7nen\u00b7far\u00b7ben", "dei\u00b7ner", "Lie\u00b7be", "schei\u00b7nen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Da senkt der Herr des Bogens bunte Spitze", "tokens": ["Da", "senkt", "der", "Herr", "des", "Bo\u00b7gens", "bun\u00b7te", "Spit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Tief in des Berges fels'gen Riesennacken,", "tokens": ["Tief", "in", "des", "Ber\u00b7ges", "fels'\u00b7gen", "Rie\u00b7sen\u00b7na\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Bis sich versteint das bunte Kind der Blitze", "tokens": ["Bis", "sich", "ver\u00b7steint", "das", "bun\u00b7te", "Kind", "der", "Blit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "VVFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In millionenfarb'gen Edelschlacken.", "tokens": ["In", "mil\u00b7li\u00b7o\u00b7nen\u00b7fa\u00b7rb'\u00b7gen", "E\u00b7del\u00b7schla\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Da ward aus Rot der Purpur der Granate,", "tokens": ["Da", "ward", "aus", "Rot", "der", "Pur\u00b7pur", "der", "Gra\u00b7na\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Chrysolith aus meeresgr\u00fcnem Tau,", "tokens": ["Der", "Chry\u00b7so\u00b7lith", "aus", "mee\u00b7res\u00b7gr\u00fc\u00b7nem", "Tau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus reinem Wei\u00df der Milchstoff der Agathe", "tokens": ["Aus", "rei\u00b7nem", "Wei\u00df", "der", "Milch\u00b7stoff", "der", "A\u00b7gat\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und der Saphir aus lichtem Himmelsblau.", "tokens": ["Und", "der", "Sa\u00b7phir", "aus", "lich\u00b7tem", "Him\u00b7mels\u00b7blau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "In Rosenfarb' ist der Rubin entglommen", "tokens": ["In", "Ro\u00b7sen\u00b7fa\u00b7rb'", "ist", "der", "Ru\u00b7bin", "ent\u00b7glom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VAFIN", "ART", "NE", "VVINF"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und der Topas im feuergelben Scheine,", "tokens": ["Und", "der", "To\u00b7pas", "im", "feu\u00b7er\u00b7gel\u00b7ben", "Schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Violet der Amethyst verschwommen;", "tokens": ["In", "Vi\u00b7o\u00b7let", "der", "A\u00b7met\u00b7hyst", "ver\u00b7schwom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das ist der Stammbaum jener Edelsteine.", "tokens": ["Das", "ist", "der", "Stamm\u00b7baum", "je\u00b7ner", "E\u00b7del\u00b7stei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}