{"textgrid.poem.28190": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Viel Helden hat es ietzt, so hats auch viel Poeten;", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viel Helden hat es ietzt, so hats auch viel Poeten;", "tokens": ["Viel", "Hel\u00b7den", "hat", "es", "ietzt", ",", "so", "hats", "auch", "viel", "Po\u00b7et\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "ADV", "$,", "ADV", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df jene nun die Zeit nicht wie der Tod mag t\u00f6dten,", "tokens": ["Da\u00df", "je\u00b7ne", "nun", "die", "Zeit", "nicht", "wie", "der", "Tod", "mag", "t\u00f6d\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADV", "ART", "NN", "PTKNEG", "KOKOM", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Darzu sind diese gut. Wiewol es auch gemein,", "tokens": ["Dar\u00b7zu", "sind", "die\u00b7se", "gut", ".", "Wie\u00b7wol", "es", "auch", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PDS", "ADJD", "$.", "KOUS", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo viel Poeten sind, da\u00df auch viel Tichter seyn.", "tokens": ["Wo", "viel", "Po\u00b7et\u00b7en", "sind", ",", "da\u00df", "auch", "viel", "Tich\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "w