{"textgrid.poem.49862": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Einer geistreichen Frau", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wem in das rein empfindende Herz holdselige Musen", "tokens": ["Wem", "in", "das", "rein", "emp\u00b7fin\u00b7den\u00b7de", "Herz", "hold\u00b7se\u00b7li\u00b7ge", "Mu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "ADJD", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Anmut hauchten und ihm liehn das bezaubernde Wort \u2013", "tokens": ["An\u00b7mut", "hauch\u00b7ten", "und", "ihm", "liehn", "das", "be\u00b7zau\u00b7bern\u00b7de", "Wort", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Alles glauben wir ihm; doch diesen schmeichelnden Lippen", "tokens": ["Al\u00b7les", "glau\u00b7ben", "wir", "ihm", ";", "doch", "die\u00b7sen", "schmei\u00b7cheln\u00b7den", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "PPER", "$.", "ADV", "PDAT", "ADJA", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Glaubt ich alles, bevor ich nur ein W\u00f6rtchen vernahm.", "tokens": ["Glaubt", "ich", "al\u00b7les", ",", "be\u00b7vor", "ich", "nur", "ein", "W\u00f6rt\u00b7chen", "ver\u00b7nahm", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}}}}}