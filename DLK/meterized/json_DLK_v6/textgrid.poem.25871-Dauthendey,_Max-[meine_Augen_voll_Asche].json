{"textgrid.poem.25871": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[meine Augen voll Asche]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Augen voll Asche,", "tokens": ["Mei\u00b7ne", "Au\u00b7gen", "voll", "A\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Meine Ohren haben die T\u00f6ne verloren,", "tokens": ["Mei\u00b7ne", "Oh\u00b7ren", "ha\u00b7ben", "die", "T\u00f6\u00b7ne", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "B\u00e4ume, Wind, Gestein,", "tokens": ["B\u00e4u\u00b7me", ",", "Wind", ",", "Ge\u00b7stein", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Eure Sprache f\u00e4llt mir nicht mehr ein.", "tokens": ["Eu\u00b7re", "Spra\u00b7che", "f\u00e4llt", "mir", "nicht", "mehr", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "H\u00f6re im Weltraum nur mich,", "tokens": ["H\u00f6\u00b7re", "im", "Welt\u00b7raum", "nur", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ADV", "PPER", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Mein wildes, hungerndes Ich.", "tokens": ["Mein", "wil\u00b7des", ",", "hun\u00b7gern\u00b7des", "Ich", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}