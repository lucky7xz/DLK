{"textgrid.poem.52955": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Den Wolken nah, auf d\u00fcrrer Felsenspitze,", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Wolken nah, auf d\u00fcrrer Felsenspitze,", "tokens": ["Den", "Wol\u00b7ken", "nah", ",", "auf", "d\u00fcr\u00b7rer", "Fel\u00b7sen\u00b7spit\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo nur die Eulen nisten und die Raben,", "tokens": ["Wo", "nur", "die", "Eu\u00b7len", "nis\u00b7ten", "und", "die", "Ra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Will ich der Liebe Kenotaph bestatten.", "tokens": ["Will", "ich", "der", "Lie\u00b7be", "Ke\u00b7no\u00b7taph", "be\u00b7stat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein letzter Blick zur\u00fcck von meinem Sitze:", "tokens": ["Ein", "letz\u00b7ter", "Blick", "zu\u00b7r\u00fcck", "von", "mei\u00b7nem", "Sit\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich bin allein, ich habe sie begraben,", "tokens": ["Ich", "bin", "al\u00b7lein", ",", "ich", "ha\u00b7be", "sie", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und ach! sie folgt mir nicht, wie einst der Schatten", "tokens": ["Und", "ach", "!", "sie", "folgt", "mir", "nicht", ",", "wie", "einst", "der", "Schat\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "XY", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Euridikes dem Gatten.", "tokens": ["Eu\u00b7ri\u00b7di\u00b7kes", "dem", "Gat\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Da unten liegt, dem Auge kaum erkennbar,", "tokens": ["Da", "un\u00b7ten", "liegt", ",", "dem", "Au\u00b7ge", "kaum", "er\u00b7kenn\u00b7bar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$,", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Die H\u00fctte wie ein S\u00e4rglein anzuschauen ...", "tokens": ["Die", "H\u00fct\u00b7te", "wie", "ein", "S\u00e4rg\u00b7lein", "an\u00b7zu\u00b7schau\u00b7en", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "VVIZU", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ein Schmerz durchzuckt mich t\u00f6dlich und unnennbar:", "tokens": ["Ein", "Schmerz", "durch\u00b7zuckt", "mich", "t\u00f6d\u00b7lich", "und", "un\u00b7nenn\u00b7bar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Aus mit der Liebe! Fertig mit den Frauen! \u2013", "tokens": ["Aus", "mit", "der", "Lie\u00b7be", "!", "Fer\u00b7tig", "mit", "den", "Frau\u00b7en", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$.", "NE", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Dann weiter in die Welt mit halber Seele,", "tokens": ["Dann", "wei\u00b7ter", "in", "die", "Welt", "mit", "hal\u00b7ber", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Der Ha\u00df erg\u00e4nze, was an Liebe fehle!", "tokens": ["Der", "Ha\u00df", "er\u00b7g\u00e4n\u00b7ze", ",", "was", "an", "Lie\u00b7be", "feh\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}