{"textgrid.poem.50204": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dass meinen Nahmen Du durch ", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dass meinen Nahmen Du durch ", "tokens": ["Dass", "mei\u00b7nen", "Nah\u00b7men", "Du", "durch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "APPR"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Und ich mich an Dir nicht auff andre Weise r\u00e4che,", "tokens": ["Und", "ich", "mich", "an", "Dir", "nicht", "auff", "and\u00b7re", "Wei\u00b7se", "r\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PRF", "APPR", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als dass ich allezeit von Dir ", "tokens": ["Als", "dass", "ich", "al\u00b7le\u00b7zeit", "von", "Dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gereichet mir zum Schimpff mehr als du woll gedenckst:", "tokens": ["Ge\u00b7rei\u00b7chet", "mir", "zum", "Schimpff", "mehr", "als", "du", "woll", "ge\u00b7denckst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV", "KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es l\u00e4sst die Welt sich nicht betr\u00fcgen,", "tokens": ["Es", "l\u00e4sst", "die", "Welt", "sich", "nicht", "be\u00b7tr\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}}}}