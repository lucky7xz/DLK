{"textgrid.poem.59264": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "In C.S. ... Stammbuch", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In verh\u00e4ngnisschweren Stunden,", "tokens": ["In", "ver\u00b7h\u00e4ng\u00b7nis\u00b7schwe\u00b7ren", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Streitend f\u00fcr das Vaterland,", "tokens": ["Strei\u00b7tend", "f\u00fcr", "das", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Haben wir uns br\u00fcderlich gefunden,", "tokens": ["Ha\u00b7ben", "wir", "uns", "br\u00fc\u00b7der\u00b7lich", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "In der Menge still erkannt.", "tokens": ["In", "der", "Men\u00b7ge", "still", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sieh! es ruhet nun der Degen", "tokens": ["Sieh", "!", "es", "ru\u00b7het", "nun", "der", "De\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die hohe Brandung f\u00e4llt,", "tokens": ["Und", "die", "ho\u00b7he", "Bran\u00b7dung", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich verlaufend auf den alten Wegen,", "tokens": ["Sich", "ver\u00b7lau\u00b7fend", "auf", "den", "al\u00b7ten", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und langweilig wird die Welt.", "tokens": ["Und", "lang\u00b7wei\u00b7lig", "wird", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch der Ernst der heil'gen Stunden", "tokens": ["Doch", "der", "Ernst", "der", "heil'\u00b7gen", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Waltet fort in mancher Brust,", "tokens": ["Wal\u00b7tet", "fort", "in", "man\u00b7cher", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und was sich wahrhaftig hat verbunden,", "tokens": ["Und", "was", "sich", "wahr\u00b7haf\u00b7tig", "hat", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Bleibt gesellt in Not und Lust.", "tokens": ["Bleibt", "ge\u00b7sellt", "in", "Not", "und", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Unsichtbar geschwungne Br\u00fccken", "tokens": ["Un\u00b7sicht\u00b7bar", "ge\u00b7schwung\u00b7ne", "Br\u00fc\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Halten Lieb und Lieb vereint,", "tokens": ["Hal\u00b7ten", "Lieb", "und", "Lieb", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in allen hellen Lebensblicken", "tokens": ["Und", "in", "al\u00b7len", "hel\u00b7len", "Le\u00b7bens\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Gr\u00fc\u00df ich fern den lieben Freund.", "tokens": ["Gr\u00fc\u00df", "ich", "fern", "den", "lie\u00b7ben", "Freund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und so mag der Herr dich segnen!", "tokens": ["Und", "so", "mag", "der", "Herr", "dich", "seg\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Frische Fahrt durchs Leben wild,", "tokens": ["Fri\u00b7sche", "Fahrt", "durchs", "Le\u00b7ben", "wild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gleichen Sinn und freudiges Begegnen,", "tokens": ["Glei\u00b7chen", "Sinn", "und", "freu\u00b7di\u00b7ges", "Be\u00b7geg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wo es immer Hohes gilt!", "tokens": ["Wo", "es", "im\u00b7mer", "Ho\u00b7hes", "gilt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}