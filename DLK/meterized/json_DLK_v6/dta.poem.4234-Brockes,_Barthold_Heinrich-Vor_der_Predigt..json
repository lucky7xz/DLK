{"dta.poem.4234": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vor der Predigt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft uns heut' aufs neue wieder", "tokens": ["La\u00dft", "uns", "heut'", "aufs", "neu\u00b7e", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "APPRART", "ADJA", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu des H\u00f6chsten Tempel wallen!", "tokens": ["Zu", "des", "H\u00f6chs\u00b7ten", "Tem\u00b7pel", "wal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00dft, mit Freuden, Erndte-Lieder", "tokens": ["La\u00dft", ",", "mit", "Freu\u00b7den", ",", "Ernd\u00b7te\u00b7Lie\u00b7der"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["VVIMP", "$,", "APPR", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jtzt, zur Erndte-Zeit, erschallen!", "tokens": ["Jtzt", ",", "zur", "Ernd\u00b7te\u00b7Zeit", ",", "er\u00b7schal\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "APPRART", "NN", "$,", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00dft uns rc.", "tokens": ["La\u00dft", "uns", "rc", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Was ihr so oft gew\u00fcnscht zu sehen,", "tokens": ["Was", "ihr", "so", "oft", "ge\u00b7w\u00fcnscht", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist, durch des H\u00f6chsten Huld, nunmehr geschehen.", "tokens": ["Ist", ",", "durch", "des", "H\u00f6chs\u00b7ten", "Huld", ",", "nun\u00b7mehr", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Seht! das Getrayd\u2019 ist reif. Nun sagt: Woher", "tokens": ["Seht", "!", "das", "Ge\u00b7trayd'", "ist", "reif", ".", "Nun", "sagt", ":", "Wo\u00b7her"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "$.", "ART", "NN", "VAFIN", "ADJD", "$.", "ADV", "VVFIN", "$.", "PWAV"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Ist euer unger\u00fchrt Gem\u00fchte,", "tokens": ["Ist", "eu\u00b7er", "un\u00b7ge\u00b7r\u00fchrt", "Ge\u00b7m\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie euer stumme Mund, f\u00fcr GOttes G\u00fcte,", "tokens": ["Wie", "eu\u00b7er", "stum\u00b7me", "Mund", ",", "f\u00fcr", "Got\u00b7tes", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "$,", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von allem Dank, von aller Freude leer?", "tokens": ["Von", "al\u00b7lem", "Dank", ",", "von", "al\u00b7ler", "Freu\u00b7de", "leer", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "$,", "APPR", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der grosse Geber will f\u00fcr alle Gaben", "tokens": ["Der", "gros\u00b7se", "Ge\u00b7ber", "will", "f\u00fcr", "al\u00b7le", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nichts, als ein fr\u00f6hliches, erkenntlichs Herze haben:", "tokens": ["Nichts", ",", "als", "ein", "fr\u00f6h\u00b7li\u00b7ches", ",", "er\u00b7kennt\u00b7lichs", "Her\u00b7ze", "ha\u00b7ben", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "ART", "ADJA", "$,", "PIS", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Warum denn f\u00e4llt es euch so schwehr,", "tokens": ["Wa\u00b7rum", "denn", "f\u00e4llt", "es", "euch", "so", "schwehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "An Seinen Wundern euch zu laben?", "tokens": ["An", "Sei\u00b7nen", "Wun\u00b7dern", "euch", "zu", "la\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ist denn, was euch GOtt beschehrt,", "tokens": ["Ist", "denn", ",", "was", "euch", "Gott", "be\u00b7schehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "PRELS", "PPER", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Etwan keines Dankens wehrt?", "tokens": ["Et\u00b7wan", "kei\u00b7nes", "Dan\u00b7kens", "wehrt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Es ist ja wohl unerh\u00f6rt,", "tokens": ["Es", "ist", "ja", "wohl", "un\u00b7er\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Da\u00df ihr solchen GOtt nicht ehrt,", "tokens": ["Da\u00df", "ihr", "sol\u00b7chen", "Gott", "nicht", "ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der, nur eure Lust, begehrt.", "tokens": ["Der", ",", "nur", "eu\u00b7re", "Lust", ",", "be\u00b7gehrt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "$,", "ADV", "PPOSAT", "NN", "$,", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist denn rc.", "tokens": ["Ist", "denn", "rc", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Wohin man geht und f\u00e4hrt auf allen Wegen,", "tokens": ["Wo\u00b7hin", "man", "geht", "und", "f\u00e4hrt", "auf", "al\u00b7len", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "KON", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da geht und f\u00e4hrt man itzt durch lauter Segen.", "tokens": ["Da", "geht", "und", "f\u00e4hrt", "man", "itzt", "durch", "lau\u00b7ter", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PIS", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Man siehet hinten, seitwerts, vorn", "tokens": ["Man", "sie\u00b7het", "hin\u00b7ten", ",", "seit\u00b7werts", ",", "vorn"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PIS", "VVFIN", "ADV", "$,", "ADV", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nichts, als nur wachsend Brodt im Korn.", "tokens": ["Nichts", ",", "als", "nur", "wach\u00b7send", "Brodt", "im", "Korn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "ADV", "ADJD", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es segnete der Sch\u00f6pfer unsre M\u00fch,", "tokens": ["Es", "seg\u00b7ne\u00b7te", "der", "Sch\u00f6p\u00b7fer", "uns\u00b7re", "M\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Jm frischen Heu, f\u00fcr unser Vieh.", "tokens": ["Jm", "fri\u00b7schen", "Heu", ",", "f\u00fcr", "un\u00b7ser", "Vieh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nur Er hat euch, zur Erndte-Zeit,", "tokens": ["Nur", "Er", "hat", "euch", ",", "zur", "Ernd\u00b7te\u00b7Zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PPER", "$,", "APPRART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und, wie das reife Gras gemeyt,", "tokens": ["Und", ",", "wie", "das", "rei\u00b7fe", "Gras", "ge\u00b7meyt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mit guter Witterung erfreut.", "tokens": ["Mit", "gu\u00b7ter", "Wit\u00b7te\u00b7rung", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Was eure Saat versehren k\u00f6nnen,", "tokens": ["Was", "eu\u00b7re", "Saat", "ver\u00b7seh\u00b7ren", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Das hat sie nicht versehrt.", "tokens": ["Das", "hat", "sie", "nicht", "ver\u00b7sehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Der Sonnen gar zu starkes Brennen,", "tokens": ["Der", "Son\u00b7nen", "gar", "zu", "star\u00b7kes", "Bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Zu vielen Regen, M\u00e4use, Brand", "tokens": ["Zu", "vie\u00b7len", "Re\u00b7gen", ",", "M\u00e4u\u00b7se", ",", "Brand"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Hat GOtt so gn\u00e4dig abgewandt.", "tokens": ["Hat", "Gott", "so", "gn\u00e4\u00b7dig", "ab\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Hingegen hat Er Sonnen-Schein und Regen", "tokens": ["Hin\u00b7ge\u00b7gen", "hat", "Er", "Son\u00b7nen\u00b7Schein", "und", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "KON", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.16": {"text": "Euch, in der rechten Maa\u00df, und Seinen Segen", "tokens": ["Euch", ",", "in", "der", "rech\u00b7ten", "Maa\u00df", ",", "und", "Sei\u00b7nen", "Se\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "APPR", "ART", "ADJA", "NN", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Dadurch, im Ueberflu\u00df, beschehrt.", "tokens": ["Da\u00b7durch", ",", "im", "Ue\u00b7berf\u00b7lu\u00df", ",", "be\u00b7schehrt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "$,", "APPRART", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Seht, wie itzt \u00fcberall Alleen", "tokens": ["Seht", ",", "wie", "itzt", "\u00fc\u00b7be\u00b7rall", "Al\u00b7leen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "ADV", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Vom reifenden Getrayde stehen!", "tokens": ["Vom", "rei\u00b7fen\u00b7den", "Ge\u00b7tray\u00b7de", "ste\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "H\u00f6ret doch, in jeder Aehre", "tokens": ["H\u00f6\u00b7ret", "doch", ",", "in", "je\u00b7der", "A\u00b7eh\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "APPR", "PIAT", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Sanftem Lispeln, diese Lehre:", "tokens": ["Sanf\u00b7tem", "Lis\u00b7peln", ",", "die\u00b7se", "Leh\u00b7re", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}