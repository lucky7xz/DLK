{"textgrid.poem.26537": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da\u00df edle Saaten stets nur langsam reifen", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df edle Saaten stets nur langsam reifen", "tokens": ["Da\u00df", "ed\u00b7le", "Saa\u00b7ten", "stets", "nur", "lang\u00b7sam", "rei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "ADV", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Eins ist mit Verzichten jedes Streben \u2013", "tokens": ["Und", "Eins", "ist", "mit", "Ver\u00b7zich\u00b7ten", "je\u00b7des", "Stre\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "APPR", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df heil'ger Schmerz nur weiht ein K\u00fcnstlerleben:", "tokens": ["Da\u00df", "heil'\u00b7ger", "Schmerz", "nur", "weiht", "ein", "K\u00fcnst\u00b7ler\u00b7le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr k\u00f6nnt es heute nimmermehr begreifen.", "tokens": ["Ihr", "k\u00f6nnt", "es", "heu\u00b7te", "nim\u00b7mer\u00b7mehr", "be\u00b7grei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nach hohen Zielen wollt ihr hastig schweifen,", "tokens": ["Nach", "ho\u00b7hen", "Zie\u00b7len", "wollt", "ihr", "has\u00b7tig", "schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr fordert Wein von kaum gepflanzten Reben;", "tokens": ["Ihr", "for\u00b7dert", "Wein", "von", "kaum", "ge\u00b7pflanz\u00b7ten", "Re\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Lohn wollt ihr und Genu\u00df \u2013 und auch daneben", "tokens": ["Lohn", "wollt", "ihr", "und", "Ge\u00b7nu\u00df", "\u2013", "und", "auch", "da\u00b7ne\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "KON", "NN", "$(", "KON", "ADV", "PAV"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Mit fl\u00fccht'ger Hand des Ruhmes Purpur weifen.", "tokens": ["Mit", "fl\u00fccht'\u00b7ger", "Hand", "des", "Ruh\u00b7mes", "Pur\u00b7pur", "wei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich aber sag' euch, die ihr arg verblendet:", "tokens": ["Ich", "a\u00b7ber", "sag'", "euch", ",", "die", "ihr", "arg", "ver\u00b7blen\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch hat, ob manches auch die Zeit beschnitten,", "tokens": ["Noch", "hat", ",", "ob", "man\u00b7ches", "auch", "die", "Zeit", "be\u00b7schnit\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KOUS", "PIS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das eherne Gesetz sich nicht gewendet:", "tokens": ["Das", "e\u00b7her\u00b7ne", "Ge\u00b7setz", "sich", "nicht", "ge\u00b7wen\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}, "line.2": {"text": "Und nur von ", "tokens": ["Und", "nur", "von"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "APPR"], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "F\u00fcr ihn geblutet und den Tod erlitten.\u00ab", "tokens": ["F\u00fcr", "ihn", "ge\u00b7blu\u00b7tet", "und", "den", "Tod", "er\u00b7lit\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "VVPP", "KON", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}