{"textgrid.poem.57037": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Palmstr\u00f6ms Uhr", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Palmstr\u00f6ms Uhr ist andrer Art,", "tokens": ["Palm\u00b7str\u00f6ms", "Uhr", "ist", "an\u00b7drer", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "reagiert mimosisch zart.", "tokens": ["re\u00b7a\u00b7giert", "mi\u00b7mo\u00b7sisch", "zart", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer sie bittet, wird empfangen.", "tokens": ["Wer", "sie", "bit\u00b7tet", ",", "wird", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oft schon ist sie so gegangen,", "tokens": ["Oft", "schon", "ist", "sie", "so", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "wie man herzlich sie gebeten,", "tokens": ["wie", "man", "herz\u00b7lich", "sie", "ge\u00b7be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "ist zur\u00fcck \u2013 und vorgetreten,", "tokens": ["ist", "zu\u00b7r\u00fcck", "\u2013", "und", "vor\u00b7ge\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PTKVZ", "$(", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "eine Stunde, zwei, drei Stunden,", "tokens": ["ei\u00b7ne", "Stun\u00b7de", ",", "zwei", ",", "drei", "Stun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "CARD", "$,", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "je nachdem sie mitempfunden.", "tokens": ["je", "nach\u00b7dem", "sie", "mi\u00b7temp\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Selbst als Uhr, mit ihren Zeiten,", "tokens": ["Selbst", "als", "Uhr", ",", "mit", "ih\u00b7ren", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "will sie nicht Prinzipien reiten:", "tokens": ["will", "sie", "nicht", "Prin\u00b7zi\u00b7pi\u00b7en", "rei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "NN", "VVFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Zwar ein Werk, wie allerw\u00e4rts,", "tokens": ["Zwar", "ein", "Werk", ",", "wie", "al\u00b7ler\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PWAV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "doch zugleich ein Werk \u2013 mit Herz.", "tokens": ["doch", "zu\u00b7gleich", "ein", "Werk", "\u2013", "mit", "Herz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$(", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}