{"textgrid.poem.41543": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am vierten Sonntage nach Ostern", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht eine Gnadenflamme hehr", "tokens": ["Nicht", "ei\u00b7ne", "Gna\u00b7den\u00b7flam\u00b7me", "hehr"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor deinem Volke soll ich gehn;", "tokens": ["Vor", "dei\u00b7nem", "Vol\u00b7ke", "soll", "ich", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nein ein versteinert Leben schwer,", "tokens": ["Nein", "ein", "ver\u00b7stei\u00b7nert", "Le\u00b7ben", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "VVPP", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Sodoms S\u00e4ule mu\u00df ich stehn,", "tokens": ["Wie", "So\u00b7doms", "S\u00e4u\u00b7le", "mu\u00df", "ich", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und um mich her", "tokens": ["Und", "um", "mich", "her"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUI", "PRF", "APZR"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Die Irren tr\u00e4umend schwanken sehn.", "tokens": ["Die", "Ir\u00b7ren", "tr\u00e4u\u00b7mend", "schwan\u00b7ken", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und ob auch \u00d6de mich umgiebt,", "tokens": ["Und", "ob", "auch", "\u00d6\u00b7de", "mich", "um\u00b7giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ob mich erstickt der Nebel fast,", "tokens": ["Ob", "mich", "er\u00b7stickt", "der", "Ne\u00b7bel", "fast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir Wirbelsand die Augen tr\u00fcbt,", "tokens": ["Mir", "Wir\u00b7bel\u00b7sand", "die", "Au\u00b7gen", "tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch wei\u00df ich, da\u00df mein Herz dich fa\u00dft,", "tokens": ["Doch", "wei\u00df", "ich", ",", "da\u00df", "mein", "Herz", "dich", "fa\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df es dich liebt,", "tokens": ["Da\u00df", "es", "dich", "liebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und da\u00df du mich gesendet hast.", "tokens": ["Und", "da\u00df", "du", "mich", "ge\u00b7sen\u00b7det", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Den Lebenshauch halt' ich von dir,", "tokens": ["Den", "Le\u00b7bens\u00b7hauch", "halt'", "ich", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Unsterblich hast du mich gemacht;", "tokens": ["U\u00b7nsterb\u00b7lich", "hast", "du", "mich", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Nicht Glut, nicht D\u00fcrre schadet mir.", "tokens": ["Nicht", "Glut", ",", "nicht", "D\u00fcr\u00b7re", "scha\u00b7det", "mir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PTKNEG", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich wei\u00df, ich bin in deiner Wacht,", "tokens": ["Ich", "wei\u00df", ",", "ich", "bin", "in", "dei\u00b7ner", "Wacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und mu\u00df ich hier", "tokens": ["Und", "mu\u00df", "ich", "hier"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Auch stehn wie ein Prophet der Nacht.", "tokens": ["Auch", "stehn", "wie", "ein", "Pro\u00b7phet", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "KOKOM", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich hebe meine Stimme laut,", "tokens": ["Ich", "he\u00b7be", "mei\u00b7ne", "Stim\u00b7me", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein W\u00fcstenherold f\u00fcr die Not:", "tokens": ["Ein", "W\u00fcs\u00b7ten\u00b7he\u00b7rold", "f\u00fcr", "die", "Not", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wacht auf ihr Tr\u00e4umer, aufgeschaut!", "tokens": ["Wacht", "auf", "ihr", "Tr\u00e4u\u00b7mer", ",", "auf\u00b7ge\u00b7schaut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am Himmel steigt das Morgenrot.", "tokens": ["Am", "Him\u00b7mel", "steigt", "das", "Mor\u00b7gen\u00b7rot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur aufgeschaut!", "tokens": ["Nur", "auf\u00b7ge\u00b7schaut", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Nur nicht zur\u00fcck, dort steht der Tod!", "tokens": ["Nur", "nicht", "zu\u00b7r\u00fcck", ",", "dort", "steht", "der", "Tod", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "PTKVZ", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nur aufgeschaut, nur nicht zur\u00fcck!", "tokens": ["Nur", "auf\u00b7ge\u00b7schaut", ",", "nur", "nicht", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00dft Menschenweisheit hinter euch!", "tokens": ["La\u00dft", "Men\u00b7schen\u00b7weis\u00b7heit", "hin\u00b7ter", "euch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie ist der Tod, ihr schn\u00f6des Gl\u00fcck", "tokens": ["Sie", "ist", "der", "Tod", ",", "ihr", "schn\u00f6\u00b7des", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist \u00fcbert\u00fcnchtem Grabe gleich.", "tokens": ["Ist", "\u00fc\u00b7ber\u00b7t\u00fcnch\u00b7tem", "Gra\u00b7be", "gleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O hebt den Blick!", "tokens": ["O", "hebt", "den", "Blick", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Der Himmel ist so mild und reich.", "tokens": ["Der", "Him\u00b7mel", "ist", "so", "mild", "und", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "K\u00f6nnt' ich mein Auge heben nur,", "tokens": ["K\u00f6nnt'", "ich", "mein", "Au\u00b7ge", "he\u00b7ben", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein steinern Auge zu dem Blau:", "tokens": ["Mein", "stei\u00b7nern", "Au\u00b7ge", "zu", "dem", "Blau", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie s\u00f6g' ich aus der Himmelsflur", "tokens": ["Wie", "s\u00f6g'", "ich", "aus", "der", "Him\u00b7mels\u00b7flur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So liebekrank den milden Tau!", "tokens": ["So", "lie\u00b7be\u00b7krank", "den", "mil\u00b7den", "Tau", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch hat Natur", "tokens": ["Doch", "hat", "Na\u00b7tur"], "token_info": ["word", "word", "word"], "pos": ["KON", "VAFIN", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Und Schuld verschlossen mir die Brau'.", "tokens": ["Und", "Schuld", "ver\u00b7schlos\u00b7sen", "mir", "die", "Brau'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ob nimmer sich die Rinde hebt?", "tokens": ["Ob", "nim\u00b7mer", "sich", "die", "Rin\u00b7de", "hebt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ach einmal, einmal mu\u00df es sein!", "tokens": ["Ach", "ein\u00b7mal", ",", "ein\u00b7mal", "mu\u00df", "es", "sein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$,", "ADV", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn Sodoms S\u00e4ule sich belebt,", "tokens": ["Wenn", "So\u00b7doms", "S\u00e4u\u00b7le", "sich", "be\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann bricht auch meine Stunde ein,", "tokens": ["Dann", "bricht", "auch", "mei\u00b7ne", "Stun\u00b7de", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn es durchbebt", "tokens": ["Wenn", "es", "durch\u00b7bebt"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Den armen blutberaubten Stein!", "tokens": ["Den", "ar\u00b7men", "blut\u00b7be\u00b7raub\u00b7ten", "Stein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Dann soll ich wissen, was ich bin,", "tokens": ["Dann", "soll", "ich", "wis\u00b7sen", ",", "was", "ich", "bin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Warum so todesstarr und satt;", "tokens": ["Wa\u00b7rum", "so", "to\u00b7des\u00b7starr", "und", "satt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann wei\u00df ich, was den klaren Sinn", "tokens": ["Dann", "wei\u00df", "ich", ",", "was", "den", "kla\u00b7ren", "Sinn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Getrieben zu der \u00f6den Statt;", "tokens": ["Ge\u00b7trie\u00b7ben", "zu", "der", "\u00f6\u00b7den", "Statt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann knie ich hin", "tokens": ["Dann", "knie", "ich", "hin"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Vor dem, der mich gesendet hat.", "tokens": ["Vor", "dem", ",", "der", "mich", "ge\u00b7sen\u00b7det", "hat", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}