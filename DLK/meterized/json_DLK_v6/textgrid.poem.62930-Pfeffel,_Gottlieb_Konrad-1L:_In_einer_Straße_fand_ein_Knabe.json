{"textgrid.poem.62930": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: In einer Stra\u00dfe fand ein Knabe", "genre": "verse", "period": "N.A.", "pub_year": 1779, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einer Stra\u00dfe fand ein Knabe", "tokens": ["In", "ei\u00b7ner", "Stra\u00b7\u00dfe", "fand", "ein", "Kna\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein altes Goldst\u00fcck. Schaut doch her,", "tokens": ["Ein", "al\u00b7tes", "Gold\u00b7st\u00fcck", ".", "Schaut", "doch", "her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Herrn, was ich gefunden habe!", "tokens": ["Ihr", "Herrn", ",", "was", "ich", "ge\u00b7fun\u00b7den", "ha\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rief er entz\u00fcckt. Von ungef\u00e4hr", "tokens": ["Rief", "er", "ent\u00b7z\u00fcckt", ".", "Von", "un\u00b7ge\u00b7f\u00e4hr"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "VVPP", "$.", "APPR", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sah es aus seiner Tr\u00f6delbude", "tokens": ["Sah", "es", "aus", "sei\u00b7ner", "Tr\u00f6\u00b7del\u00b7bu\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Rabbi, Namens Ephraim:", "tokens": ["Ein", "Rab\u00b7bi", ",", "Na\u00b7mens", "Eph\u00b7raim", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Bey meiner Schomme, bl\u00f6ckt der Jude", "tokens": ["Bey", "mei\u00b7ner", "Schom\u00b7me", ",", "bl\u00f6ckt", "der", "Ju\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und l\u00e4chelt Hohn und stillen Grimm,", "tokens": ["Und", "l\u00e4\u00b7chelt", "Hohn", "und", "stil\u00b7len", "Grimm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Das Ding ist falsch! Dem armen Knaben", "tokens": ["Das", "Ding", "ist", "falsch", "!", "Dem", "ar\u00b7men", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wars nun auf einmal bang ums Herz.", "tokens": ["Wars", "nun", "auf", "ein\u00b7mal", "bang", "ums", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ein wenig Zusatz mocht es haben,", "tokens": ["Ein", "we\u00b7nig", "Zu\u00b7satz", "mocht", "es", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Doch war es Gold. Der bittre Scherz,", "tokens": ["Doch", "war", "es", "Gold", ".", "Der", "bitt\u00b7re", "Scherz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Die schlaue Logik des Sophisten", "tokens": ["Die", "schlau\u00b7e", "Lo\u00b7gik", "des", "So\u00b7phis\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.14": {"text": "Schwieg nicht, bis er voll Aergerni\u00df", "tokens": ["Schwieg", "nicht", ",", "bis", "er", "voll", "A\u00b7er\u00b7ger\u00b7ni\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "KOUS", "PPER", "ADJD", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.15": {"text": "Das St\u00fcck in einen Brunnen schmi\u00df.", "tokens": ["Das", "St\u00fcck", "in", "ei\u00b7nen", "Brun\u00b7nen", "schmi\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr unberufnen Exorcisten", "tokens": ["Ihr", "un\u00b7be\u00b7ruf\u00b7nen", "Ex\u00b7or\u00b7cis\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Glaubens! Dieser harte Mann", "tokens": ["Des", "Glau\u00b7bens", "!", "Die\u00b7ser", "har\u00b7te", "Mann"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist euer Bild: ihr raubt dem Christen", "tokens": ["Ist", "eu\u00b7er", "Bild", ":", "ihr", "raubt", "dem", "Chris\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Gut, das euch nicht nutzen kann.", "tokens": ["Ein", "Gut", ",", "das", "euch", "nicht", "nut\u00b7zen", "kann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gelahrte Herrn Fragmentenschreiber,", "tokens": ["Ge\u00b7lahr\u00b7te", "Herrn", "Frag\u00b7men\u00b7ten\u00b7schrei\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "O werdet lieber Stra\u00dfenr\u00e4uber.", "tokens": ["O", "wer\u00b7det", "lie\u00b7ber", "Stra\u00b7\u00dfen\u00b7r\u00e4u\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}