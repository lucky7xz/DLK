{"textgrid.poem.25036": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich kenne dich in jedem Pochen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kenne dich in jedem Pochen", "tokens": ["Ich", "ken\u00b7ne", "dich", "in", "je\u00b7dem", "Po\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Herzens, das an meines schlug,", "tokens": ["Des", "Her\u00b7zens", ",", "das", "an", "mei\u00b7nes", "schlug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In jedem Wort, das du gesprochen,", "tokens": ["In", "je\u00b7dem", "Wort", ",", "das", "du", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In jedem Blick, in jedem Zug.", "tokens": ["In", "je\u00b7dem", "Blick", ",", "in", "je\u00b7dem", "Zug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Stirn, der Hals, drum leichten Falles", "tokens": ["Die", "Stirn", ",", "der", "Hals", ",", "drum", "leich\u00b7ten", "Fal\u00b7les"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PAV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich schlingt das schwarze Lockenhaar,", "tokens": ["Sich", "schlingt", "das", "schwar\u00b7ze", "Lo\u00b7cken\u00b7haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Allgegenw\u00e4rtig lebt das alles", "tokens": ["All\u00b7ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "lebt", "das", "al\u00b7les"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor meiner Seele immerdar.", "tokens": ["Vor", "mei\u00b7ner", "See\u00b7le", "im\u00b7mer\u00b7dar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und doch bei jedem Wiedersehen", "tokens": ["Und", "doch", "bei", "je\u00b7dem", "Wie\u00b7der\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bef\u00e4llt mich wunderbare Scheu;", "tokens": ["Be\u00b7f\u00e4llt", "mich", "wun\u00b7der\u00b7ba\u00b7re", "Scheu", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich kann nicht fassen, nicht verstehen,", "tokens": ["Ich", "kann", "nicht", "fas\u00b7sen", ",", "nicht", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df du so fremd mir scheinst, so neu.", "tokens": ["Da\u00df", "du", "so", "fremd", "mir", "scheinst", ",", "so", "neu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PPER", "VVFIN", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Durch Z\u00fcge, die ich sonst nicht schaute,", "tokens": ["Durch", "Z\u00fc\u00b7ge", ",", "die", "ich", "sonst", "nicht", "schau\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch T\u00f6ne, nie geh\u00f6rt vom Ohr,", "tokens": ["Durch", "T\u00f6\u00b7ne", ",", "nie", "ge\u00b7h\u00f6rt", "vom", "Ohr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wird m\u00e4hlich dann das Altvertraute", "tokens": ["Wird", "m\u00e4h\u00b7lich", "dann", "das", "Alt\u00b7ver\u00b7trau\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir lieblicher noch als zuvor.", "tokens": ["Mir", "lieb\u00b7li\u00b7cher", "noch", "als", "zu\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So bringt der Fr\u00fchling seine Lieder", "tokens": ["So", "bringt", "der", "Fr\u00fch\u00b7ling", "sei\u00b7ne", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Bl\u00fcten uns erst nach und nach,", "tokens": ["Und", "Bl\u00fc\u00b7ten", "uns", "erst", "nach", "und", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sch\u00f6ner jeden Morgen wieder", "tokens": ["Und", "sch\u00f6\u00b7ner", "je\u00b7den", "Mor\u00b7gen", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PIAT", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sehn wir ihn als am fr\u00fchern Tag.", "tokens": ["Sehn", "wir", "ihn", "als", "am", "fr\u00fc\u00b7hern", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "KOUS", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}