{"textgrid.poem.55762": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da\u00df mir der Vater gar so fr\u00fch gestorben,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df mir der Vater gar so fr\u00fch gestorben,", "tokens": ["Da\u00df", "mir", "der", "Va\u00b7ter", "gar", "so", "fr\u00fch", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das war f\u00fcr mich der aller\u00e4rgste Schlag;", "tokens": ["Das", "war", "f\u00fcr", "mich", "der", "al\u00b7ler\u00b7\u00e4rgs\u00b7te", "Schlag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das ist's, was ich beweine Tag f\u00fcr Tag,", "tokens": ["Das", "ist's", ",", "was", "ich", "be\u00b7wei\u00b7ne", "Tag", "f\u00fcr", "Tag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PWS", "PPER", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das ist der Grund, warum ich ganz verdorben.", "tokens": ["Das", "ist", "der", "Grund", ",", "wa\u00b7rum", "ich", "ganz", "ver\u00b7dor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ach, ungest\u00fcm und liebelechzend rollen", "tokens": ["Ach", ",", "un\u00b7ge\u00b7st\u00fcm", "und", "lie\u00b7be\u00b7lech\u00b7zend", "rol\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADJD", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Flammenb\u00e4che meines wilden Bluts,", "tokens": ["Die", "Flam\u00b7men\u00b7b\u00e4\u00b7che", "mei\u00b7nes", "wil\u00b7den", "Bluts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und jene Gabe dann des Wankelmuts", "tokens": ["Und", "je\u00b7ne", "Ga\u00b7be", "dann", "des", "Wan\u00b7kel\u00b7muts"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und jenes sieche, trotzig-scheue Wollen ...", "tokens": ["Und", "je\u00b7nes", "sie\u00b7che", ",", "trot\u00b7zig\u00b7scheue", "Wol\u00b7len", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "O h\u00e4tte meinem ersten Wollustlallen", "tokens": ["O", "h\u00e4t\u00b7te", "mei\u00b7nem", "ers\u00b7ten", "Wol\u00b7lust\u00b7lal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Vaters g\u00fctig starke Hand gewehrt,", "tokens": ["Des", "Va\u00b7ters", "g\u00fc\u00b7tig", "star\u00b7ke", "Hand", "ge\u00b7wehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So w\u00e4re meine Stirn nicht lustentehrt,", "tokens": ["So", "w\u00e4\u00b7re", "mei\u00b7ne", "Stirn", "nicht", "lus\u00b7ten\u00b7tehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und nimmer w\u00e4r ich gar so tief gefallen.", "tokens": ["Und", "nim\u00b7mer", "w\u00e4r", "ich", "gar", "so", "tief", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}