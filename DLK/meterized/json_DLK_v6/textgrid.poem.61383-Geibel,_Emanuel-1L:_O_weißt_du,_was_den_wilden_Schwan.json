{"textgrid.poem.61383": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: O wei\u00dft du, was den wilden Schwan", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wei\u00dft du, was den wilden Schwan", "tokens": ["O", "wei\u00dft", "du", ",", "was", "den", "wil\u00b7den", "Schwan"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Treibt \u00fcbers Meer in s\u00fcdlich Land,", "tokens": ["Treibt", "\u00fc\u00b7bers", "Meer", "in", "s\u00fcd\u00b7lich", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "APPR", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was aus dem Schacht zum Licht hinan", "tokens": ["Was", "aus", "dem", "Schacht", "zum", "Licht", "hi\u00b7nan"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das B\u00e4chlein zwingt durch Kies und Sand?", "tokens": ["Das", "B\u00e4ch\u00b7lein", "zwingt", "durch", "Kies", "und", "Sand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kannst du es sagen:", "tokens": ["Kannst", "du", "es", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Dann magst du fragen,", "tokens": ["Dann", "magst", "du", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Was mich an deine Schritte bannt.", "tokens": ["Was", "mich", "an", "dei\u00b7ne", "Schrit\u00b7te", "bannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dann magst du fragen auch, warum", "tokens": ["Dann", "magst", "du", "fra\u00b7gen", "auch", ",", "wa\u00b7rum"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "ADV", "$,", "PWAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dies Auge brennt, das stets gelacht,", "tokens": ["Dies", "Au\u00b7ge", "brennt", ",", "das", "stets", "ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Warum der kecke Mund ward stumm.", "tokens": ["Wa\u00b7rum", "der", "ke\u00b7cke", "Mund", "ward", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Becher mehr mich fr\u00f6hlich macht,", "tokens": ["Kein", "Be\u00b7cher", "mehr", "mich", "fr\u00f6h\u00b7lich", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Warum in Sorgen", "tokens": ["Wa\u00b7rum", "in", "Sor\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Mich trifft der Morgen", "tokens": ["Mich", "trifft", "der", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Und schlaflos die gestirnte Nacht.", "tokens": ["Und", "schlaf\u00b7los", "die", "ge\u00b7stirn\u00b7te", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich wei\u00df nur das: Tr\u00fcb oder froh,", "tokens": ["Ich", "wei\u00df", "nur", "das", ":", "Tr\u00fcb", "o\u00b7der", "froh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PDS", "$.", "NN", "KON", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Ein Schicksal ist's, ich gab mich drein;", "tokens": ["Ein", "Schick\u00b7sal", "ist's", ",", "ich", "gab", "mich", "drein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In meinen Sternen flammt' es so,", "tokens": ["In", "mei\u00b7nen", "Ster\u00b7nen", "flammt'", "es", "so", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Lieb' ist Lieb' in Lust und Pein.", "tokens": ["Und", "Lieb'", "ist", "Lieb'", "in", "Lust", "und", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum duld' es stille,", "tokens": ["Drum", "duld'", "es", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Da\u00df all mein Wille", "tokens": ["Da\u00df", "all", "mein", "Wil\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Um dich sich dreht: Nimm hin, was dein!", "tokens": ["Um", "dich", "sich", "dreht", ":", "Nimm", "hin", ",", "was", "dein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PRF", "VVFIN", "$.", "NE", "PTKVZ", "$,", "PRELS", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}