{"textgrid.poem.66904": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit Riesenkraft mu\u00df ich mich stemmen,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Riesenkraft mu\u00df ich mich stemmen,", "tokens": ["Mit", "Rie\u00b7sen\u00b7kraft", "mu\u00df", "ich", "mich", "stem\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich in wilden Streit nicht st\u00fcrze,", "tokens": ["Da\u00df", "ich", "in", "wil\u00b7den", "Streit", "nicht", "st\u00fcr\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Willensbarrieren hemmen,", "tokens": ["Mit", "Wil\u00b7lens\u00b7bar\u00b7rie\u00b7ren", "hem\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da\u00df ich mein Wesen nicht verk\u00fcrze.", "tokens": ["Da\u00df", "ich", "mein", "We\u00b7sen", "nicht", "ver\u00b7k\u00fcr\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O schwere Not, sich rein zu bilden,", "tokens": ["O", "schwe\u00b7re", "Not", ",", "sich", "rein", "zu", "bil\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df sch\u00f6n die Symphonie erklingt,", "tokens": ["Da\u00df", "sch\u00f6n", "die", "Sym\u00b7pho\u00b7nie", "er\u00b7klingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So lang auf rauchenden Gefilden", "tokens": ["So", "lang", "auf", "rau\u00b7chen\u00b7den", "Ge\u00b7fil\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Menschheit leidet, blutet, ringt!", "tokens": ["Die", "Menschheit", "lei\u00b7det", ",", "blu\u00b7tet", ",", "ringt", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aus allen H\u00f6hlen losgelassen,", "tokens": ["Aus", "al\u00b7len", "H\u00f6h\u00b7len", "los\u00b7ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit weltverw\u00fcstenden Gewalten", "tokens": ["Mit", "welt\u00b7ver\u00b7w\u00fcs\u00b7ten\u00b7den", "Ge\u00b7wal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PWAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "W\u00e4lzen die ekelhaften Massen", "tokens": ["W\u00e4l\u00b7zen", "die", "e\u00b7kel\u00b7haf\u00b7ten", "Mas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Sich auf die sonnigen Gestalten.", "tokens": ["Sich", "auf", "die", "son\u00b7ni\u00b7gen", "Ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Krone Gottes frech im Nacken,", "tokens": ["Die", "Kro\u00b7ne", "Got\u00b7tes", "frech", "im", "Na\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im Arm das Schwert der Barbarei,", "tokens": ["Im", "Arm", "das", "Schwert", "der", "Bar\u00b7ba\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auf ihren z\u00e4hen schwarzen Kracken", "tokens": ["Auf", "ih\u00b7ren", "z\u00e4\u00b7hen", "schwar\u00b7zen", "Kra\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Antrappeln sie mit Geierschrei.", "tokens": ["An\u00b7trap\u00b7peln", "sie", "mit", "Gei\u00b7er\u00b7schrei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihr Mantelsack voll Anleihscheinen,", "tokens": ["Ihr", "Man\u00b7tel\u00b7sack", "voll", "An\u00b7leih\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den L\u00fcgenfusel in der Flasche,", "tokens": ["Den", "L\u00fc\u00b7gen\u00b7fu\u00b7sel", "in", "der", "Fla\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Klassenhochmut in der einen,", "tokens": ["Den", "Klas\u00b7sen\u00b7hoch\u00b7mut", "in", "der", "ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Volksha\u00df in der andern Tasche,", "tokens": ["Den", "Volks\u00b7ha\u00df", "in", "der", "an\u00b7dern", "Ta\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein steppenrussisch angeeistes,", "tokens": ["Ein", "step\u00b7pen\u00b7rus\u00b7sisch", "an\u00b7ge\u00b7eis\u00b7tes", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein heulend hunnisches Geschlecht,", "tokens": ["Ein", "heu\u00b7lend", "hun\u00b7ni\u00b7sches", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die St\u00fctzen der Gesellschaft hei\u00dft es,", "tokens": ["Die", "St\u00fct\u00b7zen", "der", "Ge\u00b7sell\u00b7schaft", "hei\u00dft", "es", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "S\u00e4bel und Knute hei\u00dft ihr Recht.", "tokens": ["S\u00e4\u00b7bel", "und", "Knu\u00b7te", "hei\u00dft", "ihr", "Recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "In ihren rohen Z\u00fcgen zittert", "tokens": ["In", "ih\u00b7ren", "ro\u00b7hen", "Z\u00fc\u00b7gen", "zit\u00b7tert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der tolle Rausch der Bluttriumphe,", "tokens": ["Der", "tol\u00b7le", "Rausch", "der", "Blutt\u00b7ri\u00b7um\u00b7phe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und tief in ihren Augen wittert", "tokens": ["Und", "tief", "in", "ih\u00b7ren", "Au\u00b7gen", "wit\u00b7tert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Wahnsinn schon, der sch\u00f6nheitsstumpfe.", "tokens": ["Der", "Wahn\u00b7sinn", "schon", ",", "der", "sch\u00f6n\u00b7heits\u00b7stump\u00b7fe", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dem Gaul die Sporen in die Weichen!", "tokens": ["Dem", "Gaul", "die", "Spo\u00b7ren", "in", "die", "Wei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der P\u00f6bel meutert? Masse murrt?", "tokens": ["Der", "P\u00f6\u00b7bel", "meu\u00b7tert", "?", "Mas\u00b7se", "murrt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nun denn! Auch \u00fcber ihre Leichen", "tokens": ["Nun", "denn", "!", "Auch", "\u00fc\u00b7ber", "ih\u00b7re", "Lei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$.", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gewalt des Goldes, der Geburt!", "tokens": ["Ge\u00b7walt", "des", "Gol\u00b7des", ",", "der", "Ge\u00b7burt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Vor ihrem freien Geisteslager,", "tokens": ["Vor", "ih\u00b7rem", "frei\u00b7en", "Geis\u00b7tes\u00b7la\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Drauf hell der Wahrheit Wimpel wehen,", "tokens": ["Drauf", "hell", "der", "Wahr\u00b7heit", "Wim\u00b7pel", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Stirn der Horde ihrer Plager", "tokens": ["Die", "Stirn", "der", "Hor\u00b7de", "ih\u00b7rer", "Pla\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hoch zugewandt, die Edlen stehen.", "tokens": ["Hoch", "zu\u00b7ge\u00b7wandt", ",", "die", "Ed\u00b7len", "ste\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie schleudern keine Mordgeschosse,", "tokens": ["Sie", "schleu\u00b7dern", "kei\u00b7ne", "Mord\u00b7ge\u00b7schos\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Buch, ein Werkzeug Wehr und Schild \u2013", "tokens": ["Ein", "Buch", ",", "ein", "Werk\u00b7zeug", "Wehr", "und", "Schild", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Trotz bieten sie dem Sklaventrosse,", "tokens": ["Trotz", "bie\u00b7ten", "sie", "dem", "Skla\u00b7ven\u00b7tros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der sich heranw\u00e4lzt meutewild.", "tokens": ["Der", "sich", "her\u00b7an\u00b7w\u00e4lzt", "meu\u00b7te\u00b7wild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ein j\u00e4hes Weh, ein schwerer Schrecken", "tokens": ["Ein", "j\u00e4\u00b7hes", "Weh", ",", "ein", "schwe\u00b7rer", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ersch\u00fcttert mich bei diesem Bilde,", "tokens": ["Er\u00b7sch\u00fct\u00b7tert", "mich", "bei", "die\u00b7sem", "Bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich seh zum Todeskampf sich strecken", "tokens": ["Ich", "seh", "zum", "To\u00b7des\u00b7kampf", "sich", "stre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Recht, die Menschlichkeit, die Milde.", "tokens": ["Das", "Recht", ",", "die", "Menschlich\u00b7keit", ",", "die", "Mil\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die aufgebrachten Heunen rasen", "tokens": ["Die", "auf\u00b7ge\u00b7brach\u00b7ten", "Heu\u00b7nen", "ra\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Koppeln auf die Opfer ein,", "tokens": ["Mit", "Kop\u00b7peln", "auf", "die", "Op\u00b7fer", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die H\u00f6rner des Entsetzens blasen,", "tokens": ["Die", "H\u00f6r\u00b7ner", "des", "Ent\u00b7set\u00b7zens", "bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und schluchzend sterben die Schalmein.", "tokens": ["Und", "schluch\u00b7zend", "ster\u00b7ben", "die", "Schal\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}