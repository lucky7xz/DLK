{"textgrid.poem.24050": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Die Herberge", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du kaltes Haus voll m\u00fcder Dunkelheit ...", "tokens": ["Du", "kal\u00b7tes", "Haus", "voll", "m\u00fc\u00b7der", "Dun\u00b7kel\u00b7heit", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Spinnweben\u00fcberschleiert schl\u00e4ft in dir die Zeit;", "tokens": ["Spinn\u00b7we\u00b7ben\u00b7\u00fc\u00b7bersc\u00b7hlei\u00b7ert", "schl\u00e4ft", "in", "dir", "die", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf weichen Socken schleicht in dir der Tod;", "tokens": ["Auf", "wei\u00b7chen", "So\u00b7cken", "schleicht", "in", "dir", "der", "Tod", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Stets um dich D\u00e4mmerung; das Morgenrot", "tokens": ["Stets", "um", "dich", "D\u00e4m\u00b7me\u00b7rung", ";", "das", "Mor\u00b7gen\u00b7rot"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "PPER", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Trifft deine Schindeln nicht, die bleich wie Blei;", "tokens": ["Trifft", "dei\u00b7ne", "Schin\u00b7deln", "nicht", ",", "die", "bleich", "wie", "Blei", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "$,", "PRELS", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In weiten Kreisen bangt das Leben dir vorbei.", "tokens": ["In", "wei\u00b7ten", "Krei\u00b7sen", "bangt", "das", "Le\u00b7ben", "dir", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich aber ging hinein und sa\u00df in dir zu Gast ...", "tokens": ["Ich", "a\u00b7ber", "ging", "hin\u00b7ein", "und", "sa\u00df", "in", "dir", "zu", "Gast", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PTKVZ", "KON", "VVFIN", "APPR", "PPER", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Oh wie du mich so lieb und lind umfangen hast!", "tokens": ["Oh", "wie", "du", "mich", "so", "lieb", "und", "lind", "um\u00b7fan\u00b7gen", "hast", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["XY", "PWAV", "PPER", "PRF", "ADV", "ADJD", "KON", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich lehnte meinen Kopf an deine graue Wand,", "tokens": ["Ich", "lehn\u00b7te", "mei\u00b7nen", "Kopf", "an", "dei\u00b7ne", "grau\u00b7e", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mir streichelte das Kinn des Hausherrn harte Hand.", "tokens": ["Mir", "strei\u00b7chel\u00b7te", "das", "Kinn", "des", "Haus\u00b7herrn", "har\u00b7te", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sein Auge lud mich ein zu wei\u00dfer Lagerstatt,", "tokens": ["Sein", "Au\u00b7ge", "lud", "mich", "ein", "zu", "wei\u00b7\u00dfer", "La\u00b7ger\u00b7statt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "PTKZU", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da sank ich federntief, von weichem Wehe matt.", "tokens": ["Da", "sank", "ich", "fe\u00b7dern\u00b7tief", ",", "von", "wei\u00b7chem", "We\u00b7he", "matt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Krankenw\u00e4rter Tod sang in den Schlaf mich ein,", "tokens": ["Der", "Kran\u00b7ken\u00b7w\u00e4r\u00b7ter", "Tod", "sang", "in", "den", "Schlaf", "mich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da ward das stille Gl\u00fcck, das ... stille ...", "tokens": ["Da", "ward", "das", "stil\u00b7le", "Gl\u00fcck", ",", "das", "...", "stil\u00b7le", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "$(", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Gl\u00fcck ward mein.", "tokens": ["Gl\u00fcck", "ward", "mein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Es hauchte um mich her ein Atem moderbang,", "tokens": ["Es", "hauch\u00b7te", "um", "mich", "her", "ein", "A\u00b7tem", "mo\u00b7der\u00b7bang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "APZR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und eine Stimme dumpf aus Weltenweiten sang:", "tokens": ["Und", "ei\u00b7ne", "Stim\u00b7me", "dumpf", "aus", "Wel\u00b7ten\u00b7wei\u00b7ten", "sang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u00bbhin\u00fcber Seele nun, spann deine Fl\u00fcgel weit,", "tokens": ["\u00bb", "hin\u00b7\u00fc\u00b7ber", "See\u00b7le", "nun", ",", "spann", "dei\u00b7ne", "Fl\u00fc\u00b7gel", "weit", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NN", "ADV", "$,", "VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+++-+-+", "measure": "unknown.measure.septa"}, "line.13": {"text": "Schwimm schwanenfittichstill in blaue Ewigkeit.", "tokens": ["Schwimm", "schwa\u00b7nen\u00b7fit\u00b7tich\u00b7still", "in", "blau\u00b7e", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "H\u00f6rst du den leisen Ton? Das ist der letzte Schlag", "tokens": ["H\u00f6rst", "du", "den", "lei\u00b7sen", "Ton", "?", "Das", "ist", "der", "letz\u00b7te", "Schlag"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "PDS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Vom Turm der Erdennacht, nun goldet dir der Tag,", "tokens": ["Vom", "Turm", "der", "Er\u00b7den\u00b7nacht", ",", "nun", "gol\u00b7det", "dir", "der", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der nie sein Blut vergie\u00dft ins Abendr\u00f6tenmeer ...\u00ab", "tokens": ["Der", "nie", "sein", "Blut", "ver\u00b7gie\u00dft", "ins", "A\u00b7bend\u00b7r\u00f6\u00b7ten\u00b7meer", "...", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$(", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Da hob ich mich in Angst von meinem Pf\u00fchle schwer.", "tokens": ["Da", "hob", "ich", "mich", "in", "Angst", "von", "mei\u00b7nem", "Pf\u00fch\u00b7le", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Fort! Fort! Von hier hinaus! Hinaus ins helle Licht!", "tokens": ["Fort", "!", "Fort", "!", "Von", "hier", "hin\u00b7aus", "!", "Hin\u00b7aus", "ins", "hel\u00b7le", "Licht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "APPR", "ADV", "PTKVZ", "$.", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Noch einmal sah ich in des Hausherrn bleich Gesicht.", "tokens": ["Noch", "ein\u00b7mal", "sah", "ich", "in", "des", "Haus\u00b7herrn", "bleich", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Das l\u00e4chelte. Mir war: Dies L\u00e4cheln legte sich", "tokens": ["Das", "l\u00e4\u00b7chel\u00b7te", ".", "Mir", "war", ":", "Dies", "L\u00e4\u00b7cheln", "leg\u00b7te", "sich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$.", "PPER", "VAFIN", "$.", "PDS", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ins Herz mir wie ein Wort, kalt: ", "tokens": ["Ins", "Herz", "mir", "wie", "ein", "Wort", ",", "kalt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "KOKOM", "ART", "NN", "$,", "ADJD", "$."], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.22": {"text": "Ich schritt auf schwankem Fu\u00df, ich taumelte hinaus,", "tokens": ["Ich", "schritt", "auf", "schwan\u00b7kem", "Fu\u00df", ",", "ich", "tau\u00b7mel\u00b7te", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ich wandte meinen Blick: Versunken war das Haus.", "tokens": ["Ich", "wand\u00b7te", "mei\u00b7nen", "Blick", ":", "Ver\u00b7sun\u00b7ken", "war", "das", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$.", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und eine Grube lag an seiner Stelle, tief ...", "tokens": ["Und", "ei\u00b7ne", "Gru\u00b7be", "lag", "an", "sei\u00b7ner", "Stel\u00b7le", ",", "tief", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Mir wars, als obs aus ihr leis meinen Name rief.", "tokens": ["Mir", "wars", ",", "als", "obs", "aus", "ihr", "leis", "mei\u00b7nen", "Na\u00b7me", "rief", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "APPR", "PPER", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}