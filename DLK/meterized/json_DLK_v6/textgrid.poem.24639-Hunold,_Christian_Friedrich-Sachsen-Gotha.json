{"textgrid.poem.24639": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Sachsen-Gotha", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bi\u00df an der Sternen Hau\u00df tr\u00e4gt mich die Tapfferkeit/", "tokens": ["Bi\u00df", "an", "der", "Ster\u00b7nen", "Hau\u00df", "tr\u00e4gt", "mich", "die", "Tapf\u00b7fer\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "NN", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da sich vors Reiches Wohl mein Printz zum Opffer beut.", "tokens": ["Da", "sich", "vors", "Rei\u00b7ches", "Wohl", "mein", "Printz", "zum", "Opf\u00b7fer", "beut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPRART", "NN", "ADV", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Printz Wilhelm/ welcher l\u00e4ngst dem Purpur/ den er tr\u00e4get/", "tokens": ["Printz", "Wil\u00b7helm", "/", "wel\u00b7cher", "l\u00e4ngst", "dem", "Pur\u00b7pur", "/", "den", "er", "tr\u00e4\u00b7get", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "PRELS", "ADV", "ART", "NN", "$(", "PRELS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "So gro\u00df und hohen Glantz/ als er ihm beygeleget.", "tokens": ["So", "gro\u00df", "und", "ho\u00b7hen", "Glantz", "/", "als", "er", "ihm", "bey\u00b7ge\u00b7le\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJA", "NN", "$(", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Klug und Wei\u00dfheit so/ wie seinen Degen liebt/", "tokens": ["Der", "Klug", "und", "Wei\u00df\u00b7heit", "so", "/", "wie", "sei\u00b7nen", "De\u00b7gen", "liebt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADV", "$(", "KOKOM", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und sich dem ", "tokens": ["Und", "sich", "dem"], "token_info": ["word", "word", "word"], "pos": ["KON", "PRF", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Der (welch ein rarer Ruhm/ so gro\u00dfe Helden zieret/)", "tokens": ["Der", "(", "welch", "ein", "ra\u00b7rer", "Ruhm", "/", "so", "gro\u00b7\u00dfe", "Hel\u00b7den", "zie\u00b7ret", "/", ")"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "$(", "PWAT", "ART", "ADJA", "NN", "$(", "ADV", "ADJA", "NN", "VVFIN", "$(", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den Himmel in der Brust mit sich zu Felde f\u00fchret.", "tokens": ["Den", "Him\u00b7mel", "in", "der", "Brust", "mit", "sich", "zu", "Fel\u00b7de", "f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPR", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Auf den Ernestens Geist/ wo nicht gedoppelt/ ruht/", "tokens": ["Auf", "den", "Er\u00b7nes\u00b7tens", "Geist", "/", "wo", "nicht", "ge\u00b7dop\u00b7pelt", "/", "ruht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$(", "PWAV", "PTKNEG", "VVPP", "$(", "VVFIN", "$("], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Wo From- und Tapfer-seyn fast gleiche Wunder thut/", "tokens": ["Wo", "From", "und", "Tap\u00b7fer\u00b7seyn", "fast", "glei\u00b7che", "Wun\u00b7der", "thut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "TRUNC", "KON", "NN", "ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Friedrichs Redlichkeit sich auserwehlt zum Orden/", "tokens": ["Der", "Fried\u00b7richs", "Red\u00b7lich\u00b7keit", "sich", "au\u00b7ser\u00b7wehlt", "zum", "Or\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "PRF", "VVPP", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und unserm David ist zum Jonathan geworden.", "tokens": ["Und", "un\u00b7serm", "Da\u00b7vid", "ist", "zum", "Jo\u00b7na\u00b7than", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NE", "VAFIN", "APPRART", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Den Lieb und Ehr-Furcht k\u00fc\u00dft/ um den mein ruhig Land", "tokens": ["Den", "Lieb", "und", "Ehr\u00b7Furcht", "k\u00fc\u00dft", "/", "um", "den", "mein", "ru\u00b7hig", "Land"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$(", "APPR", "ART", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Unruhig werden mu\u00df/ weil es ihm unbekant/", "tokens": ["Un\u00b7ru\u00b7hig", "wer\u00b7den", "mu\u00df", "/", "weil", "es", "ihm", "un\u00b7be\u00b7kant", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "VMFIN", "$(", "KOUS", "PPER", "PPER", "ADJD", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.15": {"text": "Ob/ was aus Toulon wird auf meinen Printzen krachen/", "tokens": ["Ob", "/", "was", "aus", "Tou\u00b7lon", "wird", "auf", "mei\u00b7nen", "Print\u00b7zen", "kra\u00b7chen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "PWS", "APPR", "NE", "VAFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Den/ der unsterblich schon/ auch sterblich k\u00f6nne machen?", "tokens": ["Den", "/", "der", "uns\u00b7terb\u00b7lich", "schon", "/", "auch", "sterb\u00b7lich", "k\u00f6n\u00b7ne", "ma\u00b7chen", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ART", "ADJD", "ADV", "$(", "ADV", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}