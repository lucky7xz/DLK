{"textgrid.poem.40154": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Natur und Sehnsucht", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlaflos lieg' ich, wie im Fieber", "tokens": ["Schlaf\u00b7los", "lieg'", "ich", ",", "wie", "im", "Fie\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "PWAV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "starr' ich in die Schatten hin,", "tokens": ["starr'", "ich", "in", "die", "Schat\u00b7ten", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "ob mir eben nicht ihr lieber", "tokens": ["ob", "mir", "e\u00b7ben", "nicht", "ihr", "lie\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Augenstrahl ergl\u00e4nzte drin,", "tokens": ["Au\u00b7gen\u00b7strahl", "er\u00b7gl\u00e4nz\u00b7te", "drin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "ob nicht solche Gr\u00fc\u00dfe br\u00e4chten", "tokens": ["ob", "nicht", "sol\u00b7che", "Gr\u00fc\u00b7\u00dfe", "br\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "PIAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "auch zwei ", "tokens": ["auch", "zwei"], "token_info": ["word", "word"], "pos": ["ADV", "CARD"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "wie in heitren Sommern\u00e4chten", "tokens": ["wie", "in", "hei\u00b7tren", "Som\u00b7mer\u00b7n\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "f\u00e4llt vom Himmel Stern zu Stern.", "tokens": ["f\u00e4llt", "vom", "Him\u00b7mel", "Stern", "zu", "Stern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}