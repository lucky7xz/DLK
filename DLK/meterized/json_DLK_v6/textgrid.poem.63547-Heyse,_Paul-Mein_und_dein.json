{"textgrid.poem.63547": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Mein und dein", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zieh ein zu allen Toren,", "tokens": ["Zieh", "ein", "zu", "al\u00b7len", "To\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geliebtes Gl\u00fcck, zieh ein!", "tokens": ["Ge\u00b7lieb\u00b7tes", "Gl\u00fcck", ",", "zieh", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du mir zum Trost erkoren,", "tokens": ["Du", "mir", "zum", "Trost", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nimm alles hin, was mein!", "tokens": ["Nimm", "al\u00b7les", "hin", ",", "was", "mein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PIS", "PTKVZ", "$,", "PRELS", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du mir zum Trost erkoren,", "tokens": ["Du", "mir", "zum", "Trost", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich leb' in dir allein.", "tokens": ["Ich", "leb'", "in", "dir", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr dich zur Welt geboren \u2013", "tokens": ["F\u00fcr", "dich", "zur", "Welt", "ge\u00b7bo\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ach, was an mir ist mein?", "tokens": ["Ach", ",", "was", "an", "mir", "ist", "mein", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PRELS", "APPR", "PPER", "VAFIN", "PPOSAT", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.3": {"line.1": {"text": "F\u00fcr dich zur Welt geboren,", "tokens": ["F\u00fcr", "dich", "zur", "Welt", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kenn' ich kein andres Sein;", "tokens": ["Kenn'", "ich", "kein", "and\u00b7res", "Sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "PIS", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht frag' ich wie die Toren:", "tokens": ["Nicht", "frag'", "ich", "wie", "die", "To\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ach, was an ", "tokens": ["Ach", ",", "was", "an"], "token_info": ["word", "punct", "word", "word"], "pos": ["ITJ", "$,", "PRELS", "APPR"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Nicht frag' ich, wie die Toren;", "tokens": ["Nicht", "frag'", "ich", ",", "wie", "die", "To\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und riefe die H\u00f6lle nein:", "tokens": ["Und", "rie\u00b7fe", "die", "H\u00f6l\u00b7le", "nein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKANT", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wer sich ins All verloren,", "tokens": ["Wer", "sich", "ins", "All", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was gilt ihm mein und dein?", "tokens": ["Was", "gilt", "ihm", "mein", "und", "dein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "KON", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}