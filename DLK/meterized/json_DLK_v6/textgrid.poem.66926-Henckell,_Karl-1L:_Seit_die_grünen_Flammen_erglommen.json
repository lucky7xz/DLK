{"textgrid.poem.66926": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seit die gr\u00fcnen Flammen erglommen", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seit die gr\u00fcnen Flammen erglommen", "tokens": ["Seit", "die", "gr\u00fc\u00b7nen", "Flam\u00b7men", "er\u00b7glom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Aus den W\u00e4ldern vom weichen Wind \u2013", "tokens": ["Aus", "den", "W\u00e4l\u00b7dern", "vom", "wei\u00b7chen", "Wind", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Herzen, starr und schicksalbeklommen,", "tokens": ["Her\u00b7zen", ",", "starr", "und", "schick\u00b7sal\u00b7be\u00b7klom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Habt ihr wohl, trostlauschend, vernommen,", "tokens": ["Habt", "ihr", "wohl", ",", "trost\u00b7lau\u00b7schend", ",", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$,", "VVPP", "$,", "VVPP", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Was durch heilende L\u00fcfte der Erde rinnt?", "tokens": ["Was", "durch", "hei\u00b7len\u00b7de", "L\u00fcf\u00b7te", "der", "Er\u00b7de", "rinnt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Lindernd durch leidende Lande", "tokens": ["Lin\u00b7dernd", "durch", "lei\u00b7den\u00b7de", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Rinnt es, wie hoffnungschwellend Gel\u00e4ut:", "tokens": ["Rinnt", "es", ",", "wie", "hoff\u00b7nung\u00b7schwel\u00b7lend", "Ge\u00b7l\u00e4ut", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADJD", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "\u00bbl\u00f6st euch, l\u00e4hmende Bande,", "tokens": ["\u00bb", "l\u00f6st", "euch", ",", "l\u00e4h\u00b7men\u00b7de", "Ban\u00b7de", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Die ihr mit Schmach und mit Schande", "tokens": ["Die", "ihr", "mit", "Schmach", "und", "mit", "Schan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "APPR", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Nachtd\u00e4monisch das Leben qu\u00e4lt und bedr\u00e4ut!\u00ab", "tokens": ["Nacht\u00b7d\u00e4\u00b7mo\u00b7nisch", "das", "Le\u00b7ben", "qu\u00e4lt", "und", "be\u00b7dr\u00e4ut", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "KON", "ADJD", "$.", "$("], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Seit die gr\u00fcnen Flammen erglommen", "tokens": ["Seit", "die", "gr\u00fc\u00b7nen", "Flam\u00b7men", "er\u00b7glom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Aus den W\u00e4ldern vom weichen Wind \u2013", "tokens": ["Aus", "den", "W\u00e4l\u00b7dern", "vom", "wei\u00b7chen", "Wind", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Haben Herzen ein L\u00e4uten, lieblich wie Botschaft der Engel vernommen,", "tokens": ["Ha\u00b7ben", "Her\u00b7zen", "ein", "L\u00e4u\u00b7ten", ",", "lieb\u00b7lich", "wie", "Bot\u00b7schaft", "der", "En\u00b7gel", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "NN", "$,", "ADJD", "KOKOM", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+--+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Da\u00df die Tage der Pfingsten zu uns kommen,", "tokens": ["Da\u00df", "die", "Ta\u00b7ge", "der", "Pfings\u00b7ten", "zu", "uns", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wo die Feste der Menschen wieder Feste des fr\u00f6hlichen Lebens sind.", "tokens": ["Wo", "die", "Fes\u00b7te", "der", "Men\u00b7schen", "wie\u00b7der", "Fes\u00b7te", "des", "fr\u00f6h\u00b7li\u00b7chen", "Le\u00b7bens", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "ADV", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "--+--+-+-+--+--+-+", "measure": "anapaest.di.plus"}}}}}