{"textgrid.poem.57697": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Mittagsluft", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Bienen summen im Heidekraut,", "tokens": ["Die", "Bie\u00b7nen", "sum\u00b7men", "im", "Hei\u00b7de\u00b7kraut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es bebt die Mittagsluft,", "tokens": ["Es", "bebt", "die", "Mit\u00b7tags\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus all den roten Bl\u00fctchen steigt", "tokens": ["Aus", "all", "den", "ro\u00b7ten", "Bl\u00fct\u00b7chen", "steigt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein voller Honigduft.", "tokens": ["Ein", "vol\u00b7ler", "Ho\u00b7nig\u00b7duft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein kleiner blauer Schmetterling", "tokens": ["Ein", "klei\u00b7ner", "blau\u00b7er", "Schmet\u00b7ter\u00b7ling"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der flog auf deine Hand,", "tokens": ["Der", "flog", "auf", "dei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Sonne durch den Ginsterbusch", "tokens": ["Die", "Son\u00b7ne", "durch", "den", "Gins\u00b7ter\u00b7busch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch deine Finger fand.", "tokens": ["Auch", "dei\u00b7ne", "Fin\u00b7ger", "fand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Fort flog der blaue Schmetterling", "tokens": ["Fort", "flog", "der", "blau\u00b7e", "Schmet\u00b7ter\u00b7ling"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der Sonnenstrahl verschwand,", "tokens": ["Der", "Son\u00b7nen\u00b7strahl", "ver\u00b7schwand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und meine Lippen ruhen jetzt", "tokens": ["Und", "mei\u00b7ne", "Lip\u00b7pen", "ru\u00b7hen", "jetzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf deiner wei\u00dfen Hand.", "tokens": ["Auf", "dei\u00b7ner", "wei\u00b7\u00dfen", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}