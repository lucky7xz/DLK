{"dta.poem.16940": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "41.  An den Creutzfliehenden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Ach Kind ists dir denn auch zur Zeit noch nicht bewust/", "tokens": ["Ach", "Kind", "ists", "dir", "denn", "auch", "zur", "Zeit", "noch", "nicht", "be\u00b7wust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VAFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "ADV", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df man nicht jmmer liegt an unsers HErren Brust?", "tokens": ["Da\u00df", "man", "nicht", "jm\u00b7mer", "liegt", "an", "un\u00b7sers", "Her\u00b7ren", "Brust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wen Er am liebsten hat/ der mu\u00df in Creutz und Pein/", "tokens": ["Wen", "Er", "am", "liebs\u00b7ten", "hat", "/", "der", "mu\u00df", "in", "Creutz", "und", "Pein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKA", "ADJD", "VAFIN", "$(", "ART", "VMFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In Marter/ Angst und Tod der N\u00e4chste bey jhm seyn.", "tokens": ["In", "Mar\u00b7ter", "/", "Angst", "und", "Tod", "der", "N\u00e4chs\u00b7te", "bey", "jhm", "se