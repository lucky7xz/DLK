{"dta.poem.13159": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eja! \u201e, fuhr die Dicht-Kunst auf, das hab ich erst gesagt;", "tokens": ["\u201e", "ja", "!", "\u201e", ",", "fuhr", "die", "Dicht\u00b7Kunst", "auf", ",", "das", "hab", "ich", "erst", "ge\u00b7sagt", ";"], "token_info": ["punct", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$.", "$(", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PDS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eso seht, wie meine Kunst der K\u00fcnste Wercke schlagt.", "tokens": ["\u201e", "so", "seht", ",", "wie", "mei\u00b7ne", "Kunst", "der", "K\u00fcns\u00b7te", "Wer\u00b7cke", "schlagt", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ethalia zeigt nur an, da\u00df sie von mir gelernet,", "tokens": ["\u201e", "tha\u00b7lia", "zeigt", "nur", "an", ",", "da\u00df", "sie", "von", "mir", "ge\u00b7ler\u00b7net", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "VVFIN", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "\u201ewas sich von eurer Macht und Wissenschaft entfernet.", "tokens": ["\u201e", "was", "sich", "von", "eu\u00b7rer", "Macht", "und", "Wis\u00b7sen\u00b7schaft", "ent\u00b7fer\u00b7net", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PRF", "APPR", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "245\u201dSie folgt dem Dichter-Trieb den meine Cither lehrt;", "tokens": ["\"", "Sie", "folgt", "dem", "Dich\u00b7ter\u00b7Trieb", "den", "mei\u00b7ne", "Cit\u00b7her", "lehrt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ewas sie erz\u00e4hlet, ist was sie von mir geh\u00f6rt.", "tokens": ["\u201e", "was", "sie", "er\u00b7z\u00e4h\u00b7let", ",", "ist", "was", "sie", "von", "mir", "ge\u00b7h\u00f6rt", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "PWS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201esagt, welche von dem Chor vermag das aufzurichten?", "tokens": ["\u201e", "sagt", ",", "wel\u00b7che", "von", "dem", "Chor", "ver\u00b7mag", "das", "auf\u00b7zu\u00b7rich\u00b7ten", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "PDS", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ekann Marmel, Farb und Erz so viel als unser Dichten?", "tokens": ["\u201e", "kann", "Mar\u00b7mel", ",", "Farb", "und", "Erz", "so", "viel", "als", "un\u00b7ser", "Dich\u00b7ten", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "NN", "$,", "NN", "KON", "NN", "ADV", "PIAT", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201ezu dem, verstehet ihr, was das vor ein Geb\u00e4u?", "tokens": ["\u201e", "zu", "dem", ",", "ver\u00b7ste\u00b7het", "ihr", ",", "was", "das", "vor", "ein", "Ge\u00b7b\u00e4u", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "$,", "VVFIN", "PPER", "$,", "PRELS", "ART", "APPR", "ART", "NN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.10": {"text": "250\u201dGlaubt ihr vielleicht, da\u00df es ein Scherz der Strahlen sey?", "tokens": ["\"", "Glaubt", "ihr", "viel\u00b7leicht", ",", "da\u00df", "es", "ein", "Scherz", "der", "Strah\u00b7len", "sey", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201eda\u00df hier die Sonne sich mit Regenb\u00f6gen spiele?", "tokens": ["\u201e", "da\u00df", "hier", "die", "Son\u00b7ne", "sich", "mit", "Re\u00b7gen\u00b7b\u00f6\u00b7gen", "spie\u00b7le", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ADV", "ART", "NN", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201eda\u00df dessen Umfang nur auf eitlen Schimmer ziele?", "tokens": ["\u201e", "da\u00df", "des\u00b7sen", "Um\u00b7fang", "nur", "auf", "eit\u00b7len", "Schim\u00b7mer", "zie\u00b7le", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PRELAT", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201eseht ihr darinnen nicht ein hell-beleuchtes Herz?", "tokens": ["\u201e", "seht", "ihr", "da\u00b7rin\u00b7nen", "nicht", "ein", "hell\u00b7be\u00b7leuch\u00b7tes", "Herz", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADV", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201edas weder Steine braucht, noch Eisen, auch kein Erz.", "tokens": ["\u201e", "das", "we\u00b7der", "Stei\u00b7ne", "braucht", ",", "noch", "Ei\u00b7sen", ",", "auch", "kein", "Erz", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "KON", "NN", "VVFIN", "$,", "ADV", "NN", "$,", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}