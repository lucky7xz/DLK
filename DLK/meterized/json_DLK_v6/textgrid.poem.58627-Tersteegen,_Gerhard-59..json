{"textgrid.poem.58627": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "59.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Gott seine Lust an dir und an deinem Stand soll sehen,", "tokens": ["Wenn", "Gott", "sei\u00b7ne", "Lust", "an", "dir", "und", "an", "dei\u00b7nem", "Stand", "soll", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "APPR", "PPER", "KON", "APPR", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mu\u00dft du durch die W\u00fcste erst in Verlassungswegen gehen;", "tokens": ["Mu\u00dft", "du", "durch", "die", "W\u00fcs\u00b7te", "erst", "in", "Ver\u00b7las\u00b7sungs\u00b7we\u00b7gen", "ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Willst du deinem Gott zur Freud' ihm als Braut verm\u00e4hlet sein,", "tokens": ["Willst", "du", "dei\u00b7nem", "Gott", "zur", "Freud'", "ihm", "als", "Braut", "ver\u00b7m\u00e4h\u00b7let", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "APPRART", "NN", "PPER", "KOUS", "NN", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Denk, wie mu\u00dft du sein geziert und von allen Flecken rein!", "tokens": ["Denk", ",", "wie", "mu\u00dft", "du", "sein", "ge\u00b7ziert", "und", "von", "al\u00b7len", "Fle\u00b7cken", "rein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VMFIN", "PPER", "PPOSAT", "VVPP", "KON", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Ach, wenn Gott hat Lust an mir, wenn ich ihn nur kann erfreuen,", "tokens": ["Ach", ",", "wenn", "Gott", "hat", "Lust", "an", "mir", ",", "wenn", "ich", "ihn", "nur", "kann", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "NN", "VAFIN", "NN", "APPR", "PPER", "$,", "KOUS", "PPER", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Sollt' ich nicht verleugnen gern, sollt' ich Kreuz und Leiden scheuen?", "tokens": ["Sollt'", "ich", "nicht", "ver\u00b7leug\u00b7nen", "gern", ",", "sollt'", "ich", "Kreuz", "und", "Lei\u00b7den", "scheu\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVFIN", "ADV", "$,", "VMFIN", "PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Mach mich sch\u00f6n, mein Br\u00e4utigam, freu dich ewig dann in mir,", "tokens": ["Mach", "mich", "sch\u00f6n", ",", "mein", "Br\u00e4u\u00b7ti\u00b7gam", ",", "freu", "dich", "e\u00b7wig", "dann", "in", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "PPOSAT", "NE", "$,", "VVFIN", "PPER", "ADJD", "ADV", "APPR", "PPER", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Ich, von allem abgewandt, will mich freuen auch in dir!", "tokens": ["Ich", ",", "von", "al\u00b7lem", "ab\u00b7ge\u00b7wandt", ",", "will", "mich", "freu\u00b7en", "auch", "in", "dir", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "PIS", "VVPP", "$,", "VMFIN", "PPER", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}}}}}