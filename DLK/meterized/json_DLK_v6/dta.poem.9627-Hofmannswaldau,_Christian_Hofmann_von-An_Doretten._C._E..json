{"dta.poem.9627": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An Doretten.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich brenn in einer stummen glut/", "tokens": ["Ich", "brenn", "in", "ei\u00b7ner", "stum\u00b7men", "glut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und opffre leben/ geist und blut/", "tokens": ["Und", "opff\u00b7re", "le\u00b7ben", "/", "geist", "und", "blut", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$(", "VVPP", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dorette/ deinen wunder-flammen;", "tokens": ["Do\u00b7ret\u00b7te", "/", "dei\u00b7nen", "wun\u00b7der\u00b7flam\u00b7men", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$(", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein feuer quillt aus deinem brand/", "tokens": ["Mein", "feu\u00b7er", "quillt", "aus", "dei\u00b7nem", "brand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es nimt im hertzen \u00fcberhand/", "tokens": ["Es", "nimt", "im", "hert\u00b7zen", "\u00fc\u00b7ber\u00b7hand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und du wilt meine glut verdammen?", "tokens": ["Und", "du", "wilt", "mei\u00b7ne", "glut", "ver\u00b7dam\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Betrachte doch dein heisses licht/", "tokens": ["Be\u00b7trach\u00b7te", "doch", "dein", "heis\u00b7ses", "licht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das aus so s\u00fcssen augen bricht/", "tokens": ["Das", "aus", "so", "s\u00fcs\u00b7sen", "au\u00b7gen", "bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und lerne deine macht erkennen.", "tokens": ["Und", "ler\u00b7ne", "dei\u00b7ne", "macht", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein hertz ist nicht von stahl und stein/", "tokens": ["Mein", "hertz", "ist", "nicht", "von", "stahl", "und", "stein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "APPR", "ADV", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es mu\u00df bey dir empfindlich seyn/", "tokens": ["Es", "mu\u00df", "bey", "dir", "emp\u00b7find\u00b7lich", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wider deinen willen brennen.", "tokens": ["Und", "wi\u00b7der", "dei\u00b7nen", "wil\u00b7len", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nicht tadle denn die reine glut/", "tokens": ["Nicht", "tad\u00b7le", "denn", "die", "rei\u00b7ne", "glut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die dir ja keinen schaden thut/", "tokens": ["Die", "dir", "ja", "kei\u00b7nen", "scha\u00b7den", "thut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PIAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und doch aus deiner krafft entspringet.", "tokens": ["Und", "doch", "aus", "dei\u00b7ner", "krafft", "ent\u00b7sprin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich bin vergn\u00fcgt bey meiner noth", "tokens": ["Ich", "bin", "ver\u00b7gn\u00fcgt", "bey", "mei\u00b7ner", "noth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und liebe gar den s\u00fcssen todt/", "tokens": ["Und", "lie\u00b7be", "gar", "den", "s\u00fcs\u00b7sen", "todt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der mich durch dich zum sterben zwinget.", "tokens": ["Der", "mich", "durch", "dich", "zum", "ster\u00b7ben", "zwin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "APPRART", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}