{"textgrid.poem.32997": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und wenn du jetzt aufwachst morgens ...", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wenn du jetzt aufwachst morgens ...", "tokens": ["Und", "wenn", "du", "jetzt", "auf\u00b7wachst", "mor\u00b7gens", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "ADV", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "ganz leis und fein", "tokens": ["ganz", "leis", "und", "fein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "spielt um die D\u00e4cher", "tokens": ["spielt", "um", "die", "D\u00e4\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "der Sonnenschein,", "tokens": ["der", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "und du bist nicht mehr m\u00fcde,", "tokens": ["und", "du", "bist", "nicht", "mehr", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "wie sonst, und verzagt:", "tokens": ["wie", "sonst", ",", "und", "ver\u00b7zagt", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "KON", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "was soll nun wieder", "tokens": ["was", "soll", "nun", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ADV", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "voll M\u00fchsal und Plag", "tokens": ["voll", "M\u00fch\u00b7sal", "und", "Plag"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "NN", "KON", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "der ganze lange endlose Tag!?", "tokens": ["der", "gan\u00b7ze", "lan\u00b7ge", "end\u00b7lo\u00b7se", "Tag", "!?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Froh und munter", "tokens": ["Froh", "und", "mun\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "geht's ihm entgegen,", "tokens": ["geht's", "ihm", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "und alles ist so wunderbar", "tokens": ["und", "al\u00b7les", "ist", "so", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "frisch und stark und hell und klar,", "tokens": ["frisch", "und", "stark", "und", "hell", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "das ganze Leben so frei, so leicht,", "tokens": ["das", "gan\u00b7ze", "Le\u00b7ben", "so", "frei", ",", "so", "leicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "da\u00df du dich selber dr\u00fcber wunderst:", "tokens": ["da\u00df", "du", "dich", "sel\u00b7ber", "dr\u00fc\u00b7ber", "wun\u00b7derst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "PAV", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "von was f\u00fcr t\u00f6richt dummen Dingen", "tokens": ["von", "was", "f\u00fcr", "t\u00f6\u00b7richt", "dum\u00b7men", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "du das Herz dir lie\u00dfest zwingen", "tokens": ["du", "das", "Herz", "dir", "lie\u00b7\u00dfest", "zwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "PPER", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "und kaum begreifst:", "tokens": ["und", "kaum", "be\u00b7greifst", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "mit welch erb\u00e4rmlichen Kleinigkeiten", "tokens": ["mit", "welch", "er\u00b7b\u00e4rm\u00b7li\u00b7chen", "Klei\u00b7nig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.11": {"text": "die Menschen sich das Leben verleiden ...", "tokens": ["die", "Men\u00b7schen", "sich", "das", "Le\u00b7ben", "ver\u00b7lei\u00b7den", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Kleinigkeiten, ob denen es kaum", "tokens": ["Klei\u00b7nig\u00b7kei\u00b7ten", ",", "ob", "de\u00b7nen", "es", "kaum"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PRELS", "PPER", "ADV"], "meter": "+-+-+---+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "der M\u00fche wert, ein Wort zu verlieren,", "tokens": ["der", "M\u00fc\u00b7he", "wert", ",", "ein", "Wort", "zu", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "geschweige denn tage- und wochenlang", "tokens": ["ge\u00b7schwei\u00b7ge", "denn", "ta\u00b7ge", "und", "wo\u00b7chen\u00b7lang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "KON", "TRUNC", "KON", "NN"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.4": {"text": "zu qu\u00e4len sich und zu schikanieren ...", "tokens": ["zu", "qu\u00e4\u00b7len", "sich", "und", "zu", "schi\u00b7ka\u00b7nie\u00b7ren", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PRF", "KON", "PTKZU", "VVINF", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "und vollends jetzt, da's Fr\u00fchling wird", "tokens": ["und", "vol\u00b7lends", "jetzt", ",", "da's", "Fr\u00fch\u00b7ling", "wird"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "$,", "NE", "NE", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und, wenn du aufwachst morgens,", "tokens": ["und", ",", "wenn", "du", "auf\u00b7wachst", "mor\u00b7gens", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "ganz leis und fein", "tokens": ["ganz", "leis", "und", "fein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "um die D\u00e4cher spielt", "tokens": ["um", "die", "D\u00e4\u00b7cher", "spielt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "der Sonnenschein", "tokens": ["der", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "und alles rings so wunderbar", "tokens": ["und", "al\u00b7les", "rings", "so", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "frisch und stark und hell und klar ...", "tokens": ["frisch", "und", "stark", "und", "hell", "und", "klar", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "KON", "ADJD", "KON", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "wozu sich da gr\u00e4men und betr\u00fcben!", "tokens": ["wo\u00b7zu", "sich", "da", "gr\u00e4\u00b7men", "und", "be\u00b7tr\u00fc\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ADV", "VVINF", "KON", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "nein, weg mit all den Schererei'n!", "tokens": ["nein", ",", "weg", "mit", "all", "den", "Sche\u00b7re\u00b7rei'n", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "APPR", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "es lohnt sich da wahrlich nur: zu lieben!", "tokens": ["es", "lohnt", "sich", "da", "wahr\u00b7lich", "nur", ":", "zu", "lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADV", "ADV", "$.", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "es lohnt sich da wahrlich nur: froh zu sein!", "tokens": ["es", "lohnt", "sich", "da", "wahr\u00b7lich", "nur", ":", "froh", "zu", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADV", "ADV", "$.", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}