{"dta.poem.17196": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "47.  Dort geht es anderst zu.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Hier h\u00e4ngt das Lamb am Creutz/ dort sitzts auf GOttes-", "tokens": ["Hier", "h\u00e4ngt", "das", "Lamb", "am", "Creutz", "/", "dort", "sitzts", "auf", "Got\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$(", "ADV", "VVFIN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "thron/", "tokens": ["thron", "/"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Hier tr\u00e4gts den Dornen krantz/ dort eine Kaiserkron:", "tokens": ["Hier", "tr\u00e4gts", "den", "Dor\u00b7nen", "krantz", "/", "dort", "ei\u00b7ne", "Kai\u00b7ser\u00b7kron", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$(", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hier ist es Unterthan/ dort herscht es \u00fcberalle:", "tokens": ["Hier", "ist", "es", "Un\u00b7ter\u00b7than", "/", "dort", "herscht", "es", "\u00fc\u00b7be\u00b7ral\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$(", "ADV", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hier thuts den Mund nicht auf/ dort redts mit hellem", "tokens": ["Hier", "thuts", "den", "Mund", "nicht", "auf", "/", "dort", "redts", "mit", "hel\u00b7lem"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKNEG", "PTKVZ", "$(", "ADV", "VVFIN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "schalle:", "tokens": ["schal\u00b7le", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Hier weints/ und dorte Lachts: drumb tr\u00f6ste dich mein", "tokens": ["Hier", "weints", "/", "und", "dor\u00b7te", "Lachts", ":", "drumb", "tr\u00f6s\u00b7te", "dich", "mein"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$(", "KON", "ADJA", "NN", "$.", "PAV", "VVFIN", "PPER", "PPOSAT"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Christ/", "tokens": ["Christ", "/"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Da\u00df sich dein Creutz verkehrt/ wo du di\u00df Lam\u0304 nur bist.", "tokens": ["Da\u00df", "sich", "dein", "Creutz", "ver\u00b7kehrt", "/", "wo", "du", "di\u00df", "Lam\u0304", "nur", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "VVPP", "$(", "PWAV", "PPER", "PDS", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}