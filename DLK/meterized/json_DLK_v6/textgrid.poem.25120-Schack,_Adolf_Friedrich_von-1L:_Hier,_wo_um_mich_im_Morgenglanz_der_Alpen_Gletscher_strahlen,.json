{"textgrid.poem.25120": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier, wo um mich im Morgenglanz der Alpen Gletscher strahlen,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier, wo um mich im Morgenglanz der Alpen Gletscher strahlen,", "tokens": ["Hier", ",", "wo", "um", "mich", "im", "Mor\u00b7gen\u00b7glanz", "der", "Al\u00b7pen", "Glet\u00b7scher", "strah\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "KOUI", "PRF", "APPRART", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Und hinter mir Italien mit seinen Goldfruchtthalen,", "tokens": ["Und", "hin\u00b7ter", "mir", "I\u00b7ta\u00b7li\u00b7en", "mit", "sei\u00b7nen", "Gold\u00b7fruchtt\u00b7ha\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Mit seiner Myrtenh\u00fcgel Gr\u00fcn verschwimmt in duft'ge Bl\u00e4ue,", "tokens": ["Mit", "sei\u00b7ner", "Myr\u00b7ten\u00b7h\u00fc\u00b7gel", "Gr\u00fcn", "ver\u00b7schwimmt", "in", "duft'\u00b7ge", "Bl\u00e4u\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Schaut freudethr\u00e4nenvoll mein Blick, o Deutschland, dich aufs neue!", "tokens": ["Schaut", "freu\u00b7det\u00b7hr\u00e4\u00b7nen\u00b7voll", "mein", "Blick", ",", "o", "Deutschland", ",", "dich", "aufs", "neu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$,", "FM", "NE", "$,", "PRF", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Oft, aus der Ferne heimgekehrt ans Ufer deines Rheines,", "tokens": ["Oft", ",", "aus", "der", "Fer\u00b7ne", "heim\u00b7ge\u00b7kehrt", "ans", "U\u00b7fer", "dei\u00b7nes", "Rhei\u00b7nes", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Wohl dacht' ich, sch\u00f6n auf Erden sei wie du der L\u00e4nder keines,", "tokens": ["Wohl", "dacht'", "ich", ",", "sch\u00f6n", "auf", "Er\u00b7den", "sei", "wie", "du", "der", "L\u00e4n\u00b7der", "kei\u00b7nes", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "APPR", "NN", "VAFIN", "KOKOM", "PPER", "ART", "NN", "PIS", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Und Rast nicht lie\u00df mir's, bis ich dich nochmals durchpilgert hatte,", "tokens": ["Und", "Rast", "nicht", "lie\u00df", "mir's", ",", "bis", "ich", "dich", "noch\u00b7mals", "durch\u00b7pil\u00b7gert", "hat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKNEG", "VVFIN", "NE", "$,", "KOUS", "PPER", "PRF", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-++--+-+-", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "Von wo das Hochgebirg Tirols sich senkt zur gr\u00fcnen Matte,", "tokens": ["Von", "wo", "das", "Hoch\u00b7ge\u00b7birg", "Ti\u00b7rols", "sich", "senkt", "zur", "gr\u00fc\u00b7nen", "Mat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAV", "ART", "NN", "NN", "PRF", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.9": {"text": "Und in der frischen Th\u00e4ler Scho\u00df die blauen Seen tr\u00e4umen,", "tokens": ["Und", "in", "der", "fri\u00b7schen", "Th\u00e4\u00b7ler", "Scho\u00df", "die", "blau\u00b7en", "Seen", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-++-", "measure": "unknown.measure.septa"}, "line.10": {"text": "Bis wo an Schleswigs D\u00fcnenstrand die Nordseewogen sch\u00e4umen;", "tokens": ["Bis", "wo", "an", "Schles\u00b7wigs", "D\u00fc\u00b7nen\u00b7strand", "die", "Nord\u00b7see\u00b7wo\u00b7gen", "sch\u00e4u\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAV", "APPR", "NN", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.11": {"text": "Doch stolzer heut, als je zuvor, darf ich dich mit dem s\u00fc\u00dfen,", "tokens": ["Doch", "stol\u00b7zer", "heut", ",", "als", "je", "zu\u00b7vor", ",", "darf", "ich", "dich", "mit", "dem", "s\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$,", "KOUS", "ADV", "ADV", "$,", "VMFIN", "PPER", "PRF", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.12": {"text": "Dem heil'gen Namen Vaterland, du teure Heimat, gr\u00fc\u00dfen!", "tokens": ["Dem", "heil'\u00b7gen", "Na\u00b7men", "Va\u00b7ter\u00b7land", ",", "du", "teu\u00b7re", "Hei\u00b7mat", ",", "gr\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "PPER", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.13": {"text": "Wenn sonst in alten Bergen nur, wo rankendes Gewinde", "tokens": ["Wenn", "sonst", "in", "al\u00b7ten", "Ber\u00b7gen", "nur", ",", "wo", "ran\u00b7ken\u00b7des", "Ge\u00b7win\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ADJA", "NN", "ADV", "$,", "PWAV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.14": {"text": "Der Epheu schlingt und scheu bei Nacht am Brunnen trinkt die Hinde,", "tokens": ["Der", "E\u00b7pheu", "schlingt", "und", "scheu", "bei", "Nacht", "am", "Brun\u00b7nen", "trinkt", "die", "Hin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ADJD", "APPR", "NN", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.15": {"text": "Ich deines Ruhmes Kunden las auf grauen Marmorplatten,", "tokens": ["Ich", "dei\u00b7nes", "Ruh\u00b7mes", "Kun\u00b7den", "las", "auf", "grau\u00b7en", "Mar\u00b7mor\u00b7plat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.16": {"text": "Jetzt glorreich stehst du vor mir da, erstanden von den Schatten.", "tokens": ["Jetzt", "glor\u00b7reich", "stehst", "du", "vor", "mir", "da", ",", "er\u00b7stan\u00b7den", "von", "den", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "APPR", "PPER", "ADV", "$,", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "O, nun der m\u00e4cht'ge Kaiseraar, hinflatternd ob den Heeren,", "tokens": ["O", ",", "nun", "der", "m\u00e4cht'\u00b7ge", "Kai\u00b7se\u00b7raar", ",", "hin\u00b7flat\u00b7ternd", "ob", "den", "Hee\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ART", "ADJA", "NN", "$,", "VVPP", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Zu seinem alten Horste kehrt, an Siegen reich und Ehren,", "tokens": ["Zu", "sei\u00b7nem", "al\u00b7ten", "Hors\u00b7te", "kehrt", ",", "an", "Sie\u00b7gen", "reich", "und", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,", "APPR", "NN", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Wie rollt hochwallend, Adern gleich, wenn sie zu schnellern Schl\u00e4gen", "tokens": ["Wie", "rollt", "hoch\u00b7wal\u00b7lend", ",", "A\u00b7dern", "gleich", ",", "wenn", "sie", "zu", "schnel\u00b7lern", "Schl\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADJD", "$,", "NN", "ADV", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Die Freude treibt, dein deutscher Rhein voll Jubel ihm entgegen!", "tokens": ["Die", "Freu\u00b7de", "treibt", ",", "dein", "deut\u00b7scher", "Rhein", "voll", "Ju\u00b7bel", "ihm", "ent\u00b7ge\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPOSAT", "ADJA", "NE", "ADJD", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "Von den Vogesen bis zum Harz, zum Kreidestrand von R\u00fcgen", "tokens": ["Von", "den", "Vo\u00b7ge\u00b7sen", "bis", "zum", "Harz", ",", "zum", "Krei\u00b7des\u00b7trand", "von", "R\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "APPRART", "NE", "$,", "APPRART", "NN", "APPR", "NE"], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Fliegt flammenhell die Botschaft hin von seinen Siegesfl\u00fcgen;", "tokens": ["Fliegt", "flam\u00b7me\u00b7nhell", "die", "Bot\u00b7schaft", "hin", "von", "sei\u00b7nen", "Sie\u00b7ges\u00b7fl\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Aus langem, schwerem Traum erwacht, hebt Stra\u00dfburgs Kathedrale", "tokens": ["Aus", "lan\u00b7gem", ",", "schwe\u00b7rem", "Traum", "er\u00b7wacht", ",", "hebt", "Stra\u00df\u00b7burgs", "Ka\u00b7thed\u00b7ra\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "VVPP", "$,", "VVFIN", "NE", "NE"], "meter": "-+-+-+-+-+--+--", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Begeistert ihr befreites Haupt und t\u00f6nt im Morgenstrahle,", "tokens": ["Be\u00b7geis\u00b7tert", "ihr", "be\u00b7frei\u00b7tes", "Haupt", "und", "t\u00f6nt", "im", "Mor\u00b7gen\u00b7strah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.9": {"text": "Und mit der Glocken Festgel\u00e4ut vom Strome hin zu Strome", "tokens": ["Und", "mit", "der", "Glo\u00b7cken", "Fest\u00b7ge\u00b7l\u00e4ut", "vom", "Stro\u00b7me", "hin", "zu", "Stro\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "APPRART", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "Zujauchzen freudestammelnd ihr die hehren Schwesterdome.", "tokens": ["Zu\u00b7jauch\u00b7zen", "freu\u00b7des\u00b7tam\u00b7melnd", "ihr", "die", "heh\u00b7ren", "Schwes\u00b7ter\u00b7do\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.11": {"text": "Hochbrausend mit der Wogen Schlag, die um Arkona branden,", "tokens": ["Hoch\u00b7brau\u00b7send", "mit", "der", "Wo\u00b7gen", "Schlag", ",", "die", "um", "Ar\u00b7ko\u00b7na", "bran\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,", "PRELS", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.12": {"text": "Begr\u00fc\u00dft der Ostsee blaue Flut das Reich, das neu erstanden;", "tokens": ["Be\u00b7gr\u00fc\u00dft", "der", "Ost\u00b7see", "blau\u00b7e", "Flut", "das", "Reich", ",", "das", "neu", "er\u00b7stan\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.13": {"text": "Die Alpen jauchzen Antwort ihr mit donnernden Lawinen,", "tokens": ["Die", "Al\u00b7pen", "jauch\u00b7zen", "Ant\u00b7wort", "ihr", "mit", "don\u00b7nern\u00b7den", "La\u00b7wi\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.14": {"text": "Und deine Kaiserpfalzen all und deine Burgruinen", "tokens": ["Und", "dei\u00b7ne", "Kai\u00b7ser\u00b7pfal\u00b7zen", "all", "und", "dei\u00b7ne", "Bur\u00b7gru\u00b7i\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "PIAT", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.15": {"text": "Und deine St\u00e4dte altersgrau, des Ruhms erlauchte Wiegen,", "tokens": ["Und", "dei\u00b7ne", "St\u00e4d\u00b7te", "al\u00b7ters\u00b7grau", ",", "des", "Ruhms", "er\u00b7lauch\u00b7te", "Wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "$,", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.16": {"text": "Ergl\u00e4nzen in dem jungen Licht, dem Schutte halb entstiegen.", "tokens": ["Er\u00b7gl\u00e4n\u00b7zen", "in", "dem", "jun\u00b7gen", "Licht", ",", "dem", "Schut\u00b7te", "halb", "ent\u00b7stie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Ges\u00fchnt ist, was von welschem Hohn seit Karls von Anjou Tagen", "tokens": ["Ge\u00b7s\u00fchnt", "ist", ",", "was", "von", "wel\u00b7schem", "Hohn", "seit", "Karls", "von", "An\u00b7jou", "Ta\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "$,", "PRELS", "APPR", "PWAT", "NN", "APPR", "NE", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Bis zu dem Korsenunhold du, Unselige, ertragen.", "tokens": ["Bis", "zu", "dem", "Kor\u00b7sen\u00b7un\u00b7hold", "du", ",", "Un\u00b7se\u00b7li\u00b7ge", ",", "er\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "PPER", "$,", "ADJA", "$,", "VVINF", "$."], "meter": "---+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zu deinen Toten drunten selbst im kalten feuchten Grauen", "tokens": ["Zu", "dei\u00b7nen", "To\u00b7ten", "drun\u00b7ten", "selbst", "im", "kal\u00b7ten", "feuch\u00b7ten", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADV", "APPRART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Der Gr\u00e4ber rinnt der Trost hinab wie sanftes Fr\u00fchlingstauen,", "tokens": ["Der", "Gr\u00e4\u00b7ber", "rinnt", "der", "Trost", "hin\u00b7ab", "wie", "sanf\u00b7tes", "Fr\u00fch\u00b7lings\u00b7tau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "Und sie, der Franken-Frevelmut das sch\u00f6ne Herz gebrochen,", "tokens": ["Und", "sie", ",", "der", "Fran\u00b7ken\u00b7Fre\u00b7vel\u00b7mut", "das", "sch\u00f6\u00b7ne", "Herz", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-