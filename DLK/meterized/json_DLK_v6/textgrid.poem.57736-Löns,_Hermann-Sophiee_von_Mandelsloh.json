{"textgrid.poem.57736": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Sophiee von Mandelsloh", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Gang war leicht, ihr Zopf war schwer,", "tokens": ["Ihr", "Gang", "war", "leicht", ",", "ihr", "Zopf", "war", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie trug ihre Br\u00fcste stolz vor sich her", "tokens": ["Sie", "trug", "ih\u00b7re", "Br\u00fcs\u00b7te", "stolz", "vor", "sich", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJD", "APPR", "PRF", "APZR"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und lachte so laut und so froh;", "tokens": ["Und", "lach\u00b7te", "so", "laut", "und", "so", "froh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Es zwang den ungerittenen Gaul", "tokens": ["Es", "zwang", "den", "un\u00b7ge\u00b7rit\u00b7te\u00b7nen", "Gaul"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Und ri\u00df dem Bluthund den Fra\u00df aus dem Maul,", "tokens": ["Und", "ri\u00df", "dem", "Blut\u00b7hund", "den", "Fra\u00df", "aus", "dem", "Maul", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sophiee von Mandelsloh.", "tokens": ["So\u00b7phi\u00b7ee", "von", "Man\u00b7dels\u00b7loh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Auf Ricklingen ging der Becher rund,", "tokens": ["Auf", "Rick\u00b7lin\u00b7gen", "ging", "der", "Be\u00b7cher", "rund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Von Hand zu Hand, von Mund zu Mund,", "tokens": ["Von", "Hand", "zu", "Hand", ",", "von", "Mund", "zu", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gef\u00fcllt mit rotem Wein;", "tokens": ["Ge\u00b7f\u00fcllt", "mit", "ro\u00b7tem", "Wein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Auf Herzog Albrecht ein Spottlied klang,", "tokens": ["Auf", "Her\u00b7zog", "Al\u00b7brecht", "ein", "Spott\u00b7lied", "klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Die sch\u00f6ne Sophiee es l\u00e4chelnd sang,", "tokens": ["Die", "sch\u00f6\u00b7ne", "So\u00b7phi\u00b7ee", "es", "l\u00e4\u00b7chelnd", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Laut brummte ihr Vater hinein.", "tokens": ["Laut", "brumm\u00b7te", "ihr", "Va\u00b7ter", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Sie sang: \u00bbHerr Herzog, wir bitten Euch sch\u00f6n,", "tokens": ["Sie", "sang", ":", "\u00bb", "Herr", "Her\u00b7zog", ",", "wir", "bit\u00b7ten", "Euch", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "NN", "NE", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-++-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "So la\u00dft euch doch endlich auf Ricklingen sehn,", "tokens": ["So", "la\u00dft", "euch", "doch", "end\u00b7lich", "auf", "Rick\u00b7lin\u00b7gen", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Beehrt doch die Mandelsloh;", "tokens": ["Be\u00b7ehrt", "doch", "die", "Man\u00b7dels\u00b7loh", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Von L\u00fcneburg ist es ja gar nicht so weit,", "tokens": ["Von", "L\u00fc\u00b7ne\u00b7burg", "ist", "es", "ja", "gar", "nicht", "so", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "PPER", "ADV", "ADV", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Zu Eurem Empfange ist alles bereit,", "tokens": ["Zu", "Eu\u00b7rem", "Emp\u00b7fan\u00b7ge", "ist", "al\u00b7les", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PIS", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Es blitzet und blinkert nur so.\u00ab", "tokens": ["Es", "blit\u00b7zet", "und", "blin\u00b7kert", "nur", "so", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "ADV", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Laut lachte ihr Vater und kippte den Rest", "tokens": ["Laut", "lach\u00b7te", "ihr", "Va\u00b7ter", "und", "kipp\u00b7te", "den", "Rest"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und knurrte: \u00bbNun holla, ein jeder ins Nest,", "tokens": ["Und", "knurr\u00b7te", ":", "\u00bb", "Nun", "hol\u00b7la", ",", "ein", "je\u00b7der", "ins", "Nest", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "ADV", "NE", "$,", "ART", "PIAT", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Heut' abend, da gibt es nichts mehr;", "tokens": ["Heut'", "a\u00b7bend", ",", "da", "gibt", "es", "nichts", "mehr", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "VVFIN", "PPER", "PIS", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Den Spund in das Loch und geruhsame Nacht,", "tokens": ["Den", "Spund", "in", "das", "Loch", "und", "ge\u00b7ruh\u00b7sa\u00b7me", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Und morgen beizeiten h\u00fcbsch aufgewacht,", "tokens": ["Und", "mor\u00b7gen", "bei\u00b7zei\u00b7ten", "h\u00fcbsch", "auf\u00b7ge\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADJD", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Denn die Kammern und die Keller, die sind leer.\u00ab", "tokens": ["Denn", "die", "Kam\u00b7mern", "und", "die", "Kel\u00b7ler", ",", "die", "sind", "leer", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "KON", "ART", "NN", "$,", "PRELS", "VAFIN", "ADJD", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.5": {"line.1": {"text": "Er warf sich mit Stiefeln und Sporen aufs Bett", "tokens": ["Er", "warf", "sich", "mit", "Stie\u00b7feln", "und", "Spo\u00b7ren", "aufs", "Bett"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "APPRART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und schnarchte mit seinem Hund ein Duett,", "tokens": ["Und", "schnarch\u00b7te", "mit", "sei\u00b7nem", "Hund", "ein", "Du\u00b7ett", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Eule zum letztenmal schrie;", "tokens": ["Die", "Eu\u00b7le", "zum", "letz\u00b7ten\u00b7mal", "schrie", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADV", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Und es kr\u00e4hte ein Hahn und es klang ein Horn,", "tokens": ["Und", "es", "kr\u00e4h\u00b7te", "ein", "Hahn", "und", "es", "klang", "ein", "Horn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Herr Dietrich erwachte in hellichtem Zorn:", "tokens": ["Herr", "Diet\u00b7rich", "er\u00b7wach\u00b7te", "in", "hel\u00b7lich\u00b7tem", "Zorn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "\u00bbwatt schall d\u00fcsse D\u00f6llmerie?\u00ab", "tokens": ["\u00bb", "watt", "schall", "d\u00fcs\u00b7se", "D\u00f6ll\u00b7me\u00b7rie", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Er fuhr mit dem Kopf in den Eimer hinein,", "tokens": ["Er", "fuhr", "mit", "dem", "Kopf", "in", "den", "Ei\u00b7mer", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und sah in den lachenden Lenzsonnenschein,", "tokens": ["Und", "sah", "in", "den", "la\u00b7chen\u00b7den", "Lenz\u00b7son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und machte ein dummes Gesicht;", "tokens": ["Und", "mach\u00b7te", "ein", "dum\u00b7mes", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Denn rings um sein graues, griesgr\u00e4miges Schlo\u00df,", "tokens": ["Denn", "rings", "um", "sein", "grau\u00b7es", ",", "gries\u00b7gr\u00e4\u00b7mi\u00b7ges", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Da hielt des Herzogs buntscheckiger Tro\u00df,", "tokens": ["Da", "hielt", "des", "Her\u00b7zogs", "bunt\u00b7sche\u00b7cki\u00b7ger", "Tro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dreihundert, das langte noch nicht.", "tokens": ["Drei\u00b7hun\u00b7dert", ",", "das", "lang\u00b7te", "noch", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "$,", "PDS", "VVFIN", "ADV", "PTKNEG", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "\u00bbsophiee, schnell einen Schoppen her,", "tokens": ["\u00bb", "so\u00b7phi\u00b7ee", ",", "schnell", "ei\u00b7nen", "Schop\u00b7pen", "her", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Auf n\u00fcchternen Magen, da ficht es sich schwer!\u00ab", "tokens": ["Auf", "n\u00fcch\u00b7ter\u00b7nen", "Ma\u00b7gen", ",", "da", "ficht", "es", "sich", "schwer", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "PRF", "ADJD", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Im Unterrock kam sie herein;", "tokens": ["Im", "Un\u00b7ter\u00b7rock", "kam", "sie", "her\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie k\u00e4mmte dem Vater schleunigst das Haar,", "tokens": ["Sie", "k\u00e4mm\u00b7te", "dem", "Va\u00b7ter", "schleu\u00b7nigst", "das", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Das noch von der Nacht etwas strubbelig war,", "tokens": ["Das", "noch", "von", "der", "Nacht", "et\u00b7was", "strub\u00b7be\u00b7lig", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "ART", "NN", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und brachte ihm Brot und Wein.", "tokens": ["Und", "brach\u00b7te", "ihm", "Brot", "und", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Ihr Bestes nahm sie dann aus dem Schrank,", "tokens": ["Ihr", "Bes\u00b7tes", "nahm", "sie", "dann", "aus", "dem", "Schrank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Von blitzblauer Seide und reichlich lang,", "tokens": ["Von", "blitz\u00b7blau\u00b7er", "Sei\u00b7de", "und", "reich\u00b7lich", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJD", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und bunt von Stickerein,", "tokens": ["Und", "bunt", "von", "Sti\u00b7cke\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und machte sich schnell noch die hohe Frisur,", "tokens": ["Und", "mach\u00b7te", "sich", "schnell", "noch", "die", "ho\u00b7he", "Fri\u00b7sur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Die trug sie sonst zum Kirchgange nur,", "tokens": ["Die", "trug", "sie", "sonst", "zum", "Kirch\u00b7gan\u00b7ge", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Und zu ganz gro\u00dfen Gasterein.", "tokens": ["Und", "zu", "ganz", "gro\u00b7\u00dfen", "Gas\u00b7ter\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Der Herold schrie zur Mauer empor;", "tokens": ["Der", "He\u00b7rold", "schrie", "zur", "Mau\u00b7er", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambi