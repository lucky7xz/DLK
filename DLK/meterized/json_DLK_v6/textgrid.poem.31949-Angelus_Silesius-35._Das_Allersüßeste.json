{"textgrid.poem.31949": {"metadata": {"author": {"name": "Angelus Silesius", "birth": "N.A.", "death": "N.A."}, "title": "35. Das Allers\u00fc\u00dfeste", "genre": "verse", "period": "N.A.", "pub_year": 1650, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00df ist der Honigseim, s\u00fc\u00df ist der Rebenmost,", "tokens": ["S\u00fc\u00df", "ist", "der", "Ho\u00b7ni\u00b7gseim", ",", "s\u00fc\u00df", "ist", "der", "Re\u00b7ben\u00b7most", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "S\u00fc\u00df ist das Himmelsbrot, der Isreliten Kost.", "tokens": ["S\u00fc\u00df", "ist", "das", "Him\u00b7mels\u00b7brot", ",", "der", "Is\u00b7re\u00b7li\u00b7ten", "Kost", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "S\u00fc\u00df ist, was Seraphin von Anbeginn empfunden,", "tokens": ["S\u00fc\u00df", "ist", ",", "was", "Se\u00b7ra\u00b7phin", "von", "An\u00b7be\u00b7ginn", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PRELS", "NE", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Noch s\u00fc\u00dfer ist, Herr Christ, das S\u00fc\u00dfe deiner Wunden.", "tokens": ["Noch", "s\u00fc\u00b7\u00dfer", "ist", ",", "Herr", "Christ", ",", "das", "S\u00fc\u00b7\u00dfe", "dei\u00b7ner", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["