{"dta.poem.7303": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Gru\u00df an Wilhelm Junkmann.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Mein L\u00e4mpchen zuckt, sein Docht verglimmt,", "tokens": ["Mein", "L\u00e4mp\u00b7chen", "zuckt", ",", "sein", "Docht", "ver\u00b7glimmt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Funken knistern im Kamine,", "tokens": ["Die", "Fun\u00b7ken", "knis\u00b7tern", "im", "Ka\u00b7mi\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie eine Nebeldecke schwimmt", "tokens": ["Wie", "ei\u00b7ne", "Ne\u00b7bel\u00b7de\u00b7cke", "schwimmt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es an des Saales hoher B\u00fchne;", "tokens": ["Es", "an", "des", "Saa\u00b7les", "ho\u00b7her", "B\u00fch\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Im Schneegest\u00f6ber schl\u00e4ft die Luft,", "tokens": ["Im", "Schnee\u00b7ge\u00b7st\u00f6\u00b7ber", "schl\u00e4ft", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Am Scheite ist das Harz entglommen,", "tokens": ["Am", "Schei\u00b7te", "ist", "das", "Harz", "ent\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mich d\u00fcnkt, als sp\u00fcr' ich einen Duft", "tokens": ["Mich", "d\u00fcnkt", ",", "als", "sp\u00fcr'", "ich", "ei\u00b7nen", "Duft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie Weihrauch an der Gruft des Frommen.", "tokens": ["Wie", "Weih\u00b7rauch", "an", "der", "Gruft", "des", "From\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}