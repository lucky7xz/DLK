{"dta.poem.10036": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die gew\u00e4sserte Wiese.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es hatte j\u00fcngst der klare Bach", "tokens": ["Es", "hat\u00b7te", "j\u00fcngst", "der", "kla\u00b7re", "Bach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus seinem Graben allgemach", "tokens": ["Aus", "sei\u00b7nem", "Gra\u00b7ben", "all\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit sanftem rieseln sich ergossen,", "tokens": ["Mit", "sanf\u00b7tem", "rie\u00b7seln", "sich", "er\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Wiesen frisches Gr\u00fcn stand mehrentheils beflossen:", "tokens": ["Der", "Wie\u00b7sen", "fri\u00b7sches", "Gr\u00fcn", "stand", "meh\u00b7ren\u00b7theils", "be\u00b7flos\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und diese Fluth war durch die reine Lufft,", "tokens": ["Und", "die\u00b7se", "Fluth", "war", "durch", "die", "rei\u00b7ne", "Lufft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die in ihr glattes Na\u00df die heitre Klarheit dr\u00fcckte,", "tokens": ["Die", "in", "ihr", "glat\u00b7tes", "Na\u00df", "die", "heit\u00b7re", "Klar\u00b7heit", "dr\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Blau, wie Lasur, gef\u00e4rbt: In diesem sch\u00f6nen Blauen", "tokens": ["Blau", ",", "wie", "La\u00b7sur", ",", "ge\u00b7f\u00e4rbt", ":", "In", "die\u00b7sem", "sch\u00f6\u00b7nen", "Blau\u00b7en"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "NN", "$,", "VVPP", "$.", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "War, durch des jungen Grases Sprossen,", "tokens": ["War", ",", "durch", "des", "jun\u00b7gen", "Gra\u00b7ses", "Spros\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Von welchen man die Spitzen nur erblickte,", "tokens": ["Von", "wel\u00b7chen", "man", "die", "Spit\u00b7zen", "nur", "er\u00b7blick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PIS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und die, nah an der Fluth, ein blitzend Lichtlein schm\u00fcckte,", "tokens": ["Und", "die", ",", "nah", "an", "der", "Fluth", ",", "ein", "blit\u00b7zend", "Licht\u00b7lein", "schm\u00fcck\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "ADJD", "APPR", "ART", "NN", "$,", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das allersch\u00f6nste Gr\u00fcn zu schauen.", "tokens": ["Das", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Gr\u00fcn", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die\u00df liebliche Gemisch von Himmel-blau und gr\u00fcn", "tokens": ["Die\u00df", "lieb\u00b7li\u00b7che", "Ge\u00b7misch", "von", "Him\u00b7mel\u00b7blau", "und", "gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJA", "NN", "APPR", "NN", "KON", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ergetzte Blick und Hertz recht ungemein. Es schien", "tokens": ["Er\u00b7getz\u00b7te", "Blick", "und", "Hertz", "recht", "un\u00b7ge\u00b7mein", ".", "Es", "schien"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN", "ADV", "ADV", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sich mit der Welt der Himmel zu verbinden,", "tokens": ["Sich", "mit", "der", "Welt", "der", "Him\u00b7mel", "zu", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Absonderlich, wann hier und dort der Bluhmen Pracht", "tokens": ["Ab\u00b7son\u00b7der\u00b7lich", ",", "wann", "hier", "und", "dort", "der", "Bluh\u00b7men", "Pracht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "PWAV", "ADV", "KON", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den Schmuck von einer sch\u00f6nen Nacht,", "tokens": ["Den", "Schmuck", "von", "ei\u00b7ner", "sch\u00f6\u00b7nen", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Sternen-gleicher Form und Glantz, zugleich uns wiese:", "tokens": ["In", "Ster\u00b7nen\u00b7glei\u00b7cher", "Form", "und", "Glantz", ",", "zu\u00b7gleich", "uns", "wie\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wir k\u00f6nnen von dem Paradiese", "tokens": ["Wir", "k\u00f6n\u00b7nen", "von", "dem", "Pa\u00b7ra\u00b7die\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Uns keinen lieblichern Begriff formiren,", "tokens": ["Uns", "kei\u00b7nen", "lieb\u00b7li\u00b7chern", "Be\u00b7griff", "for\u00b7mi\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zumahl,", "tokens": ["Zu\u00b7mahl", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Wann von dem kaum entwichnen Sonnen-Strahl", "tokens": ["Wann", "von", "dem", "kaum", "ent\u00b7wich\u00b7nen", "Son\u00b7nen\u00b7Strahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Abend-R\u00f6the sich in diesem Spiegel bildet,", "tokens": ["Die", "A\u00b7ben\u00b7dR\u00f6\u00b7the", "sich", "in", "die\u00b7sem", "Spie\u00b7gel", "bil\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und, nebst der herrlichen beflammten Farben Glantz,", "tokens": ["Und", ",", "nebst", "der", "herr\u00b7li\u00b7chen", "be\u00b7flamm\u00b7ten", "Far\u00b7ben", "Glantz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "ADJA", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Hier viele Stellen sch\u00f6n bepurpurt, andre gantz", "tokens": ["Hier", "vie\u00b7le", "Stel\u00b7len", "sch\u00f6n", "be\u00b7pur\u00b7purt", ",", "and\u00b7re", "gantz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PIAT", "NN", "ADJD", "VVFIN", "$,", "PIS", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit Nosen-farbnem Licht bestrahlet und verg\u00fcldet.", "tokens": ["Mit", "No\u00b7sen\u00b7fa\u00b7rb\u00b7nem", "Licht", "be\u00b7strah\u00b7let", "und", "ver\u00b7g\u00fcl\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+----+-+-+-+-", "measure": "dactylic.init"}, "line.9": {"text": "Das fast Saphirne Blau, das fast Smaragdne Gr\u00fcn,", "tokens": ["Das", "fast", "Sa\u00b7phir\u00b7ne", "Blau", ",", "das", "fast", "Sma\u00b7ragd\u00b7ne", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Der Glantz, der r\u00f6ther als Rubin,", "tokens": ["Der", "Glantz", ",", "der", "r\u00f6\u00b7ther", "als", "Ru\u00b7bin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "KOKOM", "NE", "$,"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.11": {"text": "Die hier, bald in getheilt-bald in vereinten Grentzen,", "tokens": ["Die", "hier", ",", "bald", "in", "get\u00b7heil\u00b7tbald", "in", "ver\u00b7ein\u00b7ten", "Grent\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "ADV", "APPR", "NE", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bald nicht, bald durch einander gl\u00e4ntzen;", "tokens": ["Bald", "nicht", ",", "bald", "durch", "ein\u00b7an\u00b7der", "gl\u00e4nt\u00b7zen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "$,", "ADV", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Die pr\u00e4gten voller Luft, mir die Betrachtung ein:", "tokens": ["Die", "pr\u00e4g\u00b7ten", "vol\u00b7ler", "Luft", ",", "mir", "die", "Be\u00b7trach\u00b7tung", "ein", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}