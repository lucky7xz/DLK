{"dta.poem.19284": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich will auch meinen Leib zur\u00fcck vom Staube fodern;", "tokens": ["Ich", "will", "auch", "mei\u00b7nen", "Leib", "zu\u00b7r\u00fcck", "vom", "Stau\u00b7be", "fo\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn nicht ein St\u00e4ubchen des, was mein ist, soll vermodern.", "tokens": ["Denn", "nicht", "ein", "St\u00e4ub\u00b7chen", "des", ",", "was", "mein", "ist", ",", "soll", "ver\u00b7mo\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "ART", "$,", "PRELS", "PPOSAT", "VAFIN", "$,", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was ich als ein Gewand hab' abgelegt im Grabe,", "tokens": ["Was", "ich", "als", "ein", "Ge\u00b7wand", "hab'", "ab\u00b7ge\u00b7legt", "im", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "KOUS", "ART", "NN", "VAFIN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Anzieh' ichs wieder, wann ich ausgeschlafen habe.", "tokens": ["An\u00b7zieh'", "ichs", "wie\u00b7der", ",", "wann", "ich", "aus\u00b7ge\u00b7schla\u00b7fen", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "$,", "PWAV", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es wird das alte Kleid, und doch ein neues seyn;", "tokens": ["Es", "wird", "das", "al\u00b7te", "Kleid", ",", "und", "doch", "ein", "neu\u00b7es", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "KON", "ADV", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Mutter in der Nacht wusch es dem Kinde rein.", "tokens": ["Die", "Mut\u00b7ter", "in", "der", "Nacht", "wusch", "es", "dem", "Kin\u00b7de", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}