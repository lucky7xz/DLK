{"textgrid.poem.37164": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "In des K\u00f6nigs Namen", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man warb ihn mit Gewalt und ri\u00df ihn von", "tokens": ["Man", "warb", "ihn", "mit", "Ge\u00b7walt", "und", "ri\u00df", "ihn", "von"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "NN", "KON", "VVFIN", "PPER", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem jungen Weib und lieben Sohn", "tokens": ["Dem", "jun\u00b7gen", "Weib", "und", "lie\u00b7ben", "Sohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum blut\u2013 und thr\u00e4nenvollen Frohn.", "tokens": ["Zum", "blut", "\u2013", "und", "thr\u00e4\u00b7nen\u00b7vol\u00b7len", "Frohn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$(", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Man zwang ihn zur Kapitulation", "tokens": ["Man", "zwang", "ihn", "zur", "Ka\u00b7pi\u00b7tu\u00b7la\u00b7ti\u00b7on"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Auf sieben Jahr. Die hat er treu gedient", "tokens": ["Auf", "sie\u00b7ben", "Jahr", ".", "Die", "hat", "er", "treu", "ge\u00b7dient"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "$.", "PDS", "VAFIN", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und nicht zu muksen sich erk\u00fchnt.", "tokens": ["Und", "nicht", "zu", "muk\u00b7sen", "sich", "er\u00b7k\u00fchnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PTKZU", "VVINF", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Zeit war um. \u2013 Nun wollt er von", "tokens": ["Die", "Zeit", "war", "um", ".", "\u2013", "Nun", "wollt", "er", "von"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "KOUI", "$.", "$(", "ADV", "VMFIN", "PPER", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dem blut\u2013 und thr\u00e4nenvollen Frohn", "tokens": ["Dem", "blut", "\u2013", "und", "thr\u00e4\u00b7nen\u00b7vol\u00b7len", "Frohn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$(", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Zum jungen Weib und lieben Sohn.", "tokens": ["Zum", "jun\u00b7gen", "Weib", "und", "lie\u00b7ben", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Allein umsonst war sein Verlangen.", "tokens": ["Al\u00b7lein", "um\u00b7sonst", "war", "sein", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Er bat, er weint und Pr\u00fcgel war sein Lohn.", "tokens": ["Er", "bat", ",", "er", "weint", "und", "Pr\u00fc\u00b7gel", "war", "sein", "Lohn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "KON", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Was that er da? Er lief davon,", "tokens": ["Was", "that", "er", "da", "?", "Er", "lief", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ward wieder gefangen", "tokens": ["Ward", "wie\u00b7der", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADV", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Und in des K\u00f6nigs Namen aufgehangen.", "tokens": ["Und", "in", "des", "K\u00f6\u00b7nigs", "Na\u00b7men", "auf\u00b7ge\u00b7han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}