{"textgrid.poem.64812": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Jetzt, da von kalter L\u00fcfte Schauern", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jetzt, da von kalter L\u00fcfte Schauern", "tokens": ["Jetzt", ",", "da", "von", "kal\u00b7ter", "L\u00fcf\u00b7te", "Schau\u00b7ern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die B\u00e4ume blatt- und bl\u00fcthenlos,", "tokens": ["Die", "B\u00e4u\u00b7me", "blat\u00b7t", "und", "bl\u00fc\u00b7then\u00b7los", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "TRUNC", "KON", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "F\u00fchl' ich in mir ein reuig Trauern,", "tokens": ["F\u00fchl'", "ich", "in", "mir", "ein", "reu\u00b7ig", "Trau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich den Fr\u00fchling nicht geno\u00df.", "tokens": ["Da\u00df", "ich", "den", "Fr\u00fch\u00b7ling", "nicht", "ge\u00b7no\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er war so sch\u00f6n mit seinen Rosen,", "tokens": ["Er", "war", "so", "sch\u00f6n", "mit", "sei\u00b7nen", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit seinem Nachtigallensang,", "tokens": ["Mit", "sei\u00b7nem", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7sang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit seines Hauches mildem Kosen", "tokens": ["Mit", "sei\u00b7nes", "Hau\u00b7ches", "mil\u00b7dem", "Ko\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und seinem frischen Bl\u00fcthendrang.", "tokens": ["Und", "sei\u00b7nem", "fri\u00b7schen", "Bl\u00fc\u00b7then\u00b7drang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mir aber flo\u00df ein Born der Thr\u00e4nen", "tokens": ["Mir", "a\u00b7ber", "flo\u00df", "ein", "Born", "der", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Inmitten dieser Fr\u00fchlingslust,", "tokens": ["In\u00b7mit\u00b7ten", "die\u00b7ser", "Fr\u00fch\u00b7lings\u00b7lust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich f\u00fchlte bei den frohsten Scenen", "tokens": ["Ich", "f\u00fchl\u00b7te", "bei", "den", "frohs\u00b7ten", "Sce\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Jammer nur der eignen Brust.", "tokens": ["Den", "Jam\u00b7mer", "nur", "der", "eig\u00b7nen", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und jetzt erst, da die kahlen B\u00e4ume", "tokens": ["Und", "jetzt", "erst", ",", "da", "die", "kah\u00b7len", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom ernsten Winterfrost versehrt,", "tokens": ["Vom", "erns\u00b7ten", "Win\u00b7ter\u00b7frost", "ver\u00b7sehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Reut mich's, da\u00df ich der heitern Tr\u00e4ume", "tokens": ["Reut", "mich's", ",", "da\u00df", "ich", "der", "hei\u00b7tern", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des lichten Fr\u00fchlings mich erwehrt.", "tokens": ["Des", "lich\u00b7ten", "Fr\u00fch\u00b7lings", "mich", "er\u00b7wehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So werd' vielleicht in k\u00fcnft'gen Tagen", "tokens": ["So", "werd'", "viel\u00b7leicht", "in", "k\u00fcnft'\u00b7gen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An eines andern Herbstes Grenz'", "tokens": ["An", "ei\u00b7nes", "an\u00b7dern", "Herbs\u00b7tes", "Grenz'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In eitler Sehnsucht bang' ich klagen", "tokens": ["In", "eit\u00b7ler", "Sehn\u00b7sucht", "bang'", "ich", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um meines Sein's entfloh'nen Lenz!", "tokens": ["Um", "mei\u00b7nes", "Sein's", "ent\u00b7floh'\u00b7nen", "Lenz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O, jetzt schon f\u00fchl' ich, wie die Frage", "tokens": ["O", ",", "jetzt", "schon", "f\u00fchl'", "ich", ",", "wie", "die", "Fra\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Tief schmerzlich meine Brust durchbebt:", "tokens": ["Tief", "schmerz\u00b7lich", "mei\u00b7ne", "Brust", "durch\u00b7bebt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Warum ich meine Fr\u00fchlingstage", "tokens": ["Wa\u00b7rum", "ich", "mei\u00b7ne", "Fr\u00fch\u00b7lings\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf w\u00fcster Meeresfahrt verlebt?", "tokens": ["Auf", "w\u00fcs\u00b7ter", "Mee\u00b7res\u00b7fahrt", "ver\u00b7lebt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}