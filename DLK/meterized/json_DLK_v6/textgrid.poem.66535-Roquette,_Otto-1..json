{"textgrid.poem.66535": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was dich bewegt in wechselvollen Stunden,", "tokens": ["Was", "dich", "be\u00b7wegt", "in", "wech\u00b7sel\u00b7vol\u00b7len", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gef\u00fchl, Gedanke, Zweifel oder Frage,", "tokens": ["Ge\u00b7f\u00fchl", ",", "Ge\u00b7dan\u00b7ke", ",", "Zwei\u00b7fel", "o\u00b7der", "Fra\u00b7ge", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bewahr' es dir, in reine Form gebunden,", "tokens": ["Be\u00b7wahr'", "es", "dir", ",", "in", "rei\u00b7ne", "Form", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Tag zu Tage!", "tokens": ["Von", "Tag", "zu", "Ta\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Dann siehst du, mocht' auch Irrthum dich gef\u00e4hrden,", "tokens": ["Dann", "siehst", "du", ",", "mocht'", "auch", "Irr\u00b7thum", "dich", "ge\u00b7f\u00e4hr\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "ADV", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mocht' edler Wille halten ihm die Wage,", "tokens": ["Mocht'", "ed\u00b7ler", "Wil\u00b7le", "hal\u00b7ten", "ihm", "die", "Wa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie du geworden, was dir bleibt zu werden,", "tokens": ["Wie", "du", "ge\u00b7wor\u00b7den", ",", "was", "dir", "bleibt", "zu", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAPP", "$,", "PWS", "PPER", "VVFIN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Tag zu Tage.", "tokens": ["Von", "Tag", "zu", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}