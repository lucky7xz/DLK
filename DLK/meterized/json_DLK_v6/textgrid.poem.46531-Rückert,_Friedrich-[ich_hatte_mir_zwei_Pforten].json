{"textgrid.poem.46531": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ich hatte mir zwei Pforten]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hatte mir zwei Pforten", "tokens": ["Ich", "hat\u00b7te", "mir", "zwei", "Pfor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am Hause gemacht,", "tokens": ["Am", "Hau\u00b7se", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und glaubt' an allen Orten", "tokens": ["Und", "glaubt'", "an", "al\u00b7len", "Or\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mich trefflich bedacht.", "tokens": ["Mich", "treff\u00b7lich", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Verschlossen war die eine,", "tokens": ["Ver\u00b7schlos\u00b7sen", "war", "die", "ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ART", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die andere nicht,", "tokens": ["Die", "an\u00b7de\u00b7re", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIS", "PTKNEG", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und recht verschlossen keine,", "tokens": ["Und", "recht", "ver\u00b7schlos\u00b7sen", "kei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "PIAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Besehn beim Licht.", "tokens": ["Be\u00b7sehn", "beim", "Licht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Zur offnen Pforte flogen", "tokens": ["Zur", "off\u00b7nen", "Pfor\u00b7te", "flo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Freuden hinaus,", "tokens": ["Die", "Freu\u00b7den", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "APZR", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ein zur verschlo\u00dfnen zogen", "tokens": ["Ein", "zur", "ver\u00b7schlo\u00df\u00b7nen", "zo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPRART", "ADJA", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Die Sorgen ins Haus.", "tokens": ["Die", "Sor\u00b7gen", "ins", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Die hatt' ich nicht verschlossen,", "tokens": ["Die", "hatt'", "ich", "nicht", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch die sie entflohn,", "tokens": ["Durch", "die", "sie", "ent\u00b7flohn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Was wu\u00dft' ich, da\u00df verdrossen", "tokens": ["Was", "wu\u00dft'", "ich", ",", "da\u00df", "ver\u00b7dros\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie wollten davon?", "tokens": ["Sie", "woll\u00b7ten", "da\u00b7von", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PAV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Die hatt' ich wohl versiegelt,", "tokens": ["Die", "hatt'", "ich", "wohl", "ver\u00b7sie\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was hilfts, da sie nahn?", "tokens": ["Was", "hilfts", ",", "da", "sie", "nahn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Sie haben sie entriegelt", "tokens": ["Sie", "ha\u00b7ben", "sie", "ent\u00b7rie\u00b7gelt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und weit aufgethan.", "tokens": ["Und", "weit", "auf\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Nun m\u00f6gen offen bleiben", "tokens": ["Nun", "m\u00f6\u00b7gen", "of\u00b7fen", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zwei Pforten am Haus;", "tokens": ["Zwei", "Pfor\u00b7ten", "am", "Haus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Sie lassen doch sich treiben", "tokens": ["Sie", "las\u00b7sen", "doch", "sich", "trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu keiner hinaus.", "tokens": ["Zu", "kei\u00b7ner", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Sie halten hier verschlungen", "tokens": ["Sie", "hal\u00b7ten", "hier", "ver\u00b7schlun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Mondschein den Tanz,", "tokens": ["Im", "Mond\u00b7schein", "den", "Tanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wo aus Erinnerungen", "tokens": ["Wo", "aus", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie flechten den Kranz.", "tokens": ["Sie", "flech\u00b7ten", "den", "Kranz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Man merket kaum im Hause", "tokens": ["Man", "mer\u00b7ket", "kaum", "im", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die schwebende Schaar,", "tokens": ["Die", "schwe\u00b7ben\u00b7de", "Schaar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "So still ist's, wo vom Brause", "tokens": ["So", "still", "ist's", ",", "wo", "vom", "Brau\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So laut einst es war.", "tokens": ["So", "laut", "einst", "es", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "VAFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Ihr weiten R\u00e4ume schienet", "tokens": ["Ihr", "wei\u00b7ten", "R\u00e4u\u00b7me", "schie\u00b7net"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So voll, nun so leer,", "tokens": ["So", "voll", ",", "nun", "so", "leer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADV", "ADJD", "$,"], "meter": "----+", "measure": "unknown.measure.single"}, "line.3": {"text": "Seit euch zur F\u00fcllung dienet", "tokens": ["Seit", "euch", "zur", "F\u00fcl\u00b7lung", "die\u00b7net"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Schatten ein Heer.", "tokens": ["Von", "Schat\u00b7ten", "ein", "Heer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}