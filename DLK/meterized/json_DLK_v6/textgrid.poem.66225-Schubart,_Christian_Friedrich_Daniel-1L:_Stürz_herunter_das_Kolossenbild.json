{"textgrid.poem.66225": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1L: St\u00fcrz herunter das Kolossenbild", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "St\u00fcrz herunter das Kolossenbild", "tokens": ["St\u00fcrz", "her\u00b7un\u00b7ter", "das", "Ko\u00b7los\u00b7sen\u00b7bild"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APZR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "fremder Barbarei!", "tokens": ["frem\u00b7der", "Bar\u00b7ba\u00b7rei", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df es br\u00fcllt \u2013 im Falle br\u00fcllt,", "tokens": ["Da\u00df", "es", "br\u00fcllt", "\u2013", "im", "Fal\u00b7le", "br\u00fcllt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$(", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "da\u00df es Schutt im Thale sei.", "tokens": ["da\u00df", "es", "Schutt", "im", "Tha\u00b7le", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPRART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00fchr den J\u00fcngling an den Tr\u00fcmmern", "tokens": ["F\u00fchr", "den", "J\u00fcng\u00b7ling", "an", "den", "Tr\u00fcm\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "stolz vor\u00fcber \u2013 zeig ihm bald", "tokens": ["stolz", "vor\u00b7\u00fc\u00b7ber", "\u2013", "zeig", "ihm", "bald"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "$(", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "in Thuiskons Eichenwald", "tokens": ["in", "Thu\u00b7is\u00b7kons", "Ei\u00b7chen\u00b7wald"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Hermanns Krone schimmern! \u2013", "tokens": ["Her\u00b7manns", "Kro\u00b7ne", "schim\u00b7mern", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Zeig ihm dann den Silbermond,", "tokens": ["Zeig", "ihm", "dann", "den", "Sil\u00b7ber\u00b7mond", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "wo ein Heinrich thront,", "tokens": ["wo", "ein", "Hein\u00b7rich", "thront", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NE", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "wo der Denker Leibniz wohnt \u2013", "tokens": ["wo", "der", "Den\u00b7ker", "Leib\u00b7niz", "wohnt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Und die Wellen von dem alten Rhein", "tokens": ["Und", "die", "Wel\u00b7len", "von", "dem", "al\u00b7ten", "Rhein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "ADJA", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "schlagen Beifall brausend drein,", "tokens": ["schla\u00b7gen", "Bei\u00b7fall", "brau\u00b7send", "drein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "wenn der Lehrer Klein", "tokens": ["wenn", "der", "Leh\u00b7rer", "Klein"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NE"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.15": {"text": "Deutsche fleht, sie sollen Deutsche sein.", "tokens": ["Deut\u00b7sche", "fleht", ",", "sie", "sol\u00b7len", "Deut\u00b7sche", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PPER", "PIAT", "NN", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}