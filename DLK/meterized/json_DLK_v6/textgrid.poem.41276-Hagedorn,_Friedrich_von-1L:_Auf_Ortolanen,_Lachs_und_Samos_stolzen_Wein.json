{"textgrid.poem.41276": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf Ortolanen, Lachs und Samos stolzen Wein", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf Ortolanen, Lachs und Samos stolzen Wein", "tokens": ["Auf", "Or\u00b7to\u00b7la\u00b7nen", ",", "Lachs", "und", "Sa\u00b7mos", "stol\u00b7zen", "Wein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat oft Arist das Gl\u00fcck, Suffenens Gast zu sein.", "tokens": ["Hat", "oft", "A\u00b7rist", "das", "Gl\u00fcck", ",", "Suf\u00b7fe\u00b7nens", "Gast", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "ART", "NN", "$,", "NE", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dann aber liest Suffen ihm seiner Dichtkunst Proben,", "tokens": ["Dann", "a\u00b7ber", "liest", "Suf\u00b7fen", "ihm", "sei\u00b7ner", "Dicht\u00b7kunst", "Pro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "NN", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und diese mu\u00df Arist stets h\u00f6ren und stets loben.", "tokens": ["Und", "die\u00b7se", "mu\u00df", "A\u00b7rist", "stets", "h\u00f6\u00b7ren", "und", "stets", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "NN", "ADV", "VVINF", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nun \u00fcbersch\u00e4tze nicht dein theures Mahl, Suffen:", "tokens": ["Nun", "\u00fc\u00b7bersc\u00b7h\u00e4t\u00b7ze", "nicht", "dein", "theu\u00b7res", "Mahl", ",", "Suf\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "PPOSAT", "ADJA", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Gewi\u00df, nur f\u00fcr Arist k\u00f6mmt es recht hoch zu stehn.", "tokens": ["Ge\u00b7wi\u00df", ",", "nur", "f\u00fcr", "A\u00b7rist", "k\u00f6mmt", "es", "recht", "hoch", "zu", "stehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "APPR", "NN", "VVFIN", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-++-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}