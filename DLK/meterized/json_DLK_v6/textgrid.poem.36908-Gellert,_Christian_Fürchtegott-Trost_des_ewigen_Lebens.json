{"textgrid.poem.36908": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Trost des ewigen Lebens", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach einer Pr\u00fcfung kurzer Tage", "tokens": ["Nach", "ei\u00b7ner", "Pr\u00fc\u00b7fung", "kur\u00b7zer", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erwartet uns die Ewigkeit.", "tokens": ["Er\u00b7war\u00b7tet", "uns", "die", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort, dort verwandelt sich die Klage", "tokens": ["Dort", ",", "dort", "ver\u00b7wan\u00b7delt", "sich", "die", "Kla\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In g\u00f6ttliche Zufriedenheit.", "tokens": ["In", "g\u00f6tt\u00b7li\u00b7che", "Zu\u00b7frie\u00b7den\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hier \u00fcbt die Tugend ihren Flei\u00df;", "tokens": ["Hier", "\u00fcbt", "die", "Tu\u00b7gend", "ih\u00b7ren", "Flei\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und jene Welt reicht ihr den Preis.", "tokens": ["Und", "je\u00b7ne", "Welt", "reicht", "ihr", "den", "Preis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wahr ist's, der Fromme schmeckt auf Erden", "tokens": ["Wahr", "ist's", ",", "der", "From\u00b7me", "schmeckt", "auf", "Er\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schon manchen selgen Augenblick;", "tokens": ["Schon", "man\u00b7chen", "sel\u00b7gen", "Au\u00b7gen\u00b7blick", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch alle Freuden, die ihm werden,", "tokens": ["Doch", "al\u00b7le", "Freu\u00b7den", ",", "die", "ihm", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sind ihm ein unvollkommnes Gl\u00fcck.", "tokens": ["Sind", "ihm", "ein", "un\u00b7voll\u00b7komm\u00b7nes", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er bleibt ein Mensch, und seine Ruh", "tokens": ["Er", "bleibt", "ein", "Mensch", ",", "und", "sei\u00b7ne", "Ruh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nimmt in der Seele ab und zu.", "tokens": ["Nimmt", "in", "der", "See\u00b7le", "ab", "und", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Bald st\u00f6ren ihn des K\u00f6rpers Schmerzen,", "tokens": ["Bald", "st\u00f6\u00b7ren", "ihn", "des", "K\u00f6r\u00b7pers", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bald das Ger\u00e4usche dieser Welt;", "tokens": ["Bald", "das", "Ge\u00b7r\u00e4u\u00b7sche", "die\u00b7ser", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald k\u00e4mpft in seinem eignen Herzen", "tokens": ["Bald", "k\u00e4mpft", "in", "sei\u00b7nem", "eig\u00b7nen", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Feind, der \u00f6fter siegt, als f\u00e4llt;", "tokens": ["Ein", "Feind", ",", "der", "\u00f6f\u00b7ter", "siegt", ",", "als", "f\u00e4llt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,", "KOUS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bald sinkt er durch des N\u00e4chsten Schuld", "tokens": ["Bald", "sinkt", "er", "durch", "des", "N\u00e4chs\u00b7ten", "Schuld"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Kummer und in Ungeduld.", "tokens": ["In", "Kum\u00b7mer", "und", "in", "Un\u00b7ge\u00b7duld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Hier, wo die Tugend \u00f6fters leidet,", "tokens": ["Hier", ",", "wo", "die", "Tu\u00b7gend", "\u00f6f\u00b7ters", "lei\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Laster \u00f6fters gl\u00fccklich ist,", "tokens": ["Das", "Las\u00b7ter", "\u00f6f\u00b7ters", "gl\u00fcck\u00b7lich", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo man den Gl\u00fccklichen beneidet,", "tokens": ["Wo", "man", "den", "Gl\u00fcck\u00b7li\u00b7chen", "be\u00b7nei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und des Bek\u00fcmmerten vergi\u00dft;", "tokens": ["Und", "des", "Be\u00b7k\u00fcm\u00b7mer\u00b7ten", "ver\u00b7gi\u00dft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Hier kann der Mensch nie frei von Pein,", "tokens": ["Hier", "kann", "der", "Mensch", "nie", "frei", "von", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nie frei von eigner Schwachheit sein.", "tokens": ["Nie", "frei", "von", "eig\u00b7ner", "Schwach\u00b7heit", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Hier such ich's nur, dort werd ich's finden;", "tokens": ["Hier", "such", "ich's", "nur", ",", "dort", "werd", "ich's", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$,", "ADV", "VAFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dort werd ich, heilig und verkl\u00e4rt,", "tokens": ["Dort", "werd", "ich", ",", "hei\u00b7lig", "und", "ver\u00b7kl\u00e4rt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Tugend ganzen Wert empfinden,", "tokens": ["Der", "Tu\u00b7gend", "gan\u00b7zen", "Wert", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den unaussprechlich gro\u00dfen Wert;", "tokens": ["Den", "un\u00b7aus\u00b7sprech\u00b7lich", "gro\u00b7\u00dfen", "Wert", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Gott der Liebe werd ich sehn,", "tokens": ["Den", "Gott", "der", "Lie\u00b7be", "werd", "ich", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihn lieben, ewig ihn erh\u00f6hn.", "tokens": ["Ihn", "lie\u00b7ben", ",", "e\u00b7wig", "ihn", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Da wird der Vorsicht heilger Wille", "tokens": ["Da", "wird", "der", "Vor\u00b7sicht", "heil\u00b7ger", "Wil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Will und meine Wohlfahrt sein;", "tokens": ["Mein", "Will", "und", "mei\u00b7ne", "Wohl\u00b7fahrt", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und lieblich Wesen, Heil die F\u00fclle", "tokens": ["Und", "lieb\u00b7lich", "We\u00b7sen", ",", "Heil", "die", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "NN", "$,", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Am Throne Gottes mich erfreun.", "tokens": ["Am", "Thro\u00b7ne", "Got\u00b7tes", "mich", "er\u00b7freun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann l\u00e4\u00dft Gewinn stets auf Gewinn", "tokens": ["Dann", "l\u00e4\u00dft", "Ge\u00b7winn", "stets", "auf", "Ge\u00b7winn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mich f\u00fchlen, da\u00df ich ewig bin.", "tokens": ["Mich", "f\u00fch\u00b7len", ",", "da\u00df", "ich", "e\u00b7wig", "bin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Da werd ich das im Licht erkennen,", "tokens": ["Da", "werd", "ich", "das", "im", "Licht", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PDS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was ich auf Erden dunkel sah;", "tokens": ["Was", "ich", "auf", "Er\u00b7den", "dun\u00b7kel", "sah", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das wunderbar und heilig nennen,", "tokens": ["Das", "wun\u00b7der\u00b7bar", "und", "hei\u00b7lig", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was unerforschlich hier geschah;", "tokens": ["Was", "un\u00b7er\u00b7for\u00b7schlich", "hier", "ge\u00b7schah", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Da denkt mein Geist mit Preis und Dank", "tokens": ["Da", "denkt", "mein", "Geist", "mit", "Preis", "und", "Dank"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Schickung im Zusammenhang.", "tokens": ["Die", "Schi\u00b7ckung", "im", "Zu\u00b7sam\u00b7men\u00b7hang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.8": {"line.1": {"text": "Da werd ich zu dem Throne dringen,", "tokens": ["Da", "werd", "ich", "zu", "dem", "Thro\u00b7ne", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Gott, mein Heil, sich offenbart;", "tokens": ["Wo", "Gott", ",", "mein", "Heil", ",", "sich", "of\u00b7fen\u00b7bart", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PPOSAT", "NN", "$,", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Heilig, Heilig, Heilig singen", "tokens": ["Ein", "Hei\u00b7lig", ",", "Hei\u00b7lig", ",", "Hei\u00b7lig", "sin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Lamme, das erw\u00fcrget ward;", "tokens": ["Dem", "Lam\u00b7me", ",", "das", "er\u00b7w\u00fcr\u00b7get", "ward", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Cherubim und Seraphim", "tokens": ["Und", "Che\u00b7ru\u00b7bim", "und", "Se\u00b7ra\u00b7phim"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und alle Himmel jauchzen ihm.", "tokens": ["Und", "al\u00b7le", "Him\u00b7mel", "jauch\u00b7zen", "ihm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Da werd ich in der Engel Scharen", "tokens": ["Da", "werd", "ich", "in", "der", "En\u00b7gel", "Scha\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mich ihnen gleich und heilig sehn,", "tokens": ["Mich", "ih\u00b7nen", "gleich", "und", "hei\u00b7lig", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ADV", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das nie gest\u00f6rte Gl\u00fcck erfahren,", "tokens": ["Das", "nie", "ge\u00b7st\u00f6r\u00b7te", "Gl\u00fcck", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Frommen stets fromm umzugehn.", "tokens": ["Mit", "From\u00b7men", "stets", "fromm", "um\u00b7zu\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJD", "VVIZU", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Da wird durch jeden Augenblick", "tokens": ["Da", "wird", "durch", "je\u00b7den", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr Heil mein Heil, mein Gl\u00fcck ihr Gl\u00fcck.", "tokens": ["Ihr", "Heil", "mein", "Heil", ",", "mein", "Gl\u00fcck", "ihr", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Da werd ich dem den Dank bezahlen,", "tokens": ["Da", "werd", "ich", "dem", "den", "Dank", "be\u00b7zah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Gottes Weg mich gehen hie\u00df,", "tokens": ["Der", "Got\u00b7tes", "Weg", "mich", "ge\u00b7hen", "hie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ihn zu millionen Malen", "tokens": ["Und", "ihn", "zu", "mil\u00b7li\u00b7o\u00b7nen", "Ma\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch segnen, da\u00df er mir ihn wies;", "tokens": ["Noch", "seg\u00b7nen", ",", "da\u00df", "er", "mir", "ihn", "wies", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "KOUS", "PPER", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da find ich in des H\u00f6chsten Hand", "tokens": ["Da", "find", "ich", "in", "des", "H\u00f6chs\u00b7ten", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Freund, den ich auf Erden fand.", "tokens": ["Den", "Freund", ",", "den", "ich", "auf", "Er\u00b7den", "fand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Da ruft, o m\u00f6chte Gott es geben!", "tokens": ["Da", "ruft", ",", "o", "m\u00f6ch\u00b7te", "Gott", "es", "ge\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "FM", "VMFIN", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vielleicht auch mir ein Selger zu:", "tokens": ["Viel\u00b7leicht", "auch", "mir", "ein", "Sel\u00b7ger", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Heil sei dir! denn du hast mein Leben,", "tokens": ["Heil", "sei", "dir", "!", "denn", "du", "hast", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$.", "KON", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Seele mir gerettet; du!", "tokens": ["Die", "See\u00b7le", "mir", "ge\u00b7ret\u00b7tet", ";", "du", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$.", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O Gott, wie mu\u00df dies Gl\u00fcck erfreun,", "tokens": ["O", "Gott", ",", "wie", "mu\u00df", "dies", "Gl\u00fcck", "er\u00b7freun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VMFIN", "PDS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Retter einer Seele sein!", "tokens": ["Der", "Ret\u00b7ter", "ei\u00b7ner", "See\u00b7le", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Was seid ihr, Leiden dieser Erden,", "tokens": ["Was", "seid", "ihr", ",", "Lei\u00b7den", "die\u00b7ser", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch gegen jene Herrlichkeit,", "tokens": ["Doch", "ge\u00b7gen", "je\u00b7ne", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die offenbart an uns soll werden,", "tokens": ["Die", "of\u00b7fen\u00b7bart", "an", "uns", "soll", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPER", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Ewigkeit zu Ewigkeit?", "tokens": ["Von", "E\u00b7wig\u00b7keit", "zu", "E\u00b7wig\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie nichts, wie gar nichts gegen sie,", "tokens": ["Wie", "nichts", ",", "wie", "gar", "nichts", "ge\u00b7gen", "sie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "PWAV", "ADV", "PIS", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist doch ein Augenblick voll M\u00fch!", "tokens": ["Ist", "doch", "ein", "Au\u00b7gen\u00b7blick", "voll", "M\u00fch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}