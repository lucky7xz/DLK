{"textgrid.poem.32874": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Erwachet, holde Nachtigallen!", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erwachet, holde Nachtigallen!", "tokens": ["Er\u00b7wa\u00b7chet", ",", "hol\u00b7de", "Nach\u00b7ti\u00b7gal\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00dft eure fr\u00fchen Lieder schallen!", "tokens": ["La\u00dft", "eu\u00b7re", "fr\u00fc\u00b7hen", "Lie\u00b7der", "schal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Liebe wecket euch,", "tokens": ["Die", "Lie\u00b7be", "we\u00b7cket", "euch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und mich zugleich.", "tokens": ["Und", "mich", "zu\u00b7gleich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "O w\u00e4r ich euch in allem gleich!", "tokens": ["O", "w\u00e4r", "ich", "euch", "in", "al\u00b7lem", "gleich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PRF", "APPR", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Allein, ihr singt der Liebe Freuden,", "tokens": ["Al\u00b7lein", ",", "ihr", "singt", "der", "Lie\u00b7be", "Freu\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ich aber ihre Leiden; \u2013 \u2013", "tokens": ["Ich", "a\u00b7ber", "ih\u00b7re", "Lei\u00b7den", ";", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "NN", "$.", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie sehr beneid ich euch!", "tokens": ["Wie", "sehr", "be\u00b7neid", "ich", "euch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}