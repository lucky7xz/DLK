{"textgrid.poem.65131": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es ist in diesem Weltgetriebe", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist in diesem Weltgetriebe", "tokens": ["Es", "ist", "in", "die\u00b7sem", "Welt\u00b7ge\u00b7trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nichts s\u00fc\u00df und heilig als die Liebe.", "tokens": ["Nichts", "s\u00fc\u00df", "und", "hei\u00b7lig", "als", "die", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "KON", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Schmerz nur wesenhaft und wahr.", "tokens": ["Der", "Schmerz", "nur", "we\u00b7sen\u00b7haft", "und", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Drum hab' ich, frei mit mir zu schalten,", "tokens": ["Drum", "hab'", "ich", ",", "frei", "mit", "mir", "zu", "schal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "$,", "ADJD", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den beiden, g\u00f6ttlichen Gewalten", "tokens": ["Den", "bei\u00b7den", ",", "g\u00f6tt\u00b7li\u00b7chen", "Ge\u00b7wal\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "PIAT", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mich hingegeben ganz und gar!", "tokens": ["Mich", "hin\u00b7ge\u00b7ge\u00b7ben", "ganz", "und", "gar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vermag ich es des Lebens H\u00f6hen", "tokens": ["Ver\u00b7mag", "ich", "es", "des", "Le\u00b7bens", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und seine Tiefen zu verstehen,", "tokens": ["Und", "sei\u00b7ne", "Tie\u00b7fen", "zu", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So dank ich's ihnen nur allein.", "tokens": ["So", "dank", "ich's", "ih\u00b7nen", "nur", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIS", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie f\u00fchrten, wie Virgil den Dante,", "tokens": ["Sie", "f\u00fchr\u00b7ten", ",", "wie", "Vir\u00b7gil", "den", "Dan\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Herz, das still und tief entbrannt;", "tokens": ["Mein", "Herz", ",", "das", "still", "und", "tief", "ent\u00b7brannt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zur H\u00f6lle und zum Himmel ein! \u2013", "tokens": ["Zur", "H\u00f6l\u00b7le", "und", "zum", "Him\u00b7mel", "ein", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}