{"textgrid.poem.38871": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer in den Blumen, W\u00e4ldern, Bergesreihen,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer in den Blumen, W\u00e4ldern, Bergesreihen,", "tokens": ["Wer", "in", "den", "Blu\u00b7men", ",", "W\u00e4l\u00b7dern", ",", "Ber\u00b7ges\u00b7rei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im klaren Flu\u00df, der sich mit B\u00e4umen schm\u00fccket,", "tokens": ["Im", "kla\u00b7ren", "Flu\u00df", ",", "der", "sich", "mit", "B\u00e4u\u00b7men", "schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur Endliches, Verg\u00e4ngliches erblicket,", "tokens": ["Nur", "End\u00b7li\u00b7ches", ",", "Ver\u00b7g\u00e4ng\u00b7li\u00b7ches", "er\u00b7bli\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der traure tief im hellsten Glanz des Mayen.", "tokens": ["Der", "trau\u00b7re", "tief", "im", "hells\u00b7ten", "Glanz", "des", "Ma\u00b7yen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Nur der kann sich der heil'gen Sch\u00f6ne freuen,", "tokens": ["Nur", "der", "kann", "sich", "der", "heil'\u00b7gen", "Sch\u00f6\u00b7ne", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "VMFIN", "PRF", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Blume, Wald und Strom zur Tief' entr\u00fccket,", "tokens": ["Den", "Blu\u00b7me", ",", "Wald", "und", "Strom", "zur", "Tie\u00b7f'", "ent\u00b7r\u00fc\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wo unverg\u00e4nglich ihn die Bl\u00fcht' entz\u00fccket,", "tokens": ["Wo", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "ihn", "die", "Bl\u00fcht'", "ent\u00b7z\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem ew'gen Glanze keine Schatten dr\u00e4uen.", "tokens": ["Dem", "ew'\u00b7gen", "Glan\u00b7ze", "kei\u00b7ne", "Schat\u00b7ten", "dr\u00e4u\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Noch sch\u00f6ner deutet nach dem hohen Ziele", "tokens": ["Noch", "sch\u00f6\u00b7ner", "deu\u00b7tet", "nach", "dem", "ho\u00b7hen", "Zie\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Menschen Blick, erhabene Gebehrde,", "tokens": ["Des", "Men\u00b7schen", "Blick", ",", "er\u00b7ha\u00b7be\u00b7ne", "Ge\u00b7behr\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Des Busens Ahnden, Sehnsucht nach dem Frieden.", "tokens": ["Des", "Bu\u00b7sens", "Ahn\u00b7den", ",", "Sehn\u00b7sucht", "nach", "dem", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Seit ich sich sah, vertraut' ich dem Gef\u00fchle,", "tokens": ["Seit", "ich", "sich", "sah", ",", "ver\u00b7traut'", "ich", "dem", "Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du mu\u00dftest von uns gehn und dieser Erde.", "tokens": ["Du", "mu\u00df\u00b7test", "von", "uns", "gehn", "und", "die\u00b7ser", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "VVINF", "KON", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du gingst: fahr wohl; wir sind ja nicht geschieden.", "tokens": ["Du", "gingst", ":", "fahr", "wohl", ";", "wir", "sind", "ja", "nicht", "ge\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "ADV", "$.", "PPER", "VAFIN", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}