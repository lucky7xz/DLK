{"textgrid.poem.54103": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Die Frau spricht", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der deutsche Mann", "tokens": ["Der", "deut\u00b7sche", "Mann"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Mann", "tokens": ["Mann"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Mann \u2013", "tokens": ["Mann", "\u2013"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "das ist der unverstandene Mann.", "tokens": ["das", "ist", "der", "un\u00b7ver\u00b7stan\u00b7de\u00b7ne", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Er hat ein Gesch\u00e4ft, und er hat eine Pflicht.", "tokens": ["Er", "hat", "ein", "Ge\u00b7sch\u00e4ft", ",", "und", "er", "hat", "ei\u00b7ne", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "KON", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Er hat einen Sitz im Oberamtsgericht.", "tokens": ["Er", "hat", "ei\u00b7nen", "Sitz", "im", "O\u00b7be\u00b7ramts\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+---+-+-+-+", "measure": "dactylic.init"}, "line.7": {"text": "Er hat auch eine Frau \u2013 das wei\u00df er aber nicht.", "tokens": ["Er", "hat", "auch", "ei\u00b7ne", "Frau", "\u2013", "das", "wei\u00df", "er", "a\u00b7ber", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$(", "PDS", "VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Er sagt: \u00bbMein liebes Kind . . . \u00ab und ist sonst ganz vergn\u00fcgt \u2013", "tokens": ["Er", "sagt", ":", "\u00bb", "Mein", "lie\u00b7bes", "Kind", ".", ".", ".", "\u00ab", "und", "ist", "sonst", "ganz", "ver\u00b7gn\u00fcgt", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PPOSAT", "ADJA", "NN", "$.", "$.", "$.", "$(", "KON", "VAFIN", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er ist ein Mann. Und das", "tokens": ["Er", "ist", "ein", "Mann", ".", "Und", "das"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "KON", "ART"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.10": {"text": "gen\u00fcgt.", "tokens": ["ge\u00b7n\u00fcgt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Der deutsche Mann", "tokens": ["Der", "deut\u00b7sche", "Mann"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Mann", "tokens": ["Mann"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Mann \u2013", "tokens": ["Mann", "\u2013"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "das ist der unverstandene Mann.", "tokens": ["das", "ist", "der", "un\u00b7ver\u00b7stan\u00b7de\u00b7ne", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Die Frau versteht ja doch nichts, von dem, was ihn qu\u00e4lt.", "tokens": ["Die", "Frau", "ver\u00b7steht", "ja", "doch", "nichts", ",", "von", "dem", ",", "was", "ihn", "qu\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "PIS", "$,", "APPR", "ART", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Frau ist dazu da, da\u00df sie die Kragen z\u00e4hlt.", "tokens": ["Die", "Frau", "ist", "da\u00b7zu", "da", ",", "da\u00df", "sie", "die", "Kra\u00b7gen", "z\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PAV", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Frau ist daran schuld, wenn ihm ein Hemdknopf fehlt.", "tokens": ["Die", "Frau", "ist", "da\u00b7ran", "schuld", ",", "wenn", "ihm", "ein", "Hemd\u00b7knopf", "fehlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PAV", "ADJD", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und kommt es einmal vor, da\u00df er die Frau betr\u00fcgt:", "tokens": ["Und", "kommt", "es", "ein\u00b7mal", "vor", ",", "da\u00df", "er", "die", "Frau", "be\u00b7tr\u00fcgt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er ist ein Mann. Und das", "tokens": ["Er", "ist", "ein", "Mann", ".", "Und", "das"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "KON", "ART"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.10": {"text": "gen\u00fcgt.", "tokens": ["ge\u00b7n\u00fcgt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Der deutsche Mann", "tokens": ["Der", "deut\u00b7sche", "Mann"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Mann", "tokens": ["Mann"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Mann \u2013", "tokens": ["Mann", "\u2013"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "das ist der unverstandene Mann.", "tokens": ["das", "ist", "der", "un\u00b7ver\u00b7stan\u00b7de\u00b7ne", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Er gibt sich nicht viel M\u00fche, wenn er die Frau umgirrt.", "tokens": ["Er", "gibt", "sich", "nicht", "viel", "M\u00fc\u00b7he", ",", "wenn", "er", "die", "Frau", "um\u00b7girrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "PIAT", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+--++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Und kriegt er nicht die eine, kommt die andere", "tokens": ["Und", "kriegt", "er", "nicht", "die", "ei\u00b7ne", ",", "kommt", "die", "an\u00b7de\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "ART", "$,", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "angeschwirrt.", "tokens": ["an\u00b7ge\u00b7schwirrt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Daher der deutsche Mann denn stets befriedigt wird.", "tokens": ["Da\u00b7her", "der", "deut\u00b7sche", "Mann", "denn", "stets", "be\u00b7frie\u00b7digt", "wird", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "KON", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hauptsache ist, da\u00df sie bequem und sich gehorsam f\u00fcgt.", "tokens": ["Haupt\u00b7sa\u00b7che", "ist", ",", "da\u00df", "sie", "be\u00b7quem", "und", "sich", "ge\u00b7hor\u00b7sam", "f\u00fcgt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "KOUS", "PPER", "ADJD", "KON", "PRF", "ADJD", "VVFIN", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.10": {"text": "Denn er ist Mann. Und das", "tokens": ["Denn", "er", "ist", "Mann", ".", "Und", "das"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "NN", "$.", "KON", "ART"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.11": {"text": "gen\u00fcgt.", "tokens": ["ge\u00b7n\u00fcgt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.4": {"line.1": {"text": "Der deutsche Mann", "tokens": ["Der", "deut\u00b7sche", "Mann"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Mann", "tokens": ["Mann"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Mann \u2013", "tokens": ["Mann", "\u2013"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "das ist der unverstandene Mann.", "tokens": ["das", "ist", "der", "un\u00b7ver\u00b7stan\u00b7de\u00b7ne", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Er flirtet nicht mit seiner Frau. Er kauft ihr doch den Hut!", "tokens": ["Er", "flir\u00b7tet", "nicht", "mit", "sei\u00b7ner", "Frau", ".", "Er", "kauft", "ihr", "doch", "den", "Hut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Sie sieht ihn von der Seite an, wenn er so schnarchend ruht.", "tokens": ["Sie", "sieht", "ihn", "von", "der", "Sei\u00b7te", "an", ",", "wenn", "er", "so", "schnar\u00b7chend", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Ein kleines bi\u00dfchen Z\u00e4rtlichkeit \u2013 und alles w\u00e4re gut.", "tokens": ["Ein", "klei\u00b7nes", "bi\u00df\u00b7chen", "Z\u00e4rt\u00b7lich\u00b7keit", "\u2013", "und", "al\u00b7les", "w\u00e4\u00b7re", "gut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$(", "KON", "PIS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Er ist ein Beamter der Liebe. Er l\u00e4\u00dft sich gehn.", "tokens": ["Er", "ist", "ein", "Be\u00b7am\u00b7ter", "der", "Lie\u00b7be", ".", "Er", "l\u00e4\u00dft", "sich", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$.", "PPER", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.9": {"text": "Er hat sie doch geheiratet \u2013 was soll jetzt noch geschehn?", "tokens": ["Er", "hat", "sie", "doch", "ge\u00b7hei\u00b7ra\u00b7tet", "\u2013", "was", "soll", "jetzt", "noch", "ge\u00b7schehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$(", "PWS", "VMFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "Der Mensch, der soll nicht scheiden, was Gott zusammenf\u00fcgt.", "tokens": ["Der", "Mensch", ",", "der", "soll", "nicht", "schei\u00b7den", ",", "was", "Gott", "zu\u00b7sam\u00b7men\u00b7f\u00fcgt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VMFIN", "PTKNEG", "VVINF", "$,", "PRELS", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Er ist ein Mann. Und das", "tokens": ["Er", "ist", "ein", "Mann", ".", "Und", "das"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "KON", "ART"], "meter": "-+-+--", "measure": 