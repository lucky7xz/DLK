{"textgrid.poem.61132": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1. Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wenn die Primel schneewei\u00df blickt", "tokens": ["Und", "wenn", "die", "Pri\u00b7mel", "schnee\u00b7wei\u00df", "blickt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Bach, am Bach auf dem Wiesengrund,", "tokens": ["Am", "Bach", ",", "am", "Bach", "auf", "dem", "Wie\u00b7sen\u00b7grund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und wenn vom Baum die Kirschbl\u00fct' nickt,", "tokens": ["Und", "wenn", "vom", "Baum", "die", "Kirschbl\u00fct'", "nickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und die V\u00f6gelein pfeifen im Wald allstund:", "tokens": ["Und", "die", "V\u00f6\u00b7ge\u00b7lein", "pfei\u00b7fen", "im", "Wald", "alls\u00b7tund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "ADJD", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Da flickt der Fischer das Netz in Ruh',", "tokens": ["Da", "flickt", "der", "Fi\u00b7scher", "das", "Netz", "in", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Denn der See liegt heiter im Sonnenglanz,", "tokens": ["Denn", "der", "See", "liegt", "hei\u00b7ter", "im", "Son\u00b7nen\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Da sucht das M\u00e4del die roten Schuh'", "tokens": ["Da", "sucht", "das", "M\u00e4\u00b7del", "die", "ro\u00b7ten", "Schuh'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und schn\u00fcrt das Mieder sich eng zum Tanz", "tokens": ["Und", "schn\u00fcrt", "das", "Mie\u00b7der", "sich", "eng", "zum", "Tanz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PRF", "ADJD", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Und denket still,", "tokens": ["Und", "den\u00b7ket", "still", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Ob der Liebste, der Liebste nicht kommen will.", "tokens": ["Ob", "der", "Liebs\u00b7te", ",", "der", "Liebs\u00b7te", "nicht", "kom\u00b7men", "will", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ART", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Es klingt die Fiedel, es brummt der Ba\u00df,", "tokens": ["Es", "klingt", "die", "Fie\u00b7del", ",", "es", "brummt", "der", "Ba\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Dorfschulz sitzt im Schank beim Wein;", "tokens": ["Der", "Dorf\u00b7schulz", "sitzt", "im", "Schank", "beim", "Wein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die T\u00e4nzer drehn sich ohn' Unterla\u00df", "tokens": ["Die", "T\u00e4n\u00b7zer", "drehn", "sich", "ohn'", "Un\u00b7ter\u00b7la\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "CARD", "PRF", "APPR", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "An der Lind', an der Lind', im Abendschein.", "tokens": ["An", "der", "Lind'", ",", "an", "der", "Lind'", ",", "im", "A\u00b7bend\u00b7schein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,", "APPRART", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und geht's nach Haus um Mitternacht,", "tokens": ["Und", "geht's", "nach", "Haus", "um", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gl\u00fchw\u00fcrmchen tr\u00e4gt das Laternchen vor,", "tokens": ["Gl\u00fch\u00b7w\u00fcrm\u00b7chen", "tr\u00e4gt", "das", "La\u00b7tern\u00b7chen", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Da k\u00fcsset der Bube sein Dirnel sacht", "tokens": ["Da", "k\u00fcs\u00b7set", "der", "Bu\u00b7be", "sein", "Dir\u00b7nel", "sacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Und sagt ihr leis ein W\u00f6rtchen ins Ohr,", "tokens": ["Und", "sagt", "ihr", "leis", "ein", "W\u00f6rt\u00b7chen", "ins", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Und sie denken beid':", "tokens": ["Und", "sie", "den\u00b7ken", "beid'", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "O du fr\u00f6hliche, selige Maienzeit!", "tokens": ["O", "du", "fr\u00f6h\u00b7li\u00b7che", ",", "se\u00b7li\u00b7ge", "Mai\u00b7en\u00b7zeit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}