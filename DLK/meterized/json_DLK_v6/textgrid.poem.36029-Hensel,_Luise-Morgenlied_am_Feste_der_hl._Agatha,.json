{"textgrid.poem.36029": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "Morgenlied am Feste der hl. Agatha,", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh', wie der Tag St. Agathas so licht,", "tokens": ["Sieh'", ",", "wie", "der", "Tag", "St.", "A\u00b7gat\u00b7has", "so", "licht", ","], "token_info": ["word", "punct", "word", "word", "word", "abbreviation", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "NE", "NE", "ADV", "ADJD", "$,"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Der hohen Jungfrau Martyrium anbricht.", "tokens": ["Der", "ho\u00b7hen", "Jung\u00b7frau", "Mar\u00b7ty\u00b7ri\u00b7um", "an\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Tag, da Christus sie sich angetraut,", "tokens": ["Der", "Tag", ",", "da", "Chris\u00b7tus", "sie", "sich", "an\u00b7ge\u00b7traut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "NE", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Schm\u00fcckt mit zwei Siegeskronen sch\u00f6n die Braut.", "tokens": ["Schm\u00fcckt", "mit", "zwei", "Sie\u00b7ges\u00b7kro\u00b7nen", "sch\u00f6n", "die", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "CARD", "NN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hoch \u00fcberstrahlet Werk und Glauben ihr", "tokens": ["Hoch", "\u00fc\u00b7bers\u00b7trah\u00b7let", "Werk", "und", "Glau\u00b7ben", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "NN", "KON", "NN", "PPOSAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Ruhm des Stamms, des sch\u00f6nen Leibes Zier;", "tokens": ["Den", "Ruhm", "des", "Stamms", ",", "des", "sch\u00f6\u00b7nen", "Lei\u00b7bes", "Zier", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn irdisch Gut schien ihr ein leerer Tand,", "tokens": ["Denn", "ir\u00b7disch", "Gut", "schien", "ihr", "ein", "lee\u00b7rer", "Tand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die fest das Herz mit Gottes Willen band.", "tokens": ["Die", "fest", "das", "Herz", "mit", "Got\u00b7tes", "Wil\u00b7len", "band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So stellt sie stark der grimmen Henkerschar", "tokens": ["So", "stellt", "sie", "stark", "der", "grim\u00b7men", "Hen\u00b7ker\u00b7schar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zur Gei\u00dfelung die reinen Glieder dar,", "tokens": ["Zur", "Gei\u00b7\u00df\u00b7e\u00b7lung", "die", "rei\u00b7nen", "Glie\u00b7der", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und die zerriss'ne Brust zeugt von dem Mut", "tokens": ["Und", "die", "zer\u00b7riss'\u00b7ne", "Brust", "zeugt", "von", "dem", "Mut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Herzens, das in Jesu Liebe ruht.", "tokens": ["Des", "Her\u00b7zens", ",", "das", "in", "Je\u00b7su", "Lie\u00b7be", "ruht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Im Kerker ihr, wo selig sie verweilt,", "tokens": ["Im", "Ker\u00b7ker", "ihr", ",", "wo", "se\u00b7lig", "sie", "ver\u00b7weilt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Petrus, der Hirt, erscheint, sein L\u00e4mmlein heilt,", "tokens": ["Pet\u00b7rus", ",", "der", "Hirt", ",", "er\u00b7scheint", ",", "sein", "L\u00e4mm\u00b7lein", "heilt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "$,", "ART", "NN", "$,", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.3": {"text": "Bis, freudiger und h\u00f6her noch entbrannt,", "tokens": ["Bis", ",", "freu\u00b7di\u00b7ger", "und", "h\u00f6\u00b7her", "noch", "ent\u00b7brannt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "ADJA", "KON", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit Jauchzen alle Martern sie bestand.", "tokens": ["Mit", "Jauch\u00b7zen", "al\u00b7le", "Mar\u00b7tern", "sie", "be\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Nun strahlend, wie verm\u00e4hlt dem Morgenstern,", "tokens": ["Nun", "strah\u00b7lend", ",", "wie", "ver\u00b7m\u00e4hlt", "dem", "Mor\u00b7gens\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Bitt' f\u00fcr uns Finstre, Elende den Herrn,", "tokens": ["Bitt'", "f\u00fcr", "uns", "Finst\u00b7re", ",", "E\u00b7len\u00b7de", "den", "Herrn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVFIN", "$,", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Da\u00df er uns seine Siege feiern lehre,", "tokens": ["Da\u00df", "er", "uns", "sei\u00b7ne", "Sie\u00b7ge", "fei\u00b7ern", "leh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis sich den Feiernden die Siegslust mehre.", "tokens": ["Bis", "sich", "den", "Fei\u00b7ern\u00b7den", "die", "Siegs\u00b7lust", "meh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}