{"textgrid.poem.66384": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Der Bauer in der Ernte", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Buben, frisch ins Feld hinaus,", "tokens": ["Ihr", "Bu\u00b7ben", ",", "frisch", "ins", "Feld", "hin\u00b7aus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "APPRART", "NN", "APZR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es winken uns die Aehren.", "tokens": ["Es", "win\u00b7ken", "uns", "die", "A\u00b7eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wir wollen auf dem Acker draus", "tokens": ["Wir", "wol\u00b7len", "auf", "dem", "A\u00b7cker", "draus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den lieben Hergott ehren.", "tokens": ["Den", "lie\u00b7ben", "Her\u00b7gott", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "H\u00f6rt, wie der Schwalbe Lied so fein", "tokens": ["H\u00f6rt", ",", "wie", "der", "Schwal\u00b7be", "Lied", "so", "fein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf unsrer Rinne klingt,", "tokens": ["Auf", "uns\u00b7rer", "Rin\u00b7ne", "klingt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und wie dies kleine V\u00f6gelein", "tokens": ["Und", "wie", "dies", "klei\u00b7ne", "V\u00f6\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PDS", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So wunderlieblich singt.", "tokens": ["So", "wun\u00b7der\u00b7lieb\u00b7lich", "singt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da schaut einmal die Halmen an,", "tokens": ["Da", "schaut", "ein\u00b7mal", "die", "Hal\u00b7men", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von tausend Aehren tr\u00e4chtig,", "tokens": ["Von", "tau\u00b7send", "A\u00b7eh\u00b7ren", "tr\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und so viel tausend K\u00f6rnlein dran;", "tokens": ["Und", "so", "viel", "tau\u00b7send", "K\u00f6rn\u00b7lein", "dran", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ist doch Gott so m\u00e4chtig!", "tokens": ["Wie", "ist", "doch", "Gott", "so", "m\u00e4ch\u00b7tig", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So g\u00fclden, wie mein Korn, ist nicht", "tokens": ["So", "g\u00fcl\u00b7den", ",", "wie", "mein", "Korn", ",", "ist", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KOKOM", "PPOSAT", "NN", "$,", "VAFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des kleinen J\u00f6rgleins Haar.", "tokens": ["Des", "klei\u00b7nen", "J\u00f6rg\u00b7leins", "Haar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Jetzt glaub' ich, was der Pfarrer spricht,", "tokens": ["Jetzt", "glaub'", "ich", ",", "was", "der", "Pfar\u00b7rer", "spricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich seh's ja, es ist wahr.", "tokens": ["Ich", "seh's", "ja", ",", "es", "ist", "wahr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ach seht nur doch, an jenem See \u2013", "tokens": ["Ach", "seht", "nur", "doch", ",", "an", "je\u00b7nem", "See", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "ADV", "$,", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da plappern sie, die St\u00f6rche! \u2013", "tokens": ["Da", "plap\u00b7pern", "sie", ",", "die", "St\u00f6r\u00b7che", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und \u00fcber mir in blauer H\u00f6h'", "tokens": ["Und", "\u00fc\u00b7ber", "mir", "in", "blau\u00b7er", "H\u00f6h'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da singt die kleine Lerche.", "tokens": ["Da", "singt", "die", "klei\u00b7ne", "Ler\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie sch\u00fcttelt von den Fl\u00fcgelein", "tokens": ["Sie", "sch\u00fct\u00b7telt", "von", "den", "Fl\u00fc\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Thau auf meinen Hut", "tokens": ["Den", "Thau", "auf", "mei\u00b7nen", "Hut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und singt mir in das Herz hinein", "tokens": ["Und", "singt", "mir", "in", "das", "Herz", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bei meiner Arbeit Muth.", "tokens": ["Bei", "mei\u00b7ner", "Ar\u00b7beit", "Muth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie ist der Morgenwind so k\u00fchl!", "tokens": ["Wie", "ist", "der", "Mor\u00b7gen\u00b7wind", "so", "k\u00fchl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er sch\u00fcttelt von den Aehren", "tokens": ["Er", "sch\u00fct\u00b7telt", "von", "den", "A\u00b7eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die b\u00f6sen W\u00fcrmer, die oft viel", "tokens": ["Die", "b\u00f6\u00b7sen", "W\u00fcr\u00b7mer", ",", "die", "oft", "viel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von unsrem Brod verzehren.", "tokens": ["Von", "uns\u00b7rem", "Brod", "ver\u00b7zeh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Wachtel sitzt in meinem Korn;", "tokens": ["Die", "Wach\u00b7tel", "sitzt", "in", "mei\u00b7nem", "Korn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Pommer stiert sie auf;", "tokens": ["Mein", "Pom\u00b7mer", "stiert", "sie", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er springt ihr nach und schnappt im Zorn", "tokens": ["Er", "springt", "ihr", "nach", "und", "schnappt", "im", "Zorn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Umsonst nach ihr hinauf.", "tokens": ["Um\u00b7sonst", "nach", "ihr", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die W\u00f6lklein ziehen \u00fcber mir,", "tokens": ["Die", "W\u00f6lk\u00b7lein", "zie\u00b7hen", "\u00fc\u00b7ber", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie L\u00e4mmlein, still vor\u00fcber;", "tokens": ["Wie", "L\u00e4mm\u00b7lein", ",", "still", "vor\u00b7\u00fc\u00b7ber", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du guter Gott! wie dank' ich dir,", "tokens": ["Du", "gu\u00b7ter", "Gott", "!", "wie", "dank'", "ich", "dir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "PWAV", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir gehn die Augen \u00fcber.", "tokens": ["Mir", "gehn", "die", "Au\u00b7gen", "\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er, der mein Haus mit Trank und Brod,", "tokens": ["Er", ",", "der", "mein", "Haus", "mit", "Trank", "und", "Brod", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der mir mein Vieh erh\u00e4lt; \u2013", "tokens": ["Der", "mir", "mein", "Vieh", "er\u00b7h\u00e4lt", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So gut, wie unser lieber Gott,", "tokens": ["So", "gut", ",", "wie", "un\u00b7ser", "lie\u00b7ber", "Gott", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist halt nichts auf der Welt.", "tokens": ["Ist", "halt", "nichts", "auf", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "In einem Orte, weit von hier,", "tokens": ["In", "ei\u00b7nem", "Or\u00b7te", ",", "weit", "von", "hier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "APPR", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie wird's da werden theuer!", "tokens": ["Wie", "wird's", "da", "wer\u00b7den", "theu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Hagel fiel, man sagt' es mir,", "tokens": ["Der", "Ha\u00b7gel", "fiel", ",", "man", "sagt'", "es", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PIS", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So gro\u00df, wie H\u00fchnereier.", "tokens": ["So", "gro\u00df", ",", "wie", "H\u00fch\u00b7ne\u00b7rei\u00b7er", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die guten Leutlein dauern mich", "tokens": ["Die", "gu\u00b7ten", "Leut\u00b7lein", "dau\u00b7ern", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In ihrer gro\u00dfen Noth;", "tokens": ["In", "ih\u00b7rer", "gro\u00b7\u00dfen", "Noth", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gott wei\u00df, mit ihnen theilte ich", "tokens": ["Gott", "wei\u00df", ",", "mit", "ih\u00b7nen", "theil\u00b7te", "ich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "APPR", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den letzten Bissen Brod.", "tokens": ["Den", "letz\u00b7ten", "Bis\u00b7sen", "Brod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Hellt stell' ich einen Feirtag an,", "tokens": ["Hellt", "stell'", "ich", "ei\u00b7nen", "Feir\u00b7tag", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den lieben Gott zu preisen;", "tokens": ["Den", "lie\u00b7ben", "Gott", "zu", "prei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dort kommt ja mein Gevattermann,", "tokens": ["Dort", "kommt", "ja", "mein", "Ge\u00b7vat\u00b7ter\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der singt nach allen Weisen.", "tokens": ["Der", "singt", "nach", "al\u00b7len", "Wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gelt, Weibchen, gelt, du singst mit mir?", "tokens": ["Gelt", ",", "Weib\u00b7chen", ",", "gelt", ",", "du", "singst", "mit", "mir", "?"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVPP", "$,", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr Buben, lobet Gott!", "tokens": ["Ihr", "Bu\u00b7ben", ",", "lo\u00b7bet", "Gott", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nun, G'vattermann, so singt uns f\u00fcr:", "tokens": ["Nun", ",", "G'\u00b7vat\u00b7ter\u00b7mann", ",", "so", "singt", "uns", "f\u00fcr", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}}}}