{"textgrid.poem.43459": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schleppt den Fr\u00fchling in den Kerker!", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schleppt den Fr\u00fchling in den Kerker!", "tokens": ["Schleppt", "den", "Fr\u00fch\u00b7ling", "in", "den", "Ker\u00b7ker", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn er ist ein Demagog,", "tokens": ["Denn", "er", "ist", "ein", "De\u00b7ma\u00b7gog", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil er der gewohnten Herrschaft", "tokens": ["Weil", "er", "der", "ge\u00b7wohn\u00b7ten", "Herr\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seines Vaters uns entzog,", "tokens": ["Sei\u00b7nes", "Va\u00b7ters", "uns", "ent\u00b7zog", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Uns um unsre langen N\u00e4chte", "tokens": ["Uns", "um", "uns\u00b7re", "lan\u00b7gen", "N\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und den sch\u00f6nen Schlaf betrog \u2013", "tokens": ["Und", "den", "sch\u00f6\u00b7nen", "Schlaf", "be\u00b7trog", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schleppt den Fr\u00fchling in den Kerker!", "tokens": ["Schleppt", "den", "Fr\u00fch\u00b7ling", "in", "den", "Ker\u00b7ker", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Denn er ist ein Demagog.", "tokens": ["Denn", "er", "ist", "ein", "De\u00b7ma\u00b7gog", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schleppt den Fr\u00fchling in den Kerker!", "tokens": ["Schleppt", "den", "Fr\u00fch\u00b7ling", "in", "den", "Ker\u00b7ker", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der die Welt in Aufruhr bringt:", "tokens": ["Der", "die", "Welt", "in", "Auf\u00b7ruhr", "bringt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "B\u00e4che rauschen, B\u00e4ume fl\u00fcstern,", "tokens": ["B\u00e4\u00b7che", "rau\u00b7schen", ",", "B\u00e4u\u00b7me", "fl\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jeder Vogel zirpt und singt,", "tokens": ["Je\u00b7der", "Vo\u00b7gel", "zirpt", "und", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und auch in die Menschenherzen", "tokens": ["Und", "auch", "in", "die", "Men\u00b7schen\u00b7her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wunderbare Regung dringt \u2013", "tokens": ["Wun\u00b7der\u00b7ba\u00b7re", "Re\u00b7gung", "dringt", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schleppt den Fr\u00fchling in den Kerker!", "tokens": ["Schleppt", "den", "Fr\u00fch\u00b7ling", "in", "den", "Ker\u00b7ker", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der die Welt in Aufruhr bringt.", "tokens": ["Der", "die", "Welt", "in", "Auf\u00b7ruhr", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schleppt den Fr\u00fchling in den Kerker,", "tokens": ["Schleppt", "den", "Fr\u00fch\u00b7ling", "in", "den", "Ker\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Setzt den Winter auf den Thron!", "tokens": ["Setzt", "den", "Win\u00b7ter", "auf", "den", "Thron", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Legitim ist nur der Winter", "tokens": ["Le\u00b7gi\u00b7tim", "ist", "nur", "der", "Win\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ein Demagog sein Sohn,", "tokens": ["Und", "ein", "De\u00b7ma\u00b7gog", "sein", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Dieser aber will nichts weiter,", "tokens": ["Die\u00b7ser", "a\u00b7ber", "will", "nichts", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VMFIN", "PIS", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nichts als Revolution \u2013", "tokens": ["Nichts", "als", "Re\u00b7vo\u00b7lu\u00b7ti\u00b7on", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "\u00bbschleppt den Winter in den Kerker,", "tokens": ["\u00bb", "schleppt", "den", "Win\u00b7ter", "in", "den", "Ker\u00b7ker", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Setzt den Fr\u00fchling auf den Thron!\u00ab", "tokens": ["Setzt", "den", "Fr\u00fch\u00b7ling", "auf", "den", "Thron", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}