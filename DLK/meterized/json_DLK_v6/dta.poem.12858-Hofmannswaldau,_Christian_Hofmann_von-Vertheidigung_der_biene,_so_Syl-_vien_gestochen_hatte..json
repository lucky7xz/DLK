{"dta.poem.12858": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vertheidigung der biene, so Syl-  \n vien gestochen hatte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du hast die arme bien' ohn alles recht erdr\u00fcckt,", "tokens": ["Du", "hast", "die", "ar\u00b7me", "bi\u00b7en'", "ohn", "al\u00b7les", "recht", "er\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "PIS", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ob gleich ihr zarter stich dein s\u00fc\u00dfes fleisch verletzet.", "tokens": ["Ob", "gleich", "ihr", "zar\u00b7ter", "stich", "dein", "s\u00fc\u00b7\u00dfes", "fleisch", "ver\u00b7let\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "ADJD", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn als sie deinen mund, den sch\u00f6nen mund, erblickt,", "tokens": ["Denn", "als", "sie", "dei\u00b7nen", "mund", ",", "den", "sch\u00f6\u00b7nen", "mund", ",", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So hat sie freylich wol den stachel angesetzet;", "tokens": ["So", "hat", "sie", "frey\u00b7lich", "wol", "den", "sta\u00b7chel", "an\u00b7ge\u00b7set\u00b7zet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch finne nur recht nach, warum sie es gethan?", "tokens": ["Doch", "fin\u00b7ne", "nur", "recht", "nach", ",", "wa\u00b7rum", "sie", "es", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "PTKVZ", "$,", "PWAV", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie sahe deinen mund vor frische rosen an.", "tokens": ["Sie", "sa\u00b7he", "dei\u00b7nen", "mund", "vor", "fri\u00b7sche", "ro\u00b7sen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}