{"dta.poem.19099": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "LxXXI.   Auf des W\u00e4ysen-Vaters, Martin  \n Rohleders, Ehe-Verbindung, mit der W\u00e4y-  \n sen-Mutter Judith, geb. Klosin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr kennet ja di\u00df treue Hau\u00df,", "tokens": ["Ihr", "ken\u00b7net", "ja", "di\u00df", "treu\u00b7e", "Hau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als Erste von der Schaar der Br\u00fcder:", "tokens": ["Als", "Ers\u00b7te", "von", "der", "Schaar", "der", "Br\u00fc\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie zogen sich mit Freuden aus,", "tokens": ["Sie", "zo\u00b7gen", "sich", "mit", "Freu\u00b7den", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kleideten die JEsus-Glieder;", "tokens": ["Und", "klei\u00b7de\u00b7ten", "die", "Je\u00b7sus\u00b7Glie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Jhr Ruf zu diesem Dienst und Fron", "tokens": ["Ihr", "Ruf", "zu", "die\u00b7sem", "Dienst", "und", "Fron"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "PDAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist aus der Liebe hergekommen,", "tokens": ["Ist", "aus", "der", "Lie\u00b7be", "her\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Drum haben sie nicht Lohn genommen.", "tokens": ["Drum", "ha\u00b7ben", "sie", "nicht", "Lohn", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PTKNEG", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Jhr Kinder! ziehet doch", "tokens": ["Ihr", "Kin\u00b7der", "!", "zie\u00b7het", "doch"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Mit Lust an ihrem Joch:", "tokens": ["Mit", "Lust", "an", "ih\u00b7rem", "Joch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Es ist das Joch der treuen Liebe.", "tokens": ["Es", "ist", "das", "Joch", "der", "treu\u00b7en", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Sie haben mir ersetzt,", "tokens": ["Sie", "ha\u00b7ben", "mir", "er\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Was ich dahin gesch\u00e4tzt,", "tokens": ["Was", "ich", "da\u00b7hin", "ge\u00b7sch\u00e4tzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PAV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Durch ihren Dienst gehn eure Triebe.", "tokens": ["Durch", "ih\u00b7ren", "Dienst", "gehn", "eu\u00b7re", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich freue mich des festen Bands,", "tokens": ["Ich", "freu\u00b7e", "mich", "des", "fes\u00b7ten", "Bands", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das GOtt mit ihnen wollen binden,", "tokens": ["Das", "Gott", "mit", "ih\u00b7nen", "wol\u00b7len", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und ihres k\u00fcnft\u2019gen Ehestands:", "tokens": ["Und", "ih\u00b7res", "k\u00fcnft'\u00b7gen", "E\u00b7hes\u00b7tands", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich sehe sie schon \u00fcberwinden.", "tokens": ["Ich", "se\u00b7he", "sie", "schon", "\u00fc\u00b7berw\u00b7in\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Erkennet doch, die solche seyn,", "tokens": ["Er\u00b7ken\u00b7net", "doch", ",", "die", "sol\u00b7che", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PRELS", "PIAT", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Jhr Br\u00fcder! freut euch ihrer Gaben,", "tokens": ["Ihr", "Br\u00fc\u00b7der", "!", "freut", "euch", "ih\u00b7rer", "Ga\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und du, o K\u00f6nig der Gemein!", "tokens": ["Und", "du", ",", "o", "K\u00f6\u00b7nig", "der", "Ge\u00b7mein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von dessen Gnade wir sie haben:", "tokens": ["Von", "des\u00b7sen", "Gna\u00b7de", "wir", "sie", "ha\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Nimm ihrer Treue wahr.", "tokens": ["Nimm", "ih\u00b7rer", "Treu\u00b7e", "wahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Du aber, ", "tokens": ["Du", "a\u00b7ber", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "ADV", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Geh, werde durch den Geist versiegelt,", "tokens": ["Geh", ",", "wer\u00b7de", "durch", "den", "Geist", "ver\u00b7sie\u00b7gelt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Versanck ins Abgrunds Nacht.", "tokens": ["Ver\u00b7sanck", "ins", "Ab\u00b7grunds", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Er hatte sich in sich bespiegelt.)", "tokens": ["Er", "hat\u00b7te", "sich", "in", "sich", "be\u00b7spie\u00b7gelt", ".", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PRF", "APPR", "PRF", "VVPP", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}