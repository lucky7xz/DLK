{"textgrid.poem.32942": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Habe Geduld nur!", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Habe Geduld nur!", "tokens": ["Ha\u00b7be", "Ge\u00b7duld", "nur", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "tr\u00f6stet ihr freundlich,", "tokens": ["tr\u00f6s\u00b7tet", "ihr", "freund\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "\u00bbwolken sind Wolken", "tokens": ["\u00bb", "wol\u00b7ken", "sind", "Wol\u00b7ken"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "ADJD", "VAFIN", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "und gehen vor\u00fcber!", "tokens": ["und", "ge\u00b7hen", "vor\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "und nur ein Weilchen", "tokens": ["und", "nur", "ein", "Weil\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "bleibt es so grau!", "tokens": ["bleibt", "es", "so", "grau", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Habe Geduld nur!", "tokens": ["Ha\u00b7be", "Ge\u00b7duld", "nur", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "die Nebel verziehen", "tokens": ["die", "Ne\u00b7bel", "ver\u00b7zie\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "und alles strahlt wieder", "tokens": ["und", "al\u00b7les", "strahlt", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "ADV"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.10": {"text": "im goldenen Blau!\u00ab", "tokens": ["im", "gol\u00b7de\u00b7nen", "Blau", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Ja: Wolken sind Wolken", "tokens": ["Ja", ":", "Wol\u00b7ken", "sind", "Wol\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$.", "NN", "VAFIN", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "und gehen vor\u00fcber! ...", "tokens": ["und", "ge\u00b7hen", "vor\u00b7\u00fc\u00b7ber", "!", "..."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "aber inzwischen", "tokens": ["a\u00b7ber", "in\u00b7zwi\u00b7schen"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "welken die Blumen,", "tokens": ["wel\u00b7ken", "die", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "wandern die V\u00f6gel", "tokens": ["wan\u00b7dern", "die", "V\u00f6\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "und f\u00e4rbt sich das Laub", "tokens": ["und", "f\u00e4rbt", "sich", "das", "Laub"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "und Frohsinn und Jugend", "tokens": ["und", "Froh\u00b7sinn", "und", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "und Glauben um Glauben", "tokens": ["und", "Glau\u00b7ben", "um", "Glau\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "f\u00e4llt m\u00fcde", "tokens": ["f\u00e4llt", "m\u00fc\u00b7de"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADJD"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "dem ewigen", "tokens": ["dem", "e\u00b7wi\u00b7gen"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+--", "measure": "dactylic.init"}, "line.11": {"text": "Warten zu Raub!", "tokens": ["War\u00b7ten", "zu", "Raub", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Und verzogen die Wolken ...", "tokens": ["Und", "ver\u00b7zo\u00b7gen", "die", "Wol\u00b7ken", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Was frommt, sagt, die Sonne", "tokens": ["Was", "frommt", ",", "sagt", ",", "die", "Son\u00b7ne"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "VVFIN", "$,", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "verwelkenden Fluren?", "tokens": ["ver\u00b7wel\u00b7ken\u00b7den", "Flu\u00b7ren", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "was sterbenden W\u00e4ldern", "tokens": ["was", "ster\u00b7ben\u00b7den", "W\u00e4l\u00b7dern"], "token_info": ["word", "word", "word"], "pos": ["PWS", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "der maisch\u00f6nste Glanz?!", "tokens": ["der", "maisc\u00b7h\u00f6ns\u00b7te", "Glanz", "?!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "--+-+", "measure": "anapaest.init"}, "line.5": {"text": "und was einem tr\u00fcbe", "tokens": ["und", "was", "ei\u00b7nem", "tr\u00fc\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWS", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "verwarteten Herzen", "tokens": ["ver\u00b7war\u00b7te\u00b7ten", "Her\u00b7zen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "mitleidiger Liebe", "tokens": ["mit\u00b7lei\u00b7di\u00b7ger", "Lie\u00b7be"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "versp\u00e4teter Kranz?!", "tokens": ["ver\u00b7sp\u00e4\u00b7te\u00b7ter", "Kranz", "?!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}