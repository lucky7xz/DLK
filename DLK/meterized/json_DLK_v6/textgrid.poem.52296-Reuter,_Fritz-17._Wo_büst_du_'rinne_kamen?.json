{"textgrid.poem.52296": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "17. Wo b\u00fcst du 'rinne kamen?", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwo Deuwel! Dreigen mi de Ogen?", "tokens": ["\u00bb", "wo", "Deu\u00b7wel", "!", "Drei\u00b7gen", "mi", "de", "O\u00b7gen", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "NE", "$.", "NE", "NE", "NE", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo, is dat nich uns' Schulten-Jochen?", "tokens": ["Wo", ",", "is", "dat", "nich", "un\u00b7s'", "Schul\u00b7ten\u00b7Jo\u00b7chen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "FM", "ART", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Na, Brauder, ok en beten hir?", "tokens": ["Na", ",", "Brau\u00b7der", ",", "ok", "en", "be\u00b7ten", "hir", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kumm, sett di dal un drink mal Bier.\u00ab", "tokens": ["Kumm", ",", "sett", "di", "dal", "un", "drink", "mal", "Bier", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "FM", "FM", "FM", "FM", "VVFIN", "ADV", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "\u00bbih, lat mi man en beten stahn.\u00ab", "tokens": ["\u00bb", "ih", ",", "lat", "mi", "man", "en", "be\u00b7ten", "stahn", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "$,", "FM", "FM", "FM", "FM", "VVINF", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbne, segg, b\u00fcst du tau ", "tokens": ["\u00bb", "ne", ",", "segg", ",", "b\u00fcst", "du", "tau"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "NE", "$,", "VVFIN", "$,", "VVFIN", "PPER", "NE"], "meter": "--+-+", "measure": "anapaest.init"}, "line.7": {"text": "\u00bbne!\u00ab", "tokens": ["\u00bb", "ne", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "punct"], "pos": ["$(", "NE", "$.", "$("], "meter": "-", "measure": "single.down"}, "line.8": {"text": "\u00bbdenn b\u00fcst du woll herinner ", "tokens": ["\u00bb", "denn", "b\u00fcst", "du", "woll", "he\u00b7rin\u00b7ner"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "VVFIN", "PPER", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "\u00bbne!\u00ab", "tokens": ["\u00bb", "ne", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "punct"], "pos": ["$(", "NE", "$.", "$("], "meter": "-", "measure": "single.down"}, "line.10": {"text": "\u00bbdenn magst du gor rin ", "tokens": ["\u00bb", "denn", "magst", "du", "gor", "rin"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "VMFIN", "PPER", "NE", "NE"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "\u00bbne!\u00ab", "tokens": ["\u00bb", "ne", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "punct"], "pos": ["$(", "NE", "$.", "$("], "meter": "-", "measure": "single.down"}, "line.12": {"text": "\u00bbnich gahn, nich f\u00fchrt un nich rin reden?\u00ab", "tokens": ["\u00bb", "nich", "gahn", ",", "nich", "f\u00fchrt", "un", "nich", "rin", "re\u00b7den", "?", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKNEG", "VVFIN", "$,", "PTKNEG", "VVFIN", "FM", "PTKNEG", "ADV", "VVINF", "$.", "$("], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.13": {"text": "Na, segg, wo k\u00fcmmst du denn herin?\u00ab", "tokens": ["Na", ",", "segg", ",", "wo", "k\u00fcmmst", "du", "denn", "he\u00b7rin", "?", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "VVFIN", "$,", "PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "\u00bbick m\u00fc\u00dft en Ossen rinne ledden.\u00ab", "tokens": ["\u00bb", "ick", "m\u00fc\u00dft", "en", "Os\u00b7sen", "rin\u00b7ne", "led\u00b7den", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}