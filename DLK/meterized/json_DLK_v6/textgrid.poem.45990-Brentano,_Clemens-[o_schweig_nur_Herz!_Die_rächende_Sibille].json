{"textgrid.poem.45990": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[o schweig nur Herz! Die r\u00e4chende Sibille]", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O schweig nur Herz! Die r\u00e4chende Sibille", "tokens": ["O", "schweig", "nur", "Herz", "!", "Die", "r\u00e4\u00b7chen\u00b7de", "Si\u00b7bil\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die \u00fcber deiner Zukunft, Wehe! kreischt,", "tokens": ["Die", "\u00fc\u00b7ber", "dei\u00b7ner", "Zu\u00b7kunft", ",", "We\u00b7he", "!", "kreischt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "$,", "NN", "$.", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den gier'gen Geier, der dich lang zerfleischt,", "tokens": ["Den", "gier'\u00b7gen", "Gei\u00b7er", ",", "der", "dich", "lang", "zer\u00b7fleischt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bannt ein gottselig Kind, und deckt ganz stille", "tokens": ["Bannt", "ein", "gott\u00b7se\u00b7lig", "Kind", ",", "und", "deckt", "ganz", "stil\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJD", "NN", "$,", "KON", "VVFIN", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die schreinde Wunde dir mit Taubenfl\u00fcgeln,", "tokens": ["Die", "schrein\u00b7de", "Wun\u00b7de", "dir", "mit", "Tau\u00b7ben\u00b7fl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Weckt dir den Morgenstern auf stummen H\u00fcgeln.", "tokens": ["Weckt", "dir", "den", "Mor\u00b7gens\u00b7tern", "auf", "stum\u00b7men", "H\u00fc\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+---+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "O schweig nur Herz! Horch Klang von Engelschwingen", "tokens": ["O", "schweig", "nur", "Herz", "!", "Horch", "Klang", "von", "En\u00b7gel\u00b7schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "NN", "$.", "NE", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was zuckst du so, du mu\u00dft fein leise tun,", "tokens": ["Was", "zuckst", "du", "so", ",", "du", "mu\u00dft", "fein", "lei\u00b7se", "tun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "PPER", "VMFIN", "ADJD", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo man dir singet, wie so sanft sie ruhn,", "tokens": ["Wo", "man", "dir", "sin\u00b7get", ",", "wie", "so", "sanft", "sie", "ruhn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "VVFIN", "$,", "PWAV", "ADV", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Seligen, dahin wird man dich bringen,", "tokens": ["Die", "Se\u00b7li\u00b7gen", ",", "da\u00b7hin", "wird", "man", "dich", "brin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "VAFIN", "PIS", "PRF", "VVINF", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Sei still, was schreist du, einsam ist kein Leben,", "tokens": ["Sei", "still", ",", "was", "schreist", "du", ",", "ein\u00b7sam", "ist", "kein", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "PWS", "VVFIN", "PPER", "$,", "ADJD", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Kein Grab, schlaf s\u00fc\u00df, die Liebste tr\u00e4umt daneben.", "tokens": ["Kein", "Grab", ",", "schlaf", "s\u00fc\u00df", ",", "die", "Liebs\u00b7te", "tr\u00e4umt", "da\u00b7ne\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "VVFIN", "ADJD", "$,", "ART", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O schweig nur Herz! Du hast ja nichts besessen,", "tokens": ["O", "schweig", "nur", "Herz", "!", "Du", "hast", "ja", "nichts", "be\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "NN", "$.", "PPER", "VAFIN", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du l\u00e4\u00dft ja nichts zur\u00fcck, wem trauerst du?", "tokens": ["Du", "l\u00e4\u00dft", "ja", "nichts", "zu\u00b7r\u00fcck", ",", "wem", "trau\u00b7erst", "du", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "PTKVZ", "$,", "PWS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auch deines Himmels Augen fallen zu,", "tokens": ["Auch", "dei\u00b7nes", "Him\u00b7mels", "Au\u00b7gen", "fal\u00b7len", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch seiner Liebe Licht strahlt ungemessen", "tokens": ["Doch", "sei\u00b7ner", "Lie\u00b7be", "Licht", "strahlt", "un\u00b7ge\u00b7mes\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Brichst du, bricht jenes Herz? Wer bleibt, wird sagen,", "tokens": ["Brichst", "du", ",", "bricht", "je\u00b7nes", "Herz", "?", "Wer", "bleibt", ",", "wird", "sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PDAT", "NN", "$.", "PWS", "VVFIN", "$,", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "O sch\u00f6nre Lust, halb hier, halb dort zu schlagen!", "tokens": ["O", "sch\u00f6n\u00b7re", "Lust", ",", "halb", "hier", ",", "halb", "dort", "zu", "schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "ADJD", "ADV", "$,", "ADJD", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O schweig mein Herz! Du magst wohl selig schweigen,", "tokens": ["O", "schweig", "mein", "Herz", "!", "Du", "magst", "wohl", "se\u00b7lig", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$.", "PPER", "VMFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was schreist du nur, dir fiel kein S\u00fcnderlos,", "tokens": ["Was", "schreist", "du", "nur", ",", "dir", "fiel", "kein", "S\u00fcn\u00b7der\u00b7los", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dich wiegt die Unschuld ohne Graun im Scho\u00df,", "tokens": ["Dich", "wiegt", "die", "Un\u00b7schuld", "oh\u00b7ne", "Graun", "im", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aus frommen Augen blickt dein Himmelszeichen.", "tokens": ["Aus", "from\u00b7men", "Au\u00b7gen", "blickt", "dein", "Him\u00b7mels\u00b7zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sei ihr nicht schwer, sei selig, tr\u00e4ume, schwebe,", "tokens": ["Sei", "ihr", "nicht", "schwer", ",", "sei", "se\u00b7lig", ",", "tr\u00e4u\u00b7me", ",", "schwe\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "$,", "VAFIN", "ADJD", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wein' um die Traube nicht, wein' mit der Rebe.", "tokens": ["Wein'", "um", "die", "Trau\u00b7be", "nicht", ",", "wein'", "mit", "der", "Re\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKNEG", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+-++--+-", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "O schweig nur Herz! Sonst schimpft dich einen Raben", "tokens": ["O", "schweig", "nur", "Herz", "!", "Sonst", "schimpft", "dich", "ei\u00b7nen", "Ra\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "NN", "$.", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Liebste, die nur Tauben Futter giebt,", "tokens": ["Die", "Liebs\u00b7te", ",", "die", "nur", "Tau\u00b7ben", "Fut\u00b7ter", "giebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O werde rein und fromm, bis sie dich liebt", "tokens": ["O", "wer\u00b7de", "rein", "und", "fromm", ",", "bis", "sie", "dich", "liebt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADJD", "KON", "ADJD", "$,", "KOUS", "PPER", "PRF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Werd' eine Taube, die nur will sie haben.", "tokens": ["Werd'", "ei\u00b7ne", "Tau\u00b7be", ",", "die", "nur", "will", "sie", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "VMFIN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O selig! ihr als Taube zu geh\u00f6ren,", "tokens": ["O", "se\u00b7lig", "!", "ihr", "als", "Tau\u00b7be", "zu", "ge\u00b7h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$.", "PPER", "KOUS", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So lange sie sich wird der Raben wehren.", "tokens": ["So", "lan\u00b7ge", "sie", "sich", "wird", "der", "Ra\u00b7ben", "weh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PRF", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "O schweig nur Herz! Und lerne sel'ger schauen", "tokens": ["O", "schweig", "nur", "Herz", "!", "Und", "ler\u00b7ne", "sel'\u00b7ger", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "NN", "$.", "KON", "VVFIN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als andre in die Huld, die sie umgiebt,", "tokens": ["Als", "and\u00b7re", "in", "die", "Huld", ",", "die", "sie", "um\u00b7giebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df sie dir mehr als allen andern giebt,", "tokens": ["Da\u00df", "sie", "dir", "mehr", "als", "al\u00b7len", "an\u00b7dern", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PIAT", "KOKOM", "PIAT", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das zwinge sie dir einst noch zu vertrauen.", "tokens": ["Das", "zwin\u00b7ge", "sie", "dir", "einst", "noch", "zu", "ver\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Schweig, dulde, glaube, hoffe, liebe, baue", "tokens": ["Schweig", ",", "dul\u00b7de", ",", "glau\u00b7be", ",", "hof\u00b7fe", ",", "lie\u00b7be", ",", "bau\u00b7e"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dein Elend fromm, da\u00df sie dir ganz vertraue!", "tokens": ["Dein", "E\u00b7lend", "fromm", ",", "da\u00df", "sie", "dir", "ganz", "ver\u00b7trau\u00b7e", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$,", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}