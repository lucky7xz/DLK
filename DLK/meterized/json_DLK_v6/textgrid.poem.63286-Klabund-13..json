{"textgrid.poem.63286": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich huste durch die N\u00e4chte hohl und heiss.", "tokens": ["Ich", "hus\u00b7te", "durch", "die", "N\u00e4ch\u00b7te", "hohl", "und", "heiss", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJD", "KON", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Stunde klingt. Es gl\u00e4nzt der Schweiss.", "tokens": ["Die", "Stun\u00b7de", "klingt", ".", "Es", "gl\u00e4nzt", "der", "Schweiss", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich bin durch seltne H\u00e4sslichkeit versch\u00f6nt.", "tokens": ["Ich", "bin", "durch", "selt\u00b7ne", "H\u00e4ss\u00b7lich\u00b7keit", "ver\u00b7sch\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Kabarett entfaltet sich und t\u00f6nt.", "tokens": ["Ein", "Ka\u00b7ba\u00b7rett", "ent\u00b7fal\u00b7tet", "sich", "und", "t\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Im Kahne schaukelt sich mein Kahlkopf kess.", "tokens": ["Im", "Kah\u00b7ne", "schau\u00b7kelt", "sich", "mein", "Kahl\u00b7kopf", "kess", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn mich ein M\u00e4dchen s\u00e4he, weinte es.", "tokens": ["Wenn", "mich", "ein", "M\u00e4d\u00b7chen", "s\u00e4\u00b7he", ",", "wein\u00b7te", "es", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mein Auge brennt. Die Arme fl\u00fcgeln leis.", "tokens": ["Mein", "Au\u00b7ge", "brennt", ".", "Die", "Ar\u00b7me", "fl\u00fc\u00b7geln", "leis", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In meinem Schnabel h\u00e4ngt ein \u00d6lbaumreis.", "tokens": ["In", "mei\u00b7nem", "Schna\u00b7bel", "h\u00e4ngt", "ein", "\u00d6l\u00b7baum\u00b7reis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}