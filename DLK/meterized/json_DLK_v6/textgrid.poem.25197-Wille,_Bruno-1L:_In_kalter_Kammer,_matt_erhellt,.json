{"textgrid.poem.25197": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: In kalter Kammer, matt erhellt,", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In kalter Kammer, matt erhellt,", "tokens": ["In", "kal\u00b7ter", "Kam\u00b7mer", ",", "matt", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf elend knochigem Polster lieg ich,", "tokens": ["Auf", "e\u00b7lend", "kno\u00b7chi\u00b7gem", "Pols\u00b7ter", "lieg", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die F\u00fc\u00dfe frostig, den Magen verg\u00e4llt,", "tokens": ["Die", "F\u00fc\u00b7\u00dfe", "fros\u00b7tig", ",", "den", "Ma\u00b7gen", "ver\u00b7g\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und starre zur m\u00fcrrischen Decke empor ...", "tokens": ["Und", "star\u00b7re", "zur", "m\u00fcr\u00b7ri\u00b7schen", "De\u00b7cke", "em\u00b7por", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Geld!", "tokens": ["Geld", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Rundes blondes Liebchen Gold,", "tokens": ["Run\u00b7des", "blon\u00b7des", "Lieb\u00b7chen", "Gold", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sei dem Schmachtenden hold!", "tokens": ["Sei", "dem", "Schmach\u00b7ten\u00b7den", "hold", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "La\u00df dein Stimmchen mir erklingen", "tokens": ["La\u00df", "dein", "Stimm\u00b7chen", "mir", "er\u00b7klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "S\u00fc\u00df und fein", "tokens": ["S\u00fc\u00df", "und", "fein"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Wie Kristallglas,", "tokens": ["Wie", "Kris\u00b7tall\u00b7glas", ","], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Gef\u00fcllt mit gelbem Wein!", "tokens": ["Ge\u00b7f\u00fcllt", "mit", "gel\u00b7bem", "Wein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Deinen schlangenglatten k\u00fchlen Leib", "tokens": ["Dei\u00b7nen", "schlan\u00b7gen\u00b7glat\u00b7ten", "k\u00fch\u00b7len", "Leib"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "M\u00f6cht ich streicheln;", "tokens": ["M\u00f6cht", "ich", "strei\u00b7cheln", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "Das soll der fieberigen Seele", "tokens": ["Das", "soll", "der", "fie\u00b7be\u00b7ri\u00b7gen", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Wie Quellenk\u00fchlung schmeicheln.", "tokens": ["Wie", "Quel\u00b7len\u00b7k\u00fch\u00b7lung", "schmei\u00b7cheln", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Ginge doch die gr\u00e4mliche Th\u00fcre auf,", "tokens": ["Gin\u00b7ge", "doch", "die", "gr\u00e4m\u00b7li\u00b7che", "Th\u00fc\u00b7re", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Und mein Goldchen w\u00e4re da,", "tokens": ["Und", "mein", "Gold\u00b7chen", "w\u00e4\u00b7re", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Und Goldchen sagte: \u00bbJa,", "tokens": ["Und", "Gold\u00b7chen", "sag\u00b7te", ":", "\u00bb", "Ja", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$.", "$(", "PTKANT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Dein bin ich, dein!", "tokens": ["Dein", "bin", "ich", ",", "dein", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PPER", "$,", "PPOSAT", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.20": {"text": "Und heute soll die Hochzeit sein.\u00ab", "tokens": ["Und", "heu\u00b7te", "soll", "die", "Hoch\u00b7zeit", "sein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VMFIN", "ART", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie wollt ich springen kummerfrei!", "tokens": ["Wie", "wollt", "ich", "sprin\u00b7gen", "kum\u00b7mer\u00b7frei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Freunde, Freunde, hurtig herbei!", "tokens": ["Freun\u00b7de", ",", "Freun\u00b7de", ",", "hur\u00b7tig", "her\u00b7bei", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJD", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wein her, k\u00f6stliche Sch\u00fcsseln her!", "tokens": ["Wein", "her", ",", "k\u00f6st\u00b7li\u00b7che", "Sch\u00fcs\u00b7seln", "her", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "La\u00dft uns schl\u00fcrfen mit Lippen und Augen,", "tokens": ["La\u00dft", "uns", "schl\u00fcr\u00b7fen", "mit", "Lip\u00b7pen", "und", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Wonnevoll, wie durstige Wurzeln", "tokens": ["Won\u00b7ne\u00b7voll", ",", "wie", "durs\u00b7ti\u00b7ge", "Wur\u00b7zeln"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "PWAV", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "\u00dcppige Regenfluten saugen!", "tokens": ["\u00dcp\u00b7pi\u00b7ge", "Re\u00b7gen\u00b7flu\u00b7ten", "sau\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "La\u00dft uns taumeln die Nacht entlang", "tokens": ["La\u00dft", "uns", "tau\u00b7meln", "die", "Nacht", "ent\u00b7lang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVFIN", "ART", "NN", "APPO"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Mit Gesang und Gl\u00e4serklang:", "tokens": ["Mit", "Ge\u00b7sang", "und", "Gl\u00e4\u00b7ser\u00b7klang", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "\u00bbliebchen Gold soll leben!\u00ab", "tokens": ["\u00bb", "lieb\u00b7chen", "Gold", "soll", "le\u00b7ben", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "NN", "VMFIN", "VVINF", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Erwach' ich dann im Bette,", "tokens": ["Er\u00b7wach'", "ich", "dann", "im", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und hellt ein Schimmer", "tokens": ["Und", "hellt", "ein", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Den rauschgetr\u00fcbten Kopf, \u2013", "tokens": ["Den", "rauschge\u00b7tr\u00fcb\u00b7ten", "Kopf", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wo bleibt Frau Sorge,", "tokens": ["Wo", "bleibt", "Frau", "Sor\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Die gestern noch im Junggesellenzimmer", "tokens": ["Die", "ge\u00b7stern", "noch", "im", "Jung\u00b7ge\u00b7sel\u00b7len\u00b7zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "APPRART", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "H\u00fcstelnd schlich?", "tokens": ["H\u00fcs\u00b7telnd", "schlich", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Liebchen Gold", "tokens": ["Lieb\u00b7chen", "Gold"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Ist mir hold!", "tokens": ["Ist", "mir", "hold", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Auf, gl\u00fcckseliges Gem\u00fct!", "tokens": ["Auf", ",", "gl\u00fcck\u00b7se\u00b7li\u00b7ges", "Ge\u00b7m\u00fct", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["APPR", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Neue Freuden sind erbl\u00fcht.", "tokens": ["Neu\u00b7e", "Freu\u00b7den", "sind", "er\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Auf zur Hochzeitsreise! \u2013", "tokens": ["Auf", "zur", "Hoch\u00b7zeits\u00b7rei\u00b7se", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Am Wagenfenster vorbei", "tokens": ["Am", "Wa\u00b7gen\u00b7fens\u00b7ter", "vor\u00b7bei"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Kommen Felder geflogen,", "tokens": ["Kom\u00b7men", "Fel\u00b7der", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "F\u00e4cherhaft ausgespreizt,", "tokens": ["F\u00e4\u00b7cher\u00b7haft", "aus\u00b7ge\u00b7spreizt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Wiesen und Hecken und D\u00f6rfer;", "tokens": ["Wie\u00b7sen", "und", "He\u00b7cken", "und", "D\u00f6r\u00b7fer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Blaud\u00e4mmernde H\u00fcgel wogen;", "tokens": ["Blau\u00b7d\u00e4m\u00b7mern\u00b7de", "H\u00fc\u00b7gel", "wo\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Wald und Flu\u00df rauscht vorbei.", "tokens": ["Wald", "und", "Flu\u00df", "rauscht", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Mit uns eilt die stralende Sonne", "tokens": ["Mit", "uns", "eilt", "die", "stra\u00b7len\u00b7de", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Und des Himmels blauende Wonne.", "tokens": ["Und", "des", "Him\u00b7mels", "blau\u00b7en\u00b7de", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da ... in Bergesschacht", "tokens": ["Da", "...", "in", "Ber\u00b7ges\u00b7schacht"], "token_info": ["word", "punct", "word", "word"], "pos": ["KOUS", "$(", "APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Rollen wir dumpf donnernd;", "tokens": ["Rol\u00b7len", "wir", "dumpf", "don\u00b7nernd", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Der Tag erlischt; lang herrscht die Nacht;", "tokens": ["Der", "Tag", "er\u00b7lischt", ";", "lang", "herrscht", "die", "Nacht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$.", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Haupt und Augenlider", "tokens": ["Und", "Haupt", "und", "Au\u00b7gen\u00b7li\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sinken schl\u00e4frig nieder ...", "tokens": ["Sin\u00b7ken", "schl\u00e4f\u00b7rig", "nie\u00b7der", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Neu zum Licht erwacht,", "tokens": ["Neu", "zum", "Licht", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Schau' ich staunend ein Alpenthal,", "tokens": ["Schau'", "ich", "stau\u00b7nend", "ein", "Al\u00b7pen\u00b7thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Felsen und Tannen;", "tokens": ["Fel\u00b7sen", "und", "Tan\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Droben gl\u00fcht ein Schneeberg-Greis", "tokens": ["Dro\u00b7ben", "gl\u00fcht", "ein", "Schnee\u00b7ber\u00b7gGreis"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Im Abendstral", "tokens": ["Im", "A\u00b7bend\u00b7stral"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Trunken vor Lust;", "tokens": ["Trun\u00b7ken", "vor", "Lust", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Sch\u00e4umend st\u00fcrzt der Gie\u00dfbach", "tokens": ["Sch\u00e4u\u00b7mend", "st\u00fcrzt", "der", "Gie\u00df\u00b7bach"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Von seiner Felsenbrust.", "tokens": ["Von", "sei\u00b7ner", "Fel\u00b7sen\u00b7brust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Hier will ich atmen, trinken", "tokens": ["Hier", "will", "ich", "at\u00b7men", ",", "trin\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Rauhe Lust,", "tokens": ["Rau\u00b7he", "Lust", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+", "measure": 