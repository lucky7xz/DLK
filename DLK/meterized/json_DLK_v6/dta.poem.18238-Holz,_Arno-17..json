{"dta.poem.18238": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Der Wein mir im zinnernen Krug,", "tokens": ["Der", "Wein", "mir", "im", "zin\u00b7ner\u00b7nen", "Krug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Und fern in des Kellers Gedunkel", "tokens": ["Und", "fern", "in", "des", "Kel\u00b7lers", "Ge\u00b7dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Da stehn noch der F\u00e4sser genug.", "tokens": ["Da", "stehn", "noch", "der", "F\u00e4s\u00b7ser", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Doch will es mir heute nicht gl\u00fccken", "tokens": ["Doch", "will", "es", "mir", "heu\u00b7te", "nicht", "gl\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PPER", "ADV", "PTKNEG", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "So fr\u00f6hlich wie gestern zu sein,", "tokens": ["So", "fr\u00f6h\u00b7lich", "wie", "ge\u00b7stern", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADV", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und die zitternden H\u00e4nde dr\u00fccken", "tokens": ["Und", "die", "zit\u00b7tern\u00b7den", "H\u00e4n\u00b7de", "dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sich tief in die Schl\u00e4fen hinein.", "tokens": ["Sich", "tief", "in", "die", "Schl\u00e4\u00b7fen", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Des Rathsw\u00e4chters Pfeifen und Rufen", "tokens": ["Des", "Raths\u00b7w\u00e4ch\u00b7ters", "Pfei\u00b7fen", "und", "Ru\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Zeigt drau\u00dfen die Mitternacht an,", "tokens": ["Zeigt", "drau\u00b7\u00dfen", "die", "Mit\u00b7ter\u00b7nacht", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und l\u00e4ngst stieg die steinernen Stufen", "tokens": ["Und", "l\u00e4ngst", "stieg", "die", "stei\u00b7ner\u00b7nen", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der letzte der G\u00e4ste hinan.", "tokens": ["Der", "letz\u00b7te", "der", "G\u00e4s\u00b7te", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Daheim am flackernden Herde", "tokens": ["Da\u00b7heim", "am", "fla\u00b7ckern\u00b7den", "Her\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Genie\u00dft er nun traulich sein Gl\u00fcck,", "tokens": ["Ge\u00b7nie\u00dft", "er", "nun", "trau\u00b7lich", "sein", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und ich blieb hier unter der Erde", "tokens": ["Und", "ich", "blieb", "hier", "un\u00b7ter", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ach, nur mit mir selber zur\u00fcck!", "tokens": ["Ach", ",", "nur", "mit", "mir", "sel\u00b7ber", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "APPR", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Und wie es so einsam geworden", "tokens": ["Und", "wie", "es", "so", "ein\u00b7sam", "ge\u00b7wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "ADJD", "VAPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und rings um mich still wie im Grab,", "tokens": ["Und", "rings", "um", "mich", "still", "wie", "im", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "ADJD", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Da klingt es in weichen Akkorden", "tokens": ["Da", "klingt", "es", "in", "wei\u00b7chen", "Ak\u00b7kor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Bis tief in mein Gr\u00fcbeln herab.", "tokens": ["Bis", "tief", "in", "mein", "Gr\u00fc\u00b7beln", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Erst stiehlt es sich lieb und verlockend", "tokens": ["Erst", "stiehlt", "es", "sich", "lieb", "und", "ver\u00b7lo\u00b7ckend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "KON", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Hinein in das lauschende Ohr,", "tokens": ["Hin\u00b7ein", "in", "das", "lau\u00b7schen\u00b7de", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und dann schwillt es froh und frohlockend", "tokens": ["Und", "dann", "schwillt", "es", "froh", "und", "froh\u00b7lo\u00b7ckend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Zum jubelnden Hochzeitschor.", "tokens": ["Zum", "ju\u00b7beln\u00b7den", "Hoch\u00b7zeit\u00b7schor", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Und die schmeichelnden Weisen erz\u00e4hlen", "tokens": ["Und", "die", "schmei\u00b7cheln\u00b7den", "Wei\u00b7sen", "er\u00b7z\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Der Luft und dem flackernden Licht,", "tokens": ["Der", "Luft", "und", "dem", "fla\u00b7ckern\u00b7den", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wie droben in schimmernden S\u00e4len", "tokens": ["Wie", "dro\u00b7ben", "in", "schim\u00b7mern\u00b7den", "S\u00e4\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mein Gl\u00fcck in Scherben zerbricht.", "tokens": ["Mein", "Gl\u00fcck", "in", "Scher\u00b7ben", "zer\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Ich aber sitze und sinne,", "tokens": ["Ich", "a\u00b7ber", "sit\u00b7ze", "und", "sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Verloren in Gram und in Schmerz,", "tokens": ["Ver\u00b7lo\u00b7ren", "in", "Gram", "und", "in", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "KON", "APPR", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und das Lied von der sterbenden Minne", "tokens": ["Und", "das", "Lied", "von", "der", "ster\u00b7ben\u00b7den", "Min\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Durchzuckt mir das blutende Herz.", "tokens": ["Durch\u00b7zuckt", "mir", "das", "blu\u00b7ten\u00b7de", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.9": {"line.1": {"text": "Verworrene, wilde Gedanken", "tokens": ["Ver\u00b7wor\u00b7re\u00b7ne", ",", "wil\u00b7de", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Entsteigen dem fiebernden Hirn", "tokens": ["Ent\u00b7stei\u00b7gen", "dem", "fie\u00b7bern\u00b7den", "Hirn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und klammern wie dornige Ranken", "tokens": ["Und", "klam\u00b7mern", "wie", "dor\u00b7ni\u00b7ge", "Ran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sich fest um die faltige Stirn:", "tokens": ["Sich", "fest", "um", "die", "fal\u00b7ti\u00b7ge", "Stirn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.10": {"line.1": {"text": "Nun wiegt sie wohl droben im Tanze,", "tokens": ["Nun", "wiegt", "sie", "wohl", "dro\u00b7ben", "im", "Tan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Von luftigen Schleiern umwallt,", "tokens": ["Von", "luf\u00b7ti\u00b7gen", "Schlei\u00b7ern", "um\u00b7wallt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Geschm\u00fcckt mit dem br\u00e4utlichen Kranze", "tokens": ["Ge\u00b7schm\u00fcckt", "mit", "dem", "br\u00e4ut\u00b7li\u00b7chen", "Kran\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die liebliche schlanke Gestalt.", "tokens": ["Die", "lieb\u00b7li\u00b7che", "schlan\u00b7ke", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.11": {"line.1": {"text": "Doch ein Andrer f\u00fchlt jetzt erwarmen", "tokens": ["Doch", "ein", "A\u00b7ndrer", "f\u00fchlt", "jetzt", "er\u00b7war\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "VVINF"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das Herz, das einst klopfte f\u00fcr mich,", "tokens": ["Das", "Herz", ",", "das", "einst", "klopf\u00b7te", "f\u00fcr", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und ein Andrer darf sie umarmen", "tokens": ["Und", "ein", "A\u00b7ndrer", "darf", "sie", "um\u00b7ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und ein Andrer sie k\u00fcssen als ich!", "tokens": ["Und", "ein", "A\u00b7ndrer", "sie", "k\u00fcs\u00b7sen", "als", "ich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "VVFIN", "KOUS", "PPER", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.12": {"line.1": {"text": "Und lauter kreischen die Geigen", "tokens": ["Und", "lau\u00b7ter", "krei\u00b7schen", "die", "Gei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und wilder b\u00e4umt sich mein Leid,", "tokens": ["Und", "wil\u00b7der", "b\u00e4umt", "sich", "mein", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und toller verschlingt sich der Reigen", "tokens": ["Und", "tol\u00b7ler", "ver\u00b7schlingt", "sich", "der", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "VVFIN", "PRF", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Von Traum und Wirklichkeit.", "tokens": ["Von", "Traum", "und", "Wirk\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Es knistern die seidenen Schleppen,", "tokens": ["Es", "knis\u00b7tern", "die", "sei\u00b7de\u00b7nen", "Schlep\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Es funkelt der goldne Pokal,", "tokens": ["Es", "fun\u00b7kelt", "der", "gold\u00b7ne", "Po\u00b7kal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und mir ist es, als stieg ich die Treppen", "tokens": ["Und", "mir", "ist", "es", ",", "als", "stieg", "ich", "die", "Trep\u00b7pen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPER", "$,", "KOUS", "VVFIN", "PPER", "ART", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Hinauf in den Marmorsaal.", "tokens": ["Hin\u00b7auf", "in", "den", "Mar\u00b7mor\u00b7saal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.14": {"line.1": {"text": "Dort ruht unter Myrthen und Rosen", "tokens": ["Dort", "ruht", "un\u00b7ter", "Myr\u00b7then", "und", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ein Brautpaar auf schwellendem Thron,", "tokens": ["Ein", "Braut\u00b7paar", "auf", "schwel\u00b7len\u00b7dem", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Doch sein heimliches K\u00fcssen und Kosen", "tokens": ["Doch", "sein", "heim\u00b7li\u00b7ches", "K\u00fcs\u00b7sen", "und", "Ko\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "KON", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Mich trifft es wie schneidender Hohn.", "tokens": ["Mich", "trifft", "es", "wie", "schnei\u00b7den\u00b7der", "Hohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.15": {"line.1": {"text": "Gedenk der gebrochenen Eide,", "tokens": ["Ge\u00b7denk", "der", "ge\u00b7bro\u00b7che\u00b7nen", "Ei\