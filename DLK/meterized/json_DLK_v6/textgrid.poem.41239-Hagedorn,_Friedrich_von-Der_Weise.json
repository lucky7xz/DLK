{"textgrid.poem.41239": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Weise", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Midas trotzt auf den Besitz der Sch\u00e4tze,", "tokens": ["Ein", "Mi\u00b7das", "trotzt", "auf", "den", "Be\u00b7sitz", "der", "Sch\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um die der Geiz nach fernen Ufern reist.", "tokens": ["Um", "die", "der", "Geiz", "nach", "fer\u00b7nen", "U\u00b7fern", "reist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Pr\u00fcft auch der Thor der Wahrheit ew'ge S\u00e4tze,", "tokens": ["Pr\u00fcft", "auch", "der", "Thor", "der", "Wahr\u00b7heit", "ew'\u00b7ge", "S\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Weisen Gl\u00fcck, den \u00e4chten Heldengeist,", "tokens": ["Des", "Wei\u00b7sen", "Gl\u00fcck", ",", "den", "\u00e4ch\u00b7ten", "Hel\u00b7den\u00b7geist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Den Schatz, an dem kein Diebesfinger klebet,", "tokens": ["Den", "Schatz", ",", "an", "dem", "kein", "Die\u00b7bes\u00b7fin\u00b7ger", "kle\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nach dem allein der Reichen Neid nicht strebet?", "tokens": ["Nach", "dem", "al\u00b7lein", "der", "Rei\u00b7chen", "Neid", "nicht", "stre\u00b7bet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ART", "NN", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Weiser lebt, obgleich nicht krumme Grifse", "tokens": ["Ein", "Wei\u00b7ser", "lebt", ",", "ob\u00b7gleich", "nicht", "krum\u00b7me", "Grif\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PTKNEG", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihm Geld und Trost in Schr\u00e4nk' und Kasten ziehn;", "tokens": ["Ihm", "Geld", "und", "Trost", "in", "Schr\u00e4nk'", "und", "Kas\u00b7ten", "ziehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Beschweret gleich sein wuchernd Gut nicht Schiffe,", "tokens": ["Be\u00b7schwe\u00b7ret", "gleich", "sein", "wu\u00b7chernd", "Gut", "nicht", "Schif\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJD", "ADJD", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die zum Gewinn mit schnellen Segeln fliehn.", "tokens": ["Die", "zum", "Ge\u00b7winn", "mit", "schnel\u00b7len", "Se\u00b7geln", "fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Er darf sich gro\u00df, er darf sich gl\u00fccklich preisen;", "tokens": ["Er", "darf", "sich", "gro\u00df", ",", "er", "darf", "sich", "gl\u00fcck\u00b7lich", "prei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "$,", "PPER", "VMFIN", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Kein fremder Fluch versalzet seine Speisen.", "tokens": ["Kein", "frem\u00b7der", "Fluch", "ver\u00b7sal\u00b7zet", "sei\u00b7ne", "Spei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Er schl\u00e4ft mit Lust, wo Andrer Sorgen wachen;", "tokens": ["Er", "schl\u00e4ft", "mit", "Lust", ",", "wo", "A\u00b7ndrer", "Sor\u00b7gen", "wa\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "PWAV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wann Boreas um Dach und Fenster heult,", "tokens": ["Wann", "Bo\u00b7re\u00b7as", "um", "Dach", "und", "Fens\u00b7ter", "heult", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und dann vielleicht der Wellen schwarzer Rachen", "tokens": ["Und", "dann", "viel\u00b7leicht", "der", "Wel\u00b7len", "schwar\u00b7zer", "Ra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Frachten droht, und Mast und Kiel ereilt;", "tokens": ["Den", "Frach\u00b7ten", "droht", ",", "und", "Mast", "und", "Kiel", "er\u00b7eilt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "NN", "KON", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So oft der Herr der Wasser und der Erden", "tokens": ["So", "oft", "der", "Herr", "der", "Was\u00b7ser", "und", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Kr\u00e4mer beugt, da\u00df sie nicht F\u00fcrsten werden.", "tokens": ["Die", "Kr\u00e4\u00b7mer", "beugt", ",", "da\u00df", "sie", "nicht", "F\u00fcrs\u00b7ten", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was Recht und Flei\u00df und Zeit und Gl\u00fcck ihm geben,", "tokens": ["Was", "Recht", "und", "Flei\u00df", "und", "Zeit", "und", "Gl\u00fcck", "ihm", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verwaltet er mit milder Dankbarkeit,", "tokens": ["Ver\u00b7wal\u00b7tet", "er", "mit", "mil\u00b7der", "Dank\u00b7bar\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und meidet den, der den Genu\u00df vom Leben,", "tokens": ["Und", "mei\u00b7det", "den", ",", "der", "den", "Ge\u00b7nu\u00df", "vom", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "$,", "PRELS", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Der jeden Tag nur dem Gewerbe weiht,", "tokens": ["Der", "je\u00b7den", "Tag", "nur", "dem", "Ge\u00b7wer\u00b7be", "weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und j\u00fcdisch lacht, so oft er sieht und h\u00f6ret,", "tokens": ["Und", "j\u00fc\u00b7disch", "lacht", ",", "so", "oft", "er", "sieht", "und", "h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,", "ADV", "ADV", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie die Vernunft Geschmack und Wahrheit ehret.", "tokens": ["Wie", "die", "Ver\u00b7nunft", "Ge\u00b7schmack", "und", "Wahr\u00b7heit", "eh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wie edel ist die Neigung \u00e4chter Britten:", "tokens": ["Wie", "e\u00b7del", "ist", "die", "Nei\u00b7gung", "\u00e4ch\u00b7ter", "Brit\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Ueberflu\u00df bereichert den Verstand,", "tokens": ["Ihr", "Ue\u00b7berf\u00b7lu\u00df", "be\u00b7rei\u00b7chert", "den", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Handlung Frucht, und was ihr Muth erstritten,", "tokens": ["Der", "Hand\u00b7lung", "Frucht", ",", "und", "was", "ihr", "Muth", "er\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "KON", "PWS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wird, unbereut, Verdiensten zugewandt;", "tokens": ["Wird", ",", "un\u00b7be\u00b7reut", ",", "Ver\u00b7diens\u00b7ten", "zu\u00b7ge\u00b7wandt", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ADJD", "$,", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gunst kr\u00f6nt den Flei\u00df, den Macht und Freiheit sch\u00fctzen:", "tokens": ["Gunst", "kr\u00f6nt", "den", "Flei\u00df", ",", "den", "Macht", "und", "Frei\u00b7heit", "sch\u00fct\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Reichsten sind der Wissenschaften St\u00fctzen.", "tokens": ["Die", "Reichs\u00b7ten", "sind", "der", "Wis\u00b7sen\u00b7schaf\u00b7ten", "St\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "O Freiheit! dort, nur dort ist deine Wonne,", "tokens": ["O", "Frei\u00b7heit", "!", "dort", ",", "nur", "dort", "ist", "dei\u00b7ne", "Won\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ADV", "$,", "ADV", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der St\u00e4dte Schmuck, der Segen jeder Flur,", "tokens": ["Der", "St\u00e4d\u00b7te", "Schmuck", ",", "der", "Se\u00b7gen", "je\u00b7der", "Flur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Stark wie das Meer, erquickend wie die Sonne,", "tokens": ["Stark", "wie", "das", "Meer", ",", "er\u00b7qui\u00b7ckend", "wie", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$,", "VVPP", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sch\u00f6n wie das Licht, und reich wie die Natur.", "tokens": ["Sch\u00f6n", "wie", "das", "Licht", ",", "und", "reich", "wie", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$,", "KON", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Halbgl\u00fccklich sind die Sklaven, die dich nennen;", "tokens": ["Halb\u00b7gl\u00fcck\u00b7lich", "sind", "die", "Skla\u00b7ven", ",", "die", "dich", "nen\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Doch weiter nicht, als nach dem Namen, kennen!", "tokens": ["Doch", "wei\u00b7ter", "nicht", ",", "als", "nach", "dem", "Na\u00b7men", ",", "ken\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "$,", "KOUS", "APPR", "ART", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wer hei\u00dft oft gro\u00df? Der schnell nach Ehren klettert,", "tokens": ["Wer", "hei\u00dft", "oft", "gro\u00df", "?", "Der", "schnell", "nach", "Eh\u00b7ren", "klet\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADJD", "$.", "ART", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den K\u00fchnheit hebt, die H\u00f6he schwindlicht macht.", "tokens": ["Den", "K\u00fchn\u00b7heit", "hebt", ",", "die", "H\u00f6\u00b7he", "schwind\u00b7licht", "macht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch wer ist gro\u00df? Der F\u00fcrsten nicht verg\u00f6ttert,", "tokens": ["Doch", "wer", "ist", "gro\u00df", "?", "Der", "F\u00fcrs\u00b7ten", "nicht", "ver\u00b7g\u00f6t\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "ADJD", "$.", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und edler denkt, als mancher F\u00fcrst gedacht,", "tokens": ["Und", "ed\u00b7ler", "denkt", ",", "als", "man\u00b7cher", "F\u00fcrst", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VVFIN", "$,", "KOUS", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Wahrheit sucht, dich, treue Wahrheit, findet,", "tokens": ["Der", "Wahr\u00b7heit", "sucht", ",", "dich", ",", "treu\u00b7e", "Wahr\u00b7heit", ",", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "$,", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und seinen Werth auf Witz und Tugend gr\u00fcndet.", "tokens": ["Und", "sei\u00b7nen", "Werth", "auf", "Witz", "und", "Tu\u00b7gend", "gr\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Ein solcher kennt die Eitelkeit der W\u00fcrden,", "tokens": ["Ein", "sol\u00b7cher", "kennt", "die", "Ei\u00b7tel\u00b7keit", "der", "W\u00fcr\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In die das Gl\u00fcck zu selten Kluge steckt.", "tokens": ["In", "die", "das", "Gl\u00fcck", "zu", "sel\u00b7ten", "Klu\u00b7ge", "steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihn r\u00fchret nicht der Aufputz hoher B\u00fcrden;", "tokens": ["Ihn", "r\u00fch\u00b7ret", "nicht", "der", "Auf\u0