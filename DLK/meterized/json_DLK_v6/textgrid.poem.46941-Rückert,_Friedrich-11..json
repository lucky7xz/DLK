{"textgrid.poem.46941": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht mehr das Gold und Silber will ich preisen:", "tokens": ["Nicht", "mehr", "das", "Gold", "und", "Sil\u00b7ber", "will", "ich", "prei\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "KON", "NN", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Gold und Silber sank herab zum Tande,", "tokens": ["Das", "Gold", "und", "Sil\u00b7ber", "sank", "her\u00b7ab", "zum", "Tan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weil w\u00fcrdiglich vom ernsten Vaterlande", "tokens": ["Weil", "w\u00fcr\u00b7dig\u00b7lich", "vom", "erns\u00b7ten", "Va\u00b7ter\u00b7lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Statt Golds und Silbers ward erh\u00f6ht das Eisen.", "tokens": ["Statt", "Golds", "und", "Sil\u00b7bers", "ward", "er\u00b7h\u00f6ht", "das", "Ei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "VAFIN", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wer Kraft im Arm hat, geh, sie zu beweisen,", "tokens": ["Wer", "Kraft", "im", "Arm", "hat", ",", "geh", ",", "sie", "zu", "be\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "APPRART", "NN", "VAFIN", "$,", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein Eisenschwert zu schwingen ohne Schande,", "tokens": ["Ein", "Ei\u00b7sen\u00b7schwert", "zu", "schwin\u00b7gen", "oh\u00b7ne", "Schan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Es heimzutragen mit zerhau'nem Rande,", "tokens": ["Es", "heim\u00b7zu\u00b7tra\u00b7gen", "mit", "zer\u00b7hau'\u00b7nem", "Ran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und daf\u00fcr zu empfahn ein Kreuz von Eisen.", "tokens": ["Und", "da\u00b7f\u00fcr", "zu", "em\u00b7pfahn", "ein", "Kreuz", "von", "Ei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "PTKZU", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ihr goldnen, silbren, Ordenszeichen alle,", "tokens": ["Ihr", "gold\u00b7nen", ",", "silb\u00b7ren", ",", "Or\u00b7dens\u00b7zei\u00b7chen", "al\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "VVFIN", "$,", "NN", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Brecht vor dem st\u00e4rkeren Metall in Splitter,", "tokens": ["Brecht", "vor", "dem", "st\u00e4r\u00b7ke\u00b7ren", "Me\u00b7tall", "in", "Split\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Fallt, denn ihr rettetet uns nicht vom Falle;", "tokens": ["Fallt", ",", "denn", "ihr", "ret\u00b7te\u00b7tet", "uns", "nicht", "vom", "Fal\u00b7le", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "PPER", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.12": {"text": "Nur ihr, zuk\u00fcnft'ge neue Eisenritter,", "tokens": ["Nur", "ihr", ",", "zu\u00b7k\u00fcnft'\u00b7ge", "neu\u00b7e", "Ei\u00b7sen\u00b7rit\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Macht euch hinfort zu einem Eisenwalle", "tokens": ["Macht", "euch", "hin\u00b7fort", "zu", "ei\u00b7nem", "Ei\u00b7sen\u00b7wal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Dem Vaterland, das Kern jetzt sucht statt Flitter.", "tokens": ["Dem", "Va\u00b7ter\u00b7land", ",", "das", "Kern", "jetzt", "sucht", "statt", "Flit\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}