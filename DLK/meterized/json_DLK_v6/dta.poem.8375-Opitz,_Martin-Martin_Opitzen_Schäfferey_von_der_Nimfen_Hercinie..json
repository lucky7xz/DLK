{"dta.poem.8375": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Martin Opitzen Sch\u00e4fferey  \n von der Nimfen Hercinie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1630", "urn": "urn:nbn:de:kobv:b4-200905197863", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Sch\u00e4fferey Von der Nimfen Hercinie. Breslau, 1630."}, "poem": {"stanza.1": {"line.1": {"text": "Schaw hier den edlen schildt als ie der tag beschienen:", "tokens": ["Schaw", "hier", "den", "ed\u00b7len", "schildt", "als", "ie", "der", "tag", "be\u00b7schie\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "VVFIN", "KOKOM", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was zeigt der frische bawm? die tugendt mu\u00df siets gr\u00fcnen:", "tokens": ["Was", "zeigt", "der", "fri\u00b7sche", "bawm", "?", "die", "tu\u00b7gendt", "mu\u00df", "siets", "gr\u00fc\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$.", "ART", "NN", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Vndt was das schaff? ein mensch soll guer vndt guetig sein;", "tokens": ["Vndt", "was", "das", "schaff", "?", "ein", "mensch", "soll", "gu\u00b7er", "vndt", "gue\u00b7tig", "sein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PDS", "VVFIN", "$.", "ART", "NN", "VMFIN", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Das blut? wo guet nicht hilfft/ schlag mitt der faust darein.", "tokens": ["Das", "blut", "?", "wo", "guet", "nicht", "hilf