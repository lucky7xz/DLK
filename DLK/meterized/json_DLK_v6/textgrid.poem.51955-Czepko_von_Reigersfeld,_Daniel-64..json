{"textgrid.poem.51955": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "64.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott f\u00fcllt den Geist, und er die Seel: und sie die Sinnen.", "tokens": ["Gott", "f\u00fcllt", "den", "Geist", ",", "und", "er", "die", "Seel", ":", "und", "sie", "die", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "ART", "NN", "$.", "KON", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Empfindst du das, du kanst der Engel Werck beginnen.", "tokens": ["Emp\u00b7findst", "du", "das", ",", "du", "kanst", "der", "En\u00b7gel", "Werck", "be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "$,", "PPER", "VMFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}