{"dta.poem.7964": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Blumenstrau\u00df .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn Str\u00e4uchen, Blumen manche Deutung eigen,", "tokens": ["Wenn", "Str\u00e4u\u00b7chen", ",", "Blu\u00b7men", "man\u00b7che", "Deu\u00b7tung", "ei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn in den Rosen Liebe sich entz\u00fcndet,", "tokens": ["Wenn", "in", "den", "Ro\u00b7sen", "Lie\u00b7be", "sich", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vergi\u00dfmeinnicht im Namen schon sich k\u00fcndet,", "tokens": ["Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", "im", "Na\u00b7men", "schon", "sich", "k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Lorbeere Ruhm, Cypressen Trauer zeigen;", "tokens": ["Lor\u00b7bee\u00b7re", "Ruhm", ",", "Cyp\u00b7res\u00b7sen", "Trau\u00b7er", "zei\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "VVINF", "$."], "meter": "+--+---+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Wenn, wo die andern Zeichen alle schweigen,", "tokens": ["Wenn", ",", "wo", "die", "an\u00b7dern", "Zei\u00b7chen", "al\u00b7le", "schwei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "ART", "ADJA", "NN", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Man doch in Farben zarten Sinn ergr\u00fcndet,", "tokens": ["Man", "doch", "in", "Far\u00b7ben", "zar\u00b7ten", "Sinn", "er\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "APPR", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn Stolz und Neid dem Gelben sich verb\u00fcndet,", "tokens": ["Wenn", "Stolz", "und", "Neid", "dem", "Gel\u00b7ben", "sich", "ver\u00b7b\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn Hoffnung flattert in den gr\u00fcnen Zweigen:", "tokens": ["Wenn", "Hoff\u00b7nung", "flat\u00b7tert", "in", "den", "gr\u00fc\u00b7nen", "Zwei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So brach ich wohl mit Grund in meinem Garten", "tokens": ["So", "brach", "ich", "wohl", "mit", "Grund", "in", "mei\u00b7nem", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Blumen aller Farben, aller Arten,", "tokens": ["Die", "Blu\u00b7men", "al\u00b7ler", "Far\u00b7ben", ",", "al\u00b7ler", "Ar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und bring\u2019 sie dir, zu wildem Strau\u00df gereihet:", "tokens": ["Und", "bring'", "sie", "dir", ",", "zu", "wil\u00b7dem", "Strau\u00df", "ge\u00b7rei\u00b7het", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "$,", "APPR", "ADJA", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dir ist ja meine Lust, mein Hoffen, Leiden,", "tokens": ["Dir", "ist", "ja", "mei\u00b7ne", "Lust", ",", "mein", "Hof\u00b7fen", ",", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Lieben, meine Treu, mein Ruhm, mein Neiden,", "tokens": ["Mein", "Lie\u00b7ben", ",", "mei\u00b7ne", "Treu", ",", "mein", "Ruhm", ",", "mein", "Nei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dir ist mein Leben, dir mein Tod geweihet.", "tokens": ["Dir", "ist", "mein", "Le\u00b7ben", ",", "dir", "mein", "Tod", "ge\u00b7wei\u00b7het", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}}}}