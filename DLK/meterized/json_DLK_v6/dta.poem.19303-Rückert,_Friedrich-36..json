{"dta.poem.19303": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "36.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was unterscheidet dich, o Mensch, von der Natur?", "tokens": ["Was", "un\u00b7ter\u00b7schei\u00b7det", "dich", ",", "o", "Mensch", ",", "von", "der", "Na\u00b7tur", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "FM", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du bist ein Werdender, sie ist geworden nur.", "tokens": ["Du", "bist", "ein", "Wer\u00b7den\u00b7der", ",", "sie", "ist", "ge\u00b7wor\u00b7den", "nur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN", "VAPP", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie ist geworden, was sie werden sollt' und kann;", "tokens": ["Sie", "ist", "ge\u00b7wor\u00b7den", ",", "was", "sie", "wer\u00b7den", "sollt'", "und", "kann", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VAPP", "$,", "PRELS", "PPER", "VAINF", "VMFIN", "KON", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du aber bist ein Kind, das werden soll ein Mann.", "tokens": ["Du", "a\u00b7ber", "bist", "ein", "Kind", ",", "das", "wer\u00b7den", "soll", "ein", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "$,", "PRELS", "VAINF", "VMFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Darum an der Natur ist alles sch\u00f6n und gro\u00df,", "tokens": ["Da\u00b7rum", "an", "der", "Na\u00b7tur", "ist", "al\u00b7les", "sch\u00f6n", "und", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ART", "NN", "VAFIN", "PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Vollkommen, reich und stark, du schwach, nackt, arm und blo\u00df.", "tokens": ["Voll\u00b7kom\u00b7men", ",", "reich", "und", "stark", ",", "du", "schwach", ",", "nackt", ",", "arm", "und", "blo\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$,", "PPER", "VVFIN", "$,", "ADJD", "$,", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch ist die Kraft in dir, stark, reich und gro\u00df zu werden;", "tokens": ["Doch", "ist", "die", "Kraft", "in", "dir", ",", "stark", ",", "reich", "und", "gro\u00df", "zu", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "APPR", "PPER", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und da\u00df die Kraft du f\u00fchlst, seh' ich an den Geberden.", "tokens": ["Und", "da\u00df", "die", "Kraft", "du", "f\u00fchlst", ",", "seh'", "ich", "an", "den", "Ge\u00b7ber\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+----", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Und dis Gef\u00fchl der Kraft soll man dir nicht zerbrechen;", "tokens": ["Und", "dis", "Ge\u00b7f\u00fchl", "der", "Kraft", "soll", "man", "dir", "nicht", "zer\u00b7bre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "NN", "ART", "NN", "VMFIN", "PIS", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dir soll wenn es erschlafft, der Himmel Muth einsprechen.", "tokens": ["Dir", "soll", "wenn", "es", "er\u00b7schlafft", ",", "der", "Him\u00b7mel", "Muth", "ein\u00b7spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KOUS", "PPER", "VVFIN", "$,", "ART", "NN", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Du kanst nicht sinken, wenn du dich erheben willst,", "tokens": ["Du", "kanst", "nicht", "sin\u00b7ken", ",", "wenn", "du", "dich", "er\u00b7he\u00b7ben", "willst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du am Niedern nicht dein Hochverlangen stillst.", "tokens": ["Wenn", "du", "am", "Nie\u00b7dern", "nicht", "dein", "Hoch\u00b7ver\u00b7lan\u00b7gen", "stillst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Gewonnen ist das Ziel, wenn du den Muth gewannst,", "tokens": ["Ge\u00b7won\u00b7nen", "ist", "das", "Ziel", ",", "wenn", "du", "den", "Muth", "ge\u00b7wannst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df du schon jetzt bist viel, und mehr stets werden kannst.", "tokens": ["Da\u00df", "du", "schon", "jetzt", "bist", "viel", ",", "und", "mehr", "stets", "wer\u00b7den", "kannst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VAFIN", "ADV", "$,", "KON", "ADV", "ADV", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}