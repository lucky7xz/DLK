{"textgrid.poem.52190": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Der Tod und Frau Laura", "genre": "verse", "period": "N.A.", "pub_year": 1861, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war in Avignon am Karneval,", "tokens": ["Es", "war", "in", "A\u00b7vi\u00b7gnon", "am", "Kar\u00b7ne\u00b7val", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NE", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df sich ein M\u00f6rder in den Reigen stahl,", "tokens": ["Da\u00df", "sich", "ein", "M\u00f6r\u00b7der", "in", "den", "Rei\u00b7gen", "stahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und da\u00df die Pest verlarvt sich schwang im Tanz", "tokens": ["Und", "da\u00df", "die", "Pest", "ver\u00b7larvt", "sich", "schwang", "im", "Tanz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "PRF", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit einem schlotterichten Mummenschanz.", "tokens": ["Mit", "ei\u00b7nem", "schlot\u00b7te\u00b7rich\u00b7ten", "Mum\u00b7men\u00b7schanz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In einer nahen Villa t\u00e4uschen sie", "tokens": ["In", "ei\u00b7ner", "na\u00b7hen", "Vil\u00b7la", "t\u00e4u\u00b7schen", "sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Angst mit Wohllaut und mit Phantasie,", "tokens": ["Die", "Angst", "mit", "Wohl\u00b7laut", "und", "mit", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Frau Laura war und auch Petrarca da,", "tokens": ["Frau", "Lau\u00b7ra", "war", "und", "auch", "Pe\u00b7trar\u00b7ca", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "KON", "ADV", "NE", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als an das Tor ein dumpfer Schlag geschah.", "tokens": ["Als", "an", "das", "Tor", "ein", "dum\u00b7pfer", "Schlag", "ge\u00b7schah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die blassen Lippen schaudern vor dem Wein,", "tokens": ["Die", "blas\u00b7sen", "Lip\u00b7pen", "schau\u00b7dern", "vor", "dem", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es tritt ein Wei\u00dfgewandeter herein,", "tokens": ["Es", "tritt", "ein", "Wei\u00df\u00b7ge\u00b7wan\u00b7de\u00b7ter", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der eine Maske mit dem Sterbezug", "tokens": ["Der", "ei\u00b7ne", "Mas\u00b7ke", "mit", "dem", "Ster\u00b7be\u00b7zug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und einen frischgepfl\u00fcckten Lorbeer trug.", "tokens": ["Und", "ei\u00b7nen", "frischge\u00b7pfl\u00fcck\u00b7ten", "Lor\u00b7beer", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Der D\u00e4mon hebt den Lorbeer voller Ruh,", "tokens": ["Der", "D\u00e4\u00b7mon", "hebt", "den", "Lor\u00b7beer", "vol\u00b7ler", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und sinnt und schreitet auf Petrarca zu:", "tokens": ["Und", "sinnt", "und", "schrei\u00b7tet", "auf", "Pe\u00b7trar\u00b7ca", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "APPR", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbich gr\u00fc\u00dfe, Freund, und komme priesterlich,", "tokens": ["\u00bb", "ich", "gr\u00fc\u00b7\u00dfe", ",", "Freund", ",", "und", "kom\u00b7me", "pries\u00b7ter\u00b7lich", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "NN", "$,", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das ist der Sel'gen Lorbeer! Neige dich!\u00ab", "tokens": ["Das", "ist", "der", "Sel'\u00b7gen", "Lor\u00b7beer", "!", "Nei\u00b7ge", "dich", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$.", "NN", "PPER", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Der Lorbeer schwebt. Da raubt ihn eine Hand,", "tokens": ["Der", "Lor\u00b7beer", "schwebt", ".", "Da", "raubt", "ihn", "ei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Frau Laura war es, die daneben stand,", "tokens": ["Frau", "Lau\u00b7ra", "war", "es", ",", "die", "da\u00b7ne\u00b7ben", "stand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "PPER", "$,", "PRELS", "PAV", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie schmiegt ihn um die blonden Haare leicht,", "tokens": ["Sie", "schmiegt", "ihn", "um", "die", "blon\u00b7den", "Haa\u00b7re", "leicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie steht bekr\u00e4nzt. Sie schaudert. Sie erbleicht.", "tokens": ["Sie", "steht", "be\u00b7kr\u00e4nzt", ".", "Sie", "schau\u00b7dert", ".", "Sie", "er\u00b7bleicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "$.", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}