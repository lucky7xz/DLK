{"dta.poem.14695": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Vesper .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Die Abendglocken klangen", "tokens": ["Die", "A\u00b7bend\u00b7glo\u00b7cken", "klan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schon durch das stille Thal,", "tokens": ["Schon", "durch", "das", "stil\u00b7le", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da sa\u00dfen wir zusammen", "tokens": ["Da", "sa\u00b7\u00dfen", "wir", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da droben wohl Hundertmal.", "tokens": ["Da", "dro\u00b7ben", "wohl", "Hun\u00b7dert\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}