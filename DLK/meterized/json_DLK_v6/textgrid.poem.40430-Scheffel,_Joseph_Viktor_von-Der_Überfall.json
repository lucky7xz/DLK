{"textgrid.poem.40430": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "Der \u00dcberfall", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wieder sprach der Rodenstein:", "tokens": ["Und", "wie\u00b7der", "sprach", "der", "Ro\u00b7dens\u00b7tein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbhalloh! mein wildes Heer!", "tokens": ["\u00bb", "hal\u00b7loh", "!", "mein", "wil\u00b7des", "Heer", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In Tiefschluckhausen fall' ich ein", "tokens": ["In", "Tief\u00b7schluck\u00b7hau\u00b7sen", "fall'", "ich", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und trink' den Pfarrer leer.", "tokens": ["Und", "trink'", "den", "Pfar\u00b7rer", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "'raus da! 'raus aus dem Haus da!", "tokens": ["'raus", "da", "!", "'raus", "aus", "dem", "Haus", "da", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "NE", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Herr Pfarr', da\u00df Gott Euch helf'!", "tokens": ["Herr", "Pfarr'", ",", "da\u00df", "Gott", "Euch", "hel\u00b7f'", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "KOUS", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Gibt's nirgend mehr 'nen Tropfen Wein", "tokens": ["Gibt's", "nir\u00b7gend", "mehr", "'nen", "Trop\u00b7fen", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "PIAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des Nachts um halber zw\u00f6lf?\u00ab", "tokens": ["Des", "Nachts", "um", "hal\u00b7ber", "zw\u00f6lf", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "APPR", "APPO", "CARD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Pfarr', ein tapfrer Gottesmann,", "tokens": ["Der", "Pfarr'", ",", "ein", "tapf\u00b7rer", "Got\u00b7tes\u00b7mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Trat streitbar vor sein Tor,", "tokens": ["Trat", "streit\u00b7bar", "vor", "sein", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Weihbrunn, Skapulier und Bann", "tokens": ["Mit", "Weih\u00b7brunn", ",", "Ska\u00b7pu\u00b7lier", "und", "Bann"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Geister er beschwor:", "tokens": ["Die", "Geis\u00b7ter", "er", "be\u00b7schwor", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bb'raus da! 'raus aus dem Haus da!", "tokens": ["\u00bb", "'raus", "da", "!", "'raus", "aus", "dem", "Haus", "da", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "$.", "NE", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Da\u00df Euch der Satan helf',", "tokens": ["Da\u00df", "Euch", "der", "Sa\u00b7tan", "hel\u00b7f'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Kriegt Ihr ein' einzigen Tropfen Wein", "tokens": ["Kriegt", "Ihr", "ein'", "ein\u00b7zi\u00b7gen", "Trop\u00b7fen", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Des Nachts um halber zw\u00f6lf!\u00ab", "tokens": ["Des", "Nachts", "um", "hal\u00b7ber", "zw\u00f6lf", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "APPR", "APPO", "CARD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch fr\u00f6hlich brummt der Rodenstein:", "tokens": ["Doch", "fr\u00f6h\u00b7lich", "brummt", "der", "Ro\u00b7dens\u00b7tein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbo Pfarr', ich fang' dich doch!", "tokens": ["\u00bb", "o", "Pfarr'", ",", "ich", "fang'", "dich", "doch", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Geist, der nicht zum Tor kommt 'rein,", "tokens": ["Ein", "Geist", ",", "der", "nicht", "zum", "Tor", "kommt", "'rein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "APPRART", "NN", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Probiert's am Kellerloch!", "tokens": ["Pro\u00b7biert's", "am", "Kel\u00b7ler\u00b7loch", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "'nein da!...'nein zu dem Wein da!", "tokens": ["'n\u00b7ein", "da", "!", "...", "'n\u00b7ein", "zu", "dem", "Wein", "da", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "$(", "NN", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hurra! schon sind wir drin!", "tokens": ["Hur\u00b7ra", "!", "schon", "sind", "wir", "drin", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VAFIN", "PPER", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Sein Keller ist nicht schlecht besetzt,", "tokens": ["Sein", "Kel\u00b7ler", "ist", "nicht", "schlecht", "be\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hurra, wir trinken ihn!\u00ab", "tokens": ["Hur\u00b7ra", ",", "wir", "trin\u00b7ken", "ihn", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPER", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "O armes, frommes Pfarrerherz,", "tokens": ["O", "ar\u00b7mes", ",", "from\u00b7mes", "Pfar\u00b7rer\u00b7herz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Heut hat der B\u00f6se Macht!", "tokens": ["Heut", "hat", "der", "B\u00f6\u00b7se", "Macht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vergeblich rief er kellerw\u00e4rts,", "tokens": ["Ver\u00b7geb\u00b7lich", "rief", "er", "kel\u00b7ler\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da\u00df das Gew\u00f6lbe kracht:", "tokens": ["Da\u00df", "das", "Ge\u00b7w\u00f6l\u00b7be", "kracht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbschwein' da ... Schwein' da ... bei dem Wein da!", "tokens": ["\u00bb", "schwein'", "da", "...", "Schwein'", "da", "...", "bei", "dem", "Wein", "da", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ADV", "$(", "VVIMP", "ADV", "$(", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hei\u00dft das sich aufgef\u00fchrt?", "tokens": ["Hei\u00dft", "das", "sich", "auf\u00b7ge\u00b7f\u00fchrt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "PRF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So la\u00dft mir doch die Kompetenz,", "tokens": ["So", "la\u00dft", "mir", "doch", "die", "Kom\u00b7pe\u00b7tenz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die einem Pfarr' geb\u00fchrt!\u00ab", "tokens": ["Die", "ei\u00b7nem", "Pfarr'", "ge\u00b7b\u00fchrt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und als die Glocke ein Uhr schlug,", "tokens": ["Und", "als", "die", "Glo\u00b7cke", "ein", "Uhr", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das Heer sang dumpf und hohl:", "tokens": ["Das", "Heer", "sang", "dumpf", "und", "hohl", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbherr Pfarr', Herr Pfarr', jetzt ha'n wir g'nug,", "tokens": ["\u00bb", "herr", "Pfarr'", ",", "Herr", "Pfarr'", ",", "jetzt", "ha'n", "wir", "g'\u00b7nug", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "NE", "$,", "NN", "NE", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Herr Pfarr', jetzt lebet wohl!", "tokens": ["Herr", "Pfarr'", ",", "jetzt", "le\u00b7bet", "wohl", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "'raus jetzt! 'raus aus dem Haus jetzt!", "tokens": ["'raus", "jetzt", "!", "'raus", "aus", "dem", "Haus", "jetzt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "NE", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Herr Pfarr', und bleibt gesund!", "tokens": ["Herr", "Pfarr'", ",", "und", "bleibt", "ge\u00b7sund", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "'s flie\u00dft nirgend mehr ein Tropfen Wein", "tokens": ["'s", "flie\u00dft", "nir\u00b7gend", "mehr", "ein", "Trop\u00b7fen", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Aus Krug und Hahn und Spund.\u00ab", "tokens": ["Aus", "Krug", "und", "Hahn", "und", "Spund", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da flucht der Pfarr': \u00bbIch dank' recht sehr,", "tokens": ["Da", "flucht", "der", "Pfarr'", ":", "\u00bb", "Ich", "dank'", "recht", "sehr", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$(", "PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schwernot! Ist alles hin,", "tokens": ["Schwer\u00b7not", "!", "Ist", "al\u00b7les", "hin", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So will ich selbst im wilden Heer", "tokens": ["So", "will", "ich", "selbst", "im", "wil\u00b7den", "Heer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als Feldkaplan mitziehn!", "tokens": ["Als", "Feld\u00b7ka\u00b7plan", "mit\u00b7ziehn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "'naus jetzt! 'naus aus dem Haus jetzt!", "tokens": ["'naus", "jetzt", "!", "'naus", "aus", "dem", "Haus", "jetzt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "ADV", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Herr Ritter, ich schlag' ein:", "tokens": ["Herr", "Rit\u00b7ter", ",", "ich", "schlag'", "ein", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ist all mein Wein zum Teufel, soll", "tokens": ["Ist", "all", "mein", "Wein", "zum", "Teu\u00b7fel", ",", "soll"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PIAT", "PPOSAT", "NN", "APPRART", "NN", "$,", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein andrer Pfarrherr sein!", "tokens": ["Ein", "an\u00b7drer", "Pfar\u00b7rherr", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Hussah, halloh!", "tokens": ["Hus\u00b7sah", ",", "hal\u00b7loh", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Jo, hihaho!", "tokens": ["Jo", ",", "hi\u00b7ha\u00b7ho", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "ITJ", "$."], "meter": "-+--", "measure": "dactylic.init"}, "line.11": {"text": "Rumdiridi, langt's nit,", "tokens": ["Rum\u00b7di\u00b7ri\u00b7di", ",", "langt's", "nit", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "PTKNEG", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.12": {"text": "Hoidirido, selbst mit!", "tokens": ["Hoi\u00b7di\u00b7ri\u00b7do", ",", "selbst", "mit", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "H\u00f6llischer Chor,", "tokens": ["H\u00f6l\u00b7li\u00b7scher", "Chor", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "Heut reit' ich vor:", "tokens": ["Heut", "reit'", "ich", "vor", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "'naus! 'naus! 'naus!!\u00ab", "tokens": ["'naus", "!", "'naus", "!", "'naus", "!!", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["ADV", "$.", "ADV", "$.", "ADV", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}}}}}