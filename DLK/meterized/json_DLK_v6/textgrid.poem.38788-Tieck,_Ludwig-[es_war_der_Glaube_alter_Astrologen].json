{"textgrid.poem.38788": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[es war der Glaube alter Astrologen]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war der Glaube alter Astrologen,", "tokens": ["Es", "war", "der", "Glau\u00b7be", "al\u00b7ter", "Ast\u00b7ro\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df, wenn Verfinstrung kam dem Sonnensterne,", "tokens": ["Da\u00df", ",", "wenn", "Ver\u00b7finst\u00b7rung", "kam", "dem", "Son\u00b7nens\u00b7ter\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es Ungl\u00fcck deute, bis zur fernsten Ferne;", "tokens": ["Es", "Un\u00b7gl\u00fcck", "deu\u00b7te", ",", "bis", "zur", "ferns\u00b7ten", "Fer\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "$,", "KOUS", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie lehrten, wenn ihn Dunkel \u00fcberzogen,", "tokens": ["Sie", "lehr\u00b7ten", ",", "wenn", "ihn", "Dun\u00b7kel", "\u00fc\u00b7berz\u00b7o\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dann sei der Lichtplanet uns nicht gewogen,", "tokens": ["Dann", "sei", "der", "Licht\u00b7pla\u00b7net", "uns", "nicht", "ge\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Seegens Kr\u00e4fte matt, wie sie auch gerne", "tokens": ["Des", "See\u00b7gens", "Kr\u00e4f\u00b7te", "matt", ",", "wie", "sie", "auch", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "PTKVZ", "$,", "PWAV", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Welt durchdr\u00e4ngen. Wo ich jetzo lerne,", "tokens": ["Die", "Welt", "durch\u00b7dr\u00e4n\u00b7gen", ".", "Wo", "ich", "jet\u00b7zo", "ler\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Schule sagt, da\u00df sie sich nicht getrogen.", "tokens": ["Die", "Schu\u00b7le", "sagt", ",", "da\u00df", "sie", "sich", "nicht", "ge\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wenn ihre Augen auf die mein'gen blicken,", "tokens": ["Wenn", "ih\u00b7re", "Au\u00b7gen", "auf", "die", "mein'\u00b7gen", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ART", "PPOSS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ich mich ganz in Seeligkeit mu\u00df f\u00fchlen,", "tokens": ["Und", "ich", "mich", "ganz", "in", "See\u00b7lig\u00b7keit", "mu\u00df", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PRF", "ADV", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Lebendig, gl\u00e4ubig, voller Lieb' und Freuden,", "tokens": ["Le\u00b7ben\u00b7dig", ",", "gl\u00e4u\u00b7big", ",", "vol\u00b7ler", "Lieb'", "und", "Freu\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Ein Fremder mich von ihrem Blick will scheiden,", "tokens": ["Ein", "Frem\u00b7der", "mich", "von", "ih\u00b7rem", "Blick", "will", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann bin ich aus der Sonne schnell im k\u00fchlen", "tokens": ["Dann", "bin", "ich", "aus", "der", "Son\u00b7ne", "schnell", "im", "k\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "ADJD", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schatten, der Gl\u00fcck mir will und Heil entr\u00fccken.", "tokens": ["Schat\u00b7ten", ",", "der", "Gl\u00fcck", "mir", "will", "und", "Heil", "ent\u00b7r\u00fc\u00b7cken", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "PPER", "VMFIN", "KON", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}