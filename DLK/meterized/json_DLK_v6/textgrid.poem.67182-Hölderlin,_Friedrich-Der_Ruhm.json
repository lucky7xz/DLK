{"textgrid.poem.67182": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Ruhm", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es kn\u00fcpft an Gott der Wohllaut, der geleitet,", "tokens": ["Es", "kn\u00fcpft", "an", "Gott", "der", "Wohl\u00b7laut", ",", "der", "ge\u00b7lei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein sehr ber\u00fchmtes Ohr, denn wunderbar", "tokens": ["Ein", "sehr", "be\u00b7r\u00fchm\u00b7tes", "Ohr", ",", "denn", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN", "$,", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist ein ber\u00fchmtes Leben gro\u00df und klar,", "tokens": ["Ist", "ein", "be\u00b7r\u00fchm\u00b7tes", "Le\u00b7ben", "gro\u00df", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es geht der Mensch zu Fu\u00dfe oder reitet.", "tokens": ["Es", "geht", "der", "Mensch", "zu", "Fu\u00b7\u00dfe", "o\u00b7der", "rei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Erde Freuden, Freundlichkeit und G\u00fcter,", "tokens": ["Der", "Er\u00b7de", "Freu\u00b7den", ",", "Freund\u00b7lich\u00b7keit", "und", "G\u00fc\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Garten, Baum, der Weinberg mit dem H\u00fcter,", "tokens": ["Der", "Gar\u00b7ten", ",", "Baum", ",", "der", "Wein\u00b7berg", "mit", "dem", "H\u00fc\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NE", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie scheinen mir ein Widerglanz des Himmels,", "tokens": ["Sie", "schei\u00b7nen", "mir", "ein", "Wi\u00b7der\u00b7glanz", "des", "Him\u00b7mels", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gew\u00e4hret von dem Geist den S\u00f6hnen des Gewimmels. \u2013", "tokens": ["Ge\u00b7w\u00e4h\u00b7ret", "von", "dem", "Geist", "den", "S\u00f6h\u00b7nen", "des", "Ge\u00b7wim\u00b7mels", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn Einer ist mit G\u00fctern reich begl\u00fccket,", "tokens": ["Wenn", "Ei\u00b7ner", "ist", "mit", "G\u00fc\u00b7tern", "reich", "be\u00b7gl\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VAFIN", "APPR", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn Obst den Garten ihm, und Gold ausschm\u00fccket", "tokens": ["Wenn", "Obst", "den", "Gar\u00b7ten", "ihm", ",", "und", "Gold", "aus\u00b7schm\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "ART", "NN", "PPER", "$,", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Wohnung und das Haus, was mag er haben", "tokens": ["Die", "Woh\u00b7nung", "und", "das", "Haus", ",", "was", "mag", "er", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,", "PWS", "VMFIN", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch mehr in dieser Welt, sein Herz zu laben?", "tokens": ["Noch", "mehr", "in", "die\u00b7ser", "Welt", ",", "sein", "Herz", "zu", "la\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PDAT", "NN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}