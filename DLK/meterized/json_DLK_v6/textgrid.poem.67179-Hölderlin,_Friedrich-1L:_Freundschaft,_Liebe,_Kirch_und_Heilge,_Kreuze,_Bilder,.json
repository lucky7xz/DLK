{"textgrid.poem.67179": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freundschaft, Liebe, Kirch und Heilge, Kreuze, Bilder,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freundschaft, Liebe, Kirch und Heilge, Kreuze, Bilder,", "tokens": ["Freund\u00b7schaft", ",", "Lie\u00b7be", ",", "Kirch", "und", "Heil\u00b7ge", ",", "Kreu\u00b7ze", ",", "Bil\u00b7der", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Altar und Kanzel und Musik. Es t\u00f6net ihm die Predigt.", "tokens": ["Al\u00b7tar", "und", "Kan\u00b7zel", "und", "Mu\u00b7sik", ".", "Es", "t\u00f6\u00b7net", "ihm", "die", "Pre\u00b7digt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$.", "PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Die Kinderlehre scheint nach Tisch ein schlummernd m\u00fc\u00dfig", "tokens": ["Die", "Kin\u00b7der\u00b7leh\u00b7re", "scheint", "nach", "Tisch", "ein", "schlum\u00b7mernd", "m\u00fc\u00b7\u00dfig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "ART", "ADJD", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gespr\u00e4ch f\u00fcr Mann und Kind und Jungfraun, fromme Frauen;", "tokens": ["Ge\u00b7spr\u00e4ch", "f\u00fcr", "Mann", "und", "Kind", "und", "Jung\u00b7fraun", ",", "from\u00b7me", "Frau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "KON", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hernach geht er, der Herr, der Burgersmann und K\u00fcnstler", "tokens": ["Her\u00b7nach", "geht", "er", ",", "der", "Herr", ",", "der", "Bur\u00b7gers\u00b7mann", "und", "K\u00fcnst\u00b7ler"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "NN", "$,", "ART", "NN", "KON", "NN"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Auf Feldern froh umher und heimatlichen Auen,", "tokens": ["Auf", "Fel\u00b7dern", "froh", "um\u00b7her", "und", "hei\u00b7mat\u00b7li\u00b7chen", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "PTKVZ", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Jugend geht betrachtend auch.", "tokens": ["Die", "Ju\u00b7gend", "geht", "be\u00b7trach\u00b7tend", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}