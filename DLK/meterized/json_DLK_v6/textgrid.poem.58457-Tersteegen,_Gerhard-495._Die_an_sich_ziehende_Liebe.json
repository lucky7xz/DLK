{"textgrid.poem.58457": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "495. Die an sich ziehende Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Gotteslieb' ist mein Magnet,", "tokens": ["Die", "Got\u00b7tes\u00b7lieb'", "ist", "mein", "Mag\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sie zieht mich sanft, doch innig, kr\u00e4ftig,", "tokens": ["Sie", "zieht", "mich", "sanft", ",", "doch", "in\u00b7nig", ",", "kr\u00e4f\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "ADV", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df stets der Geist in Gottes Wesen geht;", "tokens": ["Da\u00df", "stets", "der", "Geist", "in", "Got\u00b7tes", "We\u00b7sen", "geht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er geht und l\u00e4uft und ist doch nicht gesch\u00e4ftig,", "tokens": ["Er", "geht", "und", "l\u00e4uft", "und", "ist", "doch", "nicht", "ge\u00b7sch\u00e4f\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VAFIN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Er h\u00e4lt sich nur ganz wirk- und willenlos,", "tokens": ["Er", "h\u00e4lt", "sich", "nur", "ganz", "wirk", "und", "wil\u00b7len\u00b7los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADV", "TRUNC", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So sinkt er wie von selbst in Gottes Scho\u00df.", "tokens": ["So", "sinkt", "er", "wie", "von", "selbst", "in", "Got\u00b7tes", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "APPR", "ADV", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}