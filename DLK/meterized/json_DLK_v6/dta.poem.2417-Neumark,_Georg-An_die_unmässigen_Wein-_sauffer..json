{"dta.poem.2417": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "An die unm\u00e4ssigen Wein-  \n sauffer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr Helden die ihr euch selbst saufft vom Witz' und", "tokens": ["Ihr", "Hel\u00b7den", "die", "ihr", "euch", "selbst", "saufft", "vom", "Witz'", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "PPER", "PPER", "ADV", "ADJD", "APPRART", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sinne", "tokens": ["Sin\u00b7ne"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Die ihr euch sp\u00e4ht und fr\u00fch mit vollem Gla\u00df\u2019 ergetzt/", "tokens": ["Die", "ihr", "euch", "sp\u00e4ht", "und", "fr\u00fch", "mit", "vol\u00b7lem", "Gla\u00df'", "er\u00b7getzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "KON", "ADJD", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da ihr an Statt der Kirch euch in ein Weinhau\u00df", "tokens": ["Da", "ihr", "an", "Statt", "der", "Kirch", "euch", "in", "ein", "Wein\u00b7hau\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "ART", "NN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "setzt/", "tokens": ["setzt", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Die ihr an Gottes Statt die geile Lustg\u00f6ttinne", "tokens": ["Die", "ihr", "an", "Got\u00b7tes", "Statt", "die", "gei\u00b7le", "Lust\u00b7g\u00f6t\u00b7tin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und euren Bachus ehrt bedenkt was Sauffen k\u00f6nne/", "tokens": ["Und", "eu\u00b7ren", "Ba\u00b7chus", "ehrt", "be\u00b7denkt", "was", "Sauf\u00b7fen", "k\u00f6n\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NE", "VVFIN", "VVFIN", "PIS", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Hat nicht der Wein den Loht zur Unzuchtangehetzt?", "tokens": ["Hat", "nicht", "der", "Wein", "den", "Loht", "zur", "Un\u00b7zuch\u00b7tan\u00b7ge\u00b7hetzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hat Alexander nicht den Klitus hingemetzt", "tokens": ["Hat", "A\u00b7lex\u00b7an\u00b7der", "nicht", "den", "Kli\u00b7tus", "hin\u00b7ge\u00b7metzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NE", "PTKNEG", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "In voller Trunkenheit? O Seelenm\u00f6rderinne!", "tokens": ["In", "vol\u00b7ler", "Trun\u00b7ken\u00b7heit", "?", "O", "See\u00b7len\u00b7m\u00f6r\u00b7de\u00b7rin\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "O Lasterquell\u2019! es ist mit Thr\u00e4nen zubeklagen/", "tokens": ["O", "Las\u00b7ter\u00b7quell'", "!", "es", "ist", "mit", "Thr\u00e4\u00b7nen", "zu\u00b7be\u00b7kla\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PPER", "VAFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wie mancher Mord/ Verraht/ wie manche Hurerey;", "tokens": ["Wie", "man\u00b7cher", "Mord", "/", "Ver\u00b7raht", "/", "wie", "man\u00b7che", "Hu\u00b7re\u00b7rey", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "$(", "NN", "$(", "KOKOM", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wie mancher Diebstahl auch durch dich ver\u00fcbet sey.", "tokens": ["Wie", "man\u00b7cher", "Dieb\u00b7stahl", "auch", "durch", "dich", "ver\u00b7\u00fc\u00b7bet", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADV", "APPR", "PPER", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Bedenk die\u00df Trunkenbold! Doch/ mu\u00df ich die\u00df auch sa-", "tokens": ["Be\u00b7denk", "die\u00df", "Trun\u00b7ken\u00b7bold", "!", "Doch", "/", "mu\u00df", "ich", "die\u00df", "auch", "sa"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PDS", "NN", "$.", "KON", "$(", "VMFIN", "PPER", "PDS", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "gen:", "tokens": ["gen", ":"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-", "measure": "single.down"}, "line.16": {"text": "Der Wein der ist gesund wenn man ihn recht geneust/", "tokens": ["Der", "Wein", "der", "ist", "ge\u00b7sund", "wenn", "man", "ihn", "recht", "ge\u00b7neust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "ADJD", "KOUS", "PIS", "PPER", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ein Gifft imfall man ihn wie Wasser in sich geust.", "tokens": ["Ein", "Gifft", "im\u00b7fall", "man", "ihn", "wie", "Was\u00b7ser", "in", "sich", "geust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PIS", "PPER", "KOKOM", "NN", "APPR", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}