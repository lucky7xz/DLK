{"textgrid.poem.25118": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die deutsche Mutter", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist ein Fest, ein herrliches, heut!", "tokens": ["Das", "ist", "ein", "Fest", ",", "ein", "herr\u00b7li\u00b7ches", ",", "heut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "$,", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Kanonengekrach und Glockengel\u00e4ut", "tokens": ["Ka\u00b7no\u00b7nen\u00b7ge\u00b7krach", "und", "Glo\u00b7cken\u00b7ge\u00b7l\u00e4ut"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und Hallen von Siegesliedern!", "tokens": ["Und", "Hal\u00b7len", "von", "Sie\u00b7ges\u00b7lie\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nein, nein! Rei\u00dft ab von den Helmen das Laub", "tokens": ["Nein", ",", "nein", "!", "Rei\u00dft", "ab", "von", "den", "Hel\u00b7men", "das", "Laub"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PTKANT", "$.", "VVFIN", "PTKVZ", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und streut auf das Schlachtfeld Asche und Staub,", "tokens": ["Und", "streut", "auf", "das", "Schlacht\u00b7feld", "A\u00b7sche", "und", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wo Br\u00fcder sich w\u00fcrgten mit Br\u00fcdern!", "tokens": ["Wo", "Br\u00fc\u00b7der", "sich", "w\u00fcrg\u00b7ten", "mit", "Br\u00fc\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PRF", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Tot beide, die ich mit Schmerzen gebar,", "tokens": ["Tot", "bei\u00b7de", ",", "die", "ich", "mit", "Schmer\u00b7zen", "ge\u00b7bar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "$,", "PRELS", "PPER", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die sch\u00f6ner und sch\u00f6ner von Jahr zu Jahr", "tokens": ["Die", "sch\u00f6\u00b7ner", "und", "sch\u00f6\u00b7ner", "von", "Jahr", "zu", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "KON", "ADJD", "APPR", "NN", "APPR", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Erbl\u00fchten an meinen K\u00fcssen!", "tokens": ["Er\u00b7bl\u00fch\u00b7ten", "an", "mei\u00b7nen", "K\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gebrochen nun in des Lebens Mai", "tokens": ["Ge\u00b7bro\u00b7chen", "nun", "in", "des", "Le\u00b7bens", "Mai"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ihr rosiges Haupt! Vom hei\u00dfen Blei", "tokens": ["Ihr", "ro\u00b7si\u00b7ges", "Haupt", "!", "Vom", "hei\u00b7\u00dfen", "Blei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die Brust den Teuern zerrissen!", "tokens": ["Die", "Brust", "den", "Teu\u00b7ern", "zer\u00b7ris\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "O h\u00e4tt' ich \u2013 das ist's, was am Herzen mir zehrt \u2013", "tokens": ["O", "h\u00e4tt'", "ich", "\u2013", "das", "ist's", ",", "was", "am", "Her\u00b7zen", "mir", "zehrt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$(", "PDS", "VAFIN", "$,", "PRELS", "APPRART", "NN", "PPER", "VVFIN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Das Wort sie nimmer stammeln gelehrt,", "tokens": ["Das", "Wort", "sie", "nim\u00b7mer", "stam\u00b7meln", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Das in den Tod sie getrieben!", "tokens": ["Das", "in", "den", "Tod", "sie", "ge\u00b7trie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mein, mein die Schuld! Mit erhobener Hand", "tokens": ["Mein", ",", "mein", "die", "Schuld", "!", "Mit", "er\u00b7ho\u00b7be\u00b7ner", "Hand"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "$,", "PPOSAT", "ART", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-++-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Gebot ich ihnen, das Vaterland,", "tokens": ["Ge\u00b7bot", "ich", "ih\u00b7nen", ",", "das", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Das deutsche, vor allem zu lieben.", "tokens": ["Das", "deut\u00b7sche", ",", "vor", "al\u00b7lem", "zu", "lie\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "APPR", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Wenn abends die zwei mir sa\u00dfen im Scho\u00df,", "tokens": ["Wenn", "a\u00b7bends", "die", "zwei", "mir", "sa\u00b7\u00dfen", "im", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "CARD", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Oft ihnen erz\u00e4hlt' ich von Waterloos,", "tokens": ["Oft", "ih\u00b7nen", "er\u00b7z\u00e4hlt'", "ich", "von", "Wa\u00b7ter\u00b7loos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Von Leipzigs herrlichen Schlachten,", "tokens": ["Von", "Leip\u00b7zigs", "herr\u00b7li\u00b7chen", "Schlach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wie heim aus dem Feld ihr Vater, ihr Ahn", "tokens": ["Wie", "heim", "aus", "dem", "Feld", "ihr", "Va\u00b7ter", ",", "ihr", "Ahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "ART", "NN", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Sich Ehren f\u00fcr Thaten, die sie gethan,", "tokens": ["Sich", "Eh\u00b7ren", "f\u00fcr", "Tha\u00b7ten", ",", "die", "sie", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "APPR", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und leuchtende Wunden brachten.", "tokens": ["Und", "leuch\u00b7ten\u00b7de", "Wun\u00b7den", "brach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Da flammten die Augen der Knaben in Glut", "tokens": ["Da", "flamm\u00b7ten", "die", "Au\u00b7gen", "der", "Kna\u00b7ben", "in", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und lie\u00dfen mit Stolz des Gatten Blut", "tokens": ["Und", "lie\u00b7\u00dfen", "mit", "Stolz", "des", "Gat\u00b7ten", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "NN", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In den Adern der S\u00f6hne mich ahnen.", "tokens": ["In", "den", "A\u00b7dern", "der", "S\u00f6h\u00b7ne", "mich", "ah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Was mehr? Die J\u00fcnglinge trieb es \u2013 kein Halt! \u2013", "tokens": ["Was", "mehr", "?", "Die", "J\u00fcng\u00b7lin\u00b7ge", "trieb", "es", "\u2013", "kein", "Halt", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PWS", "ADV", "$.", "ART", "NN", "VVFIN", "PPER", "$(", "PIAT", "NN", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Zu Habsburgs Adler den Theobald,", "tokens": ["Zu", "Habs\u00b7burgs", "Ad\u00b7ler", "den", "Theo\u00b7bald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "ART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Karl zu den preu\u00dfischen Fahnen.", "tokens": ["Den", "Karl", "zu", "den", "preu\u00b7\u00dfi\u00b7schen", "Fah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbmein Bruder, leb wohl! Doch bald vereint", "tokens": ["\u00bb", "mein", "Bru\u00b7der", ",", "leb", "wohl", "!", "Doch", "bald", "ver\u00b7eint"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "$,", "VVFIN", "ADV", "$.", "KON", "ADV", "VVPP"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wehn unsere Banner wider den Feind", "tokens": ["Wehn", "un\u00b7se\u00b7re", "Ban\u00b7ner", "wi\u00b7der", "den", "Feind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und jagen ans Meer ihn nach Westen;", "tokens": ["Und", "ja\u00b7gen", "ans", "Meer", "ihn", "nach", "Wes\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPER", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "F\u00fcr Deutschland, wie uns die Mutter gelehrt,", "tokens": ["F\u00fcr", "Deutschland", ",", "wie", "uns", "die", "Mut\u00b7ter", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PWAV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "La\u00df dann, des Ahnen, des Vaters wert,", "tokens": ["La\u00df", "dann", ",", "des", "Ah\u00b7nen", ",", "des", "Va\u00b7ters", "wert", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "ART", "NN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Uns k\u00e4mpfen unter den Besten.\u00ab", "tokens": ["Uns", "k\u00e4mp\u00b7fen", "un\u00b7ter", "den", "Bes\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Und sie tr\u00e4umten noch von vereintem Sieg;", "tokens": ["Und", "sie", "tr\u00e4um\u00b7ten", "noch", "von", "ver\u00b7ein\u00b7tem", "Sieg", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wer war es, o wer, der da den Krieg", "tokens": ["Wer", "war", "es", ",", "o", "wer", ",", "der", "da", "den", "Krieg"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "$,", "FM", "PWS", "$,", "PRELS", "ADV", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Von Deutschen mit Deutschen entflammte?", "tokens": ["Von", "Deut\u00b7schen", "mit", "Deut\u00b7schen", "ent\u00b7flamm\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wohl bebte zur\u00fcck die entsetzte Natur;", "tokens": ["Wohl", "beb\u00b7te", "zu\u00b7r\u00fcck", "die", "ent\u00b7setz\u00b7te", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Doch band an die Fahnen die zwei ihr Schwur", "tokens": ["Doch", "band", "an", "die", "Fah\u00b7nen", "die", "zwei", "ihr", "Schwur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "CARD", "PPER", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und ri\u00df sie ans Werk, das verdammte.", "tokens": ["Und", "ri\u00df", "sie", "ans", "Werk", ",", "das", "ver\u00b7damm\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$,", "ART", "ADJA", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "Die H\u00f6lle jauchzte; von S\u00fcd und Nord", "tokens": ["Die", "H\u00f6l\u00b7le", "jauchz\u00b7te", ";", "von", "S\u00fcd", "und", "Nord"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Entgegen sich zogen zum Brudermord", "tokens": ["Ent\u00b7ge\u00b7gen", "sich", "zo\u00b7gen", "zum", "Bru\u00b7der\u00b7mord"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "VVFIN", "APPRART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Die Heere mit klingendem Spiele;", "tokens": ["Die", "Hee\u00b7re", "mit", "klin\u00b7gen\u00b7dem", "Spie\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und, wie ich jammernd am Boden lag,", "tokens": ["Und", ",", "wie", "ich", "jam\u00b7mernd", "am", "Bo\u00b7den", "lag", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die beiden S\u00f6hne bei Nacht und Tag", "tokens": ["Die", "bei\u00b7den", "S\u00f6h\u00b7ne", "bei", "Nacht", "und", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Schaut' ich in dem Schlachtengew\u00fchle.", "tokens": ["Schaut'", "ich", "in", "dem", "Schlach\u00b7ten\u00b7ge\u00b7w\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Und Flammenzischen und R\u00e4dergeroll", "tokens": ["Und", "Flam\u00b7men\u00b7zi\u00b7schen", "und", "R\u00e4\u00b7der\u00b7ge\u00b7roll"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und Krachen der Feuerschl\u00fcnde erscholl", "tokens": ["Und", "Kra\u00b7chen", "der", "F