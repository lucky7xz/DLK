{"textgrid.poem.32552": {"metadata": {"author": {"name": "Scheffner, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der Maler und der Liebhaber.", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbsoll ich dir dein R\u00f6schen malen,", "tokens": ["\u00bb", "soll", "ich", "dir", "dein", "R\u00f6\u00b7schen", "ma\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach den sch\u00f6nsten Idealen,", "tokens": ["Nach", "den", "sch\u00f6ns\u00b7ten", "I\u00b7dea\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Oder Zug vor Zug genau?", "tokens": ["O\u00b7der", "Zug", "vor", "Zug", "ge\u00b7nau", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Locken, die den Hals umfangen,", "tokens": ["Lo\u00b7cken", ",", "die", "den", "Hals", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gr\u00fcbchen in den Rosenwangen", "tokens": ["Gr\u00fcb\u00b7chen", "in", "den", "Ro\u00b7sen\u00b7wan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Frey die Stirn, das Auge blau?", "tokens": ["Frey", "die", "Stirn", ",", "das", "Au\u00b7ge", "blau", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mal ich Lippen, die beym Lachen", "tokens": ["Mal", "ich", "Lip\u00b7pen", ",", "die", "beym", "La\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jene Gr\u00fcbchen tiefer machen,", "tokens": ["Je\u00b7ne", "Gr\u00fcb\u00b7chen", "tie\u00b7fer", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Z\u00e4hne, so wie Perlen sch\u00f6n?", "tokens": ["Z\u00e4h\u00b7ne", ",", "so", "wie", "Per\u00b7len", "sch\u00f6n", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "KOKOM", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter Flor die Busenh\u00fcgel,", "tokens": ["Un\u00b7ter", "Flor", "die", "Bu\u00b7sen\u00b7h\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Arme wei\u00df wie Schwanenfl\u00fcgel,", "tokens": ["Ar\u00b7me", "wei\u00df", "wie", "Schwa\u00b7nen\u00b7fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "H\u00e4nde wie die Grazien?", "tokens": ["H\u00e4n\u00b7de", "wie", "die", "Gra\u00b7zi\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mal ich ", "tokens": ["Mal", "ich"], "token_info": ["word", "word"], "pos": ["NN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Wie der Juno Hals und Schultern", "tokens": ["Wie", "der", "Ju\u00b7no", "Hals", "und", "Schul\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Glat und wei\u00df wie Elfenbein?", "tokens": ["Glat", "und", "wei\u00df", "wie", "El\u00b7fen\u00b7bein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "KOKOM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Cypria vom Meer gebohren,", "tokens": ["Cyp\u00b7ria", "vom", "Meer", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nympfen warm in Lust verlohren,", "tokens": ["Nymp\u00b7fen", "warm", "in", "Lust", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sollen diese mir \u2013 ?\u00ab Nein, nein!", "tokens": ["Sol\u00b7len", "die\u00b7se", "mir", "\u2013", "?", "\u00ab", "Nein", ",", "nein", "!"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PDS", "PPER", "$(", "$.", "$(", "PTKANT", "$,", "PTKANT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Braune Locken magst du malen,", "tokens": ["Brau\u00b7ne", "Lo\u00b7cken", "magst", "du", "ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blauer Augen Himmelsstraalen", "tokens": ["Blau\u00b7er", "Au\u00b7gen", "Him\u00b7mels\u00b7straa\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meisterz\u00fcge ins Gesicht,", "tokens": ["Meis\u00b7ter\u00b7z\u00fc\u00b7ge", "ins", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Arme, Hand und Busenh\u00f6hen,", "tokens": ["Ar\u00b7me", ",", "Hand", "und", "Bu\u00b7sen\u00b7h\u00f6\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber was ich mehr gesehen,", "tokens": ["A\u00b7ber", "was", "ich", "mehr", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nein das sch\u00f6nste triffst du nicht.", "tokens": ["Nein", "das", "sch\u00f6ns\u00b7te", "triffst", "du", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "ADJA", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Der die h\u00f6chste Kunst erf\u00fcllte,", "tokens": ["Der", "die", "h\u00f6chs\u00b7te", "Kunst", "er\u00b7f\u00fcll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als er Amors Mutter bildte,", "tokens": ["Als", "er", "A\u00b7mors", "Mut\u00b7ter", "bild\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4tt er ", "tokens": ["H\u00e4tt", "er"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Schnell h\u00e4tt' er sein Werk zernichtet,", "tokens": ["Schnell", "h\u00e4tt'", "er", "sein", "Werk", "zer\u00b7nich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Dann w\u00e4re seine Venus sch\u00f6n.", "tokens": ["Dann", "w\u00e4\u00b7re", "sei\u00b7ne", "Ve\u00b7nus", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der zum Ungl\u00fcck der Dryaden", "tokens": ["Der", "zum", "Un\u00b7gl\u00fcck", "der", "Dry\u00b7a\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einst Dianen sah sich baden,", "tokens": ["Einst", "Di\u00b7a\u00b7nen", "sah", "sich", "ba\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Diesem w\u00e4rs vielleicht gegl\u00fcckt,", "tokens": ["Die\u00b7sem", "w\u00e4rs", "viel\u00b7leicht", "ge\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie nicht den Reitz verleugnen,", "tokens": ["Da\u00df", "sie", "nicht", "den", "Reitz", "ver\u00b7leug\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Womit sich Natur geschm\u00fcckt.", "tokens": ["Wo\u00b7mit", "sich", "Na\u00b7tur", "ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und der ", "tokens": ["Und", "der"], "token_info": ["word", "word"], "pos": ["KON", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Den die G\u00f6tter einst bey Heben,", "tokens": ["Den", "die", "G\u00f6t\u00b7ter", "einst", "bey", "He\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als sie fiel, bezaubernd sahn,", "tokens": ["Als", "sie", "fiel", ",", "be\u00b7zau\u00b7bernd", "sahn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter allen ", "tokens": ["Un\u00b7ter", "al\u00b7len"], "token_info": ["word", "word"], "pos": ["APPR", "PIAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Sie die sch\u00f6nste \u2013 die zu malen,", "tokens": ["Sie", "die", "sch\u00f6ns\u00b7te", "\u2013", "die", "zu", "ma\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "$(", "ART", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sprich, darf sich die Kunst ihr nah'n?", "tokens": ["Sprich", ",", "darf", "sich", "die", "Kunst", "ihr", "nah'n", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VMFIN", "PRF", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.8": {"line.1": {"text": "Als durch Venus G\u00fcrtelskr\u00e4fte,", "tokens": ["Als", "durch", "Ve\u00b7nus", "G\u00fcr\u00b7tels\u00b7kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zeus das gro\u00dfe Weltgesch\u00e4fte,", "tokens": ["Zeus", "das", "gro\u00b7\u00dfe", "Welt\u00b7ge\u00b7sch\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hinter goldnen Wolken that,", "tokens": ["Hin\u00b7ter", "gold\u00b7nen", "Wol\u00b7ken", "that", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wagts je wer da sie zu malen?", "tokens": ["Wagts", "je", "wer", "da", "sie", "zu", "ma\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PWS", "KOUS", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und hier blenden Wolluststrahlen", "tokens": ["Und", "hier", "blen\u00b7den", "Wol\u00b7lust\u00b7strah\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mehr, als dort der Goldglanz that.", "tokens": ["Mehr", ",", "als", "dort", "der", "Gold\u00b7glanz", "that", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}