{"dta.poem.6107": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Schwere und der Leichte,  \n von der Fluht und Ebbe  \n des Meeres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nach diesem Lehr-Grund nun, der, allem Schein nach, fest,", "tokens": ["Nach", "die\u00b7sem", "Lehr\u00b7Grund", "nun", ",", "der", ",", "al\u00b7lem", "Schein", "nach", ",", "fest", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "$,", "PRELS", "$,", "PIS", "NN", "PTKVZ", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wol als leicht und einfach l\u00e4sst", "tokens": ["So", "wol", "als", "leicht", "und", "ein\u00b7fach", "l\u00e4sst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "KOKOM", "ADJD", "KON", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kan man mit mehrerm Lichte lehren", "tokens": ["Kan", "man", "mit", "meh\u00b7rerm", "Lich\u00b7te", "leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "APPR", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von allen dem, was uns in dieser Welt", "tokens": ["Von", "al\u00b7len", "dem", ",", "was", "uns", "in", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PDS", "$,", "PRELS", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sonst unbegreifflich f\u00e4llt.", "tokens": ["Sonst", "un\u00b7be\u00b7greif\u00b7flich", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Was recht geheim ist, scheint dadurch sich zu erkl\u00e4ren.", "tokens": ["Was", "recht", "ge\u00b7heim", "ist", ",", "scheint", "da\u00b7durch", "sich", "zu", "er\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VAFIN", "$,", "VVFIN", "PAV", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}