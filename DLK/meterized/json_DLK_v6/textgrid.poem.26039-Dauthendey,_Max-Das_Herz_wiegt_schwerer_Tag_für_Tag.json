{"textgrid.poem.26039": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Das Herz wiegt schwerer Tag f\u00fcr Tag", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nebel tragen die Berge fort,", "tokens": ["Ne\u00b7bel", "tra\u00b7gen", "die", "Ber\u00b7ge", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Die Erde wird so herbstlich weit,", "tokens": ["Die", "Er\u00b7de", "wird", "so", "herbst\u00b7lich", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie vom feinen Wein entr\u00fcckt", "tokens": ["Und", "wie", "vom", "fei\u00b7nen", "Wein", "ent\u00b7r\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Stirbt um uns jede Wirklichkeit.", "tokens": ["Stirbt", "um", "uns", "je\u00b7de", "Wirk\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Herz wiegt schwerer Tag f\u00fcr Tag,", "tokens": ["Das", "Herz", "wiegt", "schwe\u00b7rer", "Tag", "f\u00fcr", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und eine Hand, die unverr\u00fcckt", "tokens": ["Und", "ei\u00b7ne", "Hand", ",", "die", "un\u00b7ver\u00b7r\u00fcckt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In Frucht und Feuer bei dir lag,", "tokens": ["In", "Frucht", "und", "Feu\u00b7er", "bei", "dir", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wiegt in dem leeren Nebelland", "tokens": ["Wiegt", "in", "dem", "lee\u00b7ren", "Ne\u00b7bel\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Schwerer als deines Herzens Schlag.", "tokens": ["Schwe\u00b7rer", "als", "dei\u00b7nes", "Her\u00b7zens", "Schlag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPOSAT", "NN", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}