{"textgrid.poem.44637": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Erkl\u00e4rung", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Poneros m\u00fcht sich, und den Lohn", "tokens": ["Po\u00b7ne\u00b7ros", "m\u00fcht", "sich", ",", "und", "den", "Lohn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "$,", "KON", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Tr\u00e4gt Puseros der Wicht davon!", "tokens": ["Tr\u00e4gt", "Pu\u00b7se\u00b7ros", "der", "Wicht", "da\u00b7von", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ART", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Staunt nicht! So wars zu allen Zeiten,", "tokens": ["Staunt", "nicht", "!", "So", "wars", "zu", "al\u00b7len", "Zei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$.", "ADV", "VAFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wers leugnet, spricht verleumderisch!", "tokens": ["Wers", "leug\u00b7net", ",", "spricht", "ver\u00b7leum\u00b7de\u00b7risch", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hilft nicht der Kopf ein jedes Mahl bereiten,", "tokens": ["Hilft", "nicht", "der", "Kopf", "ein", "je\u00b7des", "Mahl", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "ART", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und setzt nicht stets der Hintern sich zum Tisch.", "tokens": ["Und", "setzt", "nicht", "stets", "der", "Hin\u00b7tern", "sich", "zum", "Tisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}