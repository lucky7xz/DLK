{"dta.poem.12942": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An Sylvien, von der h\u00e4rtigkeit  \n ihres hertzens.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als, strenge Sylvia! dich deine sch\u00f6ne mutter", "tokens": ["Als", ",", "stren\u00b7ge", "Syl\u00b7via", "!", "dich", "dei\u00b7ne", "sch\u00f6\u00b7ne", "mut\u00b7ter"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "VVFIN", "NE", "$.", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Noch unter ihrem hertzen trug;", "tokens": ["Noch", "un\u00b7ter", "ih\u00b7rem", "hert\u00b7zen", "trug", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So f\u00fcgt\u2019 es sich, da\u00df ihr ein theurer diamant", "tokens": ["So", "f\u00fcgt'", "es", "sich", ",", "da\u00df", "ihr", "ein", "theu\u00b7rer", "di\u00b7a\u00b7mant"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Recht kr\u00e4fftig in die augen blitzte:", "tokens": ["Recht", "kr\u00e4ff\u00b7tig", "in", "die", "au\u00b7gen", "blitz\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Weil nun desselben lichter zug", "tokens": ["Weil", "nun", "des\u00b7sel\u00b7ben", "lich\u00b7ter", "zug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die lust darnach ie mehr und mehr in ihr erhitzte,", "tokens": ["Die", "lust", "dar\u00b7nach", "ie", "mehr", "und", "mehr", "in", "ihr", "er\u00b7hitz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PAV", "ADV", "ADV", "KON", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So f\u00fchrte sie von ohngefehr die hand", "tokens": ["So", "f\u00fchr\u00b7te", "sie", "von", "ohn\u00b7ge\u00b7fehr", "die", "hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "In solcher regung zu dem hertzen,", "tokens": ["In", "sol\u00b7cher", "re\u00b7gung", "zu", "dem", "hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Dadurch sie, aber blos zu mehrung meiner schmertzen,", "tokens": ["Da\u00b7durch", "sie", ",", "a\u00b7ber", "blos", "zu", "meh\u00b7rung", "mei\u00b7ner", "schmert\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "$,", "ADV", "ADV", "APPR", "NN", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das wunder-werck in dich gelegt,", "tokens": ["Das", "wun\u00b7der\u00b7\u00b7werck", "in", "dich", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Da\u00df deine brust ein hertz aus diamanten tr\u00e4gt.", "tokens": ["Da\u00df", "dei\u00b7ne", "brust", "ein", "hertz", "aus", "di\u00b7a\u00b7man\u00b7ten", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}