{"textgrid.poem.64259": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der strenge, kluge Vater hat gespart,", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der strenge, kluge Vater hat gespart,", "tokens": ["Der", "stren\u00b7ge", ",", "klu\u00b7ge", "Va\u00b7ter", "hat", "ge\u00b7spart", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gedrillt mit harter Hand in strenger Zucht,", "tokens": ["Ge\u00b7drillt", "mit", "har\u00b7ter", "Hand", "in", "stren\u00b7ger", "Zucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kopfsch\u00fcttelnd ob dem andersart'gen Sohn.", "tokens": ["Kopf\u00b7sch\u00fct\u00b7telnd", "ob", "dem", "an\u00b7ders\u00b7art'\u00b7gen", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ja, Falk, dir wuchs ein Adler in dem Horst!", "tokens": ["Ja", ",", "Falk", ",", "dir", "wuchs", "ein", "Ad\u00b7ler", "in", "dem", "Horst", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NE", "$,", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bald fliegt er sonnenw\u00e4rts, der junge Aar,", "tokens": ["Bald", "fliegt", "er", "son\u00b7nen\u00b7w\u00e4rts", ",", "der", "jun\u00b7ge", "Aar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und zeigen wird sich der erstaunten Welt,", "tokens": ["Und", "zei\u00b7gen", "wird", "sich", "der", "er\u00b7staun\u00b7ten", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VAFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Was auf des Vaters m\u00fcheschwerem Grundwerk", "tokens": ["Was", "auf", "des", "Va\u00b7ters", "m\u00fc\u00b7he\u00b7schwe\u00b7rem", "Grund\u00b7werk"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Genius Friedrichs Gro\u00dfes bauen kann.", "tokens": ["Der", "Ge\u00b7nius", "Fried\u00b7richs", "Gro\u00b7\u00dfes", "bau\u00b7en", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NN", "VVINF", "VMFIN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}}}}