{"textgrid.poem.48895": {"metadata": {"author": {"name": "Kleist, Ewald Christian von", "birth": "N.A.", "death": "N.A."}, "title": "An die Morgenr\u00f6the", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aurora fahr herauf auf deinem g\u00fcldnen Wagen,", "tokens": ["Au\u00b7ro\u00b7ra", "fahr", "her\u00b7auf", "auf", "dei\u00b7nem", "g\u00fcld\u00b7nen", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da ich f\u00fcr Lieb und Schmerz nicht schlafen kann!", "tokens": ["Da", "ich", "f\u00fcr", "Lieb", "und", "Schmerz", "nicht", "schla\u00b7fen", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn Chloe bey mir ruht, dann halt die Z\u00fcgel an,", "tokens": ["Wenn", "Chloe", "bey", "mir", "ruht", ",", "dann", "halt", "die", "Z\u00fc\u00b7gel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Dann G\u00f6ttin, la\u00df es sp\u00e4te tagen.", "tokens": ["Dann", "G\u00f6t\u00b7tin", ",", "la\u00df", "es", "sp\u00e4\u00b7te", "ta\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "VVIMP", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}