{"textgrid.poem.56767": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbvon der Br\u00fccke hinunter", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbvon der Br\u00fccke hinunter", "tokens": ["\u00bb", "von", "der", "Br\u00fc\u00b7cke", "hin\u00b7un\u00b7ter"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "APPR", "ART", "NN", "APPO"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "in die dunklen, ruhlosen Fluten,", "tokens": ["in", "die", "dunk\u00b7len", ",", "ruh\u00b7lo\u00b7sen", "Flu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "deren Wellen um Wellen", "tokens": ["de\u00b7ren", "Wel\u00b7len", "um", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PRELAT", "NN", "APPR", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "deine Blicke mit sich fort ziehen,", "tokens": ["dei\u00b7ne", "Bli\u00b7cke", "mit", "sich", "fort", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PRF", "PTKVZ", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "deren Wellen um Wellen", "tokens": ["de\u00b7ren", "Wel\u00b7len", "um", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PRELAT", "NN", "APPR", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "ein St\u00fcck deines Willens", "tokens": ["ein", "St\u00fcck", "dei\u00b7nes", "Wil\u00b7lens"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "davonf\u00fchren,", "tokens": ["da\u00b7von\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["PAV", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "bis er ganz dir geraubt,", "tokens": ["bis", "er", "ganz", "dir", "ge\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "und dein Leib,", "tokens": ["und", "dein", "Leib", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "leer,", "tokens": ["leer", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-", "measure": "single.down"}, "line.11": {"text": "schwer,", "tokens": ["schwer", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+", "measure": "single.up"}, "line.12": {"text": "\u00fcbers Gel\u00e4nder schl\u00e4gt \u2013", "tokens": ["\u00fc\u00b7bers", "Ge\u00b7l\u00e4n\u00b7der", "schl\u00e4gt", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "von der Br\u00fccke hinunter", "tokens": ["von", "der", "Br\u00fc\u00b7cke", "hin\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPO"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "schaue, sp\u00e4he ...", "tokens": ["schau\u00b7e", ",", "sp\u00e4\u00b7he", "..."], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "siehst du das Wort nicht,", "tokens": ["siehst", "du", "das", "Wort", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "$,"], "meter": "+-+++", "measure": "unknown.measure.tetra"}, "line.4": {"text": "das meine Finger", "tokens": ["das", "mei\u00b7ne", "Fin\u00b7ger"], "token_info": ["word", "word", "word"], "pos": ["PDS", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "ins Wasser schreiben?", "tokens": ["ins", "Was\u00b7ser", "schrei\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Friede ... Friede ...!", "tokens": ["Frie\u00b7de", "...", "Frie\u00b7de", "...", "!"], "token_info": ["word", "punct", "word", "punct", "punct"], "pos": ["NN", "$(", "NN", "$(", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "und was ich nun schreibe?", "tokens": ["und", "was", "ich", "nun", "schrei\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Komm!", "tokens": ["Komm", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Komm!!", "tokens": ["Komm", "!!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Siehst du es nicht?", "tokens": ["Siehst", "du", "es", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Beuge dich tiefer!", "tokens": ["Beu\u00b7ge", "dich", "tie\u00b7fer", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Komm!!!\u00ab", "tokens": ["Komm", "!!!", "\u00ab"], "token_info": ["word", "punct", "punct"], "pos": ["VVFIN", "$.", "$("], "meter": "+", "measure": "single.up"}}}}}