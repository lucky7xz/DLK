{"textgrid.poem.35809": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Maria geht zum Grabe", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Maria geht zum Grabe", "tokens": ["Ma\u00b7ria", "geht", "zum", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Mit stillem, raschem Schritt;", "tokens": ["Mit", "stil\u00b7lem", ",", "ra\u00b7schem", "Schritt", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie tr\u00e4gt die duft'ge Gabe;", "tokens": ["Sie", "tr\u00e4gt", "die", "duft'\u00b7ge", "Ga\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O Heil'ge, nimm mich mit!", "tokens": ["O", "Heil'\u00b7ge", ",", "nimm", "mich", "mit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du bringst Ihm fromme Gabe,", "tokens": ["Du", "bringst", "Ihm", "from\u00b7me", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Voll Trauer und voll Schmerz;", "tokens": ["Voll", "Trau\u00b7er", "und", "voll", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und weil ich sonst nichts habe,", "tokens": ["Und", "weil", "ich", "sonst", "nichts", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PIS", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So bring' ich Ihm mein Herz.", "tokens": ["So", "bring'", "ich", "Ihm", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er wird es doch wohl nehmen?", "tokens": ["Er", "wird", "es", "doch", "wohl", "neh\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es ist voll S\u00fcnd' und Reu';", "tokens": ["Es", "ist", "voll", "S\u00fcnd'", "und", "Reu'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "NN", "KON", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er wird sich mein nicht sch\u00e4men,", "tokens": ["Er", "wird", "sich", "mein", "nicht", "sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "PPOSAT", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Heiland ist getreu.", "tokens": ["Der", "Hei\u00b7land", "ist", "ge\u00b7treu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Herr, Du mu\u00dft selbst es reinen,", "tokens": ["Herr", ",", "Du", "mu\u00dft", "selbst", "es", "rei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Von S\u00fcnden machen frei", "tokens": ["Von", "S\u00fcn\u00b7den", "ma\u00b7chen", "frei"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und es mit Dir vereinen,", "tokens": ["Und", "es", "mit", "Dir", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auf da\u00df es heilig sei.", "tokens": ["Auf", "da\u00df", "es", "hei\u00b7lig", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dein g\u00f6ttliches Erbarmen,", "tokens": ["Dein", "g\u00f6tt\u00b7li\u00b7ches", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Herr, umfange mich,", "tokens": ["O", "Herr", ",", "um\u00b7fan\u00b7ge", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich mit meinen Armen", "tokens": ["Da\u00df", "ich", "mit", "mei\u00b7nen", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Umfasse nichts als Dich.", "tokens": ["Um\u00b7fas\u00b7se", "nichts", "als", "Dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "KOKOM", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kein Sehnen und kein Bangen", "tokens": ["Kein", "Seh\u00b7nen", "und", "kein", "Ban\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "La\u00df in mein Herz hinein;", "tokens": ["La\u00df", "in", "mein", "Herz", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mein Langen und Umfangen,", "tokens": ["Mein", "Lan\u00b7gen", "und", "Um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das gelte Dir allein.", "tokens": ["Das", "gel\u00b7te", "Dir", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mein Gott! mein Herr! la\u00df sterben,", "tokens": ["Mein", "Gott", "!", "mein", "Herr", "!", "la\u00df", "ster\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "NN", "$.", "VVIMP", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "La\u00df sterben mich mit Dir,", "tokens": ["La\u00df", "ster\u00b7ben", "mich", "mit", "Dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PRF", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich mu\u00df ja doch verderben", "tokens": ["Ich", "mu\u00df", "ja", "doch", "ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor Deiner Grabesth\u00fcr.", "tokens": ["Vor", "Dei\u00b7ner", "Gra\u00b7best\u00b7h\u00fcr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Heil! mein Gott! la\u00df leben", "tokens": ["Mein", "Heil", "!", "mein", "Gott", "!", "la\u00df", "le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "NN", "$.", "VVIMP", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In Dir mich ewiglich,", "tokens": ["In", "Dir", "mich", "e\u00b7wig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mit Dir hin\u00fcberschweben,", "tokens": ["Mit", "Dir", "hin\u00b7\u00fc\u00b7bersc\u00b7hwe\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "O Herr, nicht lasse mich!", "tokens": ["O", "Herr", ",", "nicht", "las\u00b7se", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PTKNEG", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}