{"textgrid.poem.40806": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbwar einst ein K\u00f6nig, der hielt liebumfangen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwar einst ein K\u00f6nig, der hielt liebumfangen", "tokens": ["\u00bb", "war", "einst", "ein", "K\u00f6\u00b7nig", ",", "der", "hielt", "lieb\u00b7um\u00b7fan\u00b7gen"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "VAFIN", "ADV", "ART", "NN", "$,", "PRELS", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Leib der K\u00f6nigin, der sch\u00f6nen, jungen!", "tokens": ["Den", "Leib", "der", "K\u00f6\u00b7ni\u00b7gin", ",", "der", "sch\u00f6\u00b7nen", ",", "jun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ob Aug' in Aug' und Hand in Hand auch hangen,", "tokens": ["Ob", "Aug'", "in", "Aug'", "und", "Hand", "in", "Hand", "auch", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "KON", "NN", "APPR", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er h\u00e4tte gern noch fester sie umschlungen!", "tokens": ["Er", "h\u00e4t\u00b7te", "gern", "noch", "fes\u00b7ter", "sie", "um\u00b7schlun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Des Gartens Rosen formt er da zur Kette,", "tokens": ["Des", "Gar\u00b7tens", "Ro\u00b7sen", "formt", "er", "da", "zur", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die h\u00e4lt ihr Haupt in s\u00fc\u00dfer Haft umwunden.", "tokens": ["Die", "h\u00e4lt", "ihr", "Haupt", "in", "s\u00fc\u00b7\u00dfer", "Haft", "um\u00b7wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So ward aus Rosen einst die erste Kette,", "tokens": ["So", "ward", "aus", "Ro\u00b7sen", "einst", "die", "ers\u00b7te", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So ward von Liebe einst die Kett' erfunden.", "tokens": ["So", "ward", "von", "Lie\u00b7be", "einst", "die", "Kett'", "er\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zwei K\u00f6nigskinder sind's, die dort zu Ringen", "tokens": ["Zwei", "K\u00f6\u00b7nigs\u00b7kin\u00b7der", "sin\u00b7d's", ",", "die", "dort", "zu", "Rin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VAFIN", "$,", "PRELS", "ADV", "APPR", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Wiesenblumen schlichte Halme runden,", "tokens": ["Der", "Wie\u00b7sen\u00b7blu\u00b7men", "schlich\u00b7te", "Hal\u00b7me", "run\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit solchen Fesseln spielend sich umschlingen;", "tokens": ["Mit", "sol\u00b7chen", "Fes\u00b7seln", "spie\u00b7lend", "sich", "um\u00b7schlin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und so hat Lieb' die Kette fortgewunden.", "tokens": ["Und", "so", "hat", "Lieb'", "die", "Ket\u00b7te", "fort\u00b7ge\u00b7wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Den Tempel sieh', wo Priester um die Wette", "tokens": ["Den", "Tem\u00b7pel", "sieh'", ",", "wo", "Pries\u00b7ter", "um", "die", "Wet\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "PWAV", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Myrth' und Ros' Altar und S\u00e4ul' umwunden!", "tokens": ["Mit", "Myr\u00b7th'", "und", "Ros'", "Al\u00b7tar", "und", "S\u00e4ul'", "um\u00b7wun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "So hat die Liebe fest mit ihrer Kette", "tokens": ["So", "hat", "die", "Lie\u00b7be", "fest", "mit", "ih\u00b7rer", "Ket\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKVZ", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Himmel an die Erde sch\u00f6n gebunden.", "tokens": ["Den", "Him\u00b7mel", "an", "die", "Er\u00b7de", "sch\u00f6n", "ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Todt sind das K\u00f6nigspaar, die Kinder, Priester!", "tokens": ["Todt", "sind", "das", "K\u00f6\u00b7nig\u00b7spaar", ",", "die", "Kin\u00b7der", ",", "Pries\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch Kr\u00e4nze ihren Aschenkrug umkosen!", "tokens": ["Doch", "Kr\u00e4n\u00b7ze", "ih\u00b7ren", "A\u00b7schen\u00b7krug", "um\u00b7ko\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So band den Staub des Grabes, welk und d\u00fcster,", "tokens": ["So", "band", "den", "Staub", "des", "Gra\u00b7bes", ",", "welk", "und", "d\u00fcs\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Liebe Kette an des Lebens Rosen.", "tokens": ["Der", "Lie\u00b7be", "Ket\u00b7te", "an", "des", "Le\u00b7bens", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Da sah der Ha\u00df, wie Lieb' erfand die Kette,", "tokens": ["Da", "sah", "der", "Ha\u00df", ",", "wie", "Lieb'", "er\u00b7fand", "die", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PWAV", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das, was sie liebt, noch fester zu umwinden!", "tokens": ["Das", ",", "was", "sie", "liebt", ",", "noch", "fes\u00b7ter", "zu", "um\u00b7win\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "VVFIN", "$,", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er formt \u2013 aus Erzesbl\u00fcthen \u2013 nach die Kette,", "tokens": ["Er", "formt", "\u2013", "aus", "Er\u00b7zes\u00b7bl\u00fc\u00b7then", "\u2013", "nach", "die", "Ket\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "APPR", "NN", "$(", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch fester, was er ha\u00dft, an sich zu binden!", "tokens": ["Noch", "fes\u00b7ter", ",", "was", "er", "ha\u00dft", ",", "an", "sich", "zu", "bin\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWS", "PPER", "VVFIN", "$,", "APPR", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Doch von Guirlanden scheint mein Arm umwunden,", "tokens": ["Doch", "von", "Gu\u00b7ir\u00b7lan\u00b7den", "scheint", "mein", "Arm", "um\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Gleich Blumen fl\u00fcsternd mir die sch\u00f6ne M\u00e4hre:", "tokens": ["Gleich", "Blu\u00b7men", "fl\u00fcs\u00b7ternd", "mir", "die", "sch\u00f6\u00b7ne", "M\u00e4h\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVPP", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie selbst im Ha\u00df ein F\u00fcnkchen Lieb' entzunden,", "tokens": ["Wie", "selbst", "im", "Ha\u00df", "ein", "F\u00fcnk\u00b7chen", "Lieb'", "ent\u00b7zun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie selbst der Ha\u00df bei Lieb' einst ging in Lehre.\u00ab", "tokens": ["Wie", "selbst", "der", "Ha\u00df", "bei", "Lieb'", "einst", "ging", "in", "Leh\u00b7re", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "APPR", "NN", "ADV", "VVFIN", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}