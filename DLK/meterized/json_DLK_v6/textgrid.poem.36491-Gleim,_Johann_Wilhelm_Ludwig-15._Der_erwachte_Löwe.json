{"textgrid.poem.36491": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "15. Der erwachte L\u00f6we", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der K\u00f6nig L\u00f6w' erwachte. Wolf und B\u00e4r,", "tokens": ["Der", "K\u00f6\u00b7nig", "L\u00f6\u00b7w'", "er\u00b7wach\u00b7te", ".", "Wolf", "und", "B\u00e4r", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "$.", "NE", "KON", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und Luchs und Fuchs erzitterten; denn er", "tokens": ["Und", "Luchs", "und", "Fuchs", "er\u00b7zit\u00b7ter\u00b7ten", ";", "denn", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NE", "KON", "NE", "VVFIN", "$.", "KON", "PPER"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "War ein Tyrann, und schonte seiner Freunde,", "tokens": ["War", "ein", "Ty\u00b7rann", ",", "und", "schon\u00b7te", "sei\u00b7ner", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn er ergrimmte, nicht.", "tokens": ["Wenn", "er", "er\u00b7grimm\u00b7te", ",", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Erwacht, sah' er, mit zornigem Gesicht,", "tokens": ["Er\u00b7wacht", ",", "sah'", "er", ",", "mit", "zor\u00b7ni\u00b7gem", "Ge\u00b7sicht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "All seine m\u00e4chtigen Vasallen. Meine Feinde", "tokens": ["All", "sei\u00b7ne", "m\u00e4ch\u00b7ti\u00b7gen", "Va\u00b7sal\u00b7len", ".", "Mei\u00b7ne", "Fein\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Seid ihr! sprach er; ich habe nicht geschlafen,", "tokens": ["Seid", "ihr", "!", "sprach", "er", ";", "ich", "ha\u00b7be", "nicht", "ge\u00b7schla\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "$.", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Darum, ihr Herrn! mu\u00df ich euch strafen!", "tokens": ["Da\u00b7rum", ",", "ihr", "Herrn", "!", "mu\u00df", "ich", "euch", "stra\u00b7fen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PPOSAT", "NN", "$.", "VMFIN", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und nach und nach fra\u00df er", "tokens": ["Und", "nach", "und", "nach", "fra\u00df", "er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "KON", "APPR", "VVFIN", "PPER"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Den Luchs, den Fuchs, den Wolf, und auch den B\u00e4r,", "tokens": ["Den", "Luchs", ",", "den", "Fuchs", ",", "den", "Wolf", ",", "und", "auch", "den", "B\u00e4r", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "ART", "NE", "$,", "ART", "NE", "$,", "KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der sterbend, wie ein Held, noch Rache raucht.", "tokens": ["Der", "ster\u00b7bend", ",", "wie", "ein", "Held", ",", "noch", "Ra\u00b7che", "raucht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "PWAV", "ART", "NN", "$,", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ihr Menschen, Gott sei Dank, da\u00df seine K\u00f6nigsmacht", "tokens": ["Ihr", "Men\u00b7schen", ",", "Gott", "sei", "Dank", ",", "da\u00df", "sei\u00b7ne", "K\u00f6\u00b7nigs\u00b7macht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "NN", "VAFIN", "NN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein Menschenk\u00f6nig so gebraucht,", "tokens": ["Kein", "Men\u00b7schen\u00b7k\u00f6\u00b7nig", "so", "ge\u00b7braucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und keiner so erwacht!", "tokens": ["Und", "kei\u00b7ner", "so", "er\u00b7wacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}