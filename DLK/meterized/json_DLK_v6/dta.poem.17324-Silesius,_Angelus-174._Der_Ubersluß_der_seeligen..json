{"dta.poem.17324": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "174.  Der Uberslu\u00df der seeligen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Gott schenkt den seeligen so \u00fcberfl\u00fcssig ein/", "tokens": ["Gott", "schenkt", "den", "see\u00b7li\u00b7gen", "so", "\u00fc\u00b7berf\u00b7l\u00fcs\u00b7sig", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "ADV", "ADJD", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sie mehr in dem Trank/ als der in jhnen/ seyn.", "tokens": ["Da\u00df", "sie", "mehr", "in", "dem", "Trank", "/", "als", "der", "in", "jh\u00b7nen", "/", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "$(", "KOUS", "ART", "APPR", "PPER", "$(", "VAINF", "$."], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}}}}}