{"dta.poem.8584": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus grauem Himmel", "tokens": ["Aus", "grau\u00b7em", "Him\u00b7mel"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "sticht die Sonne.", "tokens": ["sticht", "die", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Jagende Wolken, blendendes Blau!", "tokens": ["Ja\u00b7gen\u00b7de", "Wol\u00b7ken", ",", "blen\u00b7den\u00b7des", "Blau", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Ins gr\u00fcne Gras greift der Wind, die Silberweiden str\u00e4uben sich.", "tokens": ["Ins", "gr\u00fc\u00b7ne", "Gras", "greift", "der", "Wind", ",", "die", "Sil\u00b7ber\u00b7wei\u00b7den", "str\u00e4u\u00b7ben", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.4": {"line.1": {"text": "Pl\u00f6tzlich \u2014 still.", "tokens": ["Pl\u00f6tz\u00b7lich", "still", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$(", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Auf einem jungen Erlenbaum", "tokens": ["Auf", "ei\u00b7nem", "jun\u00b7gen", "Er\u00b7len\u00b7baum"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wiegen sich blinkende Tropfen!", "tokens": ["wie\u00b7gen", "sich", "blin\u00b7ken\u00b7de", "Trop\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}