{"dta.poem.21371": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XVI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die D\u00fcbarry lebte lustig und flott,", "tokens": ["Die", "D\u00fc\u00b7bar\u00b7ry", "leb\u00b7te", "lus\u00b7tig", "und", "flott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-++-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "So lange Ludwig regierte,", "tokens": ["So", "lan\u00b7ge", "Lud\u00b7wig", "re\u00b7gier\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NE", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der f\u00fcnfzehnte n\u00e4mlich, sie war schon alt", "tokens": ["Der", "f\u00fcnf\u00b7zehn\u00b7te", "n\u00e4m\u00b7lich", ",", "sie", "war", "schon", "alt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "ADV", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Als man sie guillotinirte.", "tokens": ["Als", "man", "sie", "guil\u00b7lo\u00b7ti\u00b7nir\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Der K\u00f6nig Ludwig der f\u00fcnfzehnte starb", "tokens": ["Der", "K\u00f6\u00b7nig", "Lud\u00b7wig", "der", "f\u00fcnf\u00b7zehn\u00b7te", "starb"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "ART", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ganz ruhig in seinem Bette,", "tokens": ["Ganz", "ru\u00b7hig", "in", "sei\u00b7nem", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der sechszehnte aber ward guillotinirt", "tokens": ["Der", "sechs\u00b7zehn\u00b7te", "a\u00b7ber", "ward", "guil\u00b7lo\u00b7ti\u00b7nirt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "VAFIN", "VVPP"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Mit der K\u00f6nigin Antoinette.", "tokens": ["Mit", "der", "K\u00f6\u00b7ni\u00b7gin", "An\u00b7to\u00b7i\u00b7net\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Die K\u00f6nigin zeigte gro\u00dfen Muth,", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin", "zeig\u00b7te", "gro\u00b7\u00dfen", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ganz wie es sich geb\u00fchrte,", "tokens": ["Ganz", "wie", "es", "sich", "ge\u00b7b\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die D\u00fcbarry aber weinte und schrie", "tokens": ["Die", "D\u00fc\u00b7bar\u00b7ry", "a\u00b7ber", "wein\u00b7te", "und", "schrie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "ADV", "VVFIN", "KON", "VVFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als man sie guillotinirte. \u2013 \u2013", "tokens": ["Als", "man", "sie", "guil\u00b7lo\u00b7ti\u00b7nir\u00b7te", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Kaiser blieb pl\u00f6tzlich stille stehn,", "tokens": ["Der", "Kai\u00b7ser", "blieb", "pl\u00f6tz\u00b7lich", "stil\u00b7le", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ADJA", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und sah mich an mit den stieren", "tokens": ["Und", "sah", "mich", "an", "mit", "den", "stie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "APPR", "ART", "ADJA"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Augen und sprach: \u201eUm Gotteswill\u2019n,", "tokens": ["Au\u00b7gen", "und", "sprach", ":", "\u201e", "Um", "Got\u00b7tes\u00b7will'n", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$.", "$(", "KOUI", "NE", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Was ist das, guillotiniren?\u201c", "tokens": ["Was", "ist", "das", ",", "guil\u00b7lo\u00b7ti\u00b7ni\u00b7ren", "?", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "PDS", "$,", "VVINF", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}