{"textgrid.poem.29714": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "129.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df die Wercke selig machen, ist zwar nirgend zu verf\u00fchren;", "tokens": ["Da\u00df", "die", "Wer\u00b7cke", "se\u00b7lig", "ma\u00b7chen", ",", "ist", "zwar", "nir\u00b7gend", "zu", "ver\u00b7f\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVINF", "$,", "VAFIN", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Da\u00df hingegen sie dem Glauben, ist gar klar, mit Recht geb\u00fchren;", "tokens": ["Da\u00df", "hin\u00b7ge\u00b7gen", "sie", "dem", "Glau\u00b7ben", ",", "ist", "gar", "klar", ",", "mit", "Recht", "ge\u00b7b\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ART", "NN", "$,", "VAFIN", "ADV", "ADJD", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Wer wil da viel Glauben glauben, wo sich keine Wercke r\u00fchren?", "tokens": ["Wer", "wil", "da", "viel", "Glau\u00b7ben", "glau\u00b7ben", ",", "wo", "sich", "kei\u00b7ne", "Wer\u00b7cke", "r\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "PIAT", "NN", "VVINF", "$,", "PWAV", "PRF", "PIAT", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}}}}}