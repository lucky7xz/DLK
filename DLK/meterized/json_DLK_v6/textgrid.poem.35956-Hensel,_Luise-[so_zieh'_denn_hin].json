{"textgrid.poem.35956": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "[so zieh' denn hin]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So zieh' denn hin! Dir folgt der Freunde Flehn,", "tokens": ["So", "zieh'", "denn", "hin", "!", "Dir", "folgt", "der", "Freun\u00b7de", "Flehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "$.", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dir schwinde froh der leichten Horen Tanz!", "tokens": ["Dir", "schwin\u00b7de", "froh", "der", "leich\u00b7ten", "Ho\u00b7ren", "Tanz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Einst eint auch uns gewi\u00df ein Wiedersehn,", "tokens": ["Einst", "eint", "auch", "uns", "ge\u00b7wi\u00df", "ein", "Wie\u00b7der\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Wiedersehn \u2013 o Freund \u2013 im Sternenkranz.", "tokens": ["Ein", "Wie\u00b7der\u00b7sehn", "\u2013", "o", "Freund", "\u2013", "im", "Ster\u00b7nen\u00b7kranz", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "FM", "NN", "$(", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dort wo der Schmerz aus Lethe's Fluthen trinkt,", "tokens": ["Dort", "wo", "der", "Schmerz", "aus", "Lethe's", "Flut\u00b7hen", "trinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NN", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Wo Seligkeit des Dulders Haupt umkr\u00e4nzt", "tokens": ["Wo", "Se\u00b7lig\u00b7keit", "des", "Dul\u00b7ders", "Haupt", "um\u00b7kr\u00e4nzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und jede Perl', die hier dem Aug' entsinkt,", "tokens": ["Und", "je\u00b7de", "Perl'", ",", "die", "hier", "dem", "Aug'", "ent\u00b7sinkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als Demant in der Siegerkrone gl\u00e4nzt;", "tokens": ["Als", "De\u00b7mant", "in", "der", "Sie\u00b7ger\u00b7kro\u00b7ne", "gl\u00e4nzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Heil dir! Dort harrt ein Wiedersehn auch uns.", "tokens": ["Heil", "dir", "!", "Dort", "harrt", "ein", "Wie\u00b7der\u00b7sehn", "auch", "uns", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "ADV", "VVFIN", "ART", "NN", "ADV", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}