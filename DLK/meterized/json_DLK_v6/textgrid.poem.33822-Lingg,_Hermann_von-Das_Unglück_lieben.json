{"textgrid.poem.33822": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Das Ungl\u00fcck lieben", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Ungl\u00fcck lieben \u2013 o das hei\u00dft,", "tokens": ["Das", "Un\u00b7gl\u00fcck", "lie\u00b7ben", "\u2013", "o", "das", "hei\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$(", "FM", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Dorngestr\u00fcppe, das uns blutig,", "tokens": ["Durch", "Dorn\u00b7ge\u00b7str\u00fcp\u00b7pe", ",", "das", "uns", "blu\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das uns das Kleid vom Leibe rei\u00dft,", "tokens": ["Das", "uns", "das", "Kleid", "vom", "Lei\u00b7be", "rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Dunkel gehn, am Abgrund mutig;", "tokens": ["Im", "Dun\u00b7kel", "gehn", ",", "am", "Ab\u00b7grund", "mu\u00b7tig", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es hei\u00dft nicht gehn im Sonnenschein,", "tokens": ["Es", "hei\u00dft", "nicht", "gehn", "im", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "VVINF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Jedoch auch leiden nicht allein.", "tokens": ["Je\u00b7doch", "auch", "lei\u00b7den", "nicht", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Ungl\u00fcck lieben hei\u00dft, zugleich", "tokens": ["Das", "Un\u00b7gl\u00fcck", "lie\u00b7ben", "hei\u00dft", ",", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verachtung, Spott und ohne Klagen,", "tokens": ["Ver\u00b7ach\u00b7tung", ",", "Spott", "und", "oh\u00b7ne", "Kla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gefa\u00dft auf jeden Wetterstreich,", "tokens": ["Ge\u00b7fa\u00dft", "auf", "je\u00b7den", "Wet\u00b7ter\u00b7streich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Erde Doppellast ertragen,", "tokens": ["Der", "Er\u00b7de", "Dop\u00b7pel\u00b7last", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dem s\u00fc\u00dfen vorziehn bittern Trank", "tokens": ["Dem", "s\u00fc\u00b7\u00dfen", "vor\u00b7ziehn", "bit\u00b7tern", "Trank"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVINF", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und ernten, ach, nur kargen Dank.", "tokens": ["Und", "ern\u00b7ten", ",", "ach", ",", "nur", "kar\u00b7gen", "Dank", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ITJ", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das Ungl\u00fcck lieben hei\u00dft, ein Kind", "tokens": ["Das", "Un\u00b7gl\u00fcck", "lie\u00b7ben", "hei\u00dft", ",", "ein", "Kind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit heim von \u00f6der Stra\u00dfe nehmen,", "tokens": ["Mit", "heim", "von", "\u00f6\u00b7der", "Stra\u00b7\u00dfe", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Besch\u00fctzen vor dem rauhen Wind,", "tokens": ["Be\u00b7sch\u00fct\u00b7zen", "vor", "dem", "rau\u00b7hen", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hei\u00dft, harten Sinn und Stolz besch\u00e4men,", "tokens": ["Hei\u00dft", ",", "har\u00b7ten", "Sinn", "und", "Stolz", "be\u00b7sch\u00e4\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Selbst nicht vor Trotz und Widerstand", "tokens": ["Selbst", "nicht", "vor", "Trotz", "und", "Wi\u00b7der\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zur\u00fcckziehn seine Retterhand.", "tokens": ["Zu\u00b7r\u00fcck\u00b7ziehn", "sei\u00b7ne", "Ret\u00b7ter\u00b7hand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Das Ungl\u00fcck lieben hei\u00dft, nicht Flaum", "tokens": ["Das", "Un\u00b7gl\u00fcck", "lie\u00b7ben", "hei\u00dft", ",", "nicht", "Flaum"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$,", "PTKNEG", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und weiche Polsterdecken lieben,", "tokens": ["Und", "wei\u00b7che", "Pols\u00b7ter\u00b7de\u00b7cken", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch die, die umgehn wie im Traum,", "tokens": ["Doch", "die", ",", "die", "um\u00b7gehn", "wie", "im", "Traum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "PRELS", "ADJD", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die \u00c4rmsten, die zur\u00fcckgeblieben,", "tokens": ["Die", "\u00c4rms\u00b7ten", ",", "die", "zu\u00b7r\u00fcck\u00b7ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Errettend wiederum hervor", "tokens": ["Er\u00b7ret\u00b7tend", "wie\u00b7de\u00b7rum", "her\u00b7vor"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Geleiten, zu dem Gl\u00fcck empor.", "tokens": ["Ge\u00b7lei\u00b7ten", ",", "zu", "dem", "Gl\u00fcck", "em\u00b7por", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Das Ungl\u00fcck lieben hei\u00dft, die Not", "tokens": ["Das", "Un\u00b7gl\u00fcck", "lie\u00b7ben", "hei\u00dft", ",", "die", "Not"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Erdendaseins ganz empfinden,", "tokens": ["Des", "Er\u00b7den\u00b7da\u00b7seins", "ganz", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Ohnmacht vor dem Machtgebot,", "tokens": ["Die", "Ohn\u00b7macht", "vor", "dem", "Macht\u00b7ge\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Dem kein Gesch\u00f6pf sich kann entwinden,", "tokens": ["Dem", "kein", "Ge\u00b7sch\u00f6pf", "sich", "kann", "ent\u00b7win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PRF", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hei\u00dft streifen an des Engels Flug,", "tokens": ["Hei\u00dft", "strei\u00b7fen", "an", "des", "En\u00b7gels", "Flug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der auf die Welt das Mitleid trug.", "tokens": ["Der", "auf", "die", "Welt", "das", "Mit\u00b7leid", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}