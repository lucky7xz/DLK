{"textgrid.poem.37331": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Scheu und treu", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er liebte sie in aller Stille.", "tokens": ["Er", "lieb\u00b7te", "sie", "in", "al\u00b7ler", "Stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bescheiden, sch\u00fcchtern und von fern", "tokens": ["Be\u00b7schei\u00b7den", ",", "sch\u00fcch\u00b7tern", "und", "von", "fern"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "KON", "APPR", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schielt er nach ihr durch seine Brille", "tokens": ["Schielt", "er", "nach", "ihr", "durch", "sei\u00b7ne", "Bril\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und hat sie doch so schrecklich gern.", "tokens": ["Und", "hat", "sie", "doch", "so", "schreck\u00b7lich", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein M\u00fccklein, welches an der Nase", "tokens": ["Ein", "M\u00fcck\u00b7lein", ",", "wel\u00b7ches", "an", "der", "Na\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des sch\u00f6nen Kindes saugend sa\u00df,", "tokens": ["Des", "sch\u00f6\u00b7nen", "Kin\u00b7des", "sau\u00b7gend", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ertr\u00e4nkte sich in seinem Glase.", "tokens": ["Er\u00b7tr\u00e4nk\u00b7te", "sich", "in", "sei\u00b7nem", "Gla\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es schmeckt ihm fast wie Ananas.", "tokens": ["Es", "schmeckt", "ihm", "fast", "wie", "A\u00b7na\u00b7nas", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KOKOM", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie hatte Haare wie 'ne Puppe,", "tokens": ["Sie", "hat\u00b7te", "Haa\u00b7re", "wie", "'ne", "Pup\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So unvergleichlich blond und kraus.", "tokens": ["So", "un\u00b7ver\u00b7gleich\u00b7lich", "blond", "und", "kraus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Einst fand er eines in der Suppe", "tokens": ["Einst", "fand", "er", "ei\u00b7nes", "in", "der", "Sup\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und zog es hochbegl\u00fcckt heraus.", "tokens": ["Und", "zog", "es", "hoch\u00b7be\u00b7gl\u00fcckt", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er rollt es auf zu einem L\u00f6ckchen,", "tokens": ["Er", "rollt", "es", "auf", "zu", "ei\u00b7nem", "L\u00f6ck\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hat's in ein Medaillon gelegt.", "tokens": ["Hat's", "in", "ein", "Me\u00b7dail\u00b7lon", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Nun h\u00e4ngt es unter seinem R\u00f6ckchen", "tokens": ["Nun", "h\u00e4ngt", "es", "un\u00b7ter", "sei\u00b7nem", "R\u00f6ck\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da, wo sein treues Herze schl\u00e4gt.", "tokens": ["Da", ",", "wo", "sein", "treu\u00b7es", "Her\u00b7ze", "schl\u00e4gt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}