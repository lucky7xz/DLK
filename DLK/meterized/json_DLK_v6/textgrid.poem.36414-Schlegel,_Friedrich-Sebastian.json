{"textgrid.poem.36414": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Sebastian", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nebel zerrei\u00dfen, in L\u00fcften und Fluren", "tokens": ["Die", "Ne\u00b7bel", "zer\u00b7rei\u00b7\u00dfen", ",", "in", "L\u00fcf\u00b7ten", "und", "Flu\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Verk\u00fcndet ein Chaos von T\u00f6nen die Wonne,", "tokens": ["Ver\u00b7k\u00fcn\u00b7det", "ein", "Chaos", "von", "T\u00f6\u00b7nen", "die", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und Blumen begr\u00fc\u00dfen die Augen der Sonne,", "tokens": ["Und", "Blu\u00b7men", "be\u00b7gr\u00fc\u00b7\u00dfen", "die", "Au\u00b7gen", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wenn kindlich sich putzen die frohen Naturen.", "tokens": ["Wenn", "kind\u00b7lich", "sich", "put\u00b7zen", "die", "fro\u00b7hen", "Na\u00b7tu\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PRF", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Es zeigen sich fern schon flammende Spuren,", "tokens": ["Es", "zei\u00b7gen", "sich", "fern", "schon", "flam\u00b7men\u00b7de", "Spu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Da\u00df stille Gewitter die Erde umschlossen.", "tokens": ["Da\u00df", "stil\u00b7le", "Ge\u00b7wit\u00b7ter", "die", "Er\u00b7de", "um\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Wenn Tr\u00e4nen der Wollust die Wolken vergossen,", "tokens": ["Wenn", "Tr\u00e4\u00b7nen", "der", "Wol\u00b7lust", "die", "Wol\u00b7ken", "ver\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Dann gl\u00e4nzen noch reiner die lichten Azuren:", "tokens": ["Dann", "gl\u00e4n\u00b7zen", "noch", "rei\u00b7ner", "die", "lich\u00b7ten", "A\u00b7zu\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJA", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "So f\u00fcllt uns das Schauspiel auf ewig die Sinne", "tokens": ["So", "f\u00fcllt", "uns", "das", "Schau\u00b7spiel", "auf", "e\u00b7wig", "die", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJD", "ART", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Im freudigen Wechsel der bl\u00fchenden Erde", "tokens": ["Im", "freu\u00b7di\u00b7gen", "Wech\u00b7sel", "der", "bl\u00fc\u00b7hen\u00b7den", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und locket uns freundlich mit sch\u00f6ner Geb\u00e4rde,", "tokens": ["Und", "lo\u00b7cket", "uns", "freund\u00b7lich", "mit", "sch\u00f6\u00b7ner", "Ge\u00b7b\u00e4r\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Das Herz zu ergeben der ewigen Minne.", "tokens": ["Das", "Herz", "zu", "er\u00b7ge\u00b7ben", "der", "e\u00b7wi\u00b7gen", "Min\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Nun wei\u00df, wo er Mut zur Freude gewinne,", "tokens": ["Nun", "wei\u00df", ",", "wo", "er", "Mut", "zur", "Freu\u00b7de", "ge\u00b7win\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "PPER", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der heilige Mensch in den heiligen T\u00e4nzen,", "tokens": ["Der", "hei\u00b7li\u00b7ge", "Mensch", "in", "den", "hei\u00b7li\u00b7gen", "T\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Wo Leben und Liebe sich selig umkr\u00e4nzen,", "tokens": ["Wo", "Le\u00b7ben", "und", "Lie\u00b7be", "sich", "se\u00b7lig", "um\u00b7kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Da\u00df selig das Herz ihm in Rhytmus zerrinne.", "tokens": ["Da\u00df", "se\u00b7lig", "das", "Herz", "ihm", "in", "Rhyt\u00b7mus", "zer\u00b7rin\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "PPER", "APPR", "NE", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}