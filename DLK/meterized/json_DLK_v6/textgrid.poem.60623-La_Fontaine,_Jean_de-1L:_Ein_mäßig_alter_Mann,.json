{"textgrid.poem.60623": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein m\u00e4\u00dfig alter Mann,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein m\u00e4\u00dfig alter Mann,", "tokens": ["Ein", "m\u00e4\u00b7\u00dfig", "al\u00b7ter", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der schon ein Graukopf war.", "tokens": ["Der", "schon", "ein", "Grau\u00b7kopf", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gedachte, eh die Zeit entrann,", "tokens": ["Ge\u00b7dach\u00b7te", ",", "eh", "die", "Zeit", "ent\u00b7rann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu freien. Da er Kr\u00f6sus war,", "tokens": ["Zu", "frei\u00b7en", ".", "Da", "er", "Kr\u00f6\u00b7sus", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$.", "KOUS", "PPER", "NE", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So bot sich ihm die Auswahl dar;", "tokens": ["So", "bot", "sich", "ihm", "die", "Aus\u00b7wahl", "dar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er konnte unter allen", "tokens": ["Er", "konn\u00b7te", "un\u00b7ter", "al\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sich umsehn nach Gefallen.", "tokens": ["Sich", "um\u00b7sehn", "nach", "Ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "APPR", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Doch will er sich nicht \u00fcbereilen,", "tokens": ["Doch", "will", "er", "sich", "nicht", "\u00fc\u00b7be\u00b7rei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Denn gut zu w\u00e4hlen, ist nicht leicht.", "tokens": ["Denn", "gut", "zu", "w\u00e4h\u00b7len", ",", "ist", "nicht", "leicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKZU", "VVINF", "$,", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Zwei Witwen fesseln ihn zu gleichen Teilen,", "tokens": ["Zwei", "Wit\u00b7wen", "fes\u00b7seln", "ihn", "zu", "glei\u00b7chen", "Tei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "So liebt er beide denn einstweilen", "tokens": ["So", "liebt", "er", "bei\u00b7de", "denn", "einst\u00b7wei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und wartet, welche wohl der andern weicht.", "tokens": ["Und", "war\u00b7tet", ",", "wel\u00b7che", "wohl", "der", "an\u00b7dern", "weicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "ADV", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Die eine war noch gr\u00fcn;", "tokens": ["Die", "ei\u00b7ne", "war", "noch", "gr\u00fcn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Die andre hatte ihre Reife l\u00e4ngst erreicht,", "tokens": ["Die", "and\u00b7re", "hat\u00b7te", "ih\u00b7re", "Rei\u00b7fe", "l\u00e4ngst", "er\u00b7reicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Doch wu\u00dfte sie geschickt sich zu bem\u00fchn,", "tokens": ["Doch", "wu\u00df\u00b7te", "sie", "ge\u00b7schickt", "sich", "zu", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVPP", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Das aufzufrischen, was das Alter schon gebleicht.", "tokens": ["Das", "auf\u00b7zu\u00b7fri\u00b7schen", ",", "was", "das", "Al\u00b7ter", "schon", "ge\u00b7bleicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVIZU", "$,", "PRELS", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die beiden schneiden unserm Mann die Cour,", "tokens": ["Die", "bei\u00b7den", "schnei\u00b7den", "un\u00b7serm", "Mann", "die", "Cour", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Sie plaudern, lachen, schmeicheln", "tokens": ["Sie", "plau\u00b7dern", ",", "la\u00b7chen", ",", "schmei\u00b7cheln"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "VVINF", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Und lieben ihm den Kopf zu streicheln,", "tokens": ["Und", "lie\u00b7ben", "ihm", "den", "Kopf", "zu", "strei\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Als ordneten sie die Frisur.", "tokens": ["Als", "ord\u00b7ne\u00b7ten", "sie", "die", "Fri\u00b7sur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Doch wenn der ", "tokens": ["Doch", "wenn", "der"], "token_info": ["word", "word", "word"], "pos": ["KON", "KOUS", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.22": {"text": "Ihm durch die Haare fuhr,", "tokens": ["Ihm", "durch", "die", "Haa\u00b7re", "fuhr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "So trachtete sie nur,", "tokens": ["So", "trach\u00b7te\u00b7te", "sie", "nur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.25": {"text": "Ihm heimlich auszuziehn,", "tokens": ["Ihm", "heim\u00b7lich", "aus\u00b7zu\u00b7ziehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.26": {"text": "Damit der Ehe-Spekulant", "tokens": ["Da\u00b7mit", "der", "E\u00b7he\u00b7Spe\u00b7ku\u00b7lant"], "token_info": ["word", "word", "word"], "pos": ["PAV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Besser zu ihr zu passen schien.", "tokens": ["Bes\u00b7ser", "zu", "ihr", "zu", "pas\u00b7sen", "schien", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.28": {"text": "Die J", "tokens": ["Die", "J"], "token_info": ["word", "word"], "pos": ["ART", "NE"], "meter": "-+", "measure": "iambic.single"}, "line.29": {"text": "So eifrig trieb's das Witwenpaar,", "tokens": ["So", "eif\u00b7rig", "trie\u00b7b's", "das", "Wit\u00b7wen\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Da\u00df Graukopf bald ein Kahlkopf war.", "tokens": ["Da\u00df", "Grau\u00b7kopf", "bald", "ein", "Kahl\u00b7kopf", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Und nun erkannte er den Streich.", "tokens": ["Und", "nun", "er\u00b7kann\u00b7te", "er", "den", "Streich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Da sagte er: \u00bbIch danke euch,", "tokens": ["Da", "sag\u00b7te", "er", ":", "\u00bb", "Ich", "dan\u00b7ke", "euch", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Ihr Sch\u00f6nen, die ihr mich so gut geschoren.", "tokens": ["Ihr", "Sch\u00f6\u00b7nen", ",", "die", "ihr", "mich", "so", "gut", "ge\u00b7scho\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "PRF", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Ich habe mehr gewonnen als verloren:", "tokens": ["Ich", "ha\u00b7be", "mehr", "ge\u00b7won\u00b7nen", "als", "ver\u00b7lo\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "KOKOM", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Ich denke nicht mehr an die liebe Ehe,", "tokens": ["Ich", "den\u00b7ke", "nicht", "mehr", "an", "die", "lie\u00b7be", "E\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Denn die ich n\u00e4hme, s\u00e4nne, wie ich sehe,", "tokens": ["Denn", "die", "ich", "n\u00e4h\u00b7me", ",", "s\u00e4n\u00b7ne", ",", "wie", "ich", "se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Nach ihrer, nicht nach meiner Art zu leben;", "tokens": ["Nach", "ih\u00b7rer", ",", "nicht", "nach", "mei\u00b7ner", "Art", "zu", "le\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "$,", "PTKNEG", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Und daran mag ein andrer sich gew\u00f6hnen!", "tokens": ["Und", "da\u00b7ran", "mag", "ein", "an\u00b7drer", "sich", "ge\u00b7w\u00f6h\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VMFIN", "ART", "ADJA", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Ich bin euch sehr verpflichtet, meine Sch\u00f6nen,", "tokens": ["Ich", "bin", "euch", "sehr", "ver\u00b7pflich\u00b7tet", ",", "mei\u00b7ne", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "F\u00fcr diese Warnung, die ihr selbst gegeben!\u00ab", "tokens": ["F\u00fcr", "die\u00b7se", "War\u00b7nung", ",", "die", "ihr", "selbst", "ge\u00b7ge\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}