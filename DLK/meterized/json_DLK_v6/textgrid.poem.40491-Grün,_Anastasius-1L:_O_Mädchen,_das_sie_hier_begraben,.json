{"textgrid.poem.40491": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: O M\u00e4dchen, das sie hier begraben,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O M\u00e4dchen, das sie hier begraben,", "tokens": ["O", "M\u00e4d\u00b7chen", ",", "das", "sie", "hier", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Halb Jungfrau schon und noch halb Kind,", "tokens": ["Halb", "Jung\u00b7frau", "schon", "und", "noch", "halb", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ADV", "KON", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Einst konnte mich dein Anblick laben,", "tokens": ["Einst", "konn\u00b7te", "mich", "dein", "An\u00b7blick", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie eine Fr\u00fchlingslandschaft lind.", "tokens": ["Wie", "ei\u00b7ne", "Fr\u00fch\u00b7lings\u00b7land\u00b7schaft", "lind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vorsprudelnd, wie der Bergquell, flogen", "tokens": ["Vor\u00b7spru\u00b7delnd", ",", "wie", "der", "Berg\u00b7quell", ",", "flo\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVPP", "$,", "PWAV", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Einst in die Welt die Worte dein,", "tokens": ["Einst", "in", "die", "Welt", "die", "Wor\u00b7te", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Demanten st\u00e4ubend, Regenbogen!", "tokens": ["De\u00b7man\u00b7ten", "st\u00e4u\u00b7bend", ",", "Re\u00b7gen\u00b7bo\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "VVPP", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und doch so hell, gesund und rein!", "tokens": ["Und", "doch", "so", "hell", ",", "ge\u00b7sund", "und", "rein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie Rehlein wagten deine Blicke", "tokens": ["Wie", "Reh\u00b7lein", "wag\u00b7ten", "dei\u00b7ne", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Heran neugierig, arglos sich;", "tokens": ["He\u00b7ran", "neu\u00b7gie\u00b7rig", ",", "arg\u00b7los", "sich", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "ADJD", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Scheu flohn, wie jene, sie zur\u00fccke,", "tokens": ["Scheu", "flohn", ",", "wie", "je\u00b7ne", ",", "sie", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PWAV", "PDS", "$,", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn nur von fern ein Laurer schlich.", "tokens": ["Wenn", "nur", "von", "fern", "ein", "Lau\u00b7rer", "schlich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ADJD", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dir spielten, wogten die Gef\u00fchle,", "tokens": ["Dir", "spiel\u00b7ten", ",", "wog\u00b7ten", "die", "Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie junge Saat, so leichtbewegt,", "tokens": ["Wie", "jun\u00b7ge", "Saat", ",", "so", "leicht\u00b7be\u00b7wegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die in sich schon der Keime viele", "tokens": ["Die", "in", "sich", "schon", "der", "Kei\u00b7me", "vie\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PRF", "ADV", "ART", "NN", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu Bl\u00fcth' und edlem Kerne tr\u00e4gt.", "tokens": ["Zu", "Bl\u00fcth'", "und", "ed\u00b7lem", "Ker\u00b7ne", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Umflog ein jungfr\u00e4ulich Err\u00f6then", "tokens": ["Um\u00b7flog", "ein", "jung\u00b7fr\u00e4u\u00b7lich", "Er\u00b7r\u00f6\u00b7then"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJD", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Dir leis dein lieblich Angesicht,", "tokens": ["Dir", "leis", "dein", "lieb\u00b7lich", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Fr\u00fchroth war's auf Blumenbeeten,", "tokens": ["Wie", "Fr\u00fch\u00b7roth", "wa\u00b7r's", "auf", "Blu\u00b7men\u00b7bee\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das einen sonn'gen Tag verspricht.", "tokens": ["Das", "ei\u00b7nen", "sonn'\u00b7gen", "Tag", "ver\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und jauchztest du des Frohsinns Kl\u00e4nge,", "tokens": ["Und", "jauchz\u00b7test", "du", "des", "Froh\u00b7sinns", "Kl\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "War mir's, als h\u00f6rt' ich \u00fcber mir", "tokens": ["War", "mir's", ",", "als", "h\u00f6rt'", "ich", "\u00fc\u00b7ber", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NE", "$,", "KOUS", "VVFIN", "PPER", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Heimzieh'nder Wanderv\u00f6gel S\u00e4nge", "tokens": ["Heim\u00b7zieh'n\u00b7der", "Wan\u00b7der\u00b7v\u00f6\u00b7gel", "S\u00e4n\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Von S\u00fcdens sch\u00f6nem Lenzrevier.", "tokens": ["Von", "S\u00fc\u00b7dens", "sch\u00f6\u00b7nem", "Lenz\u00b7re\u00b7vier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und lie\u00dfest Liebeswort' du gleiten", "tokens": ["Und", "lie\u00b7\u00dfest", "Lie\u00b7bes\u00b7wort'", "du", "glei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu deinem greisen Vater, lag", "tokens": ["Zu", "dei\u00b7nem", "grei\u00b7sen", "Va\u00b7ter", ",", "lag"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Ohre mir's wie Glockenl\u00e4uten", "tokens": ["Im", "Oh\u00b7re", "mir's", "wie", "Glo\u00b7cken\u00b7l\u00e4u\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "NE", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "An einem sch\u00f6nen Gottestag.", "tokens": ["An", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Got\u00b7tes\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Gedenk' ich dein, seh' ich noch immer", "tokens": ["Ge\u00b7denk'", "ich", "dein", ",", "seh'", "ich", "noch", "im\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PPOSAT", "$,", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In eine Fr\u00fchlingslandschaft mild,", "tokens": ["In", "ei\u00b7ne", "Fr\u00fch\u00b7lings\u00b7land\u00b7schaft", "mild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Darauf der Abendr\u00f6the Schimmer", "tokens": ["Da\u00b7rauf", "der", "A\u00b7bend\u00b7r\u00f6\u00b7the", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Scheidegru\u00dfe sanft verquillt.", "tokens": ["Im", "Schei\u00b7de\u00b7gru\u00b7\u00dfe", "sanft", "ver\u00b7quillt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Dar\u00fcber Abendglockent\u00f6ne,", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "A\u00b7bend\u00b7glo\u00b7cken\u00b7t\u00f6\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["PAV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df mir's von Sternenn\u00e4chten ahnt;", "tokens": ["Da\u00df", "mir's", "von", "Ster\u00b7nen\u00b7n\u00e4ch\u00b7ten", "ahnt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dar\u00fcber segelnd gold'ne Schw\u00e4ne", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "se\u00b7gelnd", "gold'\u00b7ne", "Schw\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach einem fernen S\u00fcdenland.", "tokens": ["Nach", "ei\u00b7nem", "fer\u00b7nen", "S\u00fc\u00b7den\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}