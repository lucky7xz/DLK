{"dta.poem.19216": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "63.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.85", "cy:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den Maulwurf nennst du blind, weil er, wenn du ans Licht", "tokens": ["Den", "Maul\u00b7wurf", "nennst", "du", "blind", ",", "weil", "er", ",", "wenn", "du", "ans", "Licht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Ihn ziehst, geblendet scheint, blind aber ist er nicht.", "tokens": ["Ihn", "ziehst", ",", "ge\u00b7blen\u00b7det", "scheint", ",", "blind", "a\u00b7ber", "ist", "er", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVPP", "VVFIN", "$,", "ADJD", "ADV", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Vielmehr es ist so fein sein Auge, da\u00df es f\u00fchlet", "tokens": ["Viel\u00b7mehr", "es", "ist", "so", "fein", "sein", "Au\u00b7ge", ",", "da\u00df", "es", "f\u00fch\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "ADJD", "PPOSAT", "NN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Licht im dunkeln Grund, wo er die G\u00e4nge w\u00fchlet.", "tokens": ["Das", "Licht", "im", "dun\u00b7keln", "Grund", ",", "wo", "er", "die", "G\u00e4n\u00b7ge", "w\u00fch\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Drum, grabend, gr\u00e4bt er stets, die Sonn' im R\u00fccken habend,", "tokens": ["Drum", ",", "gra\u00b7bend", ",", "gr\u00e4bt", "er", "stets", ",", "die", "Sonn'", "im", "R\u00fc\u00b7cken", "ha\u00b7bend", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "ADJD", "$,", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am Morgen gegen West, und gegen Ost am Abend;", "tokens": ["Am", "Mor\u00b7gen", "ge\u00b7gen", "West", ",", "und", "ge\u00b7gen", "Ost", "am", "A\u00b7bend", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "$,", "KON", "APPR", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Sonne, die er doch nicht siehet, abgewendet,", "tokens": ["Der", "Son\u00b7ne", ",", "die", "er", "doch", "nicht", "sie\u00b7het", ",", "ab\u00b7ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "PTKNEG", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Damit nicht in der Nacht ihr scharfer Glanz ihn blendet.", "tokens": ["Da\u00b7mit", "nicht", "in", "der", "Nacht", "ihr", "schar\u00b7fer", "Glanz", "ihn", "blen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKNEG", "APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Mein Sohn, oft ist von Unempfindlichkeit der Schein", "tokens": ["Mein", "Sohn", ",", "oft", "ist", "von", "Un\u00b7emp\u00b7find\u00b7lich\u00b7keit", "der", "Schein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VAFIN", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur eine \u00e4u\u00dferste Empfindlichkeit allein.", "tokens": ["Nur", "ei\u00b7ne", "\u00e4u\u00b7\u00dfers\u00b7te", "Emp\u00b7find\u00b7lich\u00b7keit", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}