{"textgrid.poem.48390": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "4. Der 51. Psalm", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du G\u00fct' und Gnade selbst, Gott, sei mir S\u00fcndern gn\u00e4dig,", "tokens": ["Du", "G\u00fct'", "und", "Gna\u00b7de", "selbst", ",", "Gott", ",", "sei", "mir", "S\u00fcn\u00b7dern", "gn\u00e4\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "ADV", "$,", "NN", "$,", "VAFIN", "PPER", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und sprich mich meiner Schuld in Hulden quit und ledig!", "tokens": ["und", "sprich", "mich", "mei\u00b7ner", "Schuld", "in", "Hul\u00b7den", "quit", "und", "le\u00b7dig", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "NE", "NE", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Herr, wasche du mich wol von meiner Missetat!", "tokens": ["Herr", ",", "wa\u00b7sche", "du", "mich", "wol", "von", "mei\u00b7ner", "Mis\u00b7se\u00b7tat", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "PRF", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "du hast f\u00fcr meinen Kot bei dir das rechte Bad.", "tokens": ["du", "hast", "f\u00fcr", "mei\u00b7nen", "Kot", "bei", "dir", "das", "rech\u00b7te", "Bad", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "APPR", "PPER", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich sehe f\u00fcr und f\u00fcr vor mir mein Unrecht schweben,", "tokens": ["Ich", "se\u00b7he", "f\u00fcr", "und", "f\u00fcr", "vor", "mir", "mein", "Un\u00b7recht", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "KON", "APPR", "APPR", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "ich wil dir dessen nur ein klar Bekentn\u00fc\u00df geben", "tokens": ["ich", "wil", "dir", "des\u00b7sen", "nur", "ein", "klar", "Be\u00b7kent\u00b7n\u00fc\u00df", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PDS", "ADV", "ART", "ADJD", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "und sagen frei heraus, da\u00df ich dein S\u00fcnder bin.", "tokens": ["und", "sa\u00b7gen", "frei", "he\u00b7raus", ",", "da\u00df", "ich", "dein", "S\u00fcn\u00b7der", "bin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Denn dieser Ausspruch sieht auf deine Gottheit hin,", "tokens": ["Denn", "die\u00b7ser", "Aus\u00b7spruch", "sieht", "auf", "dei\u00b7ne", "Got\u00b7theit", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "da\u00df du bleibst ewig wahr und allzeit rein zu finden.", "tokens": ["da\u00df", "du", "bleibst", "e\u00b7wig", "wahr", "und", "all\u00b7zeit", "rein", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJD", "ADJD", "KON", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich kan es leugnen nicht, ich bin ein Mensch, in S\u00fcnden", "tokens": ["Ich", "kan", "es", "leug\u00b7nen", "nicht", ",", "ich", "bin", "ein", "Mensch", ",", "in", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VAFIN", "ART", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "empfangen und geborn. Der Eltern schn\u00f6de Lust", "tokens": ["emp\u00b7fan\u00b7gen", "und", "ge\u00b7born", ".", "Der", "El\u00b7tern", "schn\u00f6\u00b7de", "Lust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "NN", "$.", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "hat mir auch angekleckt den b\u00f6sen Kot und Wust.", "tokens": ["hat", "mir", "auch", "an\u00b7ge\u00b7kleckt", "den", "b\u00f6\u00b7sen", "Kot", "und", "Wust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dir aber, Herr, gef\u00e4llt die Wahrheit, die verborgen", "tokens": ["Dir", "a\u00b7ber", ",", "Herr", ",", "ge\u00b7f\u00e4llt", "die", "Wahr\u00b7heit", ",", "die", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "$,", "NN", "$,", "VVFIN", "ART", "NN", "$,", "PRELS", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "in deinem Herzen liegt. Du kanst, Herr, f\u00fcr mich sorgen.", "tokens": ["in", "dei\u00b7nem", "Her\u00b7zen", "liegt", ".", "Du", "kanst", ",", "Herr", ",", "f\u00fcr", "mich", "sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$.", "PPER", "VMFIN", "$,", "NN", "$,", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Du zeigest mir den Weg, der zu der Weisheit f\u00fchrt,", "tokens": ["Du", "zei\u00b7gest", "mir", "den", "Weg", ",", "der", "zu", "der", "Weis\u00b7heit", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "der auch sonst heimlich ist, den nie kein Heide sp\u00fcrt.", "tokens": ["der", "auch", "sonst", "heim\u00b7lich", "ist", ",", "den", "nie", "kein", "Hei\u00b7de", "sp\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "VAFIN", "$,", "PRELS", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Nimb einen Ysoppusch, ents\u00fcndige mein Leben!", "tokens": ["Nimb", "ei\u00b7nen", "Y\u00b7sop\u00b7pusch", ",", "ent\u00b7s\u00fcn\u00b7di\u00b7ge", "mein", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Du kanst alleine mir die rechte Lauge geben,", "tokens": ["Du", "kanst", "al\u00b7lei\u00b7ne", "mir", "die", "rech\u00b7te", "Lau\u00b7ge", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "die Seel' und Leib begl\u00e4nzt, gleich als der Sehen Liecht,", "tokens": ["die", "Seel'", "und", "Leib", "be\u00b7gl\u00e4nzt", ",", "gleich", "als", "der", "Se\u00b7hen", "Liecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVPP", "$,", "ADV", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "die truckne Flut, der Schnee, mit seinem Schein hinsticht.", "tokens": ["die", "truck\u00b7ne", "Flut", ",", "der", "Schnee", ",", "mit", "sei\u00b7nem", "Schein", "hin\u00b7sticht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "La\u00df mich von lautrer Lust und Wonne h\u00f6ren sagen,", "tokens": ["La\u00df", "mich", "von", "laut\u00b7rer", "Lust", "und", "Won\u00b7ne", "h\u00f6\u00b7ren", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN", "KON", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "da\u00df der Gebeine Mark, die du so sehr zuschlagen,", "tokens": ["da\u00df", "der", "Ge\u00b7bei\u00b7ne", "Mark", ",", "die", "du", "so", "sehr", "zu\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "einst wieder werde froh! Vertilge meine Schuld,", "tokens": ["einst", "wie\u00b7der", "wer\u00b7de", "froh", "!", "Ver\u00b7til\u00b7ge", "mei\u00b7ne", "Schuld", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ADJD", "$.", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "verbirge dich vor ihr und sei mir wieder huld!", "tokens": ["ver\u00b7bir\u00b7ge", "dich", "vor", "ihr", "und", "sei", "mir", "wie\u00b7der", "huld", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "KON", "VAFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Gott, schaffe du in mir ein neues reines Herze", "tokens": ["Gott", ",", "schaf\u00b7fe", "du", "in", "mir", "ein", "neu\u00b7es", "rei\u00b7nes", "Her\u00b7ze"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "APPR", "PPER", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "und gib mir einen Geist, der nicht im Glauben scherze", "tokens": ["und", "gib", "mir", "ei\u00b7nen", "Geist", ",", "der", "nicht", "im", "Glau\u00b7ben", "scher\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "ART", "NN", "$,", "PRELS", "PTKNEG", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "und wanke hin und her! Verwirf mich nicht von dir", "tokens": ["und", "wan\u00b7ke", "hin", "und", "her", "!", "Ver\u00b7wirf", "mich", "nicht", "von", "dir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$.", "VVIMP", "PPER", "PTKNEG", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "und nimb, o Vater, nicht den werthen Geist von mir!", "tokens": ["und", "nimb", ",", "o", "Va\u00b7ter", ",", "nicht", "den", "wert\u00b7hen", "Geist", "von", "mir", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "FM", "NN", "$,", "PTKNEG", "ART", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "La\u00df deine H\u00fclfe mich zu aller Zeit erquicken,", "tokens": ["La\u00df", "dei\u00b7ne", "H\u00fcl\u00b7fe", "mich", "zu", "al\u00b7ler", "Zeit", "er\u00b7qui\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PRF", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "und dein beherzter Geist la\u00df in mir nicht ersticken", "tokens": ["und", "dein", "be\u00b7herz\u00b7ter", "Geist", "la\u00df", "in", "mir", "nicht", "er\u00b7sti\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "des Glaubens schwache Frucht! Herr, tr\u00f6ste, tr\u00f6ste mich!", "tokens": ["des", "Glau\u00b7bens", "schwa\u00b7che", "Frucht", "!", "Herr", ",", "tr\u00f6s\u00b7te", ",", "tr\u00f6s\u00b7te", "mich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "NN", "$,", "VVFIN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Enthalte du mich, Herr, so bin enthalten ich!", "tokens": ["Ent\u00b7hal\u00b7te", "du", "mich", ",", "Herr", ",", "so", "bin", "ent\u00b7hal\u00b7ten", "ich", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "NN", "$,", "ADV", "VAFIN", "VVPP", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Drumb wil ich deinen Weg die \u00dcbelt\u00e4ter lehren,", "tokens": ["Drumb", "wil", "ich", "dei\u00b7nen", "Weg", "die", "\u00dc\u00b7belt\u00b7\u00e4\u00b7ter", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PPOSAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "da\u00df sich die S\u00fcnderzunft zu dir sol m\u00fcssen kehren.", "tokens": ["da\u00df", "sich", "die", "S\u00fcn\u00b7der\u00b7zunft", "zu", "dir", "sol", "m\u00fcs\u00b7sen", "keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPR", "PPER", "VMFIN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Gott, der du stets mein Gott und frischer Heiland bist,", "tokens": ["Gott", ",", "der", "du", "stets", "mein", "Gott", "und", "fri\u00b7scher", "Hei\u00b7land", "bist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "PPOSAT", "NN", "KON", "ADJA", "NN", "VAFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.36": {"text": "nimb meine Blutschuld hin, die mir das Leben fri\u00dft,", "tokens": ["nimb", "mei\u00b7ne", "Blut\u00b7schuld", "hin", ",", "die", "mir", "das", "Le\u00b7ben", "fri\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PTKVZ", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "errette mich von ihr, so sol dein recht Gerichte", "tokens": ["er\u00b7ret\u00b7te", "mich", "von", "ihr", ",", "so", "sol", "dein", "recht", "Ge\u00b7rich\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "$,", "ADV", "VMFIN", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "von mir gepriesen sein durch dieser Zungen Fr\u00fcchte.", "tokens": ["von", "mir", "ge\u00b7prie\u00b7sen", "sein", "durch", "die\u00b7ser", "Zun\u00b7gen", "Fr\u00fcch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAINF", "APPR", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Herr, \u00f6ffne mir den Mund, brich meiner Lippen Schlo\u00df,", "tokens": ["Herr", ",", "\u00f6ff\u00b7ne", "mir", "den", "Mund", ",", "brich", "mei\u00b7ner", "Lip\u00b7pen", "Schlo\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ART", "NN", 