{"dta.poem.4055": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "T\u00e4glich geh' ich heraus und such' ein Anderes immer,", "tokens": ["T\u00e4g\u00b7lich", "geh'", "ich", "he\u00b7raus", "und", "such'", "ein", "An\u00b7de\u00b7res", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "ART", "PIS", "ADV", "$,"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.2": {"text": "Habe l\u00e4ngst sie befragt, alle die Pfade des Lands;", "tokens": ["Ha\u00b7be", "l\u00e4ngst", "sie", "be\u00b7fragt", ",", "al\u00b7le", "die", "Pfa\u00b7de", "des", "Lands", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVPP", "$,", "PIS", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Droben die k\u00fchlenden H\u00f6hn, die Schatten alle", "tokens": ["Dro\u00b7ben", "die", "k\u00fch\u00b7len\u00b7den", "H\u00f6hn", ",", "die", "Schat\u00b7ten", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "ART", "NN", "PIAT"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "besuch' ich,", "tokens": ["be\u00b7such'", "ich", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Und die Quellen; hinauf irret der Geist und", "tokens": ["Und", "die", "Quel\u00b7len", ";", "hin\u00b7auf", "ir\u00b7ret", "der", "Geist", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "KON"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "hinab,", "tokens": ["hin\u00b7ab", ","], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Ruh' erbittend; so flieht das getroffene Wild in", "tokens": ["Ruh'", "er\u00b7bit\u00b7tend", ";", "so", "flieht", "das", "ge\u00b7trof\u00b7fe\u00b7ne", "Wild", "in"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN", "APPR"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "die W\u00e4lder,", "tokens": ["die", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Wo es um Mittag sonst sicher im Dunkel geruht;", "tokens": ["Wo", "es", "um", "Mit\u00b7tag", "sonst", "si\u00b7cher", "im", "Dun\u00b7kel", "ge\u00b7ruht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "ADV", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.10": {"text": "Aber nimmer erquickt sein gr\u00fcnes Lager das Herz", "tokens": ["A\u00b7ber", "nim\u00b7mer", "er\u00b7quickt", "sein", "gr\u00fc\u00b7nes", "La\u00b7ger", "das", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "ART", "NN"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "ihm,", "tokens": ["ihm", ","], "token_info": ["word", "punct"], "pos": ["PPER", "$,"], "meter": "-", "measure": "single.down"}, "line.12": {"text": "Jammernd und schlummerlos treibt es der", "tokens": ["Jam\u00b7mernd", "und", "schlum\u00b7mer\u00b7los", "treibt", "es", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "PPER", "ART"], "meter": "+--+--+--", "measure": "dactylic.tri"}, "line.13": {"text": "Stachel umher.", "tokens": ["Sta\u00b7chel", "um\u00b7her", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "Nicht die W\u00e4rme des Lichts, und nicht die K\u00fchle", "tokens": ["Nicht", "die", "W\u00e4r\u00b7me", "des", "Lichts", ",", "und", "nicht", "die", "K\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "ART", "NN", "$,", "KON", "PTKNEG", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "der Nacht hilft,", "tokens": ["der", "Nacht", "hilft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.16": {"text": "Und in Wogen des Stroms taucht es die Wun-", "tokens": ["Und", "in", "Wo\u00b7gen", "des", "Stroms", "taucht", "es", "die", "Wun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "ART", "NN", "VVFIN", "PPER", "ART", "TRUNC"], "meter": "--+--++--+", "measure": "anapaest.di.plus"}, "line.17": {"text": "den umsonst.", "tokens": ["den", "um\u00b7sonst", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "ADV", "$."], "meter": "--+", "measure": "anapaest.init"}, "line.18": {"text": "Und wie ihm vergebens die Erd' ihr fr\u00f6hliches", "tokens": ["Und", "wie", "ihm", "ver\u00b7ge\u00b7bens", "die", "Erd'", "ihr", "fr\u00f6h\u00b7li\u00b7ches"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "ART", "NN", "PPOSAT", "ADJA"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.19": {"text": "Heilkraut", "tokens": ["Heil\u00b7kraut"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.20": {"text": "Reicht, und das g\u00e4hrende Blut keiner der Ze-", "tokens": ["Reicht", ",", "und", "das", "g\u00e4h\u00b7ren\u00b7de", "Blut", "kei\u00b7ner", "der", "Ze"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "ART", "ADJA", "NN", "PIS", "ART", "TRUNC"], "meter": "+--+--++--+", "measure": "dactylic.di.plus"}, "line.21": {"text": "phyre stillt,", "tokens": ["phy\u00b7re", "stillt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.22": {"text": "So, ihr Lieben, auch mir, so will es scheinen,", "tokens": ["So", ",", "ihr", "Lie\u00b7ben", ",", "auch", "mir", ",", "so", "will", "es", "schei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "ADJA", "$,", "ADV", "PPER", "$,", "ADV", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.23": {"text": "und Niemand", "tokens": ["und", "Nie\u00b7mand"], "token_info": ["word", "word"], "pos": ["KON", "PIS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.24": {"text": "Kann von der Stirne mir nehmen den traurigen", "tokens": ["Kann", "von", "der", "Stir\u00b7ne", "mir", "neh\u00b7men", "den", "trau\u00b7ri\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ART", "NN", "PPER", "VVFIN", "ART", "ADJA"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.25": {"text": "Traum?", "tokens": ["Traum", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}}}}