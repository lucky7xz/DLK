{"dta.poem.6547": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Farben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "F\u00e4llt es durch ein Chrystall, das gantz durchsichtig, rein,", "tokens": ["F\u00e4llt", "es", "durch", "ein", "Chrys\u00b7tall", ",", "das", "gantz", "durch\u00b7sich\u00b7tig", ",", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "In welchem nichts, das ihnen gleich, zu finden.", "tokens": ["In", "wel\u00b7chem", "nichts", ",", "das", "ih\u00b7nen", "gleich", ",", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "$,", "PRELS", "PPER", "ADV", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Daselbst nun k\u00f6nnen wir den R\u00fcckbruch nicht ergr\u00fcnden,", "tokens": ["Da\u00b7selbst", "nun", "k\u00f6n\u00b7nen", "wir", "den", "R\u00fcck\u00b7bruch", "nicht", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch den so schnellen Glantz,", "tokens": ["Durch", "den", "so", "schnel\u00b7len", "Glantz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der fast das Aug erschreckt.", "tokens": ["Der", "fast", "das", "Aug", "er\u00b7schreckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Es wird der Farben Schmuck vom Regen-Bogen, gantz", "tokens": ["Es", "wird", "der", "Far\u00b7ben", "Schmuck", "vom", "Re\u00b7gen\u00b7Bo\u00b7gen", ",", "gantz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "APPRART", "NN", "$,", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In einem Prisma, uns entdeckt.", "tokens": ["In", "ei\u00b7nem", "Pris\u00b7ma", ",", "uns", "ent\u00b7deckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}