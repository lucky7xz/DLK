{"textgrid.poem.40520": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Komm, Muse meines Liedes, komm ins wilde", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm, Muse meines Liedes, komm ins wilde", "tokens": ["Komm", ",", "Mu\u00b7se", "mei\u00b7nes", "Lie\u00b7des", ",", "komm", "ins", "wil\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "PPOSAT", "NN", "$,", "VVFIN", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Steinklippenthal der Urwaldsnacht mit mir!", "tokens": ["Stein\u00b7klip\u00b7pen\u00b7thal", "der", "Ur\u00b7walds\u00b7nacht", "mit", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPER", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Vor jener Eichen alter Richtergilde", "tokens": ["Vor", "je\u00b7ner", "Ei\u00b7chen", "al\u00b7ter", "Rich\u00b7ter\u00b7gil\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dort spr\u00e4ch' ich gern ein ernstes Wort mit dir.", "tokens": ["Dort", "spr\u00e4ch'", "ich", "gern", "ein", "erns\u00b7tes", "Wort", "mit", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nicht gn\u00fcgt's, da\u00df dir der Markt, der leichtentz\u00fcckte,", "tokens": ["Nicht", "gn\u00fcgt's", ",", "da\u00df", "dir", "der", "Markt", ",", "der", "leich\u00b7tent\u00b7z\u00fcck\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "NE", "$,", "KOUS", "PPER", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Lobs Almosen zuwarf manchesmal,", "tokens": ["Des", "Lobs", "Al\u00b7mo\u00b7sen", "zu\u00b7warf", "man\u00b7ches\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Manch allzumilder Freund die Hand dir dr\u00fcckte,", "tokens": ["Manch", "all\u00b7zu\u00b7mil\u00b7der", "Freund", "die", "Hand", "dir", "dr\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Beifallswort sich seinem Mund entstahl!", "tokens": ["Und", "Bei\u00b7falls\u00b7wort", "sich", "sei\u00b7nem", "Mund", "ent\u00b7stahl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PRF", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Kein Mensch beschritt den Waldpfad, den wir w\u00e4hlen;", "tokens": ["Kein", "Mensch", "be\u00b7schritt", "den", "Wald\u00b7pfad", ",", "den", "wir", "w\u00e4h\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Horch, von den Zweigen tr\u00e4uft der V\u00f6gel Sang", "tokens": ["Horch", ",", "von", "den", "Zwei\u00b7gen", "tr\u00e4uft", "der", "V\u00f6\u00b7gel", "Sang"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Wie Fr\u00fchthau auf die Blumen unsrer Seelen!", "tokens": ["Wie", "Fr\u00fch\u00b7thau", "auf", "die", "Blu\u00b7men", "uns\u00b7rer", "See\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ach, er verstummt bei unsrer Schritte Klang!", "tokens": ["Ach", ",", "er", "ver\u00b7stummt", "bei", "uns\u00b7rer", "Schrit\u00b7te", "Klang", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie sangen nicht, um unsrem Ohr zu dienen,", "tokens": ["Sie", "san\u00b7gen", "nicht", ",", "um", "uns\u00b7rem", "Ohr", "zu", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Lerchenweisen lallt der Finke nie:", "tokens": ["Und", "Ler\u00b7chen\u00b7wei\u00b7sen", "lallt", "der", "Fin\u00b7ke", "nie", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein besser Seelentheil wohl sang aus ihnen!", "tokens": ["Mein", "bes\u00b7ser", "See\u00b7len\u00b7theil", "wohl", "sang", "aus", "ih\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sprich, Muse meines Lieds, thatst du wie sie? \u2013", "tokens": ["Sprich", ",", "Mu\u00b7se", "mei\u00b7nes", "Lieds", ",", "thatst", "du", "wie", "sie", "?", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "$,", "NN", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "PWAV", "PPER", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ein Bl\u00fcthenbaum verlor sich dort zu Eichen,", "tokens": ["Ein", "Bl\u00fc\u00b7then\u00b7baum", "ver\u00b7lor", "sich", "dort", "zu", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die bl\u00fcthenlos, wenn sonst auch sch\u00f6n und gr\u00fcn;", "tokens": ["Die", "bl\u00fc\u00b7then\u00b7los", ",", "wenn", "sonst", "auch", "sch\u00f6n", "und", "gr\u00fcn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "KOUS", "ADV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch er kann anders nicht, als Bl\u00fcthen reichen,", "tokens": ["Doch", "er", "kann", "an\u00b7ders", "nicht", ",", "als", "Bl\u00fc\u00b7then", "rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "PTKNEG", "$,", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur Axt und Blitz gef\u00e4hrden einst sein Bl\u00fchn!", "tokens": ["Nur", "Axt", "und", "Blitz", "ge\u00b7f\u00e4hr\u00b7den", "einst", "sein", "Bl\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVPP", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Froh wiegt er sein Gezweig im Sonnenlichte!", "tokens": ["Froh", "wiegt", "er", "sein", "Ge\u00b7zweig", "im", "Son\u00b7nen\u00b7lich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Blitze schl\u00e4gt sein blumiges Gestr\u00e4uch,", "tokens": ["Dem", "Blit\u00b7ze", "schl\u00e4gt", "sein", "blu\u00b7mi\u00b7ges", "Ge\u00b7str\u00e4uch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die bl\u00fch'nde Waffe, er ins Angesichte!", "tokens": ["Die", "bl\u00fch'n\u00b7de", "Waf\u00b7fe", ",", "er", "ins", "An\u00b7ge\u00b7sich\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sprich, Muse meines Lieds, thust du's ihm gleich? \u2013", "tokens": ["Sprich", ",", "Mu\u00b7se", "mei\u00b7nes", "Lieds", ",", "thust", "du's", "ihm", "gleich", "?", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "$,", "NN", "PPOSAT", "NN", "$,", "VVFIN", "PIS", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Am Grunde modert eine alte Eiche,", "tokens": ["Am", "Grun\u00b7de", "mo\u00b7dert", "ei\u00b7ne", "al\u00b7te", "Ei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Manch hundert Lenze f\u00fcllten einst ihr Mark;", "tokens": ["Manch", "hun\u00b7dert", "Len\u00b7ze", "f\u00fcll\u00b7ten", "einst", "ihr", "Mark", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "CARD", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gleichg\u00fcltig stehn die Br\u00fcder um die Leiche,", "tokens": ["Gleich\u00b7g\u00fcl\u00b7tig", "stehn", "die", "Br\u00fc\u00b7der", "um", "die", "Lei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sind alle ja noch laubig, gr\u00fcn und stark!", "tokens": ["Sind", "al\u00b7le", "ja", "noch", "lau\u00b7big", ",", "gr\u00fcn", "und", "stark", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Der Vogel, der des Baumes Lenzgef\u00fchle", "tokens": ["Der", "Vo\u00b7gel", ",", "der", "des", "Bau\u00b7mes", "Lenz\u00b7ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NE", "$,", "PRELS", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von seinem Blatte las und statt ihm sang,", "tokens": ["Von", "sei\u00b7nem", "Blat\u00b7te", "las", "und", "statt", "ihm", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "KON", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der liederreiche, d\u00fcngt in Gartenk\u00fchle", "tokens": ["Der", "lie\u00b7der\u00b7rei\u00b7che", ",", "d\u00fcngt", "in", "Gar\u00b7ten\u00b7k\u00fch\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Jetzt Blumen fern zu Duft und Bl\u00fcthendrang.", "tokens": ["Jetzt", "Blu\u00b7men", "fern", "zu", "Duft", "und", "Bl\u00fc\u00b7then\u00b7drang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "In dunkler Nacht, wenn Stern' und Mond nicht gl\u00e4nzen,", "tokens": ["In", "dunk\u00b7ler", "Nacht", ",", "wenn", "Stern'", "und", "Mond", "nicht", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "NN", "KON", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Umquillt phosphorisch Licht den morschen Baum:", "tokens": ["Um\u00b7quillt", "phos\u00b7pho\u00b7risch", "Licht", "den", "mor\u00b7schen", "Baum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Traun, ihn umwallt von seinen todten Lenzen", "tokens": ["Traun", ",", "ihn", "um\u00b7wallt", "von", "sei\u00b7nen", "tod\u00b7ten", "Len\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein leuchtender und sch\u00f6ner Grabestraum!", "tokens": ["Ein", "leuch\u00b7ten\u00b7der", "und", "sch\u00f6\u00b7ner", "Gra\u00b7best\u00b7raum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Und wird auch mir, wenn einst im Waldesd\u00fcstern", "tokens": ["Und", "wird", "auch", "mir", ",", "wenn", "einst", "im", "Wal\u00b7des\u00b7d\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "PPER", "$,", "KOUS", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fern und vergessen sich mein H\u00fcgel hebt,", "tokens": ["Fern", "und", "ver\u00b7ges\u00b7sen", "sich", "mein", "H\u00fc\u00b7gel", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein lichter Traum von dir es tr\u00f6stend fl\u00fcstern,", "tokens": ["Ein", "lich\u00b7ter", "Traum", "von", "dir", "es", "tr\u00f6s\u00b7tend", "fl\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df kein verlornes Leben ich gelebt?", "tokens": ["Da\u00df", "kein", "ver\u00b7lor\u00b7nes", "Le\u00b7ben", "ich", "ge\u00b7lebt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Sprich, wird einst meines Jugendliedes Rose", "tokens": ["Sprich", ",", "wird", "einst", "mei\u00b7nes", "Ju\u00b7gend\u00b7lie\u00b7des", "Ro\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "VAFIN", "ADV", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem greisen Haupt nur Flitter, de\u00df sich's sch\u00e4mt,", "tokens": ["Dem", "grei\u00b7sen", "Haupt", "nur", "Flit\u00b7ter", ",", "de\u00df", "sich's", "sch\u00e4mt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "NN", "$,", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht eine Zierde, gleich dem Kranz von Moose,", "tokens": ["Nicht", "ei\u00b7ne", "Zier\u00b7de", ",", "gleich", "dem", "Kranz", "von", "Moo\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der jenes kahle Felshaupt sch\u00f6n verbr\u00e4mt? \u2013", "tokens": ["Der", "je\u00b7nes", "kah\u00b7le", "Fels\u00b7haupt", "sch\u00f6n", "ver\u00b7br\u00e4mt", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PDAT", "ADJA", "NN", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Der Wildbach schl\u00e4gt sich tapfer hier durch Klippen,", "tokens": ["Der", "Wild\u00b7bach", "schl\u00e4gt", "sich", "tap\u00b7fer", "hier", "durch", "Klip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein R\u00f6slein wiegt auf seinen Wellen sich!", "tokens": ["Ein", "R\u00f6s\u0