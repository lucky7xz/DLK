{"dta.poem.886": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "FrAUENSEGEN", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Schreitest unter deinen Frau\u2019n", "tokens": ["Schrei\u00b7test", "un\u00b7ter", "dei\u00b7nen", "Frau'n"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und du l\u00e4chelst oft beklommen:", "tokens": ["Und", "du", "l\u00e4\u00b7chelst", "oft", "be\u00b7klom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind so bange Tage kommen.", "tokens": ["Sind", "so", "ban\u00b7ge", "Ta\u00b7ge", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wei\u00df verbl\u00fcht der Mohn am Zaun.", "tokens": ["Wei\u00df", "ver\u00b7bl\u00fcht", "der", "Mohn", "am", "Zaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie dein Leib so sch\u00f6n geschwellt", "tokens": ["Wie", "dein", "Leib", "so", "sch\u00f6n", "ge\u00b7schwellt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "ADJD", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Golden reift der Wein am H\u00fcgel.", "tokens": ["Gol\u00b7den", "reift", "der", "Wein", "am", "H\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ferne gl\u00e4nzt des Weihers Spiegel", "tokens": ["Fer\u00b7ne", "gl\u00e4nzt", "des", "Wei\u00b7hers", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Sense klirrt im Feld.", "tokens": ["Und", "die", "Sen\u00b7se", "klirrt", "im", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "In den B\u00fcschen rollt der Tau,", "tokens": ["In", "den", "B\u00fc\u00b7schen", "rollt", "der", "Tau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Rot die Bl\u00e4tter niederflie\u00dfen.", "tokens": ["Rot", "die", "Bl\u00e4t\u00b7ter", "nie\u00b7der\u00b7flie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine liebe Frau zu gr\u00fc\u00dfen", "tokens": ["Sei\u00b7ne", "lie\u00b7be", "Frau", "zu", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Naht ein Mohr dir braun und rauh.", "tokens": ["Naht", "ein", "Mohr", "dir", "braun", "und", "rauh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", 