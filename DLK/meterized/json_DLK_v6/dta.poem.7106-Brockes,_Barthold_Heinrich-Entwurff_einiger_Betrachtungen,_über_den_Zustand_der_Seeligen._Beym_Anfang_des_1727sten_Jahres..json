{"dta.poem.7106": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In welcher Z\u00e4rtlichkeit und inniglicher Liebe", "tokens": ["In", "wel\u00b7cher", "Z\u00e4rt\u00b7lich\u00b7keit", "und", "in\u00b7nig\u00b7li\u00b7cher", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Der GOTTHEIT mu\u00df sodann die gantze Seele stehn!", "tokens": ["Der", "GoT\u00b7T\u00b7HEIT", "mu\u00df", "so\u00b7dann", "die", "gant\u00b7ze", "See\u00b7le", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Welch Andachts-Br\u00fcnstigkeit und Ehrfurcht-volle Triebe,", "tokens": ["Welch", "An\u00b7dachts\u00b7Br\u00fcns\u00b7tig\u00b7keit", "und", "Ehr\u00b7furcht\u00b7vol\u00b7le", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht durch ihr gantzes Wesen gehn!", "tokens": ["Nicht", "durch", "ihr", "gant\u00b7zes", "We\u00b7sen", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit welchem sehnlichen und br\u00fcnstigen Vertrauen,", "tokens": ["Mit", "wel\u00b7chem", "sehn\u00b7li\u00b7chen", "und", "br\u00fcns\u00b7ti\u00b7gen", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "VVINF", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mu\u00df sie die Majest\u00e4t der GOTTHEIT schauen!", "tokens": ["Mu\u00df", "sie", "die", "Ma\u00b7jes\u00b7t\u00e4t", "der", "GoT\u00b7T\u00b7HEIT", "schau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "In ew\u2019ger Emsigkeit, sie anzusehn beflissen;", "tokens": ["In", "ew'\u00b7ger", "Em\u00b7sig\u00b7keit", ",", "sie", "an\u00b7zu\u00b7sehn", "be\u00b7flis\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PPER", "VVIZU", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wird gleichsam sie", "tokens": ["Wird", "gleich\u00b7sam", "sie"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADJD", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Zu lauter Liebe werden m\u00fcssen!", "tokens": ["Zu", "lau\u00b7ter", "Lie\u00b7be", "wer\u00b7den", "m\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}