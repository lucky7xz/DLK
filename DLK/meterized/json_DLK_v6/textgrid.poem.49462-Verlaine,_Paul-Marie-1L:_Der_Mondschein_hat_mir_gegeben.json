{"textgrid.poem.49462": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Mondschein hat mir gegeben", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mondschein hat mir gegeben", "tokens": ["Der", "Mond\u00b7schein", "hat", "mir", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Maske vom n\u00e4cht'gen Saturne,", "tokens": ["Die", "Mas\u00b7ke", "vom", "n\u00e4cht'\u00b7gen", "Sa\u00b7tur\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Der schweigend neigt seine Urne,", "tokens": ["Der", "schwei\u00b7gend", "neigt", "sei\u00b7ne", "Ur\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den blass seine Monde umschweben.", "tokens": ["Den", "blass", "sei\u00b7ne", "Mon\u00b7de", "um\u00b7schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Ohne Worte zittert ein Sang", "tokens": ["Oh\u00b7ne", "Wor\u00b7te", "zit\u00b7tert", "ein", "Sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Mit duftgem, verstimmtem Akkord", "tokens": ["Mit", "duft\u00b7gem", ",", "ver\u00b7stimm\u00b7tem", "Ak\u00b7kord"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "In dem m\u00fcden Herzen fort,", "tokens": ["In", "dem", "m\u00fc\u00b7den", "Her\u00b7zen", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "O welch tr\u00fcber, fr\u00f6stelnder Klang!", "tokens": ["O", "welch", "tr\u00fc\u00b7ber", ",", "fr\u00f6s\u00b7teln\u00b7der", "Klang", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Ihr alle habt g\u00fctig verziehen,", "tokens": ["Ihr", "al\u00b7le", "habt", "g\u00fc\u00b7tig", "ver\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wenn einer euch Wunden geschlagen,", "tokens": ["Wenn", "ei\u00b7ner", "euch", "Wun\u00b7den", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wie ich den vergangenen Tagen,", "tokens": ["Wie", "ich", "den", "ver\u00b7gan\u00b7ge\u00b7nen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die den Putz ferner Jugend geliehen.", "tokens": ["Die", "den", "Putz", "fer\u00b7ner", "Ju\u00b7gend", "ge\u00b7lie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "NN", "VVPP", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.4": {"line.1": {"text": "So vergeb ich dem leisen Trug,", "tokens": ["So", "ver\u00b7geb", "ich", "dem", "lei\u00b7sen", "Trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Weil ein wenig Lust er gebracht,", "tokens": ["Weil", "ein", "we\u00b7nig", "Lust", "er", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wann ein eitler Wunsch mir gelacht,", "tokens": ["Wann", "ein", "eit\u00b7ler", "Wunsch", "mir", "ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Verworren und schmerzlich genug.", "tokens": ["Ver\u00b7wor\u00b7ren", "und", "schmerz\u00b7lich", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}