{"dta.poem.16270": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie herzerquickend ist erf\u00fcllter Pflicht Gef\u00fchl!", "tokens": ["Wie", "herz\u00b7er\u00b7qui\u00b7ckend", "ist", "er\u00b7f\u00fcll\u00b7ter", "Pflicht", "Ge\u00b7f\u00fchl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "VAFIN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Froste macht es warm, und in der Hitze k\u00fchl.", "tokens": ["Im", "Fros\u00b7te", "macht", "es", "warm", ",", "und", "in", "der", "Hit\u00b7ze", "k\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJD", "$,", "KON", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gleichwie des Wachsthums Trieb durch Knoten an dem Rohr,", "tokens": ["Gleich\u00b7wie", "des", "Wach\u00b7sthums", "Trieb", "durch", "Kno\u00b7ten", "an", "dem", "Rohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So treibt aus Hemmungen des Sieges Kraft empor.", "tokens": ["So", "treibt", "aus", "Hem\u00b7mun\u00b7gen", "des", "Sie\u00b7ges", "Kraft", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Doch immer ist ein Kampf, wo wir zu siegen haben;", "tokens": ["Doch", "im\u00b7mer", "ist", "ein", "Kampf", ",", "wo", "wir", "zu", "sie\u00b7gen", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O selig, wer sein Herz in Frieden hat begraben.", "tokens": ["O", "se\u00b7lig", ",", "wer", "sein", "Herz", "in", "Frie\u00b7den", "hat", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "PWS", "PPOSAT", "NN", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}