{"textgrid.poem.38964": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sanft umfangen", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sanft umfangen", "tokens": ["Sanft", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADJD", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Vom Verlangen,", "tokens": ["Vom", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Abendwolken ziehn,", "tokens": ["Ab\u00b7end\u00b7wol\u00b7ken", "ziehn", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "O, gegr\u00fc\u00dft sei holdes Gl\u00fccke,", "tokens": ["O", ",", "ge\u00b7gr\u00fc\u00dft", "sei", "hol\u00b7des", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVPP", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Endlich, endlich meinem Blicke,", "tokens": ["End\u00b7lich", ",", "end\u00b7lich", "mei\u00b7nem", "Bli\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "L\u00e4ngst gepflanzte Blumen bl\u00fchn.", "tokens": ["L\u00e4ngst", "ge\u00b7pflanz\u00b7te", "Blu\u00b7men", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Abendr\u00f6the winkt herunter:", "tokens": ["A\u00b7bend\u00b7r\u00f6\u00b7the", "winkt", "her\u00b7un\u00b7ter", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hoffe auf den Morgen munter;", "tokens": ["Hof\u00b7fe", "auf", "den", "Mor\u00b7gen", "mun\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Winde eilen, verk\u00fcnden's der Ferne,", "tokens": ["Win\u00b7de", "ei\u00b7len", ",", "ver\u00b7k\u00fcn\u00b7den's", "der", "Fer\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Blicken auf mich nieder die freundlichen Sterne.", "tokens": ["Bli\u00b7cken", "auf", "mich", "nie\u00b7der", "die", "freund\u00b7li\u00b7chen", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PTKVZ", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Keiner, der nicht gr\u00fc\u00dfend niederschaute:", "tokens": ["Kei\u00b7ner", ",", "der", "nicht", "gr\u00fc\u00b7\u00dfend", "nie\u00b7der\u00b7schau\u00b7te", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ist es, singen sie, dir gelungen?", "tokens": ["Ist", "es", ",", "sin\u00b7gen", "sie", ",", "dir", "ge\u00b7lun\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "PPER", "VVPP", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Welche T\u00f6ne r\u00fchren sich in der Laute,", "tokens": ["Wel\u00b7che", "T\u00f6\u00b7ne", "r\u00fch\u00b7ren", "sich", "in", "der", "Lau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Von unsichtbarer Geisterhand durchklungen?", "tokens": ["Von", "un\u00b7sicht\u00b7ba\u00b7rer", "Geis\u00b7ter\u00b7hand", "durch\u00b7klun\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Von selbst erregt sie sich zum Spiele,", "tokens": ["Von", "selbst", "er\u00b7regt", "sie", "sich", "zum", "Spie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Will ihre Worte gern verk\u00fcnden,", "tokens": ["Will", "ih\u00b7re", "Wor\u00b7te", "gern", "ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kennst du, Vertraute, die Gef\u00fchle,", "tokens": ["Kennst", "du", ",", "Ver\u00b7trau\u00b7te", ",", "die", "Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Die qu\u00e4lend, begl\u00fcckend mein Herz entz\u00fcnden?", "tokens": ["Die", "qu\u00e4\u00b7lend", ",", "be\u00b7gl\u00fc\u00b7ckend", "mein", "Herz", "ent\u00b7z\u00fcn\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "VVPP", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "O t\u00f6ne, ich kann das Lied nicht finden,", "tokens": ["O", "t\u00f6\u00b7ne", ",", "ich", "kann", "das", "Lied", "nicht", "fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Das Leid, das Gl\u00fcck, das mich bewegt,", "tokens": ["Das", "Leid", ",", "das", "Gl\u00fcck", ",", "das", "mich", "be\u00b7wegt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und Klang und Lust in mir erregt.", "tokens": ["Und", "Klang", "und", "Lust", "in", "mir", "er\u00b7regt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Will ich von Gl\u00fcck, von Freude singen,", "tokens": ["Will", "ich", "von", "Gl\u00fcck", ",", "von", "Freu\u00b7de", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "$,", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von alten, wonnevollen Stunden?", "tokens": ["Von", "al\u00b7ten", ",", "won\u00b7ne\u00b7vol\u00b7len", "Stun\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist nicht da und fern verschwunden,", "tokens": ["Es", "ist", "nicht", "da", "und", "fern", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Geist von Entz\u00fccken festgebunden, \u2013", "tokens": ["Mein", "Geist", "von", "Ent\u00b7z\u00fc\u00b7cken", "fest\u00b7ge\u00b7bun\u00b7den", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVINF", "$,", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Beengt, beschr\u00e4nkt die goldnen Schwingen.", "tokens": ["Be\u00b7engt", ",", "be\u00b7schr\u00e4nkt", "die", "gold\u00b7nen", "Schwin\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Geht die Liebe wohl auf deinem Klange,", "tokens": ["Geht", "die", "Lie\u00b7be", "wohl", "auf", "dei\u00b7nem", "Klan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ist sie's, die deine T\u00f6ne r\u00fchrt?", "tokens": ["Ist", "sie's", ",", "die", "dei\u00b7ne", "T\u00f6\u00b7ne", "r\u00fchrt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dieses Herz mit strebendem Drange", "tokens": ["Und", "die\u00b7ses", "Herz", "mit", "stre\u00b7ben\u00b7dem", "Dran\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auf deinem Melodien entf\u00fchrt?", "tokens": ["Auf", "dei\u00b7nem", "Me\u00b7lo\u00b7dien", "ent\u00b7f\u00fchrt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Mit Zitterklang kam sie mir entgegen,", "tokens": ["Mit", "Zit\u00b7ter\u00b7klang", "kam", "sie", "mir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mein Geist in Netzen von T\u00f6nen gefangen,", "tokens": ["Mein", "Geist", "in", "Net\u00b7zen", "von", "T\u00f6\u00b7nen", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich f\u00fchlte schon dies Beben, dies Bangen,", "tokens": ["Ich", "f\u00fchl\u00b7te", "schon", "dies", "Be\u00b7ben", ",", "dies", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PDS", "NN", "$,", "PDS", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Entz\u00fccken \u00fcberstr\u00f6mte, ein goldner Regen.", "tokens": ["Ent\u00b7z\u00fc\u00b7cken", "\u00fc\u00b7ber\u00b7str\u00f6m\u00b7te", ",", "ein", "gold\u00b7ner", "Re\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Sie sa\u00df im Zimmer, wartete mein,", "tokens": ["Sie", "sa\u00df", "im", "Zim\u00b7mer", ",", "war\u00b7te\u00b7te", "mein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "VVFIN", "PPOSAT", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die Liebe f\u00fchrte mich hinein,", "tokens": ["Die", "Lie\u00b7be", "f\u00fchr\u00b7te", "mich", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erklang das alte Waldhorn drein;", "tokens": ["Er\u00b7klang", "das", "al\u00b7te", "Wald\u00b7horn", "drein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein voller Klang", "tokens": ["Dein", "vol\u00b7ler", "Klang"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Mein Herz schon oft durchdrang,", "tokens": ["Mein", "Herz", "schon", "oft", "durch\u00b7drang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Meiner Liebe vertraut,", "tokens": ["Mei\u00b7ner", "Lie\u00b7be", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Von deinem Ton mein Herz durchschaut.", "tokens": ["Von", "dei\u00b7nem", "Ton", "mein", "Herz", "durch\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nun verstummen nie die T\u00f6ne,", "tokens": ["Nun", "ver\u00b7stum\u00b7men", "nie", "die", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Lautenklang mein ganzes Leben,", "tokens": ["Lau\u00b7ten\u00b7klang", "mein", "gan\u00b7zes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Herz verkl\u00e4rt in sch\u00f6nster Sch\u00f6ne,", "tokens": ["Herz", "ver\u00b7kl\u00e4rt", "in", "sch\u00f6ns\u00b7ter", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wundervollem Glanz und Weben", "tokens": ["Wun\u00b7der\u00b7vol\u00b7lem", "Glanz", "und", "We\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Hingegeben.", "tokens": ["Hin\u00b7ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}