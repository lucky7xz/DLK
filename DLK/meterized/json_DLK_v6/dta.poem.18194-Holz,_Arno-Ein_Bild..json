{"dta.poem.18194": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Ein Bild.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Die rothen S\u00e4ulen aus Granit gehauen,", "tokens": ["Die", "ro\u00b7then", "S\u00e4u\u00b7len", "aus", "Gra\u00b7nit", "ge\u00b7hau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und seitw\u00e4rts in ein wei\u00dfes Piedestal", "tokens": ["Und", "seit\u00b7w\u00e4rts", "in", "ein", "wei\u00b7\u00dfes", "Pie\u00b7de\u00b7stal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vergr\u00e4bt ein L\u00f6we seine Marmorklauen.", "tokens": ["Ver\u00b7gr\u00e4bt", "ein", "L\u00f6\u00b7we", "sei\u00b7ne", "Mar\u00b7mor\u00b7klau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch schwarz verh\u00e4ngt sind alle Fenster heut", "tokens": ["Doch", "schwarz", "ver\u00b7h\u00e4ngt", "sind", "al\u00b7le", "Fens\u00b7ter", "heut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "VAFIN", "PIAT", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und Lichter brennen nur im Erdgeschosse,", "tokens": ["Und", "Lich\u00b7ter", "bren\u00b7nen", "nur", "im", "Erd\u00b7ge\u00b7schos\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Stra\u00dfendamm ist hoch mit Stroh bestreut", "tokens": ["Der", "Stra\u00b7\u00dfen\u00b7damm", "ist", "hoch", "mit", "Stroh", "be\u00b7streut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und lautlos dr\u00fcberhin rollt die Karosse.", "tokens": ["Und", "laut\u00b7los", "dr\u00fc\u00b7berh\u00b7in", "rollt", "die", "Ka\u00b7ros\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Das Treppenhaus vertheidigt der Portier", "tokens": ["Das", "Trep\u00b7pen\u00b7haus", "ver\u00b7thei\u00b7digt", "der", "Por\u00b7tier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und sch\u00fcttelt grimmig seine graue M\u00e4hne,", "tokens": ["Und", "sch\u00fct\u00b7telt", "grim\u00b7mig", "sei\u00b7ne", "grau\u00b7e", "M\u00e4h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und naht gar Einer aus der Haute volee,", "tokens": ["Und", "naht", "gar", "Ei\u00b7ner", "aus", "der", "Hau\u00b7te", "vo\u00b7lee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIS", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Dann fletscht er cerberusgleich seine Z\u00e4hne.", "tokens": ["Dann", "fletscht", "er", "cer\u00b7be\u00b7rus\u00b7gleich", "sei\u00b7ne", "Z\u00e4h\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Im Prunksaal trauern hinter Flor und Tafft", "tokens": ["Im", "Prunk\u00b7saal", "trau\u00b7ern", "hin\u00b7ter", "Flor", "und", "Tafft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die bunten Inderstoffe aus Lahore,", "tokens": ["Die", "bun\u00b7ten", "In\u00b7der\u00b7stof\u00b7fe", "aus", "La\u00b7ho\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Auch schleicht die goldbetre\u00dfte Dienerschaft", "tokens": ["Auch", "schleicht", "die", "gold\u00b7be\u00b7tre\u00df\u00b7te", "Die\u00b7ner\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nur auf Spitzzehen durch die Corridore.", "tokens": ["Nur", "auf", "Spitz\u00b7ze\u00b7hen", "durch", "die", "Cor\u00b7ri\u00b7do\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Der hochgeborne Hausherr, Excellenz,", "tokens": ["Der", "hoch\u00b7ge\u00b7bor\u00b7ne", "Haus\u00b7herr", ",", "Ex\u00b7cel\u00b7lenz", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schwankt wie ein Rohr umher auf bleicher D\u00fcne,", "tokens": ["Schwankt", "wie", "ein", "Rohr", "um\u00b7her", "auf", "blei\u00b7cher", "D\u00fc\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die erste Redekraft des Parlaments", "tokens": ["Die", "ers\u00b7te", "Re\u00b7de\u00b7kraft", "des", "Par\u00b7la\u00b7ments"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Fehlt heute abermals auf der Trib\u00fcne.", "tokens": ["Fehlt", "heu\u00b7te", "a\u00b7ber\u00b7mals", "auf", "der", "Tri\u00b7b\u00fc\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Zwar trat man gestern erst in den Etat,", "tokens": ["Zwar", "trat", "man", "ge\u00b7stern", "erst", "in", "den", "E\u00b7tat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Doch hat sein Fehlen diesmal gute Gr\u00fcnde:", "tokens": ["Doch", "hat", "sein", "Feh\u00b7len", "dies\u00b7mal", "gu\u00b7te", "Gr\u00fcn\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Schon viermal war der greise Hausarzt da", "tokens": ["Schon", "vier\u00b7mal", "war", "der", "grei\u00b7se", "Haus\u00b7arzt", "da"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und meinte, da\u00df es sehr bedenklich st\u00fcnde.", "tokens": ["Und", "mein\u00b7te", ",", "da\u00df", "es", "sehr", "be\u00b7denk\u00b7lich", "st\u00fcn\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nach Eis und Himbeer wird gar oft geschellt,", "tokens": ["Nach", "Eis", "und", "Him\u00b7beer", "wird", "gar", "oft", "ge\u00b7schellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch m\u00e4uschenstill ist es im Krankenzimmer,", "tokens": ["Doch", "m\u00e4u\u00b7schen\u00b7still", "ist", "es", "im", "Kran\u00b7ken\u00b7zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und seine d\u00fcstre Teppichpracht erhellt", "tokens": ["Und", "sei\u00b7ne", "d\u00fcst\u00b7re", "Tep\u00b7pich\u00b7pracht", "er\u00b7hellt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nur einer Ampel r\u00f6thliches Geflimmer.", "tokens": ["Nur", "ei\u00b7ner", "Am\u00b7pel", "r\u00f6th\u00b7li\u00b7ches", "Ge\u00b7flim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Weit offen steht die Th\u00fcr zum Vestibul", "tokens": ["Weit", "of\u00b7fen", "steht", "die", "Th\u00fcr", "zum", "Ves\u00b7ti\u00b7bul"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und wie im Traum nur pl\u00e4tschert die Font\u00e4ne,", "tokens": ["Und", "wie", "im", "Traum", "nur", "pl\u00e4t\u00b7schert", "die", "Fon\u00b7t\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPRART", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Die Luft umher ist wie gewitterschw\u00fcl,", "tokens": ["Die", "Luft", "um\u00b7her", "ist", "wie", "ge\u00b7wit\u00b7ter\u00b7schw\u00fcl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "VAFIN", "KOKOM", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Denn ach, die \u201egn\u00e4'ge Fraa\u201c hat heut \u2014 Migr\u00e4ne!", "tokens": ["Denn", "ach", ",", "die", "\u201e", "gn\u00e4'\u00b7ge", "Fraa", "\u201c", "hat", "heut", "Mig\u00b7r\u00e4\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "XY", "$,", "ART", "$(", "ADJA", "NN", "$(", "VAFIN", "ADV", "$(", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}