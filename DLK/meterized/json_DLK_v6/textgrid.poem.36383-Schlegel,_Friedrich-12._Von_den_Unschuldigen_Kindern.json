{"textgrid.poem.36383": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "12. Von den Unschuldigen Kindern", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott gr\u00fc\u00df euch Marterbl\u00fcmelein,", "tokens": ["Gott", "gr\u00fc\u00df", "euch", "Mar\u00b7ter\u00b7bl\u00fc\u00b7me\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihr so zarte Kinderlein", "tokens": ["Die", "ihr", "so", "zar\u00b7te", "Kin\u00b7der\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erlanget habt die Marterkron',", "tokens": ["Er\u00b7lan\u00b7get", "habt", "die", "Mar\u00b7ter\u00b7kron'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um Christi willen Gottes Sohn,", "tokens": ["Um", "Chris\u00b7ti", "wil\u00b7len", "Got\u00b7tes", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NE", "NN", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O junge zarte Martyrer schon.", "tokens": ["O", "jun\u00b7ge", "zar\u00b7te", "Mar\u00b7ty\u00b7rer", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Ihr seid das erste Opfer zart,", "tokens": ["Ihr", "seid", "das", "ers\u00b7te", "Op\u00b7fer", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Christo blutig Opfer ward,", "tokens": ["Das", "Chris\u00b7to", "blu\u00b7tig", "Op\u00b7fer", "ward", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Unschuld und in Reinigkeit", "tokens": ["In", "Un\u00b7schuld", "und", "in", "Rei\u00b7nig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Drob freut ihr euch in Ewigkeit,", "tokens": ["Drob", "freut", "ihr", "euch", "in", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O junge zarte Martyrer schon.", "tokens": ["O", "jun\u00b7ge", "zar\u00b7te", "Mar\u00b7ty\u00b7rer", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Gelobt sei die Dreifaltigkeit,", "tokens": ["Ge\u00b7lobt", "sei", "die", "Drei\u00b7fal\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Kraft, Ehr' und Sieg hat sie bereit,", "tokens": ["Kraft", ",", "Ehr'", "und", "Sieg", "hat", "sie", "be\u00b7reit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie gibt die Kron', den Zeugen schon,", "tokens": ["Sie", "gibt", "die", "Kron'", ",", "den", "Zeu\u00b7gen", "schon", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Lust und Freud' im Himmelsthron,", "tokens": ["Mit", "Lust", "und", "Freud'", "im", "Him\u00b7melst\u00b7hron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O junge zarte Martyrer schon.", "tokens": ["O", "jun\u00b7ge", "zar\u00b7te", "Mar\u00b7ty\u00b7rer", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}