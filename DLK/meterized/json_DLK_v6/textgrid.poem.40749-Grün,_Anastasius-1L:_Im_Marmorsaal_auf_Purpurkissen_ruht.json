{"textgrid.poem.40749": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Marmorsaal auf Purpurkissen ruht", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Marmorsaal auf Purpurkissen ruht", "tokens": ["Im", "Mar\u00b7mor\u00b7saal", "auf", "Pur\u00b7pur\u00b7kis\u00b7sen", "ruht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Trajan, der Herrscher Roms und einer Welt;", "tokens": ["Tra\u00b7jan", ",", "der", "Herr\u00b7scher", "Roms", "und", "ei\u00b7ner", "Welt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "NE", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Kreis erles'ner Freunde rings um ihn,", "tokens": ["Ein", "Kreis", "er\u00b7les'\u00b7ner", "Freun\u00b7de", "rings", "um", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die R\u00f6merlippen att'schen Scherzes voll.", "tokens": ["Die", "R\u00f6\u00b7mer\u00b7lip\u00b7pen", "att'\u00b7schen", "Scher\u00b7zes", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was Land und Meer des K\u00f6stlichen erzeugt", "tokens": ["Was", "Land", "und", "Meer", "des", "K\u00f6st\u00b7li\u00b7chen", "er\u00b7zeugt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "NN", "KON", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Vereinigt tr\u00e4gt's der Tisch von Elfenbein;", "tokens": ["Ver\u00b7ei\u00b7nigt", "tr\u00e4gt's", "der", "Tisch", "von", "El\u00b7fen\u00b7bein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Hier perlt im bauch'gen Kelch der Rebe Blut,", "tokens": ["Hier", "perlt", "im", "bauch'\u00b7gen", "Kelch", "der", "Re\u00b7be", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Pomonas Reichthum winkt dort in Kristall,", "tokens": ["Po\u00b7mo\u00b7nas", "Reicht\u00b7hum", "winkt", "dort", "in", "Kris\u00b7tall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Dar\u00fcber schwebt aus P\u00e4stums Rosenflur", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "schwebt", "aus", "P\u00e4s\u00b7tums", "Ro\u00b7sen\u00b7flur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPR", "NE", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der Kranz, verschwiegnen Lauschens Duftsymbol.", "tokens": ["Der", "Kranz", ",", "ver\u00b7schwieg\u00b7nen", "Lau\u00b7schens", "Duf\u00b7tsym\u00b7bol", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Jetzt quillt zum Ohre s\u00fc\u00dfer Saitenklang,", "tokens": ["Jetzt", "quillt", "zum", "Oh\u00b7re", "s\u00fc\u00b7\u00dfer", "Sai\u00b7ten\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Des Mimen schalkhaft Lied erquickt das Herz;", "tokens": ["Des", "Mi\u00b7men", "schalk\u00b7haft", "Lied", "er\u00b7quickt", "das", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Da fa\u00dft Trajan den Becher Feuerweins", "tokens": ["Da", "fa\u00dft", "Tra\u00b7jan", "den", "Be\u00b7cher", "Feu\u00b7er\u00b7weins"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und sch\u00fcttet opfernd ihn zur Erd' und spricht:", "tokens": ["Und", "sch\u00fct\u00b7tet", "op\u00b7fernd", "ihn", "zur", "Erd'", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPER", "APPRART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "\u00bbvor Allem hoch, was Land und Meer erzeugt,", "tokens": ["\u00bb", "vor", "Al\u00b7lem", "hoch", ",", "was", "Land", "und", "Meer", "er\u00b7zeugt", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PIS", "ADJD", "$,", "PWS", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Gepriesen sei der Rebe gold'ner Quell!\u00ab", "tokens": ["Ge\u00b7prie\u00b7sen", "sei", "der", "Re\u00b7be", "gold'\u00b7ner", "Quell", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Von Hymens Altar in das Brautgemach", "tokens": ["Von", "Hy\u00b7mens", "Al\u00b7tar", "in", "das", "Braut\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zieht ein gl\u00fcckselig Paar: der Herrscher Roms", "tokens": ["Zieht", "ein", "gl\u00fcck\u00b7se\u00b7lig", "Paar", ":", "der", "Herr\u00b7scher", "Roms"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJD", "NN", "$.", "ART", "NN", "NE"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Mit Plotina, der hohen Herzensbraut.", "tokens": ["Mit", "Plo\u00b7ti\u00b7na", ",", "der", "ho\u00b7hen", "Her\u00b7zens\u00b7braut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als sie den feuerfarb'gen Schleier hob,", "tokens": ["Als", "sie", "den", "feu\u00b7er\u00b7fa\u00b7rb'\u00b7gen", "Schlei\u00b7er", "hob", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Wie strahlte jetzt ihr br\u00e4utlich Angesicht", "tokens": ["Wie", "strahl\u00b7te", "jetzt", "ihr", "br\u00e4ut\u00b7lich", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In Pracht und Anmut, gleich dem Sonnengott", "tokens": ["In", "Pracht", "und", "An\u00b7mut", ",", "gleich", "dem", "Son\u00b7nen\u00b7gott"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Aus der Umh\u00fcllung purpurnen Gew\u00f6lks!", "tokens": ["Aus", "der", "Um\u00b7h\u00fcl\u00b7lung", "pur\u00b7pur\u00b7nen", "Ge\u00b7w\u00f6lks", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-++-++--+", "measure": "iambic.hexa.chol"}, "line.8": {"text": "Das schwarze Haar umspielt ihr Hals und Brust,", "tokens": ["Das", "schwar\u00b7ze", "Haar", "um\u00b7spielt", "ihr", "Hals", "und", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "In dunklen Locken f\u00e4llt's auf blendend Wei\u00df,", "tokens": ["In", "dunk\u00b7len", "Lo\u00b7cken", "f\u00e4llt's", "auf", "blen\u00b7dend", "Wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "APPR", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wie Rabenfl\u00fcge auf ein Schneegefild!", "tokens": ["Wie", "Ra\u00b7ben\u00b7fl\u00fc\u00b7ge", "auf", "ein", "Schnee\u00b7ge\u00b7fild", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Dann nimmt vom Haupte sie den vollen Kranz", "tokens": ["Dann", "nimmt", "vom", "Haup\u00b7te", "sie", "den", "vol\u00b7len", "Kranz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und reicht ihn l\u00e4chelnd dem Geliebten dar:", "tokens": ["Und", "reicht", "ihn", "l\u00e4\u00b7chelnd", "dem", "Ge\u00b7lieb\u00b7ten", "dar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "\u00bbwie hier die Blumen gl\u00fchn vom Fr\u00fchlingsku\u00df,", "tokens": ["\u00bb", "wie", "hier", "die", "Blu\u00b7men", "gl\u00fchn", "vom", "Fr\u00fch\u00b7lings\u00b7ku\u00df", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOKOM", "ADV", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "So gl\u00fche, treuer nur, f\u00fcr dich mein Herz;", "tokens": ["So", "gl\u00fc\u00b7he", ",", "treu\u00b7er", "nur", ",", "f\u00fcr", "dich", "mein", "Herz", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJA", "ADV", "$,", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Wie hier im Kranz zu Schmuck und Schutz zugleich", "tokens": ["Wie", "hier", "im", "Kranz", "zu", "Schmuck", "und", "Schutz", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPRART", "NN", "APPR", "NN", "KON", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Der Rebe Laub die Bl\u00fcthen all' umschlingt,", "tokens": ["Der", "Re\u00b7be", "Laub", "die", "Bl\u00fc\u00b7then", "all'", "um\u00b7schlingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "NN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "So halte du an mir, wie ich an dir!\u00ab", "tokens": ["So", "hal\u00b7te", "du", "an", "mir", ",", "wie", "ich", "an", "dir", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "$,", "PWAV", "PPER", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Er nimmt den Kranz, dr\u00fcckt ihn ans Herz und ruft:", "tokens": ["Er", "nimmt", "den", "Kranz", ",", "dr\u00fcckt", "ihn", "ans", "Herz", "und", "ruft", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "APPRART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "\u00bbsei mir gegr\u00fc\u00dft, du schmucker Blumenbund,", "tokens": ["\u00bb", "sei", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "du", "schmu\u00b7cker", "Blu\u00b7men\u00b7bund", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "VVPP", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.20": {"text": "Sei mir gepriesen, gr\u00fcnend Rebenlaub!\u00ab", "tokens": ["Sei", "mir", "ge\u00b7prie\u00b7sen", ",", "gr\u00fc\u00b7nend", "Re\u00b7ben\u00b7laub", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "ADJD", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Im hohen Rathe sitzt der Herrscher Roms,", "tokens": ["Im", "ho\u00b7hen", "Ra\u00b7the", "sitzt", "der", "Herr\u00b7scher", "Roms", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Staates V\u00e4ter all' um ihn vereint,", "tokens": ["Des", "Staa\u00b7tes", "V\u00e4\u00b7ter", "all'", "um", "ihn", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIAT", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein tiefer Ernst beseelt den w\u00fcrd'gen Kreis.", "tokens": ["Ein", "tie\u00b7fer", "Ernst", "be\u00b7seelt", "den", "w\u00fcrd'\u00b7gen", "Kreis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbes droht des Parthers wilde Macht aufs Neu',", "tokens": ["\u00bb", "es", "droht", "des", "Par\u00b7thers", "wil\u00b7de", "Macht", "aufs", "Neu'", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Beschlossen ist's: ihn b\u00e4nd'ge blut'ger Kampf!", "tokens": ["Be\u00b7schlos\u00b7sen", "ist's", ":", "ihn", "b\u00e4n\u00b7d'\u00b7ge", "blut'\u00b7ger", "Kampf", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$.", "PPER", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Doch erst zum Gott gen Heliopolis", "tokens": ["Doch", "erst", "zum", "Gott", "gen", "He\u00b7li\u00b7o\u00b7po\u00b7lis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "APPR", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mit Gru\u00df und Gaben mag ein Bote ziehn", "tokens": ["Mit", "Gru\u00df", "und", "Ga\u00b7ben", "mag", "ein", "Bo\u00b7te", "ziehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und fragen: ob und wie aus Sturm und Streit", "tokens": ["Und", "fra\u00b7gen", ":", "ob", "und", "wie", "aus", "Sturm", "und", "Streit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "$.", "KOUS", "KON", "PWAV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Zur ew'gen Stadt zu kehren mir verg\u00f6nnt?\u00ab", "tokens": ["Zur", "ew'\u00b7gen", "Stadt", "zu", "keh\u00b7ren", "mir", "ver\u00b7g\u00f6nnt", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKZU", "VVFIN", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "So sprach Trajan. Ein flinker Bote zieht", "tokens": ["So", "sprach", "Tra\u00b7jan", ".", "Ein", "flin\u00b7ker", "Bo\u00b7te", "zieht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Mit Gru\u00df und Opfern fort zur Sonnenstadt. \u2013", "tokens": ["Mit", "Gru\u00df", "und", "Op\u00b7fern", "fort", "zur", "Son\u00b7nen\u00b7stadt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Manch langer Tag verstrich und wieder sa\u00df", "tokens": ["Manch", "lan\u00b7ger", "Tag", "ver\u00b7strich", "und", "wie\u00b7der", "sa\u00df"], "to