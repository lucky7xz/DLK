{"textgrid.poem.48095": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ganz leise gehst du n\u00e4chtens durch mein Zimmer,", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ganz leise gehst du n\u00e4chtens durch mein Zimmer,", "tokens": ["Ganz", "lei\u00b7se", "gehst", "du", "n\u00e4ch\u00b7tens", "durch", "mein", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "ich h\u00f6re deine Schritte nicht. Ich f\u00fchle", "tokens": ["ich", "h\u00f6\u00b7re", "dei\u00b7ne", "Schrit\u00b7te", "nicht", ".", "Ich", "f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "nur deines Atems welke Rosenschw\u00fcle", "tokens": ["nur", "dei\u00b7nes", "A\u00b7tems", "wel\u00b7ke", "Ro\u00b7sen\u00b7schw\u00fc\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "PWAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und seh von deiner Stirn den fahlen Schimmer", "tokens": ["und", "seh", "von", "dei\u00b7ner", "Stirn", "den", "fah\u00b7len", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "sich phosphorleuchtend in die Nacht ergie\u00dfen,", "tokens": ["sich", "phos\u00b7phor\u00b7leuch\u00b7tend", "in", "die", "Nacht", "er\u00b7gie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "und seh von deiner Stirn wie Blutrubinen", "tokens": ["und", "seh", "von", "dei\u00b7ner", "Stirn", "wie", "Blu\u00b7tru\u00b7bi\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "KOKOM", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "die dunkeln Tropfen auf die Diele flie\u00dfen . . .", "tokens": ["die", "dun\u00b7keln", "Trop\u00b7fen", "auf", "die", "Die\u00b7le", "flie\u00b7\u00dfen", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$.", "$.", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wo kamst du her? \u2013 ich hatte doch die T\u00fcr", "tokens": ["Wo", "kamst", "du", "her", "?", "\u2013", "ich", "hat\u00b7te", "doch", "die", "T\u00fcr"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$.", "$(", "PPER", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "zur Nacht geschlossen, und kein Fenster klang,", "tokens": ["zur", "Nacht", "ge\u00b7schlos\u00b7sen", ",", "und", "kein", "Fens\u00b7ter", "klang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und durch die Scheiben schaut, die vorhanglosen,", "tokens": ["und", "durch", "die", "Schei\u00b7ben", "schaut", ",", "die", "vor\u00b7hang\u00b7lo\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "der scheue Mond, \u2013 wo kamst Du nur herein", "tokens": ["der", "scheu\u00b7e", "Mond", ",", "\u2013", "wo", "kamst", "Du", "nur", "her\u00b7ein"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "$(", "PWAV", "VVFIN", "PPER", "ADV", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "mit deinem wehen, wunden, schleppenden Schritt,", "tokens": ["mit", "dei\u00b7nem", "we\u00b7hen", ",", "wun\u00b7den", ",", "schlep\u00b7pen\u00b7den", "Schritt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVFIN", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "mit deinem Kranz von abgebl\u00fchten Rosen?", "tokens": ["mit", "dei\u00b7nem", "Kranz", "von", "ab\u00b7ge\u00b7bl\u00fch\u00b7ten", "Ro\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "O komm denn, komm!", "tokens": ["O", "komm", "denn", ",", "komm", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "ich will dich nicht versto\u00dfen!", "tokens": ["ich", "will", "dich", "nicht", "ver\u00b7sto\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O komm du! meine Sehnsucht schrie nach dir", "tokens": ["O", "komm", "du", "!", "mei\u00b7ne", "Sehn\u00b7sucht", "schrie", "nach", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$.", "PPOSAT", "NN", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und suchte dich auf unentdeckten Sternen,", "tokens": ["und", "such\u00b7te", "dich", "auf", "un\u00b7ent\u00b7deck\u00b7ten", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "im M\u00e4rchenwald, in blauen Inselfernen,", "tokens": ["im", "M\u00e4r\u00b7chen\u00b7wald", ",", "in", "blau\u00b7en", "In\u00b7sel\u00b7fer\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "ging fehl und irr . . .", "tokens": ["ging", "fehl", "und", "irr", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "ADJD", "KON", "NN", "$.", "$.", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "und heut kommst Du zu mir", "tokens": ["und", "heut", "kommst", "Du", "zu", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "und weilst bei mir und bist mir selig nah!", "tokens": ["und", "weilst", "bei", "mir", "und", "bist", "mir", "se\u00b7lig", "nah", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "KON", "VAFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und wie ich flehend meine Arme breite,", "tokens": ["Und", "wie", "ich", "fle\u00b7hend", "mei\u00b7ne", "Ar\u00b7me", "brei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "PPOSAT", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "weichst du zur\u00fcck und siehst mich seltsam an,", "tokens": ["weichst", "du", "zu\u00b7r\u00fcck", "und", "siehst", "mich", "selt\u00b7sam", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und deine Augen schaun in eine Weite,", "tokens": ["und", "dei\u00b7ne", "Au\u00b7gen", "schaun", "in", "ei\u00b7ne", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "die meine Seele nicht ermessen kann, \u2013", "tokens": ["die", "mei\u00b7ne", "See\u00b7le", "nicht", "er\u00b7mes\u00b7sen", "kann", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPOSAT", "NN", "PTKNEG", "VVINF", "VMFIN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "und schaust mich an und wandelst stumm vor\u00fcber", "tokens": ["und", "schaust", "mich", "an", "und", "wan\u00b7delst", "stumm", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "und gehst \u2013 auch du! \u2013 den Weg nach Golgatha.", "tokens": ["und", "gehst", "\u2013", "auch", "du", "!", "\u2013", "den", "Weg", "nach", "Gol\u00b7ga\u00b7tha", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "ADV", "PPER", "$.", "$(", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}