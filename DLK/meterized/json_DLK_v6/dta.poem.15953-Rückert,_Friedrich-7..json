{"dta.poem.15953": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein altes Wort, die Seele sei ein Licht,", "tokens": ["Es", "ist", "ein", "al\u00b7tes", "Wort", ",", "die", "See\u00b7le", "sei", "ein", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das alles um sich her erleuchtet, sich nur nicht.", "tokens": ["Das", "al\u00b7les", "um", "sich", "her", "er\u00b7leuch\u00b7tet", ",", "sich", "nur", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "APPR", "PRF", "APZR", "VVPP", "$,", "PRF", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Von seinem Glanze wird die Sch\u00f6pfung dir erschlossen,", "tokens": ["Von", "sei\u00b7nem", "Glan\u00b7ze", "wird", "die", "Sch\u00f6p\u00b7fung", "dir", "er\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allein des Lichtes Kern bleibt deinem Blick verschlossen.", "tokens": ["Al\u00b7lein", "des", "Lich\u00b7tes", "Kern", "bleibt", "dei\u00b7nem", "Blick", "ver\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Als wie die Sonne rings mit tausend Stralen sieht,", "tokens": ["Als", "wie", "die", "Son\u00b7ne", "rings", "mit", "tau\u00b7send", "Stra\u00b7len", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "ADV", "APPR", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn ihren Anblick selbst dir ein Gew\u00f6lk entzieht.", "tokens": ["Wenn", "ih\u00b7ren", "An\u00b7blick", "selbst", "dir", "ein", "Ge\u00b7w\u00f6lk", "ent\u00b7zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Zwar vor der Sonne wird der Wolkenflor zerrinnen;", "tokens": ["Zwar", "vor", "der", "Son\u00b7ne", "wird", "der", "Wol\u00b7ken\u00b7flor", "zer\u00b7rin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wird vorm Geiste so die Nacht auch gehn vonhinnen?", "tokens": ["Und", "wird", "vorm", "Geis\u00b7te", "so", "die", "Nacht", "auch", "gehn", "von\u00b7hin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "ADV", "ART", "NN", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Vonhinnen gehet sie, du schaust das Licht der Wonne,", "tokens": ["Von\u00b7hin\u00b7nen", "ge\u00b7het", "sie", ",", "du", "schaust", "das", "Licht", "der", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und siehst, geblendet, nichts, als s\u00e4hst du in die Sonne.", "tokens": ["Und", "siehst", ",", "ge\u00b7blen\u00b7det", ",", "nichts", ",", "als", "s\u00e4hst", "du", "in", "die", "Son\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVPP", "$,", "PIS", "$,", "KOUS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}