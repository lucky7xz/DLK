{"textgrid.poem.41882": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Keine Stimme h\u00f6r' ich schallen,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Keine Stimme h\u00f6r' ich schallen,", "tokens": ["Kei\u00b7ne", "Stim\u00b7me", "h\u00f6r'", "ich", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Keinen Schritt auf dunkler Bahn;", "tokens": ["Kei\u00b7nen", "Schritt", "auf", "dunk\u00b7ler", "Bahn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Selbst der Himmel hat die sch\u00f6nen", "tokens": ["Selbst", "der", "Him\u00b7mel", "hat", "die", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VAFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hellen Aeuglein zugethan.", "tokens": ["Hel\u00b7len", "A\u00b7e\u00b7u\u00b7glein", "zu\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ich nur wache, s\u00fc\u00dfes Leben,", "tokens": ["Ich", "nur", "wa\u00b7che", ",", "s\u00fc\u00b7\u00dfes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaue sehnend in die Nacht,", "tokens": ["Schau\u00b7e", "seh\u00b7nend", "in", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis dein Stern in \u00f6der Ferne", "tokens": ["Bis", "dein", "Stern", "in", "\u00f6\u00b7der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieblich leuchtend mir erwacht.", "tokens": ["Lieb\u00b7lich", "leuch\u00b7tend", "mir", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach nur einmal, nur verstohlen", "tokens": ["Ach", "nur", "ein\u00b7mal", ",", "nur", "ver\u00b7stoh\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "ADV", "ADV", "$,", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dein geliebtes Bild zu sehn,", "tokens": ["Dein", "ge\u00b7lieb\u00b7tes", "Bild", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollt' ich gern im Sturm und Wetter", "tokens": ["Wollt'", "ich", "gern", "im", "Sturm", "und", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "APPRART", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis zum sp\u00e4ten Morgen stehn.", "tokens": ["Bis", "zum", "sp\u00e4\u00b7ten", "Mor\u00b7gen", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Seh' ichs nicht schon ferne leuchten?", "tokens": ["Seh'", "ichs", "nicht", "schon", "fer\u00b7ne", "leuch\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKNEG", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Naht es nicht schon nach und nach?", "tokens": ["Naht", "es", "nicht", "schon", "nach", "und", "nach", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "ADV", "APPR", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, und freundlich h\u00f6r' ich's fl\u00fcstern:", "tokens": ["Ach", ",", "und", "freund\u00b7lich", "h\u00f6r'", "ich's", "fl\u00fcs\u00b7tern", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "ADJD", "VVFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieh, der Freund ist auch noch wach.", "tokens": ["Sieh", ",", "der", "Freund", "ist", "auch", "noch", "wach", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "S\u00fc\u00dfes Wort, geliebte Stimme,", "tokens": ["S\u00fc\u00b7\u00dfes", "Wort", ",", "ge\u00b7lieb\u00b7te", "Stim\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der mein Herz entgegenschl\u00e4gt!", "tokens": ["Der", "mein", "Herz", "ent\u00b7ge\u00b7gen\u00b7schl\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tausend sel'ge Liebesbilder", "tokens": ["Tau\u00b7send", "sel'\u00b7ge", "Lie\u00b7bes\u00b7bil\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["CARD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat dein Hauch mir aufgeregt.", "tokens": ["Hat", "dein", "Hauch", "mir", "auf\u00b7ge\u00b7regt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Alle Sterne seh' ich gl\u00e4nzen", "tokens": ["Al\u00b7le", "Ster\u00b7ne", "seh'", "ich", "gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf der dunkelblauen Bahn,", "tokens": ["Auf", "der", "dun\u00b7kel\u00b7blau\u00b7en", "Bahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und im Herzen hat und droben", "tokens": ["Und", "im", "Her\u00b7zen", "hat", "und", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VAFIN", "KON", "ADV"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Sich der Himmel aufgethan.", "tokens": ["Sich", "der", "Him\u00b7mel", "auf\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Holder Nachhall, wiege freundlich", "tokens": ["Hol\u00b7der", "Nach\u00b7hall", ",", "wie\u00b7ge", "freund\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jetzt mein Haupt in milde Ruh,", "tokens": ["Jetzt", "mein", "Haupt", "in", "mil\u00b7de", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und noch oft, ihr Tr\u00e4ume, lispelt", "tokens": ["Und", "noch", "oft", ",", "ihr", "Tr\u00e4u\u00b7me", ",", "lis\u00b7pelt"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "ADV", "$,", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ihr geliebtes Wort mir zu.", "tokens": ["Ihr", "ge\u00b7lieb\u00b7tes", "Wort", "mir", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}