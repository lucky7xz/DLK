{"textgrid.poem.24984": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Morgentraum", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn m\u00fcde von n\u00e4chtlichem Wachen", "tokens": ["Wenn", "m\u00fc\u00b7de", "von", "n\u00e4cht\u00b7li\u00b7chem", "Wa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Wimper mir sinkt beim Morgenrot,", "tokens": ["Die", "Wim\u00b7per", "mir", "sinkt", "beim", "Mor\u00b7gen\u00b7rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So freundlich in deinem Nachen", "tokens": ["So", "freund\u00b7lich", "in", "dei\u00b7nem", "Na\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wiegst du mich, Schlummer, holder Pilot!", "tokens": ["Wiegst", "du", "mich", ",", "Schlum\u00b7mer", ",", "hol\u00b7der", "Pi\u00b7lot", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Empor aus der Tiefe leise", "tokens": ["Em\u00b7por", "aus", "der", "Tie\u00b7fe", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ADJD"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wallt es zum Ohr mir wie Feengesang,", "tokens": ["Wallt", "es", "zum", "Ohr", "mir", "wie", "Fe\u00b7en\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PPER", "KOKOM", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Und um mich t\u00f6nende Kreise", "tokens": ["Und", "um", "mich", "t\u00f6\u00b7nen\u00b7de", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schlagen die Wellen bei jedem Klang.", "tokens": ["Schla\u00b7gen", "die", "Wel\u00b7len", "bei", "je\u00b7dem", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Mit Duft von Bl\u00fcten beladen,", "tokens": ["Mit", "Duft", "von", "Bl\u00fc\u00b7ten", "be\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die nicht von dieser Erde sind,", "tokens": ["Die", "nicht", "von", "die\u00b7ser", "Er\u00b7de", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "PDAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Her weht von fernen Gestaden", "tokens": ["Her", "weht", "von", "fer\u00b7nen", "Ge\u00b7sta\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mir um die Stirn ein s\u00e4uselnder Wind.", "tokens": ["Mir", "um", "die", "Stirn", "ein", "s\u00e4u\u00b7seln\u00b7der", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Und vor mir die Sonnenpalme,", "tokens": ["Und", "vor", "mir", "die", "Son\u00b7nen\u00b7pal\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Die aus den Wogen auf Felsen ragt,", "tokens": ["Die", "aus", "den", "Wo\u00b7gen", "auf", "Fel\u00b7sen", "ragt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Gr\u00fc\u00dft rauschend im Morgenpsalme", "tokens": ["Gr\u00fc\u00dft", "rau\u00b7schend", "im", "Mor\u00b7gen\u00b7psal\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das Licht, wie es h\u00f6her und h\u00f6her tagt.", "tokens": ["Das", "Licht", ",", "wie", "es", "h\u00f6\u00b7her", "und", "h\u00f6\u00b7her", "tagt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Hinein! In das himmlische Feuer", "tokens": ["Hin\u00b7ein", "!", "In", "das", "himm\u00b7li\u00b7sche", "Feu\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "F\u00fchre hinein mich, trauter Pilot,", "tokens": ["F\u00fch\u00b7re", "hin\u00b7ein", "mich", ",", "trau\u00b7ter", "Pi\u00b7lot", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APZR", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und erzittert die Hand dir am Steuer,", "tokens": ["Und", "er\u00b7zit\u00b7tert", "die", "Hand", "dir", "am", "Steu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "So lenk es dein Zwillingsbruder, der Tod!", "tokens": ["So", "lenk", "es", "dein", "Zwil\u00b7lings\u00b7bru\u00b7der", ",", "der", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}}}}