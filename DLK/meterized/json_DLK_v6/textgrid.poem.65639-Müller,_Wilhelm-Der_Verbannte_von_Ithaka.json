{"textgrid.poem.65639": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Verbannte von Ithaka", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Britten, streicht aus euren Listen meinen Namen nur heraus,", "tokens": ["Brit\u00b7ten", ",", "streicht", "aus", "eu\u00b7ren", "Lis\u00b7ten", "mei\u00b7nen", "Na\u00b7men", "nur", "he\u00b7raus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Bannet mich aus eurem Schutze, la\u00dft verkaufen auch mein Haus!", "tokens": ["Ban\u00b7net", "mich", "aus", "eu\u00b7rem", "Schut\u00b7ze", ",", "la\u00dft", "ver\u00b7kau\u00b7fen", "auch", "mein", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,", "VVIMP", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Selber will ich mich besch\u00fctzen, Gottes Himmel ist mein Dach,", "tokens": ["Sel\u00b7ber", "will", "ich", "mich", "be\u00b7sch\u00fct\u00b7zen", ",", "Got\u00b7tes", "Him\u00b7mel", "ist", "mein", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,", "NN", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und der Freiheit Fahne folg' ich freudig bis zum Tode nach.", "tokens": ["Und", "der", "Frei\u00b7heit", "Fah\u00b7ne", "fol\u00b7g'", "ich", "freu\u00b7dig", "bis", "zum", "To\u00b7de", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "PPER", "ADJD", "APPR", "APPRART", "NN", "PTKVZ", "$."], "meter": "--+-+----+-+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Hab' in ihre Werberolle schon mit meinem eignen Blut", "tokens": ["Hab'", "in", "ih\u00b7re", "Wer\u00b7be\u00b7rol\u00b7le", "schon", "mit", "mei\u00b7nem", "eig\u00b7nen", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Meinen Namen eingeschrieben, und ein Schwert ist all mein Gut.", "tokens": ["Mei\u00b7nen", "Na\u00b7men", "ein\u00b7ge\u00b7schrie\u00b7ben", ",", "und", "ein", "Schwert", "ist", "all", "mein", "Gut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "KON", "ART", "NN", "VAFIN", "PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Britten, hohe Protektoren, fragt ihr nach der Freiheit", "tokens": ["Brit\u00b7ten", ",", "ho\u00b7he", "Pro\u00b7tek\u00b7to\u00b7ren", ",", "fragt", "ihr", "nach", "der", "Frei\u00b7heit"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.8": {"text": "Zuckt ihr zweifelnd eure Achseln, zeigt ihr prahlend euer Gold? \u2013", "tokens": ["Zuckt", "ihr", "zwei\u00b7felnd", "eu\u00b7re", "Ach\u00b7seln", ",", "zeigt", "ihr", "prah\u00b7lend", "eu\u00b7er", "Gold", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Ach, die Freiheit ist auf Erden freilich nur ein armes Weib,", "tokens": ["Ach", ",", "die", "Frei\u00b7heit", "ist", "auf", "Er\u00b7den", "frei\u00b7lich", "nur", "ein", "ar\u00b7mes", "Weib", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "VAFIN", "APPR", "NN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Hat wohl kaum genug, zu kleiden ihren abgezehrten Leib;", "tokens": ["Hat", "wohl", "kaum", "ge\u00b7nug", ",", "zu", "klei\u00b7den", "ih\u00b7ren", "ab\u00b7ge\u00b7zehr\u00b7ten", "Leib", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "$,", "APPR", "ADJA", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Wundenmale, statt der Orden, halten ihre Brust bedeckt,", "tokens": ["Wun\u00b7den\u00b7ma\u00b7le", ",", "statt", "der", "Or\u00b7den", ",", "hal\u00b7ten", "ih\u00b7re", "Brust", "be\u00b7deckt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "ART", "NN", "$,", "VVFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Manchen schn\u00f6den Achtbrief haben ihr Satrapen angesteckt.", "tokens": ["Man\u00b7chen", "schn\u00f6\u00b7den", "Acht\u00b7brief", "ha\u00b7ben", "ihr", "Sa\u00b7tra\u00b7pen", "an\u00b7ge\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Also kam sie aus der Ferne, wei\u00df nicht recht, woher, verbannt,", "tokens": ["Al\u00b7so", "kam", "sie", "aus", "der", "Fer\u00b7ne", ",", "wei\u00df", "nicht", "recht", ",", "wo\u00b7her", ",", "ver\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "VVFIN", "PTKNEG", "ADJD", "$,", "PWAV", "$,", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Und zum Sterben m\u00fcde sank sie hin an des Ilissus Rand.", "tokens": ["Und", "zum", "Ster\u00b7ben", "m\u00fc\u00b7de", "sank", "sie", "hin", "an", "des", "I\u00b7lis\u00b7sus", "Rand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADJD", "VVFIN", "PPER", "ADV", "APPR", "ART", "NE", "NN", "$."], "meter": "--+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Da, da fanden wir sie liegen, und sie schien bekannt uns noch,", "tokens": ["Da", ",", "da", "fan\u00b7den", "wir", "sie", "lie\u00b7gen", ",", "und", "sie", "schien", "be\u00b7kannt", "uns", "noch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "ADJD", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Und wir sahen unsre Ketten, und wir f\u00fchlten unser Joch.", "tokens": ["Und", "wir", "sa\u00b7hen", "uns\u00b7re", "Ket\u00b7ten", ",", "und", "wir", "f\u00fchl\u00b7ten", "un\u00b7ser", "Joch", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "KON", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Flugs erwachte sie vom Schlummer, schwang sich in die Luft empor,", "tokens": ["Flugs", "er\u00b7wach\u00b7te", "sie", "vom", "Schlum\u00b7mer", ",", "schwang", "sich", "in", "die", "Luft", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.18": {"text": "Und in G\u00f6tterjugend strahlend stand sie vor Minervens Thor.", "tokens": ["Und", "in", "G\u00f6t\u00b7ter\u00b7ju\u00b7gend", "strah\u00b7lend", "stand", "sie", "vor", "Mi\u00b7ner\u00b7vens", "Thor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADJD", "VVFIN", "PPER", "APPR", "NN", "NN", "$."], "meter": "--+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Wie so froh sie auf die alten Narben ihres Leibes wies!", "tokens": ["Wie", "so", "froh", "sie", "auf", "die", "al\u00b7ten", "Nar\u00b7ben", "ih\u00b7res", "Lei\u00b7bes", "wies", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "PPER", "APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.20": {"text": "Wie so stolz ihr Auge suchte Marathon und Salamis!", "tokens": ["Wie", "so", "stolz", "ihr", "Au\u00b7ge", "such\u00b7te", "Ma\u00b7ra\u00b7thon", "und", "Sa\u00b7la\u00b7mis", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "PPOSAT", "NN", "VVFIN", "NE", "KON", "NE", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.21": {"text": "Da zerrissen wir die Ketten, brachen jedes Joch entzwei,", "tokens": ["Da", "zer\u00b7ris\u00b7sen", "wir", "die", "Ket\u00b7ten", ",", "bra\u00b7chen", "je\u00b7des", "Joch", "ent\u00b7zwei", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.22": {"text": "Und sie sprach: Seid werth der Freiheit, und ihr seid auf ewig frei,", "tokens": ["Und", "sie", "sprach", ":", "Seid", "werth", "der", "Frei\u00b7heit", ",", "und", "ihr", "seid", "auf", "e\u00b7wig", "frei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "VAIMP", "ADJD", "ART", "NN", "$,", "KON", "PPER", "VAFIN", "APPR", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.23": {"text": "Frei wie in Thessaliens P\u00e4ssen", "tokens": ["Frei", "wie", "in", "Thes\u00b7sa\u00b7li\u00b7ens", "P\u00e4s\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "APPR", "NE", "NE"], "meter": "+--++--+-", "measure": "trochaic.tetra.relaxed"}, "line.24": {"text": "Frei wie \u00fcber Erdennebel kreist im Sonnenstrahl der Aar.", "tokens": ["Frei", "wie", "\u00fc\u00b7ber", "Er\u00b7den\u00b7ne\u00b7bel", "kreist", "im", "Son\u00b7nen\u00b7strahl", "der", "Aar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "APPR", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+---+-+-+-+", "measure": "unknown.measure.septa"}}}}}