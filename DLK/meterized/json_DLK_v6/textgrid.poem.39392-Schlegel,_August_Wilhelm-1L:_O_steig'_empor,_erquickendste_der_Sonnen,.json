{"textgrid.poem.39392": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: O steig' empor, erquickendste der Sonnen,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O steig' empor, erquickendste der Sonnen,", "tokens": ["O", "steig'", "em\u00b7por", ",", "er\u00b7qui\u00b7ckends\u00b7te", "der", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Des sch\u00f6nen Tages hohe Heroldin!", "tokens": ["Des", "sch\u00f6\u00b7nen", "Ta\u00b7ges", "ho\u00b7he", "He\u00b7rol\u00b7din", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Bestrahle glorreich dieses Festes Wonnen,", "tokens": ["Be\u00b7strah\u00b7le", "glor\u00b7reich", "die\u00b7ses", "Fes\u00b7tes", "Won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie zu erblicken werde dein Gewinn.", "tokens": ["Sie", "zu", "er\u00b7bli\u00b7cken", "wer\u00b7de", "dein", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Himmel, in entw\u00f6lktes Blau zerronnen,", "tokens": ["Der", "Him\u00b7mel", ",", "in", "ent\u00b7w\u00f6lk\u00b7tes", "Blau", "zer\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sei frei und heiter wie des Volkes Sinn.", "tokens": ["Sei", "frei", "und", "hei\u00b7ter", "wie", "des", "Vol\u00b7kes", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KON", "ADJD", "KOKOM", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sieh! Weihrauch dampft von tausend Dankalt\u00e4ren,", "tokens": ["Sieh", "!", "Weih\u00b7rauch", "dampft", "von", "tau\u00b7send", "Dan\u00b7kal\u00b7t\u00e4\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "VVFIN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und lauter Jubel schallt bis an die Sph\u00e4ren.", "tokens": ["Und", "lau\u00b7ter", "Ju\u00b7bel", "schallt", "bis", "an", "die", "Sph\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein edler K\u00f6nig ist der Welt gegeben,", "tokens": ["Ein", "ed\u00b7ler", "K\u00f6\u00b7nig", "ist", "der", "Welt", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Vaterland schw\u00f6rt ihm den heil'gen Bund.", "tokens": ["Das", "Va\u00b7ter\u00b7land", "schw\u00f6rt", "ihm", "den", "heil'\u00b7gen", "Bund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.3": {"text": "Nicht Einer, der mit innerm Widerstreben", "tokens": ["Nicht", "Ei\u00b7ner", ",", "der", "mit", "in\u00b7nerm", "Wi\u00b7der\u00b7stre\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "PIS", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihm Treu' gelobt aus heuchlerischem Mund.", "tokens": ["Ihm", "Treu'", "ge\u00b7lobt", "aus", "heuch\u00b7le\u00b7ri\u00b7schem", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gef\u00fchle, die in Aller Herzen leben,", "tokens": ["Ge\u00b7f\u00fch\u00b7le", ",", "die", "in", "Al\u00b7ler", "Her\u00b7zen", "le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Macht unwillk\u00fcrlich jede Lippe kund;", "tokens": ["Macht", "un\u00b7will\u00b7k\u00fcr\u00b7lich", "je\u00b7de", "Lip\u00b7pe", "kund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Durch eine himmlische Gewalt gedrungen", "tokens": ["Durch", "ei\u00b7ne", "himm\u00b7li\u00b7sche", "Ge\u00b7walt", "ge\u00b7drun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zu innigen, entz\u00fcckten Huldigungen.", "tokens": ["Zu", "in\u00b7ni\u00b7gen", ",", "ent\u00b7z\u00fcck\u00b7ten", "Hul\u00b7di\u00b7gun\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ihr kennt Ihn ja, ihr frohen Millionen,", "tokens": ["Ihr", "kennt", "Ihn", "ja", ",", "ihr", "fro\u00b7hen", "Mil\u00b7lion\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihr saht Ihn sichern Tritts zum Ziele gehn.", "tokens": ["Ihr", "saht", "Ihn", "si\u00b7chern", "Tritts", "zum", "Zie\u00b7le", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sein Thun verdient Ihm angestammte Kronen,", "tokens": ["Sein", "Thun", "ver\u00b7dient", "Ihm", "an\u00b7ge\u00b7stamm\u00b7te", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der V\u00f6lker Wahl w\u00fcrd' Ihn dazu erh\u00f6hn,", "tokens": ["Der", "V\u00f6l\u00b7ker", "Wahl", "w\u00fcrd'", "Ihn", "da\u00b7zu", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bei dem die Geister gro\u00dfer Ahnen wohnen,", "tokens": ["Bei", "dem", "die", "Geis\u00b7ter", "gro\u00b7\u00dfer", "Ah\u00b7nen", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In dessen Bild sie neu verj\u00fcngt sich sehn;", "tokens": ["In", "des\u00b7sen", "Bild", "sie", "neu", "ver\u00b7j\u00fcngt", "sich", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "ADJD", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Den k\u00f6niglichen Mann, gerecht und g\u00fctig,", "tokens": ["Den", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Mann", ",", "ge\u00b7recht", "und", "g\u00fc\u00b7tig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit W\u00fcrde mild, mit Ruhe heldenm\u00fcthig.", "tokens": ["Mit", "W\u00fcr\u00b7de", "mild", ",", "mit", "Ru\u00b7he", "hel\u00b7den\u00b7m\u00fct\u00b7hig", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie k\u00f6nnte je sich Ihm der Himmel schw\u00e4rzen?", "tokens": ["Wie", "k\u00f6nn\u00b7te", "je", "sich", "Ihm", "der", "Him\u00b7mel", "schw\u00e4r\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "PRF", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er sucht' und fand der Liebe sch\u00f6nsten Lohn.", "tokens": ["Er", "sucht'", "und", "fand", "der", "Lie\u00b7be", "sch\u00f6ns\u00b7ten", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Louisens L\u00e4cheln hei\u00dft den Kummer scherzen,", "tokens": ["Lou\u00b7i\u00b7sens", "L\u00e4\u00b7cheln", "hei\u00dft", "den", "Kum\u00b7mer", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor Ihrem Blick ist jedes Leid entflohn.", "tokens": ["Vor", "Ih\u00b7rem", "Blick", "ist", "je\u00b7des", "Leid", "ent\u00b7flohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie w\u00e4r' in H\u00fctten K\u00f6nigin der Herzen,", "tokens": ["Sie", "w\u00e4r'", "in", "H\u00fct\u00b7ten", "K\u00f6\u00b7ni\u00b7gin", "der", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie ist der Anmuth G\u00f6ttin auf dem Thron;", "tokens": ["Sie", "ist", "der", "An\u00b7muth", "G\u00f6t\u00b7tin", "auf", "dem", "Thron", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ihr zartes Werk, Ihr seligstes Gelingen,", "tokens": ["Ihr", "zar\u00b7tes", "Werk", ",", "Ihr", "se\u00b7ligs\u00b7tes", "Ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "In Seinen Lorbeer Myrten einzuschlingen.", "tokens": ["In", "Sei\u00b7nen", "Lor\u00b7beer", "Myr\u00b7ten", "ein\u00b7zu\u00b7schlin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Das Schicksal wird sein seltnes Wunder pflegen,", "tokens": ["Das", "Schick\u00b7sal", "wird", "sein", "selt\u00b7nes", "Wun\u00b7der", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir d\u00fcrfen still der dunkeln Zukunft traun:", "tokens": ["Wir", "d\u00fcr\u00b7fen", "still", "der", "dun\u00b7keln", "Zu\u00b7kunft", "traun", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der heil'ge Lorbeer trotzt des Blitzes Schl\u00e4gen,", "tokens": ["Der", "heil'\u00b7ge", "Lor\u00b7beer", "trotzt", "des", "Blit\u00b7zes", "Schl\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Myrte spro\u00dfet nur auf Blumenau'n;", "tokens": ["Die", "Myr\u00b7te", "spro\u00b7\u00dfet", "nur", "auf", "Blu\u00b7me\u00b7nau'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die hohen M\u00e4chte la\u00dfen alle Segen", "tokens": ["Die", "ho\u00b7hen", "M\u00e4ch\u00b7te", "la\u00b7\u00dfen", "al\u00b7le", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In Purpurwolken auf sie niederthaun;", "tokens": ["In", "Pur\u00b7pur\u00b7wol\u00b7ken", "auf", "sie", "nie\u00b7dert\u00b7haun", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Friede tr\u00e4gt die immer gr\u00fcne Palme,", "tokens": ["Der", "Frie\u00b7de", "tr\u00e4gt", "die", "im\u00b7mer", "gr\u00fc\u00b7ne", "Pal\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und Ueberflu\u00df neigt seine schweren Halme.", "tokens": ["Und", "Ue\u00b7berf\u00b7lu\u00df", "neigt", "sei\u00b7ne", "schwe\u00b7ren", "Hal\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Es blickt auf Euch die Muse der Geschichte,", "tokens": ["Es", "blickt", "auf", "Euch", "die", "Mu\u00b7se", "der", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erhabner Herrscher! holde K\u00f6nigin!", "tokens": ["Er\u00b7hab\u00b7ner", "Herr\u00b7scher", "!", "hol\u00b7de", "K\u00f6\u00b7ni\u00b7gin", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr strenges Zeugni\u00df wird zum Lobgedichte;", "tokens": ["Ihr", "stren\u00b7ges", "Zeug\u00b7ni\u00df", "wird", "zum", "Lob\u00b7ge\u00b7dich\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie ist der goldnen Zeit Verk\u00fcnderin.", "tokens": ["Sie", "ist", "der", "gold\u00b7nen", "Zeit", "Ver\u00b7k\u00fcn\u00b7de\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ach! j\u00fcngst noch sah sie grausende Gesichte,", "tokens": ["Ach", "!", "j\u00fcngst", "noch", "sah", "sie", "grau\u00b7sen\u00b7de", "Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Griffel sank als Dolch ihr blutig hin.", "tokens": ["Der", "Grif\u00b7fel", "sank", "als", "Dolch", "ihr", "blu\u00b7tig", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOUS", "NN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So schritt sie ernst auf tragischem Kothurne,", "tokens": ["So", "schritt", "sie", "ernst", "auf", "tra\u00b7gi\u00b7schem", "Ko\u00b7thur\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und ruhte sinnnend an der Menschheit Urne.", "tokens": ["Und", "ruh\u00b7te", "sinn\u00b7nend", "an", "der", "Menschheit", "Ur\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["K