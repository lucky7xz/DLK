{"textgrid.poem.67920": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "16. Erinnerung des Gesanges der Vorzeit", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "R\u00fchr Saite, du Sohn Alpins des Gesangs,", "tokens": ["R\u00fchr", "Sai\u00b7te", ",", "du", "Sohn", "Al\u00b7pins", "des", "Ge\u00b7sangs", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "NN", "NE", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wohnt Trost in deiner Harfe der L\u00fcfte?", "tokens": ["Wohnt", "Trost", "in", "dei\u00b7ner", "Har\u00b7fe", "der", "L\u00fcf\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Geu\u00df \u00fcber Ossian, den Traurigen, sie,", "tokens": ["Geu\u00df", "\u00fc\u00b7ber", "Os\u00b7si\u00b7an", ",", "den", "Trau\u00b7ri\u00b7gen", ",", "sie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "NE", "$,", "ART", "NN", "$,", "PPER", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Dem Nebel einh\u00fcllen die Seele.", "tokens": ["Dem", "Ne\u00b7bel", "ein\u00b7h\u00fcl\u00b7len", "die", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Ich h\u00f6r dich Bard' in meiner Nacht,", "tokens": ["Ich", "h\u00f6r", "dich", "Bard'", "in", "mei\u00b7ner", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Halt an die Saite, die zitternde", "tokens": ["Halt", "an", "die", "Sai\u00b7te", ",", "die", "zit\u00b7tern\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "APPR", "ART", "NN", "$,", "ART", "ADJA"], "meter": "+--+--+--", "measure": "dactylic.tri"}, "line.3": {"text": "Der Wehmuth Freude geb\u00fchret Ossian,", "tokens": ["Der", "Weh\u00b7muth", "Freu\u00b7de", "ge\u00b7b\u00fch\u00b7ret", "Os\u00b7si\u00b7an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "NE", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "In seinen braunen Jahren.", "tokens": ["In", "sei\u00b7nen", "brau\u00b7nen", "Jah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gr\u00fcndorn, auf dem H\u00fcgel der Geister,", "tokens": ["Gr\u00fcn\u00b7dorn", ",", "auf", "dem", "H\u00fc\u00b7gel", "der", "Geis\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Webend das Haupt in Stimmen der Nacht,", "tokens": ["We\u00b7bend", "das", "Haupt", "in", "Stim\u00b7men", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "ART", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ich sp\u00fcre ja deinen Laut nicht,", "tokens": ["Ich", "sp\u00fc\u00b7re", "ja", "dei\u00b7nen", "Laut", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "APPR", "PTKNEG", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Geistergewand nicht rauschend im Laube dir.", "tokens": ["Geis\u00b7ter\u00b7ge\u00b7wand", "nicht", "rau\u00b7schend", "im", "Lau\u00b7be", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADJD", "APPRART", "NN", "PPER", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Oft sind die Tritte der Todten,", "tokens": ["Oft", "sind", "die", "Trit\u00b7te", "der", "Tod\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Auf L\u00fcftchen im kreisenden Sturm.", "tokens": ["Auf", "L\u00fcft\u00b7chen", "im", "krei\u00b7sen\u00b7den", "Sturm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wenn schwimmt von Osten der Mond,", "tokens": ["Wenn", "schwimmt", "von", "Os\u00b7ten", "der", "Mond", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Ein blasser Schild, ziehend den Himmel hindurch.", "tokens": ["Ein", "blas\u00b7ser", "Schild", ",", "zie\u00b7hend", "den", "Him\u00b7mel", "hin\u00b7durch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP", "ART", "NN", "PAV", "$."], "meter": "-+-++--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Ullin und Carril und Raono,", "tokens": ["Ul\u00b7lin", "und", "Car\u00b7ril", "und", "Rao\u00b7no", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "KON", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vergangne Stimmen der Tage vor Alters,", "tokens": ["Ver\u00b7gang\u00b7ne", "Stim\u00b7men", "der", "Ta\u00b7ge", "vor", "Al\u00b7ters", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "H\u00f6rt' ich Euch im Dunkel von Selma;", "tokens": ["H\u00f6rt'", "ich", "Euch", "im", "Dun\u00b7kel", "von", "Sel\u00b7ma", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPRART", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Es erh\u00fcbe die Seele des Lieds.", "tokens": ["Es", "er\u00b7h\u00fc\u00b7be", "die", "See\u00b7le", "des", "Lieds", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.6": {"line.1": {"text": "Nicht h\u00f6r' ich euch, S\u00f6hne des Gesangs,", "tokens": ["Nicht", "h\u00f6r'", "ich", "euch", ",", "S\u00f6h\u00b7ne", "des", "Ge\u00b7sangs", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In welcher Wohnung der Wolken ist eure Ruh?", "tokens": ["In", "wel\u00b7cher", "Woh\u00b7nung", "der", "Wol\u00b7ken", "ist", "eu\u00b7re", "Ruh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-++-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "R\u00fchrt ihr die Harfe, die d\u00fcstre,", "tokens": ["R\u00fchrt", "ihr", "die", "Har\u00b7fe", ",", "die", "d\u00fcst\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Geh\u00fcllt in Morgengrau,", "tokens": ["Ge\u00b7h\u00fcllt", "in", "Mor\u00b7gen\u00b7grau", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo aufsteigt t\u00f6nend die Sonne,", "tokens": ["Wo", "auf\u00b7steigt", "t\u00f6\u00b7nend", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "ADJD", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Von Wellen, die H\u00e4upter blau?", "tokens": ["Von", "Wel\u00b7len", ",", "die", "H\u00e4up\u00b7ter", "blau", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}