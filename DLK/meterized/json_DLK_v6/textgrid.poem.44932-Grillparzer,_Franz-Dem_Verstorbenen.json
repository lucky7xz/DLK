{"textgrid.poem.44932": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Dem Verstorbenen", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bist du der Meinung, da\u00df nicht Wert,", "tokens": ["Bist", "du", "der", "Mei\u00b7nung", ",", "da\u00df", "nicht", "Wert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "KOUS", "PTKNEG", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nein, sechzehn Ahnen adeln,", "tokens": ["Nein", ",", "sech\u00b7zehn", "Ah\u00b7nen", "a\u00b7deln", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Erhabner F\u00fcrst, dann kannst du mich \u2013", "tokens": ["Er\u00b7hab\u00b7ner", "F\u00fcrst", ",", "dann", "kannst", "du", "mich", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "VMFIN", "PPER", "PRF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch loben oder tadeln.", "tokens": ["Auch", "lo\u00b7ben", "o\u00b7der", "ta\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Des F\u00fcrsten und des Schreibers Amt", "tokens": ["Des", "F\u00fcrs\u00b7ten", "und", "des", "Schrei\u00b7bers", "Amt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sind also ihm bemessen:", "tokens": ["Sind", "al\u00b7so", "ihm", "be\u00b7mes\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Edelmann gibt dem Schreiber Rang,", "tokens": ["Der", "E\u00b7del\u00b7mann", "gibt", "dem", "Schrei\u00b7ber", "Rang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Schreiber dem Edeln zu essen.", "tokens": ["Der", "Schrei\u00b7ber", "dem", "E\u00b7deln", "zu", "es\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Die Sorge um den Stammbaum f\u00fchrt", "tokens": ["Die", "Sor\u00b7ge", "um", "den", "Stamm\u00b7baum", "f\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihn rings umher von Ips bis W\u00e4hring,", "tokens": ["Ihn", "rings", "um\u00b7her", "von", "Ips", "bis", "W\u00e4h\u00b7ring", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKVZ", "APPR", "NE", "APPR", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ob P\u00fcckler her von P\u00f6chlarn r\u00fchrt,", "tokens": ["Ob", "P\u00fcck\u00b7ler", "her", "von", "P\u00f6ch\u00b7larn", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von P\u00f6ckeln oder Pickelhering.", "tokens": ["Von", "P\u00f6\u00b7ckeln", "o\u00b7der", "Pi\u00b7ckel\u00b7he\u00b7ring", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}