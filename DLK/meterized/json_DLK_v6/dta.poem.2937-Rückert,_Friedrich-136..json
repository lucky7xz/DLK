{"dta.poem.2937": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "136.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Schlimme l\u00e4\u00dft nicht gut sich machen, aber immer", "tokens": ["Das", "Schlim\u00b7me", "l\u00e4\u00dft", "nicht", "gut", "sich", "ma\u00b7chen", ",", "a\u00b7ber", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADJD", "PRF", "VVINF", "$,", "ADV", "ADV"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Ertr\u00e4glich durch Vernunft, und durch Unweisheit schlimmer.", "tokens": ["Er\u00b7tr\u00e4g\u00b7lich", "durch", "Ver\u00b7nunft", ",", "und", "durch", "Un\u00b7weis\u00b7heit", "schlim\u00b7mer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "KON", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Weis' ist, wer, so gut es gieng, zurecht sich machte", "tokens": ["Der", "Weis'", "ist", ",", "wer", ",", "so", "gut", "es", "gieng", ",", "zu\u00b7recht", "sich", "mach\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "PWS", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$,", "VVFIN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Lag', in die er sich, in die das Gl\u00fcck ihn brachte.", "tokens": ["Die", "Lag'", ",", "in", "die", "er", "sich", ",", "in", "die", "das", "Gl\u00fcck", "ihn", "brach\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "PRF", "$,", "APPR", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Bracht' ihn das Gl\u00fcck hinein, so bring' er sich heraus;", "tokens": ["Bracht'", "ihn", "das", "Gl\u00fcck", "hin\u00b7ein", ",", "so", "bring'", "er", "sich", "he\u00b7raus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bracht' er selber sich hinein, so halt' er aus.", "tokens": ["Und", "bracht'", "er", "sel\u00b7ber", "sich", "hin\u00b7ein", ",", "so", "halt'", "er", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PRF", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}