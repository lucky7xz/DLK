{"textgrid.poem.58915": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer von sich selbst will werden frei,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer von sich selbst will werden frei,", "tokens": ["Wer", "von", "sich", "selbst", "will", "wer\u00b7den", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PRF", "ADV", "VMFIN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der mu\u00df sich selber sehr vergessen;", "tokens": ["Der", "mu\u00df", "sich", "sel\u00b7ber", "sehr", "ver\u00b7ges\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PRF", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denk nur an Gott und senk dich ein", "tokens": ["Denk", "nur", "an", "Gott", "und", "senk", "dich", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "APPR", "NN", "KON", "VVFIN", "PRF", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In dieses Wesen unermessen!", "tokens": ["In", "die\u00b7ses", "We\u00b7sen", "un\u00b7er\u00b7mes\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}