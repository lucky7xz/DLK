{"textgrid.poem.60805": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Namen der Trefflichen wurden an schm\u00e4hlichen Galgen geheftet,", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Namen der Trefflichen wurden an schm\u00e4hlichen Galgen geheftet,", "tokens": ["Na\u00b7men", "der", "Treff\u00b7li\u00b7chen", "wur\u00b7den", "an", "schm\u00e4h\u00b7li\u00b7chen", "Gal\u00b7gen", "ge\u00b7hef\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VAFIN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+--+-+---+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Weil sie, den Polen vereint, tapfer, die Polen, gek\u00e4mpft;", "tokens": ["Weil", "sie", ",", "den", "Po\u00b7len", "ver\u00b7eint", ",", "tap\u00b7fer", ",", "die", "Po\u00b7len", ",", "ge\u00b7k\u00e4mpft", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "NN", "VVPP", "$,", "ADJD", "$,", "ART", "NN", "$,", "VVPP", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Aber das Volk nahm, ging es vorbei, vor dem Galgen den Hut ab,", "tokens": ["A\u00b7ber", "das", "Volk", "nahm", ",", "ging", "es", "vor\u00b7bei", ",", "vor", "dem", "Gal\u00b7gen", "den", "Hut", "ab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+-+--++", "measure": "iambic.octa.plus.invert"}, "line.4": {"text": "Ja, bei n\u00e4chtlicher Zeit ward er mit Blumen bekr\u00e4nzt.", "tokens": ["Ja", ",", "bei", "n\u00e4cht\u00b7li\u00b7cher", "Zeit", "ward", "er", "mit", "Blu\u00b7men", "be\u00b7kr\u00e4nzt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPR", "ADJA", "NN", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}}}}}