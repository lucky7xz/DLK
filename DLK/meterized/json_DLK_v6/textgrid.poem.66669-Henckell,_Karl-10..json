{"textgrid.poem.66669": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und ward ein Bl\u00fchen an dem Rosenbusche,", "tokens": ["Und", "ward", "ein", "Bl\u00fc\u00b7hen", "an", "dem", "Ro\u00b7sen\u00b7bu\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df besser mir kein Paradies gef\u00e4llt,", "tokens": ["Da\u00df", "bes\u00b7ser", "mir", "kein", "Pa\u00b7ra\u00b7dies", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Womit ich der Natur ins Handwerk pfusche,", "tokens": ["Wo\u00b7mit", "ich", "der", "Na\u00b7tur", "ins", "Hand\u00b7werk", "pfu\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Phantastisch dichtend eine \u00bbsch\u00f6nere\u00ab Welt ...", "tokens": ["Phan\u00b7tas\u00b7tisch", "dich\u00b7tend", "ei\u00b7ne", "\u00bb", "sch\u00f6\u00b7ne\u00b7re", "\u00ab", "Welt", "..."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "ADJD", "ART", "$(", "ADJA", "$(", "NN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Was gibt es k\u00f6stlicher als eine Dusche?", "tokens": ["Was", "gibt", "es", "k\u00f6st\u00b7li\u00b7cher", "als", "ei\u00b7ne", "Du\u00b7sche", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Hat Adam wonniger sich dem Weib gesellt?", "tokens": ["Hat", "A\u00b7dam", "won\u00b7ni\u00b7ger", "sich", "dem", "Weib", "ge\u00b7sellt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJD", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Umspielten sie noch bl\u00fchendere Ranken,", "tokens": ["Um\u00b7spiel\u00b7ten", "sie", "noch", "bl\u00fc\u00b7hen\u00b7de\u00b7re", "Ran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wenn tief im Neglig\u00e9 sie Fruchtsaft tranken?", "tokens": ["Wenn", "tief", "im", "Ne\u00b7gli\u00b7g\u00e9", "sie", "Fruch\u00b7tsaft", "tran\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPRART", "NN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das Paradies und die gesamte H\u00f6lle", "tokens": ["Das", "Pa\u00b7ra\u00b7dies", "und", "die", "ge\u00b7sam\u00b7te", "H\u00f6l\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind Tag f\u00fcr Tag auf dieser Welt daheim,", "tokens": ["Sind", "Tag", "f\u00fcr", "Tag", "auf", "die\u00b7ser", "Welt", "da\u00b7heim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "NN", "APPR", "PDAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da braucht's kein unterirdisches Ger\u00f6lle", "tokens": ["Da", "braucht's", "kein", "un\u00b7ter\u00b7ir\u00b7di\u00b7sches", "Ge\u00b7r\u00f6l\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und keinen \u00fcberirdischen Honigseim.", "tokens": ["Und", "kei\u00b7nen", "\u00fc\u00b7be\u00b7rir\u00b7di\u00b7schen", "Ho\u00b7ni\u00b7gseim", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Wem der Gemarterten Geschrei ersch\u00f6lle,", "tokens": ["Wem", "der", "Ge\u00b7mar\u00b7ter\u00b7ten", "Ge\u00b7schrei", "er\u00b7sch\u00f6l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dem kl\u00e4ng' es grauser als aus Dantes Reim,", "tokens": ["Dem", "kl\u00e4ng'", "es", "grau\u00b7ser", "als", "aus", "Dan\u00b7tes", "Reim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "KOKOM", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und staunend w\u00fcrde Miltons Satan lachen,", "tokens": ["Und", "stau\u00b7nend", "w\u00fcr\u00b7de", "Mil\u00b7tons", "Sa\u00b7tan", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "NE", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "S\u00e4h er Lyddit- und Splitterbomben krachen.", "tokens": ["S\u00e4h", "er", "Lyd\u00b7dit", "und", "Split\u00b7ter\u00b7bom\u00b7ben", "kra\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "TRUNC", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Schl\u00e4gt ewig Kains Neid auf Bruder Abel,", "tokens": ["Schl\u00e4gt", "e\u00b7wig", "Kains", "Neid", "auf", "Bru\u00b7der", "A\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "NN", "APPR", "NN", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Weil gottgesegneter sein Opfer dampft?", "tokens": ["Weil", "gott\u00b7ge\u00b7se\u00b7gne\u00b7ter", "sein", "Op\u00b7fer", "dampft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wird trotz der internationalen Kabel", "tokens": ["Wird", "trotz", "der", "in\u00b7ter\u00b7na\u00b7ti\u00b7o\u00b7na\u00b7len", "Ka\u00b7bel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die Saat vom Ungeheuer Krieg zerstampft?", "tokens": ["Die", "Saat", "vom", "Un\u00b7ge\u00b7heu\u00b7er", "Krieg", "zer\u00b7stampft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Schlachtengeier wetzt voll Gier den Schnabel,", "tokens": ["Der", "Schlach\u00b7ten\u00b7gei\u00b7er", "wetzt", "voll", "Gier", "den", "Schna\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df sich vor Weh das Herz zusammenkrampft \u2013", "tokens": ["Da\u00df", "sich", "vor", "Weh", "das", "Herz", "zu\u00b7sam\u00b7men\u00b7krampft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPR", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es w\u00fchlt der Ha\u00df von Zone fort zu Zone,", "tokens": ["Es", "w\u00fchlt", "der", "Ha\u00df", "von", "Zo\u00b7ne", "fort", "zu", "Zo\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NE", "PTKVZ", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dumpf heult die menschenhungrige Kanone.", "tokens": ["Dumpf", "heult", "die", "men\u00b7schen\u00b7hung\u00b7ri\u00b7ge", "Ka\u00b7no\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Noch h\u00e4lt der rohe Wahnsinn rachesch\u00e4umend", "tokens": ["Noch", "h\u00e4lt", "der", "ro\u00b7he", "Wahn\u00b7sinn", "ra\u00b7ch\u00b7e\u00b7sch\u00e4u\u00b7mend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bluternte, bis die Scheuern \u00fcbervoll,", "tokens": ["Blu\u00b7tern\u00b7te", ",", "bis", "die", "Scheu\u00b7ern", "\u00fc\u00b7ber\u00b7voll", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "ADJD", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "In Kn\u00e4ueln packt sich durcheinanderb\u00e4umend", "tokens": ["In", "Kn\u00e4u\u00b7eln", "packt", "sich", "durch\u00b7ein\u00b7an\u00b7der\u00b7b\u00e4u\u00b7mend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PRF", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der z\u00e4hnefletschend eingefleischte Groll.", "tokens": ["Der", "z\u00e4h\u00b7ne\u00b7flet\u00b7schend", "ein\u00b7ge\u00b7fleischte", "Groll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Was weltvers\u00f6hnend, v\u00f6lkerfriedetr\u00e4umend,", "tokens": ["Was", "welt\u00b7ver\u00b7s\u00f6h\u00b7nend", ",", "v\u00f6l\u00b7ker\u00b7frie\u00b7de\u00b7tr\u00e4u\u00b7mend", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PWS", "ADJD", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Moloch verschlingt es, blindhinw\u00fctend, toll,", "tokens": ["Mo\u00b7loch", "ver\u00b7schlingt", "es", ",", "blind\u00b7hin\u00b7w\u00fc\u00b7tend", ",", "toll", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Gef\u00fcttert mit Verleumdung, l\u00fcgenstrotzend,", "tokens": ["Ge\u00b7f\u00fct\u00b7tert", "mit", "Ver\u00b7leum\u00b7dung", ",", "l\u00fc\u00b7gen\u00b7strot\u00b7zend", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Auf Leichenpyramiden grausig glotzend.", "tokens": ["Auf", "Lei\u00b7chen\u00b7py\u00b7ra\u00b7mi\u00b7den", "grau\u00b7sig", "glot\u00b7zend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch Heer und Flotte sind die Heiligt\u00fcmer", "tokens": ["Doch", "Heer", "und", "Flot\u00b7te", "sind", "die", "Hei\u00b7lig\u00b7t\u00fc\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom europ\u00e4ischen Weltmachtkontinent,", "tokens": ["Vom", "eu\u00b7ro\u00b7p\u00e4\u00b7i\u00b7schen", "Welt\u00b7macht\u00b7kon\u00b7ti\u00b7nent", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Fall nieder, Mensch, und sei ein frommer R\u00fchmer", "tokens": ["Fall", "nie\u00b7der", ",", "Mensch", ",", "und", "sei", "ein", "from\u00b7mer", "R\u00fch\u00b7mer"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "NN", "$,", "KON", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der gro\u00dfen G\u00f6tzen, die mit Furcht man nennt!", "tokens": ["Der", "gro\u00b7\u00dfen", "G\u00f6t\u00b7zen", ",", "die", "mit", "Furcht", "man", "nennt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sei aller Schanden schmeichelnder Verbl\u00fcmer", "tokens": ["Sei", "al\u00b7ler", "Schan\u00b7den", "schmei\u00b7cheln\u00b7der", "Ver\u00b7bl\u00fc\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und weih dem evangelischen Regiment", "tokens": ["Und", "weih", "dem", "e\u00b7van\u00b7ge\u00b7li\u00b7schen", "Re\u00b7gi\u00b7ment"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Der Massenmordsarmeen und -marinen", "tokens": ["Der", "Mas\u00b7sen\u00b7mord\u00b7sar\u00b7me\u00b7en", "und"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den Kult, den solche Fetische verdienen!", "tokens": ["Den", "Kult", ",", "den", "sol\u00b7che", "Fe\u00b7ti\u00b7sche", "ver\u00b7die\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Hinweg! Wer wird am Wahnsinn sich ereifern?", "tokens": ["Hin\u00b7weg", "!", "Wer", "wird", "am", "Wahn\u00b7sinn", "sich", "er\u00b7ei\u00b7fern", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PWS", "VAFIN", "APPRART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich tat es lang. Mein Hirn blieb dennoch heil.", "tokens": ["Ich", "tat", "es", "lang", ".", "Mein", "Hirn", "blieb", "den\u00b7noch", "heil", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$.", "PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mich wundert's nur. Ohnm\u00e4chtig Schaum zu geifern,", "tokens": ["Mich", "wun\u00b7dert's", "nur", ".", "Ohn\u00b7m\u00e4ch\u00b7tig", "Schaum", "zu", "gei\u00b7fern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "ADJD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Daf\u00fcr ist Geisteskraft mir nicht mehr feil.", "tokens": ["Da\u00b7f\u00fcr", "ist", "Geis\u00b7tes\u00b7kraft", "mir", "nicht", "mehr", "feil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "NN", "PPER", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So schreit Hurra gekr\u00f6nten Volkseinseifern,", "tokens": ["So", "schreit", "Hur\u00b7ra", "ge\u00b7kr\u00f6n\u00b7ten", "Volks\u00b7ein\u00b7sei\u00b7fern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "ADJA", "NN", "$,"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Die Ehrgeiz selber h\u00e4lt am Narrenseil!", "tokens": ["Die", "Ehr\u00b7geiz", "sel\u00b7ber", "h\u00e4lt", "am", "Nar\u00b7ren\u00b7seil", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ihr wi\u00dft's und m\u00f6gt und wollt es ja nicht besser,", "tokens": ["Ihr", "wi\u00dft's", "und", "m\u00f6gt", "und", "wollt", "es", "ja", "nicht", "bes\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "ADJD", "KON", "VMFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Drum bleibt euch treu und wetzt mit Gott das Messer!", "tokens": ["Drum", "bleibt", "euch", "treu", "und", "wetzt", "mit", "Gott", "das", "Mes\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text":