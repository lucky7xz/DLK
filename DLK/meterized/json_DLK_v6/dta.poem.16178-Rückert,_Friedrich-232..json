{"dta.poem.16178": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "232.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Dinge, spielen sie mit dir, spielst du mit ihnen?", "tokens": ["Die", "Din\u00b7ge", ",", "spie\u00b7len", "sie", "mit", "dir", ",", "spielst", "du", "mit", "ih\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "APPR", "PPER", "$,", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zur Irrung gegenseits nur scheint ihr euch zu dienen.", "tokens": ["Zur", "Ir\u00b7rung", "ge\u00b7gen\u00b7seits", "nur", "scheint", "ihr", "euch", "zu", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADV", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "In diesem Augenblick will dieses wahr dir scheinen,", "tokens": ["In", "die\u00b7sem", "Au\u00b7gen\u00b7blick", "will", "die\u00b7ses", "wahr", "dir", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VMFIN", "PDAT", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im andern Augenblick willst du's als falsch verneinen.", "tokens": ["Im", "an\u00b7dern", "Au\u00b7gen\u00b7blick", "willst", "du's", "als", "falsch", "ver\u00b7nei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VMFIN", "PIS", "KOKOM", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was ist von beiden nun? ist beides wohl zugleich?", "tokens": ["Was", "ist", "von", "bei\u00b7den", "nun", "?", "ist", "bei\u00b7des", "wohl", "zu\u00b7gleich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PIAT", "ADV", "$.", "VAFIN", "PIS", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist nacheinander es, ein Werden wechselreich?", "tokens": ["Ist", "na\u00b7ch\u00b7ein\u00b7an\u00b7der", "es", ",", "ein", "Wer\u00b7den", "wech\u00b7sel\u00b7reich", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Allbeides ist in dir, von einem Nu getrennt.", "tokens": ["All\u00b7bei\u00b7des", "ist", "in", "dir", ",", "von", "ei\u00b7nem", "Nu", "ge\u00b7trennt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "PPER", "$,", "APPR", "ART", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ist nun das in dir, das so und so es nennt?", "tokens": ["Was", "ist", "nun", "das", "in", "dir", ",", "das", "so", "und", "so", "es", "nennt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ART", "APPR", "PPER", "$,", "PRELS", "ADV", "KON", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Das ist dein Wechselndes, das Wechsel bringt den Dingen;", "tokens": ["Das", "ist", "dein", "Wech\u00b7seln\u00b7des", ",", "das", "Wech\u00b7sel", "bringt", "den", "Din\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo ist ein Stehendes, um sie zum Stehn zu bringen?", "tokens": ["Wo", "ist", "ein", "Ste\u00b7hen\u00b7des", ",", "um", "sie", "zum", "Stehn", "zu", "brin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "KOUI", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Dis Stehende kann seyn das Ewige allein,", "tokens": ["Dis", "Ste\u00b7hen\u00b7de", "kann", "seyn", "das", "E\u00b7wi\u00b7ge", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VMFIN", "PPOSAT", "ART", "ADJA", "ADV", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Vor dem die Wahrheit steht und niederf\u00e4llt der Schein.", "tokens": ["Vor", "dem", "die", "Wahr\u00b7heit", "steht", "und", "nie\u00b7der\u00b7f\u00e4llt", "der", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Zieh alles Irdische vor dieses Gottgericht!", "tokens": ["Zieh", "al\u00b7les", "Ir\u00b7di\u00b7sche", "vor", "die\u00b7ses", "Gott\u00b7ge\u00b7richt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wahr ist, was mit ihm stimmt, und falsch was widerspricht.", "tokens": ["Wahr", "ist", ",", "was", "mit", "ihm", "stimmt", ",", "und", "falsch", "was", "wi\u00b7der\u00b7spricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,", "KON", "ADJD", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}