{"textgrid.poem.49441": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "[der Klang des Horns fliesst trauernd nach dem Wald]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Klang des Horns fliesst trauernd nach dem Wald,", "tokens": ["Der", "Klang", "des", "Horns", "fliesst", "trau\u00b7ernd", "nach", "dem", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Um m\u00e4hlich wie verwaister Schmerz im Schweigen", "tokens": ["Um", "m\u00e4h\u00b7lich", "wie", "ver\u00b7wais\u00b7ter", "Schmerz", "im", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ADJD", "KOKOM", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu sterben, wo die fernen H\u00fcgel steigen,", "tokens": ["Zu", "ster\u00b7ben", ",", "wo", "die", "fer\u00b7nen", "H\u00fc\u00b7gel", "stei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo bang erzitternd er im Wind verhallt.", "tokens": ["Wo", "bang", "er\u00b7zit\u00b7ternd", "er", "im", "Wind", "ver\u00b7hallt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVPP", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Des Wolfes Seele weint im Klang, der schallt", "tokens": ["Des", "Wol\u00b7fes", "See\u00b7le", "weint", "im", "Klang", ",", "der", "schallt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "APPRART", "NN", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und steigt, wann sich die Sonnenstrahlen neigen,", "tokens": ["Und", "steigt", ",", "wann", "sich", "die", "Son\u00b7nen\u00b7strah\u00b7len", "nei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem Herzen schmeichelt s\u00fcss der Todesreigen,", "tokens": ["Dem", "Her\u00b7zen", "schmei\u00b7chelt", "s\u00fcss", "der", "To\u00b7des\u00b7rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der es entz\u00fcckt zugleich und fasst mit Schmerzgewalt.", "tokens": ["Der", "es", "ent\u00b7z\u00fcckt", "zu\u00b7gleich", "und", "fasst", "mit", "Schmerz\u00b7ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADV", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dass sanfter noch der Schlummerklage Locken", "tokens": ["Dass", "sanf\u00b7ter", "noch", "der", "Schlum\u00b7mer\u00b7kla\u00b7ge", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ert\u00f6ne, f\u00e4llt der Schnee in weichen Flocken", "tokens": ["Er\u00b7t\u00f6\u00b7ne", ",", "f\u00e4llt", "der", "Schnee", "in", "wei\u00b7chen", "Flo\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Quer durch der Abendsonne Purpurglut.", "tokens": ["Quer", "durch", "der", "A\u00b7bend\u00b7son\u00b7ne", "Pur\u00b7pur\u00b7glut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und wie mit herbstlich tr\u00fcbem Hauch uns labend", "tokens": ["Und", "wie", "mit", "herbst\u00b7lich", "tr\u00fc\u00b7bem", "Hauch", "uns", "la\u00b7bend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ADJD", "ADJA", "NN", "PPER", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "K\u00fchlt unser Herz der stille, graue Abend,", "tokens": ["K\u00fchlt", "un\u00b7ser", "Herz", "der", "stil\u00b7le", ",", "grau\u00b7e", "A\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo z\u00e4rtlich eine sanfte Landschaft ruht.", "tokens": ["Wo", "z\u00e4rt\u00b7lich", "ei\u00b7ne", "sanf\u00b7te", "Land\u00b7schaft", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}