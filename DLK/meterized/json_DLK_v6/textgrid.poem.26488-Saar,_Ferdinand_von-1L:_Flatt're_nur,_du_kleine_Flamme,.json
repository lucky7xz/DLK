{"textgrid.poem.26488": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Flatt're nur, du kleine Flamme,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Flatt're nur, du kleine Flamme,", "tokens": ["Flatt'\u00b7re", "nur", ",", "du", "klei\u00b7ne", "Flam\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um der Nelke Purpurpracht;", "tokens": ["Um", "der", "Nel\u00b7ke", "Pur\u00b7pur\u00b7pracht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob aus dumpfem Grund sie stamme,", "tokens": ["Ob", "aus", "dum\u00b7pfem", "Grund", "sie", "stam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Herrlich ist sie doch entfacht.", "tokens": ["Herr\u00b7lich", "ist", "sie", "doch", "ent\u00b7facht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und vom Anbeginn der Dinge", "tokens": ["Und", "vom", "An\u00b7be\u00b7ginn", "der", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist's dasselbe Element,", "tokens": ["Ist's", "das\u00b7sel\u00b7be", "E\u00b7le\u00b7ment", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was auf deiner zarten Schwinge", "tokens": ["Was", "auf", "dei\u00b7ner", "zar\u00b7ten", "Schwin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und in ihrem Hauche brennt!", "tokens": ["Und", "in", "ih\u00b7rem", "Hau\u00b7che", "brennt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}