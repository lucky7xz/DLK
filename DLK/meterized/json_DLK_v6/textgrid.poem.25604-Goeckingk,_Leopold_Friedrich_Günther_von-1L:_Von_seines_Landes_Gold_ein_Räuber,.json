{"textgrid.poem.25604": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von seines Landes Gold ein R\u00e4uber,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von seines Landes Gold ein R\u00e4uber,", "tokens": ["Von", "sei\u00b7nes", "Lan\u00b7des", "Gold", "ein", "R\u00e4u\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Held im Serail, staatsklug im Kartenspiel';", "tokens": ["Held", "im", "Se\u00b7rail", ",", "staats\u00b7klug", "im", "Kar\u00b7ten\u00b7spiel'", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ihn lobt kein Unterthan. \u2013 Doch halt! das war zu viel!", "tokens": ["Ihn", "lobt", "kein", "Un\u00b7ter\u00b7than", ".", "\u2013", "Doch", "halt", "!", "das", "war", "zu", "viel", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$.", "$(", "KON", "VVFIN", "$.", "PDS", "VAFIN", "PTKA", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Unterthan ist ja sein Zeitungsschreiber.", "tokens": ["Ein", "Un\u00b7ter\u00b7than", "ist", "ja", "sein", "Zei\u00b7tungs\u00b7schrei\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}