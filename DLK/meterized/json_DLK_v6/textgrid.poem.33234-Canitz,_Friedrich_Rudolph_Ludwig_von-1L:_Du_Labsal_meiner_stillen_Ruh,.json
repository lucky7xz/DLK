{"textgrid.poem.33234": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du Labsal meiner stillen Ruh,", "genre": "verse", "period": "N.A.", "pub_year": 1676, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Labsal meiner stillen Ruh,", "tokens": ["Du", "Lab\u00b7sal", "mei\u00b7ner", "stil\u00b7len", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du lieblich-rauchend Pfeiffgen du;", "tokens": ["Du", "lieb\u00b7lich\u00b7rau\u00b7chend", "Pfeiff\u00b7gen", "du", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das, wie ein kleiner Ofen, gl\u00fcet,", "tokens": ["Das", ",", "wie", "ein", "klei\u00b7ner", "O\u00b7fen", ",", "gl\u00fcet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "$,", "PWAV", "ART", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das mein Gehirn von Fl\u00fcssen leert,", "tokens": ["Das", "mein", "Ge\u00b7hirn", "von", "Fl\u00fcs\u00b7sen", "leert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und, wenn ein Kummer mich beschwehrt,", "tokens": ["Und", ",", "wenn", "ein", "Kum\u00b7mer", "mich", "be\u00b7schwehrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihn unvermerckt vom Hertzen ziehet.", "tokens": ["Ihn", "un\u00b7ver\u00b7merckt", "vom", "Hert\u00b7zen", "zie\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Taback, der meinen Geist erfreut,", "tokens": ["Ta\u00b7back", ",", "der", "mei\u00b7nen", "Geist", "er\u00b7freut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seh ich schnell deinen Rauch verschwinden,", "tokens": ["Seh", "ich", "schnell", "dei\u00b7nen", "Rauch", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "So kan ich hier zu gleicher Zeit", "tokens": ["So", "kan", "ich", "hier", "zu", "glei\u00b7cher", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Bildni\u00df meines Lebens finden.", "tokens": ["Ein", "Bild\u00b7ni\u00df", "mei\u00b7nes", "Le\u00b7bens", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du giebst mir deutlich zu verstehen,", "tokens": ["Du", "giebst", "mir", "deut\u00b7lich", "zu", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da ich nur Asche, die noch glimmt,", "tokens": ["Da", "ich", "nur", "A\u00b7sche", ",", "die", "noch", "glimmt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was f\u00fcr ein End einst mir bestimmt.", "tokens": ["Was", "f\u00fcr", "ein", "End", "einst", "mir", "be\u00b7stimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und folgt mein Auge deinem Rauch,", "tokens": ["Und", "folgt", "mein", "Au\u00b7ge", "dei\u00b7nem", "Rauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So merck ich sichtbar, da\u00df ich auch", "tokens": ["So", "merck", "ich", "sicht\u00b7bar", ",", "da\u00df", "ich", "auch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dereinst selbst mu\u00df, wie du, vergehen.", "tokens": ["De\u00b7reinst", "selbst", "mu\u00df", ",", "wie", "du", ",", "ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "$,", "PWAV", "PPER", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}