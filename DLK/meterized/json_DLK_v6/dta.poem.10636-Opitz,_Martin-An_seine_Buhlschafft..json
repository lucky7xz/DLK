{"dta.poem.10636": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "An seine Buhlschafft.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Den Spiegel send ich euch/ jhr Spiegel aller Frawen/", "tokens": ["Den", "Spie\u00b7gel", "send", "ich", "euch", "/", "jhr", "Spie\u00b7gel", "al\u00b7ler", "Fra\u00b7wen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "PPER", "PPER", "$(", "PPOSAT", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df jhr die G\u00f6ttligkeit an euch recht m\u00f6get schawen/", "tokens": ["Da\u00df", "jhr", "die", "G\u00f6tt\u00b7lig\u00b7keit", "an", "euch", "recht", "m\u00f6\u00b7get", "scha\u00b7wen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPER", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ob gleich kein Spiegel ist zutreffen jrgendt an/", "tokens": ["Ob", "gleich", "kein", "Spie\u00b7gel", "ist", "zu\u00b7tref\u00b7fen", "jr\u00b7gendt", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "VAFIN", "VVFIN", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der euch/ jhr sch\u00f6nes Bild/ schnurrecht entwerffen kan/", "tokens": ["Der", "euch", "/", "jhr", "sch\u00f6\u00b7nes", "Bild", "/", "schnur\u00b7recht", "ent\u00b7werf\u00b7fen", "kan", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$(", "PPOSAT", "ADJA", "NN", "$(", "ADJD", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch/ soltet jhr gleichwol erkennen meine Sinnen/", "tokens": ["Doch", "/", "sol\u00b7tet", "jhr", "gleich\u00b7wol", "er\u00b7ken\u00b7nen", "mei\u00b7ne", "Sin\u00b7nen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "VMFIN", "PPER", "ADV", "VVINF", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ihr w\u00fcrdet auch gewi\u00df leibhafftig sehen k\u00fcnnen/", "tokens": ["Ihr", "w\u00fcr\u00b7det", "auch", "ge\u00b7wi\u00df", "leib\u00b7haff\u00b7tig", "se\u00b7hen", "k\u00fcn\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dann jhr mir seyt alldar so starck gebildet ein/", "tokens": ["Dann", "jhr", "mir", "seyt", "all\u00b7dar", "so", "starck", "ge\u00b7bil\u00b7det", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPER", "VAFIN", "ADV", "ADV", "ADJD", "VVPP", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df jhr euch selber nicht so ehnlich k\u00fcnnet sein.", "tokens": ["Da\u00df", "jhr", "euch", "sel\u00b7ber", "nicht", "so", "ehn\u00b7lich", "k\u00fcn\u00b7net", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "PTKNEG", "ADV", "ADJD", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}