{"dta.poem.1062": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Antistrophe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Montmartin/ der halb-g\u00f6tter zier/", "tokens": ["Mont\u00b7mar\u00b7tin", "/", "der", "halb\u00b7g\u00f6t\u00b7ter", "zier", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vn\u0303 vo\u0303 de\u0303 Musen selbs geschm\u00fccket/", "tokens": ["V\u00f1", "v\u00f5", "d\u1ebd", "Mu\u00b7sen", "selbs", "ge\u00b7schm\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das lob/ so ich von dir formier", "tokens": ["Das", "lob", "/", "so", "ich", "von", "dir", "for\u00b7mier"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ADV", "PPER", "APPR", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist nicht au\u00df fremb dem schmuck gest\u00fccket:", "tokens": ["Ist", "nicht", "au\u00df", "fremb", "dem", "schmuck", "ge\u00b7st\u00fc\u00b7cket", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PTKVZ", "VVFIN", "ART", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich nem dazu auch nicht das lob", "tokens": ["Ich", "nem", "da\u00b7zu", "auch", "nicht", "das", "lob"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "PAV", "ADV", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Deines Vatters so hochgeprisen/", "tokens": ["Dei\u00b7nes", "Vat\u00b7ters", "so", "hoch\u00b7ge\u00b7pri\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Welcher/ Marschalck/ mit kh\u00fcner prob", "tokens": ["Wel\u00b7cher", "/", "Mar\u00b7schalck", "/", "mit", "kh\u00fc\u00b7ner", "prob"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWAT", "$(", "NE", "$(", "APPR", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Seinem k\u00f6nig vil dienst erwisen:", "tokens": ["Sei\u00b7nem", "k\u00f6\u00b7nig", "vil", "dienst", "er\u00b7wi\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "In dir find ich ein \u00e4rnd reich gnug/", "tokens": ["In", "dir", "find", "ich", "ein", "\u00e4rnd", "reich", "gnug", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ART", "ADJD", "ADJD", "ADV", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Davon ich dan mit gutem fug", "tokens": ["Da\u00b7von", "ich", "dan", "mit", "gu\u00b7tem", "fug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Zusamen samle dise fr\u00fcchten/", "tokens": ["Zu\u00b7sa\u00b7men", "sam\u00b7le", "di\u00b7se", "fr\u00fcch\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PDAT", "ADJA", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "Dir dises opfer zuzurichten.", "tokens": ["Dir", "di\u00b7ses", "op\u00b7fer", "zu\u00b7zu\u00b7rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}