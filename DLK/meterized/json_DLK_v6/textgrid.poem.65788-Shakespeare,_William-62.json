{"textgrid.poem.65788": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "62", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Aug' und Seel' und allen meinen Teilen", "tokens": ["In", "Aug'", "und", "Seel'", "und", "al\u00b7len", "mei\u00b7nen", "Tei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bin ich der Eigenliebe mir bewu\u00dft;", "tokens": ["Bin", "ich", "der", "Ei\u00b7gen\u00b7lie\u00b7be", "mir", "be\u00b7wu\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und diese S\u00fcnd' in mir ist nicht zu heilen,", "tokens": ["Und", "die\u00b7se", "S\u00fcnd'", "in", "mir", "ist", "nicht", "zu", "hei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "APPR", "PPER", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So wurzelt sie in meiner tiefsten Brust.", "tokens": ["So", "wur\u00b7zelt", "sie", "in", "mei\u00b7ner", "tiefs\u00b7ten", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So reizend scheint mir kein Gesicht, so fein", "tokens": ["So", "rei\u00b7zend", "scheint", "mir", "kein", "Ge\u00b7sicht", ",", "so", "fein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PIAT", "NN", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Kein Wuchs wie meiner, keine Treu so echt:", "tokens": ["Kein", "Wuchs", "wie", "mei\u00b7ner", ",", "kei\u00b7ne", "Treu", "so", "echt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KOKOM", "PPOSAT", "$,", "PIAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und sch\u00e4tz' ich mein Verdienst f\u00fcr mich allein,", "tokens": ["Und", "sch\u00e4tz'", "ich", "mein", "Ver\u00b7dienst", "f\u00fcr", "mich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Als w\u00e4r kein andres neben mir gerecht.", "tokens": ["Als", "w\u00e4r", "kein", "and\u00b7res", "ne\u00b7ben", "mir", "ge\u00b7recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PIAT", "PIS", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Doch wenn mich dann mein Spiegel selbst mir weist,", "tokens": ["Doch", "wenn", "mich", "dann", "mein", "Spie\u00b7gel", "selbst", "mir", "weist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Von fahler Zeit zerr\u00fcttet und verbogen,", "tokens": ["Von", "fah\u00b7ler", "Zeit", "zer\u00b7r\u00fct\u00b7tet", "und", "ver\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Dann les' ich erst was Eigenliebe hei\u00dft;", "tokens": ["Dann", "les'", "ich", "erst", "was", "Ei\u00b7gen\u00b7lie\u00b7be", "hei\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "PWS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Denn nur ein Tor blieb ", "tokens": ["Denn", "nur", "ein", "Tor", "blieb"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Du bist's, mein Selbst, das mich als Ich entz\u00fcckt,", "tokens": ["Du", "bist's", ",", "mein", "Selbst", ",", "das", "mich", "als", "Ich", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$,", "PPOSAT", "NN", "$,", "PRELS", "PPER", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Mit deinem Jugendreiz mein Alter schm\u00fcckt!", "tokens": ["Mit", "dei\u00b7nem", "Ju\u00b7gen\u00b7dreiz", "mein", "Al\u00b7ter", "schm\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}