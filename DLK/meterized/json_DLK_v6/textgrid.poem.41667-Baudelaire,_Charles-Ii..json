{"textgrid.poem.41667": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Paris wird anders, doch in meiner tiefen Trauer", "tokens": ["Pa\u00b7ris", "wird", "an\u00b7ders", ",", "doch", "in", "mei\u00b7ner", "tie\u00b7fen", "Trau\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADV", "$,", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bleibt alles! Der Pal\u00e4st' und der Ger\u00fcste Meer,", "tokens": ["Bleibt", "al\u00b7les", "!", "Der", "Pa\u00b7l\u00e4st'", "und", "der", "Ge\u00b7r\u00fcs\u00b7te", "Meer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$.", "ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Vorstadt h\u00fcllen sich in deutungsvolle Schauer,", "tokens": ["Die", "Vor\u00b7stadt", "h\u00fcl\u00b7len", "sich", "in", "deu\u00b7tungs\u00b7vol\u00b7le", "Schau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und die Erinnerung liegt auf mir felsenschwer.", "tokens": ["Und", "die", "E\u00b7rin\u00b7ne\u00b7rung", "liegt", "auf", "mir", "fel\u00b7sen\u00b7schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So \u00fcberkommt ein Bild vorm Louvre mich bedr\u00fcckend,", "tokens": ["So", "\u00fc\u00b7ber\u00b7kommt", "ein", "Bild", "vorm", "Louv\u00b7re", "mich", "be\u00b7dr\u00fc\u00b7ckend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein denk ich, gro\u00dfer Schwan, gequ\u00e4lt, fast l\u00e4cherlich,", "tokens": ["Dein", "denk", "ich", ",", "gro\u00b7\u00dfer", "Schwan", ",", "ge\u00b7qu\u00e4lt", ",", "fast", "l\u00e4\u00b7cher\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "VVPP", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch wie Verbannte mit Erhabenheit sich schm\u00fcckend,", "tokens": ["Doch", "wie", "Ver\u00b7bann\u00b7te", "mit", "Er\u00b7ha\u00b7ben\u00b7heit", "sich", "schm\u00fc\u00b7ckend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "APPR", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verzehrt von einem Wunsch ohn Ende, und an dich,", "tokens": ["Ver\u00b7zehrt", "von", "ei\u00b7nem", "Wunsch", "ohn", "En\u00b7de", ",", "und", "an", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "APPR", "NN", "$,", "KON", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Andromache, die, j\u00e4h des Gatten Arm entsunken,", "tokens": ["And\u00b7ro\u00b7ma\u00b7che", ",", "die", ",", "j\u00e4h", "des", "Gat\u00b7ten", "Arm", "ent\u00b7sun\u00b7ken", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "PRELS", "$,", "ADJD", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ein Tier, vorm m\u00e4chtgen Griff des Pyrrhus niederbrach.", "tokens": ["Ein", "Tier", ",", "vorm", "m\u00e4cht\u00b7gen", "Griff", "des", "Pyrr\u00b7hus", "nie\u00b7der\u00b7brach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "ADJA", "NN", "ART", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "An einer leeren Gruft sich beugend, gramestrunken,", "tokens": ["An", "ei\u00b7ner", "lee\u00b7ren", "Gruft", "sich", "beu\u00b7gend", ",", "gra\u00b7mes\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PRF", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Weib des Helenus und Hektors Witwe, ach!", "tokens": ["Das", "Weib", "des", "He\u00b7le\u00b7nus", "und", "Hek\u00b7tors", "Wit\u00b7we", ",", "ach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "NE", "NN", "$,", "ITJ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Negrin denke ich, die krank zum Niedersinken,", "tokens": ["Der", "Ne\u00b7grin", "den\u00b7ke", "ich", ",", "die", "krank", "zum", "Nie\u00b7der\u00b7sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Schmutze watend und das Auge unverwandt,", "tokens": ["Im", "Schmut\u00b7ze", "wa\u00b7tend", "und", "das", "Au\u00b7ge", "un\u00b7ver\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "KON", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Palmen sucht, die schlank in Afrika ihr winken,", "tokens": ["Die", "Pal\u00b7men", "sucht", ",", "die", "schlank", "in", "Af\u00b7ri\u00b7ka", "ihr", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "ADJD", "APPR", "NE", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch die gewaltige, endlose Nebelwand;", "tokens": ["Durch", "die", "ge\u00b7wal\u00b7ti\u00b7ge", ",", "end\u00b7lo\u00b7se", "Ne\u00b7bel\u00b7wand", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}, "stanza.5": {"line.1": {"text": "An jeden, der verlor, was nie, nie seine Augen", "tokens": ["An", "je\u00b7den", ",", "der", "ver\u00b7lor", ",", "was", "nie", ",", "nie", "sei\u00b7ne", "Au\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "$,", "PRELS", "VVFIN", "$,", "PRELS", "ADV", "$,", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mehr schauen; an die Schar, gestillt vom Tr\u00e4nentrank,", "tokens": ["Mehr", "schau\u00b7en", ";", "an", "die", "Schar", ",", "ge\u00b7stillt", "vom", "Tr\u00e4\u00b7nen\u00b7trank", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "$.", "APPR", "ART", "NN", "$,", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die an dem Schmerze, wie an g\u00fctger W\u00f6lfin saugen,", "tokens": ["Die", "an", "dem", "Schmer\u00b7ze", ",", "wie", "an", "g\u00fct\u00b7ger", "W\u00f6l\u00b7fin", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$,", "PWAV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die magren Waisen, die gleich Blumen welk und krank!", "tokens": ["Die", "mag\u00b7ren", "Wai\u00b7sen", ",", "die", "gleich", "Blu\u00b7men", "welk", "und", "krank", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Es t\u00f6nt das Horn im Wald, in den mein Geist vertrieben,", "tokens": ["Es", "t\u00f6nt", "das", "Horn", "im", "Wald", ",", "in", "den", "mein", "Geist", "ver\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,", "APPR", "ART", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein alt Erinnern mir mit vollgeschwelltem Hauch!", "tokens": ["Ein", "alt", "E\u00b7rin\u00b7nern", "mir", "mit", "voll\u00b7ge\u00b7schwell\u00b7tem", "Hauch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Der Schiffer denke ich, auf fernem Riff geblieben,", "tokens": ["Der", "Schif\u00b7fer", "den\u00b7ke", "ich", ",", "auf", "fer\u00b7nem", "Riff", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gefangner, Fliehender! ... und mancher andern auch!", "tokens": ["Ge\u00b7fang\u00b7ner", ",", "Flie\u00b7hen\u00b7der", "!", "...", "und", "man\u00b7cher", "an\u00b7dern", "auch", "!"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "$(", "KON", "PIAT", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}