{"textgrid.poem.40178": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Erwachen", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schw\u00fcle Stille f\u00fcllet die Rotunde;", "tokens": ["Schw\u00fc\u00b7le", "Stil\u00b7le", "f\u00fcl\u00b7let", "die", "Ro\u00b7tun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "um die blanken S\u00e4ulen", "tokens": ["um", "die", "blan\u00b7ken", "S\u00e4u\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "h\u00e4ngt die Mittagsglut.", "tokens": ["h\u00e4ngt", "die", "Mit\u00b7tags\u00b7glut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Auf der Marmorstufe", "tokens": ["Auf", "der", "Mar\u00b7mor\u00b7stu\u00b7fe"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "matt das M\u00e4dchen ruht;", "tokens": ["matt", "das", "M\u00e4d\u00b7chen", "ruht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "eine Rose f\u00fchret sie zum Munde.", "tokens": ["ei\u00b7ne", "Ro\u00b7se", "f\u00fch\u00b7ret", "sie", "zum", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "M\u00fcde schwimmen ihre Kinderaugen;", "tokens": ["M\u00fc\u00b7de", "schwim\u00b7men", "ih\u00b7re", "Kin\u00b7der\u00b7au\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "in den k\u00fchlen Kelchgrund", "tokens": ["in", "den", "k\u00fch\u00b7len", "Kelch\u00b7grund"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "tief hineingetaucht,", "tokens": ["tief", "hin\u00b7ein\u00b7ge\u00b7taucht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "lange Seufzer ihre", "tokens": ["lan\u00b7ge", "Seuf\u00b7zer", "ih\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["ADV", "NN", "PPOSAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "hei\u00dfe Lippe haucht,", "tokens": ["hei\u00b7\u00dfe", "Lip\u00b7pe", "haucht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "d\u00fcrstender den linden Duft zu saugen.", "tokens": ["d\u00fcrs\u00b7ten\u00b7der", "den", "lin\u00b7den", "Duft", "zu", "sau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Nach der Halle schaut sie, durch die gestern", "tokens": ["Nach", "der", "Hal\u00b7le", "schaut", "sie", ",", "durch", "die", "ge\u00b7stern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$,", "APPR", "ART", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Daidalos, der junge", "tokens": ["Dai\u00b7da\u00b7los", ",", "der", "jun\u00b7ge"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADJD", "$,", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Gastfreund, zog herein.", "tokens": ["Gast\u00b7freund", ",", "zog", "her\u00b7ein", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Und sie sinnt: \u00bbWarum wohl", "tokens": ["Und", "sie", "sinnt", ":", "\u00bb", "Wa\u00b7rum", "wohl"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "$(", "PWAV", "ADV"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "gab er ", "tokens": ["gab", "er"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "heut die Blume? ", "tokens": ["heut", "die", "Blu\u00b7me", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Warum hat er so mich ", "tokens": ["Wa\u00b7rum", "hat", "er", "so", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "PPER"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "anders als die Schwestern,", "tokens": ["an\u00b7ders", "als", "die", "Schwes\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "da die Hand er gab? \u2013", "tokens": ["da", "die", "Hand", "er", "gab", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Bin ein kindisch M\u00e4dchen!", "tokens": ["Bin", "ein", "kin\u00b7disch", "M\u00e4d\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "wei\u00df nicht, was ich hab'!", "tokens": ["wei\u00df", "nicht", ",", "was", "ich", "hab'", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Wie ich gl\u00fche! ich will baden gehen.\u00ab", "tokens": ["Wie", "ich", "gl\u00fc\u00b7he", "!", "ich", "will", "ba\u00b7den", "ge\u00b7hen", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$.", "PPER", "VMFIN", "VVINF", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Und sie hebt sich von der warmen Schwelle;", "tokens": ["Und", "sie", "hebt", "sich", "von", "der", "war\u00b7men", "Schwel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "aus dem Bl\u00fctenscho\u00dfe", "tokens": ["aus", "dem", "Bl\u00fc\u00b7ten\u00b7scho\u00b7\u00dfe"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "ein gel\u00f6stes Blatt", "tokens": ["ein", "ge\u00b7l\u00f6s\u00b7tes", "Blatt"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "traumbefangen kosend,", "tokens": ["traum\u00b7be\u00b7fan\u00b7gen", "ko\u00b7send", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "wandelt sie zum Bad,", "tokens": ["wan\u00b7delt", "sie", "zum", "Bad", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "\u00f6ffnet sie versunknen Blicks die Zelle.", "tokens": ["\u00f6ff\u00b7net", "sie", "ver\u00b7sun\u00b7knen", "Blicks", "die", "Zel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Und sie steht gebannt, und steht und staunet:", "tokens": ["Und", "sie", "steht", "ge\u00b7bannt", ",", "und", "steht", "und", "stau\u00b7net", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "VVPP", "$,", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "vor ihr liegt der Gastfreund", "tokens": ["vor", "ihr", "liegt", "der", "Gast\u00b7freund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "schlafend hingedehnt.", "tokens": ["schla\u00b7fend", "hin\u00b7ge\u00b7dehnt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Und verwirrt belauscht sie,", "tokens": ["Und", "ver\u00b7wirrt", "be\u00b7lauscht", "sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "bang ans Thor gelehnt,", "tokens": ["bang", "ans", "Thor", "ge\u00b7lehnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "wie das Niegeschaute in ihr raunet.", "tokens": ["wie", "das", "Nie\u00b7ge\u00b7schau\u00b7te", "in", "ihr", "rau\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Lauscht die starken nackten J\u00fcnglingsglieder,", "tokens": ["Lauscht", "die", "star\u00b7ken", "nack\u00b7ten", "J\u00fcng\u00b7lings\u00b7glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "lauscht sein schlummerselig", "tokens": ["lauscht", "sein", "schlum\u00b7mer\u00b7se\u00b7lig"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "weiches L\u00e4cheln an,", "tokens": ["wei\u00b7ches", "L\u00e4\u00b7cheln", "an", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "will davon \u2013 und wei\u00df nicht,", "tokens": ["will", "da\u00b7von", "\u2013", "und", "wei\u00df", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PAV", "$(", "KON", "VVFIN", "PTKNEG", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "was sie ", "tokens": ["was", "sie"], "token_info": ["word", "word"], "pos": ["PWS", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "stillverwundert starrt sie auf ihn nieder.", "tokens": ["still\u00b7ver\u00b7wun\u00b7dert", "starrt", "sie", "auf", "ihn", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Da erschrickt sie: dunkle W\u00fcnsche wallen", "tokens": ["Da", "er\u00b7schrickt", "sie", ":", "dunk\u00b7le", "W\u00fcn\u00b7sche", "wal\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "ihr ums Herz, ein lohend", "tokens": ["ihr", "ums", "Herz", ",", "ein", "lo\u00b7hend"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "APPRART", "NN", "$,", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Rot ihr Antlitz s\u00e4umt!", "tokens": ["Rot", "ihr", "Ant\u00b7litz", "s\u00e4umt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "und auf Einmal ", "tokens": ["und", "auf", "Ein\u00b7mal"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "-++-", "measure": "unknown.measure.di"}, "line.5": {"text": "was in ihr getr\u00e4umt,", "tokens": ["was", "in", "ihr", "ge\u00b7tr\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "und die Rose l\u00e4\u00dft sie zitternd fallen \u2013", "tokens": ["und", "die", "Ro\u00b7se", "l\u00e4\u00dft", "sie", "zit\u00b7ternd", "fal\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "und entflieht \u2013 ein Sehnsuchtslaut verwehet ...", "tokens": ["und", "ent\u00b7flieht", "\u2013", "ein", "Sehn\u00b7suchts\u00b7laut", "ver\u00b7we\u00b7het", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da erwacht der J\u00fcngling,", "tokens": ["Da", "er\u00b7wacht", "der", "J\u00fcng\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "sieht die Ros' am Thor, \u2013", "tokens": ["sieht", "die", "Ros'", "am", "Thor", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "und auf Einmal, jauchzend,", "tokens": ["und", "auf", "Ein\u00b7mal", ",", "jauch\u00b7zend", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ADV", "$,", "VVPP", "$,"], "meter": "-++-+-", "measure": "unknown.measure.tri"}, "line.5": {"text": "rei\u00dft er sie empor:", "tokens": ["rei\u00dft", "er", "sie", "em\u00b7por", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}}}}}