{"dta.poem.17658": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "280.  Gott kan sich selbst nicht messen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Gott ist so hoch und gro\u00df/ wolt\u2019 Er sich selber messen/", "tokens": ["Gott", "ist", "so", "hoch", "und", "gro\u00df", "/", "wolt'", "Er", "sich", "sel\u00b7ber", "mes\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$(", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er w\u00fcrd/ ob er gleich Gott/ de\u00df Ma\u00dfstabs zahl ver-", "tokens": ["Er", "w\u00fcrd", "/", "ob", "er", "gleich", "Gott", "/", "de\u00df", "Ma\u00df\u00b7stabs", "zahl", "ver"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$(", "KOUS", "PPER", "ADV", "NN", "$(", "ART", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "gessen.", "tokens": ["ges\u00b7sen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}