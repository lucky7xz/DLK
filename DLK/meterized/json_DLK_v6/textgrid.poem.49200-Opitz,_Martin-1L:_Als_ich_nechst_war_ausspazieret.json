{"textgrid.poem.49200": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als ich nechst war ausspazieret", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich nechst war ausspazieret", "tokens": ["Als", "ich", "nechst", "war", "aus\u00b7spa\u00b7zie\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu den Hirten in den Wald", "tokens": ["Zu", "den", "Hir\u00b7ten", "in", "den", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit ihnen musicieret,", "tokens": ["Und", "mit", "ih\u00b7nen", "mu\u00b7si\u00b7cie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df der ganze Pusch erschallt,", "tokens": ["Da\u00df", "der", "gan\u00b7ze", "Pusch", "er\u00b7schallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kam die Venus selbst zu mir,", "tokens": ["Kam", "die", "Ve\u00b7nus", "selbst", "zu", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bracht' auch ihren Sohn mit ihr,", "tokens": ["Bracht'", "auch", "ih\u00b7ren", "Sohn", "mit", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der bei mir verbleiben solte,", "tokens": ["Der", "bei", "mir", "ver\u00b7blei\u00b7ben", "sol\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wo ich ihn was lehren wolte.", "tokens": ["Wo", "ich", "ihn", "was", "leh\u00b7ren", "wol\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "PIS", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Alles, was du wilt bedingen,", "tokens": ["Al\u00b7les", ",", "was", "du", "wilt", "be\u00b7din\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sagte sie, ist dir verg\u00fcnnt,", "tokens": ["Sag\u00b7te", "sie", ",", "ist", "dir", "ver\u00b7g\u00fcnnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Wo du deine Kunst, zu singen,", "tokens": ["Wo", "du", "dei\u00b7ne", "Kunst", ",", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lehren wirst mein kleines Kind.", "tokens": ["Leh\u00b7ren", "wirst", "mein", "klei\u00b7nes", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wol, ich weis' ihm ganz bereit,", "tokens": ["Wol", ",", "ich", "weis'", "ihm", "ganz", "be\u00b7reit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was man noch hat dieser Zeit", "tokens": ["Was", "man", "noch", "hat", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PIS", "ADV", "VAFIN", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Von den G\u00f6ttern aufgeschrieben,", "tokens": ["Von", "den", "G\u00f6t\u00b7tern", "auf\u00b7ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und im Hirtenbuch ist blieben.", "tokens": ["Und", "im", "Hir\u00b7ten\u00b7buch", "ist", "blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "VVFIN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Wie da\u00df Pan auf sieben R\u00f6hren", "tokens": ["Wie", "da\u00df", "Pan", "auf", "sie\u00b7ben", "R\u00f6h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "NE", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Anzustimmen hat erdacht", "tokens": ["An\u00b7zu\u00b7stim\u00b7men", "hat", "er\u00b7dacht"], "token_info": ["word", "word", "word"], "pos": ["NN", "VAFIN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und, ganz lieblich anzuh\u00f6ren,", "tokens": ["Und", ",", "ganz", "lieb\u00b7lich", "an\u00b7zu\u00b7h\u00f6\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "ADJD", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen neuen Ton aufbracht;", "tokens": ["Ei\u00b7nen", "neu\u00b7en", "Ton", "auf\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie da\u00df Arist\u00e4us weit", "tokens": ["Wie", "da\u00df", "A\u00b7ris\u00b7t\u00e4us", "weit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "NE", "ADJD"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Mit dem Bacchus kam in Streit,", "tokens": ["Mit", "dem", "Bac\u00b7chus", "kam", "in", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ob die S\u00fc\u00dfigkeit der Bienen", "tokens": ["Ob", "die", "S\u00fc\u00b7\u00dfig\u00b7keit", "der", "Bie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mehr als Wein uns k\u00f6nte dienen.", "tokens": ["Mehr", "als", "Wein", "uns", "k\u00f6n\u00b7te", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "NN", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber nein, der lose Knabe", "tokens": ["A\u00b7ber", "nein", ",", "der", "lo\u00b7se", "Kna\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PTKANT", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Machte, was er vor gethan;", "tokens": ["Mach\u00b7te", ",", "was", "er", "vor", "ge\u00b7than", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "APPR", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wann ich ihm was anders gabe,", "tokens": ["Wann", "ich", "ihm", "was", "an\u00b7ders", "ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "PIS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So hub er von Buhlen an;", "tokens": ["So", "hub", "er", "von", "Buh\u00b7len", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Allzeit ward von ihm geh\u00f6rt,", "tokens": ["All\u00b7zeit", "ward", "von", "ihm", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Wie die Lieb' uns so beth\u00f6rt,", "tokens": ["Wie", "die", "Lieb'", "uns", "so", "be\u00b7th\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie nach seiner Mutter Sinnen", "tokens": ["Wie", "nach", "sei\u00b7ner", "Mut\u00b7ter", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Jedermann mu\u00df lieb gewinnen.", "tokens": ["Je\u00b7der\u00b7mann", "mu\u00df", "lieb", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Solt er Lection aufsagen,", "tokens": ["Solt", "er", "Lec\u00b7ti\u00b7on", "auf\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wust' er lauter nichts darvon,", "tokens": ["Wust'", "er", "lau\u00b7ter", "nichts", "dar\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "PIS", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Brachte selbst mir vorgetragen", "tokens": ["Brach\u00b7te", "selbst", "mir", "vor\u00b7ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine schwere Lection;", "tokens": ["Ei\u00b7ne", "schwe\u00b7re", "Lec\u00b7ti\u00b7on", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jetzt ich also nichts mehr wei\u00df,", "tokens": ["Jetzt", "ich", "al\u00b7so", "nichts", "mehr", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "PIS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dann von Lieb' und ihrem Preis;", "tokens": ["Dann", "von", "Lieb'", "und", "ih\u00b7rem", "Preis", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Jetzt ist g\u00e4nzlich mir entfallen,", "tokens": ["Jetzt", "ist", "g\u00e4nz\u00b7lich", "mir", "ent\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Was ich konte vor f\u00fcr allen.", "tokens": ["Was", "ich", "kon\u00b7te", "vor", "f\u00fcr", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "APPR", "APPR", "PIAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Nun ade, ihr Feldg\u00f6ttinnen,", "tokens": ["Nun", "a\u00b7de", ",", "ihr", "Feld\u00b7g\u00f6t\u00b7tin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nun ade, du gr\u00fcne Lust;", "tokens": ["Nun", "a\u00b7de", ",", "du", "gr\u00fc\u00b7ne", "Lust", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Corydon mu\u00df jetz beginnen,", "tokens": ["Co\u00b7ry\u00b7don", "mu\u00df", "jetz", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was er vorhin nie gewust:", "tokens": ["Was", "er", "vor\u00b7hin", "nie", "ge\u00b7wust", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Es ist, wo ich geh' und steh',", "tokens": ["Es", "ist", ",", "wo", "ich", "geh'", "und", "steh'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Alles nichts dann Galathee;", "tokens": ["Al\u00b7les", "nichts", "dann", "Ga\u00b7la\u00b7thee", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PIS", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In den strengen Liebesorden", "tokens": ["In", "den", "stren\u00b7gen", "Lie\u00b7be\u00b7sor\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bin ich durch ein Kind bracht worden.", "tokens": ["Bin", "ich", "durch", "ein", "Kind", "bracht", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}