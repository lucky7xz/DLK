{"textgrid.poem.25377": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dein Stern ergl\u00e4nzt in Auferstehungsfr\u00fchen,", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Stern ergl\u00e4nzt in Auferstehungsfr\u00fchen,", "tokens": ["Dein", "Stern", "er\u00b7gl\u00e4nzt", "in", "Auf\u00b7er\u00b7ste\u00b7hungs\u00b7fr\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein Schicksal treibt, als Opfer sich zu spenden,", "tokens": ["Dein", "Schick\u00b7sal", "treibt", ",", "als", "Op\u00b7fer", "sich", "zu", "spen\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KOUS", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durstige Flamme, k\u00fchn, sich zu verschwenden,", "tokens": ["Durs\u00b7ti\u00b7ge", "Flam\u00b7me", ",", "k\u00fchn", ",", "sich", "zu", "ver\u00b7schwen\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "$,", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Wie Laubgerinnsel, die im Herbstwald sich vergl\u00fchen.", "tokens": ["Wie", "Laub\u00b7ge\u00b7rinn\u00b7sel", ",", "die", "im", "Herbst\u00b7wald", "sich", "ver\u00b7gl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "APPRART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "In Fernen sind die H\u00f6lzer schon geschichtet,", "tokens": ["In", "Fer\u00b7nen", "sind", "die", "H\u00f6l\u00b7zer", "schon", "ge\u00b7schich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Leib zu neuer Weihe zu empfangen \u2013", "tokens": ["Den", "Leib", "zu", "neu\u00b7er", "Wei\u00b7he", "zu", "emp\u00b7fan\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und schwellend ist, um das die Wimpel deiner Tr\u00e4ume hangen,", "tokens": ["Und", "schwel\u00b7lend", "ist", ",", "um", "das", "die", "Wim\u00b7pel", "dei\u00b7ner", "Tr\u00e4u\u00b7me", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,", "KOUI", "ART", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Das Brautbett deiner letzten Sehnsucht aufgerichtet.", "tokens": ["Das", "Braut\u00b7bett", "dei\u00b7ner", "letz\u00b7ten", "Sehn\u00b7sucht", "auf\u00b7ge\u00b7rich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}