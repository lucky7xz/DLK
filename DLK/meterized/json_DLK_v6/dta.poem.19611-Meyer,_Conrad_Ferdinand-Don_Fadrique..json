{"dta.poem.19611": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Don Fadrique.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Don Fadrique bringt ein St\u00e4ndchen", "tokens": ["Don", "Fad\u00b7ri\u00b7que", "bringt", "ein", "St\u00e4nd\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "ART", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Seiner drolligen Pepita:", "tokens": ["Sei\u00b7ner", "drol\u00b7li\u00b7gen", "Pe\u00b7pi\u00b7ta", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "\u201eliebchen, strecke durch die Th\u00fcre", "tokens": ["\u201e", "lieb\u00b7chen", ",", "stre\u00b7cke", "durch", "die", "Th\u00fc\u00b7re"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deines F\u00fc\u00dfchens Spitze nur!\u201c", "tokens": ["Dei\u00b7nes", "F\u00fc\u00df\u00b7chens", "Spit\u00b7ze", "nur", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "NN", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und die drollige Pepita", "tokens": ["Und", "die", "drol\u00b7li\u00b7ge", "Pe\u00b7pi\u00b7ta"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NE"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Streckt durch eine schmale Spalte", "tokens": ["Streckt", "durch", "ei\u00b7ne", "schma\u00b7le", "Spal\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eines allerliebsten Fu\u00dfes", "tokens": ["Ei\u00b7nes", "al\u00b7ler\u00b7liebs\u00b7ten", "Fu\u00b7\u00dfes"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wei\u00dfes Spitzchen in die Luft.", "tokens": ["Wei\u00b7\u00dfes", "Spitz\u00b7chen", "in", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Don Fadrique kr\u00fcmmt den R\u00fccken,", "tokens": ["Don", "Fad\u00b7ri\u00b7que", "kr\u00fcmmt", "den", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Will das wei\u00dfe Spitzchen k\u00fcssen,", "tokens": ["Will", "das", "wei\u00b7\u00dfe", "Spitz\u00b7chen", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Knabe Amor steht bei Seite,", "tokens": ["Kna\u00b7be", "A\u00b7mor", "steht", "bei", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der den Bogen lachend spannt.", "tokens": ["Der", "den", "Bo\u00b7gen", "la\u00b7chend", "spannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nach dem ewig jungen Herzen", "tokens": ["Nach", "dem", "e\u00b7wig", "jun\u00b7gen", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zielt er, doch wer lacht, der zielt schlecht:", "tokens": ["Zielt", "er", ",", "doch", "wer", "lacht", ",", "der", "zielt", "schlecht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "PWS", "VVFIN", "$,", "PRELS", "VVFIN", "ADJD", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "In des Ritters alten R\u00fccken", "tokens": ["In", "des", "Rit\u00b7ters", "al\u00b7ten", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schie\u00dft er einen Hexenschu\u00df.", "tokens": ["Schie\u00dft", "er", "ei\u00b7nen", "He\u00b7xen\u00b7schu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Don Fadrique's Knochen rasseln,", "tokens": ["Don", "Fa\u00b7dri\u00b7que's", "Kno\u00b7chen", "ras\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Don Fadrique st\u00fcrzt zusammen,", "tokens": ["Don", "Fad\u00b7ri\u00b7que", "st\u00fcrzt", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PTKVZ", "$,"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Figaro holt eine S\u00e4nfte,", "tokens": ["Fi\u00b7ga\u00b7ro", "holt", "ei\u00b7ne", "S\u00e4nf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Figaro bringt ihn zu Bett.", "tokens": ["Fi\u00b7ga\u00b7ro", "bringt", "ihn", "zu", "Bett", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "\u201efrommer Bruder Agostino,", "tokens": ["\u201e", "from\u00b7mer", "Bru\u00b7der", "A\u00b7gos\u00b7ti\u00b7no", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "NE", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Exorcire mir das frevle", "tokens": ["Ex\u00b7or\u00b7ci\u00b7re", "mir", "das", "frev\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Allerliebste wei\u00dfe F\u00fc\u00dfchen", "tokens": ["Al\u00b7ler\u00b7liebs\u00b7te", "wei\u00b7\u00dfe", "F\u00fc\u00df\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das durch meine Beichte tanzt!\u201c", "tokens": ["Das", "durch", "mei\u00b7ne", "Beich\u00b7te", "tanzt", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "APPR", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Don Fadrique sucht den Hades,", "tokens": ["Don", "Fad\u00b7ri\u00b7que", "sucht", "den", "Ha\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zierlich schreitend wie ein Stutzer,", "tokens": ["Zier\u00b7lich", "schrei\u00b7tend", "wie", "ein", "Stut\u00b7zer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "T\u00e4nzelnd leuchtet ihm ein wei\u00dfes", "tokens": ["T\u00e4n\u00b7zelnd", "leuch\u00b7tet", "ihm", "ein", "wei\u00b7\u00dfes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fc\u00dfchen durch die Unterwelt.", "tokens": ["F\u00fc\u00df\u00b7chen", "durch", "die", "Un\u00b7ter\u00b7welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}