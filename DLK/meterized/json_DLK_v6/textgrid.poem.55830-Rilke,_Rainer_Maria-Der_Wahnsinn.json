{"textgrid.poem.55830": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Der Wahnsinn", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie mu\u00df immer sinnen: Ich bin... ich bin...", "tokens": ["Sie", "mu\u00df", "im\u00b7mer", "sin\u00b7nen", ":", "Ich", "bin", "...", "ich", "bin", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$.", "PPER", "VAFIN", "$(", "PPER", "VAFIN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wer bist du denn, Marie?", "tokens": ["Wer", "bist", "du", "denn", ",", "Ma\u00b7rie", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "$,", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Eine K\u00f6nigin, eine K\u00f6nigin!", "tokens": ["Ei\u00b7ne", "K\u00f6\u00b7ni\u00b7gin", ",", "ei\u00b7ne", "K\u00f6\u00b7ni\u00b7gin", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "In die Kniee vor mir, in die Knie!", "tokens": ["In", "die", "Kni\u00b7ee", "vor", "mir", ",", "in", "die", "Knie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPER", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Sie mu\u00df immer weinen: Ich war... ich war...", "tokens": ["Sie", "mu\u00df", "im\u00b7mer", "wei\u00b7nen", ":", "Ich", "war", "...", "ich", "war", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$.", "PPER", "VAFIN", "$(", "PPER", "VAFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wer warst du denn, Marie?", "tokens": ["Wer", "warst", "du", "denn", ",", "Ma\u00b7rie", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "$,", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Niemandskind, ganz arm und bar,", "tokens": ["Ein", "Nie\u00b7mands\u00b7kind", ",", "ganz", "arm", "und", "bar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und ich kann dir nicht sagen wie.", "tokens": ["und", "ich", "kann", "dir", "nicht", "sa\u00b7gen", "wie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "KOKOM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wurdest aus einem solchen Kind", "tokens": ["Und", "wur\u00b7dest", "aus", "ei\u00b7nem", "sol\u00b7chen", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ART", "PIAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "eine F\u00fcrstin, vor der man kniet?", "tokens": ["ei\u00b7ne", "F\u00fcrs\u00b7tin", ",", "vor", "der", "man", "kniet", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "PIS", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Weil die Dinge alle anders sind,", "tokens": ["Weil", "die", "Din\u00b7ge", "al\u00b7le", "an\u00b7ders", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PIS", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "als man sie beim Betteln sieht.", "tokens": ["als", "man", "sie", "beim", "Bet\u00b7teln", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "So haben die Dinge dich gro\u00df gemacht,", "tokens": ["So", "ha\u00b7ben", "die", "Din\u00b7ge", "dich", "gro\u00df", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "und kannst du noch sagen wann?", "tokens": ["und", "kannst", "du", "noch", "sa\u00b7gen", "wann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF", "PWAV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Eine Nacht, eine Nacht, \u00fcber ", "tokens": ["Ei\u00b7ne", "Nacht", ",", "ei\u00b7ne", "Nacht", ",", "\u00fc\u00b7ber"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "APPR"], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "und sie sprachen mich anders an.", "tokens": ["und", "sie", "spra\u00b7chen", "mich", "an\u00b7ders", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Ich trat in die Gasse hinaus und sieh:", "tokens": ["Ich", "trat", "in", "die", "Gas\u00b7se", "hin\u00b7aus", "und", "sieh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "KON", "VVIMP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "die ist wie mit Saiten bespannt;", "tokens": ["die", "ist", "wie", "mit", "Sai\u00b7ten", "be\u00b7spannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "KOKOM", "APPR", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "da wurde Marie Melodie, Melodie...", "tokens": ["da", "wur\u00b7de", "Ma\u00b7rie", "Me\u00b7lo\u00b7die", ",", "Me\u00b7lo\u00b7die", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "NE", "$,", "NN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "und tanzte von Rand zu Rand.", "tokens": ["und", "tanz\u00b7te", "von", "Rand", "zu", "Rand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die Leute schlichen so \u00e4ngstlich hin,", "tokens": ["Die", "Leu\u00b7te", "schli\u00b7chen", "so", "\u00e4ngst\u00b7lich", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "wie hart an die H\u00e4user gepflanzt, \u2013", "tokens": ["wie", "hart", "an", "die", "H\u00e4u\u00b7ser", "ge\u00b7pflanzt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADJD", "APPR", "ART", "NN", "VVPP", "$,", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "denn das darf doch nur eine K\u00f6nigin,", "tokens": ["denn", "das", "darf", "doch", "nur", "ei\u00b7ne", "K\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "da\u00df sie tanzt in den Gassen: tanzt!...", "tokens": ["da\u00df", "sie", "tanzt", "in", "den", "Gas\u00b7sen", ":", "tanzt", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$.", "VVFIN", "$.", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}