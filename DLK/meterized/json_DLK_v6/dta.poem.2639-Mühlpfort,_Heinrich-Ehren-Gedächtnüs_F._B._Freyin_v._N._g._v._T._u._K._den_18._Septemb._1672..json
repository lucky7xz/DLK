{"dta.poem.2639": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ehren-Ged\u00e4chtn\u00fcs  \n F. B. Freyin v. N. g. v. T. u. K. den 18.  \n Septemb. 1672.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein hartes Wort: Mensch/ du must Asche werden:", "tokens": ["Es", "ist", "ein", "har\u00b7tes", "Wort", ":", "Mensch", "/", "du", "must", "A\u00b7sche", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "NN", "$(", "PPER", "VMFIN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ja die Natur erschrickt/ wenn sie soll untergehn/", "tokens": ["Ja", "die", "Na\u00b7tur", "er\u00b7schrickt", "/", "wenn", "sie", "soll", "un\u00b7ter\u00b7gehn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "VVPP", "$(", "KOUS", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und kl\u00e4glich/ da\u00df der Herr und Herrscher dieser Erden", "tokens": ["Und", "kl\u00e4g\u00b7lich", "/", "da\u00df", "der", "Herr", "und", "Herr\u00b7scher", "die\u00b7ser", "Er\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$(", "KOUS", "ART", "NN", "KON", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Offt nicht so lange kan als Baum/ und Blumen stehn.", "tokens": ["Offt", "nicht", "so", "lan\u00b7ge", "kan", "als", "Baum", "/", "und", "Blu\u00b7men", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "ADV", "VMFIN", "KOKOM", "NE", "$(", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Di\u00df Elend hat auch l\u00e4ngst die kluge Welt bewogen/", "tokens": ["Di\u00df", "E\u00b7lend", "hat", "auch", "l\u00e4ngst", "die", "klu\u00b7ge", "Welt", "be\u00b7wo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df sie die Sterbligkeit zu trotzen hat gedacht/", "tokens": ["Da\u00df", "sie", "die", "Ster\u00b7blig\u00b7keit", "zu", "trot\u00b7zen", "hat", "ge\u00b7dacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKZU", "VVINF", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und durch viel hohe B\u00e4u/ und sch\u00f6ne Sieges-Bogen", "tokens": ["Und", "durch", "viel", "ho\u00b7he", "B\u00e4u", "/", "und", "sch\u00f6\u00b7ne", "Sie\u00b7ges\u00b7Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN", "$(", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sich gleichsam wie bewehrt f\u00fcr Tod und Fall gemacht.", "tokens": ["Sich", "gleich\u00b7sam", "wie", "be\u00b7wehrt", "f\u00fcr", "Tod", "und", "Fall", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "KOKOM", "VVPP", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es lag die Ewigkeit den Heyden in dem Hertzen/", "tokens": ["Es", "lag", "die", "E\u00b7wig\u00b7keit", "den", "Hey\u00b7den", "in", "dem", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So doch ein Schatten-Werck von eitlem Hochmuth war;", "tokens": ["So", "doch", "ein", "Schat\u00b7ten\u00b7\u00b7Werck", "von", "eit\u00b7lem", "Hoch\u00b7muth", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sie f\u00fchrten Tempel auff/ und weyhten Opffer-Kertzen/", "tokens": ["Sie", "f\u00fchr\u00b7ten", "Tem\u00b7pel", "auff", "/", "und", "weyh\u00b7ten", "Opf\u00b7fer\u00b7Kert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "$(", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ja wurden gar zuletzt Abg\u00f6tter ihrer Baar\u2019.", "tokens": ["Ja", "wur\u00b7den", "gar", "zu\u00b7letzt", "Ab\u00b7g\u00f6t\u00b7ter", "ih\u00b7rer", "Baa\u00b7r'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VAFIN", "ADV", "ADV", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich wil Egyptens Pracht und Wunder nicht erzehlen/", "tokens": ["Ich", "wil", "E\u00b7gyp\u00b7tens", "Pracht", "und", "Wun\u00b7der", "nicht", "er\u00b7zeh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NE", "NN", "KON", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und wie es G\u00f6ttern gleich die Leichen hat geehrt:", "tokens": ["Und", "wie", "es", "G\u00f6t\u00b7tern", "gleich", "die", "Lei\u00b7chen", "hat", "ge\u00b7ehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "NN", "ADV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es wird auch Persien an Herrligkeit nicht fehlen/", "tokens": ["Es", "wird", "auch", "Per\u00b7si\u00b7en", "an", "Herr\u00b7lig\u00b7keit", "nicht", "feh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NE", "APPR", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da\u00df in dem Mesar noch der Todten Lob vermehrt.", "tokens": ["Da\u00df", "in", "dem", "Me\u00b7sar", "noch", "der", "Tod\u00b7ten", "Lob", "ver\u00b7mehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADV", "ART", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Doch der Sineser Witz hat alles \u00fcberstiegen/", "tokens": ["Doch", "der", "Si\u00b7ne\u00b7ser", "Witz", "hat", "al\u00b7les", "\u00fc\u00b7bers\u00b7tie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "PIS", "VVINF", "$("], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "Wo Jrrthum auch ein Theil kan von der Wei\u00dfheit seyn:", "tokens": ["Wo", "Jrr\u00b7thum", "auch", "ein", "Theil", "kan", "von", "der", "Wei\u00df\u00b7heit", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ART", "NN", "VMFIN", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Sie wolten mit Gewalt den Untergang besiegen/", "tokens": ["Sie", "wol\u00b7ten", "mit", "Ge\u00b7walt", "den", "Un\u00b7ter\u00b7gang", "be\u00b7sie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und hieben Grab und Grufft den harten Felsen ein.", "tokens": ["Und", "hie\u00b7ben", "Grab", "und", "Grufft", "den", "har\u00b7ten", "Fel\u00b7sen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Kein F\u00fcrstlich Zimmer kan an Kostbarkeit so prangen/", "tokens": ["Kein", "F\u00fcrst\u00b7lich", "Zim\u00b7mer", "kan", "an", "Kost\u00b7bar\u00b7keit", "so", "pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VMFIN", "APPR", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Als sie des Grabes Pfort und Kammern ausgeziert/", "tokens": ["Als", "sie", "des", "Gra\u00b7bes", "Pfort", "und", "Kam\u00b7mern", "aus\u00b7ge\u00b7ziert", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Worinn des Todten Bild/ und Thaten auffgehangen/", "tokens": ["Wo\u00b7rinn", "des", "Tod\u00b7ten", "Bild", "/", "und", "Tha\u00b7ten", "auff\u00b7ge\u00b7han\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$(", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und aller Uberflu\u00df des Reichthums ward gesp\u00fcrt.", "tokens": ["Und", "al\u00b7ler", "U\u00b7ber\u00b7flu\u00df", "des", "Reicht\u00b7hums", "ward", "ge\u00b7sp\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Doch die Pagoden sind mit nichten zu vergleichen", "tokens": ["Doch", "die", "Pa\u00b7go\u00b7den", "sind", "mit", "nich\u00b7ten", "zu", "ver\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "APPR", "PIS", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der jenen Raserey/ womit sie mehr befleckt/", "tokens": ["Der", "je\u00b7nen", "Ra\u00b7se\u00b7rey", "/", "wo\u00b7mit", "sie", "mehr", "be\u00b7fleckt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "$(", "PWAV", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wenn sie ein ewig Ziel gedachten zu erreichen/", "tokens": ["Wenn", "sie", "ein", "e\u00b7wig", "Ziel", "ge\u00b7dach\u00b7ten", "zu", "er\u00b7rei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJD", "NN", "VVPP", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und da\u00df ihr Leben sey unendlich ausgestreckt:", "tokens": ["Und", "da\u00df", "ihr", "Le\u00b7ben", "sey", "un\u00b7end\u00b7lich", "aus\u00b7ge\u00b7streckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Es solt ein Nectar-Tranck sie von der Grufft befreyen/", "tokens": ["Es", "solt", "ein", "Nec\u00b7ta\u00b7rTranck", "sie", "von", "der", "Grufft", "be\u00b7fre\u00b7yen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.30": {"text": "So ihnen der Chymi verborgne Kunst geschenckt;", "tokens": ["So", "ih\u00b7nen", "der", "Chy\u00b7mi", "ver\u00b7borg\u00b7ne", "Kunst", "ge\u00b7schenckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NE", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.31": {"text": "Allein die Lachesis hat nie gescheut ihr dreuen/", "tokens": ["Al\u00b7lein", "die", "La\u00b7che\u00b7sis", "hat", "nie", "ge\u00b7scheut", "ihr", "dreu\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADV", "VVPP", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Sie wurden wie zuvor nur in den Sand gesenckt.", "tokens": ["Sie", "wur\u00b7den", "wie", "zu\u00b7vor", "nur", "in", "den", "Sand", "ge\u00b7senckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "ADV", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "So viel vermag der Mensch/ so weit gehn seine Wercke/", "tokens": ["So", "viel", "ver\u00b7mag", "der", "Mensch", "/", "so", "weit", "gehn", "sei\u00b7ne", "Wer\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "$(", "ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und von der Ewigkeit blo\u00df eingebildter Wahn:", "tokens": ["Und", "von", "der", "E\u00b7wig\u00b7keit", "blo\u00df", "ein\u00b7ge\u00b7bild\u00b7ter", "Wahn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Gekr\u00f6nter H\u00e4upter Macht/ ber\u00fchmter Helden St\u00e4rcke/", "tokens": ["Ge\u00b7kr\u00f6n\u00b7ter", "H\u00e4up\u00b7ter", "Macht", "/", "be\u00b7r\u00fchm\u00b7ter", "Hel\u00b7den", "St\u00e4r\u00b7cke", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$(", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Gelehrter K\u00f6pffe Witz geht die gemeine Bahn.", "tokens": ["Ge\u00b7lehr\u00b7ter", "K\u00f6pf\u00b7fe", "Witz", "geht", "die", "ge\u00b7mei\u00b7ne", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Der Schauplatz dieser Welt hegt seine Trauer-Spiele/", "tokens": ["Der", "Schau\u00b7platz", "die\u00b7ser", "Welt", "hegt", "sei\u00b7ne", "Trau\u00b7e\u00b7rSpie\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "So der elende Mensch nicht satt bejammern mag/", "tokens": ["So", "der", "e\u00b7len\u00b7de", "Mensch", "nicht", "satt", "be\u00b7jam\u00b7mern", "mag", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKNEG", "ADJD", "VVINF", "VMFIN", "$("], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.39": {"text": "Er ist Gefahr und Noth gesetzt zu einem Ziele/", "tokens": ["Er", "ist", "Ge\u00b7fahr", "und", "Noth", "ge\u00b7setzt", "zu", "ei\u00b7nem", "Zie\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ein Ambo\u00df wo das Gl\u00fcck vollf\u00fchret manchen Schlag.", "tokens": ["Ein", "Am\u00b7bo\u00df", "wo", "das", "Gl\u00fcck", "voll\u00b7f\u00fch\u00b7ret", "man\u00b7chen", "Schlag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PWAV", "ART", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.41": {"text": "Des Solons Schlu\u00df bleibt wahr/ da\u00df vor der letzten Stunde", "tokens": ["Des", "So\u00b7lons", "Schlu\u00df", "bleibt", "wahr", "/", "da\u00df", "vor", "der", "letz\u00b7ten", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "ADJD", "$(", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "K