{"textgrid.poem.32485": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Antwort auf die Frage: wer ist der gro\u00dfe Duns?", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mann in \u2013 \u2013, welchen ", "tokens": ["Der", "Mann", "in", "\u2013", "\u2013", ",", "wel\u00b7chen"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word"], "pos": ["ART", "NN", "APPR", "$(", "$(", "$,", "PWAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Nicht schuf zum Dichter und Kunstrichter,", "tokens": ["Nicht", "schuf", "zum", "Dich\u00b7ter", "und", "Kuns\u00b7trich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der, d\u00fcmmer als ein Hottentott,", "tokens": ["Der", ",", "d\u00fcm\u00b7mer", "als", "ein", "Hot\u00b7ten\u00b7tott", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sagt, er und S *** w\u00e4ren Dichter;", "tokens": ["Sagt", ",", "er", "und", "S", "*", "*", "*", "w\u00e4\u00b7ren", "Dich\u00b7ter", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "KON", "XY", "XY", "XY", "XY", "VAFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der Philip Zesen unsrer Zeit;", "tokens": ["Der", "Phi\u00b7lip", "Ze\u00b7sen", "uns\u00b7rer", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der B\u00fcttel der Sprachreinigkeit", "tokens": ["Der", "B\u00fct\u00b7tel", "der", "Sprach\u00b7rei\u00b7nig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In Ober- und in Niedersachsen,", "tokens": ["In", "O\u00b7ber", "und", "in", "Nie\u00b7der\u00b7sach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der alle Worte Lands verweist,", "tokens": ["Der", "al\u00b7le", "Wor\u00b7te", "Lands", "ver\u00b7weist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die nicht auf deutschem Boden wachsen;", "tokens": ["Die", "nicht", "auf", "deut\u00b7schem", "Bo\u00b7den", "wach\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der gro\u00dfe Mann, der stark von Leib", "tokens": ["Der", "gro\u00b7\u00dfe", "Mann", ",", "der", "stark", "von", "Leib"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ein kleines artigs freundlichs Weib", "tokens": ["Ein", "klei\u00b7nes", "ar\u00b7tigs", "freund\u00b7lichs", "Weib"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Kalt, wie er denkt und schreibt, umarmt,", "tokens": ["Kalt", ",", "wie", "er", "denkt", "und", "schreibt", ",", "um\u00b7armt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Das aber seiner sich erbarmt,", "tokens": ["Das", "a\u00b7ber", "sei\u00b7ner", "sich", "er\u00b7barmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PPOSAT", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und gleicher Meinung ist und bleibt,", "tokens": ["Und", "glei\u00b7cher", "Mei\u00b7nung", "ist", "und", "bleibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und wider ihn nicht denkt nicht schreibt,", "tokens": ["Und", "wi\u00b7der", "ihn", "nicht", "denkt", "nicht", "schreibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "PTKNEG", "VVFIN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Weil es den Zank der Ehe scheut,", "tokens": ["Weil", "es", "den", "Zank", "der", "E\u00b7he", "scheut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und lieber aus Gef\u00e4lligkeit", "tokens": ["Und", "lie\u00b7ber", "aus", "Ge\u00b7f\u00e4l\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Sich an des Manns Gedanken bindet;", "tokens": ["Sich", "an", "des", "Manns", "Ge\u00b7dan\u00b7ken", "bin\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Der Mann der unter uns", "tokens": ["Der", "Mann", "der", "un\u00b7ter", "uns"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Viel gro\u00dfe Geister findet,", "tokens": ["Viel", "gro\u00b7\u00dfe", "Geis\u00b7ter", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Der ist der gro\u00dfe Duns!", "tokens": ["Der", "ist", "der", "gro\u00b7\u00dfe", "Duns", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}