{"textgrid.poem.35221": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "[das mich gebohren hat/ hat einen rohten Kopf]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das mich gebohren hat/ hat ein\u0113 rohten Kopf:", "tokens": ["Das", "mich", "ge\u00b7boh\u00b7ren", "hat", "/", "hat", "ein\u0113", "roh\u00b7ten", "Kopf", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVPP", "VAFIN", "$(", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Doch auch offt einen gelb-/ offt wei\u00df-/ offt schwartzen Schopf/", "tokens": ["Doch", "auch", "offt", "ei\u00b7nen", "gelb", "/", "offt", "wei\u00df", "/", "offt", "schwart\u00b7zen", "Schopf", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "TRUNC", "$(", "ADV", "TRUNC", "$(", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich werd'/ und sterbe bald. Bin offt nur eine Beute.", "tokens": ["Ich", "werd'", "/", "und", "ster\u00b7be", "bald", ".", "Bin", "offt", "nur", "ei\u00b7ne", "Beu\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "KON", "VVFIN", "ADV", "$.", "VAFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Man z\u00fcrnet \u00fcber mir: und hat mich doch so lieb/", "tokens": ["Man", "z\u00fcr\u00b7net", "\u00fc\u00b7ber", "mir", ":", "und", "hat", "mich", "doch", "so", "lieb", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPER", "$.", "KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als etwas von der Welt. Mein Vatter ist ein Dieb:", "tokens": ["Als", "et\u00b7was", "von", "der", "Welt", ".", "Mein", "Vat\u00b7ter", "ist", "ein", "Dieb", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "$.", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Mutter offt ein Sakk. Ich scheue Liecht und Leute.", "tokens": ["Die", "Mut\u00b7ter", "offt", "ein", "Sakk", ".", "Ich", "scheu\u00b7e", "Liecht", "und", "Leu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "$.", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich werd' in einer Stund wohl tausendmahl erzeugt.", "tokens": ["Ich", "werd'", "in", "ei\u00b7ner", "Stund", "wohl", "tau\u00b7send\u00b7mahl", "er\u00b7zeugt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mein Freund ist auch mein Feind. Mein Feind ist mir geneigt.", "tokens": ["Mein", "Freund", "ist", "auch", "mein", "Feind", ".", "Mein", "Feind", "ist", "mir", "ge\u00b7neigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}