{"textgrid.poem.66564": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Warum verstehn sie's denn nicht, warum?", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum verstehn sie's denn nicht, warum?", "tokens": ["Wa\u00b7rum", "ver\u00b7stehn", "sie's", "denn", "nicht", ",", "wa\u00b7rum", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "PWAV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es ist ja doch klar zu sehen!", "tokens": ["Es", "ist", "ja", "doch", "klar", "zu", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ei was, sie schn\u00fcffeln nur dran herum,", "tokens": ["Ei", "was", ",", "sie", "schn\u00fcf\u00b7feln", "nur", "dran", "he\u00b7rum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "$,", "PPER", "VVFIN", "ADV", "PAV", "PTKVZ", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und wollen's gar nicht verstehen!", "tokens": ["Und", "wol\u00b7len's", "gar", "nicht", "ver\u00b7ste\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Vergn\u00fcglicher ist der Mi\u00dfverstand,", "tokens": ["Ver\u00b7gn\u00fcg\u00b7li\u00b7cher", "ist", "der", "Mi\u00df\u00b7ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dem Tro\u00df und seinem Geschwister;", "tokens": ["Dem", "Tro\u00df", "und", "sei\u00b7nem", "Ge\u00b7schwis\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Im Bl\u00f6dsinn reichen sich l\u00e4chelnd die Hand", "tokens": ["Im", "Bl\u00f6d\u00b7sinn", "rei\u00b7chen", "sich", "l\u00e4\u00b7chelnd", "die", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "ADJD", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Herr Leichtfu\u00df und Herr Philister.", "tokens": ["Herr", "Leicht\u00b7fu\u00df", "und", "Herr", "Phi\u00b7lis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "NE", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}}}}}