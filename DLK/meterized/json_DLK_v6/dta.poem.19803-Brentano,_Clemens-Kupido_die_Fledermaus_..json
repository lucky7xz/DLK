{"dta.poem.19803": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Kupido die Fledermaus .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als ich verwichen lag in sanfter Ruh,               ", "tokens": ["Als", "ich", "ver\u00b7wi\u00b7chen", "lag", "in", "sanf\u00b7ter", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da klopft an meiner Th\u00fcr,", "tokens": ["Da", "klopft", "an", "mei\u00b7ner", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und kommet auch zu mir,", "tokens": ["Und", "kom\u00b7met", "auch", "zu", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein kleiner Bue!", "tokens": ["Ein", "klei\u00b7ner", "Bue", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Schneewei\u00df ist er gekleidt, von Angesicht blind,", "tokens": ["Schnee\u00b7wei\u00df", "ist", "er", "ge\u00b7kleidt", ",", "von", "An\u00b7ge\u00b7sicht", "blind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Er stellt sich an die Wand,", "tokens": ["Er", "stellt", "sich", "an", "die", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Fackel in der Hand,", "tokens": ["Ein", "Fa\u00b7ckel", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das lose Kind!", "tokens": ["Das", "lo\u00b7se", "Kind", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Was das bedeuten soll, schrie ich darauf,", "tokens": ["Was", "das", "be\u00b7deu\u00b7ten", "soll", ",", "schrie", "ich", "da\u00b7rauf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "VVINF", "VMFIN", "$,", "VVFIN", "PPER", "PAV", "$,"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Schweig still, es geschieht dir nichts!", "tokens": ["Schweig", "still", ",", "es", "ge\u00b7schieht", "dir", "nichts", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "PIS", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Schweig still, ich thu dir nichts,", "tokens": ["Schweig", "still", ",", "ich", "thu", "dir", "nichts", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "PIS", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sprach er darauf?", "tokens": ["Sprach", "er", "da\u00b7rauf", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "PAV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Er geht zum Bette hin, der kleine Fratz,", "tokens": ["Er", "geht", "zum", "Bet\u00b7te", "hin", ",", "der", "klei\u00b7ne", "Fratz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er bittet mich gar sch\u00f6n,", "tokens": ["Er", "bit\u00b7tet", "mich", "gar", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sollt aus dem Wege gehn,", "tokens": ["Sollt", "aus", "dem", "We\u00b7ge", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sollt machen Platz.", "tokens": ["Sollt", "ma\u00b7chen", "Platz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Ey du verdammtes Kind! was bildst dir ein,", "tokens": ["Ey", "du", "ver\u00b7damm\u00b7tes", "Kind", "!", "was", "bildst", "dir", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADJA", "NN", "$.", "PWS", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Willst schon im Bette liegen,", "tokens": ["Willst", "schon", "im", "Bet\u00b7te", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Geh\u00f6rst noch in die Wiegen,", "tokens": ["Ge\u00b7h\u00f6rst", "noch", "in", "die", "Wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In die Wickel hinein.", "tokens": ["In", "die", "Wi\u00b7ckel", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Scheer dich vom Bett und geh nach Haus", "tokens": ["Scheer", "dich", "vom", "Bett", "und", "geh", "nach", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPRART", "NN", "KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Anstatt der Liebesglut", "tokens": ["An\u00b7statt", "der", "Lie\u00b7bes\u00b7glut"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Geh\u00f6rt dir noch die Ruth,", "tokens": ["Ge\u00b7h\u00f6rt", "dir", "noch", "die", "Ruth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du Fledermaus!", "tokens": ["Du", "Fle\u00b7der\u00b7maus", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}