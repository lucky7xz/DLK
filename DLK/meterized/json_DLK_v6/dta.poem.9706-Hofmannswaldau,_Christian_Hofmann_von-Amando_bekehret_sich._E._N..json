{"dta.poem.9706": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Amando bekehret sich.  \n E. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die liebe lescht nun aus/ da ihre pf\u00e4nder brennen.", "tokens": ["Die", "lie\u00b7be", "lescht", "nun", "aus", "/", "da", "ih\u00b7re", "pf\u00e4n\u00b7der", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PTKVZ", "$(", "KOUS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man soll mein kaltes hertz an dieser glut erkennen.", "tokens": ["Man", "soll", "mein", "kal\u00b7tes", "hertz", "an", "die\u00b7ser", "glut", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPOSAT", "ADJA", "NN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jhr briefe lodert auf/ ihr lieder m\u00fcst hinein/", "tokens": ["Ihr", "brie\u00b7fe", "lo\u00b7dert", "auf", "/", "ihr", "lie\u00b7der", "m\u00fcst", "hin\u00b7ein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "$(", "PPOSAT", "NN", "VMFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du solst/ du zauberband/ zu staub und asche seyn.", "tokens": ["Du", "solst", "/", "du", "zau\u00b7ber\u00b7band", "/", "zu", "staub", "und", "asc\u00b7he", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "PPER", "VVFIN", "$(", "PTKA", "ADJD", "KON", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So bleibt mir dieser trost von meinen liebes-sachen/", "tokens": ["So", "bleibt", "mir", "die\u00b7ser", "trost", "von", "mei\u00b7nen", "lie\u00b7bes\u00b7sa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN", "APPR", "PPOSAT", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df sie zu guter letzt ein freuden-feuer machen.", "tokens": ["Da\u00df", "sie", "zu", "gu\u00b7ter", "letzt", "ein", "freu\u00b7den\u00b7feu\u00b7er", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}