{"textgrid.poem.59446": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der stille Grund", "genre": "verse", "period": "N.A.", "pub_year": 1835, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mondenschein verwirret", "tokens": ["Der", "Mon\u00b7den\u00b7schein", "ver\u00b7wir\u00b7ret"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die T\u00e4ler weit und breit,", "tokens": ["Die", "T\u00e4\u00b7ler", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die B\u00e4chlein, wie verirret,", "tokens": ["Die", "B\u00e4ch\u00b7lein", ",", "wie", "ver\u00b7ir\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gehn durch die Einsamkeit.", "tokens": ["Gehn", "durch", "die", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da dr\u00fcben sah ich stehen", "tokens": ["Da", "dr\u00fc\u00b7ben", "sah", "ich", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Wald auf steiler H\u00f6h,", "tokens": ["Den", "Wald", "auf", "stei\u00b7ler", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die finstern Tannen sehen", "tokens": ["Die", "fins\u00b7tern", "Tan\u00b7nen", "se\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In einen tiefen See.", "tokens": ["In", "ei\u00b7nen", "tie\u00b7fen", "See", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein Kahn wohl sah ich ragen,", "tokens": ["Ein", "Kahn", "wohl", "sah", "ich", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch niemand, der es lenkt,", "tokens": ["Doch", "nie\u00b7mand", ",", "der", "es", "lenkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Ruder war zerschlagen,", "tokens": ["Das", "Ru\u00b7der", "war", "zer\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Schifflein halb versenkt.", "tokens": ["Das", "Schif\u00b7flein", "halb", "ver\u00b7senkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Eine Nixe auf dem Steine", "tokens": ["Ei\u00b7ne", "Ni\u00b7xe", "auf", "dem", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Flocht dort ihr goldnes Haar,", "tokens": ["Flocht", "dort", "ihr", "gold\u00b7nes", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sie meint' sie w\u00e4r alleine,", "tokens": ["Sie", "meint'", "sie", "w\u00e4r", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sang so wunderbar.", "tokens": ["Und", "sang", "so", "wun\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie sang und sang, in den B\u00e4umen", "tokens": ["Sie", "sang", "und", "sang", ",", "in", "den", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und Quellen rauscht' es sacht", "tokens": ["Und", "Quel\u00b7len", "rauscht'", "es", "sacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und fl\u00fcsterte wie in Tr\u00e4umen", "tokens": ["Und", "fl\u00fcs\u00b7ter\u00b7te", "wie", "in", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die mondbegl\u00e4nzte Nacht.", "tokens": ["Die", "mond\u00b7be\u00b7gl\u00e4nz\u00b7te", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich aber stand erschrocken,", "tokens": ["Ich", "a\u00b7ber", "stand", "er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Denn \u00fcber Wald und Kluft", "tokens": ["Denn", "\u00fc\u00b7ber", "Wald", "und", "Kluft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Klangen die Morgenglocken", "tokens": ["Klan\u00b7gen", "die", "Mor\u00b7gen\u00b7glo\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Schon ferne durch die Luft.", "tokens": ["Schon", "fer\u00b7ne", "durch", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und h\u00e4tt ich nicht vernommen", "tokens": ["Und", "h\u00e4tt", "ich", "nicht", "ver\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Klang zu guter Stund,", "tokens": ["Den", "Klang", "zu", "gu\u00b7ter", "Stund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "W\u00e4r nimmermehr gekommen", "tokens": ["W\u00e4r", "nim\u00b7mer\u00b7mehr", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus diesem stillen Grund.", "tokens": ["Aus", "die\u00b7sem", "stil\u00b7len", "Grund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}