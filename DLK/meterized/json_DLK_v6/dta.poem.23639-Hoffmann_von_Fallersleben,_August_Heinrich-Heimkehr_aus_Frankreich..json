{"dta.poem.23639": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Heimkehr aus Frankreich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Deutsche Worte h\u00f6r' ich wieder \u2014", "tokens": ["Deut\u00b7sche", "Wor\u00b7te", "h\u00f6r'", "ich", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sei gegr\u00fc\u00dft mit Herz und Hand!", "tokens": ["Sei", "ge\u00b7gr\u00fc\u00dft", "mit", "Herz", "und", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Land der Freude, Land der Lieder,", "tokens": ["Land", "der", "Freu\u00b7de", ",", "Land", "der", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00f6nes heitres Vaterland!", "tokens": ["Sch\u00f6\u00b7nes", "heit\u00b7res", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fr\u00f6hlich kehr' ich nun zur\u00fcck,", "tokens": ["Fr\u00f6h\u00b7lich", "kehr'", "ich", "nun", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Deutschland du mein Trost, mein Gl\u00fcck!", "tokens": ["Deutschland", "du", "mein", "Trost", ",", "mein", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O wie sehnt' ich mich so lange", "tokens": ["O", "wie", "sehnt'", "ich", "mich", "so", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "PRF", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch nach dir, du meine Braut,", "tokens": ["Doch", "nach", "dir", ",", "du", "mei\u00b7ne", "Braut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wie ward mir freudebange,", "tokens": ["Und", "wie", "ward", "mir", "freu\u00b7de\u00b7ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als ich wieder dich erschaut!", "tokens": ["Als", "ich", "wie\u00b7der", "dich", "er\u00b7schaut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weg mit w\u00e4lschem Lug und Tand \u2014", "tokens": ["Weg", "mit", "w\u00e4l\u00b7schem", "Lug", "und", "Tand"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Deutschland ist mein Vaterland!", "tokens": ["Deutschland", "ist", "mein", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Alles Guten, alles Sch\u00f6nen", "tokens": ["Al\u00b7les", "Gu\u00b7ten", ",", "al\u00b7les", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reich sel'ge Heimath du!", "tokens": ["Reich", "sel'\u00b7ge", "Hei\u00b7math", "du", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Fluch den Fremden die dich h\u00f6hnen,", "tokens": ["Fluch", "den", "Frem\u00b7den", "die", "dich", "h\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fluch den Feinden deiner Ruh!", "tokens": ["Fluch", "den", "Fein\u00b7den", "dei\u00b7ner", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sei gegr\u00fc\u00dft mit Herz und Hand", "tokens": ["Sei", "ge\u00b7gr\u00fc\u00dft", "mit", "Herz", "und", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Deutschland, du mein Vaterland!", "tokens": ["Deutschland", ",", "du", "mein", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}