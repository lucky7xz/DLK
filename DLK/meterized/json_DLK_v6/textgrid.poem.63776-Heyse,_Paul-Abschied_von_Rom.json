{"textgrid.poem.63776": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Abschied von Rom", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer dich erkannt hat, scheidet nie von dir,", "tokens": ["Wer", "dich", "er\u00b7kannt", "hat", ",", "schei\u00b7det", "nie", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VAFIN", "$,", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie von der Mutter nie, die ihn geboren,", "tokens": ["Wie", "von", "der", "Mut\u00b7ter", "nie", ",", "die", "ihn", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ADV", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und trennt sich unser Leib von deinen Toren,", "tokens": ["Und", "trennt", "sich", "un\u00b7ser", "Leib", "von", "dei\u00b7nen", "To\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zur\u00fcck ein St\u00fcck der Seele lassen wir.", "tokens": ["Zu\u00b7r\u00fcck", "ein", "St\u00fcck", "der", "See\u00b7le", "las\u00b7sen", "wir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Umschlie\u00dft nicht dies geheiligte Revier,", "tokens": ["Um\u00b7schlie\u00dft", "nicht", "dies", "ge\u00b7hei\u00b7lig\u00b7te", "Re\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was sich an G\u00f6ttern je der Mensch erkoren?", "tokens": ["Was", "sich", "an", "G\u00f6t\u00b7tern", "je", "der", "Mensch", "er\u00b7ko\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bewahrt der H\u00fcgelsand nicht unverloren", "tokens": ["Be\u00b7wahrt", "der", "H\u00fc\u00b7gel\u00b7sand", "nicht", "un\u00b7ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Fu\u00dfspur aller Weltgeschlechter hier?", "tokens": ["Die", "Fu\u00df\u00b7spur", "al\u00b7ler", "Welt\u00b7ge\u00b7schlech\u00b7ter", "hier", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und wie an l\u00e4ngst vergessne Schulgeschichten", "tokens": ["Und", "wie", "an", "l\u00e4ngst", "ver\u00b7gess\u00b7ne", "Schul\u00b7ge\u00b7schich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die treue Mutter mahnt und uns dazwischen", "tokens": ["Die", "treu\u00b7e", "Mut\u00b7ter", "mahnt", "und", "uns", "da\u00b7zwi\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "PPER", "PAV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit Lieblingsspeisen pflegt und s\u00fc\u00dfen Fr\u00fcchten,", "tokens": ["Mit", "Lieb\u00b7lings\u00b7spei\u00b7sen", "pflegt", "und", "s\u00fc\u00b7\u00dfen", "Fr\u00fcch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So lockt dies Rom, das Herz sich zu erfrischen", "tokens": ["So", "lockt", "dies", "Rom", ",", "das", "Herz", "sich", "zu", "er\u00b7fri\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PDS", "NE", "$,", "ART", "NN", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An Vorzeithauch \u2013 und r\u00f6mischen Leibgerichten,", "tokens": ["An", "Vor\u00b7zeit\u00b7hauch", "\u2013", "und", "r\u00f6\u00b7mi\u00b7schen", "Leib\u00b7ge\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wie der Falcone sie wei\u00df aufzutischen.", "tokens": ["Wie", "der", "Fal\u00b7co\u00b7ne", "sie", "wei\u00df", "auf\u00b7zu\u00b7ti\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVFIN", "VVIZU", "$."], "meter": "--++--+-+-+", "measure": "iambic.penta.relaxed"}}}}}