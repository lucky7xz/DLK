{"textgrid.poem.39094": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Deutscher Kaiser, deutscher Kaiser,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deutscher Kaiser, deutscher Kaiser,", "tokens": ["Deut\u00b7scher", "Kai\u00b7ser", ",", "deut\u00b7scher", "Kai\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "sag, wie wirst du nur erscheinen?", "tokens": ["sag", ",", "wie", "wirst", "du", "nur", "er\u00b7schei\u00b7nen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tr\u00e4gst du etwa Birkenreiser,", "tokens": ["Tr\u00e4gst", "du", "et\u00b7wa", "Bir\u00b7ken\u00b7rei\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "wie der Ruprecht unsrer Kleinen?", "tokens": ["wie", "der", "Ru\u00b7precht", "uns\u00b7rer", "Klei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Oder f\u00fchrst du, mit Permisse,", "tokens": ["O\u00b7der", "f\u00fchrst", "du", ",", "mit", "Per\u00b7mis\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "eine Spritze zum Klistieren,", "tokens": ["ei\u00b7ne", "Sprit\u00b7ze", "zum", "Klis\u00b7tie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "um Beschwerden uns, gewisse", "tokens": ["um", "Be\u00b7schwer\u00b7den", "uns", ",", "ge\u00b7wis\u00b7se"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "PPER", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "dr\u00fcckende, uns abzuf\u00fchren?", "tokens": ["dr\u00fc\u00b7cken\u00b7de", ",", "uns", "ab\u00b7zu\u00b7f\u00fch\u00b7ren", "?"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVIZU", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Deutscher Kaiser, deutscher Kaiser,", "tokens": ["Deut\u00b7scher", "Kai\u00b7ser", ",", "deut\u00b7scher", "Kai\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "zwar es fr\u00f6stelt mir im Blute,", "tokens": ["zwar", "es", "fr\u00f6s\u00b7telt", "mir", "im", "Blu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "doch ich fasse mich als Weiser \u2013:", "tokens": ["doch", "ich", "fas\u00b7se", "mich", "als", "Wei\u00b7ser", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "KOUS", "NN", "$(", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "f\u00fchrst gewi\u00dflich eine Knute!", "tokens": ["f\u00fchrst", "ge\u00b7wi\u00df\u00b7lich", "ei\u00b7ne", "Knu\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}