{"textgrid.poem.46340": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Der Trinker", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hebt Vitellius den Becher in die H\u00f6h,", "tokens": ["Es", "hebt", "Vi\u00b7tel\u00b7lius", "den", "Be\u00b7cher", "in", "die", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-++-+-+-+-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Trinkt einen langen Trunk, ruft: Evan Evoe", "tokens": ["Trinkt", "ei\u00b7nen", "lan\u00b7gen", "Trunk", ",", "ruft", ":", "E\u00b7van", "E\u00b7voe"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "$.", "NE", "NE"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "O Stifter aller Lust! schenkt sch\u00e4umend wieder ein", "tokens": ["O", "Stif\u00b7ter", "al\u00b7ler", "Lust", "!", "schenkt", "sch\u00e4u\u00b7mend", "wie\u00b7der", "ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "PIAT", "NN", "$.", "VVFIN", "ADJD", "ADV", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Flicht Epheu um sein Haar und Ros' und Mirten drein,", "tokens": ["Flicht", "E\u00b7pheu", "um", "sein", "Haar", "und", "Ros'", "und", "Mir\u00b7ten", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PPOSAT", "NN", "KON", "NE", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Trinkt, ruft: o Evoe du k\u00fchlst mein hei\u00df Gehirn,", "tokens": ["Trinkt", ",", "ruft", ":", "o", "E\u00b7voe", "du", "k\u00fchlst", "mein", "hei\u00df", "Ge\u00b7hirn", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$.", "FM", "FM", "PPER", "VVFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Ich f\u00fchle Kraft und Muth und H\u00f6rner an der Stirn,", "tokens": ["Ich", "f\u00fch\u00b7le", "Kraft", "und", "Muth", "und", "H\u00f6r\u00b7ner", "an", "der", "Stirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er trinkt, h\u00fcpft wie ein Bock; die Laube dreht sich um,", "tokens": ["Er", "trinkt", ",", "h\u00fcpft", "wie", "ein", "Bock", ";", "die", "Lau\u00b7be", "dreht", "sich", "um", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "KOKOM", "ART", "NE", "$.", "ART", "NN", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Er sieht zwee Monden gehn, sein schlaffer Leib wird krum,", "tokens": ["Er", "sieht", "zwee", "Mon\u00b7den", "gehn", ",", "sein", "schlaf\u00b7fer", "Leib", "wird", "krum", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "VVINF", "$,", "PPOSAT", "ADJA", "NN", "VAFIN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er trinkt; sieht Bachus Schaar, h\u00f6rt seine Priester schreyn:", "tokens": ["Er", "trinkt", ";", "sieht", "Ba\u00b7chus", "Schaar", ",", "h\u00f6rt", "sei\u00b7ne", "Pries\u00b7ter", "schreyn", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "NE", "NN", "$,", "VVFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "O Mann und Kind zugleich, Lenaee, o Hyeu,", "tokens": ["O", "Mann", "und", "Kind", "zu\u00b7gleich", ",", "Le\u00b7nae\u00b7e", ",", "o", "Hy\u00b7eu", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "ADV", "$,", "NE", "$,", "FM", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "O Pan, o Indier, Evasta, Ligyreu!", "tokens": ["O", "Pan", ",", "o", "In\u00b7dier", ",", "E\u00b7vas\u00b7ta", ",", "Li\u00b7gy\u00b7reu", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "NN", "$,", "NE", "$,", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Sie trommeln: und er tanzt, wankt, taumelt, lallt, schl\u00e4ft ein.", "tokens": ["Sie", "trom\u00b7meln", ":", "und", "er", "tanzt", ",", "wankt", ",", "tau\u00b7melt", ",", "lallt", ",", "schl\u00e4ft", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KON", "PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}