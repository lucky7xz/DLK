{"textgrid.poem.41900": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Um, s\u00fc\u00dfe Liebste, dir verstohlen", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um, s\u00fc\u00dfe Liebste, dir verstohlen", "tokens": ["Um", ",", "s\u00fc\u00b7\u00dfe", "Liebs\u00b7te", ",", "dir", "ver\u00b7stoh\u00b7len"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUI", "$,", "ADJA", "NN", "$,", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Lied zu bringen, kam ich her;", "tokens": ["Ein", "Lied", "zu", "brin\u00b7gen", ",", "kam", "ich", "her", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch sollt' ich Schmerz f\u00fcr Lust mir holen,", "tokens": ["Doch", "sollt'", "ich", "Schmerz", "f\u00fcr", "Lust", "mir", "ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "NN", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn einsam war dein Haus und leer.", "tokens": ["Denn", "ein\u00b7sam", "war", "dein", "Haus", "und", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie soll ich jetzt die Sehnsucht zwingen,", "tokens": ["Wie", "soll", "ich", "jetzt", "die", "Sehn\u00b7sucht", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die mich nicht rasten l\u00e4\u00dft, noch ruhn?", "tokens": ["Die", "mich", "nicht", "ras\u00b7ten", "l\u00e4\u00dft", ",", "noch", "ruhn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VVINF", "VVFIN", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nichts kann ich thun,", "tokens": ["Nichts", "kann", "ich", "thun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Als schon ein neues Lied dir singen.", "tokens": ["Als", "schon", "ein", "neu\u00b7es", "Lied", "dir", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kannst du der Blumen Zahl mir nennen,", "tokens": ["Kannst", "du", "der", "Blu\u00b7men", "Zahl", "mir", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wovon die bunte Wiese gl\u00e4nzt?", "tokens": ["Wo\u00b7von", "die", "bun\u00b7te", "Wie\u00b7se", "gl\u00e4nzt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kannst du die tausend Bl\u00e4tter trennen,", "tokens": ["Kannst", "du", "die", "tau\u00b7send", "Bl\u00e4t\u00b7ter", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Womit im May der Baum sich kr\u00e4nzt?", "tokens": ["Wo\u00b7mit", "im", "May", "der", "Baum", "sich", "kr\u00e4nzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So ist die Lieb' ein Fr\u00fchlingsgarten,", "tokens": ["So", "ist", "die", "Lieb'", "ein", "Fr\u00fch\u00b7lings\u00b7gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und Lieder sind die Blumen drin;", "tokens": ["Und", "Lie\u00b7der", "sind", "die", "Blu\u00b7men", "drin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Eins welkt dahin,", "tokens": ["Eins", "welkt", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PAV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Das andre keimt, und keins' will warten.", "tokens": ["Das", "and\u00b7re", "keimt", ",", "und", "keins'", "will", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "$,", "KON", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wie mit tausendfarb'gen Strahlen", "tokens": ["Und", "wie", "mit", "tau\u00b7send\u00b7fa\u00b7rb'\u00b7gen", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dein Reiz in meine Seele dringt,", "tokens": ["Dein", "Reiz", "in", "mei\u00b7ne", "See\u00b7le", "dringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mu\u00df tausendfach der Schmuck sich malen,", "tokens": ["Mu\u00df", "tau\u00b7send\u00b7fach", "der", "Schmuck", "sich", "ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den dir die bl\u00fch'nde Liebe bringt.", "tokens": ["Den", "dir", "die", "bl\u00fch'n\u00b7de", "Lie\u00b7be", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Lust, Leid und Sehnsucht, Scherz und Klagen,", "tokens": ["Lust", ",", "Leid", "und", "Sehn\u00b7sucht", ",", "Scherz", "und", "Kla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Furcht, Hoffnung, Wahn und Tr\u00e4umerey,", "tokens": ["Furcht", ",", "Hoff\u00b7nung", ",", "Wahn", "und", "Tr\u00e4u\u00b7me\u00b7rey", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ach, was es sey,", "tokens": ["Ach", ",", "was", "es", "sey", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "PPER", "VAFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Oft wei\u00df ich's selbst dir nicht zu sagen!", "tokens": ["Oft", "wei\u00df", "ich's", "selbst", "dir", "nicht", "zu", "sa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und ob sie ewig auch sich mehren,", "tokens": ["Und", "ob", "sie", "e\u00b7wig", "auch", "sich", "meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "ADV", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie wollen alle zu dir hin,", "tokens": ["Sie", "wol\u00b7len", "al\u00b7le", "zu", "dir", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie wissen wohl, wem sie geh\u00f6ren,", "tokens": ["Sie", "wis\u00b7sen", "wohl", ",", "wem", "sie", "ge\u00b7h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und da\u00df ich nur ihr Pfleger bin.", "tokens": ["Und", "da\u00df", "ich", "nur", "ihr", "Pfle\u00b7ger", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du wirst die Kleinen nicht betr\u00fcben,", "tokens": ["Du", "wirst", "die", "Klei\u00b7nen", "nicht", "be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wird auch der Raum dir bald zu voll,", "tokens": ["Wird", "auch", "der", "Raum", "dir", "bald", "zu", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Mutter soll", "tokens": ["Die", "Mut\u00b7ter", "soll"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VMFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Ja ihre eignen Kinder lieben.", "tokens": ["Ja", "ih\u00b7re", "eig\u00b7nen", "Kin\u00b7der", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}