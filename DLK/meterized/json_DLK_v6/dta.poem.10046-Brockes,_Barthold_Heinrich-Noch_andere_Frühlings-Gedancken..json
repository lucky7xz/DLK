{"dta.poem.10046": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Noch andere Fr\u00fchlings-Gedancken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jetzt geb\u00e4hren alle Zweige junge Bl\u00e4tter ohne Zahl.", "tokens": ["Jetzt", "ge\u00b7b\u00e4h\u00b7ren", "al\u00b7le", "Zwei\u00b7ge", "jun\u00b7ge", "Bl\u00e4t\u00b7ter", "oh\u00b7ne", "Zahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Die ernehret und vergr\u00f6ssert der erw\u00e4rm\u2019nde Son-", "tokens": ["Die", "er\u00b7neh\u00b7ret", "und", "ver\u00b7gr\u00f6s\u00b7sert", "der", "er\u00b7w\u00e4r\u00b7m'n\u00b7de", "Son"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "TRUNC"], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "nen-Strahl.", "tokens": ["nen\u00b7Strahl", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Diese kleine Bl\u00e4tter bringen dem Gesicht schon eine Frucht,", "tokens": ["Die\u00b7se", "klei\u00b7ne", "Bl\u00e4t\u00b7ter", "brin\u00b7gen", "dem", "Ge\u00b7sicht", "schon", "ei\u00b7ne", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Das derselben Zart- und Sch\u00f6nheit \u00e4msig zu betrachten sucht.", "tokens": ["Das", "der\u00b7sel\u00b7ben", "Zar\u00b7t", "und", "Sch\u00f6n\u00b7heit", "\u00e4m\u00b7sig", "zu", "be\u00b7trach\u00b7ten", "sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PDAT", "TRUNC", "KON", "NN", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "--+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Nichts ist angenehm- und s\u00fcsser, als wenn das entw\u00f6lckte", "tokens": ["Nichts", "ist", "an\u00b7ge\u00b7nehm", "und", "s\u00fcs\u00b7ser", ",", "als", "wenn", "das", "ent\u00b7w\u00f6lck\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "TRUNC", "KON", "ADJD", "$,", "KOKOM", "KOUS", "PDS", "VVFIN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.7": {"text": "Licht", "tokens": ["Licht"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Durch die\u00df liebliche Gewebe, mit gef\u00e4rbtem Schimmer,", "tokens": ["Durch", "die\u00df", "lieb\u00b7li\u00b7che", "Ge\u00b7we\u00b7be", ",", "mit", "ge\u00b7f\u00e4rb\u00b7tem", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.9": {"text": "bricht,", "tokens": ["bricht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.10": {"text": "Und wir denn, von unten auf, durch sie, als durch gr\u00fcne Netze,", "tokens": ["Und", "wir", "denn", ",", "von", "un\u00b7ten", "auf", ",", "durch", "sie", ",", "als", "durch", "gr\u00fc\u00b7ne", "Net\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "$,", "APPR", "ADV", "PTKVZ", "$,", "APPR", "PPER", "$,", "KOUS", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Helle Wolcken, schlancke Zweige, auch, wenn wir erhaben", "tokens": ["Hel\u00b7le", "Wol\u00b7cken", ",", "schlan\u00b7cke", "Zwei\u00b7ge", ",", "auch", ",", "wenn", "wir", "er\u00b7ha\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VVFIN", "NN", "$,", "ADV", "$,", "KOUS", "PPER", "ADJD"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.12": {"text": "stehn,", "tokens": ["stehn", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Gleichfalls durch sie unterw\u00e4rts viele sch\u00f6ne Fr\u00fchlings-", "tokens": ["Gleich\u00b7falls", "durch", "sie", "un\u00b7ter\u00b7w\u00e4rts", "vie\u00b7le", "sch\u00f6\u00b7ne", "Fr\u00fch\u00b7lings"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "ADV", "PIAT", "ADJA", "TRUNC"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "Sch\u00e4tze,", "tokens": ["Sch\u00e4t\u00b7ze", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Dunckle Kr\u00e4nter, Gras und Bluhmen durch ihr gelbes gr\u00fc-", "tokens": ["Dunck\u00b7le", "Kr\u00e4n\u00b7ter", ",", "Gras", "und", "Bluh\u00b7men", "durch", "ihr", "gel\u00b7bes", "gr\u00fc"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN", "APPR", "PPOSAT", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.16": {"text": "ne sehn.", "tokens": ["ne", "sehn", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVINF", "$."], "meter": "-+", "measure": "iambic.single"}, "line.17": {"text": "Liebster GOTT! welch ein Gemisch von vergn\u00fcglichen", "tokens": ["Liebs\u00b7ter", "GoTT", "!", "welch", "ein", "Ge\u00b7misch", "von", "ver\u00b7gn\u00fcg\u00b7li\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "$.", "PWAT", "ART", "NN", "APPR", "ADJA"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "Figuren,", "tokens": ["Fi\u00b7gu\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Und von angenehmen Farben, treffen wir im Fr\u00fchling an!", "tokens": ["Und", "von", "an\u00b7ge\u00b7neh\u00b7men", "Far\u00b7ben", ",", "tref\u00b7fen", "wir", "im", "Fr\u00fch\u00b7ling", "an", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.20": {"text": "Keiner lebt, der alle Pracht Deiner sch\u00f6nen Creaturen,", "tokens": ["Kei\u00b7ner", "lebt", ",", "der", "al\u00b7le", "Pracht", "Dei\u00b7ner", "sch\u00f6\u00b7nen", "Crea\u00b7tu\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PRELS", "PIAT", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.21": {"text": "Und derselben Nutz und Ordnung zehlen und erzehlen kann.", "tokens": ["Und", "der\u00b7sel\u00b7ben", "Nutz", "und", "Ord\u00b7nung", "zeh\u00b7len", "und", "er\u00b7zeh\u00b7len", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "KON", "NN", "VVINF", "KON", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.22": {"text": "Grosser Sch\u00f6pffer! m\u00f6gten doch solche Lieb- und Herrlich-", "tokens": ["Gros\u00b7ser", "Sch\u00f6pf\u00b7fer", "!", "m\u00f6g\u00b7ten", "doch", "sol\u00b7che", "Lie\u00b7b", "und", "Herr\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "VMFIN", "ADV", "PIAT", "TRUNC", "KON", "TRUNC"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.23": {"text": "keiten,", "tokens": ["kei\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.24": {"text": "Uns zu Dir, Quell aller Dinge, Ursprung der Vollkommen-", "tokens": ["Uns", "zu", "Dir", ",", "Quell", "al\u00b7ler", "Din\u00b7ge", ",", "Ur\u00b7sprung", "der", "Voll\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "APPR", "PPER", "$,", "NN", "PIAT", "NN", "$,", "NN", "ART", "TRUNC"], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.25": {"text": "heiten,", "tokens": ["hei\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.26": {"text": "Durch die Lust, zur Danckbarkeit, und zu wahrer Andacht", "tokens": ["Durch", "die", "Lust", ",", "zur", "Dan\u00b7ck\u00b7bar\u00b7keit", ",", "und", "zu", "wah\u00b7rer", "An\u00b7dacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPRART", "NN", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.27": {"text": "leiten!", "tokens": ["lei\u00b7ten", "!"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}