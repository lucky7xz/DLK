{"dta.poem.20081": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Um Dich .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Sei milde stets, und halte fern", "tokens": ["Sei", "mil\u00b7de", "stets", ",", "und", "hal\u00b7te", "fern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "ADV", "$,", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Hofart Deine Seele,", "tokens": ["Von", "Hof\u00b7art", "Dei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir wandeln alle vor dem Herrn", "tokens": ["Wir", "wan\u00b7deln", "al\u00b7le", "vor", "dem", "Herrn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Wegs in Schuld und Fehle.", "tokens": ["Des", "Wegs", "in", "Schuld", "und", "Feh\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Woll ", "tokens": ["Woll"], "token_info": ["word"], "pos": ["NE"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Dir in die Seele sch\u00e4rfen:", "tokens": ["Dir", "in", "die", "See\u00b7le", "sch\u00e4r\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201ees m\u00f6ge, wer sich schuldlos wei\u00df,", "tokens": ["\u201e", "es", "m\u00f6\u00b7ge", ",", "wer", "sich", "schuld\u00b7los", "wei\u00df", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "$,", "PWS", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Stein auf Andre werfen.\u201c", "tokens": ["Den", "Stein", "auf", "And\u00b7re", "wer\u00b7fen", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "PIS", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Tugend, die voll Stolz sich giebt,", "tokens": ["Die", "Tu\u00b7gend", ",", "die", "voll", "Stolz", "sich", "giebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist eitles Selbsterheben;", "tokens": ["Ist", "eit\u00b7les", "Selbs\u00b7ter\u00b7he\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer alles ", "tokens": ["Wer", "al\u00b7les"], "token_info": ["word", "word"], "pos": ["PWS", "PIS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Wei\u00df ", "tokens": ["Wei\u00df"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}}}}}