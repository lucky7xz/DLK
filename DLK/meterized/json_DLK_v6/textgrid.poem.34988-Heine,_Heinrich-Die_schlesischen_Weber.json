{"textgrid.poem.34988": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die schlesischen Weber", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im d\u00fcstern Auge keine Tr\u00e4ne,", "tokens": ["Im", "d\u00fcs\u00b7tern", "Au\u00b7ge", "kei\u00b7ne", "Tr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie sitzen am Webstuhl und fletschen die Z\u00e4hne:", "tokens": ["Sie", "sit\u00b7zen", "am", "Web\u00b7stuhl", "und", "flet\u00b7schen", "die", "Z\u00e4h\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "\u00bbdeutschland, wir weben dein Leichentuch,", "tokens": ["\u00bb", "deutschland", ",", "wir", "we\u00b7ben", "dein", "Lei\u00b7chen\u00b7tuch", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Wir weben hinein den dreifachen Fluch \u2013", "tokens": ["Wir", "we\u00b7ben", "hin\u00b7ein", "den", "drei\u00b7fa\u00b7chen", "Fluch", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wir weben, wir weben!", "tokens": ["Wir", "we\u00b7ben", ",", "wir", "we\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein Fluch dem Gotte, zu dem wir gebeten", "tokens": ["Ein", "Fluch", "dem", "Got\u00b7te", ",", "zu", "dem", "wir", "ge\u00b7be\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVPP"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In Wintersk\u00e4lte und Hungersn\u00f6ten", "tokens": ["In", "Win\u00b7ters\u00b7k\u00e4l\u00b7te", "und", "Hun\u00b7gers\u00b7n\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wir haben vergebens gehofft und geharrt,", "tokens": ["Wir", "ha\u00b7ben", "ver\u00b7ge\u00b7bens", "ge\u00b7hofft", "und", "ge\u00b7harrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "KON", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Er hat uns ge\u00e4fft und gefoppt und genarrt \u2013", "tokens": ["Er", "hat", "uns", "ge\u00b7\u00e4fft", "und", "ge\u00b7foppt", "und", "ge\u00b7narrt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "KON", "VVPP", "KON", "VVPP", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Wir weben, wir weben!", "tokens": ["Wir", "we\u00b7ben", ",", "wir", "we\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Ein Fluch dem K\u00f6nig, dem K\u00f6nig der Reichen,", "tokens": ["Ein", "Fluch", "dem", "K\u00f6\u00b7nig", ",", "dem", "K\u00f6\u00b7nig", "der", "Rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den unser Elend nicht konnte erweichen,", "tokens": ["Den", "un\u00b7ser", "E\u00b7lend", "nicht", "konn\u00b7te", "er\u00b7wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der den letzten Groschen von uns erpre\u00dft,", "tokens": ["Der", "den", "letz\u00b7ten", "Gro\u00b7schen", "von", "uns", "er\u00b7pre\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und uns wie Hunde erschie\u00dfen l\u00e4\u00dft \u2013", "tokens": ["Und", "uns", "wie", "Hun\u00b7de", "er\u00b7schie\u00b7\u00dfen", "l\u00e4\u00dft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KOKOM", "NN", "VVINF", "VVFIN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wir weben, wir weben!", "tokens": ["Wir", "we\u00b7ben", ",", "wir", "we\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Ein Fluch dem falschen Vaterlande,", "tokens": ["Ein", "Fluch", "dem", "fal\u00b7schen", "Va\u00b7ter\u00b7lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo nur gedeihen Schmach und Schande,", "tokens": ["Wo", "nur", "ge\u00b7dei\u00b7hen", "Schmach", "und", "Schan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo jede Blume fr\u00fch geknickt,", "tokens": ["Wo", "je\u00b7de", "Blu\u00b7me", "fr\u00fch", "ge\u00b7knickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo F\u00e4ulnis und Moder den Wurm erquickt \u2013", "tokens": ["Wo", "F\u00e4ul\u00b7nis", "und", "Mo\u00b7der", "den", "Wurm", "er\u00b7quickt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Wir weben, wir weben!", "tokens": ["Wir", "we\u00b7ben", ",", "wir", "we\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Das Schiffchen fliegt, der Webstuhl kracht,", "tokens": ["Das", "Schiff\u00b7chen", "fliegt", ",", "der", "Web\u00b7stuhl", "kracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir weben emsig Tag und Nacht \u2013", "tokens": ["Wir", "we\u00b7ben", "em\u00b7sig", "Tag", "und", "Nacht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Altdeutschland, wir weben dein Leichentuch,", "tokens": ["Alt\u00b7deutschland", ",", "wir", "we\u00b7ben", "dein", "Lei\u00b7chen\u00b7tuch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wir weben hinein den dreifachen Fluch,", "tokens": ["Wir", "we\u00b7ben", "hin\u00b7ein", "den", "drei\u00b7fa\u00b7chen", "Fluch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wir weben, wir weben!\u00ab", "tokens": ["Wir", "we\u00b7ben", ",", "wir", "we\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVINF", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}