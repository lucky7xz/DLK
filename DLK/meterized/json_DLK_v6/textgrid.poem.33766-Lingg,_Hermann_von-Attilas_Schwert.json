{"textgrid.poem.33766": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Attilas Schwert", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unterm Eichbaum auf der Heide", "tokens": ["Un\u00b7term", "Eich\u00b7baum", "auf", "der", "Hei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liegt ein Riesenschwert uralt;", "tokens": ["Liegt", "ein", "Rie\u00b7sen\u00b7schwert", "ur\u00b7alt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oft in seiner dunklen Scheide", "tokens": ["Oft", "in", "sei\u00b7ner", "dunk\u00b7len", "Schei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zuckt es durch den Felsenspalt.", "tokens": ["Zuckt", "es", "durch", "den", "Fel\u00b7sen\u00b7spalt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Heimlich warten Gnom und Elfe", "tokens": ["Heim\u00b7lich", "war\u00b7ten", "Gnom", "und", "El\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NE", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wachsam bei dem gro\u00dfen Schatz,", "tokens": ["Wach\u00b7sam", "bei", "dem", "gro\u00b7\u00dfen", "Schatz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber Eber nur und W\u00f6lfe", "tokens": ["A\u00b7ber", "E\u00b7ber", "nur", "und", "W\u00f6l\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wissen den gefeiten Platz.", "tokens": ["Wis\u00b7sen", "den", "ge\u00b7fei\u00b7ten", "Platz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Endlich finden's Hunnenkrieger,", "tokens": ["End\u00b7lich", "fin\u00b7den's", "Hun\u00b7nen\u00b7krie\u00b7ger", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Attila empf\u00e4ngt den Hort,", "tokens": ["At\u00b7ti\u00b7la", "emp\u00b7f\u00e4ngt", "den", "Hort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er ruft: \u00bbAls Weltbesieger", "tokens": ["Und", "er", "ruft", ":", "\u00bb", "Als", "Welt\u00b7be\u00b7sie\u00b7ger"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "$(", "KOUS", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Gr\u00fc\u00dft mich hier ein G\u00f6tterwort.\u00ab", "tokens": ["Gr\u00fc\u00dft", "mich", "hier", "ein", "G\u00f6t\u00b7ter\u00b7wort", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Spricht's und schwingt das Schwert der Ahnen", "tokens": ["Spricht's", "und", "schwingt", "das", "Schwert", "der", "Ah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie zum Wurf nach West empor;", "tokens": ["Wie", "zum", "Wurf", "nach", "West", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "APPR", "NE", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Allen Hunnen und Alanen", "tokens": ["Al\u00b7len", "Hun\u00b7nen", "und", "A\u00b7la\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schien es wie ein Meteor.", "tokens": ["Schien", "es", "wie", "ein", "Me\u00b7te\u00b7or", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Hoher Widerschein am Himmel", "tokens": ["Ho\u00b7her", "Wi\u00b7der\u00b7schein", "am", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dehnt sich wie Kometenglanz;", "tokens": ["Dehnt", "sich", "wie", "Ko\u00b7me\u00b7ten\u00b7glanz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "KOKOM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch die Luft ein Schlachtget\u00fcmmel", "tokens": ["Durch", "die", "Luft", "ein", "Schlacht\u00b7ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6rt der Kaiser in Byzanz.", "tokens": ["H\u00f6rt", "der", "Kai\u00b7ser", "in", "By\u00b7zanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NE", "$."], "meter": "+-+-++-", "measure": "unknown.measure.tetra"}}, "stanza.6": {"line.1": {"text": "H\u00f6rt's und ruft den Astrologen,", "tokens": ["H\u00f6rt's", "und", "ruft", "den", "Ast\u00b7ro\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der ihm nun, wie alles schweigt,", "tokens": ["Der", "ihm", "nun", ",", "wie", "al\u00b7les", "schweigt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf des Bospors dunklen Wogen", "tokens": ["Auf", "des", "Bos\u00b7pors", "dunk\u00b7len", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwanke blasse Sterne zeigt:", "tokens": ["Schwan\u00b7ke", "blas\u00b7se", "Ster\u00b7ne", "zeigt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbkaiser, Gott und G\u00f6tter schlafen,", "tokens": ["\u00bb", "kai\u00b7ser", ",", "Gott", "und", "G\u00f6t\u00b7ter", "schla\u00b7fen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine gro\u00dfen Feinde nahn;", "tokens": ["Dei\u00b7ne", "gro\u00b7\u00dfen", "Fein\u00b7de", "nahn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mische Gift und opfre Sklaven,", "tokens": ["Mi\u00b7sche", "Gift", "und", "opf\u00b7re", "Skla\u00b7ven", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Taten hast du nie getan!\u00ab", "tokens": ["Ta\u00b7ten", "hast", "du", "nie", "ge\u00b7tan", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}