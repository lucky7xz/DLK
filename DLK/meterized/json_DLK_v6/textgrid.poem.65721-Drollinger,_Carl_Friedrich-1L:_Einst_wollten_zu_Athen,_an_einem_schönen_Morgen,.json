{"textgrid.poem.65721": {"metadata": {"author": {"name": "Drollinger, Carl Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einst wollten zu Athen, an einem sch\u00f6nen Morgen,", "genre": "verse", "period": "N.A.", "pub_year": 1715, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst wollten zu Athen, an einem sch\u00f6nen Morgen,", "tokens": ["Einst", "woll\u00b7ten", "zu", "A\u00b7then", ",", "an", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "NE", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die B\u00fcrger ihre Stadt mit einem Gott versorgen.", "tokens": ["Die", "B\u00fcr\u00b7ger", "ih\u00b7re", "Stadt", "mit", "ei\u00b7nem", "Gott", "ver\u00b7sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Stimmen wurden bald bed\u00e4chtlich abgez\u00e4hlt,", "tokens": ["Die", "Stim\u00b7men", "wur\u00b7den", "bald", "be\u00b7d\u00e4cht\u00b7lich", "ab\u00b7ge\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und mit gemeinem Schlu\u00df Minervens Schutz erw\u00e4hlt.", "tokens": ["Und", "mit", "ge\u00b7mei\u00b7nem", "Schlu\u00df", "Mi\u00b7ner\u00b7vens", "Schutz", "er\u00b7w\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "NN", "NN", "VVPP", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Der trotzige Neptun, durch diesen Schimpf erbittert,", "tokens": ["Der", "trot\u00b7zi\u00b7ge", "Nep\u00b7tun", ",", "durch", "die\u00b7sen", "Schimpf", "er\u00b7bit\u00b7tert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Hub seinen Dreyzack auf, der See und Flut ersch\u00fcttert,", "tokens": ["Hub", "sei\u00b7nen", "Drey\u00b7zack", "auf", ",", "der", "See", "und", "Flut", "er\u00b7sch\u00fct\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "ART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und sprach: O blindes Volk, das allen Witz verlor!", "tokens": ["Und", "sprach", ":", "O", "blin\u00b7des", "Volk", ",", "das", "al\u00b7len", "Witz", "ver\u00b7lor", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NE", "ADJA", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So ziehst du denn ein Weib Neptunus Gottheit vor?", "tokens": ["So", "ziehst", "du", "denn", "ein", "Weib", "Nep\u00b7tu\u00b7nus", "Got\u00b7theit", "vor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "NE", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wer k\u00f6nnte, fuhr er fort, mit einem herben Lachen,", "tokens": ["Wer", "k\u00f6nn\u00b7te", ",", "fuhr", "er", "fort", ",", "mit", "ei\u00b7nem", "her\u00b7ben", "La\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dich mehr an Handlung reich, den Feinden furchtbar machen,", "tokens": ["Dich", "mehr", "an", "Hand\u00b7lung", "reich", ",", "den", "Fein\u00b7den", "furcht\u00b7bar", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "ADJD", "$,", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.11": {"text": "Als ich, der Wellen Herr? Wolan! es ist erkannt:", "tokens": ["Als", "ich", ",", "der", "Wel\u00b7len", "Herr", "?", "Wo\u00b7lan", "!", "es", "ist", "er\u00b7kannt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "NN", "NN", "$.", "ADV", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Es sey Athen forthin der Narren Vaterland!", "tokens": ["Es", "sey", "A\u00b7then", "for\u00b7thin", "der", "Nar\u00b7ren", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "ADV", "ART", "NN", "NN", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.13": {"text": "Er sprach. Der Hauffe stund verwirrt, als wie im Schlafe;", "tokens": ["Er", "sprach", ".", "Der", "Hauf\u00b7fe", "stund", "ver\u00b7wirrt", ",", "als", "wie", "im", "Schla\u00b7fe", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VVFIN", "ADJD", "$,", "KOUS", "KOKOM", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Aus Tummheit f\u00fchlte kaum ein Jeder seine Strafe.", "tokens": ["Aus", "Tumm\u00b7heit", "f\u00fchl\u00b7te", "kaum", "ein", "Je\u00b7der", "sei\u00b7ne", "Stra\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADV", "ART", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Doch bracht ein Rest von Witz noch Einem endlich bey,", "tokens": ["Doch", "bracht", "ein", "Rest", "von", "Witz", "noch", "Ei\u00b7nem", "end\u00b7lich", "bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "ADV", "PIS", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Was f\u00fcr ein kl\u00e4glichs Ding ein Volk von Narren sey.", "tokens": ["Was", "f\u00fcr", "ein", "kl\u00e4g\u00b7lichs", "Ding", "ein", "Volk", "von", "Nar\u00b7ren", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "ART", "NN", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Drum naht er sich geb\u00fcckt zu der Minerven Trohne:", "tokens": ["Drum", "naht", "er", "sich", "ge\u00b7b\u00fcckt", "zu", "der", "Mi\u00b7ner\u00b7ven", "Troh\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "O G\u00f6ttin, ste\u00fcre doch dem unverdienten Hohne!", "tokens": ["O", "G\u00f6t\u00b7tin", ",", "ste\u00fc\u00b7re", "doch", "dem", "un\u00b7ver\u00b7dien\u00b7ten", "Hoh\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die Liebe, die dein Volk zu deiner Weysheit trug,", "tokens": ["Die", "Lie\u00b7be", ",", "die", "dein", "Volk", "zu", "dei\u00b7ner", "Weys\u00b7heit", "trug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Hat uns darum gebracht. Ach mach uns wieder klug!", "tokens": ["Hat", "uns", "da\u00b7rum", "ge\u00b7bracht", ".", "Ach", "mach", "uns", "wie\u00b7der", "klug", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PAV", "VVPP", "$.", "ITJ", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Nein, Kinder! sprach sie, Nein! Das hab ich nicht in H\u00e4nden;", "tokens": ["Nein", ",", "Kin\u00b7der", "!", "sprach", "sie", ",", "Nein", "!", "Das", "hab", "ich", "nicht", "in", "H\u00e4n\u00b7den", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$.", "VVFIN", "PPER", "$,", "PTKANT", "$.", "PDS", "VAFIN", "PPER", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Denn, was ein Gott gef\u00fcgt, kan keine G\u00f6ttin wenden.", "tokens": ["Denn", ",", "was", "ein", "Gott", "ge\u00b7f\u00fcgt", ",", "kan", "kei\u00b7ne", "G\u00f6t\u00b7tin", "wen\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "ART", "NN", "VVPP", "$,", "VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Doch, wenn Neptunus e\u00fcch Verst\u00e4nd und Witz verkehrt,", "tokens": ["Doch", ",", "wenn", "Nep\u00b7tu\u00b7nus", "e\u00b7\u00fcch", "Ver\u00b7st\u00e4nd", "und", "Witz", "ver\u00b7kehrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "NE", "NE", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "So mach ich, ihm zu Trotz, e\u00fcch allesamt gelehrt.", "tokens": ["So", "mach", "ich", ",", "ihm", "zu", "Trotz", ",", "e\u00b7\u00fcch", "al\u00b7le\u00b7samt", "ge\u00b7lehrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "APPR", "NN", "$,", "ADV", "ADJD", "VVPP", "$."], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Vernunft und Wissenschaft, wir lernens von Athene,", "tokens": ["Ver\u00b7nunft", "und", "Wis\u00b7sen\u00b7schaft", ",", "wir", "ler\u00b7nens", "von", "A\u00b7the\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PPER", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sind \u00f6fters nicht gepaart; beysammen stehn sie sch\u00f6ne.", "tokens": ["Sind", "\u00f6f\u00b7ters", "nicht", "ge\u00b7paart", ";", "bey\u00b7sam\u00b7men", "stehn", "sie", "sch\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "VVPP", "$.", "VVIZU", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}