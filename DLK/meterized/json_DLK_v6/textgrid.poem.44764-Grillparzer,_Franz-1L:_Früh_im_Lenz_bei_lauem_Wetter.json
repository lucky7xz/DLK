{"textgrid.poem.44764": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fr\u00fch im Lenz bei lauem Wetter", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fch im Lenz bei lauem Wetter", "tokens": ["Fr\u00fch", "im", "Lenz", "bei", "lau\u00b7em", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Treibt der Baum vereint die Bl\u00e4tter,", "tokens": ["Treibt", "der", "Baum", "ver\u00b7eint", "die", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie winden dichtbelaubt", "tokens": ["Und", "sie", "win\u00b7den", "dicht\u00b7be\u00b7laubt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVINF", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Frische Kronen um sein Haupt.", "tokens": ["Fri\u00b7sche", "Kro\u00b7nen", "um", "sein", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber nach des Sp\u00e4tjahrs Fr\u00f6sten", "tokens": ["A\u00b7ber", "nach", "des", "Sp\u00e4t\u00b7jahrs", "Fr\u00f6s\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wehn sie einzeln an den \u00c4sten,", "tokens": ["Wehn", "sie", "ein\u00b7zeln", "an", "den", "\u00c4s\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sinken endlich lebenssatt.", "tokens": ["Sin\u00b7ken", "end\u00b7lich", "le\u00b7bens\u00b7satt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Unbekannte! hier ein Blatt.", "tokens": ["Un\u00b7be\u00b7kann\u00b7te", "!", "hier", "ein", "Blatt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}