{"textgrid.poem.36100": {"metadata": {"author": {"name": "Gloger, Georg", "birth": "N.A.", "death": "N.A."}, "title": "13. Ode", "genre": "verse", "period": "N.A.", "pub_year": 1617, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wilstu dich gleich vor mir kehren,", "tokens": ["Wils\u00b7tu", "dich", "gleich", "vor", "mir", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Und was Liebes suchen dir,", "tokens": ["Und", "was", "Lie\u00b7bes", "su\u00b7chen", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJA", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch so kanst du kaum entbehren,", "tokens": ["Doch", "so", "kanst", "du", "kaum", "ent\u00b7beh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was du funden hast bei mir.", "tokens": ["Was", "du", "fun\u00b7den", "hast", "bei", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "VAFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was du hast bei mir gefunden,", "tokens": ["Was", "du", "hast", "bei", "mir", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Findest du nicht alle Stunden.", "tokens": ["Fin\u00b7dest", "du", "nicht", "al\u00b7le", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gute Wort' ein iedes gibet,", "tokens": ["Gu\u00b7te", "Wort'", "ein", "ie\u00b7des", "gi\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trewe Herzen seltsam sind.", "tokens": ["Tre\u00b7we", "Her\u00b7zen", "selt\u00b7sam", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wol dem, wer was ihn liebt, liebet,", "tokens": ["Wol", "dem", ",", "wer", "was", "ihn", "liebt", ",", "lie\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "$,", "PWS", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ein gleich Gem\u00fcte findt.", "tokens": ["Und", "ein", "gleich", "Ge\u00b7m\u00fc\u00b7te", "findt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ich auch wil f\u00fcr alle Gaben", "tokens": ["Ich", "auch", "wil", "f\u00fcr", "al\u00b7le", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ein getrewes Herze haben.", "tokens": ["Ein", "ge\u00b7tre\u00b7wes", "Her\u00b7ze", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Zwar ich habe mich beflie\u00dfen", "tokens": ["Zwar", "ich", "ha\u00b7be", "mich", "be\u00b7flie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und beworben alle Zeit,", "tokens": ["Und", "be\u00b7wor\u00b7ben", "al\u00b7le", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ich m\u00f6chte doch genie\u00dfen", "tokens": ["Wie", "ich", "m\u00f6ch\u00b7te", "doch", "ge\u00b7nie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VMFIN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trewer Lieb' und Seligkeit.", "tokens": ["Tre\u00b7wer", "Lieb'", "und", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber die mir war mein Leben,", "tokens": ["A\u00b7ber", "die", "mir", "war", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hat sich anderwerts begeben.", "tokens": ["Hat", "sich", "an\u00b7der\u00b7werts", "be\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sol ich denn nun Solches lieben,", "tokens": ["Sol", "ich", "denn", "nun", "Sol\u00b7ches", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was doch nicht wil meine sein?", "tokens": ["Was", "doch", "nicht", "wil", "mei\u00b7ne", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKNEG", "VMFIN", "PPOSAT", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mich in fremden Fewern \u00fcben", "tokens": ["Mich", "in", "frem\u00b7den", "Fe\u00b7wern", "\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mir machen newe Pein?", "tokens": ["Und", "mir", "ma\u00b7chen", "ne\u00b7we", "Pein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sol ich meine Trewe binden,", "tokens": ["Sol", "ich", "mei\u00b7ne", "Tre\u00b7we", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo Nichts trewes ist zu finden?", "tokens": ["Wo", "Nichts", "tre\u00b7wes", "ist", "zu", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJA", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "W\u00e4rst du doch, weil du so liebest", "tokens": ["W\u00e4rst", "du", "doch", ",", "weil", "du", "so", "lie\u00b7best"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Frembde Gunst, wie ich ietzt bin,", "tokens": ["Fremb\u00b7de", "Gunst", ",", "wie", "ich", "ietzt", "bin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PWAV", "PPER", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der, dem du dich ergiebest,", "tokens": ["Und", "der", ",", "dem", "du", "dich", "er\u00b7gie\u00b7best", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "PRELS", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4tte deinen falschen Sinn,", "tokens": ["H\u00e4t\u00b7te", "dei\u00b7nen", "fal\u00b7schen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So w\u00fcrdstu nach solchem W\u00e4hlen", "tokens": ["So", "w\u00fcrd\u00b7stu", "nach", "sol\u00b7chem", "W\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dich, wie ich mich mu\u00df, auch qu\u00e4len.", "tokens": ["Dich", ",", "wie", "ich", "mich", "mu\u00df", ",", "auch", "qu\u00e4\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PWAV", "PPER", "PPER", "VMFIN", "$,", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch du wilst und must nun scheiden,", "tokens": ["Doch", "du", "wilst", "und", "must", "nun", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "KON", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine Segel gehn schon auf,", "tokens": ["Dei\u00b7ne", "Se\u00b7gel", "gehn", "schon", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollen keinen Anker leiden,", "tokens": ["Wol\u00b7len", "kei\u00b7nen", "An\u00b7ker", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind bedacht auf vollen Lauf.", "tokens": ["Sind", "be\u00b7dacht", "auf", "vol\u00b7len", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nun du solst bei diesen Winden", "tokens": ["Nun", "du", "solst", "bei", "die\u00b7sen", "Win\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VMFIN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenig trewe Porte finden.", "tokens": ["We\u00b7nig", "tre\u00b7we", "Por\u00b7te", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wirst du denn in Tod und Leben", "tokens": ["Wirst", "du", "denn", "in", "Tod", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dort auf wilder Wellenbahn", "tokens": ["Dort", "auf", "wil\u00b7der", "Wel\u00b7len\u00b7bahn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf dem Untrewbette schweben", "tokens": ["Auf", "dem", "Un\u00b7trew\u00b7bet\u00b7te", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hellenunter, himmelan:", "tokens": ["Hel\u00b7len\u00b7un\u00b7ter", ",", "him\u00b7me\u00b7lan", ":"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn wirstu auch nicht vergessen,", "tokens": ["Denn", "wirs\u00b7tu", "auch", "nicht", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was du hast bei mir besessen.", "tokens": ["Was", "du", "hast", "bei", "mir", "be\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ich wil nunmehr einsam leben,", "tokens": ["Ich", "wil", "nun\u00b7mehr", "ein\u00b7sam", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und mich immer halten mir,", "tokens": ["Und", "mich", "im\u00b7mer", "hal\u00b7ten", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wil mich keinem so ergeben,", "tokens": ["Wil", "mich", "kei\u00b7nem", "so", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ich zwar getan bei dir,", "tokens": ["Wie", "ich", "zwar", "ge\u00b7tan", "bei", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVPP", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df ich meine Lieb und Trewe", "tokens": ["Da\u00df", "ich", "mei\u00b7ne", "Lieb", "und", "Tre\u00b7we"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht aufs ander mal berewe.", "tokens": ["Nicht", "aufs", "an\u00b7der", "mal", "be\u00b7re\u00b7we", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "ADJD", "ADV", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}