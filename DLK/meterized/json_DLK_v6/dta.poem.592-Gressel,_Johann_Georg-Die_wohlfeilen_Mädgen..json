{"dta.poem.592": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die wohlfeilen M\u00e4dgen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Z\u00fcrnet doch ihr M\u00e4dgens nicht", "tokens": ["Z\u00fcr\u00b7net", "doch", "ihr", "M\u00e4d\u00b7gens", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Uber mein Beginnen/", "tokens": ["U\u00b7ber", "mein", "Be\u00b7gin\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und was ich auf euch gedicht/", "tokens": ["Und", "was", "ich", "auf", "euch", "ge\u00b7dicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "APPR", "PPER", "VVPP", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Zieht euch nicht zu Sinnen/", "tokens": ["Zieht", "euch", "nicht", "zu", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Schlaget es nur in dem Wind/", "tokens": ["Schla\u00b7get", "es", "nur", "in", "dem", "Wind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Denn die Wahrheit selten findt", "tokens": ["Denn", "die", "Wahr\u00b7heit", "sel\u00b7ten", "findt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ein gew\u00fcnscht und wohlgesinnt Geh\u00f6re.", "tokens": ["Ein", "ge\u00b7w\u00fcnscht", "und", "wohl\u00b7ge\u00b7sinnt", "Ge\u00b7h\u00f6\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJD", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Leget euren Stoltz hinweg/", "tokens": ["Le\u00b7get", "eu\u00b7ren", "Stoltz", "hin\u00b7weg", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APZR", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Angenehme Dinger/", "tokens": ["An\u00b7ge\u00b7neh\u00b7me", "Din\u00b7ger", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Eure Hoheit liegt im Dreck/", "tokens": ["Eu\u00b7re", "Ho\u00b7heit", "liegt", "im", "Dreck", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wird stets geringer/", "tokens": ["Und", "wird", "stets", "ge\u00b7rin\u00b7ger", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Allenthalben seyd ihr feil/", "tokens": ["Al\u00b7len\u00b7thal\u00b7ben", "seyd", "ihr", "feil", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "So da\u00df um ein strohern Seil", "tokens": ["So", "da\u00df", "um", "ein", "stro\u00b7hern", "Seil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Man kan vier und zwantzig M\u00e4dgen kauffen.", "tokens": ["Man", "kan", "vier", "und", "zwant\u00b7zig", "M\u00e4d\u00b7gen", "kauf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "CARD", "KON", "CARD", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "So f\u00e4llt eure Hoheit hin/", "tokens": ["So", "f\u00e4llt", "eu\u00b7re", "Ho\u00b7heit", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und wil gantz verschwinden", "tokens": ["Und", "wil", "gantz", "ver\u00b7schwin\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Weil kein Vortheil kein Gewinn/", "tokens": ["Weil", "kein", "Vor\u00b7theil", "kein", "Ge\u00b7winn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PIAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "An euch ist zu finden/", "tokens": ["An", "euch", "ist", "zu", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Denn ihr gar zu h\u00e4uffig laufft/", "tokens": ["Denn", "ihr", "gar", "zu", "h\u00e4uf\u00b7fig", "laufft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "PTKA", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und man hundert Dirnens kaufft.", "tokens": ["Und", "man", "hun\u00b7dert", "Dir\u00b7nens", "kaufft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "CARD", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auf den Tr\u00f6del-Marckt um einen D\u00fctgen.", "tokens": ["Auf", "den", "Tr\u00f6\u00b7del\u00b7Ma\u00b7rckt", "um", "ei\u00b7nen", "D\u00fct\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Welche kommen selber her/", "tokens": ["Wel\u00b7che", "kom\u00b7men", "sel\u00b7ber", "her", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Um sich zu verhandeln/", "tokens": ["Um", "sich", "zu", "ver\u00b7han\u00b7deln", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "PTKZU", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Doch die m\u00fcssen ohnbeschwer", "tokens": ["Doch", "die", "m\u00fcs\u00b7sen", "ohn\u00b7be\u00b7schwer"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDS", "VMFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gleich zur\u00fccke wandeln/", "tokens": ["Gleich", "zu\u00b7r\u00fc\u00b7cke", "wan\u00b7deln", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Denn wo man gnug haben kan/", "tokens": ["Denn", "wo", "man", "gnug", "ha\u00b7ben", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "ADV", "VAINF", "VMFIN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Nimmt man das nicht gerne an/", "tokens": ["Nimmt", "man", "das", "nicht", "ger\u00b7ne", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PDS", "PTKNEG", "ADV", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Was kein K\u00e4uffer einzukauffen suchet.", "tokens": ["Was", "kein", "K\u00e4uf\u00b7fer", "ein\u00b7zu\u00b7kauf\u00b7fen", "su\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVIZU", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Werfft ihr schon dagegen ein/", "tokens": ["Werfft", "ihr", "schon", "da\u00b7ge\u00b7gen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PAV", "ART", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und fangt an zu bellen", "tokens": ["Und", "fangt", "an", "zu", "bel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "PTKZU", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "M\u00e4nner/ wolt ihr besser seyn/", "tokens": ["M\u00e4n\u00b7ner", "/", "wolt", "ihr", "bes\u00b7ser", "seyn", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VMFIN", "PPER", "ADJD", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als wir ", "tokens": ["Als", "wir"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Ja/ stellet ihr euch gleich erbost/", "tokens": ["Ja", "/", "stel\u00b7let", "ihr", "euch", "gleich", "er\u00b7bost", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "VVFIN", "PPER", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bleibt doch dieses unser Trost/", "tokens": ["Bleibt", "doch", "die\u00b7ses", "un\u00b7ser", "Trost", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PDAT", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df wir vor uns einsam leben k\u00f6nnen.", "tokens": ["Da\u00df", "wir", "vor", "uns", "ein\u00b7sam", "le\u00b7ben", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADJD", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}