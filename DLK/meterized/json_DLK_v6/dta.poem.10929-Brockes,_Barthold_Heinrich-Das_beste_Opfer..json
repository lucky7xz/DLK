{"dta.poem.10929": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das beste Opfer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Sch\u00f6pfer, in empfundnem Gl\u00fccke,", "tokens": ["Mein", "Sch\u00f6p\u00b7fer", ",", "in", "emp\u00b7fund\u00b7nem", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In Sicherheit und sanfter Ruh,", "tokens": ["In", "Si\u00b7cher\u00b7heit", "und", "sanf\u00b7ter", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bring ich hier diese Augenblicke,", "tokens": ["Bring", "ich", "hier", "die\u00b7se", "Au\u00b7gen\u00b7bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die du mir schenkest, fr\u00f6lich zu.", "tokens": ["Die", "du", "mir", "schen\u00b7kest", ",", "fr\u00f6\u00b7lich", "zu", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "$,", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich seh sie an als dein Geschenke,", "tokens": ["Ich", "seh", "sie", "an", "als", "dein", "Ge\u00b7schen\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und die\u00df vergr\u00f6\u00dfert meine Lust,", "tokens": ["Und", "die\u00df", "ver\u00b7gr\u00f6\u00b7\u00dfert", "mei\u00b7ne", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und das Vergn\u00fcgen meiner Brust,", "tokens": ["Und", "das", "Ver\u00b7gn\u00fc\u00b7gen", "mei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df ich dabey an dich gedenke.", "tokens": ["Da\u00df", "ich", "da\u00b7bey", "an", "dich", "ge\u00b7den\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ich will dein Lob vergn\u00fcgt erz\u00e4hlen;", "tokens": ["Ich", "will", "dein", "Lob", "ver\u00b7gn\u00fcgt", "er\u00b7z\u00e4h\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und weil ich sonst nichts geben kann:", "tokens": ["Und", "weil", "ich", "sonst", "nichts", "ge\u00b7ben", "kann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "So nimm, o Herr, zum Opfer an,", "tokens": ["So", "nimm", ",", "o", "Herr", ",", "zum", "Op\u00b7fer", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "$,", "FM", "NN", "$,", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Die Freude der ger\u00fchrten Seelen.", "tokens": ["Die", "Freu\u00b7de", "der", "ge\u00b7r\u00fchr\u00b7ten", "See\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}