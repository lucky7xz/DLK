{"textgrid.poem.24780": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Stunden! wo wir auf das helle Blauen", "tokens": ["Die", "Stun\u00b7den", "!", "wo", "wir", "auf", "das", "hel\u00b7le", "Blau\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "PWAV", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Meeres starren und den Tod verstehn,", "tokens": ["Des", "Mee\u00b7res", "star\u00b7ren", "und", "den", "Tod", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So leicht und feierlich und ohne Grauen,", "tokens": ["So", "leicht", "und", "fei\u00b7er\u00b7lich", "und", "oh\u00b7ne", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie kleine M\u00e4dchen, die sehr bla\u00df aussehn,", "tokens": ["Wie", "klei\u00b7ne", "M\u00e4d\u00b7chen", ",", "die", "sehr", "bla\u00df", "aus\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit gro\u00dfen Augen, und die immer frieren,", "tokens": ["Mit", "gro\u00b7\u00dfen", "Au\u00b7gen", ",", "und", "die", "im\u00b7mer", "frie\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "ART", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An einem Abend stumm vor sich hinsehn", "tokens": ["An", "ei\u00b7nem", "A\u00b7bend", "stumm", "vor", "sich", "hin\u00b7sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "APPR", "PRF", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und wissen, da\u00df das Leben jetzt aus ihren", "tokens": ["Und", "wis\u00b7sen", ",", "da\u00df", "das", "Le\u00b7ben", "jetzt", "aus", "ih\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "$,", "KOUS", "ART", "NN", "ADV", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schlaftrunknen Gliedern still hin\u00fcberflie\u00dft", "tokens": ["Schlaf\u00b7trunk\u00b7nen", "Glie\u00b7dern", "still", "hin\u00b7\u00fc\u00b7berf\u00b7lie\u00dft"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJD", "VVFIN"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "In B\u00e4um und Gras, und sich matt l\u00e4chelnd zieren", "tokens": ["In", "B\u00e4um", "und", "Gras", ",", "und", "sich", "matt", "l\u00e4\u00b7chelnd", "zie\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "KON", "PRF", "ADJD", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie eine Heilige, die ihr Blut vergie\u00dft.", "tokens": ["Wie", "ei\u00b7ne", "Hei\u00b7li\u00b7ge", ",", "die", "ihr", "Blut", "ver\u00b7gie\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}}}}