{"textgrid.poem.37419": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Modern", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hinweg mit diesen alten Herrn,", "tokens": ["Hin\u00b7weg", "mit", "die\u00b7sen", "al\u00b7ten", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie sind zu nichts mehr n\u00fctz!", "tokens": ["Sie", "sind", "zu", "nichts", "mehr", "n\u00fctz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "PIS", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So rufen sie und n\u00e4hmen gern", "tokens": ["So", "ru\u00b7fen", "sie", "und", "n\u00e4h\u00b7men", "gern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Erbe in Besitz.", "tokens": ["Das", "Er\u00b7be", "in", "Be\u00b7sitz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie andre Erben, die in Not,", "tokens": ["Wie", "and\u00b7re", "Er\u00b7ben", ",", "die", "in", "Not", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vergeblich warten sie.", "tokens": ["Ver\u00b7geb\u00b7lich", "war\u00b7ten", "sie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der alte reiche Hoffetot,", "tokens": ["Der", "al\u00b7te", "rei\u00b7che", "Hof\u00b7fe\u00b7tot", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der stirbt bekanntlich nie.", "tokens": ["Der", "stirbt", "be\u00b7kannt\u00b7lich", "nie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}