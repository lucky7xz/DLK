{"textgrid.poem.40633": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein reizvoll Eiland lieblichster Umschr\u00e4nkung", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein reizvoll Eiland lieblichster Umschr\u00e4nkung", "tokens": ["Ein", "reiz\u00b7voll", "Ei\u00b7land", "lieb\u00b7lichs\u00b7ter", "Um\u00b7schr\u00e4n\u00b7kung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "D\u00fcnkt das Sonett mir in der Dichtung Meere,", "tokens": ["D\u00fcnkt", "das", "So\u00b7nett", "mir", "in", "der", "Dich\u00b7tung", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein kunstreich Schiff, in dessen enger F\u00e4hre", "tokens": ["Ein", "kuns\u00b7treich", "Schiff", ",", "in", "des\u00b7sen", "en\u00b7ger", "F\u00e4h\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "$,", "APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Weltenreichthum f\u00fchrt ma\u00dfvolle Lenkung.", "tokens": ["Den", "Wel\u00b7ten\u00b7reicht\u00b7hum", "f\u00fchrt", "ma\u00df\u00b7vol\u00b7le", "Len\u00b7kung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Sarg auch ist's, de\u00df tiefere Versenkung", "tokens": ["Ein", "Sarg", "auch", "ist's", ",", "de\u00df", "tie\u00b7fe\u00b7re", "Ver\u00b7sen\u00b7kung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VAFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Zur Ganzheit ein geschlo\u00dfnes Sein verkl\u00e4re;", "tokens": ["Zur", "Ganz\u00b7heit", "ein", "ge\u00b7schlo\u00df\u00b7nes", "Sein", "ver\u00b7kl\u00e4\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Bau der Bretter selbst und Brettchen kehre", "tokens": ["Der", "Bau", "der", "Bret\u00b7ter", "selbst", "und", "Brett\u00b7chen", "keh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ADV", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In das Sonett als sinn'ge Reimverschr\u00e4nkung.", "tokens": ["In", "das", "So\u00b7nett", "als", "sinn'\u00b7ge", "Reim\u00b7ver\u00b7schr\u00e4n\u00b7kung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Im Ma\u00df die Macht, Gew\u00e4hren im Entbehren,", "tokens": ["Im", "Ma\u00df", "die", "Macht", ",", "Ge\u00b7w\u00e4h\u00b7ren", "im", "Ent\u00b7beh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das ist sein Zauber, das ist auch der deine,", "tokens": ["Das", "ist", "sein", "Zau\u00b7ber", ",", "das", "ist", "auch", "der", "dei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "PDS", "VAFIN", "ADV", "ART", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du rother Fels, selbst ein Sonett von Steine!", "tokens": ["Du", "ro\u00b7ther", "Fels", ",", "selbst", "ein", "So\u00b7nett", "von", "Stei\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "ADV", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So will dein eigner Spiegel dich verkl\u00e4ren,", "tokens": ["So", "will", "dein", "eig\u00b7ner", "Spie\u00b7gel", "dich", "ver\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein Abbild wird zum Kranze deiner Ehren,", "tokens": ["Dein", "Ab\u00b7bild", "wird", "zum", "Kran\u00b7ze", "dei\u00b7ner", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dir bl\u00fchend aus dem eignen Wiederscheine.", "tokens": ["Dir", "bl\u00fc\u00b7hend", "aus", "dem", "eig\u00b7nen", "Wie\u00b7der\u00b7schei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}