{"dta.poem.18812": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Suleika .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Wie! Mit innigstem Behagen,", "tokens": ["Wie", "!", "Mit", "in\u00b7nigs\u00b7tem", "Be\u00b7ha\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Lied, empfind\u2019 ich deinen Sinn!", "tokens": ["Lied", ",", "emp\u00b7find'", "ich", "dei\u00b7nen", "Sinn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liebevoll, du scheinst zu sagen:", "tokens": ["Lie\u00b7be\u00b7voll", ",", "du", "scheinst", "zu", "sa\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dass ich ihm zur Seite bin.", "tokens": ["Dass", "ich", "ihm", "zur", "Sei\u00b7te", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPRART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dass Er ewig mein gedenket,", "tokens": ["Dass", "Er", "e\u00b7wig", "mein", "ge\u00b7den\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPOSAT", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seiner Liebe Seligkeit", "tokens": ["Sei\u00b7ner", "Lie\u00b7be", "Se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Immerdar der Fernen schenket,", "tokens": ["Im\u00b7mer\u00b7dar", "der", "Fer\u00b7nen", "schen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die ein Leben ihm geweiht.", "tokens": ["Die", "ein", "Le\u00b7ben", "ihm", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja! mein Herz, es ist der Spiegel,", "tokens": ["Ja", "!", "mein", "Herz", ",", "es", "ist", "der", "Spie\u00b7gel", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freund! worinn du dich erblickt,", "tokens": ["Freund", "!", "wo\u00b7rinn", "du", "dich", "er\u00b7blickt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese Brust, wo deine Siegel", "tokens": ["Die\u00b7se", "Brust", ",", "wo", "dei\u00b7ne", "Sie\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "PWAV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kuss auf Kuss hereingedr\u00fcckt.", "tokens": ["Kuss", "auf", "Kuss", "her\u00b7ein\u00b7ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "S\u00fcsses Dichten, lautre Wahrheit", "tokens": ["S\u00fcs\u00b7ses", "Dich\u00b7ten", ",", "laut\u00b7re", "Wahr\u00b7heit"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fesselt mich in Sympathie!", "tokens": ["Fes\u00b7selt", "mich", "in", "Sym\u00b7pa\u00b7thie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rein verk\u00f6rpert Liebesklarheit,", "tokens": ["Rein", "ver\u00b7k\u00f6r\u00b7pert", "Lie\u00b7bes\u00b7klar\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVPP", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Im Gewand der Poesie.", "tokens": ["Im", "Ge\u00b7wand", "der", "Poe\u00b7sie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}