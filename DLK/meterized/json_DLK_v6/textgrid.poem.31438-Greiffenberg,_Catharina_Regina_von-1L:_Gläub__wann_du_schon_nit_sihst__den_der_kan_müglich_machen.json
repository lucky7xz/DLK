{"textgrid.poem.31438": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gl\u00e4ub / wann du schon nit sihst / den der kan m\u00fcglich machen", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00e4ub / wann du schon nit sihst / den der kan m\u00fcglich machen", "tokens": ["Gl\u00e4ub", "/", "wann", "du", "schon", "nit", "sihst", "/", "den", "der", "kan", "m\u00fcg\u00b7lich", "ma\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "PWAV", "PPER", "ADV", "PTKNEG", "VVFIN", "$(", "ART", "ART", "VMFIN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die selbst' unm\u00fcglichkeit / bey welchem Sonnen-Liecht", "tokens": ["die", "selbst'", "un\u00b7m\u00fcg\u00b7lich\u00b7keit", "/", "bey", "wel\u00b7chem", "Son\u00b7nen\u00b7Liecht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "APPR", "PWAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "dein dunkles schicksel ist. Die ausgangs-schnur Er flicht", "tokens": ["dein", "dunk\u00b7les", "schick\u00b7sel", "ist", ".", "Die", "aus\u00b7gangs\u00b7schnur", "Er", "flicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "$.", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "in dem zerr\u00fctten Strenn so seltner sinnen-sachen.", "tokens": ["in", "dem", "zer\u00b7r\u00fct\u00b7ten", "Strenn", "so", "selt\u00b7ner", "sin\u00b7nen\u00b7sa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er giebet nach / und dreht den Faden bey den schwachen /", "tokens": ["Er", "gie\u00b7bet", "nach", "/", "und", "dreht", "den", "Fa\u00b7den", "bey", "den", "schwa\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "$(", "KON", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "wie sehr verhenkt und klenkt er ist / ihn doch nicht bricht /", "tokens": ["wie", "sehr", "ver\u00b7henkt", "und", "klenkt", "er", "ist", "/", "ihn", "doch", "nicht", "bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVPP", "KON", "VVFIN", "PPER", "VAFIN", "$(", "PPER", "ADV", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "am Gl\u00fcckes-Haspel / mit der zeit / ihn recht ausricht.", "tokens": ["am", "Gl\u00fc\u00b7ckes\u00b7Has\u00b7pel", "/", "mit", "der", "zeit", "/", "ihn", "recht", "aus\u00b7richt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$(", "APPR", "ART", "NN", "$(", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sein flei\u00df und wei\u00dfheit pflegt (schl\u00e4fft lust und Gl\u00fcck) zu wachen.", "tokens": ["Sein", "flei\u00df", "und", "wei\u00df\u00b7heit", "pflegt", "(", "schl\u00e4fft", "lust", "und", "Gl\u00fcck", ")", "zu", "wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVFIN", "$(", "VVFIN", "ADJD", "KON", "NN", "$(", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Denk nicht / da\u00df ihm / wie dir / das mittel sey verdeckt.", "tokens": ["Denk", "nicht", "/", "da\u00df", "ihm", "/", "wie", "dir", "/", "das", "mit\u00b7tel", "sey", "ver\u00b7deckt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "$(", "KOUS", "PPER", "$(", "PWAV", "PPER", "$(", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sein Allsicht-Aug durchtringt die undurchdringlichkeiten", "tokens": ["Sein", "All\u00b7sicht\u00b7Aug", "durch\u00b7tringt", "die", "un\u00b7durch\u00b7dring\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "der heimlichkeit geheim / im wunder-Berg versteckt.", "tokens": ["der", "heim\u00b7lich\u00b7keit", "ge\u00b7heim", "/", "im", "wun\u00b7der\u00b7Berg", "ver\u00b7steckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$(", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sein' Allmachts hitz / den Stahl zu lob-Gold kan breiten.", "tokens": ["Sein'", "All\u00b7machts", "hitz", "/", "den", "Stahl", "zu", "lob\u00b7Gold", "kan", "brei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PIAT", "NN", "$(", "ART", "NN", "APPR", "NE", "VMFIN", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Sein' Ehr' hat ihr in ihr ein Ehren-\u00e4rz erweckt /", "tokens": ["Sein'", "Ehr'", "hat", "ihr", "in", "ihr", "ein", "Eh\u00b7ren\u00b7\u00e4rz", "er\u00b7weckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "APPR", "PPOSAT", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "das wird mit seinem Bild sich in die Welt ausbreiten.", "tokens": ["das", "wird", "mit", "sei\u00b7nem", "Bild", "sich", "in", "die", "Welt", "aus\u00b7brei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPOSAT", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}