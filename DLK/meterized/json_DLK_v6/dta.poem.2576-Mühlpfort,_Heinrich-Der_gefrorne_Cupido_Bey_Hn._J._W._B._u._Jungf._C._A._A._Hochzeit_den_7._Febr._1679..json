{"dta.poem.2576": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der gefrorne Cupido/  \n  Bey Hn. J. W. B. u. Jungf. C. A. A.  \n Hochzeit/ den 7. Febr. 1679.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der kleine Liebes-Gott der stets gewohnt zu siegen/", "tokens": ["Der", "klei\u00b7ne", "Lie\u00b7bes\u00b7Gott", "der", "stets", "ge\u00b7wohnt", "zu", "sie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADV", "VVPP", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der jede Stunde was von Raub und Beuthe zehlt/", "tokens": ["Der", "je\u00b7de", "Stun\u00b7de", "was", "von", "Raub", "und", "Beu\u00b7the", "zehlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PWS", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und dessen g\u00fcldner Pfeil kein Hertze hat verfehlt/", "tokens": ["Und", "des\u00b7sen", "g\u00fcld\u00b7ner", "Pfeil", "kein", "Hert\u00b7ze", "hat", "ver\u00b7fehlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADJA", "NN", "PIAT", "VVFIN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Lie\u00df auch den K\u00f6cher nicht im Winter m\u00fcssig liegen:", "tokens": ["Lie\u00df", "auch", "den", "K\u00f6\u00b7cher", "nicht", "im", "Win\u00b7ter", "m\u00fcs\u00b7sig", "lie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKNEG", "APPRART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er schwung sich wie er pflegt gewaffnet in die H\u00f6h/", "tokens": ["Er", "schwung", "sich", "wie", "er", "pflegt", "ge\u00b7waff\u00b7net", "in", "die", "H\u00f6h", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PWAV", "PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und lachte trotzig au\u00df de\u00df Himmels Frost und Schnee.", "tokens": ["Und", "lach\u00b7te", "trot\u00b7zig", "au\u00df", "de\u00df", "Him\u00b7mels", "Frost", "und", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es war das albre Kind kaum durch die Lufft gedrungen/", "tokens": ["Es", "war", "das", "alb\u00b7re", "Kind", "kaum", "durch", "die", "Lufft", "ge\u00b7drun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als voller Flocken hieng das aufgekrauste Haar.", "tokens": ["Als", "vol\u00b7ler", "Flo\u00b7cken", "hieng", "das", "auf\u00b7ge\u00b7kraus\u00b7te", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es fuhr durch seinen Leib der grimme Februar/", "tokens": ["Es", "fuhr", "durch", "sei\u00b7nen", "Leib", "der", "grim\u00b7me", "Feb\u00b7ru\u00b7ar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So da\u00df im Augenblick der Glieder Wachs zersprungen.", "tokens": ["So", "da\u00df", "im", "Au\u00b7gen\u00b7blick", "der", "Glie\u00b7der", "Wachs", "zer\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "APPRART", "NN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Er starrt als wie Metall/ ist durch und durch beeist/", "tokens": ["Er", "starrt", "als", "wie", "Me\u00b7tall", "/", "ist", "durch", "und", "durch", "be\u00b7eist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "KOKOM", "NN", "$(", "VAFIN", "APPR", "KON", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und sincket au\u00df der Lufft gantz ohne Seel und Geist.", "tokens": ["Und", "sin\u00b7cket", "au\u00df", "der", "Lufft", "gantz", "oh\u00b7ne", "Seel", "und", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Er lag da in dem Schnee ein Raub der wilden Thiere.", "tokens": ["Er", "lag", "da", "in", "dem", "Schnee", "ein", "Raub", "der", "wil\u00b7den", "Thie\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Nicht einer nahm sich mehr von G\u00f6ttern seiner an.", "tokens": ["Nicht", "ei\u00b7ner", "nahm", "sich", "mehr", "von", "G\u00f6t\u00b7tern", "sei\u00b7ner", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIS", "VVFIN", "PRF", "ADV", "APPR", "NN", "PPOSAT", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sie sprachen/ der uns hat viel Schalckheit angethan/", "tokens": ["Sie", "spra\u00b7chen", "/", "der", "uns", "hat", "viel", "Schalck\u00b7heit", "an\u00b7ge\u00b7than", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PRELS", "PPER", "VAFIN", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ist w\u00fcrdig da\u00df er auch sein Leben so verliere.", "tokens": ["Ist", "w\u00fcr\u00b7dig", "da\u00df", "er", "auch", "sein", "Le\u00b7ben", "so", "ver\u00b7lie\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Es t\u00f6dt\u2019 ihn K\u00e4lt und Frost und der erboste Nord/", "tokens": ["Es", "t\u00f6dt'", "ihn", "K\u00e4lt", "und", "Frost", "und", "der", "er\u00b7bos\u00b7te", "Nord", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "KON", "NN", "KON", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Vollziehe wie er sol den wolverdienten Mord.", "tokens": ["Voll\u00b7zie\u00b7he", "wie", "er", "sol", "den", "wol\u00b7ver\u00b7dien\u00b7ten", "Mord", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPER", "VMFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Inde\u00df kommt ", "tokens": ["In\u00b7de\u00df", "kommt"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.20": {"text": "Der so geraume Zeit in Einsamkeit gelebt/", "tokens": ["Der", "so", "ge\u00b7rau\u00b7me", "Zeit", "in", "Ein\u00b7sam\u00b7keit", "ge\u00b7lebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und mit bedachtem Muth den Flammen widerstrebt/", "tokens": ["Und", "mit", "be\u00b7dach\u00b7tem", "Muth", "den", "Flam\u00b7men", "wi\u00b7der\u00b7strebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da\u00df sein getreues Hertz nicht irgend Glut erlitten.", "tokens": ["Da\u00df", "sein", "ge\u00b7treu\u00b7es", "Hertz", "nicht", "ir\u00b7gend", "Glut", "er\u00b7lit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PTKNEG", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Er siehet recht best\u00fcrtzt den Knaben hingestreckt/", "tokens": ["Er", "sie\u00b7het", "recht", "be\u00b7st\u00fcrtzt", "den", "Kna\u00b7ben", "hin\u00b7ge\u00b7streckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "VVFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und wie sonst nichts als Schnee die zarte Bl\u00f6sse deckt.", "tokens": ["Und", "wie", "sonst", "nichts", "als", "Schnee", "die", "zar\u00b7te", "Bl\u00f6s\u00b7se", "deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "PIS", "KOKOM", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Dann naht er sich herzu und zwar nicht ohn\u2019 erbarmen/", "tokens": ["Dann", "naht", "er", "sich", "her\u00b7zu", "und", "zwar", "nicht", "ohn'", "er\u00b7bar\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "KON", "ADV", "PTKNEG", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Es schien die G\u00f6ttligkeit auch unterm Frost herf\u00fcr.", "tokens": ["Es", "schien", "die", "G\u00f6tt\u00b7lig\u00b7keit", "auch", "un\u00b7term", "Frost", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Er lobte die Gestalt und Engel-gleiche Zier/", "tokens": ["Er", "lob\u00b7te", "die", "Ge\u00b7stalt", "und", "En\u00b7gel\u00b7glei\u00b7che", "Zier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Schlo\u00df endlich bey sich selbst. Verla\u00df ich nun den Armen/", "tokens": ["Schlo\u00df", "end\u00b7lich", "bey", "sich", "selbst", ".", "Ver\u00b7la\u00df", "ich", "nun", "den", "Ar\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PRF", "ADV", "$.", "NN", "PPER", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "So f\u00fcrcht ich Venus Zorn. Der ", "tokens": ["So", "f\u00fcrcht", "ich", "Ve\u00b7nus", "Zorn", ".", "Der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "NN", "$.", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Zeugt/ da\u00df er mu\u00df ein Sohn der grossen ", "tokens": ["Zeugt", "/", "da\u00df", "er", "mu\u00df", "ein", "Sohn", "der", "gros\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$(", "KOUS", "PPER", "VMFIN", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Er nimmt ihn zu sich ein/ kein Leben war zufinden.", "tokens": ["Er", "nimmt", "ihn", "zu", "sich", "ein", "/", "kein", "Le\u00b7ben", "war", "zu\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PRF", "ART", "$(", "PIAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Der sch\u00f6ne Purpur-Mund sah als wie Kreide bleich/", "tokens": ["Der", "sch\u00f6\u00b7ne", "Pur\u00b7pur\u00b7Mund", "sah", "als", "wie", "Krei\u00b7de", "bleich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KOUS", "KOKOM", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Das Haar hieng voller Ei\u00df so vor wie Wolle weich/", "tokens": ["Das", "Haar", "hieng", "vol\u00b7ler", "Ei\u00df", "so", "vor", "wie", "Wol\u00b7le", "weich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "ADV", "PTKVZ", "KOKOM", "NE", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Es wei\u00df nicht Celadon die Ursach zuergr\u00fcnden/", "tokens": ["Es", "wei\u00df", "nicht", "Ce\u00b7la\u00b7don", "die", "Ur\u00b7sach", "zu\u00b7er\u00b7gr\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Begehrt nur wohl zuthun und reiset ferner hin/", "tokens": ["Be\u00b7gehrt", "nur", "wohl", "zu\u00b7thun", "und", "rei\u00b7set", "fer\u00b7ner", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "VVINF", "KON", "VVFIN", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Bi\u00df da\u00df er kan zu Brig nach seinem Wunsch einziehn.", "tokens": ["Bi\u00df", "da\u00df", "er", "kan", "zu", "Brig", "nach", "sei\u00b7nem", "Wunsch", "ein\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "VMFIN", "APPR", "NE", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "So bald nur Celadon den Fu\u00df in Brieg gesetzet/", "tokens": ["So", "bald", "nur", "Ce\u00b7la\u00b7don", "den", "Fu\u00df", "in", "Brieg", "ge\u00b7set\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "NN", "ART", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "So wittert sich bey ihm ein ungewohnter Trieb/", "tokens": ["So", "wit\u00b7tert", "sich", "bey", "ihm", "ein", "un\u00b7ge\u00b7wohn\u00b7ter", "Trieb", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.39": {"text": "Er f\u00fchlt durch Marck und Blut den Zunder neuer Lieb\u2019", "tokens": ["Er", "f\u00fchlt", "durch", "Marck", "und", "Blut", "den", "Zun\u00b7der", "neu\u00b7er", "Lieb'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und sucht ein sch\u00f6nes Bild das Aug und Seel ergetzet.", "tokens": ["Und", "sucht", "ein", "sch\u00f6\u00b7nes", "Bild", "das", "Aug", "und", "Seel", "er\u00b7get\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Der Himmel stimmt mit bey und l\u00e4st es drauf geschehn/", "tokens": ["Der", "Him\u00b7mel", "stimmt", "mit", "bey", "und", "l\u00e4st", "es", "drauf", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPR", "KON", "VVFIN", "PPER", "PAV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Da\u00df es ", "tokens": ["Da\u00df", "es"], "token_info": ["