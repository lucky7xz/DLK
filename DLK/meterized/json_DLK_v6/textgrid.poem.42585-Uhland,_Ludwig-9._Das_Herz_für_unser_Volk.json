{"textgrid.poem.42585": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "9. Das Herz f\u00fcr unser Volk", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An unsrer V\u00e4ter Taten", "tokens": ["An", "uns\u00b7rer", "V\u00e4\u00b7ter", "Ta\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Liebe sich erbaun,", "tokens": ["Mit", "Lie\u00b7be", "sich", "er\u00b7baun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Fortpflanzen ihre Saaten,", "tokens": ["Fort\u00b7pflan\u00b7zen", "ih\u00b7re", "Saa\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem alten Grund vertraun;", "tokens": ["Dem", "al\u00b7ten", "Grund", "ver\u00b7traun", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In solchem Angedenken", "tokens": ["In", "sol\u00b7chem", "An\u00b7ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des Landes Heil erneun;", "tokens": ["Des", "Lan\u00b7des", "Heil", "er\u00b7neun", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Um unsre Schmach sich kr\u00e4nken,", "tokens": ["Um", "uns\u00b7re", "Schmach", "sich", "kr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sich unsrer Ehre freun;", "tokens": ["Sich", "uns\u00b7rer", "Eh\u00b7re", "freun", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Sein eignes Ich vergessen", "tokens": ["Sein", "eig\u00b7nes", "Ich", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "In Aller Lust und Schmerz:", "tokens": ["In", "Al\u00b7ler", "Lust", "und", "Schmerz", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Das nennt man, wohlermessen,", "tokens": ["Das", "nennt", "man", ",", "woh\u00b7ler\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "$,", "PWAV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "F\u00fcr unser Volk ein Herz.", "tokens": ["F\u00fcr", "un\u00b7ser", "Volk", "ein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was unsre V\u00e4ter schufen,", "tokens": ["Was", "uns\u00b7re", "V\u00e4\u00b7ter", "schu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zertr\u00fcmmern ohne Scheu,", "tokens": ["Zer\u00b7tr\u00fcm\u00b7mern", "oh\u00b7ne", "Scheu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Um dann hervorzurufen", "tokens": ["Um", "dann", "her\u00b7vor\u00b7zu\u00b7ru\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das eigne Luftgeb\u00e4u;", "tokens": ["Das", "eig\u00b7ne", "Luft\u00b7ge\u00b7b\u00e4u", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fchllos die M\u00e4nner l\u00e4stern,", "tokens": ["F\u00fchl\u00b7los", "die", "M\u00e4n\u00b7ner", "l\u00e4s\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die wir uns ausgew\u00e4hlt,", "tokens": ["Die", "wir", "uns", "aus\u00b7ge\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Weil sie dem Plan von gestern", "tokens": ["Weil", "sie", "dem", "Plan", "von", "ge\u00b7stern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Zu huldigen verfehlt;", "tokens": ["Zu", "hul\u00b7di\u00b7gen", "ver\u00b7fehlt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Die alten Namen nennen", "tokens": ["Die", "al\u00b7ten", "Na\u00b7men", "nen\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.10": {"text": "Nicht anders als zum Scherz:", "tokens": ["Nicht", "an\u00b7ders", "als", "zum", "Scherz", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KOKOM", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Das hei\u00dft, ich darf's bekennen,", "tokens": ["Das", "hei\u00dft", ",", "ich", "dar\u00b7f's", "be\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PPER", "PAV", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "F\u00fcr unser Volk ", "tokens": ["F\u00fcr", "un\u00b7ser", "Volk"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Jetzt, da von neuem Lichte", "tokens": ["Jetzt", ",", "da", "von", "neu\u00b7em", "Lich\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Hoffnung sich belebt,", "tokens": ["Die", "Hoff\u00b7nung", "sich", "be\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und da die Volksgeschichte", "tokens": ["Und", "da", "die", "Volks\u00b7ge\u00b7schich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Griffel wartend hebt:", "tokens": ["Den", "Grif\u00b7fel", "war\u00b7tend", "hebt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O F\u00fcrst! f\u00fcr dessen Ahnen", "tokens": ["O", "F\u00fcrst", "!", "f\u00fcr", "des\u00b7sen", "Ah\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "APPR", "PRELAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Unsern Brust gepocht", "tokens": ["Der", "Un\u00b7sern", "Brust", "ge\u00b7pocht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und unter dessen Fahnen", "tokens": ["Und", "un\u00b7ter", "des\u00b7sen", "Fah\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PRELAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Jugend Ruhm erfocht,", "tokens": ["Die", "Ju\u00b7gend", "Ruhm", "er\u00b7focht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Jetzt, ", "tokens": ["Jetzt", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Du dich zu unsrem Schmerz!", "tokens": ["Du", "dich", "zu", "uns\u00b7rem", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Ja! du vor allen zeige", "tokens": ["Ja", "!", "du", "vor", "al\u00b7len", "zei\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "F\u00fcr unser Volk ein Herz!", "tokens": ["F\u00fcr", "un\u00b7ser", "Volk", "ein", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}