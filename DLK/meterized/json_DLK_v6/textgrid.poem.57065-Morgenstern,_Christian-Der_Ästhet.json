{"textgrid.poem.57065": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Der \u00c4sthet", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich sitze, will ich nicht", "tokens": ["Wenn", "ich", "sit\u00b7ze", ",", "will", "ich", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VMFIN", "PPER", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "sitzen, wie mein Sitz-Fleisch m\u00f6chte,", "tokens": ["sit\u00b7zen", ",", "wie", "mein", "Sitz\u00b7Fleisch", "m\u00f6ch\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOKOM", "PPOSAT", "NN", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "sondern wie mein Sitz-Geist sich,", "tokens": ["son\u00b7dern", "wie", "mein", "Sitz\u00b7Geist", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "PRF", "$,"], "meter": "+-+-++-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "s\u00e4\u00dfe er, den Stuhl sich fl\u00f6chte.", "tokens": ["s\u00e4\u00b7\u00dfe", "er", ",", "den", "Stuhl", "sich", "fl\u00f6ch\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Der jedoch bedarf nicht viel,", "tokens": ["Der", "je\u00b7doch", "be\u00b7darf", "nicht", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "sch\u00e4tzt am Stuhl allein den Stil,", "tokens": ["sch\u00e4tzt", "am", "Stuhl", "al\u00b7lein", "den", "Stil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00fcberl\u00e4\u00dft den Zweck des M\u00f6bels", "tokens": ["\u00fc\u00b7berl\u00b7\u00e4\u00dft", "den", "Zweck", "des", "M\u00f6\u00b7bels"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "ohne Grimm der Gier des P\u00f6bels.", "tokens": ["oh\u00b7ne", "Grimm", "der", "Gier", "des", "P\u00f6\u00b7bels", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}