{"textgrid.poem.32657": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von dir, o Chloe! fern, betracht' ich weinend", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von dir, o Chloe! fern, betracht' ich weinend", "tokens": ["Von", "dir", ",", "o", "Chloe", "!", "fern", ",", "be\u00b7tracht'", "ich", "wei\u00b7nend"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "FM", "NE", "$.", "ADJD", "$,", "VVFIN", "PPER", "VVPP"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den Mond, der still vor\u00fcber zieht;", "tokens": ["Den", "Mond", ",", "der", "still", "vor\u00b7\u00fc\u00b7ber", "zieht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und d\u00fcster, aus zerri\u00dfnen Wolken scheinend,", "tokens": ["Und", "d\u00fcs\u00b7ter", ",", "aus", "zer\u00b7ri\u00df\u00b7nen", "Wol\u00b7ken", "schei\u00b7nend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf meinen Schmerz herunter sieht.", "tokens": ["Auf", "mei\u00b7nen", "Schmerz", "her\u00b7un\u00b7ter", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APZR", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So war die Nacht, in der ich deinen letzten", "tokens": ["So", "war", "die", "Nacht", ",", "in", "der", "ich", "dei\u00b7nen", "letz\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und feuervollen Ku\u00df empfing,", "tokens": ["Und", "feu\u00b7er\u00b7vol\u00b7len", "Ku\u00df", "emp\u00b7fing", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als an dem Mund, den deine Thr\u00e4nen netzten,", "tokens": ["Als", "an", "dem", "Mund", ",", "den", "dei\u00b7ne", "Thr\u00e4\u00b7nen", "netz\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der meine, stumm und bebend, hing.", "tokens": ["Der", "mei\u00b7ne", ",", "stumm", "und", "be\u00b7bend", ",", "hing", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADJD", "KON", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da sah er noch, im Purpur aufgegangen,", "tokens": ["Da", "sah", "er", "noch", ",", "im", "Pur\u00b7pur", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf meinen Lippen Rosen bl\u00fchn;", "tokens": ["Auf", "mei\u00b7nen", "Lip\u00b7pen", "Ro\u00b7sen", "bl\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun sieht er Totenbl\u00e4sse meine Wangen,", "tokens": ["Nun", "sieht", "er", "To\u00b7ten\u00b7bl\u00e4s\u00b7se", "mei\u00b7ne", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie feinen Trauerflor, umziehn.", "tokens": ["Wie", "fei\u00b7nen", "Trau\u00b7er\u00b7flor", ",", "um\u00b7ziehn", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Seufzst du auch jetzt, mit bangen Klaget\u00f6nen,", "tokens": ["Seufzst", "du", "auch", "jetzt", ",", "mit", "ban\u00b7gen", "Kla\u00b7ge\u00b7t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu ihm, um deinen Freund, hinauf;", "tokens": ["Zu", "ihm", ",", "um", "dei\u00b7nen", "Freund", ",", "hin\u00b7auf", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KOUI", "PPOSAT", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und folgt dein Aug', erf\u00fcllt mit Liebesthr\u00e4nen,", "tokens": ["Und", "folgt", "dein", "Aug'", ",", "er\u00b7f\u00fcllt", "mit", "Lie\u00b7best\u00b7hr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihm in dem einsam stillen Lauf,", "tokens": ["Ihm", "in", "dem", "ein\u00b7sam", "stil\u00b7len", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So tr\u00f6ste dich, und denke, welche Freude", "tokens": ["So", "tr\u00f6s\u00b7te", "dich", ",", "und", "den\u00b7ke", ",", "wel\u00b7che", "Freu\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "VVFIN", "$,", "PWAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dereinst auf unsern Wangen gl\u00fcht,", "tokens": ["De\u00b7reinst", "auf", "un\u00b7sern", "Wan\u00b7gen", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wann er uns, Arm in Arm geschlungen, beide", "tokens": ["Wann", "er", "uns", ",", "Arm", "in", "Arm", "ge\u00b7schlun\u00b7gen", ",", "bei\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "PPER", "PPER", "$,", "NN", "APPR", "NN", "VVPP", "$,", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von neuer Wonne trunken sieht:", "tokens": ["Von", "neu\u00b7er", "Won\u00b7ne", "trun\u00b7ken", "sieht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dann trennt, o Chloe, kein Geschick uns wieder,", "tokens": ["Dann", "trennt", ",", "o", "Chloe", ",", "kein", "Ge\u00b7schick", "uns", "wie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "FM", "NE", "$,", "PIAT", "NN", "PPER", "ADV", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bis sterbend sich dies Auge schlie\u00dft,", "tokens": ["Bis", "ster\u00b7bend", "sich", "dies", "Au\u00b7ge", "schlie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "PRF", "PDS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und er sein d\u00e4mmernd Licht aufs Grabmal nieder", "tokens": ["Und", "er", "sein", "d\u00e4m\u00b7mernd", "Licht", "aufs", "Grab\u00b7mal", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PPOSAT", "VVPP", "NN", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus d\u00fcstern Eibenschatten gie\u00dft.", "tokens": ["Aus", "d\u00fcs\u00b7tern", "Ei\u00b7ben\u00b7schat\u00b7ten", "gie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}