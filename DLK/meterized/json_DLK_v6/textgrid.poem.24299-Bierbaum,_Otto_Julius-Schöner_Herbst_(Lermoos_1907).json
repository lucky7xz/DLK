{"textgrid.poem.24299": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Sch\u00f6ner Herbst (Lermoos 1907)", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Klar, kr\u00e4ftig, edler Wonnen voll ist dieser Herbst:", "tokens": ["Klar", ",", "kr\u00e4f\u00b7tig", ",", "ed\u00b7ler", "Won\u00b7nen", "voll", "ist", "die\u00b7ser", "Herbst", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJA", "NN", "ADJD", "VAFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein stilles Fest f\u00fcr mich der sp\u00e4ten Reifezeit.", "tokens": ["Ein", "stil\u00b7les", "Fest", "f\u00fcr", "mich", "der", "sp\u00e4\u00b7ten", "Rei\u00b7fe\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Leer ist das Kornfeld, und die weite Wiese trifft", "tokens": ["Leer", "ist", "das", "Korn\u00b7feld", ",", "und", "die", "wei\u00b7te", "Wie\u00b7se", "trifft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der zweite Schnitt. Pan tr\u00e4umt nicht mehr im Rosenbusch.", "tokens": ["Der", "zwei\u00b7te", "Schnitt", ".", "Pan", "tr\u00e4umt", "nicht", "mehr", "im", "Ro\u00b7sen\u00b7busch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "VVFIN", "PTKNEG", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Auf keinem schwanken Bl\u00fctenaste schaukelt sich", "tokens": ["Auf", "kei\u00b7nem", "schwan\u00b7ken", "Bl\u00fc\u00b7ten\u00b7as\u00b7te", "schau\u00b7kelt", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Eros, der falterfl\u00fcgelige, leichte: still,", "tokens": ["E\u00b7ros", ",", "der", "fal\u00b7ter\u00b7fl\u00fc\u00b7ge\u00b7li\u00b7ge", ",", "leich\u00b7te", ":", "still", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "$,", "VVFIN", "$.", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch munter l\u00e4chelnd sitzt er auf dem Apfelbaum", "tokens": ["Doch", "mun\u00b7ter", "l\u00e4\u00b7chelnd", "sitzt", "er", "auf", "dem", "Ap\u00b7fel\u00b7baum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und reicht mir liebensw\u00fcrdig Frucht auf Frucht. Ihm ist", "tokens": ["Und", "reicht", "mir", "lie\u00b7bens\u00b7w\u00fcr\u00b7dig", "Frucht", "auf", "Frucht", ".", "Ihm", "ist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "NN", "APPR", "NN", "$.", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sehr wohl in diesem Herbst. \u2013 Wie mir. \u2013 Jetzt ist Halkyone", "tokens": ["Sehr", "wohl", "in", "die\u00b7sem", "Herbst", ".", "\u2013", "Wie", "mir", ".", "\u2013", "Jetzt", "ist", "Hal\u00b7ky\u00b7o\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PDAT", "NN", "$.", "$(", "PWAV", "PPER", "$.", "$(", "ADV", "VAFIN", "NN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Die heitre Erde. H\u00f6her, blauer w\u00f6lbt sich nun", "tokens": ["Die", "heit\u00b7re", "Er\u00b7de", ".", "H\u00f6\u00b7her", ",", "blau\u00b7er", "w\u00f6lbt", "sich", "nun"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "$,", "ADJA", "VVFIN", "PRF", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der klare Himmel. Keine Schw\u00fcle mehr bewegt", "tokens": ["Der", "kla\u00b7re", "Him\u00b7mel", ".", "Kei\u00b7ne", "Schw\u00fc\u00b7le", "mehr", "be\u00b7wegt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PIAT", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die herbstlich fein gewordne Luft mit zitterndem", "tokens": ["Die", "herbst\u00b7lich", "fein", "ge\u00b7word\u00b7ne", "Luft", "mit", "zit\u00b7tern\u00b7dem"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJD", "ADJA", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gewelle sommerlicher Glut, die jedem Ding", "tokens": ["Ge\u00b7wel\u00b7le", "som\u00b7mer\u00b7li\u00b7cher", "Glut", ",", "die", "je\u00b7dem", "Ding"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "$,", "PRELS", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Den scharfen Umri\u00df raubte; klar, fest, rein", "tokens": ["Den", "schar\u00b7fen", "Um\u00b7ri\u00df", "raub\u00b7te", ";", "klar", ",", "fest", ",", "rein"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "ADJD", "$,", "PTKVZ", "$,", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und ruhig konturiert sich nun die reife Welt.", "tokens": ["Und", "ru\u00b7hig", "kon\u00b7tu\u00b7riert", "sich", "nun", "die", "rei\u00b7fe", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "PRF", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch bald, ich wei\u00df es, f\u00fcllt der Herbst mit Farben aus,", "tokens": ["Doch", "bald", ",", "ich", "wei\u00df", "es", ",", "f\u00fcllt", "der", "Herbst", "mit", "Far\u00b7ben", "aus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPER", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit br\u00fcnstig satteren, als sie der Sommer sah,", "tokens": ["Mit", "br\u00fcns\u00b7tig", "sat\u00b7te\u00b7ren", ",", "als", "sie", "der", "Som\u00b7mer", "sah", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVINF", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die klare Zeichnung dieses ruhesamen Gl\u00fccks.", "tokens": ["Die", "kla\u00b7re", "Zeich\u00b7nung", "die\u00b7ses", "ru\u00b7he\u00b7sa\u00b7men", "Gl\u00fccks", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es kommen tragisch Flammen rot und gelb. Und braun", "tokens": ["Es", "kom\u00b7men", "tra\u00b7gisch", "Flam\u00b7men", "rot", "und", "gelb", ".", "Und", "braun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "ADJD", "KON", "ADJD", "$.", "KON", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Kommt heldisch gro\u00dfes Pathos. Tiefste Leidenschaft", "tokens": ["Kommt", "hel\u00b7disch", "gro\u00b7\u00dfes", "Pa\u00b7thos", ".", "Tiefs\u00b7te", "Lei\u00b7den\u00b7schaft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADJD", "ADJA", "NN", "$.", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kommt in das ruhesch\u00f6ne Bild: In Purpur geht", "tokens": ["Kommt", "in", "das", "ru\u00b7he\u00b7sch\u00f6\u00b7ne", "Bild", ":", "In", "Pur\u00b7pur", "geht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$.", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Medea Sonne, geht das Leben in die Nacht.", "tokens": ["Me\u00b7dea", "Son\u00b7ne", ",", "geht", "das", "Le\u00b7ben", "in", "die", "Nacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}