{"textgrid.poem.52676": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein armes Herz, mein guter Muth,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein armes Herz, mein guter Muth,", "tokens": ["Mein", "ar\u00b7mes", "Herz", ",", "mein", "gu\u00b7ter", "Muth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So frisch wie der junge Morgen,", "tokens": ["So", "frisch", "wie", "der", "jun\u00b7ge", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Verrathen sind sie und verkauft", "tokens": ["Ver\u00b7ra\u00b7then", "sind", "sie", "und", "ver\u00b7kauft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "KON", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An die Welt der kleinen Sorgen.", "tokens": ["An", "die", "Welt", "der", "klei\u00b7nen", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Der kleinen Sorgen, die so z\u00e4h", "tokens": ["Der", "klei\u00b7nen", "Sor\u00b7gen", ",", "die", "so", "z\u00e4h"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich mit der Seele zanken,", "tokens": ["Sich", "mit", "der", "See\u00b7le", "zan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gleich einem gro\u00dfen Heuschreckenheer", "tokens": ["Gleich", "ei\u00b7nem", "gro\u00b7\u00dfen", "Heu\u00b7schre\u00b7cken\u00b7heer"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Abweiden alle Gedanken.", "tokens": ["Ab\u00b7wei\u00b7den", "al\u00b7le", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Mir ist, als ob der Sonnenschein", "tokens": ["Mir", "ist", ",", "als", "ob", "der", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In hohlen Sch\u00e4del mir schiene,", "tokens": ["In", "hoh\u00b7len", "Sch\u00e4\u00b7del", "mir", "schie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie, droben auf dem Berge dort,", "tokens": ["Wie", ",", "dro\u00b7ben", "auf", "dem", "Ber\u00b7ge", "dort", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "ADV", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In die ausgebrannte Ruine.", "tokens": ["In", "die", "aus\u00b7ge\u00b7brann\u00b7te", "Ru\u00b7i\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Dann aber kommt es mir wieder vor,", "tokens": ["Dann", "a\u00b7ber", "kommt", "es", "mir", "wie\u00b7der", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Als ob ich Schauer litte,", "tokens": ["Als", "ob", "ich", "Schau\u00b7er", "lit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als regnet es mir in die Seel hinein,", "tokens": ["Als", "reg\u00b7net", "es", "mir", "in", "die", "Seel", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie dort in die alte H\u00fctte.", "tokens": ["Wie", "dort", "in", "die", "al\u00b7te", "H\u00fct\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Meine Freud ist hin, mein Muth dahin,", "tokens": ["Mei\u00b7ne", "Freud", "ist", "hin", ",", "mein", "Muth", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$,", "PPOSAT", "NN", "PAV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und alle Lust verdorben,", "tokens": ["Und", "al\u00b7le", "Lust", "ver\u00b7dor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn ach, mein freier, leichter Sinn", "tokens": ["Denn", "ach", ",", "mein", "frei\u00b7er", ",", "leich\u00b7ter", "Sinn"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "XY", "$,", "PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er ist verdorben, gestorben.", "tokens": ["Er", "ist", "ver\u00b7dor\u00b7ben", ",", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}