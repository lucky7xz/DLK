{"textgrid.poem.44579": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Nachruf", "genre": "verse", "period": "N.A.", "pub_year": 1850, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So bist du hingegangen, armer Mann,", "tokens": ["So", "bist", "du", "hin\u00b7ge\u00b7gan\u00b7gen", ",", "ar\u00b7mer", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und bist im w\u00fcsten Irrenhaus erblichen,", "tokens": ["Und", "bist", "im", "w\u00fcs\u00b7ten", "Ir\u00b7ren\u00b7haus", "er\u00b7bli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geh\u00f6rend so im Ende denn auch an", "tokens": ["Ge\u00b7h\u00f6\u00b7rend", "so", "im", "En\u00b7de", "denn", "auch", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "ADV", "APPRART", "NN", "KON", "ADV", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Zeit, der du in deinem Lauf geglichen.", "tokens": ["Der", "Zeit", ",", "der", "du", "in", "dei\u00b7nem", "Lauf", "ge\u00b7gli\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Bestimmt, ein bl\u00fchend gr\u00fcner Ast zu sein", "tokens": ["Be\u00b7stimmt", ",", "ein", "bl\u00fc\u00b7hend", "gr\u00fc\u00b7ner", "Ast", "zu", "sein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ART", "ADJD", "ADJA", "NN", "PTKZU", "VAINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "An deines Vaterlandes K\u00fcnstlerbaume,", "tokens": ["An", "dei\u00b7nes", "Va\u00b7ter\u00b7lan\u00b7des", "K\u00fcnst\u00b7ler\u00b7bau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fandst dus zu eng in dem beengten Raume,", "tokens": ["Fandst", "dus", "zu", "eng", "in", "dem", "be\u00b7eng\u00b7ten", "Rau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PTKA", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und, selbst als Baum zu gelten, luds dich ein.", "tokens": ["Und", ",", "selbst", "als", "Baum", "zu", "gel\u00b7ten", ",", "luds", "dich", "ein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "KOUS", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Also entr\u00fcckt der vaterl\u00e4ndschen Erde,", "tokens": ["Al\u00b7so", "ent\u00b7r\u00fcckt", "der", "va\u00b7ter\u00b7l\u00e4nd\u00b7schen", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Verpflanztest du, was so versprechend schien,", "tokens": ["Ver\u00b7pflanz\u00b7test", "du", ",", "was", "so", "ver\u00b7spre\u00b7chend", "schien", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "ADV", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hin, wo im Treibhaus am geheizten Herde", "tokens": ["Hin", ",", "wo", "im", "Treib\u00b7haus", "am", "ge\u00b7heiz\u00b7ten", "Her\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "APPRART", "NN", "APPRART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und unter Glas sie bleiche Pflanzen ziehn.", "tokens": ["Und", "un\u00b7ter", "Glas", "sie", "blei\u00b7che", "Pflan\u00b7zen", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Triebe Kraft blieb deiner Heimat eigen,", "tokens": ["Der", "Trie\u00b7be", "Kraft", "blieb", "dei\u00b7ner", "Hei\u00b7mat", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur Laub und Holz, es ward mit dir versetzt.", "tokens": ["Nur", "Laub", "und", "Holz", ",", "es", "ward", "mit", "dir", "ver\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$,", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein wenig gor der Saft noch in den Zweigen,", "tokens": ["Ein", "we\u00b7nig", "gor", "der", "Saft", "noch", "in", "den", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann starb er ab und du mit ihm zuletzt.", "tokens": ["Dann", "starb", "er", "ab", "und", "du", "mit", "ihm", "zu\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "PPER", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Da\u00df du ein Ehrenmann, hat dich get\u00f6tet,", "tokens": ["Da\u00df", "du", "ein", "Eh\u00b7ren\u00b7mann", ",", "hat", "dich", "ge\u00b7t\u00f6\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da\u00df du kein Tor, war deines Wahnsinns Grund,", "tokens": ["Da\u00df", "du", "kein", "Tor", ",", "war", "dei\u00b7nes", "Wahn\u00b7sinns", "Grund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "$,", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wem Selbsterkenntnis noch die Stirne r\u00f6tet,", "tokens": ["Wem", "Selbs\u00b7ter\u00b7kennt\u00b7nis", "noch", "die", "Stir\u00b7ne", "r\u00f6\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der straft sich L\u00fcgen selbst mit eignem Mund.", "tokens": ["Der", "straft", "sich", "L\u00fc\u00b7gen", "selbst", "mit", "eig\u00b7nem", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Vom Lob getragen und vom Ruhm beschienen,", "tokens": ["Vom", "Lob", "ge\u00b7tra\u00b7gen", "und", "vom", "Ruhm", "be\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "KON", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fandst du dich selbst zu arm f\u00fcr solchen Wert,", "tokens": ["Fandst", "du", "dich", "selbst", "zu", "arm", "f\u00fcr", "sol\u00b7chen", "Wert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "PTKA", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und ehrlich, so viel Beifall zu verdienen,", "tokens": ["Und", "ehr\u00b7lich", ",", "so", "viel", "Bei\u00b7fall", "zu", "ver\u00b7die\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADV", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hast sp\u00e4ter Bildung du dich zugekehrt.", "tokens": ["Hast", "sp\u00e4\u00b7ter", "Bil\u00b7dung", "du", "dich", "zu\u00b7ge\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "NN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Mit \u00f6sterreichscher alter Treue,", "tokens": ["Mit", "\u00f6s\u00b7ter\u00b7reich\u00b7scher", "al\u00b7ter", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Um auszuf\u00fcllen, was dir noch zu weit,", "tokens": ["Um", "aus\u00b7zu\u00b7f\u00fcl\u00b7len", ",", "was", "dir", "noch", "zu", "weit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "VVIZU", "$,", "PWS", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nahmst du die Torenweisheit, alt und neue,", "tokens": ["Nahmst", "du", "die", "To\u00b7ren\u00b7weis\u00b7heit", ",", "alt", "und", "neu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Rasch auf in deines Ruhmes schwellend Kleid.", "tokens": ["Rasch", "auf", "in", "dei\u00b7nes", "Ruh\u00b7mes", "schwel\u00b7lend", "Kleid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "APPR", "PPOSAT", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Und weil dem Liebchen gerne nah der Buhle,", "tokens": ["Und", "weil", "dem", "Lieb\u00b7chen", "ger\u00b7ne", "nah", "der", "Buh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Wind am st\u00e4rksten da, woher er weht,", "tokens": ["Der", "Wind", "am", "st\u00e4rks\u00b7ten", "da", ",", "wo\u00b7her", "er", "weht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Begabst du dich in Schwabens Dichterschule,", "tokens": ["Be\u00b7gabst", "du", "dich", "in", "Schwa\u00b7bens", "Dich\u00b7ter\u00b7schu\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo fern ein Meister seinen Sch\u00fclern steht.", "tokens": ["Wo", "fern", "ein", "Meis\u00b7ter", "sei\u00b7nen", "Sch\u00fc\u00b7lern", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Dort in der alten Heimat alter Sparren,", "tokens": ["Dort", "in", "der", "al\u00b7ten", "Hei\u00b7mat", "al\u00b7ter", "Spar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum M\u00e4rchen schon gewordenen von je,", "tokens": ["Zum", "M\u00e4r\u00b7chen", "schon", "ge\u00b7wor\u00b7de\u00b7nen", "von", "je", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJA", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dem Vaterlande der Genies und Narren,", "tokens": ["Dem", "Va\u00b7ter\u00b7lan\u00b7de", "der", "Ge\u00b7nies", "und", "Nar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weil fix, als beiden eigen, die Idee,", "tokens": ["Weil", "fix", ",", "als", "bei\u00b7den", "ei\u00b7gen", ",", "die", "I\u00b7dee", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "KOUS", "PIAT", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Warst du von einem M\u00e4nnerkreis umgeben,", "tokens": ["Warst", "du", "von", "ei\u00b7nem", "M\u00e4n\u00b7ner\u00b7kreis", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die granweis, wie einst K\u00f6nig Mithridat,", "tokens": ["Die", "gran\u00b7weis", ",", "wie", "einst", "K\u00f6\u00b7nig", "Mith\u00b7ri\u00b7dat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADV", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "An Gift gew\u00f6hnt sich all ihr ganzes Leben,", "tokens": ["An", "Gift", "ge\u00b7w\u00f6hnt", "sich", "all", "ihr", "gan\u00b7zes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "PRF", "PIAT", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So da\u00df sie nun verdauen jeden Grad.", "tokens": ["So", "da\u00df", "sie", "nun", "ver\u00b7dau\u00b7en", "je\u00b7den", "Grad", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Du aber mit den unentweihten Kr\u00e4ften,", "tokens": ["Du", "a\u00b7ber", "mit", "den", "un\u00b7ent\u00b7weih\u00b7ten", "Kr\u00e4f\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der sein du wolltest, was f\u00fcr jene Scherz,", "tokens": ["Der", "sein", "du", "woll\u00b7test", ",", "was", "f\u00fcr", "je\u00b7ne", "Scherz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "PPER", "VMFIN", "$,", "PRELS", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du trankst dir Tod in jenen Taumels\u00e4ften,", "tokens": ["Du", "trankst", "dir", "Tod", "in", "je\u00b7nen", "Tau\u00b7mel\u00b7s\u00e4f\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was f\u00fcr den Kopf bestimmt, es traf dein Herz.", "tokens": ["Was", "f\u00fcr", "den", "Kopf", "be\u00b7stimmt", ",", "es", "traf", "dein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVPP", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Da trat, was du geflohn in allen Tagen,", "tokens": ["Da", "trat", ",", "was", "du", "ge\u00b7flohn", "in", "al\u00b7len", "Ta\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWS", "PPER", "VVPP", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Wirklichkeit dich an, von Inhalt schwer,", "tokens": ["Die", "Wirk\u00b7lich\u00b7keit", "dich", "an", ",", "von", "In\u00b7halt", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Halb selbst sich \u00dcberheben, halb Verzagen,", "tokens": ["Halb", "selbst", "sich", "\u00dc\u00b7ber\u00b7he\u00b7ben", ",", "halb", "Ver\u00b7za\u00b7gen", ","], "t