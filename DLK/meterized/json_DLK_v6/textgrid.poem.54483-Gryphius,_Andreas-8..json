{"textgrid.poem.54483": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der/ so der Menschen Hertz eh als man meint verwend", "tokens": ["Der", "/", "so", "der", "Men\u00b7schen", "Hertz", "eh", "als", "man", "meint", "ver\u00b7wend"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "$(", "ADV", "ART", "NN", "NN", "KOUS", "KOUS", "PIS", "VVFIN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat offt der Feinde Zorn/ vnd vngest\u00fcmes pochen.", "tokens": ["Hat", "offt", "der", "Fein\u00b7de", "Zorn", "/", "vnd", "vn\u00b7ge\u00b7st\u00fc\u00b7mes", "po\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN", "$(", "KON", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd das schon blosse Schwerdt in einem nu/ zubrochen.", "tokens": ["Vnd", "das", "schon", "blos\u00b7se", "Schwerdt", "in", "ei\u00b7nem", "nu", "/", "zu\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PDS", "ADV", "ADJA", "NN", "APPR", "ART", "ADV", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer seine Stachel leckt/ nimbt ein erschrecklich End.", "tokens": ["Wer", "sei\u00b7ne", "Sta\u00b7chel", "leckt", "/", "nimbt", "ein", "er\u00b7schreck\u00b7lich", "End", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$(", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ob gleich der grimme Saul vor lauter toben brennt/", "tokens": ["Ob", "gleich", "der", "grim\u00b7me", "Saul", "vor", "lau\u00b7ter", "to\u00b7ben", "brennt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hat doch/ eh denn er recht der Christen Blutt gerochen/", "tokens": ["Hat", "doch", "/", "eh", "denn", "er", "recht", "der", "Chris\u00b7ten", "Blutt", "ge\u00b7ro\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$(", "KOUS", "KON", "PPER", "ADV", "ART", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sein Muth sich vnversehns vor Gottes Plitz verkrochen", "tokens": ["Sein", "Muth", "sich", "vn\u00b7ver\u00b7sehns", "vor", "Got\u00b7tes", "Plitz", "ver\u00b7kro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PRF", "ADJD", "APPR", "NN", "NE", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sein schnarchen fellt in Staub/ sein Auge wird geblendt/", "tokens": ["Sein", "schnar\u00b7chen", "fellt", "in", "Staub", "/", "sein", "Au\u00b7ge", "wird", "ge\u00b7blendt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "APPR", "NN", "$(", "PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ia/ was noch mehr/ der Mann/ so mit Creutz/ Band vnd Plagen/", "tokens": ["I\u00b7a", "/", "was", "noch", "mehr", "/", "der", "Mann", "/", "so", "mit", "Creutz", "/", "Band", "vnd", "Pla\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PWS", "ADV", "ADV", "$(", "ART", "NN", "$(", "ADV", "APPR", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.10": {"text": "Sich wieder Christum setzt/ lernt nichts als Christum sagen/", "tokens": ["Sich", "wie\u00b7der", "Chris\u00b7tum", "setzt", "/", "lernt", "nichts", "als", "Chris\u00b7tum", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "NE", "VVFIN", "$(", "VVFIN", "PIS", "KOKOM", "NE", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Vnd bereit Christi Ruhm vor allen Printzen au\u00df.", "tokens": ["Vnd", "be\u00b7reit", "Chris\u00b7ti", "Ruhm", "vor", "al\u00b7len", "Print\u00b7zen", "au\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NE", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.12": {"text": "So st\u00fcrtzt der grosse Gott auch alle/ die dich letzen;", "tokens": ["So", "st\u00fcrtzt", "der", "gros\u00b7se", "Gott", "auch", "al\u00b7le", "/", "die", "dich", "let\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADV", "PIAT", "$(", "PRELS", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Vnd wil dir hundertfach/ was ihr Geitz raubt ersetzen.", "tokens": ["Vnd", "wil", "dir", "hun\u00b7dert\u00b7fach", "/", "was", "ihr", "Geitz", "raubt", "er\u00b7set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "$(", "PWS", "PPOSAT", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Drumb Leid vnd la\u00df f\u00fcr ihn/ Kind/ Vater/ Freund/ vnd Haus.", "tokens": ["Drumb", "Leid", "vnd", "la\u00df", "f\u00fcr", "ihn", "/", "Kind", "/", "Va\u00b7ter", "/", "Freund", "/", "vnd", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "NN", "KON", "VVIMP", "APPR", "PPER", "$(", "NN", "$(", "NN", "$(", "NN", "$(", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}