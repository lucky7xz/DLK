{"textgrid.poem.37262": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[die Tante winkt, die Tante lacht]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Tante winkt, die Tante lacht:", "tokens": ["Die", "Tan\u00b7te", "winkt", ",", "die", "Tan\u00b7te", "lacht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "He, Fritz, komm mal herein!", "tokens": ["He", ",", "Fritz", ",", "komm", "mal", "her\u00b7ein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NE", "$,", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sieh, welch ein h\u00fcbsches Br\u00fcderlein", "tokens": ["Sieh", ",", "welch", "ein", "h\u00fcb\u00b7sches", "Br\u00fc\u00b7derl\u00b7ein"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der gute Storch in letzter Nacht", "tokens": ["Der", "gu\u00b7te", "Storch", "in", "letz\u00b7ter", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ganz heimlich der Mama gebracht.", "tokens": ["Ganz", "heim\u00b7lich", "der", "Ma\u00b7ma", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ei ja, das wird dich freun!", "tokens": ["Ei", "ja", ",", "das", "wird", "dich", "freun", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$,", "PDS", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Fritz, der sagte kurz und grob:", "tokens": ["Der", "Fritz", ",", "der", "sag\u00b7te", "kurz", "und", "grob", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich hol 'n dicken Stein", "tokens": ["Ich", "hol", "'n", "di\u00b7cken", "Stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Und schmei\u00df ihn an den Kopp!", "tokens": ["Und", "schmei\u00df", "ihn", "an", "den", "Kopp", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}