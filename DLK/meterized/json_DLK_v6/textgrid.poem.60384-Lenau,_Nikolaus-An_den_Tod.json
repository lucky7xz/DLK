{"textgrid.poem.60384": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "An den Tod", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenns mir einst im Herzen modert,", "tokens": ["Wenns", "mir", "einst", "im", "Her\u00b7zen", "mo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn der Dichtkunst k\u00fchne Flammen", "tokens": ["Wenn", "der", "Dicht\u00b7kunst", "k\u00fch\u00b7ne", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Liebe Brand verlodert,", "tokens": ["Und", "der", "Lie\u00b7be", "Brand", "ver\u00b7lo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tod, dann brich den Leib zusammen!", "tokens": ["Tod", ",", "dann", "brich", "den", "Leib", "zu\u00b7sam\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Brich ihn schnell, nicht langsam w\u00fchle,", "tokens": ["Brich", "ihn", "schnell", ",", "nicht", "lang\u00b7sam", "w\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "$,", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deinen S\u00e4nger la\u00df entschweben,", "tokens": ["Dei\u00b7nen", "S\u00e4n\u00b7ger", "la\u00df", "ent\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVIMP", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "D\u00fcngen nicht das Feld dem Leben", "tokens": ["D\u00fcn\u00b7gen", "nicht", "das", "Feld", "dem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit der Asche der Gef\u00fchle.", "tokens": ["Mit", "der", "A\u00b7sche", "der", "Ge\u00b7f\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}