{"dta.poem.8350": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die Keuschheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer ist die, die im weisen Kleide", "tokens": ["Wer", "ist", "die", ",", "die", "im", "wei\u00b7sen", "Klei\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von ungef\u00e4rbter heller Seide", "tokens": ["Von", "un\u00b7ge\u00b7f\u00e4rb\u00b7ter", "hel\u00b7ler", "Sei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und in dem reinen Silberglanz", "tokens": ["Und", "in", "dem", "rei\u00b7nen", "Sil\u00b7ber\u00b7glanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit einem frohen Angesichte", "tokens": ["Mit", "ei\u00b7nem", "fro\u00b7hen", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und das umstrahlt von Himmelslichte", "tokens": ["Und", "das", "um\u00b7strahlt", "von", "Him\u00b7mels\u00b7lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Geschm\u00fckt im unverwelkten Kranz,", "tokens": ["Ge\u00b7schm\u00fckt", "im", "un\u00b7ver\u00b7welk\u00b7ten", "Kranz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und die mit einem Ehren-Bogen", "tokens": ["Und", "die", "mit", "ei\u00b7nem", "Eh\u00b7ren\u00b7Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ist \u00fcber ihren Haupt bezogen?", "tokens": ["Ist", "\u00fc\u00b7ber", "ih\u00b7ren", "Haupt", "be\u00b7zo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich lese es an deiner Stirne,", "tokens": ["Ich", "le\u00b7se", "es", "an", "dei\u00b7ner", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist es Keuschheit, holde Dirne", "tokens": ["Du", "bist", "es", "Keuschheit", ",", "hol\u00b7de", "Dir\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "NN", "$,", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die so in ungeschminkter Pracht,", "tokens": ["Die", "so", "in", "un\u00b7ge\u00b7schmink\u00b7ter", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als eine Braut sich herrlich kr\u00e4nzet;", "tokens": ["Als", "ei\u00b7ne", "Braut", "sich", "herr\u00b7lich", "kr\u00e4n\u00b7zet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du bist es, die so lieblich gl\u00e4nzet", "tokens": ["Du", "bist", "es", ",", "die", "so", "lieb\u00b7lich", "gl\u00e4n\u00b7zet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PRELS", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und mit bescheidner Demut lacht:", "tokens": ["Und", "mit", "be\u00b7scheid\u00b7ner", "De\u00b7mut", "lacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O! m\u00f6chte sich du Schmuk der Seelen,", "tokens": ["O", "!", "m\u00f6ch\u00b7te", "sich", "du", "Schmuk", "der", "See\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VMFIN", "PRF", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Jugend doch mit dir verm\u00e4hlen!", "tokens": ["Die", "Ju\u00b7gend", "doch", "mit", "dir", "ver\u00b7m\u00e4h\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O! Keuschheit auserw\u00e4hlte Tugend,", "tokens": ["O", "!", "Keuschheit", "au\u00b7ser\u00b7w\u00e4hl\u00b7te", "Tu\u00b7gend", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du Kron der Alten, Schmuk der Jugend!", "tokens": ["Du", "Kron", "der", "Al\u00b7ten", ",", "Schmuk", "der", "Ju\u00b7gend", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie reinlich bist du anzusehn!", "tokens": ["Wie", "rein\u00b7lich", "bist", "du", "an\u00b7zu\u00b7sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es ist an dir kein Schmuz, noch Flekken", "tokens": ["Es", "ist", "an", "dir", "kein", "Schmuz", ",", "noch", "Flek\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "PIAT", "NN", "$,", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit scharffen Augen zu entdekken,", "tokens": ["Mit", "scharf\u00b7fen", "Au\u00b7gen", "zu", "ent\u00b7dek\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du bist in allen herrlich sch\u00f6n;", "tokens": ["Du", "bist", "in", "al\u00b7len", "herr\u00b7lich", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Man kann dein ungeschminktes Wesen,", "tokens": ["Man", "kann", "dein", "un\u00b7ge\u00b7schmink\u00b7tes", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "An Mienen, Kleidern deutlich lesen.", "tokens": ["An", "Mie\u00b7nen", ",", "Klei\u00b7dern", "deut\u00b7lich", "le\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wer dich ansieht der wird ger\u00fchret,", "tokens": ["Wer", "dich", "an\u00b7sieht", "der", "wird", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "ART", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist mit keinen Schmuk gezieret,", "tokens": ["Du", "bist", "mit", "kei\u00b7nen", "Schmuk", "ge\u00b7zie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der mit erborgten Firnis prahlt;", "tokens": ["Der", "mit", "er\u00b7borg\u00b7ten", "Fir\u00b7nis", "prahlt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Flittergold mit falschen Scheine,", "tokens": ["Kein", "Flit\u00b7ter\u00b7gold", "mit", "fal\u00b7schen", "Schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nur \u00e4chte Farb ist es alleine,", "tokens": ["Nur", "\u00e4ch\u00b7te", "Farb", "ist", "es", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die aus den reinen Kleidern strahlt;", "tokens": ["Die", "aus", "den", "rei\u00b7nen", "Klei\u00b7dern", "strahlt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dein Licht blizt nicht aus Diamanten,", "tokens": ["Dein", "Licht", "blizt", "nicht", "aus", "Di\u00b7a\u00b7man\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Noch von den wollgew\u00fcrkten Kandten.", "tokens": ["Noch", "von", "den", "woll\u00b7ge\u00b7w\u00fcrk\u00b7ten", "Kand\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dein Glanz entsteht aus eigner Helle,", "tokens": ["Dein", "Glanz", "ent\u00b7steht", "aus", "eig\u00b7ner", "Hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du selbsten bist die wahre Quelle,", "tokens": ["Du", "selbs\u00b7ten", "bist", "die", "wah\u00b7re", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Woraus das Angenehm entspringt,", "tokens": ["Wo\u00b7raus", "das", "An\u00b7ge\u00b7nehm", "ent\u00b7springt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das alle die dich recht beschauen,", "tokens": ["Das", "al\u00b7le", "die", "dich", "recht", "be\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "ART", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In die Enz\u00fckkungs-s\u00fcsse Auen,", "tokens": ["In", "die", "En\u00b7z\u00fc\u00b7kkungs\u00b7s\u00fcs\u00b7se", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-++-+-+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Des innigen Vergn\u00fcgens bringt;", "tokens": ["Des", "in\u00b7ni\u00b7gen", "Ver\u00b7gn\u00fc\u00b7gens", "bringt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Nat\u00fcrlich sch\u00f6n ist deine Farbe,", "tokens": ["Na\u00b7t\u00fcr\u00b7lich", "sch\u00f6n", "ist", "dei\u00b7ne", "Far\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ohn alle \u00fcberschminkte Narbe.", "tokens": ["Ohn", "al\u00b7le", "\u00fc\u00b7bersc\u00b7hmink\u00b7te", "Nar\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Es geht aus deinem Augenlichte,", "tokens": ["Es", "geht", "aus", "dei\u00b7nem", "Au\u00b7gen\u00b7lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Strahl der deinem Angesichte:", "tokens": ["Kein", "Strahl", "der", "dei\u00b7nem", "An\u00b7ge\u00b7sich\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Frechheits-Zeichen angebrandt:", "tokens": ["Ein", "Frech\u00b7heits\u00b7Zei\u00b7chen", "an\u00b7ge\u00b7brandt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Man sieht dieselben lieblich funkeln,", "tokens": ["Man", "sieht", "die\u00b7sel\u00b7ben", "lieb\u00b7lich", "fun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PDAT", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht durch Tr\u00fcbsinnigkeit verdunkeln,", "tokens": ["Nicht", "durch", "Tr\u00fcb\u00b7sin\u00b7nig\u00b7keit", "ver\u00b7dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Sie haben einen freien Stand,", "tokens": ["Sie", "ha\u00b7ben", "ei\u00b7nen", "frei\u00b7en", "Stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch fliegen daraus keine Blizze,", "tokens": ["Doch", "flie\u00b7gen", "da\u00b7raus", "kei\u00b7ne", "Bliz\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Von einer wilden Jugend Hizze.", "tokens": ["Von", "ei\u00b7ner", "wil\u00b7den", "Ju\u00b7gend", "Hiz\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}