{"textgrid.poem.32616": {"metadata": {"author": {"name": "Pet\u00f6fi, S\u00e1ndor", "birth": "N.A.", "death": "N.A."}, "title": "1L: Pferdegetrappel, da\u00df es staubt,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Pferdegetrappel, da\u00df es staubt,", "tokens": ["Pfer\u00b7de\u00b7ge\u00b7trap\u00b7pel", ",", "da\u00df", "es", "staubt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Lustig schmettern die Trompeten:", "tokens": ["Lus\u00b7tig", "schmet\u00b7tern", "die", "Trom\u00b7pe\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbhollah, die Husaren sind's,", "tokens": ["\u00bb", "hol\u00b7lah", ",", "die", "Hu\u00b7sa\u00b7ren", "sin\u00b7d's", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "ART", "NN", "VAFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Nachtquartier wird heut erbeten!\u00ab", "tokens": ["Nacht\u00b7quar\u00b7tier", "wird", "heut", "er\u00b7be\u00b7ten", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "ADV", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und am Platz wird Halt gemacht,", "tokens": ["Und", "am", "Platz", "wird", "Halt", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Flinke, schmucke Heldenscharen;", "tokens": ["Flin\u00b7ke", ",", "schmu\u00b7cke", "Hel\u00b7den\u00b7scha\u00b7ren", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alt und jung im Dorfe staunt,", "tokens": ["Alt", "und", "jung", "im", "Dor\u00b7fe", "staunt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gibt's was Sch\u00f6n'res als Husaren?", "tokens": ["Gibt's", "was", "Sch\u00f6n'\u00b7res", "als", "Hu\u00b7sa\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWS", "NN", "KOUS", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch, ist auch die Freude gro\u00df,", "tokens": ["Doch", ",", "ist", "auch", "die", "Freu\u00b7de", "gro\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Macht's die Sorge nicht geringer:", "tokens": ["Macht's", "die", "Sor\u00b7ge", "nicht", "ge\u00b7rin\u00b7ger", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schmuck sind die Husaren wohl,", "tokens": ["Schmuck", "sind", "die", "Hu\u00b7sa\u00b7ren", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch \u2013 sie haben flinke Finger.", "tokens": ["Doch", "\u2013", "sie", "ha\u00b7ben", "flin\u00b7ke", "Fin\u00b7ger", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PPER", "VAFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbm\u00e4del, gib nur flei\u00dfig acht!\u00ab", "tokens": ["\u00bb", "m\u00e4\u00b7del", ",", "gib", "nur", "flei\u00b7\u00dfig", "acht", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$,", "VVIMP", "ADV", "ADJD", "CARD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sagt das M\u00fctterlein beklommen", "tokens": ["Sagt", "das", "M\u00fct\u00b7ter\u00b7lein", "be\u00b7klom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbdenn, wie leicht kann \u00fcber Nacht", "tokens": ["\u00bb", "denn", ",", "wie", "leicht", "kann", "\u00fc\u00b7ber", "Nacht"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "$,", "PWAV", "ADJD", "VMFIN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier etwas abhanden kommen! ...\u00ab", "tokens": ["Hier", "et\u00b7was", "ab\u00b7han\u00b7den", "kom\u00b7men", "!", "...", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVINF", "$.", "$(", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "... Da der Morgen neu erwacht,", "tokens": ["...", "Da", "der", "Mor\u00b7gen", "neu", "er\u00b7wacht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind auch wach die Kriegersleute.", "tokens": ["Sind", "auch", "wach", "die", "Krie\u00b7gers\u00b7leu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein flotter Marsch ert\u00f6nt,", "tokens": ["Und", "ein", "flot\u00b7ter", "Marsch", "er\u00b7t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Rasch aufs Pferd, und fort ins Weite!", "tokens": ["Rasch", "aufs", "Pferd", ",", "und", "fort", "ins", "Wei\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,", "KON", "PTKVZ", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und die Kleine folgt bewegt", "tokens": ["Und", "die", "Klei\u00b7ne", "folgt", "be\u00b7wegt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "VVFIN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihnen nach mit stummen Blicken,", "tokens": ["Ih\u00b7nen", "nach", "mit", "stum\u00b7men", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tr\u00e4nen stehn ihr im Gesicht,", "tokens": ["Tr\u00e4\u00b7nen", "stehn", "ihr", "im", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Leid und Kummer sie bedr\u00fccken.", "tokens": ["Leid", "und", "Kum\u00b7mer", "sie", "be\u00b7dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbm\u00e4del, sprich, was hast du denn,", "tokens": ["\u00bb", "m\u00e4\u00b7del", ",", "sprich", ",", "was", "hast", "du", "denn", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "ADJD", "$,", "PWS", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was ist \u00fcber dich gekommen?\u00ab", "tokens": ["Was", "ist", "\u00fc\u00b7ber", "dich", "ge\u00b7kom\u00b7men", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PPER", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbm\u00fctterlein, ein Dieb, ein Dieb", "tokens": ["\u00bb", "m\u00fct\u00b7ter\u00b7lein", ",", "ein", "Dieb", ",", "ein", "Dieb"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADJD", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat mein Herz mit sich genommen!\u00ab", "tokens": ["Hat", "mein", "Herz", "mit", "sich", "ge\u00b7nom\u00b7men", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "PRF", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}