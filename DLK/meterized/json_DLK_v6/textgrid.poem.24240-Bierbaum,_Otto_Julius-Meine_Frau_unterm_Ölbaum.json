{"textgrid.poem.24240": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Meine Frau unterm \u00d6lbaum", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der \u00d6lbaum: ein zarter Strau\u00df, ...", "tokens": ["Der", "\u00d6l\u00b7baum", ":", "ein", "zar\u00b7ter", "Strau\u00df", ",", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$.", "ART", "ADJA", "NE", "$,", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie aus grauen Seidenspitzen, ...", "tokens": ["Wie", "aus", "grau\u00b7en", "Sei\u00b7den\u00b7spit\u00b7zen", ",", "..."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie Silberschmiedarbeit sieht er aus,", "tokens": ["Wie", "Sil\u00b7ber\u00b7schmie\u00b7dar\u00b7beit", "sieht", "er", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auf der schwarzblaue Edelsteine blitzen.", "tokens": ["Auf", "der", "schwarz\u00b7blau\u00b7e", "E\u00b7del\u00b7stei\u00b7ne", "blit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+++-+-+-+-", "measure": "unknown.measure.hexa"}}, "stanza.2": {"line.1": {"text": "Stehst du darunter in dem Blumenkleide", "tokens": ["Stehst", "du", "da\u00b7run\u00b7ter", "in", "dem", "Blu\u00b7men\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus Indiens buddhabunter Seide,", "tokens": ["Aus", "In\u00b7di\u00b7ens", "budd\u00b7ha\u00b7bun\u00b7ter", "Sei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Dann ist der ganze Garten ein Geschmeide.", "tokens": ["Dann", "ist", "der", "gan\u00b7ze", "Gar\u00b7ten", "ein", "Ge\u00b7schmei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und, tr\u00e4ume ich denn, was ich da seh?", "tokens": ["Und", ",", "tr\u00e4u\u00b7me", "ich", "denn", ",", "was", "ich", "da", "seh", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "ADV", "$,", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wen seh ich dort in Signor Bardis Haus", "tokens": ["Wen", "seh", "ich", "dort", "in", "Sig\u00b7nor", "Bar\u00b7dis", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "NE", "NE", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Vor seiner Staffelei im T\u00fcrgevierte sitzen? \u2013", "tokens": ["Vor", "sei\u00b7ner", "Staf\u00b7fe\u00b7lei", "im", "T\u00fcr\u00b7ge\u00b7vier\u00b7te", "sit\u00b7zen", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Frate Giovanni da Fiesole!", "tokens": ["Fra\u00b7te", "Gio\u00b7van\u00b7ni", "da", "Fie\u00b7so\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Der wei\u00dfen Kutte weite \u00c4rmel sind", "tokens": ["Der", "wei\u00b7\u00dfen", "Kut\u00b7te", "wei\u00b7te", "\u00c4r\u00b7mel", "sind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den mageren Arm hinaufgeschlagen,", "tokens": ["Den", "ma\u00b7ge\u00b7ren", "Arm", "hin\u00b7auf\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und seine frommen braunen Augen fragen", "tokens": ["Und", "sei\u00b7ne", "from\u00b7men", "brau\u00b7nen", "Au\u00b7gen", "fra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Just seine Farbentiegel, ob f\u00fcr dich, mein Kind,", "tokens": ["Just", "sei\u00b7ne", "Far\u00b7ben\u00b7tie\u00b7gel", ",", "ob", "f\u00fcr", "dich", ",", "mein", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "KOUS", "APPR", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie auch genug der reinen Farben tragen.", "tokens": ["Sie", "auch", "ge\u00b7nug", "der", "rei\u00b7nen", "Far\u00b7ben", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Steh still, steh still! Und blicke klar und froh,", "tokens": ["Steh", "still", ",", "steh", "still", "!", "Und", "bli\u00b7cke", "klar", "und", "froh", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$.", "KON", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gradaus, mich an! Aus deinen Augen lesen", "tokens": ["Gra\u00b7daus", ",", "mich", "an", "!", "Aus", "dei\u00b7nen", "Au\u00b7gen", "le\u00b7sen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "PTKVZ", "$.", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Soll ferne Zeit noch, wie du gut und klar,", "tokens": ["Soll", "fer\u00b7ne", "Zeit", "noch", ",", "wie", "du", "gut", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "ADV", "$,", "PWAV", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie sch\u00f6n und lauter du, wie treu und wahr", "tokens": ["Wie", "sch\u00f6n", "und", "lau\u00b7ter", "du", ",", "wie", "treu", "und", "wahr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "PPER", "$,", "PWAV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dein starkes, liebevolles Herz gewesen!", "tokens": ["Dein", "star\u00b7kes", ",", "lie\u00b7be\u00b7vol\u00b7les", "Herz", "ge\u00b7we\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Drum malt dich Fra Angelico.", "tokens": ["Drum", "malt", "dich", "Fra", "An\u00b7ge\u00b7li\u00b7co", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "NE", "NE", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}