{"textgrid.poem.65700": {"metadata": {"author": {"name": "Drollinger, Carl Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich sah Helvetien in Gram und Unmuht sinken,", "genre": "verse", "period": "N.A.", "pub_year": 1724, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah Helvetien in Gram und Unmuht sinken,", "tokens": ["Ich", "sah", "Hel\u00b7ve\u00b7ti\u00b7en", "in", "Gram", "und", "Un\u00b7muht", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "APPR", "NE", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als durch sein weites Land, zum Vorwurf unsrer Zeit,", "tokens": ["Als", "durch", "sein", "wei\u00b7tes", "Land", ",", "zum", "Vor\u00b7wurf", "uns\u00b7rer", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "ADJA", "NN", "$,", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Fast keinem Dichter mehr ein de\u00fctsches Lied gedeiht.", "tokens": ["Fast", "kei\u00b7nem", "Dich\u00b7ter", "mehr", "ein", "de\u00fct\u00b7sches", "Lied", "ge\u00b7deiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie, sprach Es, wollt ihr nie aus eignen Quellen trinken?", "tokens": ["Wie", ",", "sprach", "Es", ",", "wollt", "ihr", "nie", "aus", "eig\u00b7nen", "Quel\u00b7len", "trin\u00b7ken", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Soll nur Athen und Rom e\u00fcch e\u00fcre Lieder schmincken?", "tokens": ["Soll", "nur", "A\u00b7then", "und", "Rom", "e\u00b7\u00fcch", "e\u00b7\u00fc\u00b7re", "Lie\u00b7der", "schmin\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NE", "KON", "NE", "ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Wird doch ein de\u00fctscher Mund verh\u00f6hnet und entweiht,", "tokens": ["Wird", "doch", "ein", "de\u00fct\u00b7scher", "Mund", "ver\u00b7h\u00f6h\u00b7net", "und", "ent\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dem ein besiegtes Volk die waichen Worte leiht,", "tokens": ["Dem", "ein", "be\u00b7sieg\u00b7tes", "Volk", "die", "wai\u00b7chen", "Wor\u00b7te", "leiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und dessen Sch\u00e4tze stets in fremdem Schmucke blinken.", "tokens": ["Und", "des\u00b7sen", "Sch\u00e4t\u00b7ze", "stets", "in", "frem\u00b7dem", "Schmu\u00b7cke", "blin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es kam Ihm dieser Schimpf ganz unertr\u00e4glich vor.", "tokens": ["Es", "kam", "Ihm", "die\u00b7ser", "Schimpf", "ganz", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PDAT", "NN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie aber schaute nicht Helvetien empor,", "tokens": ["Wie", "a\u00b7ber", "schau\u00b7te", "nicht", "Hel\u00b7ve\u00b7ti\u00b7en", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PTKNEG", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als seines Sprengen Kiel zu seiner H\u00fclf erschienen!", "tokens": ["Als", "sei\u00b7nes", "Spren\u00b7gen", "Kiel", "zu", "sei\u00b7ner", "H\u00fclf", "er\u00b7schie\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NE", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Es rief: O werter Sohn! der seine Sprache kr\u00f6nt,", "tokens": ["Es", "rief", ":", "O", "wer\u00b7ter", "Sohn", "!", "der", "sei\u00b7ne", "Spra\u00b7che", "kr\u00f6nt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NE", "ADJA", "NN", "$.", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil noch ein Heldenlied um meine Berge t\u00f6hnt,", "tokens": ["Weil", "noch", "ein", "Hel\u00b7den\u00b7lied", "um", "mei\u00b7ne", "Ber\u00b7ge", "t\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Soll dein Ged\u00e4chtni\u00df auch in stetem Ruhme gr\u00fcnen.", "tokens": ["Soll", "dein", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "auch", "in", "ste\u00b7tem", "Ruh\u00b7me", "gr\u00fc\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}