{"dta.poem.1670": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ein Bienen-Stock mit aus- und einfliegen-  \n den Bienen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Die Biene sucht nicht ihr den Honig nur allein:", "tokens": ["Die", "Bie\u00b7ne", "sucht", "nicht", "ihr", "den", "Ho\u00b7nig", "nur", "al\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "PPER", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Manch Mensch und Thier pflegt Gast bey ihrer Kost zu", "tokens": ["Manch", "Mensch", "und", "Thier", "pflegt", "Gast", "bey", "ih\u00b7rer", "Kost", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "NN", "VVFIN", "NN", "APPR", "PPOSAT", "NN", "PTKZU"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "seyn.", "tokens": ["seyn", "."], "token_info": ["word", "punct"], "pos": ["VAINF", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Wer andern nicht zu Nutz anwendet seine Gaben/", "tokens": ["Wer", "an\u00b7dern", "nicht", "zu", "Nutz", "an\u00b7wen\u00b7det", "sei\u00b7ne", "Ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "PTKNEG", "APPR", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mu\u00df einen Stachel nur und keinen Honig haben.", "tokens": ["Mu\u00df", "ei\u00b7nen", "Sta\u00b7chel", "nur", "und", "kei\u00b7nen", "Ho\u00b7nig", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "KON", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}