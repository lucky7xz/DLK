{"textgrid.poem.34545": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Lande der Thorheit k\u00fcsst ich die H\u00e4nde der sch\u00f6nen Fraun,", "genre": "verse", "period": "N.A.", "pub_year": 1897, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Lande der Thorheit k\u00fcsst ich die H\u00e4nde der sch\u00f6nen Fraun,", "tokens": ["Im", "Lan\u00b7de", "der", "Thor\u00b7heit", "k\u00fcsst", "ich", "die", "H\u00e4n\u00b7de", "der", "sch\u00f6\u00b7nen", "Fraun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "sie waren schmeichelnd und weiss, mit blitzenden Ringen geschm\u00fcckt.", "tokens": ["sie", "wa\u00b7ren", "schmei\u00b7chelnd", "und", "weiss", ",", "mit", "blit\u00b7zen\u00b7den", "Rin\u00b7gen", "ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ich lachte wohl auch beim lieblich klingenden, lockenden Wort", "tokens": ["Ich", "lach\u00b7te", "wohl", "auch", "beim", "lieb\u00b7lich", "klin\u00b7gen\u00b7den", ",", "lo\u00b7cken\u00b7den", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPRART", "ADJD", "VVINF", "$,", "ADJA", "NN"], "meter": "-+--+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "und eitel genoss ich des eigenen spielenden \u00dcbermuths.", "tokens": ["und", "ei\u00b7tel", "ge\u00b7noss", "ich", "des", "ei\u00b7ge\u00b7nen", "spie\u00b7len\u00b7den", "\u00dc\u00b7ber\u00b7muths", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+--+--+-+", "measure": "amphibrach.penta.plus"}}, "stanza.2": {"line.1": {"text": "Doch immer wieder irrte mein Blick ins Leere ab:", "tokens": ["Doch", "im\u00b7mer", "wie\u00b7der", "irr\u00b7te", "mein", "Blick", "ins", "Lee\u00b7re", "ab", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ich sah und f\u00fchlte die H\u00e4nde meiner lieben Frau,", "tokens": ["Ich", "sah", "und", "f\u00fchl\u00b7te", "die", "H\u00e4n\u00b7de", "mei\u00b7ner", "lie\u00b7ben", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "die weich und still in ruhender G\u00fcte sich nach mir", "tokens": ["die", "weich", "und", "still", "in", "ru\u00b7hen\u00b7der", "G\u00fc\u00b7te", "sich", "nach", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "KON", "ADJD", "APPR", "ADJA", "NN", "PRF", "APPR", "PPER"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "hersehnen aus der Ferne \u2013 deine H\u00e4nde; die", "tokens": ["her\u00b7seh\u00b7nen", "aus", "der", "Fer\u00b7ne", "\u2013", "dei\u00b7ne", "H\u00e4n\u00b7de", ";", "die"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$(", "PPOSAT", "NN", "$.", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "allein die Wirrniss dumpfen Wollens je gebannt \u2013", "tokens": ["al\u00b7lein", "die", "Wirr\u00b7niss", "dum\u00b7pfen", "Wol\u00b7lens", "je", "ge\u00b7bannt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "ADV", "VVPP", "$("], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.6": {"text": "und ich gedachte jener Stunde, da mir einst", "tokens": ["und", "ich", "ge\u00b7dach\u00b7te", "je\u00b7ner", "Stun\u00b7de", ",", "da", "mir", "einst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PDAT", "NN", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "im Tode diese H\u00e4nde stummen Trost verleihn.", "tokens": ["im", "To\u00b7de", "die\u00b7se", "H\u00e4n\u00b7de", "stum\u00b7men", "Trost", "ver\u00b7leihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDAT", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}