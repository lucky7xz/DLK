{"textgrid.poem.40270": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und der Abschied war kein Ende,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und der Abschied war kein Ende,", "tokens": ["Und", "der", "Ab\u00b7schied", "war", "kein", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "und mein Blick bewegte dich;", "tokens": ["und", "mein", "Blick", "be\u00b7weg\u00b7te", "dich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und es war, als legte sich", "tokens": ["und", "es", "war", ",", "als", "leg\u00b7te", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "still dein Herz in meine H\u00e4nde ...", "tokens": ["still", "dein", "Herz", "in", "mei\u00b7ne", "H\u00e4n\u00b7de", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber wenn du wiederkehrst,", "tokens": ["A\u00b7ber", "wenn", "du", "wie\u00b7der\u00b7kehrst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "will ich deine Hand nicht k\u00fcssen,", "tokens": ["will", "ich", "dei\u00b7ne", "Hand", "nicht", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "will es nur empfinden m\u00fcssen,", "tokens": ["will", "es", "nur", "emp\u00b7fin\u00b7den", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "VMINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "wie du deinem Herzen wehrst ...", "tokens": ["wie", "du", "dei\u00b7nem", "Her\u00b7zen", "wehrst", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}