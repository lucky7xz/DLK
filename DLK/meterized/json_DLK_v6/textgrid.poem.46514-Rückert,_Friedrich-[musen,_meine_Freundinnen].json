{"textgrid.poem.46514": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[musen, meine Freundinnen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Musen, meine Freundinnen,", "tokens": ["Mu\u00b7sen", ",", "mei\u00b7ne", "Freun\u00b7din\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Oft schon in betr\u00fcbten Lagen", "tokens": ["Oft", "schon", "in", "be\u00b7tr\u00fcb\u00b7ten", "La\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Brachtet ihr mir Trost ins Haus,", "tokens": ["Brach\u00b7tet", "ihr", "mir", "Trost", "ins", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nie doch wie in diesen Tagen,", "tokens": ["Nie", "doch", "wie", "in", "die\u00b7sen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOKOM", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Als die lieben Kinder mir", "tokens": ["Als", "die", "lie\u00b7ben", "Kin\u00b7der", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "An der Seuche niederlagen,", "tokens": ["An", "der", "Seu\u00b7che", "nie\u00b7der\u00b7la\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Deren Todesfunken so", "tokens": ["De\u00b7ren", "To\u00b7des\u00b7fun\u00b7ken", "so"], "token_info": ["word", "word", "word"], "pos": ["PDS", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Leicht ansteckend weiter schlagen,", "tokens": ["Leicht", "an\u00b7ste\u00b7ckend", "wei\u00b7ter", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Freunde viel und Freundinnen", "tokens": ["Freun\u00b7de", "viel", "und", "Freun\u00b7din\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "KON", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.10": {"text": "Z\u00e4hlt' ich sonst mit Wohlbehagen", "tokens": ["Z\u00e4hlt'", "ich", "sonst", "mit", "Wohl\u00b7be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "In der Stadt, die gegen mich", "tokens": ["In", "der", "Stadt", ",", "die", "ge\u00b7gen", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Jeder zarten Sorgfalt pflagen;", "tokens": ["Je\u00b7der", "zar\u00b7ten", "Sorg\u00b7falt", "pfla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Doch f\u00fcr eigne Kinder jetzt", "tokens": ["Doch", "f\u00fcr", "eig\u00b7ne", "Kin\u00b7der", "jetzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Hatten Sorge sie zu tragen,", "tokens": ["Hat\u00b7ten", "Sor\u00b7ge", "sie", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Keiner durfte einen Schritt", "tokens": ["Kei\u00b7ner", "durf\u00b7te", "ei\u00b7nen", "Schritt"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "In mein Haus zu setzen wagen,", "tokens": ["In", "mein", "Haus", "zu", "set\u00b7zen", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Aus gerechter Furcht, das Gift", "tokens": ["Aus", "ge\u00b7rech\u00b7ter", "Furcht", ",", "das", "Gift"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "In sein eignes Haus zu tragen.", "tokens": ["In", "sein", "eig\u00b7nes", "Haus", "zu", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Keiner kam, um meinem Tod", "tokens": ["Kei\u00b7ner", "kam", ",", "um", "mei\u00b7nem", "Tod"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "KOUI", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Oder Leben nachzufragen.", "tokens": ["O\u00b7der", "Le\u00b7ben", "nach\u00b7zu\u00b7fra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Keiner, um aus Freundes Mund", "tokens": ["Kei\u00b7ner", ",", "um", "aus", "Freun\u00b7des", "Mund"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "$,", "KOUI", "APPR", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Mir ein Trostwort anzutragen", "tokens": ["Mir", "ein", "Trost\u00b7wort", "an\u00b7zu\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "VVIZU"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Und mit mir zu klagen, als", "tokens": ["Und", "mit", "mir", "zu", "kla\u00b7gen", ",", "als"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "APPR", "PPER", "PTKZU", "VVINF", "$,", "KOUS"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Lag mein Liebstes auf dem Schragen:", "tokens": ["Lag", "mein", "Liebs\u00b7tes", "auf", "dem", "Schra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Ihr nur, meine Freundinnen,", "tokens": ["Ihr", "nur", ",", "mei\u00b7ne", "Freun\u00b7din\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.26": {"text": "Lie\u00dfet nicht in Furcht euch jagen;", "tokens": ["Lie\u00b7\u00dfet", "nicht", "in", "Furcht", "euch", "ja\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Denn ihr wisset, Himmlische,", "tokens": ["Denn", "ihr", "wis\u00b7set", ",", "Himm\u00b7li\u00b7sche", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "In die Flucht die Furcht zu schlagen,", "tokens": ["In", "die", "Flucht", "die", "Furcht", "zu", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Und Ansteckung droht euch nicht;", "tokens": ["Und", "An\u00b7ste\u00b7ckung", "droht", "euch", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.30": {"text": "Darum d\u00fcrft ihr mit mir klagen,", "tokens": ["Da\u00b7rum", "d\u00fcrft", "ihr", "mit", "mir", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Krankenw\u00e4rterinnen sein,", "tokens": ["Kran\u00b7ken\u00b7w\u00e4r\u00b7te\u00b7rin\u00b7nen", "sein", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Und die K\u00fcchensch\u00fcrze tragen.", "tokens": ["Und", "die", "K\u00fc\u00b7chen\u00b7sch\u00fcr\u00b7ze", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Und solang' ihr bei mir seid,", "tokens": ["Und", "so\u00b7lang'", "ihr", "bei", "mir", "seid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.34": {"text": "Wird mein Herz nicht ganz verzagen,", "tokens": ["Wird", "mein", "Herz", "nicht", "ganz", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Und solang' ihr tragen helft,", "tokens": ["Und", "so\u00b7lang'", "ihr", "tra\u00b7gen", "helft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Trag' ich muthig alle Plagen.", "tokens": ["Trag'", "ich", "mut\u00b7hig", "al\u00b7le", "Pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Drum vor allen Freundinnen,", "tokens": ["Drum", "vor", "al\u00b7len", "Freun\u00b7din\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "PIAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.38": {"text": "Ohne eine zu verklagen,", "tokens": ["Oh\u00b7ne", "ei\u00b7ne", "zu", "ver\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Weil sie sterblich, hab' ich Dank", "tokens": ["Weil", "sie", "sterb\u00b7lich", ",", "hab'", "ich", "Dank"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "$,", "VAFIN", "PPER", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "Euch, Unsterbliche, zu sagen.", "tokens": ["Euch", ",", "U\u00b7nsterb\u00b7li\u00b7che", ",", "zu", "sa\u00b7gen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}