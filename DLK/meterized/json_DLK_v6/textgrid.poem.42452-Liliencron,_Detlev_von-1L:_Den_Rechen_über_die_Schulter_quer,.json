{"textgrid.poem.42452": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Den Rechen \u00fcber die Schulter quer,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Rechen \u00fcber die Schulter quer,", "tokens": ["Den", "Re\u00b7chen", "\u00fc\u00b7ber", "die", "Schul\u00b7ter", "quer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wippwappt zum Heuen die Grete daher.", "tokens": ["Wipp\u00b7wappt", "zum", "Heu\u00b7en", "die", "Gre\u00b7te", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NE", "PAV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Was lacht sie doch bei jedem Schritt,", "tokens": ["Was", "lacht", "sie", "doch", "bei", "je\u00b7dem", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer baumelt an ihrem Rechen mit?", "tokens": ["Wer", "bau\u00b7melt", "an", "ih\u00b7rem", "Re\u00b7chen", "mit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "An den Zinken, an bunten B\u00e4ndern viel,", "tokens": ["An", "den", "Zin\u00b7ken", ",", "an", "bun\u00b7ten", "B\u00e4n\u00b7dern", "viel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ADJA", "NN", "ADV", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wer treibt da solch ein Kirmesspiel:", "tokens": ["Wer", "treibt", "da", "solch", "ein", "Kir\u00b7mes\u00b7spiel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein Kautschuckm\u00e4nnchen, ein Hampelmann,", "tokens": ["Ein", "Kaut\u00b7schuck\u00b7m\u00e4nn\u00b7chen", ",", "ein", "Ham\u00b7pel\u00b7mann", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der sich nicht entwirren und l\u00f6sen kann.", "tokens": ["Der", "sich", "nicht", "ent\u00b7wir\u00b7ren", "und", "l\u00f6\u00b7sen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PTKNEG", "VVINF", "KON", "VVINF", "VMFIN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Wie sehr er's anhebt, immer mehr erbost,", "tokens": ["Wie", "sehr", "er's", "an\u00b7hebt", ",", "im\u00b7mer", "mehr", "er\u00b7bost", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIS", "VVFIN", "$,", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er zerrt sich nicht aus den Schlingen los.", "tokens": ["Er", "zerrt", "sich", "nicht", "aus", "den", "Schlin\u00b7gen", "los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Vergeblich strampelt er, schilt er und schreit,", "tokens": ["Ver\u00b7geb\u00b7lich", "stram\u00b7pelt", "er", ",", "schilt", "er", "und", "schreit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "KON", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Grete hat ihn nicht befreit.", "tokens": ["Die", "Gre\u00b7te", "hat", "ihn", "nicht", "be\u00b7freit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Beim Himmel, das ist ja der kleine Schuft,", "tokens": ["Beim", "Him\u00b7mel", ",", "das", "ist", "ja", "der", "klei\u00b7ne", "Schuft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PDS", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nun bitt' ich, da rast er sich ab in der Luft.", "tokens": ["Nun", "bitt'", "ich", ",", "da", "rast", "er", "sich", "ab", "in", "der", "Luft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.8": {"line.1": {"text": "Was, hatte die Grete Liebesverdru\u00df,", "tokens": ["Was", ",", "hat\u00b7te", "die", "Gre\u00b7te", "Lie\u00b7bes\u00b7ver\u00b7dru\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da\u00df Amorl so j\u00e4mmerlich zappeln mu\u00df?", "tokens": ["Da\u00df", "A\u00b7morl", "so", "j\u00e4m\u00b7mer\u00b7lich", "zap\u00b7peln", "mu\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}