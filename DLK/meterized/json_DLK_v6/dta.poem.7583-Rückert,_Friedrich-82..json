{"dta.poem.7583": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "82.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es str\u00f6mt ein Quell aus Gott, und str\u00f6mt in Gott zur\u00fcck,", "tokens": ["Es", "str\u00f6mt", "ein", "Quell", "aus", "Gott", ",", "und", "str\u00f6mt", "in", "Gott", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,", "KON", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Einstrom hohe Lust, der Ausstrom h\u00f6chstes Gl\u00fcck.", "tokens": ["Der", "Ein\u00b7strom", "ho\u00b7he", "Lust", ",", "der", "Aus\u00b7strom", "h\u00f6chs\u00b7tes", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er str\u00f6met in dich ein durchs offne Thor der Sinnen,", "tokens": ["Er", "str\u00f6\u00b7met", "in", "dich", "ein", "durchs", "off\u00b7ne", "Thor", "der", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "ART", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und str\u00f6met aus dadurch, und nimmt dich mit von hinnen.", "tokens": ["Und", "str\u00f6\u00b7met", "aus", "da\u00b7durch", ",", "und", "nimmt", "dich", "mit", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PAV", "$,", "KON", "VVFIN", "PRF", "APPR", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Durchs Auge str\u00f6mt er ein als Licht, da\u00df er verkl\u00e4re", "tokens": ["Durchs", "Au\u00b7ge", "str\u00f6mt", "er", "ein", "als", "Licht", ",", "da\u00df", "er", "ver\u00b7kl\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "KOUS", "NN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Innres, und entstr\u00f6mt verkl\u00e4rt als Freudenz\u00e4hre.", "tokens": ["Dein", "Inn\u00b7res", ",", "und", "ent\u00b7str\u00f6mt", "ver\u00b7kl\u00e4rt", "als", "Freu\u00b7den\u00b7z\u00e4h\u00b7re", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "ADJD", "VVPP", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Den Geist zu wecken, str\u00f6mt er ein als Ton durchs Ohr,", "tokens": ["Den", "Geist", "zu", "we\u00b7cken", ",", "str\u00f6mt", "er", "ein", "als", "Ton", "durchs", "Ohr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "PPER", "ART", "KOUS", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und str\u00f6mt aus deinem Mund als Dankgebet hervor.", "tokens": ["Und", "str\u00f6mt", "aus", "dei\u00b7nem", "Mund", "als", "Dank\u00b7ge\u00b7bet", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "KOKOM", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Einstr\u00f6mt er dem Geruch als Lenzduft, Sehnsuchtshauch,", "tokens": ["Eins\u00b7tr\u00f6mt", "er", "dem", "Ge\u00b7ruch", "als", "Lenz\u00b7duft", ",", "Sehn\u00b7suchts\u00b7hauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KOUS", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und str\u00f6mt im Athem aus als Seufzeropferrauch.", "tokens": ["Und", "str\u00f6mt", "im", "A\u00b7them", "aus", "als", "Seuf\u00b7ze\u00b7rop\u00b7fer\u00b7rauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Er str\u00f6mt durch den Geschmack ins Mark und ins Gehirne,", "tokens": ["Er", "str\u00f6mt", "durch", "den", "Ge\u00b7schmack", "ins", "Mark", "und", "ins", "Ge\u00b7hir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und als Gedanke tritt er leuchtend aus der Stirne.", "tokens": ["Und", "als", "Ge\u00b7dan\u00b7ke", "tritt", "er", "leuch\u00b7tend", "aus", "der", "Stir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "VVFIN", "PPER", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Er str\u00f6mt als irdischer Empfindungen Gew\u00fchle", "tokens": ["Er", "str\u00f6mt", "als", "ir\u00b7di\u00b7scher", "Emp\u00b7fin\u00b7dun\u00b7gen", "Ge\u00b7w\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJA", "NN", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ins Herz, und aus der Brust als himmlische Gef\u00fchle.", "tokens": ["Ins", "Herz", ",", "und", "aus", "der", "Brust", "als", "himm\u00b7li\u00b7sche", "Ge\u00b7f\u00fch\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "APPR", "ART", "NN", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Du f\u00fchlest: Was du bist, ist er in dir, nicht du;", "tokens": ["Du", "f\u00fch\u00b7lest", ":", "Was", "du", "bist", ",", "ist", "er", "in", "dir", ",", "nicht", "du", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWS", "PPER", "VAFIN", "$,", "VAFIN", "PPER", "APPR", "PPER", "$,", "PTKNEG", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und str\u00f6mst in dem Gef\u00fchl dich deinem Urquell zu.", "tokens": ["Und", "str\u00f6mst", "in", "dem", "Ge\u00b7f\u00fchl", "dich", "dei\u00b7nem", "Ur\u00b7quell", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}