{"textgrid.poem.51103": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Durch B\u00e4ume dringt ein leiser Ton,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch B\u00e4ume dringt ein leiser Ton,", "tokens": ["Durch", "B\u00e4u\u00b7me", "dringt", "ein", "lei\u00b7ser", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Fluten h\u00f6rt man rauschen schon,", "tokens": ["Die", "Flu\u00b7ten", "h\u00f6rt", "man", "rau\u00b7schen", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da zieht er her die breite Bahn,", "tokens": ["Da", "zieht", "er", "her", "die", "brei\u00b7te", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein altes St\u00e4dtlein h\u00e4ngt daran", "tokens": ["Ein", "al\u00b7tes", "St\u00e4dt\u00b7lein", "h\u00e4ngt", "da\u00b7ran"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mit T\u00fcrmen, Linden, Burg und Tor,", "tokens": ["Mit", "T\u00fcr\u00b7men", ",", "Lin\u00b7den", ",", "Burg", "und", "Tor", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NE", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Rathaus, Markt und Kirchenchor;", "tokens": ["Mit", "Rat\u00b7haus", ",", "Markt", "und", "Kir\u00b7chen\u00b7chor", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So schwimmt denn auf dem gr\u00fcnen Rhein", "tokens": ["So", "schwimmt", "denn", "auf", "dem", "gr\u00fc\u00b7nen", "Rhein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der goldne Nachmittag herein.", "tokens": ["Der", "gold\u00b7ne", "Nach\u00b7mit\u00b7tag", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Im Erkerh\u00e4uschen den Dechant", "tokens": ["Im", "Er\u00b7ker\u00b7h\u00e4usc\u00b7hen", "den", "De\u00b7chant"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sieht man, den R\u00f6mer in der Hand,", "tokens": ["Sieht", "man", ",", "den", "R\u00f6\u00b7mer", "in", "der", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und \u00fcber ihm sehr stille steht", "tokens": ["Und", "\u00fc\u00b7ber", "ihm", "sehr", "stil\u00b7le", "steht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ADV", "ADJA", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das F\u00e4hnlein, da kein L\u00fcftchen geht.", "tokens": ["Das", "F\u00e4hn\u00b7lein", ",", "da", "kein", "L\u00fcft\u00b7chen", "geht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie still! Nur auf der Klosterau", "tokens": ["Wie", "still", "!", "Nur", "auf", "der", "Klos\u00b7te\u00b7rau"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$.", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Keift fernhin eine alte Frau;", "tokens": ["Keift", "fern\u00b7hin", "ei\u00b7ne", "al\u00b7te", "Frau", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im k\u00fchlen Schatten nebendran", "tokens": ["Im", "k\u00fch\u00b7len", "Schat\u00b7ten", "ne\u00b7ben\u00b7dran"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dumpf donnert's auf der Kegelbahn.", "tokens": ["Dumpf", "don\u00b7nert's", "auf", "der", "Ke\u00b7gel\u00b7bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}