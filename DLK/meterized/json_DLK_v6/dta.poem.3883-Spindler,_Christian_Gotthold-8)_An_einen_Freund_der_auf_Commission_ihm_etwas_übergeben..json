{"dta.poem.3883": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "8) An einen Freund/ der auf  Commission  \n ihm etwas \u00fcbergeben.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Hoher G\u00f6nner! deinen Brief hab ich richtig", "tokens": ["Ho\u00b7her", "G\u00f6n\u00b7ner", "!", "dei\u00b7nen", "Brief", "hab", "ich", "rich\u00b7tig"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPOSAT", "NN", "VAFIN", "PPER", "ADJD"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "\u00fcberkommen,", "tokens": ["\u00fc\u00b7ber\u00b7kom\u00b7men", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Herr ", "tokens": ["Herr"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "genommen;", "tokens": ["ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Er verspricht mit Ernst zu sorgen, und versichert", "tokens": ["Er", "ver\u00b7spricht", "mit", "Ernst", "zu", "sor\u00b7gen", ",", "und", "ver\u00b7si\u00b7chert"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "PTKZU", "VVINF", "$,", "KON", "VVFIN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "mich dabey,", "tokens": ["mich", "da\u00b7bey", ","], "token_info": ["word", "word", "punct"], "pos": ["PRF", "PAV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Da\u00df er dir allstets zu dienen so bereit als willig sey.", "tokens": ["Da\u00df", "er", "dir", "alls\u00b7tets", "zu", "die\u00b7nen", "so", "be\u00b7reit", "als", "wil\u00b7lig", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "PTKZU", "VVINF", "ADV", "ADJD", "KOKOM", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Ich, dein dir ergebner Knecht, scheu auch k\u00fcnfftig", "tokens": ["Ich", ",", "dein", "dir", "er\u00b7geb\u00b7ner", "Knecht", ",", "scheu", "auch", "k\u00fcnff\u00b7tig"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PPOSAT", "PPER", "ADJA", "NN", "$,", "ADJD", "ADV", "ADJD"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "keine M\u00fche,", "tokens": ["kei\u00b7ne", "M\u00fc\u00b7he", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Da\u00df durch meinen schlechten Flei\u00df deiner Sachen", "tokens": ["Da\u00df", "durch", "mei\u00b7nen", "schlech\u00b7ten", "Flei\u00df", "dei\u00b7ner", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "ADJA", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Fortgang bl\u00fche;", "tokens": ["Fort\u00b7gang", "bl\u00fc\u00b7he", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Ich will sorgen, lauffen, rennen, bis es uns zuletzt", "tokens": ["Ich", "will", "sor\u00b7gen", ",", "lauf\u00b7fen", ",", "ren\u00b7nen", ",", "bis", "es", "uns", "zu\u00b7letzt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,", "KOUS", "PPER", "PRF", "ADV"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "gelingt,", "tokens": ["ge\u00b7lingt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.14": {"text": "Und bis meine frohe Feder dir hiervon die Nach-", "tokens": ["Und", "bis", "mei\u00b7ne", "fro\u00b7he", "Fe\u00b7der", "dir", "hier\u00b7von", "die", "Nach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "PPER", "PAV", "ART", "TRUNC"], "meter": "--+-+-+-+-+-+", "measure": "anapaest.init"}, "line.15": {"text": "richt bringt.", "tokens": ["richt", "bringt", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Was Herr ", "tokens": ["Was", "Herr"], "token_info": ["word", "word"], "pos": ["PWS", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.17": {"text": "Bl\u00e4tter", "tokens": ["Bl\u00e4t\u00b7ter"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Des Virginischen Tobacks; so bald, bey nur bes-", "tokens": ["Des", "Vir\u00b7gi\u00b7ni\u00b7schen", "To\u00b7backs", ";", "so", "bald", ",", "bey", "nur", "bes"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ADV", "ADV", "$,", "APPR", "ADV", "TRUNC"], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.19": {"text": "sern Wetter,", "tokens": ["sern", "Wet\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.20": {"text": "Nur der erste Fuhrmann k\u00f6mmet, und zwar \u00fcber", "tokens": ["Nur", "der", "ers\u00b7te", "Fuhr\u00b7mann", "k\u00f6m\u00b7met", ",", "und", "zwar", "\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$,", "KON", "ADV", "APPR"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.21": {"text": "Scheibenberg.", "tokens": ["Schei\u00b7ben\u00b7berg", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+--", "measure": "dactylic.init"}, "line.22": {"text": "Er besorget deine Sachen so gut als sein eigen", "tokens": ["Er", "be\u00b7sor\u00b7get", "dei\u00b7ne", "Sa\u00b7chen", "so", "gut", "als", "sein", "ei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "KOKOM", "PPOSAT", "NN"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.23": {"text": "Werck.", "tokens": ["Werck", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.24": {"text": "Dieser treuen Freundschaffts-Regung stimmet auch", "tokens": ["Die\u00b7ser", "treu\u00b7en", "Freund\u00b7schaffts\u00b7Re\u00b7gung", "stim\u00b7met", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.25": {"text": "die meine bey;", "tokens": ["die", "mei\u00b7ne", "bey", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.26": {"text": "Glaube, bey Beschlu\u00df der Zeilen, da\u00df ich stets", "tokens": ["Glau\u00b7be", ",", "bey", "Be\u00b7schlu\u00df", "der", "Zei\u00b7len", ",", "da\u00df", "ich", "stets"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "NN", "ART", "NN", "$,", "KOUS", "PPER", "ADV"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.27": {"text": "dein Diener sey", "tokens": ["dein", "Die\u00b7ner", "sey"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}}}}}