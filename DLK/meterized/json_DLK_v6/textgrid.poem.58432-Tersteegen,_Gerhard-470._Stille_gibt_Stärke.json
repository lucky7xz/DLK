{"textgrid.poem.58432": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "470. Stille gibt St\u00e4rke", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein stiller Geist veracht't", "tokens": ["Ein", "stil\u00b7ler", "Geist", "ver\u00b7acht't"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Des Satans List und Macht,", "tokens": ["Des", "Sa\u00b7tans", "List", "und", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sein getroster Mut", "tokens": ["Und", "sein", "ge\u00b7tros\u00b7ter", "Mut"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Besieget Fleisch und Blut,", "tokens": ["Be\u00b7sie\u00b7get", "Fleisch", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Welt mit ihren Sachen", "tokens": ["Die", "Welt", "mit", "ih\u00b7ren", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Kann er gar leicht verlachen,", "tokens": ["Kann", "er", "gar", "leicht", "ver\u00b7la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Weil er in Gott vertrauet,", "tokens": ["Weil", "er", "in", "Gott", "ver\u00b7trau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den er im Glauben schauet.", "tokens": ["Den", "er", "im", "Glau\u00b7ben", "schau\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}