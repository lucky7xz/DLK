{"textgrid.poem.34615": {"metadata": {"author": {"name": "Neukirch, Benjamin", "birth": "N.A.", "death": "N.A."}, "title": "Ein anders", "genre": "verse", "period": "N.A.", "pub_year": 1697, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schaut/ b\u00fcrger! dieses ist der treue Danckelmann.", "tokens": ["Schaut", "/", "b\u00fcr\u00b7ger", "!", "die\u00b7ses", "ist", "der", "treu\u00b7e", "Dan\u00b7ckel\u00b7mann", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "ADJD", "$.", "PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Di\u00df bild zeigt sein gesicht und seine minen an;", "tokens": ["Di\u00df", "bild", "zeigt", "sein", "ge\u00b7sicht", "und", "sei\u00b7ne", "mi\u00b7nen", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "PPOSAT", "VVPP", "KON", "PPOSAT", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das wapen seine treu und unverdro\u00dfnes wachen.", "tokens": ["Das", "wa\u00b7pen", "sei\u00b7ne", "treu", "und", "un\u00b7ver\u00b7dro\u00df\u00b7nes", "wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "ADJD", "KON", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer aber mahlet uns den wunder-grossen geist?", "tokens": ["Wer", "a\u00b7ber", "mah\u00b7let", "uns", "den", "wun\u00b7der\u00b7gros\u00b7sen", "geist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das kanst du/ Friederich; dann du alleine weist/", "tokens": ["Das", "kanst", "du", "/", "Frie\u00b7de\u00b7rich", ";", "dann", "du", "al\u00b7lei\u00b7ne", "weist", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "$(", "NE", "$.", "ADV", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie man der tugend soll ihr wahres bildni\u00df machen.", "tokens": ["Wie", "man", "der", "tu\u00b7gend", "soll", "ihr", "wah\u00b7res", "bild\u00b7ni\u00df", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VMFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}