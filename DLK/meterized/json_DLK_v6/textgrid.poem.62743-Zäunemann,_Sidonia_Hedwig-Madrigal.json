{"textgrid.poem.62743": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Madrigal", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man steigt auf einen Berg,", "tokens": ["Man", "steigt", "auf", "ei\u00b7nen", "Berg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da man das Thal und Feld", "tokens": ["Da", "man", "das", "Thal", "und", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sch\u00f6n \u00fcbersehen kan,", "tokens": ["Sch\u00f6n", "\u00fc\u00b7ber\u00b7se\u00b7hen", "kan", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit Arbeit, M\u00fch und Schwei\u00df hinan.", "tokens": ["Mit", "Ar\u00b7beit", ",", "M\u00fch", "und", "Schwei\u00df", "hi\u00b7nan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So ist es mit dem Ehren-Gipfel auch bestellt:", "tokens": ["So", "ist", "es", "mit", "dem", "Eh\u00b7ren\u00b7Gip\u00b7fel", "auch", "be\u00b7stellt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Man kan es mit dem Ehren-Gipfel auch bestellt", "tokens": ["Man", "kan", "es", "mit", "dem", "Eh\u00b7ren\u00b7Gip\u00b7fel", "auch", "be\u00b7stellt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PPER", "APPR", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und M\u00fch denselbigen erlangen.", "tokens": ["Und", "M\u00fch", "den\u00b7sel\u00b7bi\u00b7gen", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}