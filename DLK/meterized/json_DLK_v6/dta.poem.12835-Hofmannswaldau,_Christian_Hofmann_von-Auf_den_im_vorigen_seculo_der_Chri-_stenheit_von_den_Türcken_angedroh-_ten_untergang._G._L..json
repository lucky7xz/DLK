{"dta.poem.12835": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf den im vorigen seculo der Chri-  \n stenheit von den T\u00fcrcken angedroh-  \n ten untergang.  \n G. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schlag muthig los, du flammen-reiche lufft!", "tokens": ["Schlag", "mut\u00b7hig", "los", ",", "du", "flam\u00b7men\u00b7rei\u00b7che", "lufft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wilst du der wuth verwegner hunde schonen?", "tokens": ["Wilst", "du", "der", "wuth", "ver\u00b7weg\u00b7ner", "hun\u00b7de", "scho\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schmei\u00df diesen schwarm in eine finstre klufft,", "tokens": ["Schmei\u00df", "die\u00b7sen", "schwarm", "in", "ei\u00b7ne", "finst\u00b7re", "klufft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo drachen-brut und basilisken wohnen.", "tokens": ["Wo", "dra\u00b7chen\u00b7brut", "und", "ba\u00b7si\u00b7lis\u00b7ken", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zerschmettre doch sein scharffes mord-gewehr,", "tokens": ["Zer\u00b7schmett\u00b7re", "doch", "sein", "scharf\u00b7fes", "mord\u00b7ge\u00b7wehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df von dem blut erschlagner Christen sch\u00e4umet!", "tokens": ["Da\u00df", "von", "dem", "blut", "er\u00b7schlag\u00b7ner", "Chris\u00b7ten", "sch\u00e4u\u00b7met", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Es ist ja sonst dem donner nicht zu schwer,", "tokens": ["Es", "ist", "ja", "sonst", "dem", "don\u00b7ner", "nicht", "zu", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df er die welt mit seinem br\u00fcllen z\u00e4umet.", "tokens": ["Da\u00df", "er", "die", "welt", "mit", "sei\u00b7nem", "br\u00fcl\u00b7len", "z\u00e4u\u00b7met", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Bewegt dich nicht ein banges angst-geschrey,", "tokens": ["Be\u00b7wegt", "dich", "nicht", "ein", "ban\u00b7ges", "angst\u00b7ge\u00b7schrey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Das unser mund mit schrecken von sich bl\u00e4set?", "tokens": ["Das", "un\u00b7ser", "mund", "mit", "schre\u00b7cken", "von", "sich", "bl\u00e4\u00b7set", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "APPR", "VVFIN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Indem der feind aus grimm und raferey", "tokens": ["In\u00b7dem", "der", "feind", "aus", "grimm", "und", "ra\u00b7fe\u00b7rey"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Das feste schwerd uns in die seite st\u00f6set.", "tokens": ["Das", "fes\u00b7te", "schwerd", "uns", "in", "die", "sei\u00b7te", "st\u00f6\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Ach siehe doch der waffen tollheit an!", "tokens": ["Ach", "sie\u00b7he", "doch", "der", "waf\u00b7fen", "toll\u00b7heit", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVIMP", "ADV", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Jhr kuhner stich ist wie ein bi\u00df der schlangen.", "tokens": ["Ihr", "kuh\u00b7ner", "stich", "ist", "wie", "ein", "bi\u00df", "der", "schlan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJD", "VAFIN", "KOKOM", "ART", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Was furien und tieger nicht gethan,", "tokens": ["Was", "fu\u00b7ri\u00b7en", "und", "tie\u00b7ger", "nicht", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "ADJD", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Das haben sie auf einen tag begangen.", "tokens": ["Das", "ha\u00b7ben", "sie", "auf", "ei\u00b7nen", "tag", "be\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Versluchtes bley! das der canonen krafft", "tokens": ["Ver\u00b7sluch\u00b7tes", "bley", "!", "das", "der", "ca\u00b7no\u00b7nen", "krafft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "ART", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Mit lichtem dampff bis an die wolcken spielet.", "tokens": ["Mit", "lich\u00b7tem", "dampff", "bis", "an", "die", "wol\u00b7cken", "spie\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Verdammtes heer! das in dem rothen safft", "tokens": ["Ver\u00b7damm\u00b7tes", "heer", "!", "das", "in", "dem", "ro\u00b7then", "safft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Von unserm blut die schwartzen br\u00fcste k\u00fchlet!", "tokens": ["Von", "un\u00b7serm", "blut", "die", "schwart\u00b7zen", "br\u00fcs\u00b7te", "k\u00fch\u00b7let", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Wer bl\u00e4set euch denn solchen frevel ein,", "tokens": ["Wer", "bl\u00e4\u00b7set", "euch", "denn", "sol\u00b7chen", "fre\u00b7vel", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Ein frommes volck mit feuer auszurotten?", "tokens": ["Ein", "from\u00b7mes", "volck", "mit", "feu\u00b7er", "aus\u00b7zu\u00b7rot\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Soll dieses denn die helden-probe seyn,", "tokens": ["Soll", "die\u00b7ses", "denn", "die", "hel\u00b7den\u00b7pro\u00b7be", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "KON", "ART", "ADJA", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Als ihr vermeynt den himmel zu verspotten?", "tokens": ["Als", "ihr", "ver\u00b7meynt", "den", "him\u00b7mel", "zu", "ver\u00b7spot\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Jtzt schweiget GOtt zu eurer bosheit still;", "tokens": ["Jtzt", "schwei\u00b7get", "Gott", "zu", "eu\u00b7rer", "bos\u00b7heit", "still", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Doch wird es wol einmal gewi\u00df geschehen.", "tokens": ["Doch", "wird", "es", "wol", "ein\u00b7mal", "ge\u00b7wi\u00df", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Denn m\u00fcsset ihr, es komme wie es will,", "tokens": ["Denn", "m\u00fcs\u00b7set", "ihr", ",", "es", "kom\u00b7me", "wie", "es", "will", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "$,", "PPER", "VVFIN", "KOKOM", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Des adlers macht in eurem lande sehen.", "tokens": ["Des", "ad\u00b7lers", "macht", "in", "eu\u00b7rem", "lan\u00b7de", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}