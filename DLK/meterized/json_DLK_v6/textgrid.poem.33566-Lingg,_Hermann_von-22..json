{"textgrid.poem.33566": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zerrisse je das Liebesband,", "tokens": ["Zer\u00b7ris\u00b7se", "je", "das", "Lie\u00b7bes\u00b7band", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das unsre Herzen h\u00e4lt verbunden,", "tokens": ["Das", "uns\u00b7re", "Her\u00b7zen", "h\u00e4lt", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann bleib' kein welkes Treuepfand", "tokens": ["Dann", "bleib'", "kein", "wel\u00b7kes", "Treu\u00b7e\u00b7pfand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als Trauerrest der sch\u00f6nen Stunden;", "tokens": ["Als", "Trau\u00b7er\u00b7rest", "der", "sch\u00f6\u00b7nen", "Stun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kein Katafalk, auf dem noch lang", "tokens": ["Kein", "Ka\u00b7ta\u00b7falk", ",", "auf", "dem", "noch", "lang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "APPR", "ART", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Schau l\u00e4g' unsre tote Liebe,", "tokens": ["Zur", "Schau", "l\u00e4g'", "uns\u00b7re", "to\u00b7te", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Angedenken, nicht ein Klang,", "tokens": ["Kein", "An\u00b7ge\u00b7den\u00b7ken", ",", "nicht", "ein", "Klang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An dem der Schmerz verewigt bliebe.", "tokens": ["An", "dem", "der", "Schmerz", "ve\u00b7re\u00b7wigt", "blie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was aus ist, sei der Nacht zum Raub,", "tokens": ["Was", "aus", "ist", ",", "sei", "der", "Nacht", "zum", "Raub", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "VAFIN", "$,", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vergessen sei es und versunken,", "tokens": ["Ver\u00b7ges\u00b7sen", "sei", "es", "und", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und \u00fcbrig bleibe nicht ein Staub,", "tokens": ["Und", "\u00fcb\u00b7rig", "blei\u00b7be", "nicht", "ein", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nicht ein Hauch, und nicht ein Funken!", "tokens": ["Und", "nicht", "ein", "Hauch", ",", "und", "nicht", "ein", "Fun\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "$,", "KON", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}