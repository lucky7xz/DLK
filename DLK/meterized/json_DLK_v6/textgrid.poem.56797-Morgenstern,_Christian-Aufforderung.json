{"textgrid.poem.56797": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Aufforderung", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stiller W\u00e4lder s\u00fc\u00dfen Frieden", "tokens": ["Stil\u00b7ler", "W\u00e4l\u00b7der", "s\u00fc\u00b7\u00dfen", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "la\u00dft uns suchen und genie\u00dfen!", "tokens": ["la\u00dft", "uns", "su\u00b7chen", "und", "ge\u00b7nie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "St\u00e4tten, heimlich, abgeschieden,", "tokens": ["St\u00e4t\u00b7ten", ",", "heim\u00b7lich", ",", "ab\u00b7ge\u00b7schie\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "m\u00f6gen uns der Welt verschlie\u00dfen!", "tokens": ["m\u00f6\u00b7gen", "uns", "der", "Welt", "ver\u00b7schlie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Seht ihr dort das braune Tierchen \u2013", "tokens": ["Seht", "ihr", "dort", "das", "brau\u00b7ne", "Tier\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "unsern kleinen N\u00fcsseknacker,", "tokens": ["un\u00b7sern", "klei\u00b7nen", "N\u00fcs\u00b7se\u00b7kna\u00b7cker", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "unser schelmisches Possierchen,", "tokens": ["un\u00b7ser", "schel\u00b7mi\u00b7sches", "Pos\u00b7sier\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "unsern blitzbehenden Racker?", "tokens": ["un\u00b7sern", "blitz\u00b7be\u00b7hen\u00b7den", "Ra\u00b7cker", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wirf uns nicht mit Bucheneckern,", "tokens": ["Wirf", "uns", "nicht", "mit", "Bu\u00b7chen\u00b7ec\u00b7kern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00e4tzchen, f\u00fchr uns leise Wege,", "tokens": ["K\u00e4tz\u00b7chen", ",", "f\u00fchr", "uns", "lei\u00b7se", "We\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "wo Gel\u00e4chter heimlich meckern,", "tokens": ["wo", "Ge\u00b7l\u00e4ch\u00b7ter", "heim\u00b7lich", "me\u00b7ckern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "kommen Menschen ins Gehege ...", "tokens": ["kom\u00b7men", "Men\u00b7schen", "ins", "Ge\u00b7he\u00b7ge", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPRART", "NN", "$("], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Nachtigallenchor dem Reigen", "tokens": ["Nach\u00b7ti\u00b7gal\u00b7len\u00b7chor", "dem", "Rei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "lichter Elben schl\u00e4gt und fl\u00f6tet,", "tokens": ["lich\u00b7ter", "El\u00b7ben", "schl\u00e4gt", "und", "fl\u00f6\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NE", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "bis der Mondnacht Silberschweigen", "tokens": ["bis", "der", "Mond\u00b7nacht", "Sil\u00b7ber\u00b7schwei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "erste Fr\u00fche \u00fcberr\u00f6tet ...", "tokens": ["ers\u00b7te", "Fr\u00fc\u00b7he", "\u00fc\u00b7ber\u00b7r\u00f6\u00b7tet", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wo auf gro\u00dfge\u00e4ugter Hinde", "tokens": ["Wo", "auf", "gro\u00df\u00b7ge\u00b7\u00e4ug\u00b7ter", "Hin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "lauscht die stumme Elbin Stille,", "tokens": ["lauscht", "die", "stum\u00b7me", "El\u00b7bin", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "wenn das Ungest\u00fcm der Winde", "tokens": ["wenn", "das", "Un\u00b7ge\u00b7st\u00fcm", "der", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "endlich zwang ihr flehnder Wille ...", "tokens": ["end\u00b7lich", "zwang", "ihr", "flehn\u00b7der", "Wil\u00b7le", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wo der Gnomen kluge V\u00f6lkchen", "tokens": ["Wo", "der", "Gno\u00b7men", "klu\u00b7ge", "V\u00f6lk\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "aus Irrfl\u00e4mmchen, Neumond-Tauen,", "tokens": ["aus", "Irr\u00b7fl\u00e4mm\u00b7chen", ",", "Neu\u00b7mon\u00b7dTau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Regenruch, Gewitterw\u00f6lkchen", "tokens": ["Re\u00b7gen\u00b7ruch", ",", "Ge\u00b7wit\u00b7ter\u00b7w\u00f6lk\u00b7chen"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "ihr geheimes Wissen brauen ...", "tokens": ["ihr", "ge\u00b7hei\u00b7mes", "Wis\u00b7sen", "brau\u00b7en", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Stiller W\u00e4lder s\u00fc\u00dfen Frieden", "tokens": ["Stil\u00b7ler", "W\u00e4l\u00b7der", "s\u00fc\u00b7\u00dfen", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "la\u00dft uns suchen und genie\u00dfen!", "tokens": ["la\u00dft", "uns", "su\u00b7chen", "und", "ge\u00b7nie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "St\u00e4tten, heimlich, abgeschieden,", "tokens": ["St\u00e4t\u00b7ten", ",", "heim\u00b7lich", ",", "ab\u00b7ge\u00b7schie\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "m\u00f6gen uns der Welt verschlie\u00dfen!", "tokens": ["m\u00f6\u00b7gen", "uns", "der", "Welt", "ver\u00b7schlie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}