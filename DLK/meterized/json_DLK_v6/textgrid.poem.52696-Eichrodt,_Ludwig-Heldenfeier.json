{"textgrid.poem.52696": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Heldenfeier", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Singet ihnen und bewundert", "tokens": ["Sin\u00b7get", "ih\u00b7nen", "und", "be\u00b7wun\u00b7dert"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die am Thermopylenpa\u00df", "tokens": ["Die", "am", "Ther\u00b7mo\u00b7py\u00b7len\u00b7pa\u00df"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruhmvoll fielen, die dreihundert,", "tokens": ["Ruhm\u00b7voll", "fie\u00b7len", ",", "die", "drei\u00b7hun\u00b7dert", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "PRELS", "CARD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sparter und Leonidas!", "tokens": ["Spar\u00b7ter", "und", "Leo\u00b7ni\u00b7das", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "M\u00e4nner, die in deutschen Landen", "tokens": ["M\u00e4n\u00b7ner", ",", "die", "in", "deut\u00b7schen", "Lan\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gleichen Tod und Nachruhm fanden,", "tokens": ["Glei\u00b7chen", "Tod", "und", "Nach\u00b7ruhm", "fan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Meine Leier nicht verga\u00df.", "tokens": ["Mei\u00b7ne", "Lei\u00b7er", "nicht", "ver\u00b7ga\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hunnen waren saubre Br\u00fcder,", "tokens": ["Hun\u00b7nen", "wa\u00b7ren", "saub\u00b7re", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00fcrmten \u00fcber Stock und Stein", "tokens": ["St\u00fcrm\u00b7ten", "\u00fc\u00b7ber", "Stock", "und", "Stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle V\u00f6lkersitze nieder,", "tokens": ["Al\u00b7le", "V\u00f6l\u00b7ker\u00b7sit\u00b7ze", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kamen auch zu uns, zum Rhein.", "tokens": ["Ka\u00b7men", "auch", "zu", "uns", ",", "zum", "Rhein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "$,", "APPRART", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sa\u00dfen dortlands die Burgunden,", "tokens": ["Sa\u00b7\u00dfen", "dort\u00b7lands", "die", "Bur\u00b7gun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "L\u00f6sten gern mit Todeswunden", "tokens": ["L\u00f6s\u00b7ten", "gern", "mit", "To\u00b7des\u00b7wun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ihre ewige Freiheit ein.", "tokens": ["Ih\u00b7re", "e\u00b7wi\u00b7ge", "Frei\u00b7heit", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Wie ein Fels den Flu\u00df in Arme", "tokens": ["Wie", "ein", "Fels", "den", "Flu\u00df", "in", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Theilt und seinen Vollstrom hemmt,", "tokens": ["Theilt", "und", "sei\u00b7nen", "Voll\u00b7strom", "hemmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Haben sie dem Hunnenschwarme", "tokens": ["Ha\u00b7ben", "sie", "dem", "Hun\u00b7nen\u00b7schwar\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keck entgegen sich gestemmt.", "tokens": ["Keck", "ent\u00b7ge\u00b7gen", "sich", "ge\u00b7stemmt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fels, von \u00e4chtem Schrot und Korne,", "tokens": ["Fels", ",", "von", "\u00e4ch\u00b7tem", "Schrot", "und", "Kor\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wurdest von dem Wellenzorne", "tokens": ["Wur\u00b7dest", "von", "dem", "Wel\u00b7len\u00b7zor\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Unzertr\u00fcmmert \u00fcberschwemmt!", "tokens": ["Un\u00b7zer\u00b7tr\u00fcm\u00b7mert", "\u00fc\u00b7bersc\u00b7hwemmt", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Preis euch Tapfern, euch Zehntausend,", "tokens": ["Preis", "euch", "Tap\u00b7fern", ",", "euch", "Zehn\u00b7tau\u00b7send", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$,", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eurem Stolze wetterk\u00fchn,", "tokens": ["Eu\u00b7rem", "Stol\u00b7ze", "wet\u00b7ter\u00b7k\u00fchn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der die Woge, welche brausend", "tokens": ["Der", "die", "Wo\u00b7ge", ",", "wel\u00b7che", "brau\u00b7send"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ART", "NN", "$,", "PRELS", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Euch verschlang, verschm\u00e4ht zu fliehn!", "tokens": ["Euch", "ver\u00b7schlang", ",", "ver\u00b7schm\u00e4ht", "zu", "fliehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVPP", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Opfertod im Schlachtget\u00f6se,", "tokens": ["Op\u00b7fer\u00b7tod", "im", "Schlacht\u00b7ge\u00b7t\u00f6\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Untergang der Heldengr\u00f6\u00dfe,", "tokens": ["Un\u00b7ter\u00b7gang", "der", "Hel\u00b7den\u00b7gr\u00f6\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nie soll dieser Ruhm verbl\u00fchn!", "tokens": ["Nie", "soll", "die\u00b7ser", "Ruhm", "ver\u00b7bl\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PDAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Bleibe drum auch unvergessen", "tokens": ["Blei\u00b7be", "drum", "auch", "un\u00b7ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PAV", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hochsinn einer sp\u00e4tern Zeit!", "tokens": ["Hoch\u00b7sinn", "ei\u00b7ner", "sp\u00e4\u00b7tern", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seid mit gleichem Ma\u00df gemessen,", "tokens": ["Seid", "mit", "glei\u00b7chem", "Ma\u00df", "ge\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wimpfens Helden, Leun im Streit!", "tokens": ["Wim\u00b7pfens", "Hel\u00b7den", ",", "Leun", "im", "Streit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus Kordovas Feuerschl\u00fcnden", "tokens": ["Aus", "Kor\u00b7do\u00b7vas", "Feu\u00b7er\u00b7schl\u00fcn\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NN"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Euren Tod und Ruhm verk\u00fcnden", "tokens": ["Eu\u00b7ren", "Tod", "und", "Ruhm", "ver\u00b7k\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "H\u00f6rt die spanische Tapferkeit.", "tokens": ["H\u00f6rt", "die", "spa\u00b7ni\u00b7sche", "Tap\u00b7fer\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Trommeln wirbeln, Pulverwagen", "tokens": ["Trom\u00b7meln", "wir\u00b7beln", ",", "Pul\u00b7ver\u00b7wa\u00b7gen"], "token_info": ["word", "word", "punct", "word"], "pos": ["NN", "VVFIN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Knallen auf, Verwirrung, Flucht,", "tokens": ["Knal\u00b7len", "auf", ",", "Ver\u00b7wir\u00b7rung", ",", "Flucht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Georg Friedrich war geschlagen,", "tokens": ["Ge\u00b7org", "Fried\u00b7rich", "war", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hat die letzte Kraft versucht.", "tokens": ["Hat", "die", "letz\u00b7te", "Kraft", "ver\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Pforzheims B\u00fcrger stehn vierhundert", "tokens": ["Pforz\u00b7heims", "B\u00fcr\u00b7ger", "stehn", "vier\u00b7hun\u00b7dert"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "VVFIN", "CARD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fest im Blut, vom Feind bewundert,", "tokens": ["Fest", "im", "Blut", ",", "vom", "Feind", "be\u00b7wun\u00b7dert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Halten aus die Reiterwucht.", "tokens": ["Hal\u00b7ten", "aus", "die", "Rei\u00b7ter\u00b7wucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Seid im Liede stets gefeiert", "tokens": ["Seid", "im", "Lie\u00b7de", "stets", "ge\u00b7fei\u00b7ert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAIMP", "APPRART", "NN", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihr vom \u00bbwei\u00dfen Regiment\u00ab!", "tokens": ["Ihr", "vom", "\u00bb", "wei\u00b7\u00dfen", "Re\u00b7gi\u00b7ment", "\u00ab", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "APPRART", "$(", "ADJA", "NN", "$(", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Solch Ged\u00e4chtni\u00df ist erneuert,", "tokens": ["Solch", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "ist", "er\u00b7neu\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo sich Treu am Muth erkennt.", "tokens": ["Wo", "sich", "Treu", "am", "Muth", "er\u00b7kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Euren F\u00fcrsten vor den Ketten,", "tokens": ["Eu\u00b7ren", "F\u00fcrs\u00b7ten", "vor", "den", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eurer Sache Ehr zu retten,", "tokens": ["Eu\u00b7rer", "Sa\u00b7che", "Ehr", "zu", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nahmet ihr ein r\u00fchmlich End.", "tokens": ["Nah\u00b7met", "ihr", "ein", "r\u00fchm\u00b7lich", "End", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Singet ihnen und bewundert", "tokens": ["Sin\u00b7get", "ih\u00b7nen", "und", "be\u00b7wun\u00b7dert"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die am Thermopylenpa\u00df", "tokens": ["Die", "am", "Ther\u00b7mo\u00b7py\u00b7len\u00b7pa\u00df"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruhmvoll fielen, die dreihundert,", "tokens": ["Ruhm\u00b7voll", "fie\u00b7len", ",", "die", "drei\u00b7hun\u00b7dert", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "PRELS", "CARD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sparter und Leonidas \u2013", "tokens": ["Spar\u00b7ter", "und", "Leo\u00b7ni\u00b7das", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "M\u00e4nner, die in deutschen Landen", "tokens": ["M\u00e4n\u00b7ner", ",", "die", "in", "deut\u00b7schen", "Lan\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gleichen Tod und Nachruhm fanden,", "tokens": ["Glei\u00b7chen", "Tod", "und", "Nach\u00b7ruhm", "fan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Meine Leier nicht verga\u00df!", "tokens": ["Mei\u00b7ne", "Lei\u00b7er", "nicht", "ver\u00b7ga\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}