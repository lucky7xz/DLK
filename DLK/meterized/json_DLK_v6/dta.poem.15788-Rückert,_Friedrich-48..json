{"dta.poem.15788": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "48.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist ein Sinnbild? Was der sch\u00f6ne Name meint:", "tokens": ["Was", "ist", "ein", "Sinn\u00b7bild", "?", "Was", "der", "sch\u00f6\u00b7ne", "Na\u00b7me", "meint", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "PWS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Sinn mit einem Bild aufs innigste vereint.", "tokens": ["Ein", "Sinn", "mit", "ei\u00b7nem", "Bild", "aufs", "in\u00b7nigs\u00b7te", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPRART", "ADJA", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein tiefer Sinn, der in ein sch\u00f6nes Bild sich senkt,", "tokens": ["Ein", "tie\u00b7fer", "Sinn", ",", "der", "in", "ein", "sch\u00f6\u00b7nes", "Bild", "sich", "senkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein sch\u00f6nes Bild, bei dem ein tiefer Sinn sich denkt.", "tokens": ["Ein", "sch\u00f6\u00b7nes", "Bild", ",", "bei", "dem", "ein", "tie\u00b7fer", "Sinn", "sich", "denkt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRELS", "ART", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6n sei das Bild und klar, tief sei der Sinn und wahr,", "tokens": ["Sch\u00f6n", "sei", "das", "Bild", "und", "klar", ",", "tief", "sei", "der", "Sinn", "und", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "KON", "ADJD", "$,", "ADJD", "VAFIN", "ART", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mit einander eins untrennbar sei das Paar.", "tokens": ["Und", "mit", "ein\u00b7an\u00b7der", "eins", "un\u00b7trenn\u00b7bar", "sei", "das", "Paar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRF", "PIS", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}