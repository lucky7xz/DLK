{"textgrid.poem.39361": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Eine holde s\u00fc\u00dfe Kranke", "genre": "verse", "period": "N.A.", "pub_year": 1805, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine holde s\u00fc\u00dfe Kranke", "tokens": ["Ei\u00b7ne", "hol\u00b7de", "s\u00fc\u00b7\u00dfe", "Kran\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist mein st\u00e4tiger Gedanke.", "tokens": ["Ist", "mein", "st\u00e4\u00b7ti\u00b7ger", "Ge\u00b7dan\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Milde Sonne, la\u00df dich schauen!", "tokens": ["Mil\u00b7de", "Son\u00b7ne", ",", "la\u00df", "dich", "schau\u00b7en", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Haltet inne, rauhe L\u00fcfte!", "tokens": ["Hal\u00b7tet", "in\u00b7ne", ",", "rau\u00b7he", "L\u00fcf\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gie\u00dfe deine Balsamd\u00fcfte,", "tokens": ["Gie\u00b7\u00dfe", "dei\u00b7ne", "Bal\u00b7sam\u00b7d\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fr\u00fchling, auf Toscanas Auen.", "tokens": ["Fr\u00fch\u00b7ling", ",", "auf", "Tos\u00b7ca\u00b7nas", "Au\u00b7en", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "NE", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gr\u00fcne Lauben will ich bauen,", "tokens": ["Gr\u00fc\u00b7ne", "Lau\u00b7ben", "will", "ich", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df sie nicht im Sturme wanke,", "tokens": ["Da\u00df", "sie", "nicht", "im", "Stur\u00b7me", "wan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Diese Blume, diese Kranke.", "tokens": ["Die\u00b7se", "Blu\u00b7me", ",", "die\u00b7se", "Kran\u00b7ke", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, es wechselt auf den Wangen", "tokens": ["Ach", ",", "es", "wech\u00b7selt", "auf", "den", "Wan\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liljenbl\u00e4\u00df' und Glut der Rosen.", "tokens": ["Lil\u00b7jen\u00b7bl\u00e4\u00df'", "und", "Glut", "der", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wag' ich's, ihnen liebzukosen,", "tokens": ["Wag'", "ich's", ",", "ih\u00b7nen", "lieb\u00b7zu\u00b7ko\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter Ahnden, unter Bangen?", "tokens": ["Un\u00b7ter", "Ahn\u00b7den", ",", "un\u00b7ter", "Ban\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Darf sich schmiegen mein Verlangen,", "tokens": ["Darf", "sich", "schmie\u00b7gen", "mein", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Innig, wie des Epheu's Ranke,", "tokens": ["In\u00b7nig", ",", "wie", "des", "E\u00b7pheu's", "Ran\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Um die lieblich bl\u00fch'nde Kranke?", "tokens": ["Um", "die", "lieb\u00b7lich", "bl\u00fch'n\u00b7de", "Kran\u00b7ke", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Deine Stimme fl\u00fcstert leise,", "tokens": ["Dei\u00b7ne", "Stim\u00b7me", "fl\u00fcs\u00b7tert", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dem Gesange sonst vertraut;", "tokens": ["Dem", "Ge\u00b7san\u00b7ge", "sonst", "ver\u00b7traut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber Eines Wortes Laut", "tokens": ["A\u00b7ber", "Ei\u00b7nes", "Wor\u00b7tes", "Laut"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Haucht sie doch in Fl\u00f6ten-Weise.", "tokens": ["Haucht", "sie", "doch", "in", "Fl\u00f6\u00b7ten\u00b7Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses Wort, des Kraft ich preise", "tokens": ["Die\u00b7ses", "Wort", ",", "des", "Kraft", "ich", "prei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "ART", "NN", "PPER", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Vor der G\u00f6tter Nektartranke,", "tokens": ["Vor", "der", "G\u00f6t\u00b7ter", "Nekt\u00b7ar\u00b7tran\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Liebe hei\u00dft es, liebe Kranke.", "tokens": ["Lie\u00b7be", "hei\u00dft", "es", ",", "lie\u00b7be", "Kran\u00b7ke", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aus der Wimpern Schatten strahlen", "tokens": ["Aus", "der", "Wim\u00b7pern", "Schat\u00b7ten", "strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df mir deine dunkeln Augen;", "tokens": ["La\u00df", "mir", "dei\u00b7ne", "dun\u00b7keln", "Au\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df von deinen Lippen saugen", "tokens": ["La\u00df", "von", "dei\u00b7nen", "Lip\u00b7pen", "sau\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bange Wonn' und s\u00fc\u00dfe Qualen.", "tokens": ["Ban\u00b7ge", "Wonn'", "und", "s\u00fc\u00b7\u00dfe", "Qua\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Einmal noch! zu tausendmalen!", "tokens": ["Ein\u00b7mal", "noch", "!", "zu", "tau\u00b7send\u00b7ma\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis ich bebe, bis ich schwanke,", "tokens": ["Bis", "ich", "be\u00b7be", ",", "bis", "ich", "schwan\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und im Sehnen selbst erkranke.", "tokens": ["Und", "im", "Seh\u00b7nen", "selbst", "er\u00b7kran\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Unbeklommen wird sich heben", "tokens": ["Un\u00b7be\u00b7klom\u00b7men", "wird", "sich", "he\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bald die liebevolle Brust,", "tokens": ["Bald", "die", "lie\u00b7be\u00b7vol\u00b7le", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchlt sie nur ein Theil der Lust,", "tokens": ["F\u00fchlt", "sie", "nur", "ein", "Theil", "der", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die sie kann in F\u00fclle geben.", "tokens": ["Die", "sie", "kann", "in", "F\u00fcl\u00b7le", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles Heil soll dich umschweben,", "tokens": ["Al\u00b7les", "Heil", "soll", "dich", "um\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der ich neues Dasein danke,", "tokens": ["Der", "ich", "neu\u00b7es", "Da\u00b7sein", "dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nina, meine holde Kranke!", "tokens": ["Ni\u00b7na", ",", "mei\u00b7ne", "hol\u00b7de", "Kran\u00b7ke", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}