{"textgrid.poem.46113": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die rosen, lieb, in deinem kranz", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die rosen, lieb, in deinem kranz", "tokens": ["Die", "ro\u00b7sen", ",", "lieb", ",", "in", "dei\u00b7nem", "kranz"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJD", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "seind rot, wie deiner lefzen glanz;", "tokens": ["seind", "rot", ",", "wie", "dei\u00b7ner", "lef\u00b7zen", "glanz", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "PWAV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die frischen gilgen sich vergleichen", "tokens": ["die", "fri\u00b7schen", "gil\u00b7gen", "sich", "ver\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "hie deiner zart und glatten hand,", "tokens": ["hie", "dei\u00b7ner", "zart", "und", "glat\u00b7ten", "hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und dan das g\u00fcldinklare band", "tokens": ["und", "dan", "das", "g\u00fcl\u00b7dink\u00b7la\u00b7re", "band"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "mu\u00df deines krausen haars gold weichen.", "tokens": ["mu\u00df", "dei\u00b7nes", "krau\u00b7sen", "haars", "gold", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wan nu, herzlieb, dir witz gnug w\u00e4r,", "tokens": ["Wan", "nu", ",", "herz\u00b7lieb", ",", "dir", "witz", "gnug", "w\u00e4r", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "VVFIN", "$,", "PPER", "ADJD", "ADV", "VAFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "zu merken deiner arbeit lehr,", "tokens": ["zu", "mer\u00b7ken", "dei\u00b7ner", "ar\u00b7beit", "lehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "w\u00fcrd deine zeit so nicht hingehen", "tokens": ["w\u00fcrd", "dei\u00b7ne", "zeit", "so", "nicht", "hin\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "aus jugend und unachtsamkeit,", "tokens": ["aus", "ju\u00b7gend", "und", "u\u00b7nacht\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "sondern mit mehr f\u00fcrsichtigkeit", "tokens": ["son\u00b7dern", "mit", "mehr", "f\u00fcr\u00b7sich\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "w\u00fcrd dein herz seinen nachtheil sehen.", "tokens": ["w\u00fcrd", "dein", "herz", "sei\u00b7nen", "nacht\u00b7heil", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der rosen gibt ein tag den gang,", "tokens": ["Der", "ro\u00b7sen", "gibt", "ein", "tag", "den", "gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "die gilgen bl\u00fchen auch nicht lang,", "tokens": ["die", "gil\u00b7gen", "bl\u00fc\u00b7hen", "auch", "nicht", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und deine blum ohn widerkehren", "tokens": ["und", "dei\u00b7ne", "blum", "ohn", "wi\u00b7der\u00b7keh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "veraltet und verwelket sich;", "tokens": ["ver\u00b7al\u00b7tet", "und", "ver\u00b7wel\u00b7ket", "sich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "so solt auch diser goldfad dich", "tokens": ["so", "solt", "auch", "di\u00b7ser", "gold\u00b7fad", "dich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "PDAT", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "alsbald dein br\u00fcchigs leben lehren.", "tokens": ["als\u00b7bald", "dein", "br\u00fc\u00b7chigs", "le\u00b7ben", "leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Warum dan bist du so feindlich?", "tokens": ["Wa\u00b7rum", "dan", "bist", "du", "so", "feind\u00b7lich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "warum redst du so unfreindlich?", "tokens": ["wa\u00b7rum", "redst", "du", "so", "un\u00b7freind\u00b7lich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "warum thust du mich stets betr\u00fcben?", "tokens": ["wa\u00b7rum", "thust", "du", "mich", "stets", "be\u00b7tr\u00fc\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "erbarmst du dich nicht \u00fcber mich,", "tokens": ["er\u00b7barmst", "du", "dich", "nicht", "\u00fc\u00b7ber", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "mein, so erbarm dich \u00fcber dich", "tokens": ["mein", ",", "so", "er\u00b7barm", "dich", "\u00fc\u00b7ber", "dich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "$,", "ADV", "ADJD", "PRF", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und la\u00df uns nu einander lieben!", "tokens": ["und", "la\u00df", "uns", "nu", "ein\u00b7an\u00b7der", "lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}