{"textgrid.poem.59346": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Im Abendrot", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir sind durch Not und Freude", "tokens": ["Wir", "sind", "durch", "Not", "und", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gegangen Hand in Hand,", "tokens": ["Ge\u00b7gan\u00b7gen", "Hand", "in", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom Wandern ruhn wir beide", "tokens": ["Vom", "Wan\u00b7dern", "ruhn", "wir", "bei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nun \u00fcberm stillen Land.", "tokens": ["Nun", "\u00fc\u00b7berm", "stil\u00b7len", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Rings sich die T\u00e4ler neigen,", "tokens": ["Rings", "sich", "die", "T\u00e4\u00b7ler", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es dunkelt schon die Luft,", "tokens": ["Es", "dun\u00b7kelt", "schon", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zwei Lerchen nur noch steigen", "tokens": ["Zwei", "Ler\u00b7chen", "nur", "noch", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nachtr\u00e4umend in den Duft.", "tokens": ["Nach\u00b7tr\u00e4u\u00b7mend", "in", "den", "Duft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Tritt her, und la\u00df sie schwirren,", "tokens": ["Tritt", "her", ",", "und", "la\u00df", "sie", "schwir\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KON", "VVIMP", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bald ist es Schlafenszeit,", "tokens": ["Bald", "ist", "es", "Schla\u00b7fens\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df wir uns nicht verirren", "tokens": ["Da\u00df", "wir", "uns", "nicht", "ver\u00b7ir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In dieser Einsamkeit.", "tokens": ["In", "die\u00b7ser", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O weiter, stiller Friede!", "tokens": ["O", "wei\u00b7ter", ",", "stil\u00b7ler", "Frie\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So tief im Abendrot", "tokens": ["So", "tief", "im", "A\u00b7ben\u00b7drot"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie sind wir wanderm\u00fcde \u2013", "tokens": ["Wie", "sind", "wir", "wan\u00b7der\u00b7m\u00fc\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist das etwa der Tod?", "tokens": ["Ist", "das", "et\u00b7wa", "der", "Tod", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}