{"textgrid.poem.49401": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "1L: Voran mein armes Herz, mein alter Kampfgenosse,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Voran mein armes Herz, mein alter Kampfgenosse,", "tokens": ["Vo\u00b7ran", "mein", "ar\u00b7mes", "Herz", ",", "mein", "al\u00b7ter", "Kampf\u00b7ge\u00b7nos\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Neu baue im Triumph dein buntes Siegestor,", "tokens": ["Neu", "bau\u00b7e", "im", "Tri\u00b7umph", "dein", "bun\u00b7tes", "Sie\u00b7ge\u00b7stor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPRART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Von falschem Goldaltar steig' Weihrauchduft empor,", "tokens": ["Von", "fal\u00b7schem", "Gold\u00b7al\u00b7tar", "steig'", "Weih\u00b7rauch\u00b7duft", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gib, dass an Abgrundshang der Flor der Blumen sprosse,", "tokens": ["Gib", ",", "dass", "an", "Ab\u00b7grunds\u00b7hang", "der", "Flor", "der", "Blu\u00b7men", "spros\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "APPR", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Voran mein armes Herz, mein alter Kampfgenosse.", "tokens": ["Vo\u00b7ran", "mein", "ar\u00b7mes", "Herz", ",", "mein", "al\u00b7ter", "Kampf\u00b7ge\u00b7nos\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zu Gott hin dringe deines Lieds verj\u00fcngter Klang,", "tokens": ["Zu", "Gott", "hin", "drin\u00b7ge", "dei\u00b7nes", "Lieds", "ver\u00b7j\u00fcng\u00b7ter", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lass, heis're Orgel, das Tedeum m\u00e4chtig t\u00f6nen,", "tokens": ["Lass", ",", "heis'\u00b7re", "Or\u00b7gel", ",", "das", "Te\u00b7de\u00b7um", "m\u00e4ch\u00b7tig", "t\u00f6\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Die fr\u00fchen Runzeln schmink', dein Antlitz zu versch\u00f6nen,", "tokens": ["Die", "fr\u00fc\u00b7hen", "Run\u00b7zeln", "schmink'", ",", "dein", "Ant\u00b7litz", "zu", "ver\u00b7sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "H\u00e4ng' rote Teppiche die morsche Wand entlang,", "tokens": ["H\u00e4ng'", "ro\u00b7te", "Tep\u00b7pi\u00b7che", "die", "mor\u00b7sche", "Wand", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "ADJA", "NN", "APPO", "$,"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "Zu Gott hin dringe deines Lieds verj\u00fcngter Klang.", "tokens": ["Zu", "Gott", "hin", "drin\u00b7ge", "dei\u00b7nes", "Lieds", "ver\u00b7j\u00fcng\u00b7ter", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Klingt Schellen, l\u00e4utet Gl\u00f6ckchen, t\u00f6net Glocken!", "tokens": ["Klingt", "Schel\u00b7len", ",", "l\u00e4u\u00b7tet", "Gl\u00f6ck\u00b7chen", ",", "t\u00f6\u00b7net", "Glo\u00b7cken", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "NN", "$,", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein weltentr\u00fcckter Traum ward Wahrheit, es umschlingt", "tokens": ["Mein", "wel\u00b7ten\u00b7tr\u00fcck\u00b7ter", "Traum", "ward", "Wahr\u00b7heit", ",", "es", "um\u00b7schlingt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein froher Arm das Gl\u00fcck, den Fremdling, leicht beschwingt,", "tokens": ["Mein", "fro\u00b7her", "Arm", "das", "Gl\u00fcck", ",", "den", "Fremd\u00b7ling", ",", "leicht", "be\u00b7schwingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "$,", "ART", "NN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der sch\u00fcchtern flieht beim Nah'n der Menschen, die ihn locken.", "tokens": ["Der", "sch\u00fcch\u00b7tern", "flieht", "beim", "Nah'n", "der", "Men\u00b7schen", ",", "die", "ihn", "lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPRART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Klingt Schellen, l\u00e4utet Gl\u00f6ckchen, t\u00f6net Glocken!", "tokens": ["Klingt", "Schel\u00b7len", ",", "l\u00e4u\u00b7tet", "Gl\u00f6ck\u00b7chen", ",", "t\u00f6\u00b7net", "Glo\u00b7cken", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "NN", "$,", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Lebendig Seit' an Seite ging mit mir das Gl\u00fcck;", "tokens": ["Le\u00b7ben\u00b7dig", "Seit'", "an", "Sei\u00b7te", "ging", "mit", "mir", "das", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "NN", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Das Leben, mitleidlos, entschreitet ohne S\u00e4umen,", "tokens": ["Das", "Le\u00b7ben", ",", "mit\u00b7leid\u00b7los", ",", "ent\u00b7schrei\u00b7tet", "oh\u00b7ne", "S\u00e4u\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+++-+-+-+-", "measure": "unknown.measure.septa"}, "line.3": {"text": "Der Wurm ist in der Frucht, Erwachen ist im Tr\u00e4umen,", "tokens": ["Der", "Wurm", "ist", "in", "der", "Frucht", ",", "Er\u00b7wa\u00b7chen", "ist", "im", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "$,", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Reu' ist in der Lieb', so zwingt uns das Geschick.", "tokens": ["Die", "Reu'", "ist", "in", "der", "Lieb'", ",", "so", "zwingt", "uns", "das", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Lebendig Seit' an Seite ging mit mir das Gl\u00fcck.", "tokens": ["Le\u00b7ben\u00b7dig", "Seit'", "an", "Sei\u00b7te", "ging", "mit", "mir", "das", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "NN", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}}}}