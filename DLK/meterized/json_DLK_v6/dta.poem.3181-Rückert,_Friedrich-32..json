{"dta.poem.3181": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "32.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Eine, das du liebst, wird dir vom Tod entzogen,", "tokens": ["Das", "Ei\u00b7ne", ",", "das", "du", "liebst", ",", "wird", "dir", "vom", "Tod", "ent\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und um das Andre hat die Ferne dich betrogen.", "tokens": ["Und", "um", "das", "And\u00b7re", "hat", "die", "Fer\u00b7ne", "dich", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "PIS", "VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein Drittes lebt, und ist dir nah, und doch getrennt;", "tokens": ["Ein", "Drit\u00b7tes", "lebt", ",", "und", "ist", "dir", "nah", ",", "und", "doch", "ge\u00b7trennt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$,", "KON", "VAFIN", "PPER", "ADJD", "$,", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das ist die Trennung, die ein Herz am meisten brennt.", "tokens": ["Das", "ist", "die", "Tren\u00b7nung", ",", "die", "ein", "Herz", "am", "meis\u00b7ten", "brennt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "w