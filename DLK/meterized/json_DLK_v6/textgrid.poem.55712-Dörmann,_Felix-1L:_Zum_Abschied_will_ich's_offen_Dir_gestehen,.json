{"textgrid.poem.55712": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zum Abschied will ich's offen Dir gestehen,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum Abschied will ich's offen Dir gestehen,", "tokens": ["Zum", "Ab\u00b7schied", "will", "ich's", "of\u00b7fen", "Dir", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PIS", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein helles Lachen machte mich nerv\u00f6s;", "tokens": ["Dein", "hel\u00b7les", "La\u00b7chen", "mach\u00b7te", "mich", "ner\u00b7v\u00f6s", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du wei\u00dft, ich bin bizarr \u2013 vielleicht auch b\u00f6s,", "tokens": ["Du", "wei\u00dft", ",", "ich", "bin", "bi\u00b7zarr", "\u2013", "viel\u00b7leicht", "auch", "b\u00f6s", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADJD", "$(", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Denn manchmal ", "tokens": ["Denn", "manch\u00b7mal"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Und wenn auch grudgescheite M\u00e4nner meinen,", "tokens": ["Und", "wenn", "auch", "grud\u00b7ge\u00b7schei\u00b7te", "M\u00e4n\u00b7ner", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der gr\u00f6\u00dfte ", "tokens": ["Der", "gr\u00f6\u00df\u00b7te"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "So sag' ich trotzdem keck: Von Zeit zu Zeit", "tokens": ["So", "sag'", "ich", "trotz\u00b7dem", "keck", ":", "Von", "Zeit", "zu", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "PTKVZ", "$.", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verleiht pikanten Reiz ein bi\u00dfchen Weinen.", "tokens": ["Ver\u00b7leiht", "pi\u00b7kan\u00b7ten", "Reiz", "ein", "bi\u00df\u00b7chen", "Wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}