{"dta.poem.10393": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XiX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Ja, du bist elend, und ich grolle nicht;", "tokens": ["Ja", ",", "du", "bist", "e\u00b7lend", ",", "und", "ich", "grol\u00b7le", "nicht", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ADJD", "$,", "KON", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein Lieb, wir sollen beide elend seyn!", "tokens": ["Mein", "Lieb", ",", "wir", "sol\u00b7len", "bei\u00b7de", "e\u00b7lend", "seyn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VMFIN", "PIS", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bis uns der Tod das kranke Herze bricht,", "tokens": ["Bis", "uns", "der", "Tod", "das", "kran\u00b7ke", "Her\u00b7ze", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mein Lieb, wir sollen beide elend seyn.", "tokens": ["Mein", "Lieb", ",", "wir", "sol\u00b7len", "bei\u00b7de", "e\u00b7lend", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VMFIN", "PIS", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wohl seh ich Spott, der deinen Mund umschwebt,", "tokens": ["Wohl", "seh", "ich", "Spott", ",", "der", "dei\u00b7nen", "Mund", "um\u00b7schwebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und seh dein Auge blitzen trotziglich,", "tokens": ["Und", "seh", "dein", "Au\u00b7ge", "blit\u00b7zen", "trot\u00b7zig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und seh den Stolz, der deinen Busen hebt, \u2014", "tokens": ["Und", "seh", "den", "Stolz", ",", "der", "dei\u00b7nen", "Bu\u00b7sen", "hebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und elend bist du doch, elend wie ich.", "tokens": ["Und", "e\u00b7lend", "bist", "du", "doch", ",", "e\u00b7lend", "wie", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "ADV", "$,", "ADJD", "KOKOM", "PPER", "$."], "meter": "-+-+--+-++", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Unsichtbar zuckt auch Schmerz um deinen Mund,", "tokens": ["Un\u00b7sicht\u00b7bar", "zuckt", "auch", "Schmerz", "um", "dei\u00b7nen", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.2": {"text": "Verborgne Thr\u00e4ne tr\u00fcbt des Auges Schein,", "tokens": ["Ver\u00b7borg\u00b7ne", "Thr\u00e4\u00b7ne", "tr\u00fcbt", "des", "Au\u00b7ges", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der stolze Busen hegt geheime Wund', \u2014", "tokens": ["Der", "stol\u00b7ze", "Bu\u00b7sen", "hegt", "ge\u00b7hei\u00b7me", "Wund'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mein Lieb, wir sollen beide elend seyn.", "tokens": ["Mein", "Lieb", ",", "wir", "sol\u00b7len", "bei\u00b7de", "e\u00b7lend", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VMFIN", "PIS", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}