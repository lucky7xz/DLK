{"textgrid.poem.38960": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach du rother Sonnenschimmer,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach du rother Sonnenschimmer,", "tokens": ["Ach", "du", "ro\u00b7ther", "Son\u00b7nen\u00b7schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach wann kommst du k\u00fchler Abend?", "tokens": ["Ach", "wann", "kommst", "du", "k\u00fch\u00b7ler", "A\u00b7bend", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWAV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wehen deine milden L\u00fcfte", "tokens": ["We\u00b7hen", "dei\u00b7ne", "mil\u00b7den", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So wie gestern auf mich labend?", "tokens": ["So", "wie", "ge\u00b7stern", "auf", "mich", "la\u00b7bend", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Als ein s\u00fc\u00dfes Baumgefl\u00fc\u00dfter", "tokens": ["Als", "ein", "s\u00fc\u00b7\u00dfes", "Baum\u00b7ge\u00b7fl\u00fc\u00df\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ein Duft von Blumen wallte,", "tokens": ["Und", "ein", "Duft", "von", "Blu\u00b7men", "wall\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der ferne Strom wie Musik,", "tokens": ["Und", "der", "fer\u00b7ne", "Strom", "wie", "Mu\u00b7sik", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "KOKOM", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Und die Wogen wie die Harfen,", "tokens": ["Und", "die", "Wo\u00b7gen", "wie", "die", "Har\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und dazwischen seine Worte", "tokens": ["Und", "da\u00b7zwi\u00b7schen", "sei\u00b7ne", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PAV", "PPOSAT", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.10": {"text": "Paradiesisch hold erklangen;", "tokens": ["Pa\u00b7ra\u00b7die\u00b7sisch", "hold", "er\u00b7klan\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und ein Streben und Beleben,", "tokens": ["Und", "ein", "Stre\u00b7ben", "und", "Be\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und Verlangen und Ermatten,", "tokens": ["Und", "Ver\u00b7lan\u00b7gen", "und", "Er\u00b7mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "In dem sch\u00f6nsten Freudentaumel", "tokens": ["In", "dem", "sch\u00f6ns\u00b7ten", "Freu\u00b7den\u00b7tau\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Hinzugeben sich, entbrannte,", "tokens": ["Hin\u00b7zu\u00b7ge\u00b7ben", "sich", ",", "ent\u00b7brann\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "VVFIN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.15": {"text": "Da\u00df er nur die volle Liebe", "tokens": ["Da\u00df", "er", "nur", "die", "vol\u00b7le", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Die ihm lebt' und starb, erkannte.", "tokens": ["Die", "ihm", "lebt'", "und", "starb", ",", "er\u00b7kann\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Aus der lieben dunkeln Ferne", "tokens": ["Aus", "der", "lie\u00b7ben", "dun\u00b7keln", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Klagten laut die Nachtigallen,", "tokens": ["Klag\u00b7ten", "laut", "die", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Die die labend k\u00fchlen T\u00f6ne", "tokens": ["Die", "die", "la\u00b7bend", "k\u00fch\u00b7len", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "In den Abendschimmer sandten,", "tokens": ["In", "den", "A\u00b7bend\u00b7schim\u00b7mer", "sand\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Wie die T\u00f6ne kamen, zogen,", "tokens": ["Wie", "die", "T\u00f6\u00b7ne", "ka\u00b7men", ",", "zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Und in ihnen Sehnsucht hallte,", "tokens": ["Und", "in", "ih\u00b7nen", "Sehn\u00b7sucht", "hall\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Waren sie wie dunkle Grotten,", "tokens": ["Wa\u00b7ren", "sie", "wie", "dunk\u00b7le", "Grot\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KOKOM", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Mit den Schatten, mit den kalten,", "tokens": ["Mit", "den", "Schat\u00b7ten", ",", "mit", "den", "kal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Und die Seele, die so br\u00fcnstig,", "tokens": ["Und", "die", "See\u00b7le", ",", "die", "so", "br\u00fcns\u00b7tig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Die so liebend, die so bange,", "tokens": ["Die", "so", "lie\u00b7bend", ",", "die", "so", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Wohnte wie in sichrer K\u00fchle,", "tokens": ["Wohn\u00b7te", "wie", "in", "sich\u00b7rer", "K\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Ruhte wie in mildem Schatten:", "tokens": ["Ruh\u00b7te", "wie", "in", "mil\u00b7dem", "Schat\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Wie ein Zelt von Lebensbalsam", "tokens": ["Wie", "ein", "Zelt", "von", "Le\u00b7bens\u00b7bal\u00b7sam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "War es um uns her geschlagen,", "tokens": ["War", "es", "um", "uns", "her", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PRF", "APZR", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Und wir hielten inn'ger, lieber,", "tokens": ["Und", "wir", "hiel\u00b7ten", "inn'\u00b7ger", ",", "lie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "$,", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Schmachtender uns noch umfangen. \u2013", "tokens": ["Schmach\u00b7ten\u00b7der", "uns", "noch", "um\u00b7fan\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Ach, und wie entfremdet ist mir", "tokens": ["Ach", ",", "und", "wie", "ent\u00b7frem\u00b7det", "ist", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KON", "PWAV", "VVPP", "VAFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Alles, da entfernt mein Gatte,", "tokens": ["Al\u00b7les", ",", "da", "ent\u00b7fernt", "mein", "Gat\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Ungetreu ist Wasser, Blume,", "tokens": ["Un\u00b7ge\u00b7treu", "ist", "Was\u00b7ser", ",", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VAFIN", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "V\u00f6gel, die noch gestern sangen,", "tokens": ["V\u00f6\u00b7gel", ",", "die", "noch", "ge\u00b7stern", "san\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Und im innern Herzen Geister,", "tokens": ["Und", "im", "in\u00b7nern", "Her\u00b7zen", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.38": {"text": "Die so muthig Fl\u00fcgel schwangen. \u2013", "tokens": ["Die", "so", "mut\u00b7hig", "Fl\u00fc\u00b7gel", "schwan\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "ADJD", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Wirst du mir nicht wiederkehren?", "tokens": ["Wirst", "du", "mir", "nicht", "wie\u00b7der\u00b7keh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Wozu dieses Zittern, Bangen?", "tokens": ["Wo\u00b7zu", "die\u00b7ses", "Zit\u00b7tern", ",", "Ban\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Ja, dann sterb' ich freudig gerne,", "tokens": ["Ja", ",", "dann", "sterb'", "ich", "freu\u00b7dig", "ger\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "PPER", "ADJD", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Denn das H\u00f6chste, Einz'ge, Alles,", "tokens": ["Denn", "das", "H\u00f6chs\u00b7te", ",", "Einz'\u00b7ge", ",", "Al\u00b7les", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "NN", "$,", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Was das Leben, was die Erde,", "tokens": ["Was", "das", "Le\u00b7ben", ",", "was", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "$,", "PRELS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Was der Gottheit volle Gaben", "tokens": ["Was", "der", "Got\u00b7theit", "vol\u00b7le", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Je gew\u00e4hren, seine Liebe", "tokens": ["Je", "ge\u00b7w\u00e4h\u00b7ren", ",", "sei\u00b7ne", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVINF", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Ward mir und ich konnte sagen,", "tokens": ["Ward", "mir", "und", "ich", "konn\u00b7te", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KON", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Wie ich ihn geliebt; Erw\u00fcnscht, Tod,", "tokens": ["Wie", "ich", "ihn", "ge\u00b7liebt", ";", "Er\u00b7w\u00fcnscht", ",", "Tod", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVPP", "$.", "NN", "$,", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.48": {"text": "Wann wir beide also starben.", "tokens": ["Wann", "wir", "bei\u00b7de", "al\u00b7so", "star\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIS", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}