{"dta.poem.4062": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Dich nur, Dich erh\u00e4lt Dein Licht, o Heldin! im", "tokens": ["Dich", "nur", ",", "Dich", "er\u00b7h\u00e4lt", "Dein", "Licht", ",", "o", "Hel\u00b7din", "!", "im"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "ADV", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "FM", "NN", "$.", "APPRART"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Lichte,", "tokens": ["Lich\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und Dein Dulden erh\u00e4lt liebend, o G\u00fctige!", "tokens": ["Und", "Dein", "Dul\u00b7den", "er\u00b7h\u00e4lt", "lie\u00b7bend", ",", "o", "G\u00fc\u00b7ti\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD", "$,", "FM", "NN", "$."], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Dich;", "tokens": ["Dich", ";"], "token_info": ["word", "punct"], "pos": ["PPER", "$."], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Und nicht einmal bist Du allein, Gespielen genug", "tokens": ["Und", "nicht", "ein\u00b7mal", "bist", "Du", "al\u00b7lein", ",", "Ge\u00b7spie\u00b7len", "ge\u00b7nug"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PTKNEG", "ADV", "VAFIN", "PPER", "ADV", "$,", "NN", "ADV"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.6": {"text": "sind,", "tokens": ["sind", ","], "token_info": ["word", "punct"], "pos": ["VAFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Wo bl\u00fchest und ruhst unter den Rosen des", "tokens": ["Wo", "bl\u00fc\u00b7hest", "und", "ruhst", "un\u00b7ter", "den", "Ro\u00b7sen", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN", "ART"], "meter": "-+--++--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Jahrs;", "tokens": ["Jahrs", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Und der Vater, er selbst, durch sanft muthath-", "tokens": ["Und", "der", "Va\u00b7ter", ",", "er", "selbst", ",", "durch", "sanft", "muthat\u00b7h"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PPER", "ADV", "$,", "APPR", "ADJD", "TRUNC"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "mende Musen", "tokens": ["men\u00b7de", "Mu\u00b7sen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.11": {"text": "Sendet die z\u00e4rtlichen Wiegenges\u00e4nge Dir zu.", "tokens": ["Sen\u00b7det", "die", "z\u00e4rt\u00b7li\u00b7chen", "Wie\u00b7gen\u00b7ge\u00b7s\u00e4n\u00b7ge", "Dir", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--+--+---", "measure": "dactylic.tetra.plus"}, "line.12": {"text": "Ja! noch ist sie es ganz! noch schwebt vom Haupte", "tokens": ["Ja", "!", "noch", "ist", "sie", "es", "ganz", "!", "noch", "schwebt", "vom", "Haup\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "ADV", "VAFIN", "PPER", "PPER", "ADV", "$.", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "zur Sohle,", "tokens": ["zur", "Soh\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Still herwandelnd, wie sonst, mir die Athene-", "tokens": ["Still", "her\u00b7wan\u00b7delnd", ",", "wie", "sonst", ",", "mir", "die", "A\u00b7the\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVPP", "$,", "PWAV", "ADV", "$,", "PPER", "ART", "TRUNC"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.15": {"text": "rin vor.", "tokens": ["rin", "vor", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.16": {"text": "Und wie, freundlicher Geist! von heitersinnender", "tokens": ["Und", "wie", ",", "freund\u00b7li\u00b7cher", "Geist", "!", "von", "hei\u00b7ter\u00b7sin\u00b7nen\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWAV", "$,", "ADJA", "NN", "$.", "APPR", "ADJA"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.17": {"text": "Stirne", "tokens": ["Stir\u00b7ne"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Segnend und sicher Dein Stral unter die Sterb-", "tokens": ["Seg\u00b7nend", "und", "si\u00b7cher", "Dein", "Stral", "un\u00b7ter", "die", "Ster\u00b7b"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "PPOSAT", "NN", "APPR", "ART", "TRUNC"], "meter": "+--+--++--+-", "measure": "dactylic.di.plus"}, "line.19": {"text": "lichen f\u00e4llt,", "tokens": ["li\u00b7chen", "f\u00e4llt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "VVFIN", "$,"], "meter": "--+", "measure": "anapaest.init"}, "line.20": {"text": "So bezeugest Du mir's, und sagst mir's, da\u00df ich", "tokens": ["So", "be\u00b7zeu\u00b7gest", "Du", "mir's", ",", "und", "sagst", "mir's", ",", "da\u00df", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NE", "$,", "KON", "VVFIN", "NE", "$,", "KOUS", "PPER"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.21": {"text": "es Andern", "tokens": ["es", "An\u00b7dern"], "token_info": ["word", "word"], "pos": ["PPER", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.22": {"text": "Wiedersage, denn auch Andere glauben es nicht,", "tokens": ["Wie\u00b7der\u00b7sa\u00b7ge", ",", "denn", "auch", "An\u00b7de\u00b7re", "glau\u00b7ben", "es", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADV", "PIS", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "----+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Da\u00df unsterblicher doch, denn Sorg' und Z\u00fcrnen,", "tokens": ["Da\u00df", "uns\u00b7terb\u00b7li\u00b7cher", "doch", ",", "denn", "Sor\u00b7g'", "und", "Z\u00fcr\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.24": {"text": "die Freude", "tokens": ["die", "Freu\u00b7de"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.25": {"text": "Und ein goldener Tag t\u00e4glich am Ende noch ist.", "tokens": ["Und", "ein", "gol\u00b7de\u00b7ner", "Tag", "t\u00e4g\u00b7lich", "am", "En\u00b7de", "noch", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADJD", "APPRART", "NN", "ADV", "VAFIN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}}}}}