{"dta.poem.17166": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "17.  Zu dem HErren  JESU .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Ich nah mich HERR zu dir als meinen Sonneschein/", "tokens": ["Ich", "nah", "mich", "HeRR", "zu", "dir", "als", "mei\u00b7nen", "Son\u00b7ne\u00b7schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "APPR", "PPER", "KOUS", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der mich erleucht/ erw\u00e4rmt/ und macht lebendig seyn.", "tokens": ["Der", "mich", "er\u00b7leucht", "/", "er\u00b7w\u00e4rmt", "/", "und", "macht", "le\u00b7ben\u00b7dig", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$(", "VVPP", "$(", "KON", "VVFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nahstu dich wiederumb zu mir als deiner Erden/", "tokens": ["Nahs\u00b7tu", "dich", "wie\u00b7de\u00b7rumb", "zu", "mir", "als", "dei\u00b7ner", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "PPER", "KOUS", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wird mein Hertze bald zum sch\u00f6nsten Fr\u00fcling werden.", "tokens": ["So", "wird", "mein", "Hert\u00b7ze", "bald", "zum", "sch\u00f6ns\u00b7ten", "Fr\u00fc\u00b7ling", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}