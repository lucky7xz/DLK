{"textgrid.poem.46316": {"metadata": {"author": {"name": "Spee, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[noth, angst vnd schmertzen vngestumb]", "genre": "verse", "period": "N.A.", "pub_year": 1613, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noth, angst vnd schmertzen vngestumb,", "tokens": ["Noth", ",", "angst", "vnd", "schmert\u00b7zen", "vn\u00b7ge\u00b7stumb", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu mir starck einher dringen:", "tokens": ["Zu", "mir", "starck", "ein\u00b7her", "drin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "ADJD", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Vmbgeben mich rings vmb, vnd vmb,", "tokens": ["Vmb\u00b7ge\u00b7ben", "mich", "rings", "vmb", ",", "vnd", "vmb", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PTKVZ", "$,", "KON", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Mitt ihnen mu\u00df ich ringen.", "tokens": ["Mitt", "ih\u00b7nen", "mu\u00df", "ich", "rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein trewer Herr, mein frommer Gott,", "tokens": ["Mein", "tre\u00b7wer", "Herr", ",", "mein", "from\u00b7mer", "Gott", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht wollest mich verla\u00dfen:", "tokens": ["Nicht", "wol\u00b7lest", "mich", "ver\u00b7la\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Schaw Herr auff mich in meiner noth,", "tokens": ["Schaw", "Herr", "auff", "mich", "in", "mei\u00b7ner", "noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mein leid ist ohne massen.", "tokens": ["Mein", "leid", "ist", "oh\u00b7ne", "mas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich heule fast in schwerer pein,", "tokens": ["Ich", "heu\u00b7le", "fast", "in", "schwe\u00b7rer", "pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das heil ist weit noch hinden:", "tokens": ["Das", "heil", "ist", "weit", "noch", "hin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VAFIN", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O Gott la\u00df doch bald anders sein,", "tokens": ["O", "Gott", "la\u00df", "doch", "bald", "an\u00b7ders", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVIMP", "ADV", "ADV", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vergi\u00df nicht deiner kinden:", "tokens": ["Ver\u00b7gi\u00df", "nicht", "dei\u00b7ner", "kin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Weich nit von mir, zu diser frist,", "tokens": ["Weich", "nit", "von", "mir", ",", "zu", "di\u00b7ser", "frist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "PPER", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil elend noch f\u00fcrhanden,", "tokens": ["Weil", "e\u00b7lend", "noch", "f\u00fcr\u00b7han\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dan sonst ich keinen helffer wist,", "tokens": ["Dan", "sonst", "ich", "kei\u00b7nen", "helf\u00b7fer", "wist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich w\u00fcrde ja zu schanden.", "tokens": ["Ich", "w\u00fcr\u00b7de", "ja", "zu", "schan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hilff mir, O Gott, au\u00df diser flut,", "tokens": ["Hilff", "mir", ",", "O", "Gott", ",", "au\u00df", "di\u00b7ser", "flut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NE", "NN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df hulff vom himmel kommen", "tokens": ["La\u00df", "hulff", "vom", "him\u00b7mel", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "NN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gro\u00df wasser mich ergreiffen thut,", "tokens": ["Gro\u00df", "was\u00b7ser", "mich", "er\u00b7greif\u00b7fen", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWS", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat vberhand genommen.", "tokens": ["Hat", "vber\u00b7hand", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wan ich zu dir nicht schreyen thet", "tokens": ["Wan", "ich", "zu", "dir", "nicht", "schre\u00b7yen", "thet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "PPER", "PTKNEG", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im tieffen schlam versuncken,", "tokens": ["Im", "tief\u00b7fen", "schlam", "ver\u00b7sun\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich schon verloren geben hett,", "tokens": ["Ich", "schon", "ver\u00b7lo\u00b7ren", "ge\u00b7ben", "hett", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.8": {"text": "Vnd w\u00e4r so gar erdruncken.", "tokens": ["Vnd", "w\u00e4r", "so", "gar", "er\u00b7drun\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Von ruffen bin ich h\u00e4iser sehr,", "tokens": ["Von", "ruf\u00b7fen", "bin", "ich", "h\u00e4i\u00b7ser", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "VAFIN", "PPER", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Gott kom doch zur stunde", "tokens": ["O", "Gott", "kom", "doch", "zur", "stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "VVFIN", "ADV", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bald, bald, ach bald, kan ja nicht mehr", "tokens": ["Bald", ",", "bald", ",", "ach", "bald", ",", "kan", "ja", "nicht", "mehr"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "$,", "ITJ", "ADV", "$,", "VMFIN", "ADV", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bald, bald, ich geh zu grunde", "tokens": ["Bald", ",", "bald", ",", "ich", "geh", "zu", "grun\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "$,", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O o! O o! du frommer Gott!", "tokens": ["O", "o", "!", "O", "o", "!", "du", "from\u00b7mer", "Gott", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "FM", "$.", "NE", "FM", "$.", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O Gott, war schier mi\u00dflungen!", "tokens": ["O", "Gott", ",", "war", "schier", "mi\u00df\u00b7lun\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "War eben zeit; war halber tod:", "tokens": ["War", "e\u00b7ben", "zeit", ";", "war", "hal\u00b7ber", "tod", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "$.", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die seel w\u00e4r bald zersprungen.", "tokens": ["Die", "seel", "w\u00e4r", "bald", "zer\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nun rettest mich mit deiner hand,", "tokens": ["Nun", "ret\u00b7test", "mich", "mit", "dei\u00b7ner", "hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd haltest noch bey leben,", "tokens": ["Vnd", "hal\u00b7test", "noch", "bey", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von oben hast mir h\u00fclff gesand;", "tokens": ["Von", "o\u00b7ben", "hast", "mir", "h\u00fclff", "ge\u00b7sand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mag nun in hoffnung schweben.", "tokens": ["Mag", "nun", "in", "hoff\u00b7nung", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O Gott, wer sich auff dich verlast,", "tokens": ["O", "Gott", ",", "wer", "sich", "auff", "dich", "ver\u00b7last", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWS", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Ist nie kein angst so schwinde,", "tokens": ["Ist", "nie", "kein", "angst", "so", "schwin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da nicht, wan er nur haltet fast,", "tokens": ["Da", "nicht", ",", "wan", "er", "nur", "hal\u00b7tet", "fast", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "$,", "PWAV", "PPER", "ADV", "VVFIN", "ADV", "$,"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Er endlich vberwinde.", "tokens": ["Er", "end\u00b7lich", "vber\u00b7win\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}