{"textgrid.poem.33270": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "O-Tahiti", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O gl\u00fccklich Land! auf das, wie's hei\u00dft, hernieder", "tokens": ["O", "gl\u00fcck\u00b7lich", "Land", "!", "auf", "das", ",", "wie's", "hei\u00dft", ",", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "ADJD", "NN", "$.", "APPR", "ART", "$,", "PWAV", "VVFIN", "$,", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So reichlich Gottes bester Segen quoll,", "tokens": ["So", "reich\u00b7lich", "Got\u00b7tes", "bes\u00b7ter", "Se\u00b7gen", "quoll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bist du's, auf dem ein Theilchen uns'rer Br\u00fcder", "tokens": ["Bist", "du's", ",", "auf", "dem", "ein", "Theil\u00b7chen", "un\u00b7s'\u00b7rer", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "$,", "APPR", "PRELS", "ART", "NN", "PPOSAT", "NN"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sein gold'nes Alter wirklich leben soll?", "tokens": ["Sein", "gold'\u00b7nes", "Al\u00b7ter", "wirk\u00b7lich", "le\u00b7ben", "soll", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und du, o Volk! das, laut so vieler Sagen,", "tokens": ["Und", "du", ",", "o", "Volk", "!", "das", ",", "laut", "so", "vie\u00b7ler", "Sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "NN", "$.", "PDS", "$,", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Erdens\u00f6hne h\u00f6chstes Gl\u00fcck genie\u00dft,", "tokens": ["Der", "Er\u00b7den\u00b7s\u00f6h\u00b7ne", "h\u00f6chs\u00b7tes", "Gl\u00fcck", "ge\u00b7nie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist's wahr, da\u00df du so frei von allen Plagen", "tokens": ["Ist's", "wahr", ",", "da\u00df", "du", "so", "frei", "von", "al\u00b7len", "Pla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "ADJD", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Menschheit, und so \u00fcbergl\u00fccklich bist?", "tokens": ["Der", "Menschheit", ",", "und", "so", "\u00fc\u00b7berg\u00b7l\u00fcck\u00b7lich", "bist", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Zwar malt man in so reizendem Gewande", "tokens": ["Zwar", "malt", "man", "in", "so", "rei\u00b7zen\u00b7dem", "Ge\u00b7wan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Bild uns vor, das deine Fluren kr\u00f6nt,", "tokens": ["Das", "Bild", "uns", "vor", ",", "das", "dei\u00b7ne", "Flu\u00b7ren", "kr\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df mancher sich aus seinem Vaterlande", "tokens": ["Da\u00df", "man\u00b7cher", "sich", "aus", "sei\u00b7nem", "Va\u00b7ter\u00b7lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hinaus, und hin nach deinen H\u00fctten sehnt.", "tokens": ["Hin\u00b7aus", ",", "und", "hin", "nach", "dei\u00b7nen", "H\u00fct\u00b7ten", "sehnt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KON", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Allein erlaube mir nur wenig Fragen,", "tokens": ["Al\u00b7lein", "er\u00b7lau\u00b7be", "mir", "nur", "we\u00b7nig", "Fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Eh' auch mein Mund dich gl\u00fccklich preist;", "tokens": ["Eh'", "auch", "mein", "Mund", "dich", "gl\u00fcck\u00b7lich", "preist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vielleicht l\u00e4\u00dft auch von dir der Spruch sich wagen:", "tokens": ["Viel\u00b7leicht", "l\u00e4\u00dft", "auch", "von", "dir", "der", "Spruch", "sich", "wa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "PPER", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es ist nicht alles \u00e4chtes Gold, was glei\u00dft.", "tokens": ["Es", "ist", "nicht", "al\u00b7les", "\u00e4ch\u00b7tes", "Gold", ",", "was", "glei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "PIAT", "ADJA", "NN", "$,", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Hast du Pandorens B\u00fcchse, die uns allen", "tokens": ["Hast", "du", "Pan\u00b7do\u00b7rens", "B\u00fcch\u00b7se", ",", "die", "uns", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "NN", "NN", "$,", "PRELS", "PPER", "PIAT"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In der Vernunft Natur, die Mutter schenkt,", "tokens": ["In", "der", "Ver\u00b7nunft", "Na\u00b7tur", ",", "die", "Mut\u00b7ter", "schenkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Noch nicht so aufgethan, da\u00df d'raus der Qualen", "tokens": ["Noch", "nicht", "so", "auf\u00b7ge\u00b7than", ",", "da\u00df", "d'raus", "der", "Qua\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "ADV", "VVPP", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vollz\u00e4hlig Heer um deine Flur sich dr\u00e4ngt?", "tokens": ["Voll\u00b7z\u00e4h\u00b7lig", "Heer", "um", "dei\u00b7ne", "Flur", "sich", "dr\u00e4ngt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ist's wirklich falsch, was ich im Angesichte", "tokens": ["Ist's", "wirk\u00b7lich", "falsch", ",", "was", "ich", "im", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "ADJD", "$,", "PWS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Menschenleiden, und bei manchem Grab", "tokens": ["Der", "Men\u00b7schen\u00b7lei\u00b7den", ",", "und", "bei", "man\u00b7chem", "Grab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mir dachte, da\u00df mit jenem Seelenlichte", "tokens": ["Mir", "dach\u00b7te", ",", "da\u00df", "mit", "je\u00b7nem", "See\u00b7len\u00b7lich\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Uns die Natur \u2013 ein spitzes Messer gab?", "tokens": ["Uns", "die", "Na\u00b7tur", "\u2013", "ein", "spit\u00b7zes", "Mes\u00b7ser", "gab", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$(", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Fuhrst du mit jenem spitzen Himmelsstahle", "tokens": ["Fuhrst", "du", "mit", "je\u00b7nem", "spit\u00b7zen", "Him\u00b7mels\u00b7stah\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nie an die eigne Kehle, wie ein Kind?", "tokens": ["Nie", "an", "die", "eig\u00b7ne", "Keh\u00b7le", ",", "wie", "ein", "Kind", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erfuhrst du wirklich nie, was in der Schale", "tokens": ["Er\u00b7fuhrst", "du", "wirk\u00b7lich", "nie", ",", "was", "in", "der", "Scha\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des hellsten Lichts f\u00fcr Irrwischflammen sind?", "tokens": ["Des", "hells\u00b7ten", "Lichts", "f\u00fcr", "Irr\u00b7wischflam\u00b7men", "sind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Hat die Vernunft, der edlen Freiheit Mutter,", "tokens": ["Hat", "die", "Ver\u00b7nunft", ",", "der", "ed\u00b7len", "Frei\u00b7heit", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht selbst um ihre Gabe dich gebracht?", "tokens": ["Nicht", "selbst", "um", "ih\u00b7re", "Ga\u00b7be", "dich", "ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Streust du nicht K\u00f6nigen gezwungen Futter,", "tokens": ["Streust", "du", "nicht", "K\u00f6\u00b7ni\u00b7gen", "ge\u00b7zwun\u00b7gen", "Fut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "NN", "VVPP", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und huldigst G\u00f6tzen, die du selbst gemacht?", "tokens": ["Und", "hul\u00b7digst", "G\u00f6t\u00b7zen", ",", "die", "du", "selbst", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Sind nicht ein Heer von deinen Br\u00fcdern Sklaven,", "tokens": ["Sind", "nicht", "ein", "Heer", "von", "dei\u00b7nen", "Br\u00fc\u00b7dern", "Skla\u00b7ven", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcllt ihre Hand nicht manches H\u00f6flings Wanst,", "tokens": ["F\u00fcllt", "ih\u00b7re", "Hand", "nicht", "man\u00b7ches", "H\u00f6f\u00b7lings", "Wanst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die, w\u00e4hrend diese K\u00f6nigsthiere schlafen,", "tokens": ["Die", ",", "w\u00e4h\u00b7rend", "die\u00b7se", "K\u00f6\u00b7nigst\u00b7hie\u00b7re", "schla\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr ihre leckern Gaumen kocht und pflanzt?", "tokens": ["F\u00fcr", "ih\u00b7re", "le\u00b7ckern", "Gau\u00b7men", "kocht", "und", "pflanzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Ist's nicht ein Raub an deinem eignen Gute,", "tokens": ["Ist's", "nicht", "ein", "Raub", "an", "dei\u00b7nem", "eig\u00b7nen", "Gu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df dir der St\u00e4rkere die Schweine nahm:", "tokens": ["Da\u00df", "dir", "der", "St\u00e4r\u00b7ke\u00b7re", "die", "Schwei\u00b7ne", "nahm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erh\u00e4lst du nicht mit deinem eignen Blute", "tokens": ["Er\u00b7h\u00e4lst", "du", "nicht", "mit", "dei\u00b7nem", "eig\u00b7nen", "Blu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die fremden Sklaven deines K\u00f6nigs zahm?", "tokens": ["Die", "frem\u00b7den", "Skla\u00b7ven", "dei\u00b7nes", "K\u00f6\u00b7nigs", "zahm", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "H\u00f6rt man dich nicht am harten Ruder winseln,", "tokens": ["H\u00f6rt", "man", "dich", "nicht", "am", "har\u00b7ten", "Ru\u00b7der", "win\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "PTKNEG", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das du mit saurer Arbeit selbst geschnitzt?", "tokens": ["Das", "du", "mit", "sau\u00b7rer", "Ar\u00b7beit", "selbst", "ge\u00b7schnitzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fchrst du nicht Tausende nach fremden Inseln,", "tokens": ["F\u00fchrst", "du", "nicht", "Tau\u00b7sen\u00b7de", "nach", "frem\u00b7den", "In\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Wo dann ihr Blut \u2013 nicht f\u00fcr die Freiheit \u2013 spritzt?", "tokens": ["Wo", "dann", "ihr", "Blut", "\u2013", "nicht", "f\u00fcr", "die", "Frei\u00b7heit", "\u2013", "spritzt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "$(", "PTKNEG", "APPR", "ART", "NN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Hat deine Seele keine Abentheuer,", "tokens": ["Hat", "dei\u00b7ne", "See\u00b7le", "kei\u00b7ne", "A\u00b7bent\u00b7heu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des tollen Aberglaubens ausgeheckt?", "tokens": ["Des", "tol\u00b7len", "A\u00b7berg\u00b7lau\u00b7bens", "aus\u00b7ge\u00b7heckt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hast du nicht Pfaff