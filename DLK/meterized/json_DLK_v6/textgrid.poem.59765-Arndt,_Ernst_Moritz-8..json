{"textgrid.poem.59765": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer fest will, fest und unverr\u00fcckt dasselbe,", "tokens": ["Wer", "fest", "will", ",", "fest", "und", "un\u00b7ver\u00b7r\u00fcckt", "das\u00b7sel\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VMFIN", "$,", "ADJD", "KON", "ADJD", "PDAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der sprengt vom festen Himmel das Gew\u00f6lbe,", "tokens": ["Der", "sprengt", "vom", "fes\u00b7ten", "Him\u00b7mel", "das", "Ge\u00b7w\u00f6l\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem m\u00fcssen alle Geister sich verneigen", "tokens": ["Dem", "m\u00fcs\u00b7sen", "al\u00b7le", "Geis\u00b7ter", "sich", "ver\u00b7nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PIAT", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und rufen: ", "tokens": ["Und", "ru\u00b7fen", ":"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}