{"textgrid.poem.25966": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Des Sommers singende H\u00e4user vergehen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herbstwinde wehen durch das Gel\u00e4nde,", "tokens": ["Herbst\u00b7win\u00b7de", "we\u00b7hen", "durch", "das", "Ge\u00b7l\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "++-+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Die H\u00e4nde der B\u00e4ume werden so schwach.", "tokens": ["Die", "H\u00e4n\u00b7de", "der", "B\u00e4u\u00b7me", "wer\u00b7den", "so", "schwach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wir sehen den gleitenden Bl\u00e4ttern nach,", "tokens": ["Wir", "se\u00b7hen", "den", "glei\u00b7ten\u00b7den", "Bl\u00e4t\u00b7tern", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Des Sommers singende H\u00e4user vergehen,", "tokens": ["Des", "Som\u00b7mers", "sin\u00b7gen\u00b7de", "H\u00e4u\u00b7ser", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wir schauen durch fallende W\u00e4nde.", "tokens": ["Wir", "schau\u00b7en", "durch", "fal\u00b7len\u00b7de", "W\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Auf leeren Wegen die Winde klagen,", "tokens": ["Auf", "lee\u00b7ren", "We\u00b7gen", "die", "Win\u00b7de", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Viel fortgetragen haben die Wege.", "tokens": ["Viel", "fort\u00b7ge\u00b7tra\u00b7gen", "ha\u00b7ben", "die", "We\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und wo ich auch meine Wange hinlege,", "tokens": ["Und", "wo", "ich", "auch", "mei\u00b7ne", "Wan\u00b7ge", "hin\u00b7le\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Ich pflege nirgends der Ruhe mehr,", "tokens": ["Ich", "pfle\u00b7ge", "nir\u00b7gends", "der", "Ru\u00b7he", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wie der Baum ohne Blatt ist mein Tag lustleer.", "tokens": ["Wie", "der", "Baum", "oh\u00b7ne", "Blatt", "ist", "mein", "Tag", "lust\u00b7leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "VAFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}}}}