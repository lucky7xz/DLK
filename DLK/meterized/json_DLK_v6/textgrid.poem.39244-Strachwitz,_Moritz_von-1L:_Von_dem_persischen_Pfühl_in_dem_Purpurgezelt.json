{"textgrid.poem.39244": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von dem persischen Pf\u00fchl in dem Purpurgezelt", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von dem persischen Pf\u00fchl in dem Purpurgezelt", "tokens": ["Von", "dem", "per\u00b7si\u00b7schen", "Pf\u00fchl", "in", "dem", "Pur\u00b7pur\u00b7ge\u00b7zelt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Sprang s\u00e4belumg\u00fcrtet der Herr der Welt;", "tokens": ["Sprang", "s\u00e4\u00b7be\u00b7lum\u00b7g\u00fcr\u00b7tet", "der", "Herr", "der", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wie die Schl\u00fcnde der See bei des Nordsturms Nahn,", "tokens": ["Wie", "die", "Schl\u00fcn\u00b7de", "der", "See", "bei", "des", "Nord\u00b7sturms", "Nahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "So ertosten die Tale von Hindostan,", "tokens": ["So", "er\u00b7tos\u00b7ten", "die", "Ta\u00b7le", "von", "Hin\u00b7dos\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Denn der Mogul ritt zum Jagen.", "tokens": ["Denn", "der", "Mo\u00b7gul", "ritt", "zum", "Ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und es tanzte der Hengst \u00fcber knirschenden Sand,", "tokens": ["Und", "es", "tanz\u00b7te", "der", "Hengst", "\u00fc\u00b7ber", "knir\u00b7schen\u00b7den", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "--+---+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Doch schwer hin stampfte der Elefant,", "tokens": ["Doch", "schwer", "hin", "stampf\u00b7te", "der", "E\u00b7le\u00b7fant", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Wie ein Wandelgebirg, mit dem Turme geschm\u00fcckt,", "tokens": ["Wie", "ein", "Wan\u00b7del\u00b7ge\u00b7birg", ",", "mit", "dem", "Tur\u00b7me", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.9": {"text": "Und des Turmes Geb\u00e4lk war lanzengespickt,", "tokens": ["Und", "des", "Tur\u00b7mes", "Ge\u00b7b\u00e4lk", "war", "lan\u00b7zen\u00b7ge\u00b7spickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Und sein Dach mit Schilden beschlagen.", "tokens": ["Und", "sein", "Dach", "mit", "Schil\u00b7den", "be\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und die Zeltwand fiel, und der Kaiser erschien,", "tokens": ["Und", "die", "Zelt\u00b7wand", "fiel", ",", "und", "der", "Kai\u00b7ser", "er\u00b7schien", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In den Staub hin sanken die V\u00f6lker um ihn,", "tokens": ["In", "den", "Staub", "hin", "san\u00b7ken", "die", "V\u00f6l\u00b7ker", "um", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Tief beugte sein Knie der Elefant,", "tokens": ["Tief", "beug\u00b7te", "sein", "Knie", "der", "E\u00b7le\u00b7fant", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und der F\u00fcrstin Stirne ward wund im Sand,", "tokens": ["Und", "der", "F\u00fcrs\u00b7tin", "Stir\u00b7ne", "ward", "wund", "im", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VAFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und es zitterte Sklave und Rajah.", "tokens": ["Und", "es", "zit\u00b7ter\u00b7te", "Skla\u00b7ve", "und", "Ra\u00b7jah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "KON", "NE", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Doch im schnellenden Satz auf sein perlfarb' Tier", "tokens": ["Doch", "im", "schnel\u00b7len\u00b7den", "Satz", "auf", "sein", "per\u00b7lfa\u00b7rb'", "Tier"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Von des Negers Genick sprang Dschehan-Gir,", "tokens": ["Von", "des", "Ne\u00b7gers", "Ge\u00b7nick", "sprang", "Dsche\u00b7han\u00b7Gir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "NE", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Es ergl\u00e4nzte der F\u00fcrst, wie des Geri Haupt,", "tokens": ["Es", "er\u00b7gl\u00e4nz\u00b7te", "der", "F\u00fcrst", ",", "wie", "des", "Ge\u00b7ri", "Haupt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "ART", "NN", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.9": {"text": "Wenn das Donnergew\u00f6lk tief unten schnaubt,", "tokens": ["Wenn", "das", "Don\u00b7ner\u00b7ge\u00b7w\u00f6lk", "tief", "un\u00b7ten", "schnaubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "ADV", "VVFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "In den Schl\u00fcnden des Himalajah.", "tokens": ["In", "den", "Schl\u00fcn\u00b7den", "des", "Hi\u00b7ma\u00b7la\u00b7jah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Sein geschmeidiger Leib war goldgeschuppt,", "tokens": ["Sein", "ge\u00b7schmei\u00b7di\u00b7ger", "Leib", "war", "gold\u00b7ge\u00b7schuppt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und in Scharlachgeweb' der Schenkel verluppt,", "tokens": ["Und", "in", "Scha\u00b7rlach\u00b7ge\u00b7web'", "der", "Schen\u00b7kel", "ver\u00b7luppt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "All' Sattel und Zaum mit Perlen gezackt,", "tokens": ["All'", "Sat\u00b7tel", "und", "Zaum", "mit", "Per\u00b7len", "ge\u00b7zackt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und der S\u00e4belgriff ein einz'ger Smaragd,", "tokens": ["Und", "der", "S\u00e4\u00b7bel\u00b7griff", "ein", "einz'\u00b7ger", "Sma\u00b7ragd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Der Goldhelm reiherbefiedert.", "tokens": ["Der", "Gold\u00b7helm", "rei\u00b7her\u00b7be\u00b7fie\u00b7dert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und der Goldstoff rauschte, die Feder stob,", "tokens": ["Und", "der", "Gold\u00b7stoff", "rauschte", ",", "die", "Fe\u00b7der", "stob", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Und der silberbeschlagene Schimmel schnob,", "tokens": ["Und", "der", "sil\u00b7ber\u00b7be\u00b7schla\u00b7ge\u00b7ne", "Schim\u00b7mel", "schnob", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Wie die Schlange, die lange sich stumm geballt,", "tokens": ["Wie", "die", "Schlan\u00b7ge", ",", "die", "lan\u00b7ge", "sich", "stumm", "ge\u00b7ballt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ADV", "PRF", "ADJD", "VVPP", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.9": {"text": "So rasselte durch den Palmenwald", "tokens": ["So", "ras\u00b7sel\u00b7te", "durch", "den", "Pal\u00b7men\u00b7wald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Der Jagdzug, farbig gegliedert.", "tokens": ["Der", "Jagd\u00b7zug", ",", "far\u00b7big", "ge\u00b7glie\u00b7dert", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Und der Wald war dicht und schwarz das Gr\u00fcn,", "tokens": ["Und", "der", "Wald", "war", "dicht", "und", "schwarz", "das", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "ART", "NN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Und pr\u00e4chtig des Palmdachs Baldachin,", "tokens": ["Und", "pr\u00e4ch\u00b7tig", "des", "Palm\u00b7dachs", "Bal\u00b7da\u00b7ch\u00b7in", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Durch das Rankengewirr, da kam es gesetzt,", "tokens": ["Durch", "das", "Ran\u00b7ken\u00b7ge\u00b7wirr", ",", "da", "kam", "es", "ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und es schnarchten die Pferde und standen zuletzt,", "tokens": ["Und", "es", "schnarch\u00b7ten", "die", "Pfer\u00b7de", "und", "stan\u00b7den", "zu\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "ADV", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "Den Odem zogen die Krieger.", "tokens": ["Den", "O\u00b7dem", "zo\u00b7gen", "die", "Krie\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und der F\u00fcrst hielt vorn, in den B\u00fcgel gestemmt;", "tokens": ["Und", "der", "F\u00fcrst", "hielt", "vorn", ",", "in", "den", "B\u00fc\u00b7gel", "ge\u00b7stemmt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "$,", "APPR", "ART", "NN", "VVPP", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Doch die Zunge heraus und den Schweif geklemmt,", "tokens": ["Doch", "die", "Zun\u00b7ge", "he\u00b7raus", "und", "den", "Schweif", "ge\u00b7klemmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKVZ", "KON", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Das gelbliche Fell schwarzrot gestreift,", "tokens": ["Das", "gelb\u00b7li\u00b7che", "Fell", "schwarz\u00b7rot", "ge\u00b7streift", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Und das gelbliche Aug' blutrot gereift,", "tokens": ["Und", "das", "gelb\u00b7li\u00b7che", "Aug'", "blut\u00b7rot", "ge\u00b7reift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Ansprang den Kaiser der Tiger.", "tokens": ["An\u00b7sprang", "den", "Kai\u00b7ser", "der", "Ti\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Hoch b\u00e4umte der Hengst von der Schaufel gepre\u00dft,", "tokens": ["Hoch", "b\u00e4um\u00b7te", "der", "Hengst", "von", "der", "Schau\u00b7fel", "ge\u00b7pre\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Doch es sa\u00df das Getier und krallte sich fest,", "tokens": ["Doch", "es", "sa\u00df", "das", "Ge\u00b7tier", "und", "krall\u00b7te", "sich", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Schwer st\u00f6hnte das Ro\u00df in des Raubtiers Druck,", "tokens": ["Schwer", "st\u00f6hn\u00b7te", "das", "Ro\u00df", "in", "des", "Raub\u00b7tiers", "Druck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und es ri\u00df sein Fell von der Pranken Ruck,", "tokens": ["Und", "es", "ri\u00df", "sein", "Fell", "von", "der", "Pran\u00b7ken", "Ruck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Aus den H\u00f6hlen quollen die Lichter.", "tokens": ["Aus", "den", "H\u00f6h\u00b7len", "quol\u00b7len", "die", "Lich\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Doch der Kaiser sa\u00df fest, das Haupt nach vorn,", "tokens": ["Doch", "der", "Kai\u00b7ser", "sa\u00df", "fest", ",", "das", "Haupt", "nach", "vorn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "APPR", "ADV", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Seinen seidenen Bart aufstr\u00e4ubte der Zorn.", "tokens": ["Sei\u00b7nen", "sei\u00b7de\u00b7nen", "Bart", "auf\u00b7str\u00e4ub\u00b7te", "der", "Zorn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Wild war der Tiger und wilder der Khan,", "tokens": ["Wild", "war", "der", "Ti\u00b7ger", "und", "wil\u00b7der", "der", "Khan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", 