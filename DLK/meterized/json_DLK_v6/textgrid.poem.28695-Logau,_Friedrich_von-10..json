{"textgrid.poem.28695": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, mir ist ein treuer Freund von dem Gl\u00fccke zugewand!", "tokens": ["Ach", ",", "mir", "ist", "ein", "treu\u00b7er", "Freund", "von", "dem", "Gl\u00fc\u00b7cke", "zu\u00b7ge\u00b7wand", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Sagt ein Weib, sein gantzes Hertz hab ich mir in meiner Hand.", "tokens": ["Sagt", "ein", "Weib", ",", "sein", "gant\u00b7zes", "Hertz", "hab", "ich", "mir", "in", "mei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Dieses h\u00f6rt ein andrer Freund, sahe drauff, schwur ohne Danck,", "tokens": ["Die\u00b7ses", "h\u00f6rt", "ein", "an\u00b7drer", "Freund", ",", "sa\u00b7he", "drauff", ",", "schwur", "oh\u00b7ne", "Danck", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "PTKVZ", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Was er in der Hand gesehn, sey kein Hertz, es sey zu lang.", "tokens": ["Was", "er", "in", "der", "Hand", "ge\u00b7sehn", ",", "sey", "kein", "Hertz", 