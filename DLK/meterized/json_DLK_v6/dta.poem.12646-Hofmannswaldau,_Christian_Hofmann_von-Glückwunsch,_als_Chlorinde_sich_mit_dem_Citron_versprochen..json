{"dta.poem.12646": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcckwunsch, als Chlorinde sich  \n mit dem Citron versprochen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der himmel lacht euch an, auch bey der winters-zeit,", "tokens": ["Der", "him\u00b7mel", "lacht", "euch", "an", ",", "auch", "bey", "der", "win\u00b7ter\u00b7szeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er heisset euren fu\u00df auf lilg und rosen gehen,", "tokens": ["Er", "heis\u00b7set", "eu\u00b7ren", "fu\u00df", "auf", "lilg", "und", "ro\u00b7sen", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das zimmer, das euch h\u00e4lt, ist mit jesmin durchstreut,", "tokens": ["Das", "zim\u00b7mer", ",", "das", "euch", "h\u00e4lt", ",", "ist", "mit", "jes\u00b7min", "durch\u00b7streut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich sehe tausendsch\u00f6n, und was noch sch\u00f6ner, stehen.", "tokens": ["Ich", "se\u00b7he", "tau\u00b7send\u00b7sch\u00f6n", ",", "und", "was", "noch", "sch\u00f6\u00b7ner", ",", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "KON", "PWS", "ADV", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die sonne scheinet auch hier starck auf beyder brust,", "tokens": ["Die", "son\u00b7ne", "schei\u00b7net", "auch", "hier", "starck", "auf", "bey\u00b7der", "brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie treibt die flammen ein und f\u00fchrt sie zu dem hertzen,", "tokens": ["Sie", "treibt", "die", "flam\u00b7men", "ein", "und", "f\u00fchrt", "sie", "zu", "dem", "hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "VVFIN", "PTKVZ", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es macht euch di\u00df zwar hei\u00df, doch ists auch eure lust,", "tokens": ["Es", "macht", "euch", "di\u00df", "zwar", "hei\u00df", ",", "doch", "ists", "auch", "eu\u00b7re", "lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PDS", "ADV", "ADJD", "$,", "ADV", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Denn itzund findet ihr selbst in dem schmertze schertzen.", "tokens": ["Denn", "it\u00b7zund", "fin\u00b7det", "ihr", "selbst", "in", "dem", "schmert\u00b7ze", "schert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch ist auch winter hier, ich sehe ja den schnee", "tokens": ["Doch", "ist", "auch", "win\u00b7ter", "hier", ",", "ich", "se\u00b7he", "ja", "den", "schnee"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "NE", "ADV", "$,", "PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Auf jenen wangen dort, und ei\u00df auf deren h\u00f6hen,", "tokens": ["Auf", "je\u00b7nen", "wan\u00b7gen", "dort", ",", "und", "ei\u00df", "auf", "de\u00b7ren", "h\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "$,", "KON", "ADJD", "APPR", "PRELAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Doch w\u00e4chst noch unter uns vierbl\u00e4tteriger klee,", "tokens": ["Doch", "w\u00e4chst", "noch", "un\u00b7ter", "uns", "vier\u00b7bl\u00e4t\u00b7te\u00b7ri\u00b7ger", "klee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Auf dem das gl\u00fccke soll, man sagts, gebildet stehen.", "tokens": ["Auf", "dem", "das", "gl\u00fc\u00b7cke", "soll", ",", "man", "sagts", ",", "ge\u00b7bil\u00b7det", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VMFIN", "$,", "PIS", "VVFIN", "$,", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch gebt ihr mir itzund wohl nicht gar gerne raum,", "tokens": ["Doch", "gebt", "ihr", "mir", "it\u00b7zund", "wohl", "nicht", "gar", "ger\u00b7ne", "raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "ADV", "PTKNEG", "ADV", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Jhr habt itzt sonsten was, und heimlich, zu verrichten,", "tokens": ["Ihr", "habt", "itzt", "sons\u00b7ten", "was", ",", "und", "heim\u00b7lich", ",", "zu", "ver\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "PIS", "$,", "KON", "ADJD", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Denn liebes-verse sind nur gegen dem ein traum,", "tokens": ["Denn", "lie\u00b7bes\u00b7ver\u00b7se", "sind", "nur", "ge\u00b7gen", "dem", "ein", "traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADV", "APPR", "ART", "ART", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Was ihr itzunder wolt in eurem hertzen schlichten.", "tokens": ["Was", "ihr", "it\u00b7zun\u00b7der", "wolt", "in", "eu\u00b7rem", "hert\u00b7zen", "schlich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VMFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich weiche gerne weg, und la\u00df euch hier allein,", "tokens": ["Ich", "wei\u00b7che", "ger\u00b7ne", "weg", ",", "und", "la\u00df", "euch", "hier", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "KON", "VVIMP", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Verkn\u00fcpffet euch fein fest, ich werd es wohl erleben,", "tokens": ["Ver\u00b7kn\u00fcpf\u00b7fet", "euch", "fein", "fest", ",", "ich", "werd", "es", "wohl", "er\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da\u00df ein verr\u00e4ther denn des gantzen thuns wird seyn;", "tokens": ["Da\u00df", "ein", "ver\u00b7r\u00e4\u00b7ther", "denn", "des", "gant\u00b7zen", "thuns", "wird", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "KON", "ART", "ADJA", "NN", "VAFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und eur verb\u00fcndni\u00df wird ans licht mit freuden geben.", "tokens": ["Und", "eur", "ver\u00b7b\u00fcn\u00b7dni\u00df", "wird", "ans", "licht", "mit", "freu\u00b7den", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPRART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der winter sey euch warm, die liebe sey euch hei\u00df,", "tokens": ["Der", "win\u00b7ter", "sey", "euch", "warm", ",", "die", "lie\u00b7be", "sey", "euch", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$,", "PRELS", "VVFIN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die blumen machen euch den sommer in dem bette,", "tokens": ["Die", "blu\u00b7men", "ma\u00b7chen", "euch", "den", "som\u00b7mer", "in", "dem", "bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADV", "APPR", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und weil ich euch sonst nichts itzt mehr zu sagen wei\u00df,", "tokens": ["Und", "weil", "ich", "euch", "sonst", "nichts", "itzt", "mehr", "zu", "sa\u00b7gen", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADV", "PIS", "ADV", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So schlie\u00df ich: Wenn ich doch auch so den winter h\u00e4tte!", "tokens": ["So", "schlie\u00df", "ich", ":", "Wenn", "ich", "doch", "auch", "so", "den", "win\u00b7ter", "h\u00e4t\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "KOUS", "PPER", "ADV", "ADV", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}