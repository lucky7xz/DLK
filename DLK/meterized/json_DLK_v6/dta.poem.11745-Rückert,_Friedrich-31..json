{"dta.poem.11745": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Farbenbogen der Empfindungen erscheint,", "tokens": ["Der", "Far\u00b7ben\u00b7bo\u00b7gen", "der", "Emp\u00b7fin\u00b7dun\u00b7gen", "er\u00b7scheint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wenn hier die Sonne lacht, und dort die Wolke weint.", "tokens": ["Wenn", "hier", "die", "Son\u00b7ne", "lacht", ",", "und", "dort", "die", "Wol\u00b7ke", "weint", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$,", "KON", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie G\u00f6tter wandeln auf besonnter Wolkenbr\u00fccke,", "tokens": ["Wie", "G\u00f6t\u00b7ter", "wan\u00b7deln", "auf", "be\u00b7sonn\u00b7ter", "Wol\u00b7ken\u00b7br\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wandeln drunterhin wir zwischen Leid und Gl\u00fccke.", "tokens": ["So", "wan\u00b7deln", "drun\u00b7ter\u00b7hin", "wir", "zwi\u00b7schen", "Leid", "und", "Gl\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PAV", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du sagst: die Sonne lacht; du sagst: die Wolke weint;", "tokens": ["Du", "sagst", ":", "die", "Son\u00b7ne", "lacht", ";", "du", "sagst", ":", "die", "Wol\u00b7ke", "weint", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil die zu lachen dir und die zu weinen scheint.", "tokens": ["Weil", "die", "zu", "la\u00b7chen", "dir", "und", "die", "zu", "wei\u00b7nen", "scheint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PTKZU", "VVINF", "PPER", "KON", "ART", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du tauchest die Natur in deines Innern Farben,", "tokens": ["Du", "tau\u00b7chest", "die", "Na\u00b7tur", "in", "dei\u00b7nes", "In\u00b7nern", "Far\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die leben, wenn es lebt, und wenn es starb, erstarben.", "tokens": ["Die", "le\u00b7ben", ",", "wenn", "es", "lebt", ",", "und", "wenn", "es", "starb", ",", "er\u00b7star\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "KON", "KOUS", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Dir gebe Gott in dir das ewige Lebendige,", "tokens": ["Dir", "ge\u00b7be", "Gott", "in", "dir", "das", "e\u00b7wi\u00b7ge", "Le\u00b7ben\u00b7di\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+---", "measure": "iambic.penta.invert"}, "line.2": {"text": "Im Unbestand der Welt das einzige Best\u00e4ndige.", "tokens": ["Im", "Un\u00b7be\u00b7stand", "der", "Welt", "das", "ein\u00b7zi\u00b7ge", "Be\u00b7st\u00e4n\u00b7di\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.6": {"line.1": {"text": "Dir gebe Gott in dir das heitere Verst\u00e4ndige,", "tokens": ["Dir", "ge\u00b7be", "Gott", "in", "dir", "das", "hei\u00b7te\u00b7re", "Ver\u00b7st\u00e4n\u00b7di\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.2": {"text": "Da\u00df mit dem Geist der Welt sich klar dein Geist verst\u00e4ndige.", "tokens": ["Da\u00df", "mit", "dem", "Geist", "der", "Welt", "sich", "klar", "dein", "Geist", "ver\u00b7st\u00e4n\u00b7di\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "PRF", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.7": {"line.1": {"text": "Dein Weinen m\u00f6ge dir zum L\u00e4cheln, nie zum Lachen,", "tokens": ["Dein", "Wei\u00b7nen", "m\u00f6\u00b7ge", "dir", "zum", "L\u00e4\u00b7cheln", ",", "nie", "zum", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "APPRART", "NN", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nie dir dein Lachen Gott zum Quell der Thr\u00e4nen machen.", "tokens": ["Nie", "dir", "dein", "La\u00b7chen", "Gott", "zum", "Quell", "der", "Thr\u00e4\u00b7nen", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPOSAT", "NN", "NN", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Des Menschen Aug' allein kann lachen und kann weinen,", "tokens": ["Des", "Men\u00b7schen", "Aug'", "al\u00b7lein", "kann", "la\u00b7chen", "und", "kann", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VMFIN", "VVINF", "KON", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nur die Sch\u00f6nheit kann die beiden sch\u00f6n vereinen.", "tokens": ["Und", "nur", "die", "Sch\u00f6n\u00b7heit", "kann", "die", "bei\u00b7den", "sch\u00f6n", "ver\u00b7ei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VMFIN", "ART", "PIAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Mit einem Auge lacht die Lieb', ihr andres weint;", "tokens": ["Mit", "ei\u00b7nem", "Au\u00b7ge", "lacht", "die", "Lieb'", ",", "ihr", "and\u00b7res", "weint", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was meinest du, da\u00df sie mit Lachen-Weinen meint?", "tokens": ["Was", "mei\u00b7nest", "du", ",", "da\u00df", "sie", "mit", "La\u00b7chen\u00b7Wei\u00b7nen", "meint", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Sie l\u00e4chelt, wenn die Welt sie um die Welt sieht weinen,", "tokens": ["Sie", "l\u00e4\u00b7chelt", ",", "wenn", "die", "Welt", "sie", "um", "die", "Welt", "sieht", "wei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und weint, wenn sie sich sieht verlachen und verneinen.", "tokens": ["Und", "weint", ",", "wenn", "sie", "sich", "sieht", "ver\u00b7la\u00b7chen", "und", "ver\u00b7nei\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}