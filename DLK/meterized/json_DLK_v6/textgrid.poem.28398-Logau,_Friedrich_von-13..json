{"textgrid.poem.28398": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil Christus ist in dem, das seines Vaters heist,", "tokens": ["Weil", "Chris\u00b7tus", "ist", "in", "dem", ",", "das", "sei\u00b7nes", "Va\u00b7ters", "heist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VAFIN", "APPR", "ART", "$,", "PRELS", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ist er auch in uns; wann Tr\u00fcbsal sich erweist,", "tokens": ["So", "ist", "er", "auch", "in", "uns", ";", "wann", "Tr\u00fcb\u00b7sal", "sich", "er\u00b7weist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "$.", "PWAV", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und Christus ist in uns, so ist er mit im Leiden.", "tokens": ["Und", "Chris\u00b7tus", "ist", "in", "uns", ",", "so", "ist", "er", "mit", "im", "Lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "APPR", "PPER", "$,", "ADV", "VAFIN", "PPER", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wol dem, der ihn beh\u00e4lt; weh dem, der ihn l\u00e4st scheiden!", "tokens": ["Wol", "dem", ",", "der", "ihn", "be\u00b7h\u00e4lt", ";", "weh", "dem", ",", "der", "ihn", "l\u00e4st", "schei\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "PPER", "VVFIN", "$.", "ADV", "ART", "$,", "PRELS", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}