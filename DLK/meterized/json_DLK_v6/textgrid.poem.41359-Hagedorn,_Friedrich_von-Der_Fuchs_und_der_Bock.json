{"textgrid.poem.41359": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Fuchs und der Bock", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst reiste Meister Fuchs zu einem seiner Schw\u00e4ger,", "tokens": ["Einst", "reis\u00b7te", "Meis\u00b7ter", "Fuchs", "zu", "ei\u00b7nem", "sei\u00b7ner", "Schw\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NE", "APPR", "ART", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im schw\u00fclen Sommer, \u00fcber Feld;", "tokens": ["Im", "schw\u00fc\u00b7len", "Som\u00b7mer", ",", "\u00fc\u00b7ber", "Feld", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es hatte sich zu ihm der Ziegenbock gesellt,", "tokens": ["Es", "hat\u00b7te", "sich", "zu", "ihm", "der", "Zie\u00b7gen\u00b7bock", "ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "APPR", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der dumm und sicher war, wie viele H\u00f6rnertr\u00e4ger.", "tokens": ["Der", "dumm", "und", "si\u00b7cher", "war", ",", "wie", "vie\u00b7le", "H\u00f6r\u00b7ner\u00b7tr\u00e4\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "VAFIN", "$,", "PWAV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein Abweg f\u00fchrte sie vor eines Pachters Haus;", "tokens": ["Ein", "Ab\u00b7weg", "f\u00fchr\u00b7te", "sie", "vor", "ei\u00b7nes", "Pach\u00b7ters", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da ward f\u00fcr ihren Durst ein Sch\u00f6pfbrunn angetroffen.", "tokens": ["Da", "ward", "f\u00fcr", "ih\u00b7ren", "Durst", "ein", "Sch\u00f6pf\u00b7brunn", "an\u00b7ge\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hier tranken beiderseits. Das hei\u00df' ich recht gesoffen!", "tokens": ["Hier", "tran\u00b7ken", "bei\u00b7der\u00b7seits", ".", "Das", "hei\u00df'", "ich", "recht", "ge\u00b7sof\u00b7fen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$.", "PDS", "VVFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hub Reinke bellend an, und zum vollkommnen Schmaus", "tokens": ["Hub", "Rein\u00b7ke", "bel\u00b7lend", "an", ",", "und", "zum", "voll\u00b7komm\u00b7nen", "Schmaus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "ADJD", "PTKVZ", "$,", "KON", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Fehlt nur ein feister Hahn: der H\u00fchnerstall steht offen;", "tokens": ["Fehlt", "nur", "ein", "feis\u00b7ter", "Hahn", ":", "der", "H\u00fch\u00b7ner\u00b7stall", "steht", "of\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie aber k\u00f6mmt man hier heraus?", "tokens": ["Wie", "a\u00b7ber", "k\u00f6mmt", "man", "hier", "he\u00b7raus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PIS", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mein Herr! darf ich den Anschlag geben,", "tokens": ["Mein", "Herr", "!", "darf", "ich", "den", "An\u00b7schlag", "ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So stellen sie den R\u00fccken hin;", "tokens": ["So", "stel\u00b7len", "sie", "den", "R\u00fc\u00b7cken", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sobald ich aus dem Brunnen bin,", "tokens": ["So\u00b7bald", "ich", "aus", "dem", "Brun\u00b7nen", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ist's ihrem Diener leicht, sie schuldigst nachzuheben!", "tokens": ["Ist's", "ih\u00b7rem", "Die\u00b7ner", "leicht", ",", "sie", "schul\u00b7digst", "nach\u00b7zu\u00b7he\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ADJD", "$,", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ha! meckerte der Bock: nichts kann gescheidter sein.", "tokens": ["Ha", "!", "me\u00b7cker\u00b7te", "der", "Bock", ":", "nichts", "kann", "ge\u00b7scheid\u00b7ter", "sein", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "ART", "NE", "$.", "PIS", "VMFIN", "ADJD", "VAINF", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "Bei meinem Bart! mir fiel der Streich nicht ein.", "tokens": ["Bei", "mei\u00b7nem", "Bart", "!", "mir", "fiel", "der", "Streich", "nicht", "ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Die klugen K\u00f6pfe sollen leben!", "tokens": ["Die", "klu\u00b7gen", "K\u00f6p\u00b7fe", "sol\u00b7len", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Hierauf bequemt er sich, und dienet ihm zur Br\u00fccke;", "tokens": ["Hier\u00b7auf", "be\u00b7quemt", "er", "sich", ",", "und", "die\u00b7net", "ihm", "zur", "Br\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "$,", "KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allein der Fuchs l\u00e4\u00dft seinen Freund zur\u00fccke,", "tokens": ["Al\u00b7lein", "der", "Fuchs", "l\u00e4\u00dft", "sei\u00b7nen", "Freund", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NE", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sagt: Vorjetzt entschuldge mich;", "tokens": ["Und", "sagt", ":", "Vor\u00b7jetzt", "ent\u00b7schuld\u00b7ge", "mich", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Mein Schwager wartet schon; sonst wollt' ich bei dir bleiben.", "tokens": ["Mein", "Schwa\u00b7ger", "war\u00b7tet", "schon", ";", "sonst", "wollt'", "ich", "bei", "dir", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$.", "ADV", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dort jene Ziege guckt auf dich,", "tokens": ["Dort", "je\u00b7ne", "Zie\u00b7ge", "guckt", "auf", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie wird dir unterde\u00df die Zeit recht wohl vertreiben.", "tokens": ["Sie", "wird", "dir", "un\u00b7ter\u00b7de\u00df", "die", "Zeit", "recht", "wohl", "ver\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Falsche rennt davon, und l\u00e4\u00dft mit scheelem Blick", "tokens": ["Der", "Fal\u00b7sche", "rennt", "da\u00b7von", ",", "und", "l\u00e4\u00dft", "mit", "schee\u00b7lem", "Blick"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PAV", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem armen Bock nur diesen Trost zur\u00fcck:", "tokens": ["Dem", "ar\u00b7men", "Bock", "nur", "die\u00b7sen", "Trost", "zu\u00b7r\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "ADV", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sobald wirst du dich nicht des Rettens unterfangen,", "tokens": ["So\u00b7bald", "wirst", "du", "dich", "nicht", "des", "Ret\u00b7tens", "un\u00b7ter\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "PTKNEG", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bevor du selbst der Noth entgangen.", "tokens": ["Be\u00b7vor", "du", "selbst", "der", "Noth", "ent\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du murrest; fasse dich; der Mensch ist deiner Art:", "tokens": ["Du", "mur\u00b7rest", ";", "fas\u00b7se", "dich", ";", "der", "Mensch", "ist", "dei\u00b7ner", "Art", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "PPER", "$.", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Oft steckt sein Wissen nur im Bart.", "tokens": ["Oft", "steckt", "sein", "Wis\u00b7sen", "nur", "im", "Bart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}