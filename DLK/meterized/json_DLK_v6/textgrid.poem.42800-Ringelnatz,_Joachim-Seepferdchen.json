{"textgrid.poem.42800": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Seepferdchen", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich noch ein Seepferdchen war,", "tokens": ["Als", "ich", "noch", "ein", "See\u00b7pferd\u00b7chen", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im vorigen Leben,", "tokens": ["Im", "vo\u00b7ri\u00b7gen", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wie war das wonnig, wunderbar", "tokens": ["Wie", "war", "das", "won\u00b7nig", ",", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "VAFIN", "ART", "ADJD", "$,", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Unter Wasser zu schweben.", "tokens": ["Un\u00b7ter", "Was\u00b7ser", "zu", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "In den tr\u00e4umenden Fluten", "tokens": ["In", "den", "tr\u00e4u\u00b7men\u00b7den", "Flu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Wogte, wie G\u00fcte, das Haar", "tokens": ["Wog\u00b7te", ",", "wie", "G\u00fc\u00b7te", ",", "das", "Haar"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "NN", "$,", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Der zierlichsten aller Seestuten,", "tokens": ["Der", "zier\u00b7lichs\u00b7ten", "al\u00b7ler", "Sees\u00b7tu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,"], "meter": "-+--+-++-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Die meine Geliebte war.", "tokens": ["Die", "mei\u00b7ne", "Ge\u00b7lieb\u00b7te", "war", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Wir senkten uns still oder stiegen,", "tokens": ["Wir", "senk\u00b7ten", "uns", "still", "o\u00b7der", "stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "Tanzten harmonisch um einand,", "tokens": ["Tanz\u00b7ten", "har\u00b7mo\u00b7nisch", "um", "ei\u00b7nand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PIS", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Ohne Arm, ohne Bein, ohne Hand,", "tokens": ["Oh\u00b7ne", "Arm", ",", "oh\u00b7ne", "Bein", ",", "oh\u00b7ne", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "$,", "KOUI", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "Wie Wolken sich in Wolken wiegen.", "tokens": ["Wie", "Wol\u00b7ken", "sich", "in", "Wol\u00b7ken", "wie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Sie spielte manchmal grazi\u00f6ses Entfliehn,", "tokens": ["Sie", "spiel\u00b7te", "manch\u00b7mal", "gra\u00b7zi\u00b7\u00f6\u00b7ses", "Ent\u00b7fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.14": {"text": "Auf da\u00df ich ihr folge, sie hasche,", "tokens": ["Auf", "da\u00df", "ich", "ihr", "fol\u00b7ge", ",", "sie", "ha\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.15": {"text": "Und legte mir einmal im Ansichziehn", "tokens": ["Und", "leg\u00b7te", "mir", "ein\u00b7mal", "im", "An\u00b7sich\u00b7ziehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Eierchen in die Tasche.", "tokens": ["Ei\u00b7er\u00b7chen", "in", "die", "Ta\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Sie blickte traurig und stellte sich froh,", "tokens": ["Sie", "blick\u00b7te", "trau\u00b7rig", "und", "stell\u00b7te", "sich", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Schnappte nach einem Wasserfloh", "tokens": ["Schnapp\u00b7te", "nach", "ei\u00b7nem", "Was\u00b7ser\u00b7floh"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.19": {"text": "Und ringelte sich", "tokens": ["Und", "rin\u00b7gel\u00b7te", "sich"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PRF"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.20": {"text": "An einem Stengelchen fest und sprach so:", "tokens": ["An", "ei\u00b7nem", "Sten\u00b7gel\u00b7chen", "fest", "und", "sprach", "so", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Ich Hebe dich!", "tokens": ["Ich", "He\u00b7be", "dich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.22": {"text": "Du wieherst nicht, du \u00e4pfelst nicht,", "tokens": ["Du", "wie\u00b7herst", "nicht", ",", "du", "\u00e4p\u00b7felst", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "$,", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Du tr\u00e4gst ein farbloses Panzerkleid", "tokens": ["Du", "tr\u00e4gst", "ein", "farb\u00b7lo\u00b7ses", "Pan\u00b7zer\u00b7kleid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.24": {"text": "Und hast ein bek\u00fcmmertes altes Gesicht,", "tokens": ["Und", "hast", "ein", "be\u00b7k\u00fcm\u00b7mer\u00b7tes", "al\u00b7tes", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.25": {"text": "Als w\u00fc\u00dftest du um kommendes Leid.", "tokens": ["Als", "w\u00fc\u00df\u00b7test", "du", "um", "kom\u00b7men\u00b7des", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.26": {"text": "Seest\u00fctchen! Schn\u00f6rkelchen! Ringelna\u00df!", "tokens": ["See\u00b7st\u00fct\u00b7chen", "!", "Schn\u00f6r\u00b7kel\u00b7chen", "!", "Rin\u00b7gel\u00b7na\u00df", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.27": {"text": "Wann war wohl das?", "tokens": ["Wann", "war", "wohl", "das", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "PDS", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.28": {"text": "Und wer bedauert wohl sp\u00e4ter meine restlichen Knochen?", "tokens": ["Und", "wer", "be\u00b7dau\u00b7ert", "wohl", "sp\u00e4\u00b7ter", "mei\u00b7ne", "rest\u00b7li\u00b7chen", "Kno\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "ADV", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "Es ist beinahe so, da\u00df ich weine \u2013", "tokens": ["Es", "ist", "bei\u00b7na\u00b7he", "so", ",", "da\u00df", "ich", "wei\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Lollo hat das vertrocknete, kleine", "tokens": ["Lol\u00b7lo", "hat", "das", "ver\u00b7trock\u00b7ne\u00b7te", ",", "klei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NE", "VAFIN", "PDS", "VVFIN", "$,", "ADJA"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.31": {"text": "Schmerzverkr\u00fcmmte Seepferd zerbrochen.", "tokens": ["Schmerz\u00b7ver\u00b7kr\u00fcmm\u00b7te", "See\u00b7pferd", "zer\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}