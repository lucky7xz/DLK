{"textgrid.poem.49748": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Erziehung zur Kunst", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch ein Leben, welch ein reges Treiben", "tokens": ["Welch", "ein", "Le\u00b7ben", ",", "welch", "ein", "re\u00b7ges", "Trei\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "NN", "$,", "PWAT", "ART", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Herrscht doch in Florenzens Galerien!", "tokens": ["Herrscht", "doch", "in", "Flo\u00b7ren\u00b7zens", "Ga\u00b7le\u00b7ri\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NE", "NE", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Weil hieher ja alle bessern Klassen", "tokens": ["Weil", "hie\u00b7her", "ja", "al\u00b7le", "bes\u00b7sern", "Klas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PAV", "ADV", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Aus dem n\u00f6rdlichen Europa ziehen.", "tokens": ["Aus", "dem", "n\u00f6rd\u00b7li\u00b7chen", "Eu\u00b7ro\u00b7pa", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NE", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "M\u00e4nner, die daheim in dem Berufe", "tokens": ["M\u00e4n\u00b7ner", ",", "die", "da\u00b7heim", "in", "dem", "Be\u00b7ru\u00b7fe"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Keine Zeit und keine Mu\u00dfe haben,", "tokens": ["Kei\u00b7ne", "Zeit", "und", "kei\u00b7ne", "Mu\u00b7\u00dfe", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "M\u00fcssen hier an ungewohnten Sch\u00e4tzen", "tokens": ["M\u00fcs\u00b7sen", "hier", "an", "un\u00b7ge\u00b7wohn\u00b7ten", "Sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ihre ungewohnte Bildung laben.", "tokens": ["Ih\u00b7re", "un\u00b7ge\u00b7wohn\u00b7te", "Bil\u00b7dung", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "M\u00fctter, die der H\u00e4uslichkeit sich widmen", "tokens": ["M\u00fct\u00b7ter", ",", "die", "der", "H\u00e4us\u00b7lich\u00b7keit", "sich", "wid\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "PRF", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und die Str\u00fcmpfe ihrer S\u00f6hne stopfen,", "tokens": ["Und", "die", "Str\u00fcmp\u00b7fe", "ih\u00b7rer", "S\u00f6h\u00b7ne", "stop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sind verpflichtet, ihr Gehirn mit Dingen,", "tokens": ["Sind", "ver\u00b7pflich\u00b7tet", ",", "ihr", "Ge\u00b7hirn", "mit", "Din\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die sie bald vergessen, vollzupfropfen.", "tokens": ["Die", "sie", "bald", "ver\u00b7ges\u00b7sen", ",", "voll\u00b7zu\u00b7pfrop\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "$,", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Seht die Guten mit erhitzten Wangen", "tokens": ["Seht", "die", "Gu\u00b7ten", "mit", "er\u00b7hitz\u00b7ten", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Durch die lange Flucht der S\u00e4le eilen!", "tokens": ["Durch", "die", "lan\u00b7ge", "Flucht", "der", "S\u00e4\u00b7le", "ei\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "L\u00e4nger nicht, als h\u00f6chstens zwei Sekunden", "tokens": ["L\u00e4n\u00b7ger", "nicht", ",", "als", "h\u00f6chs\u00b7tens", "zwei", "Se\u00b7kun\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "KOUS", "ADV", "CARD", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "D\u00fcrfen sie vor einem Bilde weilen.", "tokens": ["D\u00fcr\u00b7fen", "sie", "vor", "ei\u00b7nem", "Bil\u00b7de", "wei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "\u00bbhalt! Das m\u00fcssen wir genau betrachten\u00ab,", "tokens": ["\u00bb", "halt", "!", "Das", "m\u00fcs\u00b7sen", "wir", "ge\u00b7nau", "be\u00b7trach\u00b7ten", "\u00ab", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$.", "PDS", "VMFIN", "PPER", "ADJD", "VVINF", "$(", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Spricht der Vater, \u00bbdenn bedenkt, wir stehen", "tokens": ["Spricht", "der", "Va\u00b7ter", ",", "\u00bb", "denn", "be\u00b7denkt", ",", "wir", "ste\u00b7hen"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "$(", "KON", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Offenbar vor einem Meisterwerke,", "tokens": ["Of\u00b7fen\u00b7bar", "vor", "ei\u00b7nem", "Meis\u00b7ter\u00b7wer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mit zwei Kreuzen ist's im Buch versehen.\u00ab", "tokens": ["Mit", "zwei", "Kreu\u00b7zen", "ist's", "im", "Buch", "ver\u00b7se\u00b7hen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "CARD", "NN", "VAFIN", "APPRART", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Leere Augen glotzen, es ert\u00f6nen", "tokens": ["Lee\u00b7re", "Au\u00b7gen", "glot\u00b7zen", ",", "es", "er\u00b7t\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "VVINF", "$,", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ah! und Oh!'s vermischt mit Pr\u00e4dikaten,", "tokens": ["Ah", "!", "und", "Oh", "!", "'s", "ver\u00b7mischt", "mit", "Pr\u00e4\u00b7di\u00b7ka\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "NN", "$.", "PPER", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sie st\u00fcrzen fort in andre S\u00e4le", "tokens": ["Und", "sie", "st\u00fcr\u00b7zen", "fort", "in", "and\u00b7re", "S\u00e4\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Von dem treuen Baedeker beraten.", "tokens": ["Von", "dem", "treu\u00b7en", "Bae\u00b7de\u00b7ker", "be\u00b7ra\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "M\u00fcde kehren wieder sie zur Heimat,", "tokens": ["M\u00fc\u00b7de", "keh\u00b7ren", "wie\u00b7der", "sie", "zur", "Hei\u00b7mat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo sie die Erinnerung genie\u00dfen.", "tokens": ["Wo", "sie", "die", "E\u00b7rin\u00b7ne\u00b7rung", "ge\u00b7nie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVINF", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Und wir sehen \u00fcberall die Fr\u00fcchte", "tokens": ["Und", "wir", "se\u00b7hen", "\u00fc\u00b7be\u00b7rall", "die", "Fr\u00fcch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der erworb'nen Bildung reichlich sprie\u00dfen.", "tokens": ["Der", "er\u00b7wor\u00b7b'\u00b7nen", "Bil\u00b7dung", "reich\u00b7lich", "sprie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}