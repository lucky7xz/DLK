{"textgrid.poem.26255": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Qual", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Berge einsam bin ich. M\u00f6chte klagen.", "tokens": ["Wie", "Ber\u00b7ge", "ein\u00b7sam", "bin", "ich", ".", "M\u00f6ch\u00b7te", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "VAFIN", "PPER", "$.", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mu\u00df t\u00e4glich, st\u00fcndlich in die Leere fragen.", "tokens": ["Mu\u00df", "t\u00e4g\u00b7lich", ",", "st\u00fcnd\u00b7lich", "in", "die", "Lee\u00b7re", "fra\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "$,", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Reisv\u00f6glein hat es gut dort im Ge\u00e4st,", "tokens": ["Reis\u00b7v\u00f6\u00b7glein", "hat", "es", "gut", "dort", "im", "Ge\u00b7\u00e4st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das ab und zu fliegt zu der Brut im Nest.", "tokens": ["Das", "ab", "und", "zu", "fliegt", "zu", "der", "Brut", "im", "Nest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKVZ", "KON", "PTKZU", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Der Leute Schritte in der B\u00e4ume Schatten,", "tokens": ["Der", "Leu\u00b7te", "Schrit\u00b7te", "in", "der", "B\u00e4u\u00b7me", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die vor dem Haus hinwandern ohn' Ermatten,", "tokens": ["Die", "vor", "dem", "Haus", "hin\u00b7wan\u00b7dern", "ohn'", "Er\u00b7mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie wissen still und stet ihr t\u00e4glich Ziel.", "tokens": ["Sie", "wis\u00b7sen", "still", "und", "stet", "ihr", "t\u00e4g\u00b7lich", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch Ungewi\u00dfheit treibt mit mir ihr Spiel.", "tokens": ["Doch", "Un\u00b7ge\u00b7wi\u00df\u00b7heit", "treibt", "mit", "mir", "ihr", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Hahnenschreie, die vom Zaun herschallen,", "tokens": ["Die", "Hah\u00b7nen\u00b7schrei\u00b7e", ",", "die", "vom", "Zaun", "her\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hell heimatlich im Ohr mir widerhallen.", "tokens": ["Hell", "hei\u00b7mat\u00b7lich", "im", "Ohr", "mir", "wi\u00b7der\u00b7hal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Rechen vor der T\u00fcr scharrt hin und her, \u2013", "tokens": ["Ein", "Re\u00b7chen", "vor", "der", "T\u00fcr", "scharrt", "hin", "und", "her", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Einfachste Laute, von Erinnrung schwer.", "tokens": ["Ein\u00b7fachs\u00b7te", "Lau\u00b7te", ",", "von", "E\u00b7rinn\u00b7rung", "schwer", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "NN", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch Krieg verh\u00fcllt mit grauer Luft die Ferne.", "tokens": ["Doch", "Krieg", "ver\u00b7h\u00fcllt", "mit", "grau\u00b7er", "Luft", "die", "Fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vergeblich such' ich nach der Heimat Sterne.", "tokens": ["Ver\u00b7geb\u00b7lich", "such'", "ich", "nach", "der", "Hei\u00b7mat", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kein Frieden zieht mir in die bange Brust.", "tokens": ["Kein", "Frie\u00b7den", "zieht", "mir", "in", "die", "ban\u00b7ge", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nie hat mein Blut von solcher Qual gewu\u00dft.", "tokens": ["Nie", "hat", "mein", "Blut", "von", "sol\u00b7cher", "Qual", "ge\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}