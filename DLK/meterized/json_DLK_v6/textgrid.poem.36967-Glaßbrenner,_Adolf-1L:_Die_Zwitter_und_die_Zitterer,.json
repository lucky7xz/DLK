{"textgrid.poem.36967": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Zwitter und die Zitterer,", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Zwitter und die Zitterer,", "tokens": ["Die", "Zwit\u00b7ter", "und", "die", "Zit\u00b7te\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die zischelten zusammen,", "tokens": ["Die", "zi\u00b7schel\u00b7ten", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Ob's schon zur Zeit sei, die Censur", "tokens": ["Ob's", "schon", "zur", "Zeit", "sei", ",", "die", "Cen\u00b7sur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADV", "APPRART", "NN", "VAFIN", "$,", "ART", "NN"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Aus Deutschland zu verdammen.", "tokens": ["Aus", "Deutschland", "zu", "ver\u00b7dam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Erste sagt', es m\u00fcsse gehn;", "tokens": ["Der", "Ers\u00b7te", "sagt'", ",", "es", "m\u00fcs\u00b7se", "gehn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Zweite sprach: es macht sich;", "tokens": ["Der", "Zwei\u00b7te", "sprach", ":", "es", "macht", "sich", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PRF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Dritte setzt die Brille auf", "tokens": ["Der", "Drit\u00b7te", "setzt", "die", "Bril\u00b7le", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hat erst noch bedacht sich.", "tokens": ["Und", "hat", "erst", "noch", "be\u00b7dacht", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "VVFIN", "PRF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Zwitter und die Zitterer", "tokens": ["Die", "Zwit\u00b7ter", "und", "die", "Zit\u00b7te\u00b7rer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie disputirten leise;", "tokens": ["Sie", "dis\u00b7pu\u00b7tir\u00b7ten", "lei\u00b7se", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie schlo\u00dfen Th\u00fcr und Fenster zu,", "tokens": ["Sie", "schlo\u00b7\u00dfen", "Th\u00fcr", "und", "Fens\u00b7ter", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und das war \u00e4u\u00dferst weise.", "tokens": ["Und", "das", "war", "\u00e4u\u00b7\u00dferst", "wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Vierte sprach: seid nicht zu schnell!", "tokens": ["Der", "Vier\u00b7te", "sprach", ":", "seid", "nicht", "zu", "schnell", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "VAFIN", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00fcbsch vorsichtig, ihr Leutchen!", "tokens": ["H\u00fcbsch", "vor\u00b7sich\u00b7tig", ",", "ihr", "Leut\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "PPOSAT", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Es ist noch manches Hinderni\u00df", "tokens": ["Es", "ist", "noch", "man\u00b7ches", "Hin\u00b7der\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr solch Ziel zu beseit'gen!", "tokens": ["F\u00fcr", "solch", "Ziel", "zu", "be\u00b7seit'\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Da dachten gleich die Zwitter nach", "tokens": ["Da", "dach\u00b7ten", "gleich", "die", "Zwit\u00b7ter", "nach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den letzten Interdicten;", "tokens": ["Den", "letz\u00b7ten", "In\u00b7ter\u00b7dic\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Zitt'rer aber sahen sich", "tokens": ["Die", "Zitt'\u00b7rer", "a\u00b7ber", "sa\u00b7hen", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bedenklich an und nickten.", "tokens": ["Be\u00b7denk\u00b7lich", "an", "und", "nick\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der F\u00fcnfte sprach: 's ist noch nicht Zeit;", "tokens": ["Der", "F\u00fcnf\u00b7te", "sprach", ":", "'s", "ist", "noch", "nicht", "Zeit", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "PTKNEG", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die F\u00fcrsten sind dagegen!", "tokens": ["Die", "F\u00fcrs\u00b7ten", "sind", "da\u00b7ge\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PAV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die beiden Letzten wollten sich", "tokens": ["Die", "bei\u00b7den", "Letz\u00b7ten", "woll\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VMFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sach' noch \u00fcberlegen.", "tokens": ["Die", "Sach'", "noch", "\u00fc\u00b7berl\u00b7e\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sie zankten leis' und zischelten,", "tokens": ["Sie", "zank\u00b7ten", "leis'", "und", "zi\u00b7schel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Zitt'rer und die Zwitter;", "tokens": ["Die", "Zitt'\u00b7rer", "und", "die", "Zwit\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie sahen scheu und duckten sich", "tokens": ["Sie", "sa\u00b7hen", "scheu", "und", "duck\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Schafe beim Gewitter.", "tokens": ["Wie", "Scha\u00b7fe", "beim", "Ge\u00b7wit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ein Secretair, der r\u00e4uspert' sich:", "tokens": ["Ein", "Se\u00b7cre\u00b7tair", ",", "der", "r\u00e4us\u00b7pert'", "sich", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da fuhr'n sie auseinander;", "tokens": ["Da", "fuhr'n", "sie", "aus\u00b7ein\u00b7an\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie hatten einen Schreck gekriegt,", "tokens": ["Sie", "hat\u00b7ten", "ei\u00b7nen", "Schreck", "ge\u00b7kriegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und zitterten selbander.", "tokens": ["Und", "zit\u00b7ter\u00b7ten", "sel\u00b7ban\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Die Zwitter und die Zitterer,", "tokens": ["Die", "Zwit\u00b7ter", "und", "die", "Zit\u00b7te\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie schwiegen nicht sehr lange;", "tokens": ["Sie", "schwie\u00b7gen", "nicht", "sehr", "lan\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch ward bei dem Politisirn", "tokens": ["Doch", "ward", "bei", "dem", "Po\u00b7li\u00b7ti\u00b7sirn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Den Meisten angst und bange.", "tokens": ["Den", "Meis\u00b7ten", "angst", "und", "ban\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Sie zischelten und zankten leis',", "tokens": ["Sie", "zi\u00b7schel\u00b7ten", "und", "zank\u00b7ten", "leis'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADJD", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Auf da\u00df es Niemand h\u00f6re;", "tokens": ["Auf", "da\u00df", "es", "Nie\u00b7mand", "h\u00f6\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ob die Gedankenfreiheit jetzt", "tokens": ["Ob", "die", "Ge\u00b7dan\u00b7ken\u00b7frei\u00b7heit", "jetzt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schon einzuf\u00fchren w\u00e4re.", "tokens": ["Schon", "ein\u00b7zu\u00b7f\u00fch\u00b7ren", "w\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}