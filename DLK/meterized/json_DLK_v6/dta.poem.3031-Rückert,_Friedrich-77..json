{"dta.poem.3031": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "77.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O wende dich an das, mein liebendes Gedicht,", "tokens": ["O", "wen\u00b7de", "dich", "an", "das", ",", "mein", "lie\u00b7ben\u00b7des", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "PDS", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Im Menschen, was vereint, an das, was trennet, nicht!", "tokens": ["Im", "Men\u00b7schen", ",", "was", "ver\u00b7eint", ",", "an", "das", ",", "was", "tren\u00b7net", ",", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "VVPP", "$,", "APPR", "PDS", "$,", "PWS", "VVFIN", "$,", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "An das nicht, was nur trennt, und ewige Trennung stiftet,", "tokens": ["An", "das", "nicht", ",", "was", "nur", "trennt", ",", "und", "e\u00b7wi\u00b7ge", "Tren\u00b7nung", "stif\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "PTKNEG", "$,", "PRELS", "ADV", "VVFIN", "$,", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Der beiden Welten Heil mit heiligem Gift vergiftet;", "tokens": ["Der", "bei\u00b7den", "Wel\u00b7ten", "Heil", "mit", "hei\u00b7li\u00b7gem", "Gift", "ver\u00b7gif\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Was als das einzige Heil f\u00fcr hier des Staates Norm", "tokens": ["Was", "als", "das", "ein\u00b7zi\u00b7ge", "Heil", "f\u00fcr", "hier", "des", "Staa\u00b7tes", "Norm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "KOKOM", "ART", "ADJA", "NN", "APPR", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Aufstellen und f\u00fcr dort will eines Glaubens Form;", "tokens": ["Auf\u00b7stel\u00b7len", "und", "f\u00fcr", "dort", "will", "ei\u00b7nes", "Glau\u00b7bens", "Form", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "ADV", "VMFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da\u00df vor dem heiligen unheiligen Kriege Frieden", "tokens": ["Da\u00df", "vor", "dem", "hei\u00b7li\u00b7gen", "un\u00b7hei\u00b7li\u00b7gen", "Krie\u00b7ge", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "ADJA", "NN", "NN"], "meter": "---+---+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und Gl\u00fcck zu finden sei nicht droben noch hienieden.", "tokens": ["Und", "Gl\u00fcck", "zu", "fin\u00b7den", "sei", "nicht", "dro\u00b7ben", "noch", "hien\u00b7ie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKZU", "VVINF", "VAFIN", "PTKNEG", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Von dieses Fiebers Frost, von dieses Fiebers Glut,", "tokens": ["Von", "die\u00b7ses", "Fie\u00b7bers", "Frost", ",", "von", "die\u00b7ses", "Fie\u00b7bers", "Glut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$,", "APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erstarrt der Menschheit Herz, versiegt ihr Lebensblut.", "tokens": ["Er\u00b7starrt", "der", "Menschheit", "Herz", ",", "ver\u00b7siegt", "ihr", "Le\u00b7bens\u00b7blut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "In diesen Todesfrost blas' einen warmen Hauch,", "tokens": ["In", "die\u00b7sen", "To\u00b7des\u00b7frost", "blas'", "ei\u00b7nen", "war\u00b7men", "Hauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und einen kl\u00e4renden in diesen dumpfen Rauch!", "tokens": ["Und", "ei\u00b7nen", "kl\u00e4\u00b7ren\u00b7den", "in", "die\u00b7sen", "dum\u00b7pfen", "Rauch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Das reine Menschliche im Menschen wend' hervor,", "tokens": ["Das", "rei\u00b7ne", "Menschli\u00b7che", "im", "Men\u00b7schen", "wend'", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der ewigen Sonne zu den Liebesfr\u00fchlingsflor!", "tokens": ["Der", "e\u00b7wi\u00b7gen", "Son\u00b7ne", "zu", "den", "Lie\u00b7bes\u00b7fr\u00fch\u00b7lings\u00b7flor", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}}}}