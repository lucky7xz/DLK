{"textgrid.poem.35530": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auch ich \u2013 auch ich, in unseligem Drang,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch ich \u2013 auch ich, in unseligem Drang,", "tokens": ["Auch", "ich", "\u2013", "auch", "ich", ",", "in", "un\u00b7se\u00b7li\u00b7gem", "Drang", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$(", "ADV", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.2": {"text": "Hab' mit zuckenden Fingern, so lang, so lang,", "tokens": ["Hab'", "mit", "zu\u00b7cken\u00b7den", "Fin\u00b7gern", ",", "so", "lang", ",", "so", "lang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Von verzehrendem Fieber zerspalten,", "tokens": ["Von", "ver\u00b7zeh\u00b7ren\u00b7dem", "Fie\u00b7ber", "zer\u00b7spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Gehascht nach des Ruhmes Lorbeergezweig,", "tokens": ["Ge\u00b7hascht", "nach", "des", "Ruh\u00b7mes", "Lor\u00b7beer\u00b7ge\u00b7zweig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Mit fliegendem Atem, ringerbleich,", "tokens": ["Mit", "flie\u00b7gen\u00b7dem", "A\u00b7tem", ",", "rin\u00b7ger\u00b7bleich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Eine \u00fcppige Krone zu halten!", "tokens": ["Ei\u00b7ne", "\u00fcp\u00b7pi\u00b7ge", "Kro\u00b7ne", "zu", "hal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Auch ich entrafft' mich dem heimischen Herd \u2013", "tokens": ["Auch", "ich", "ent\u00b7ra\u00b7fft'", "mich", "dem", "hei\u00b7mi\u00b7schen", "Herd", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PRF", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Was hat mich die Tr\u00e4ne der Mutter geschert,", "tokens": ["Was", "hat", "mich", "die", "Tr\u00e4\u00b7ne", "der", "Mut\u00b7ter", "ge\u00b7schert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Was Marias geschluchzte Klagen? \u2013", "tokens": ["Was", "Ma\u00b7rias", "ge\u00b7schluchz\u00b7te", "Kla\u00b7gen", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "NE", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Es trieb mich so wild, so st\u00fcrmisch hinaus", "tokens": ["Es", "trieb", "mich", "so", "wild", ",", "so", "st\u00fcr\u00b7misch", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD", "PTKVZ"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Auf des Lebens wei\u00dfsch\u00e4umigen Wogenbraus,", "tokens": ["Auf", "des", "Le\u00b7bens", "wei\u00df\u00b7sch\u00e4u\u00b7mi\u00b7gen", "Wo\u00b7gen\u00b7braus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Den strahlenden Ruhm zu erjagen.", "tokens": ["Den", "strah\u00b7len\u00b7den", "Ruhm", "zu", "er\u00b7ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Wie ward's mir so schw\u00fcl im umz\u00e4unten Kreis \u2013", "tokens": ["Wie", "ward's", "mir", "so", "schw\u00fcl", "im", "um\u00b7z\u00e4un\u00b7ten", "Kreis", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADJD", "APPRART", "ADJA", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Nach Atem rang ich \u2013 aus altem Geleis", "tokens": ["Nach", "A\u00b7tem", "rang", "ich", "\u2013", "aus", "al\u00b7tem", "Ge\u00b7leis"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$(", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Zog's mich in phantastischem Wahne!", "tokens": ["Zo\u00b7g's", "mich", "in", "phan\u00b7tas\u00b7ti\u00b7schem", "Wah\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Die Mutter hat mich gesegnet beim Zieh'n", "tokens": ["Die", "Mut\u00b7ter", "hat", "mich", "ge\u00b7seg\u00b7net", "beim", "Zieh'n"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "APPRART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und gab mir zum Abschied den Flammenrubin", "tokens": ["Und", "gab", "mir", "zum", "Ab\u00b7schied", "den", "Flam\u00b7men\u00b7ru\u00b7bin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Zum schirmenden Talismane.", "tokens": ["Zum", "schir\u00b7men\u00b7den", "Ta\u00b7lis\u00b7ma\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich spannte mir Fl\u00fcgel zum D\u00e4dalusflug \u2013", "tokens": ["Ich", "spann\u00b7te", "mir", "Fl\u00fc\u00b7gel", "zum", "D\u00e4\u00b7da\u00b7lus\u00b7flug", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "APPRART", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Nicht war mir ", "tokens": ["Nicht", "war", "mir"], "token_info": ["word", "word", "word"], "pos": ["PTKNEG", "VAFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Ich lechzte nach \u00fcpp'gem Gewinde ...", "tokens": ["Ich", "lechz\u00b7te", "nach", "\u00fcpp'\u00b7gem", "Ge\u00b7win\u00b7de", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Da brachten mir die T\u00f6chter der Lust", "tokens": ["Da", "brach\u00b7ten", "mir", "die", "T\u00f6ch\u00b7ter", "der", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Mit lachendem Auge, mit lockender Brust", "tokens": ["Mit", "la\u00b7chen\u00b7dem", "Au\u00b7ge", ",", "mit", "lo\u00b7cken\u00b7der", "Brust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Die s\u00fc\u00dfe, die lustige S\u00fcnde.", "tokens": ["Die", "s\u00fc\u00b7\u00dfe", ",", "die", "lus\u00b7ti\u00b7ge", "S\u00fcn\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Und ich trank und ich trank und lie\u00df die Spur,", "tokens": ["Und", "ich", "trank", "und", "ich", "trank", "und", "lie\u00df", "die", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und mit heldengro\u00dfer Siegerbravour", "tokens": ["Und", "mit", "hel\u00b7den\u00b7gro\u00b7\u00dfer", "Sie\u00b7ger\u00b7bra\u00b7vour"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Bracht' ich die Kom\u00f6die in Stanzen ...", "tokens": ["Bracht'", "ich", "die", "Ko\u00b7m\u00f6\u00b7die", "in", "Stan\u00b7zen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da nahten sie alle \u2013 be\u00e4ugelten links,", "tokens": ["Da", "nah\u00b7ten", "sie", "al\u00b7le", "\u2013", "be\u00b7\u00e4u\u00b7gel\u00b7ten", "links", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "$(", "VVFIN", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Be\u00e4ugelten rechts die schnurrige Sphinx", "tokens": ["Be\u00b7\u00e4u\u00b7gel\u00b7ten", "rechts", "die", "schnur\u00b7ri\u00b7ge", "Sphinx"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und kamen mir einen Ganzen.", "tokens": ["Und", "ka\u00b7men", "mir", "ei\u00b7nen", "Gan\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Holla hoch! Das war ein lustiges Fest \u2013", "tokens": ["Hol\u00b7la", "hoch", "!", "Das", "war", "ein", "lus\u00b7ti\u00b7ges", "Fest", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$.", "PDS", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Der Morgen ward mir weidlich durchn\u00e4\u00dft,", "tokens": ["Der", "Mor\u00b7gen", "ward", "mir", "weid\u00b7lich", "durc\u00b7hn\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und die Stirne schwamm in Wonne:", "tokens": ["Und", "die", "Stir\u00b7ne", "schwamm", "in", "Won\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie trug ja nun gl\u00e4nzende Lorbeerzier,", "tokens": ["Sie", "trug", "ja", "nun", "gl\u00e4n\u00b7zen\u00b7de", "Lor\u00b7be\u00b7er\u00b7zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Und sie trug sie mit W\u00fcrde, nicht blo\u00df zum Pl\u00e4sier \u2013", "tokens": ["Und", "sie", "trug", "sie", "mit", "W\u00fcr\u00b7de", ",", "nicht", "blo\u00df", "zum", "Pl\u00e4\u00b7sier", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "NN", "$,", "PTKNEG", "ADV", "APPRART", "NN", "$("], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Stolz leuchtete meine Sonne.", "tokens": ["Stolz", "leuch\u00b7te\u00b7te", "mei\u00b7ne", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Da kam auch f\u00fcr mich der Damaskustag \u2013", "tokens": ["Da", "kam", "auch", "f\u00fcr", "mich", "der", "Da\u00b7mas\u00b7kus\u00b7tag", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "PPER", "ART", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Binde fiel, und die brennende Schmach", "tokens": ["Die", "Bin\u00b7de", "fiel", ",", "und", "die", "bren\u00b7nen\u00b7de", "Schmach"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Schlug zischend mir in die Seele ...", "tokens": ["Schlug", "zi\u00b7schend", "mir", "in", "die", "See\u00b7le", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "O du Wahn! O du Wahn der Unsterblichkeit,", "tokens": ["O", "du", "Wahn", "!", "O", "du", "Wahn", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$.", "NE", "NE", "NN", "ART", "NN", "$,"], "meter": "+-++-+-+--+", "measure": "iambic.hexa.chol"}, "line.5": {"text": "Wenn ein wetterwendisch Gesindel schreit", "tokens": ["Wenn", "ein", "wet\u00b7ter\u00b7wen\u00b7disch", "Ge\u00b7sin\u00b7del", "schreit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJD", "NN", "NN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "In hochwillkommnem Krakehle:", "tokens": ["In", "hoch\u00b7will\u00b7komm\u00b7nem", "Kra\u00b7keh\u00b7le", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbder Kerl \u2013 bei Gott! \u2013 ist ein Pionier \u2013", "tokens": ["\u00bb", "der", "Kerl", "\u2013", "bei", "Gott", "!", "\u2013", "ist", "ein", "Pi\u00b7o\u00b7nier", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$(", "APPR", "NN", "$.", "$(", "VAFIN", "ART", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Prophet \u2013 Messias \u2013 ein Wundertier \u2013", "tokens": ["Pro\u00b7phet", "\u2013", "Mes\u00b7si\u00b7as", "\u2013", "ein", "Wun\u00b7der\u00b7tier", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "NE", "$(", "ART", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Er schreibt brillante Sachen!", "tokens": ["Er", "schreibt", "bril\u00b7lan\u00b7te", "Sa\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gedankentief und doch popul\u00e4r", "tokens": ["Ge\u00b7dan\u00b7ken\u00b7tief", "und", "doch", "po\u00b7pu\u00b7l\u00e4r"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADV", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Und so bilderreich! Und so schneidig wie er", "tokens": ["Und", "so", "bil\u00b7der\u00b7reich", "!", "Und", "so", "schnei\u00b7dig", "wie", "er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "$.", "KON", "ADV", "ADJD", "KOKOM", "PPER"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Kann keiner Verse machen!", "tokens": ["Kann", "kei\u00b7ner", "Ver\u00b7se", "ma\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wie w\u00e4r's drum, wir d\u00e4chten beizeiten schon", "tokens": ["Wie", "w\u00e4r's", "drum", ",", "wir", "d\u00e4ch\u00b7ten", "bei\u00b7zei\u00b7ten", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PAV", "$,", "PPER", "VVFIN", "PIS", "ADV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "An ein S\u00e4ulchen, ein Denkmal \u2013 die Nation! \u2013", "tokens": ["An", "ein", "S\u00e4ul\u00b7chen", ",", "ein", "Denk\u00b7mal", "\u2013", "die", "Na\u00b7tion", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", 