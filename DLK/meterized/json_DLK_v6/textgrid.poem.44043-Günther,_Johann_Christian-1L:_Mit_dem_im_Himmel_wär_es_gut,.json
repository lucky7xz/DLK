{"textgrid.poem.44043": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit dem im Himmel w\u00e4r es gut,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit dem im Himmel w\u00e4r es gut,", "tokens": ["Mit", "dem", "im", "Him\u00b7mel", "w\u00e4r", "es", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPRART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ach, wer vers\u00f6hnt mir den auf Erden?", "tokens": ["Ach", ",", "wer", "ver\u00b7s\u00f6hnt", "mir", "den", "auf", "Er\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "VVFIN", "PPER", "ART", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wofern es nicht die Liebe thut,", "tokens": ["Wo\u00b7fern", "es", "nicht", "die", "Lie\u00b7be", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird alles blind und fruchtlos werden,", "tokens": ["Wird", "al\u00b7les", "blind", "und", "frucht\u00b7los", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "KON", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wer glaubt wohl, hartes Vaterherz,", "tokens": ["Wer", "glaubt", "wohl", ",", "har\u00b7tes", "Va\u00b7ter\u00b7herz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df so viel Ungl\u00fcck, Flehn und Schmerz", "tokens": ["Da\u00df", "so", "viel", "Un\u00b7gl\u00fcck", ",", "Flehn", "und", "Schmerz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADV", "PIAT", "NN", "$,", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Eltern Blut nicht r\u00fchren sollen?", "tokens": ["Der", "El\u00b7tern", "Blut", "nicht", "r\u00fch\u00b7ren", "sol\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich d\u00e4cht, ich h\u00e4tt in kurzer Zeit", "tokens": ["Ich", "d\u00e4cht", ",", "ich", "h\u00e4tt", "in", "kur\u00b7zer", "Zeit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die allerh\u00e4rtste Grausamkeit", "tokens": ["Die", "al\u00b7ler\u00b7h\u00e4rts\u00b7te", "Grau\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Blos durch mein Elend beugen wollen.", "tokens": ["Blos", "durch", "mein", "E\u00b7lend", "beu\u00b7gen", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich bin und bin auch nicht verwaist;", "tokens": ["Ich", "bin", "und", "bin", "auch", "nicht", "ver\u00b7waist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "VAFIN", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dies R\u00e4thsel kostet mich viel Thr\u00e4nen.", "tokens": ["Dies", "R\u00e4th\u00b7sel", "kos\u00b7tet", "mich", "viel", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ach Vater, bistu, was du heist,", "tokens": ["Ach", "Va\u00b7ter", ",", "bis\u00b7tu", ",", "was", "du", "heist", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "ADV", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So h\u00f6re mein gerechtes Sehnen.", "tokens": ["So", "h\u00f6\u00b7re", "mein", "ge\u00b7rech\u00b7tes", "Seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich k\u00fc\u00dfe dich mit Mund und Hand;", "tokens": ["Ich", "k\u00fc\u00b7\u00dfe", "dich", "mit", "Mund", "und", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du kanst ja wohl dies Ehrfurchtspfand", "tokens": ["Du", "kanst", "ja", "wohl", "dies", "Ehr\u00b7furchts\u00b7pfand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PDS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht ganz und gar zur\u00fcckeschlagen.", "tokens": ["Nicht", "ganz", "und", "gar", "zu\u00b7r\u00fc\u00b7ckesc\u00b7hla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Verschm\u00e4hst auch du dies L\u00f6segeld,", "tokens": ["Ver\u00b7schm\u00e4hst", "auch", "du", "dies", "L\u00f6\u00b7se\u00b7geld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "PDS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Zu welchem soll ich auf der Welt", "tokens": ["Zu", "wel\u00b7chem", "soll", "ich", "auf", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mehr Neigung, Herz und Zuflucht tragen?", "tokens": ["Mehr", "Nei\u00b7gung", ",", "Herz", "und", "Zu\u00b7flucht", "tra\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich bitte, pr\u00fcfe Straf und Schuld.", "tokens": ["Ich", "bit\u00b7te", ",", "pr\u00fc\u00b7fe", "Straf", "und", "Schuld", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Eifer streckt sich in die L\u00e4nge,", "tokens": ["Dein", "Ei\u00b7fer", "streckt", "sich", "in", "die", "L\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er stiehlt mir aller G\u00f6nner Huld,", "tokens": ["Er", "stiehlt", "mir", "al\u00b7ler", "G\u00f6n\u00b7ner", "Huld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er mehrt der Feinde Spott und Menge,", "tokens": ["Er", "mehrt", "der", "Fein\u00b7de", "Spott", "und", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mein k\u00fcnftig Wohlseyn geht in Grund.", "tokens": ["Mein", "k\u00fcnf\u00b7tig", "Wohl\u00b7seyn", "geht", "in", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verleumdet uns der Eltern Mund,", "tokens": ["Ver\u00b7leum\u00b7det", "uns", "der", "El\u00b7tern", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was wollen Fremde thun und glauben?", "tokens": ["Was", "wol\u00b7len", "Frem\u00b7de", "thun", "und", "glau\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Beh\u00e4lt dein Herz noch eine Spur", "tokens": ["Be\u00b7h\u00e4lt", "dein", "Herz", "noch", "ei\u00b7ne", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der ehmahls g\u00fctigen Natur,", "tokens": ["Der", "eh\u00b7mahls", "g\u00fc\u00b7ti\u00b7gen", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So mustu mir die Frag erlauben:", "tokens": ["So", "mus\u00b7tu", "mir", "die", "Frag", "er\u00b7lau\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wer s\u00fcndigt mit Entschuldigung,", "tokens": ["Wer", "s\u00fcn\u00b7digt", "mit", "Ent\u00b7schul\u00b7di\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Der alle Rechte Statt verg\u00f6nnen?", "tokens": ["Der", "al\u00b7le", "Rech\u00b7te", "Statt", "ver\u00b7g\u00f6n\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Strafe dient zur Be\u00dferung,", "tokens": ["Die", "Stra\u00b7fe", "dient", "zur", "Be\u00b7\u00dfe\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Ja, wenn wir sie gebrauchen k\u00f6nnen;", "tokens": ["Ja", ",", "wenn", "wir", "sie", "ge\u00b7brau\u00b7chen", "k\u00f6n\u00b7nen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "PPER", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Allein, wer gar zu Boden liegt", "tokens": ["Al\u00b7lein", ",", "wer", "gar", "zu", "Bo\u00b7den", "liegt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWS", "ADV", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und nirgends Rath noch H\u00fclfe kriegt,", "tokens": ["Und", "nir\u00b7gends", "Rath", "noch", "H\u00fcl\u00b7fe", "kriegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der ist den Krancken beyzuzehlen,", "tokens": ["Der", "ist", "den", "Kran\u00b7cken", "bey\u00b7zu\u00b7zeh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die, wenn der Brand das Haupt gewinnt,", "tokens": ["Die", ",", "wenn", "der", "Brand", "das", "Haupt", "ge\u00b7winnt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ohn eigne Schuld vernunftlos sind", "tokens": ["Ohn", "eig\u00b7ne", "Schuld", "ver\u00b7nunft\u00b7los", "sind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und Gift vor Mithridat erwehlen.", "tokens": ["Und", "Gift", "vor", "Mith\u00b7ri\u00b7dat", "er\u00b7weh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was bringen dich vor Laster auf,", "tokens": ["Was", "brin\u00b7gen", "dich", "vor", "Las\u00b7ter", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und was vor Bo\u00dfheit reizt die Rache?", "tokens": ["Und", "was", "vor", "Bo\u00df\u00b7heit", "reizt", "die", "Ra\u00b7che", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was ist, wodurch mein Lebenslauf", "tokens": ["Was", "ist", ",", "wo\u00b7durch", "mein", "Le\u00b7bens\u00b7lauf"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "PWAV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Eltern Zucht zu Schanden mache?", "tokens": ["Der", "El\u00b7tern", "Zucht", "zu", "Schan\u00b7den", "ma\u00b7che", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich falle, ja, wie jeder f\u00e4llt,", "tokens": ["Ich", "fal\u00b7le", ",", "ja", ",", "wie", "je\u00b7der", "f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PTKANT", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Fleisch und Jugend Neze stellt;", "tokens": ["Dem", "Fleisch", "und", "Ju\u00b7gend", "Ne\u00b7ze", "stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und h\u00e4tt ich etwas Grobs begangen,", "tokens": ["Und", "h\u00e4tt", "ich", "et\u00b7was", "Grobs", "be\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So w\u00fcrde nach bewiesner That", "tokens": ["So", "w\u00fcr\u00b7de", "nach", "be\u00b7wi\u00b7es\u00b7ner", "That"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Ein Strafbrief und geheimer Rath", "tokens": ["Ein", "Straf\u00b7brief", "und", "ge\u00b7hei\u00b7mer", "Rath"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Viel mehr als Fluch und Schimpf verfangen.", "tokens": ["Viel", "mehr", "als", "Fluch", "und", "Schimpf", "ver\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Was zwischen uns vor Streit geschehn,", "tokens": ["Was", "zwi\u00b7schen", "uns", "vor", "Streit", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was darf denn dies die Misgunst h\u00f6ren?", "tokens": ["Was", "darf", "denn", "dies", "die", "Mis\u00b7gunst", "h\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "PDS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie wird sich desto stolzer blehn,", "tokens": ["Sie", "wird", "sich", "des\u00b7to", "stol\u00b7zer", "blehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch dir gereicht es nicht zu Ehren,", "tokens": ["Auch", "dir", "ge\u00b7reicht", "es", "nicht", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word"