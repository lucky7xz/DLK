{"dta.poem.5973": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Bau der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Erd im Gegentheil, die Regungs-los und schwer,", "tokens": ["Die", "Erd", "im", "Ge\u00b7gen\u00b7theil", ",", "die", "Re\u00b7gungs\u00b7los", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "ART", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die sich unsern Augen zeiget,", "tokens": ["Und", "die", "sich", "un\u00b7sern", "Au\u00b7gen", "zei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als wenn sie dunckel, dicht, materialisch w\u00e4r;", "tokens": ["Als", "wenn", "sie", "dun\u00b7ckel", ",", "dicht", ",", "ma\u00b7te\u00b7ri\u00b7a\u00b7lisch", "w\u00e4r", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADJD", "$,", "ADJD", "$,", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Scheint in der Lufft, die fl\u00fcssig ist und rein,", "tokens": ["Scheint", "in", "der", "Lufft", ",", "die", "fl\u00fcs\u00b7sig", "ist", "und", "rein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "VAFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein grober runder Klump zu seyn.", "tokens": ["Ein", "gro\u00b7ber", "run\u00b7der", "Klump", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Jhr Ganzes war vermuhtlich anfangs nicht", "tokens": ["Ihr", "Gan\u00b7zes", "war", "ver\u00b7muht\u00b7lich", "an\u00b7fangs", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "ADV", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So fest und dicht:", "tokens": ["So", "fest", "und", "dicht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "KON", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Es war ein ungleich-luckrer Hauffeu", "tokens": ["Es", "war", "ein", "un\u00b7gleich\u00b7luck\u00b7rer", "Hauf\u00b7feu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Von St\u00e4ubgen, die theils lang, und theils gekr\u00fcmmt,", "tokens": ["Von", "St\u00e4ub\u00b7gen", ",", "die", "theils", "lang", ",", "und", "theils", "ge\u00b7kr\u00fcmmt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "ADJD", "$,", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Theils voller Aest\u2019 und theils gewunden.", "tokens": ["Theils", "vol\u00b7ler", "A\u00b7est'", "und", "theils", "ge\u00b7wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "KON", "ADV", "ADJD", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Wenn solcher C\u00f6rper Meng\u2019 in weichen L\u00fcfften schwimmt,", "tokens": ["Wenn", "sol\u00b7cher", "C\u00f6r\u00b7per", "Meng'", "in", "wei\u00b7chen", "L\u00fcff\u00b7ten", "schwimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "NE", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sind sie gepresst, geh\u00e4ckelt und verbunden,", "tokens": ["Sind", "sie", "ge\u00b7presst", ",", "ge\u00b7h\u00e4c\u00b7kelt", "und", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Wenn eines nun sich in das andre dringt,", "tokens": ["Wenn", "ei\u00b7nes", "nun", "sich", "in", "das", "and\u00b7re", "dringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PRF", "APPR", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und sich zusammen schlingt;", "tokens": ["Und", "sich", "zu\u00b7sam\u00b7men", "schlingt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "H\u00e4ngt eines stets sich an das ander\u2019 an,", "tokens": ["H\u00e4ngt", "ei\u00b7nes", "stets", "sich", "an", "das", "an\u00b7der'", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVFIN", "PRF", "APPR", "ART", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Wodurch denn, da die Masse sich vermehret,", "tokens": ["Wo\u00b7durch", "denn", ",", "da", "die", "Mas\u00b7se", "sich", "ver\u00b7meh\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "KOUS", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Jhr eigner Druck und ihr Gewicht verwehret", "tokens": ["Ihr", "eig\u00b7ner", "Druck", "und", "ihr", "Ge\u00b7wicht", "ver\u00b7weh\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Da\u00df sie sich nicht mehr drehn und regen kan.", "tokens": ["Da\u00df", "sie", "sich", "nicht", "mehr", "drehn", "und", "re\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "ADV", "VVINF", "KON", "ADJA", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Die z\u00e4serichte C\u00f6rperlein,", "tokens": ["Die", "z\u00e4\u00b7se\u00b7rich\u00b7te", "C\u00f6r\u00b7per\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Die von der \u00e4ussern Creyse R\u00fcnden", "tokens": ["Die", "von", "der", "\u00e4us\u00b7sern", "Crey\u00b7se", "R\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Zusammen stets gedr\u00fcckt, gepresst, getrieben seyn,", "tokens": ["Zu\u00b7sam\u00b7men", "stets", "ge\u00b7dr\u00fcckt", ",", "ge\u00b7presst", ",", "ge\u00b7trie\u00b7ben", "seyn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "$,", "VVPP", "$,", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die m\u00fcssen sich vereinen und verbinden.", "tokens": ["Die", "m\u00fcs\u00b7sen", "sich", "ver\u00b7ei\u00b7nen", "und", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PRF", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Dahero durch des Druckes-Krafft", "tokens": ["Da\u00b7he\u00b7ro", "durch", "des", "Dru\u00b7ckes\u00b7Krafft"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "APPR", "ART", "NN"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.24": {"text": "Der sie umgebenden und fl\u00fcss\u2019gen Eigenschafft", "tokens": ["Der", "sie", "um\u00b7ge\u00b7ben\u00b7den", "und", "fl\u00fcss'\u00b7gen", "Ei\u00b7gen\u00b7schafft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVPP", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wir sie im Mittel-Punct, als eine Kugel, finden.", "tokens": ["Wir", "sie", "im", "Mit\u00b7tel\u00b7Punct", ",", "als", "ei\u00b7ne", "Ku\u00b7gel", ",", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PPER", "APPRART", "NN", "$,", "KOUS", "ART", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}