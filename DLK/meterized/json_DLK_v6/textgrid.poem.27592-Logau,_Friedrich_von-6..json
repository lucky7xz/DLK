{"textgrid.poem.27592": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nacht ist nun dahin; die Sonn ist wieder kommen;", "tokens": ["Die", "Nacht", "ist", "nun", "da\u00b7hin", ";", "die", "Sonn", "ist", "wie\u00b7der", "kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PAV", "$.", "ART", "NN", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Schlaf, de\u00df Todes Bild, ist weg von uns genommen.", "tokens": ["Der", "Schlaf", ",", "de\u00df", "To\u00b7des", "Bild", ",", "ist", "weg", "von", "uns", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN", "$,", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Herr Gott, du reines Liecht, la\u00df ferne von mir sein", "tokens": ["Herr", "Gott", ",", "du", "rei\u00b7nes", "Liecht", ",", "la\u00df", "fer\u00b7ne", "von", "mir", "sein"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "PPER", "ADJA", "NN", "$,", "VVIMP", "ADV", "APPR", "PPER", "PPOSAT"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der S\u00fcnden finstre Werk und gib mir deinen Schein!", "tokens": ["Der", "S\u00fcn\u00b7den", "finst\u00b7re", "Werk", "und", "gib", "mir", "dei\u00b7nen", "Schein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "KON", "VVIMP", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "La\u00df mich dein werthes Wort frei \u00f6ffentlich bekennen;", "tokens": ["La\u00df", "mich", "dein", "wert\u00b7hes", "Wort", "frei", "\u00f6f\u00b7fent\u00b7lich", "be\u00b7ken\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJA", "NN", "ADJD", "ADJD", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "La\u00df mich in deiner Lieb und meines Nechsten brennen;", "tokens": ["La\u00df", "mich", "in", "dei\u00b7ner", "Lieb", "und", "mei\u00b7nes", "Nechs\u00b7ten", "bren\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "La\u00df meinen Sinn und Geist seyn wacker f\u00fcr und f\u00fcr", "tokens": ["La\u00df", "mei\u00b7nen", "Sinn", "und", "Geist", "seyn", "wa\u00b7cker", "f\u00fcr", "und", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "KON", "NN", "PPOSAT", "ADJD", "APPR", "KON", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Zu thun, was mir geb\u00fchrt und wol gef\u00e4llet dir!", "tokens": ["Zu", "thun", ",", "was", "mir", "ge\u00b7b\u00fchrt", "und", "wol", "ge\u00b7f\u00e4l\u00b7let", "dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PWS", "PPER", "VVPP", "KON", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und so mein m\u00fcder Leib noch l\u00e4nger soll beschauen", "tokens": ["Und", "so", "mein", "m\u00fc\u00b7der", "Leib", "noch", "l\u00e4n\u00b7ger", "soll", "be\u00b7schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPOSAT", "ADJA", "NN", "ADV", "ADJD", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das Unrecht dieser Welt und dieses Elend bauen:", "tokens": ["Das", "Un\u00b7recht", "die\u00b7ser", "Welt", "und", "die\u00b7ses", "E\u00b7lend", "bau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "KON", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Herr Gott, so gib Geduld, verleih' best\u00e4ndigkeit;", "tokens": ["Herr", "Gott", ",", "so", "gib", "Ge\u00b7duld", ",", "ver\u00b7leih'", "be\u00b7st\u00e4n\u00b7dig\u00b7keit", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADV", "VVIMP", "NN", "$,", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "La\u00df scheinen deinen Trost und hilff zu rechter Zeit!", "tokens": ["La\u00df", "schei\u00b7nen", "dei\u00b7nen", "Trost", "und", "hilff", "zu", "rech\u00b7ter", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PPOSAT", "NN", "KON", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "La\u00df mir mein Augen nicht von eitlen Dingen blenden,", "tokens": ["La\u00df", "mir", "mein", "Au\u00b7gen", "nicht", "von", "eit\u00b7len", "Din\u00b7gen", "blen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "PTKNEG", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Nach k\u00f6stlich Ding der Welt von dir mein Hertze wenden;", "tokens": ["Nach", "k\u00f6st\u00b7lich", "Ding", "der", "Welt", "von", "dir", "mein", "Hert\u00b7ze", "wen\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "APPR", "PPER", "PPOSAT", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Hilff, da\u00df ich mich nicht theil und bleibe gantz an dir,", "tokens": ["Hilff", ",", "da\u00df", "ich", "mich", "nicht", "theil", "und", "blei\u00b7be", "gantz", "an", "dir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "ADJD", "KON", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Auff da\u00df du, h\u00f6chstes Gutt, m\u00f6gst bleiben auch in mir!", "tokens": ["Auff", "da\u00df", "du", ",", "h\u00f6chs\u00b7tes", "Gutt", ",", "m\u00f6gst", "blei\u00b7ben", "auch", "in", "mir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "$,", "ADJA", "NN", "$,", "VMFIN", "VVINF", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wenn endlich denn mein Liecht und Leben mu\u00df vergehen,", "tokens": ["Wenn", "end\u00b7lich", "denn", "mein", "Liecht", "und", "Le\u00b7ben", "mu\u00df", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "KON", "PPOSAT", "NN", "KON", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So la\u00df mich dort gantz sch\u00f6n und wie verkl\u00e4ret stehen", "tokens": ["So", "la\u00df", "mich", "dort", "gantz", "sch\u00f6n", "und", "wie", "ver\u00b7kl\u00e4\u00b7ret", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "ADV", "ADJD", "KON", "PWAV", "VVPP", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da, wo du Sonnenstral, voll von Gerechtigkeit,", "tokens": ["Da", ",", "wo", "du", "Son\u00b7nen\u00b7stral", ",", "voll", "von", "Ge\u00b7rech\u00b7tig\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Sch\u00f6n hell erleuchten wirst die selig Ewigkeit!", "tokens": ["Sch\u00f6n", "hell", "er\u00b7leuch\u00b7ten", "wirst", "die", "se\u00b7lig", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVINF", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}