{"textgrid.poem.37751": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wacht auf mit innern Sinnen,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wacht auf mit innern Sinnen,", "tokens": ["Wacht", "auf", "mit", "in\u00b7nern", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erhebt die Augenlieder,", "tokens": ["Er\u00b7hebt", "die", "Au\u00b7gen\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von denen Thr\u00e4nen rinnen,", "tokens": ["Von", "de\u00b7nen", "Thr\u00e4\u00b7nen", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Innen strahlts hernieder:", "tokens": ["Von", "In\u00b7nen", "strahlts", "her\u00b7nie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In tiefe Kerkernacht", "tokens": ["In", "tie\u00b7fe", "Ker\u00b7ker\u00b7nacht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Unsichtbar Lauernden,", "tokens": ["Un\u00b7sicht\u00b7bar", "Lau\u00b7ern\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.7": {"text": "Strahlt frei des Herren Macht,", "tokens": ["Strahlt", "frei", "des", "Her\u00b7ren", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Unschuldig Trauernden.", "tokens": ["Un\u00b7schul\u00b7dig", "Trau\u00b7ern\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "In Geistes D\u00e4mmerungen", "tokens": ["In", "Geis\u00b7tes", "D\u00e4m\u00b7me\u00b7run\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Naht euch der Unerreichte,", "tokens": ["Naht", "euch", "der", "Un\u00b7er\u00b7reich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hat euer Herz durchdrungen,", "tokens": ["Hat", "eu\u00b7er", "Herz", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df Geist vom Geiste leuchte;", "tokens": ["Da\u00df", "Geist", "vom", "Geis\u00b7te", "leuch\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In seiner Gnade Macht", "tokens": ["In", "sei\u00b7ner", "Gna\u00b7de", "Macht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Strahlt der Verachtete,", "tokens": ["Strahlt", "der", "Ver\u00b7ach\u00b7te\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Er hat ans Licht gebracht,", "tokens": ["Er", "hat", "ans", "Licht", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Schuldlos Umnachtete.", "tokens": ["Schuld\u00b7los", "Um\u00b7nach\u00b7te\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "++-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr hebt die tr\u00fcben Blicke", "tokens": ["Ihr", "hebt", "die", "tr\u00fc\u00b7ben", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hinauf zu dunklen Fernen,", "tokens": ["Hin\u00b7auf", "zu", "dunk\u00b7len", "Fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie bauen euch die Br\u00fccke,", "tokens": ["Sie", "bau\u00b7en", "euch", "die", "Br\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus ewgen Himmelssternen:", "tokens": ["Aus", "ew\u00b7gen", "Him\u00b7melss\u00b7ter\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein jeder Blick zum Herrn", "tokens": ["Ein", "je\u00b7der", "Blick", "zum", "Herrn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Vom still Erliegenden,", "tokens": ["Vom", "still", "Er\u00b7lie\u00b7gen\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.7": {"text": "Gl\u00e4nzt hell als ew'ger Stern", "tokens": ["Gl\u00e4nzt", "hell", "als", "ew'\u00b7ger", "Stern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Am Thron des Siegenden.", "tokens": ["Am", "Thron", "des", "Sie\u00b7gen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}}, "stanza.4": {"line.1": {"text": "Er braucht nicht Menschenh\u00e4nde,", "tokens": ["Er", "braucht", "nicht", "Men\u00b7schen\u00b7h\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit seinen Gnadenworten", "tokens": ["Mit", "sei\u00b7nen", "Gna\u00b7den\u00b7wor\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Durchbricht er Kerkerw\u00e4nde,", "tokens": ["Durch\u00b7bricht", "er", "Ker\u00b7ker\u00b7w\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und \u00f6ffnet Himmelspforten:", "tokens": ["Und", "\u00f6ff\u00b7net", "Him\u00b7mel\u00b7spfor\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was euch geschieht auf Erden,", "tokens": ["Was", "euch", "ge\u00b7schieht", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihr schuldlos Leidenden,", "tokens": ["Ihr", "schuld\u00b7los", "Lei\u00b7den\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.7": {"text": "Wird reich verg\u00fctigt werden", "tokens": ["Wird", "reich", "ver\u00b7g\u00fc\u00b7tigt", "wer\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "VVPP", "VAINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Euch selig Scheidenden.", "tokens": ["Euch", "se\u00b7lig", "Schei\u00b7den\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}}}}}