{"textgrid.poem.24754": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich ging den Weg einmal: da war ich sieben,", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ging den Weg einmal: da war ich sieben,", "tokens": ["Ich", "ging", "den", "Weg", "ein\u00b7mal", ":", "da", "war", "ich", "sie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$.", "ADV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So arm und reich!", "tokens": ["So", "arm", "und", "reich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mir war, ich hielt ein nacktes Schwert in H\u00e4nden", "tokens": ["Mir", "war", ",", "ich", "hielt", "ein", "nack\u00b7tes", "Schwert", "in", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und selbst die Sterne bebten seinem Streich.", "tokens": ["Und", "selbst", "die", "Ster\u00b7ne", "beb\u00b7ten", "sei\u00b7nem", "Streich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mit siebzehn ging ich wiederum den Weg", "tokens": ["Mit", "sieb\u00b7zehn", "ging", "ich", "wie\u00b7de\u00b7rum", "den", "Weg"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erst recht allein:", "tokens": ["Erst", "recht", "al\u00b7lein", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ein Etwas huschte in den blassen Winden,", "tokens": ["Ein", "Et\u00b7was", "huschte", "in", "den", "blas\u00b7sen", "Win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von oben kam der fremden Welten Schein.", "tokens": ["Von", "o\u00b7ben", "kam", "der", "frem\u00b7den", "Wel\u00b7ten", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun f\u00fchr ich dich, du sp\u00fcrst nur meine Hand:", "tokens": ["Nun", "f\u00fchr", "ich", "dich", ",", "du", "sp\u00fcrst", "nur", "mei\u00b7ne", "Hand", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Einst war ich sieben ...", "tokens": ["Einst", "war", "ich", "sie\u00b7ben", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und das Vergangne glimmt, von Geisterhand", "tokens": ["Und", "das", "Ver\u00b7gang\u00b7ne", "glimmt", ",", "von", "Geis\u00b7ter\u00b7hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit blassem Schein ins Dunkel hingeschrieben!", "tokens": ["Mit", "blas\u00b7sem", "Schein", "ins", "Dun\u00b7kel", "hin\u00b7ge\u00b7schrie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}