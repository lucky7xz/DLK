{"textgrid.poem.53329": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Klage \u00fcber Menschliche Hinf\u00e4lligkeit. Als Herr George Blum, Churfl. Pr. Cantzley-Verwandter in Gott entschlaffen", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist Zeit vnd Welt?", "tokens": ["Was", "ist", "Zeit", "vnd", "Welt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Was jhr schn\u00f6des Wesen,", "tokens": ["Was", "jhr", "schn\u00f6\u00b7des", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ansehn, Kunst vnd Geld?", "tokens": ["An\u00b7sehn", ",", "Kunst", "vnd", "Geld", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVINF", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Nichts ist au\u00dferlesen.", "tokens": ["Nichts", "ist", "au\u00b7\u00dfer\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Vnbestand vnd Fall", "tokens": ["Vn\u00b7be\u00b7stand", "vnd", "Fall"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Herscht nur \u00fcberal.", "tokens": ["Herscht", "nur", "\u00fc\u00b7be\u00b7ral", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Keine Hoffnung sol", "tokens": ["Kei\u00b7ne", "Hoff\u00b7nung", "sol"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VMFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Vns den Muth erheben,", "tokens": ["Vns", "den", "Muth", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Taug auch etwas wol", "tokens": ["Taug", "auch", "et\u00b7was", "wol"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Trost in Noht zu geben", "tokens": ["Trost", "in", "Noht", "zu", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ist das Ruder fort,", "tokens": ["Ist", "das", "Ru\u00b7der", "fort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Ohn des Herrn Wort?", "tokens": ["Ohn", "des", "Herrn", "Wort", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-++", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Nichts sonst, was es sey,", "tokens": ["Nichts", "sonst", ",", "was", "es", "sey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "$,", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "S\u00e4ttigt ein Gem\u00fcthe,", "tokens": ["S\u00e4t\u00b7tigt", "ein", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Alles Fleisch ist Hew,", "tokens": ["Al\u00b7les", "Fleisch", "ist", "Hew", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Alle seine G\u00fcte,", "tokens": ["Al\u00b7le", "sei\u00b7ne", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Seine Zier, sein Ruhm", "tokens": ["Sei\u00b7ne", "Zier", ",", "sein", "Ruhm"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Eine Wiesen-Blum.", "tokens": ["Ei\u00b7ne", "Wie\u00b7sen\u00b7Blum", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Herrlich pranget zwar", "tokens": ["Herr\u00b7lich", "pran\u00b7get", "zwar"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Eine Blum im Lentzen,", "tokens": ["Ei\u00b7ne", "Blum", "im", "Lent\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Die auch unser Haar", "tokens": ["Die", "auch", "un\u00b7ser", "Haar"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Artig kan bekr\u00e4ntzen.", "tokens": ["Ar\u00b7tig", "kan", "be\u00b7kr\u00b7\u00e4nt\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Auch wird Gras vnd Kraut", "tokens": ["Auch", "wird", "Gras", "vnd", "Kraut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NN", "KON", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Lieblich angeschaut;", "tokens": ["Lieb\u00b7lich", "an\u00b7ge\u00b7schaut", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Wann jhr Stoltz nun meist", "tokens": ["Wann", "jhr", "Stoltz", "nun", "meist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Sich beginnt zu blehen", "tokens": ["Sich", "be\u00b7ginnt", "zu", "ble\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "VVFIN", "PTKZU", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Vnd des Herrn Geist", "tokens": ["Vnd", "des", "Herrn", "Geist"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Anhebt drein zu wehen,", "tokens": ["An\u00b7hebt", "drein", "zu", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "So ist alles bald", "tokens": ["So", "ist", "al\u00b7les", "bald"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Welck vnd ungestalt:", "tokens": ["Welck", "vnd", "un\u00b7ge\u00b7stalt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Also sind auch wir,", "tokens": ["Al\u00b7so", "sind", "auch", "wir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "O wir armen Leute!", "tokens": ["O", "wir", "ar\u00b7men", "Leu\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Vnsers Lebens Zier", "tokens": ["Vn\u00b7sers", "Le\u00b7bens", "Zier"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Br\u00fcstet sich zwar heute", "tokens": ["Br\u00fcs\u00b7tet", "sich", "zwar", "heu\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADV", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Vnd ist Rosen-roht,", "tokens": ["Vnd", "ist", "Ro\u00b7sen\u00b7roht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Morgen kranck vnd todt.", "tokens": ["Mor\u00b7gen", "kranck", "vnd", "todt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Nur was Gottes Mund", "tokens": ["Nur", "was", "Got\u00b7tes", "Mund"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PWS", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Trewlich vns versprochen,", "tokens": ["Trew\u00b7lich", "vns", "ver\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Hat bewehrten Grund", "tokens": ["Hat", "be\u00b7wehr\u00b7ten", "Grund"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Vnd bleibt ungebrochen,", "tokens": ["Vnd", "bleibt", "un\u00b7ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wenn nun gleich die Welt", "tokens": ["Wenn", "nun", "gleich", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "In einander f\u00e4llt.", "tokens": ["In", "ein\u00b7an\u00b7der", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Was denn ist das Wort", "tokens": ["Was", "denn", "ist", "das", "Wort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VAFIN", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Das so fest bekleibet?", "tokens": ["Das", "so", "fest", "be\u00b7klei\u00b7bet", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df Er unser Hort", "tokens": ["Da\u00df", "Er", "un\u00b7ser", "Hort"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Stets in Christo bleibet,", "tokens": ["Stets", "in", "Chris\u00b7to", "blei\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Vnd zu aller frist", "tokens": ["Vnd", "zu", "al\u00b7ler", "frist"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Vnser Leben ist.", "tokens": ["Vn\u00b7ser", "Le\u00b7ben", "ist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}