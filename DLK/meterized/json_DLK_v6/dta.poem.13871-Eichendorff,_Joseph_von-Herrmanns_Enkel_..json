{"dta.poem.13871": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Herrmanns Enkel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "'s ist so \u00fcber'm schlanken Leibe", "tokens": ["'s", "ist", "so", "\u00fc\u00b7ber'm", "schlan\u00b7ken", "Lei\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ein Regenschirm gespannt,", "tokens": ["Wie", "ein", "Re\u00b7gen\u00b7schirm", "ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oben drauf dann, statt dem Knopf,", "tokens": ["O\u00b7ben", "drauf", "dann", ",", "statt", "dem", "Knopf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "ADV", "$,", "KOUI", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwebt der holde Frauenkopf,", "tokens": ["Schwebt", "der", "hol\u00b7de", "Frau\u00b7en\u00b7kopf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In das Bl\u00fctenmeer von Kragen,", "tokens": ["In", "das", "Bl\u00fc\u00b7ten\u00b7meer", "von", "Kra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ariadnen gleich, verschlagen. \u2014", "tokens": ["A\u00b7riad\u00b7nen", "gleich", ",", "ver\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["NE", "ADV", "$,", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "O, und ein moral'scher Kragen!", "tokens": ["O", ",", "und", "ein", "mo\u00b7ral'\u00b7scher", "Kra\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Denn wer ist da so gewandt,", "tokens": ["Denn", "wer", "ist", "da", "so", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Fl\u00fcsternd was in's Ohr zu sagen,", "tokens": ["Fl\u00fcs\u00b7ternd", "was", "in's", "Ohr", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PRELS", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Was nicht gleich die Andern wissen?", "tokens": ["Was", "nicht", "gleich", "die", "An\u00b7dern", "wis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ADV", "ART", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und \u2014 unm\u00f6glich ist das K\u00fcssen!", "tokens": ["Und", "un\u00b7m\u00f6g\u00b7lich", "ist", "das", "K\u00fcs\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}