{"textgrid.poem.61868": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich bin, der ich bin,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin, der ich bin,", "tokens": ["Ich", "bin", ",", "der", "ich", "bin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Und lernt' ich von vielen:", "tokens": ["Und", "lernt'", "ich", "von", "vie\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PIAT", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Nach eigensten Zielen", "tokens": ["Nach", "ei\u00b7gen\u00b7sten", "Zie\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Stand immer mein Sinn.", "tokens": ["Stand", "im\u00b7mer", "mein", "Sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Ein Strahl Poesie", "tokens": ["Ein", "Strahl", "Poe\u00b7sie"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Beschien mir die Pfade,", "tokens": ["Be\u00b7schien", "mir", "die", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ich sp\u00fcrt' ihn als Gnade", "tokens": ["Ich", "sp\u00fcrt'", "ihn", "als", "Gna\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KOUS", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und r\u00fchmte mich nie.", "tokens": ["Und", "r\u00fchm\u00b7te", "mich", "nie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Und hat sich's gef\u00fcgt,", "tokens": ["Und", "hat", "sich's", "ge\u00b7f\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Und la\u00dft ihr mich gelten,", "tokens": ["Und", "la\u00dft", "ihr", "mich", "gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PRF", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "So glaubt, da\u00df ich selten", "tokens": ["So", "glaubt", ",", "da\u00df", "ich", "sel\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mir selber gen\u00fcgt.", "tokens": ["Mir", "sel\u00b7ber", "ge\u00b7n\u00fcgt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Und wi\u00dft ihr dahin", "tokens": ["Und", "wi\u00dft", "ihr", "da\u00b7hin"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PAV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Mein Lied nicht zu nehmen,", "tokens": ["Mein", "Lied", "nicht", "zu", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "So darf's mich nicht gr\u00e4men;", "tokens": ["So", "dar\u00b7f's", "mich", "nicht", "gr\u00e4\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich bin, der ich bin.", "tokens": ["Ich", "bin", ",", "der", "ich", "bin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "PPER", "VAFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}