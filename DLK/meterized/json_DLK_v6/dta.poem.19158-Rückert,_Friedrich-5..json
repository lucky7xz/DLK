{"dta.poem.19158": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie augentr\u00f6stlich auch und lieblich lenzverj\u00fcnglich", "tokens": ["Wie", "au\u00b7gen\u00b7tr\u00f6st\u00b7lich", "auch", "und", "lieb\u00b7lich", "lenz\u00b7ver\u00b7j\u00fcng\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADV", "KON", "ADJD", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Gr\u00fcn der Fluren sei, es ist doch nicht urspr\u00fcnglich.", "tokens": ["Das", "Gr\u00fcn", "der", "Flu\u00b7ren", "sei", ",", "es", "ist", "doch", "nicht", "ur\u00b7spr\u00fcng\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "$,", "PPER", "VAFIN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das Gr\u00fcn ist, wie bekannt, gemischt aus Gelb und Blau;", "tokens": ["Das", "Gr\u00fcn", "ist", ",", "wie", "be\u00b7kannt", ",", "ge\u00b7mischt", "aus", "Gelb", "und", "Blau", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PWAV", "ADJD", "$,", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun welches Blau und Gelb mischt so das Gr\u00fcn der Au?", "tokens": ["Nun", "wel\u00b7ches", "Blau", "und", "Gelb", "mischt", "so", "das", "Gr\u00fcn", "der", "Au", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAT", "NN", "KON", "NN", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Sonne goldner Schein, das Blau im Aetherraum;", "tokens": ["Der", "Son\u00b7ne", "gold\u00b7ner", "Schein", ",", "das", "Blau", "im", "A\u00b7e\u00b7ther\u00b7raum", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Aus beiden ist gewebt des Fr\u00fchlings gr\u00fcner Traum.", "tokens": ["Aus", "bei\u00b7den", "ist", "ge\u00b7webt", "des", "Fr\u00fch\u00b7lings", "gr\u00fc\u00b7ner", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "VAFIN", "VVPP", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das Gr\u00fcn unzweifelhaft stammt nicht aus gr\u00fcnem Saft,", "tokens": ["Das", "Gr\u00fcn", "un\u00b7zwei\u00b7fel\u00b7haft", "stammt", "nicht", "aus", "gr\u00fc\u00b7nem", "Saft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn nur durch Luft und Licht erlangt es solche Kraft.", "tokens": ["Denn", "nur", "durch", "Luft", "und", "Licht", "er\u00b7langt", "es", "sol\u00b7che", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Drum ist von Fr\u00fchlingsgr\u00fcn dein Auge so erquickt,", "tokens": ["Drum", "ist", "von", "Fr\u00fch\u00b7lings\u00b7gr\u00fcn", "dein", "Au\u00b7ge", "so", "er\u00b7quickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "APPR", "NN", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weils drin vereint die zwei unsichtbaren erblickt.", "tokens": ["Weils", "drin", "ver\u00b7eint", "die", "zwei", "un\u00b7sicht\u00b7ba\u00b7ren", "er\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "CARD", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.6": {"line.1": {"text": "Heil ihm, wenn dankbar es den Erdentraum genie\u00dft,", "tokens": ["Heil", "ihm", ",", "wenn", "dank\u00b7bar", "es", "den", "Er\u00b7den\u00b7traum", "ge\u00b7nie\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "ADJD", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Bis er in Sonnengold und Aetherblau zerflie\u00dft.", "tokens": ["Bis", "er", "in", "Son\u00b7nen\u00b7gold", "und", "A\u00b7e\u00b7ther\u00b7blau", "zer\u00b7flie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}