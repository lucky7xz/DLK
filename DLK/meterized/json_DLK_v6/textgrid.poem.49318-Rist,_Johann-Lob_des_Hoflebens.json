{"textgrid.poem.49318": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Lob des Hoflebens", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Himmel, dir sei Lob gesungen,", "tokens": ["Him\u00b7mel", ",", "dir", "sei", "Lob", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich der bin, der ich bin,", "tokens": ["Da\u00df", "ich", "der", "bin", ",", "der", "ich", "bin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "VAFIN", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Auch annoch fein ungezwungen", "tokens": ["Auch", "an\u00b7noch", "fein", "un\u00b7ge\u00b7zwun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "ADJA"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Leben kan nach meinem Sinn.", "tokens": ["Le\u00b7ben", "kan", "nach", "mei\u00b7nem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hoher Dinge Lieb und Lust", "tokens": ["Ho\u00b7her", "Din\u00b7ge", "Lieb", "und", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Herschen nicht in meiner Brust.", "tokens": ["Her\u00b7schen", "nicht", "in", "mei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Neulich als ich angesehen", "tokens": ["Neu\u00b7lich", "als", "ich", "an\u00b7ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gro\u00dfer Leute Stand und Pracht,", "tokens": ["Gro\u00b7\u00dfer", "Leu\u00b7te", "Stand", "und", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Wust ich kaum wie mir geschehen,", "tokens": ["Wust", "ich", "kaum", "wie", "mir", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KOKOM", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Denn ich h\u00e4tt' es kaum gedacht,", "tokens": ["Denn", "ich", "h\u00e4tt'", "es", "kaum", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Da\u00df so gro\u00dfe Schlaverei", "tokens": ["Da\u00df", "so", "gro\u00b7\u00dfe", "Schla\u00b7ve\u00b7rei"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Bei der F\u00fcrsten H\u00f6fen sei.", "tokens": ["Bei", "der", "F\u00fcrs\u00b7ten", "H\u00f6\u00b7fen", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Hei\u00dfet das in Freud und Ehren", "tokens": ["Hei\u00b7\u00dfet", "das", "in", "Freud", "und", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Seine Jahre bringen zu?", "tokens": ["Sei\u00b7ne", "Jah\u00b7re", "brin\u00b7gen", "zu", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "PTKZU", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Gott, wie l\u00e4\u00dft man sich beth\u00f6ren!", "tokens": ["Gott", ",", "wie", "l\u00e4\u00dft", "man", "sich", "be\u00b7th\u00f6\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VVFIN", "PIS", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Ist doch weder Rast noch Ruh'", "tokens": ["Ist", "doch", "we\u00b7der", "Rast", "noch", "Ruh'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "KON", "NN", "ADV", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "An den H\u00f6fen, wo man sich", "tokens": ["An", "den", "H\u00f6\u00b7fen", ",", "wo", "man", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "PIS", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Plagen mu\u00df so j\u00e4mmerlich.", "tokens": ["Pla\u00b7gen", "mu\u00df", "so", "j\u00e4m\u00b7mer\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Wann der Hofeman wil essen,", "tokens": ["Wann", "der", "Ho\u00b7fe\u00b7man", "wil", "es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.20": {"text": "Mu\u00df er erstlich auf die Jagd,", "tokens": ["Mu\u00df", "er", "erst\u00b7lich", "auf", "die", "Jagd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Da der Mahlzeit wird vergessen", "tokens": ["Da", "der", "Mahl\u00b7zeit", "wird", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Und nur an das Wild gedacht.", "tokens": ["Und", "nur", "an", "das", "Wild", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Ach, da murren Magn und Mund,", "tokens": ["Ach", ",", "da", "mur\u00b7ren", "Magn", "und", "Mund", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Hungrig ist man als ein Hund.", "tokens": ["Hung\u00b7rig", "ist", "man", "als", "ein", "Hund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PIS", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Wann der Hofemann wil schlafen,", "tokens": ["Wann", "der", "Ho\u00b7fe\u00b7mann", "wil", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+++-", "measure": "unknown.measure.penta"}, "line.26": {"text": "Mu\u00df er f\u00fcr der Tafel stehn;", "tokens": ["Mu\u00df", "er", "f\u00fcr", "der", "Ta\u00b7fel", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Hat er N\u00f6tigs gleich zu schaffen,", "tokens": ["Hat", "er", "N\u00f6\u00b7tigs", "gleich", "zu", "schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NE", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Mu\u00df er doch nach Hofe gehn;", "tokens": ["Mu\u00df", "er", "doch", "nach", "Ho\u00b7fe", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Bittet ihn ein Freund zu sich,", "tokens": ["Bit\u00b7tet", "ihn", "ein", "Freund", "zu", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Spricht der F\u00fcrst: \u00bbIch fodre dich.\u00ab", "tokens": ["Spricht", "der", "F\u00fcrst", ":", "\u00bb", "Ich", "fod\u00b7re", "dich", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "$(", "PPER", "VVFIN", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Wann der Hofemann wil schreiben", "tokens": ["Wann", "der", "Ho\u00b7fe\u00b7mann", "wil", "schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Was sein eignes Werk betrifft,", "tokens": ["Was", "sein", "eig\u00b7nes", "Werk", "be\u00b7tr\u00b7ifft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.33": {"text": "Ruft der Junker: \u00bbLa\u00dft das bleiben!", "tokens": ["Ruft", "der", "Jun\u00b7ker", ":", "\u00bb", "La\u00dft", "das", "blei\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "$(", "VVIMP", "PDS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Man wird heut' ein' ander Schrift", "tokens": ["Man", "wird", "heut'", "ein'", "an\u00b7der", "Schrift"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "In Pokalen setzen auf.", "tokens": ["In", "Po\u00b7ka\u00b7len", "set\u00b7zen", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Bruder, scher herauf und sauf!\u00ab", "tokens": ["Bru\u00b7der", ",", "scher", "her\u00b7auf", "und", "sauf", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "ADJD", "PTKVZ", "KON", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Ei, du feines Hofeleben!", "tokens": ["Ei", ",", "du", "fei\u00b7nes", "Ho\u00b7fe\u00b7le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Solt ein Mensch, der witzig ist,", "tokens": ["Solt", "ein", "Mensch", ",", "der", "wit\u00b7zig", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Dir den h\u00f6chsten Preis nicht geben,", "tokens": ["Dir", "den", "h\u00f6chs\u00b7ten", "Preis", "nicht", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Da du doch so j\u00e4mrich bist!", "tokens": ["Da", "du", "doch", "so", "j\u00e4m\u00b7rich", "bist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Ei, da\u00df solchem Ungemach", "tokens": ["Ei", ",", "da\u00df", "sol\u00b7chem", "Un\u00b7ge\u00b7mach"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.42": {"text": "Edle Seelen laufen nach!", "tokens": ["Ed\u00b7le", "See\u00b7len", "lau\u00b7fen", "nach", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Recht das hei\u00dft zu Hofe laufen", "tokens": ["Recht", "das", "hei\u00dft", "zu", "Ho\u00b7fe", "lau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PDS", "VVFIN", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Und zu Hof' ein J\u00e4ger sein,", "tokens": ["Und", "zu", "Hof'", "ein", "J\u00e4\u00b7ger", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "Tag und Nacht zu Hofe saufen", "tokens": ["Tag", "und", "Nacht", "zu", "Ho\u00b7fe", "sau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Den geschmierten Schwefelwein;", "tokens": ["Den", "ge\u00b7schmier\u00b7ten", "Schwe\u00b7fel\u00b7wein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Wachen, hoffen, h\u00f6hnisch sehen,", "tokens": ["Wa\u00b7chen", ",", "hof\u00b7fen", ",", "h\u00f6h\u00b7nisch", "se\u00b7hen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Das hei\u00dft recht zu Hofe gehen.", "tokens": ["Das", "hei\u00dft", "recht", "zu", "Ho\u00b7fe", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.49": {"text": "O, wie selig ist zu sch\u00e4tzen,", "tokens": ["O", ",", "wie", "se\u00b7lig", "ist", "zu", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADJD", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Der in seinem H\u00fcttelein", "tokens": ["Der", "in", "sei\u00b7nem", "H\u00fct\u00b7tel\u00b7ein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "Auf gut sch\u00e4frisch sich ergetzen", "tokens": ["Auf", "gut", "sch\u00e4f\u00b7risch", "sich", "er\u00b7get\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJD", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Und sein eigner Herr kan sein,", "tokens": ["Und", "sein", "eig\u00b7ner", "Herr", "kan", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VMFIN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.53": {"text": "Essen da, was Gott beschert,", "tokens": ["Es\u00b7sen", "da", ",", "was", "Gott", "be\u00b7schert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PRELS", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.54": {"text": "Werden nie durch Zank beschwert.", "tokens": ["Wer\u00b7den", "nie", "durch", "Zank", "be\u00b7schwert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.55": {"text": "Himmel, dir sei Lob gesungen,", "tokens": ["Him\u00b7mel", ",", "dir", "sei", "Lob", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Da\u00df ich der bin, der ich bin,", "tokens": ["Da\u00df", "ich", "der", "bin", ",", "der", "ich", "bin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "VAFIN", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.57": {"text": "Auch annoch fein ungezwungen", "tokens": ["Auch", "an\u00b7noch", "fein", "un\u00b7ge\u00b7zwun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "ADJA"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.58": {"text": "Leben kan nach meinem Sinn.", "tokens": ["Le\u00b7ben", "kan", "nach", "mei\u00b7nem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.59": {"text": "Aller H\u00f6fe Glanz und Pracht", "tokens": ["Al\u00b7ler", "H\u00f6\u00b7fe", "Glanz", "und", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.60": {"text": "Sing' und sag' ich gute Nacht.", "tokens": ["Sing'", "und", "sag'", "ich", "gu\u00b7te", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}