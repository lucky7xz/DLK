{"dta.poem.4195": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die ungl\u00fcckliche  \n  Unaufmerksamkeit der Menschen,  \n auch so gar  \n  beym Anblick der Sonnen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Indem ich j\u00fcngst durch einen Wald,", "tokens": ["In\u00b7dem", "ich", "j\u00fcngst", "durch", "ei\u00b7nen", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dessen liebliches Geb\u00fcsche,", "tokens": ["Und", "des\u00b7sen", "lieb\u00b7li\u00b7ches", "Ge\u00b7b\u00fc\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der k\u00fchlen Schatten Aufenthalt,", "tokens": ["Der", "k\u00fch\u00b7len", "Schat\u00b7ten", "Auf\u00b7ent\u00b7halt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In einem angenehmen Wetter,", "tokens": ["In", "ei\u00b7nem", "an\u00b7ge\u00b7neh\u00b7men", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vergn\u00fcgt spatzier, und mich erfrische;", "tokens": ["Ver\u00b7gn\u00fcgt", "spat\u00b7zier", ",", "und", "mich", "er\u00b7fri\u00b7sche", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erblick ich, durch die jungen Bl\u00e4tter,", "tokens": ["Er\u00b7blick", "ich", ",", "durch", "die", "jun\u00b7gen", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Sonnen flammende Gestalt,", "tokens": ["Der", "Son\u00b7nen", "flam\u00b7men\u00b7de", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.8": {"text": "Bey ganz entw\u00f6lktem Untergehn,", "tokens": ["Bey", "ganz", "ent\u00b7w\u00f6lk\u00b7tem", "Un\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Auf einmal wunder-wundersch\u00f6n.", "tokens": ["Auf", "ein\u00b7mal", "wun\u00b7der\u00b7wun\u00b7der\u00b7sch\u00f6n", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Es flammte durch das zarte Gr\u00fcn,", "tokens": ["Es", "flamm\u00b7te", "durch", "das", "zar\u00b7te", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Das sch\u00f6ner, als Smaragden, schien,", "tokens": ["Das", "sch\u00f6\u00b7ner", ",", "als", "Sma\u00b7rag\u00b7den", ",", "schien", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "KOUS", "NN", "$,", "VVFIN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Ein Strahl, der r\u00f6hter, als Rubien.", "tokens": ["Ein", "Strahl", ",", "der", "r\u00f6h\u00b7ter", ",", "als", "Ru\u00b7bi\u00b7en", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "KOUS", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es fiel ein wirklich himmlisch Licht,", "tokens": ["Es", "fiel", "ein", "wirk\u00b7lich", "himm\u00b7lisch", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In diesem Glanz, mir ins Gesicht.", "tokens": ["In", "die\u00b7sem", "Glanz", ",", "mir", "ins", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich rief verschiedne zu mir her,", "tokens": ["Ich", "rief", "ver\u00b7schied\u00b7ne", "zu", "mir", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zeigt\u2019 ihnen dieses g\u00fcldne Meer,", "tokens": ["Zeigt'", "ih\u00b7nen", "die\u00b7ses", "g\u00fcld\u00b7ne", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Woraus best\u00e4ndig Strahlen quillen,", "tokens": ["Wo\u00b7raus", "be\u00b7st\u00e4n\u00b7dig", "Strah\u00b7len", "quil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Himmel, Welt und Augen f\u00fcllen.", "tokens": ["Die", "Him\u00b7mel", ",", "Welt", "und", "Au\u00b7gen", "f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "(ach, f\u00fcllten sie auch unsre Brust!)", "tokens": ["(", "ach", ",", "f\u00fcll\u00b7ten", "sie", "auch", "uns\u00b7re", "Brust", "!", ")"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie sahn es auch mit Freuden an;", "tokens": ["Sie", "sahn", "es", "auch", "mit", "Freu\u00b7den", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Allein es war um ihre Lust,", "tokens": ["Al\u00b7lein", "es", "war", "um", "ih\u00b7re", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "In einem Augenblick, gethan.", "tokens": ["In", "ei\u00b7nem", "Au\u00b7gen\u00b7blick", ",", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sie sagten: Ey! o das ist sch\u00f6n!", "tokens": ["Sie", "sag\u00b7ten", ":", "Ey", "!", "o", "das", "ist", "sch\u00f6n", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "$.", "FM", "PDS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und kehrten darauf allgemach", "tokens": ["Und", "kehr\u00b7ten", "da\u00b7rauf", "all\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PAV", "ADV"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.13": {"text": "Sich abwerts, wie ich fast noch sprach,", "tokens": ["Sich", "ab\u00b7werts", ",", "wie", "ich", "fast", "noch", "sprach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "$,", "PWAV", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und liessen mich alleine stehn.", "tokens": ["Und", "lies\u00b7sen", "mich", "al\u00b7lei\u00b7ne", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich stutzte und betr\u00fcbte mich,", "tokens": ["Ich", "stutz\u00b7te", "und", "be\u00b7tr\u00fcb\u00b7te", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ob dem Betragen, innerlich.", "tokens": ["Ob", "dem", "Be\u00b7tra\u00b7gen", ",", "in\u00b7ner\u00b7lich", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ADJD", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie? r\u00fchrt denn selbst des Himmels Licht,", "tokens": ["Wie", "?", "r\u00fchrt", "denn", "selbst", "des", "Him\u00b7mels", "Licht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "ADV", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Rief ich, der Menschen Seelen nicht?", "tokens": ["Rief", "ich", ",", "der", "Men\u00b7schen", "See\u00b7len", "nicht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das heut so wundersch\u00f6n zu sp\u00fcren;", "tokens": ["Das", "heut", "so", "wun\u00b7der\u00b7sch\u00f6n", "zu", "sp\u00fc\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was soll, was kann sie denn doch r\u00fchren?", "tokens": ["Was", "soll", ",", "was", "kann", "sie", "denn", "doch", "r\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$,", "PWS", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wer wundert sich, da\u00df, von der Erden,", "tokens": ["Wer", "wun\u00b7dert", "sich", ",", "da\u00df", ",", "von", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "$,", "KOUS", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wunder nicht empfunden werden;", "tokens": ["Die", "Wun\u00b7der", "nicht", "emp\u00b7fun\u00b7den", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da sie selbst, vor der Sonnen, blind,", "tokens": ["Da", "sie", "selbst", ",", "vor", "der", "Son\u00b7nen", ",", "blind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "APPR", "ART", "NN", "$,", "ADJD", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "F\u00fchl- Acht- ja gleichsam Sinn- los, sind.", "tokens": ["F\u00fchl", "Acht", "ja", "gleich\u00b7sam", "Sinn", "los", ",", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["TRUNC", "TRUNC", "ADV", "ADJD", "TRUNC", "PTKVZ", "$,", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es scheint, die\u00df \u00fcberf\u00fchr\u2019 euch ganz:", "tokens": ["Es", "scheint", ",", "die\u00df", "\u00fc\u00b7ber\u00b7f\u00fchr'", "euch", "ganz", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Erden Pracht, des Himmels Glanz", "tokens": ["Der", "Er\u00b7den", "Pracht", ",", "des", "Him\u00b7mels", "Glanz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sey f\u00fcr uns Menschen nicht gemacht,", "tokens": ["Sey", "f\u00fcr", "uns", "Men\u00b7schen", "nicht", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie wir doch sonst so schw\u00fclstig wollen.", "tokens": ["Wie", "wir", "doch", "sonst", "so", "schw\u00fcls\u00b7tig", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ADV", "ADJD", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Denn wenns f\u00fcr euch, und GOtt zur Ehr", "tokens": ["Denn", "wenns", "f\u00fcr", "euch", ",", "und", "Gott", "zur", "Ehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "PPER", "$,", "KON", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Hervorgebracht und worden w\u00e4r;", "tokens": ["Her\u00b7vor\u00b7ge\u00b7bracht", "und", "wor\u00b7den", "w\u00e4r", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "So h\u00e4ttet ihrs auch merken sollen,", "tokens": ["So", "h\u00e4t\u00b7tet", "ihrs", "auch", "mer\u00b7ken", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "So h\u00e4ttet ihrs empfinden, wissen,", "tokens": ["So", "h\u00e4t\u00b7tet", "ihrs", "emp\u00b7fin\u00b7den", ",", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Euch freuen, und GOtt danken m\u00fcssen.", "tokens": ["Euch", "freu\u00b7en", ",", "und", "Gott", "dan\u00b7ken", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "KON", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Jhr st\u00fcrzt demnach euch selbst vom Thron", "tokens": ["Ihr", "st\u00fcrzt", "dem\u00b7nach", "euch", "selbst", "vom", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Des Vorrechts, vor den andern Thieren,", "tokens": ["Des", "Vor\u00b7rechts", ",", "vor", "den", "an\u00b7dern", "Thie\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Als welche, von des Sch\u00f6pfers Werken,", "tokens": ["Als", "wel\u00b7che", ",", "von", "des", "Sch\u00f6p\u00b7fers", "Wer\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Die Pracht nicht weniger bemerken,", "tokens": ["Die", "Pracht", "nicht", "we\u00b7ni\u00b7ger", "be\u00b7mer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Nicht in geringerm Grad versp\u00fcren.", "tokens": ["Nicht", "in", "ge\u00b7rin\u00b7germ", "Grad", "ver\u00b7sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Denn wahrlich, weniger als ihr,", "tokens": ["Denn", "wahr\u00b7lich", ",", "we\u00b7ni\u00b7ger", "als", "ihr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "KOUS", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Vergn\u00fcget sich, an aller Pracht", "tokens": ["Ver\u00b7gn\u00fc\u00b7get", "sich", ",", "an", "al\u00b7ler", "Pracht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PRF", "$,", "APPR", "PIAT", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.21": {"text": "De\u00df, was von GOtt hervorgebracht,", "tokens": ["De\u00df", 