{"textgrid.poem.62972": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es traf sich einst ein rascher Hase", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es traf sich einst ein rascher Hase", "tokens": ["Es", "traf", "sich", "einst", "ein", "ra\u00b7scher", "Ha\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit einem wilden Kater auf der Stra\u00dfe.", "tokens": ["Mit", "ei\u00b7nem", "wil\u00b7den", "Ka\u00b7ter", "auf", "der", "Stra\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Man schwatzte viel, auch von der Tapferkeit,", "tokens": ["Man", "schwatz\u00b7te", "viel", ",", "auch", "von", "der", "Tap\u00b7fer\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und itzt entstand ein Ehrenstreit.", "tokens": ["Und", "itzt", "ent\u00b7stand", "ein", "Eh\u00b7ren\u00b7streit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Herr Murner pries den k\u00fchnen Muth der Katzen", "tokens": ["Herr", "Mur\u00b7ner", "pries", "den", "k\u00fch\u00b7nen", "Muth", "der", "Kat\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "VVFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und hies die Hasen feige Matzen.", "tokens": ["Und", "hies", "die", "Ha\u00b7sen", "fei\u00b7ge", "Mat\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Verl\u00e4umdung, rief Herr Lamp, du sollst mich sehn", "tokens": ["Ver\u00b7l\u00e4um\u00b7dung", ",", "rief", "Herr", "Lamp", ",", "du", "sollst", "mich", "sehn"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "NN", "NE", "$,", "PPER", "VMFIN", "PRF", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Dem ersten Hund beherzt entgegen gehn.", "tokens": ["Dem", "ers\u00b7ten", "Hund", "be\u00b7herzt", "ent\u00b7ge\u00b7gen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKVZ", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Gut, gut. Sie trabten fort; auf einmal stie\u00dfen", "tokens": ["Gut", ",", "gut", ".", "Sie", "trab\u00b7ten", "fort", ";", "auf", "ein\u00b7mal", "stie\u00b7\u00dfen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "$.", "PPER", "VVFIN", "PTKVZ", "$.", "APPR", "ADV", "VVINF"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.10": {"text": "Sie auf das Aas von einem Sch\u00e4ferhund.", "tokens": ["Sie", "auf", "das", "Aas", "von", "ei\u00b7nem", "Sch\u00e4\u00b7fer\u00b7hund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Lamp sahs zuerst. Mit schnellen F\u00fc\u00dfen", "tokens": ["Lamp", "sahs", "zu\u00b7erst", ".", "Mit", "schnel\u00b7len", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "L\u00e4uft er so weit er kann, l\u00e4uft sich die Sohlen wund", "tokens": ["L\u00e4uft", "er", "so", "weit", "er", "kann", ",", "l\u00e4uft", "sich", "die", "Soh\u00b7len", "wund"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PPER", "VMFIN", "$,", "VVFIN", "PRF", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und bleibt ersch\u00f6pft von Mattigkeit und Schrecken", "tokens": ["Und", "bleibt", "er\u00b7sch\u00f6pft", "von", "Mat\u00b7tig\u00b7keit", "und", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "VVPP", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "In einem dichten Busche stecken.", "tokens": ["In", "ei\u00b7nem", "dich\u00b7ten", "Bu\u00b7sche", "ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Hier fand zuletzt der Kater ihn;", "tokens": ["Hier", "fand", "zu\u00b7letzt", "der", "Ka\u00b7ter", "ihn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Nun, nun, das hei\u00df ich mir vor einem Aase fliehn,", "tokens": ["Nun", ",", "nun", ",", "das", "hei\u00df", "ich", "mir", "vor", "ei\u00b7nem", "Aa\u00b7se", "fliehn", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,", "PRELS", "ADJD", "PPER", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Rief er, ich suche dich schon eine halbe Stunde,", "tokens": ["Rief", "er", ",", "ich", "su\u00b7che", "dich", "schon", "ei\u00b7ne", "hal\u00b7be", "Stun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Du tapfrer Held. Ey liebes Kind,", "tokens": ["Du", "tapf\u00b7rer", "Held", ".", "Ey", "lie\u00b7bes", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Versetzte Lamp, ein andres sind", "tokens": ["Ver\u00b7setz\u00b7te", "Lamp", ",", "ein", "and\u00b7res", "sind"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ART", "PIS", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Lebendige, ein andres todte Hunde.", "tokens": ["Le\u00b7ben\u00b7di\u00b7ge", ",", "ein", "and\u00b7res", "tod\u00b7te", "Hun\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+----+-+-+-", "measure": "dactylic.init"}}}}}