{"textgrid.poem.26471": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: H\u00f6rst du dort die Stimmen schallen?", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rst du dort die Stimmen schallen?", "tokens": ["H\u00f6rst", "du", "dort", "die", "Stim\u00b7men", "schal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dort sind Menschen, roh und kalt \u2013", "tokens": ["Dort", "sind", "Men\u00b7schen", ",", "roh", "und", "kalt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,", "ADJD", "KON", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df uns, Theu're, tiefer wallen", "tokens": ["La\u00df", "uns", ",", "Theu'\u00b7re", ",", "tie\u00b7fer", "wal\u00b7len"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "NE", "$,", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den heilig stillen Wald.", "tokens": ["In", "den", "hei\u00b7lig", "stil\u00b7len", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jene bleiben wohl am Rande,", "tokens": ["Je\u00b7ne", "blei\u00b7ben", "wohl", "am", "Ran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df sie noch die Stra\u00dfe seh'n;", "tokens": ["Da\u00df", "sie", "noch", "die", "Stra\u00b7\u00dfe", "seh'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn sie sind im Festgewande,", "tokens": ["Denn", "sie", "sind", "im", "Fest\u00b7ge\u00b7wan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und da mu\u00df man sicher geh'n.", "tokens": ["Und", "da", "mu\u00df", "man", "si\u00b7cher", "geh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "ADV", "VMFIN", "PIS", "ADJD", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wir doch wollen uns erst schm\u00fccken", "tokens": ["Wir", "doch", "wol\u00b7len", "uns", "erst", "schm\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit des Lenzes jungem Gr\u00fcn,", "tokens": ["Mit", "des", "Len\u00b7zes", "jun\u00b7gem", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollen uns erst Veilchen pfl\u00fccken,", "tokens": ["Wol\u00b7len", "uns", "erst", "Veil\u00b7chen", "pfl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die im Dickicht reicher bl\u00fch'n.", "tokens": ["Die", "im", "Di\u00b7ckicht", "rei\u00b7cher", "bl\u00fch'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "APPRART", "NN", "ADJD", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sind wir l\u00e4ssig nicht im Suchen,", "tokens": ["Sind", "wir", "l\u00e4s\u00b7sig", "nicht", "im", "Su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "PTKNEG", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist dein T\u00fcchlein bald gef\u00fcllt,", "tokens": ["Ist", "dein", "T\u00fcch\u00b7lein", "bald", "ge\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und im Schatten hoher Buchen", "tokens": ["Und", "im", "Schat\u00b7ten", "ho\u00b7her", "Bu\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird der holde Raub enth\u00fcllt.", "tokens": ["Wird", "der", "hol\u00b7de", "Raub", "ent\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Deine Kunst magst du nun zeigen;", "tokens": ["Dei\u00b7ne", "Kunst", "magst", "du", "nun", "zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um und um ist tiefe Ruh',", "tokens": ["Um", "und", "um", "ist", "tie\u00b7fe", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "KON", "APPR", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur die Amsel in den Zweigen", "tokens": ["Nur", "die", "Am\u00b7sel", "in", "den", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pfeift ein Liedchen dir dazu.", "tokens": ["Pfeift", "ein", "Lied\u00b7chen", "dir", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "M\u00fc\u00dfig sollst du mich nicht finden;", "tokens": ["M\u00fc\u00b7\u00dfig", "sollst", "du", "mich", "nicht", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob ich es auch nie verstand,", "tokens": ["Ob", "ich", "es", "auch", "nie", "ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zarte Blumen einzuwinden,", "tokens": ["Zar\u00b7te", "Blu\u00b7men", "ein\u00b7zu\u00b7win\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bin ich hilfreich doch zur Hand.", "tokens": ["Bin", "ich", "hilf\u00b7reich", "doch", "zur", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Denn ich l\u00f6se deine Flechten", "tokens": ["Denn", "ich", "l\u00f6\u00b7se", "dei\u00b7ne", "Flech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Still in gold'ne Locken auf \u2013", "tokens": ["Still", "in", "gold'\u00b7ne", "Lo\u00b7cken", "auf", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "APPR", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dann dr\u00fcckst du mit der Rechten", "tokens": ["Und", "dann", "dr\u00fcckst", "du", "mit", "der", "Rech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gleich den fert'gen Kranz darauf!", "tokens": ["Gleich", "den", "fert'\u00b7gen", "Kranz", "da\u00b7rauf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}