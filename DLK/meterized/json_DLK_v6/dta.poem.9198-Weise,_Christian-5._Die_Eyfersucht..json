{"dta.poem.9198": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "5.  Die Eyfersucht.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Der eyfer/ der m\u00f6chte mich fressen/", "tokens": ["Der", "ey\u00b7fer", "/", "der", "m\u00f6ch\u00b7te", "mich", "fres\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "VMFIN", "PRF", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "So hat er mein hertze besessen/", "tokens": ["So", "hat", "er", "mein", "hert\u00b7ze", "be\u00b7ses\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ich bilde mir wunderlich ein/", "tokens": ["Ich", "bil\u00b7de", "mir", "wun\u00b7der\u00b7lich", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ART", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Sie w\u00e4re mein liebgen allein/", "tokens": ["Sie", "w\u00e4\u00b7re", "mein", "lieb\u00b7gen", "al\u00b7lein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "VVFIN", "ADV", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Und freuet mich/ - - - - aber/", "tokens": ["Und", "freu\u00b7et", "mich", "/", "a\u00b7ber", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$(", "$(", "$(", "$(", "$(", "ADV", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ein heimlicher dieb/", "tokens": ["Ein", "heim\u00b7li\u00b7cher", "dieb", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Der hat sie auch lieb/", "tokens": ["Der", "hat", "sie", "auch", "lieb", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADJD", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Und geht mir in haber.", "tokens": ["Und", "geht", "mir", "in", "ha\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}