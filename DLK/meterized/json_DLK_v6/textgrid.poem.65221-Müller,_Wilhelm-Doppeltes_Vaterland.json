{"textgrid.poem.65221": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Doppeltes Vaterland", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An der Elbe Strand", "tokens": ["An", "der", "El\u00b7be", "Strand"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Liegt mein Vaterland,", "tokens": ["Liegt", "mein", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Lieb's von ganzer Seele.", "tokens": ["Lie\u00b7b's", "von", "gan\u00b7zer", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aber meine Kehle", "tokens": ["A\u00b7ber", "mei\u00b7ne", "Keh\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ist zu Haus am Rhein,", "tokens": ["Ist", "zu", "Haus", "am", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "APPRART", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "D\u00fcrstet nur nach Wein.", "tokens": ["D\u00fcrs\u00b7tet", "nur", "nach", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wem es Freude schafft,", "tokens": ["Wem", "es", "Freu\u00b7de", "schafft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Trinke Br\u00fcderschaft", "tokens": ["Trin\u00b7ke", "Br\u00fc\u00b7der\u00b7schaft"], "token_info": ["word", "word"], "pos": ["VVFIN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Mit den kalten Fr\u00f6schen!", "tokens": ["Mit", "den", "kal\u00b7ten", "Fr\u00f6\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Meinen Durst zu l\u00f6schen", "tokens": ["Mei\u00b7nen", "Durst", "zu", "l\u00f6\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Hol' ich mir vom Rhein", "tokens": ["Hol'", "ich", "mir", "vom", "Rhein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PPER", "APPRART", "NE"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Lebenswarmen Wein.", "tokens": ["Le\u00b7bens\u00b7war\u00b7men", "Wein", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Spricht ein kluger Mund", "tokens": ["Spricht", "ein", "klu\u00b7ger", "Mund"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wein sei nicht gesund,", "tokens": ["Wein", "sei", "nicht", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ei, so trink' er keinen!", "tokens": ["Ei", ",", "so", "trink'", "er", "kei\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "PIAT", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Doch mir will es scheinen:", "tokens": ["Doch", "mir", "will", "es", "schei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Der den Geist erfreut,", "tokens": ["Der", "den", "Geist", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Thut dem Leib kein Leid.", "tokens": ["Thut", "dem", "Leib", "kein", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PIAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Mancher Medikus", "tokens": ["Man\u00b7cher", "Me\u00b7di\u00b7kus"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Trank sich aus dem Flu\u00df", "tokens": ["Trank", "sich", "aus", "dem", "Flu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Fl\u00fcsse in die Glieder.", "tokens": ["Fl\u00fcs\u00b7se", "in", "die", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Hei\u00dft mein Rezipe", "tokens": ["Hei\u00dft", "mein", "Re\u00b7zi\u00b7pe"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wider jedes Weh.", "tokens": ["Wi\u00b7der", "je\u00b7des", "Weh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Und mu\u00df einst es sein,", "tokens": ["Und", "mu\u00df", "einst", "es", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PPER", "VAINF", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Sterb' ich doch an Wein", "tokens": ["Sterb'", "ich", "doch", "an", "Wein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Lieber als an Pillen.", "tokens": ["Lie\u00b7ber", "als", "an", "Pil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Vor dem letzten Willen", "tokens": ["Vor", "dem", "letz\u00b7ten", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Leer' ich erst mein Fa\u00df", "tokens": ["Leer'", "ich", "erst", "mein", "Fa\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "ADV", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Bis auf's letzte Glas.", "tokens": ["Bis", "auf's", "letz\u00b7te", "Glas", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}