{"dta.poem.10543": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Tannenbaum, mit gr\u00fcnen Fingern,", "tokens": ["Tan\u00b7nen\u00b7baum", ",", "mit", "gr\u00fc\u00b7nen", "Fin\u00b7gern", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Pocht an's nied're Fensterlein,", "tokens": ["Pocht", "an's", "nie\u00b7d'\u00b7re", "Fens\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Und der Mond, der gelbe Lauscher,", "tokens": ["Und", "der", "Mond", ",", "der", "gel\u00b7be", "Lau\u00b7scher", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Wirft sein s\u00fc\u00dfes Licht herein.", "tokens": ["Wirft", "sein", "s\u00fc\u00b7\u00dfes", "Licht", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Vater, Mutter schnarchen leise", "tokens": ["Va\u00b7ter", ",", "Mut\u00b7ter", "schnar\u00b7chen", "lei\u00b7se"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem nahen Schlafgemach,", "tokens": ["In", "dem", "na\u00b7hen", "Schlaf\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch wir beide, selig schwatzend,", "tokens": ["Doch", "wir", "bei\u00b7de", ",", "se\u00b7lig", "schwat\u00b7zend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "PIS", "$,", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Halten uns einander wach.", "tokens": ["Hal\u00b7ten", "uns", "ein\u00b7an\u00b7der", "wach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u201eda\u00df du gar zu oft gebetet,", "tokens": ["\u201e", "da\u00df", "du", "gar", "zu", "oft", "ge\u00b7be\u00b7tet", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ADV", "PTKA", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das zu glauben wird mir schwer,", "tokens": ["Das", "zu", "glau\u00b7ben", "wird", "mir", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKZU", "VVINF", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jenes Zucken deiner Lippen", "tokens": ["Je\u00b7nes", "Zu\u00b7cken", "dei\u00b7ner", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kommt wohl nicht vom Beten her.", "tokens": ["Kommt", "wohl", "nicht", "vom", "Be\u00b7ten", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PTKNEG", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u201ejenes b\u00f6se, kalte Zucken,", "tokens": ["\u201e", "je\u00b7nes", "b\u00f6\u00b7se", ",", "kal\u00b7te", "Zu\u00b7cken", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PDAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das erschreckt mich jedesmal,", "tokens": ["Das", "er\u00b7schreckt", "mich", "je\u00b7des\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch die dunkle Angst beschwichtigt", "tokens": ["Doch", "die", "dunk\u00b7le", "Angst", "be\u00b7schwich\u00b7tigt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deiner Augen frommer Strahl.", "tokens": ["Dei\u00b7ner", "Au\u00b7gen", "from\u00b7mer", "Strahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u201eauch bezweifl' ich, da\u00df du glaubest,", "tokens": ["\u201e", "auch", "be\u00b7zweifl'", "ich", ",", "da\u00df", "du", "glau\u00b7best", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was so rechter Glauben hei\u00dft,", "tokens": ["Was", "so", "rech\u00b7ter", "Glau\u00b7ben", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Glaubst wohl nicht an Gott den Vater,", "tokens": ["Glaubst", "wohl", "nicht", "an", "Gott", "den", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An den Sohn und heil'gen Geist?\u201c \u2014", "tokens": ["An", "den", "Sohn", "und", "heil'\u00b7gen", "Geist", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ADJA", "NN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach, mein Kindchen, schon als Knabe,", "tokens": ["Ach", ",", "mein", "Kind\u00b7chen", ",", "schon", "als", "Kna\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "$,", "ADV", "KOUS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als ich sa\u00df auf Mutters Schoo\u00df,", "tokens": ["Als", "ich", "sa\u00df", "auf", "Mut\u00b7ters", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Glaubte ich an Gott den Vater,", "tokens": ["Glaub\u00b7te", "ich", "an", "Gott", "den", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der da waltet gut und gro\u00df;", "tokens": ["Der", "da", "wal\u00b7tet", "gut", "und", "gro\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Der die sch\u00f6ne Erd' erschaffen,", "tokens": ["Der", "die", "sch\u00f6\u00b7ne", "Erd'", "er\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die sch\u00f6nen Menschen d'rauf,", "tokens": ["Und", "die", "sch\u00f6\u00b7nen", "Men\u00b7schen", "d'\u00b7rauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der den Sonnen, Monden, Sternen", "tokens": ["Der", "den", "Son\u00b7nen", ",", "Mon\u00b7den", ",", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "ART", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vorgezeichnet ihren Lauf.", "tokens": ["Vor\u00b7ge\u00b7zeich\u00b7net", "ih\u00b7ren", "Lauf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Als ich gr\u00f6\u00dfer wurde, Kindchen,", "tokens": ["Als", "ich", "gr\u00f6\u00b7\u00dfer", "wur\u00b7de", ",", "Kind\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch viel mehr begriff ich schon,", "tokens": ["Noch", "viel", "mehr", "be\u00b7griff", "ich", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und begriff, und ward vern\u00fcnftig,", "tokens": ["Und", "be\u00b7griff", ",", "und", "ward", "ver\u00b7n\u00fcnf\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich glaub' auch an den Sohn;", "tokens": ["Und", "ich", "glaub'", "auch", "an", "den", "Sohn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "An den lieben Sohn, der liebend", "tokens": ["An", "den", "lie\u00b7ben", "Sohn", ",", "der", "lie\u00b7bend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Uns die Liebe offenbart,", "tokens": ["Uns", "die", "Lie\u00b7be", "of\u00b7fen\u00b7bart", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zum Lohne, wie gebr\u00e4uchlich,", "tokens": ["Und", "zum", "Loh\u00b7ne", ",", "wie", "ge\u00b7br\u00e4uch\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "$,", "PWAV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von dem Volk gekreuzigt ward.", "tokens": ["Von", "dem", "Volk", "ge\u00b7kreu\u00b7zigt", "ward", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Jetzo, da ich ausgewachsen,", "tokens": ["Jet\u00b7zo", ",", "da", "ich", "aus\u00b7ge\u00b7wach\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Viel gelesen, viel gereist,", "tokens": ["Viel", "ge\u00b7le\u00b7sen", ",", "viel", "ge\u00b7reist", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwillt mein Herz, und ganz von Herzen", "tokens": ["Schwillt", "mein", "Herz", ",", "und", "ganz", "von", "Her\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KON", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Glaub' ich an den heil'gen Geist.", "tokens": ["Glaub'", "ich", "an", "den", "heil'\u00b7gen", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Dieser that die gr\u00f6\u00dften Wunder,", "tokens": ["Die\u00b7ser", "that", "die", "gr\u00f6\u00df\u00b7ten", "Wun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und viel gr\u00f6\u00df're thut er noch;", "tokens": ["Und", "viel", "gr\u00f6\u00df'\u00b7re", "thut", "er", "noch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Er zerbrach die Zwingherrnburgen,", "tokens": ["Er", "zer\u00b7brach", "die", "Zwing\u00b7herrn\u00b7bur\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und zerbrach des Knechtes Joch.", "tokens": ["Und", "zer\u00b7brach", "des", "Knech\u00b7tes", "Joch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Alte Todeswunden heilt er,", "tokens": ["Al\u00b7te", "To\u00b7des\u00b7wun\u00b7den", "heilt", "er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und erneut das alte Recht:", "tokens": ["Und", "er\u00b7neut", "das", "al\u00b7te", "Recht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Menschen, gleichgeboren,", "tokens": ["Al\u00b7le", "Men\u00b7schen", ",", "gleich\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind ein adliges Geschlecht.", "tokens": ["Sind", "ein", "ad\u00b7li\u00b7ges", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Er verscheucht die b\u00f6sen Nebel,", "tokens": ["Er", "ver\u00b7scheucht", "die", "b\u00f6\u00b7sen", "Ne\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das dunkle Hirngespinst,", "tokens": ["Und", "das", "dunk\u00b7le", "Hirn\u00b7ge\u00b7spinst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das uns Lieb' und Lust verleidet,", "tokens": ["Das", "uns", "Lieb'", "und", "Lust", "ver\u00b7lei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tag und Nacht uns angegrinzt.", "tokens": ["Tag", "und", "Nacht", "uns", "an\u00b7ge\u00b7grinzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Tausend Ritter, wohlgewappnet,", "tokens": ["Tau\u00b7send", "Rit\u00b7ter", ",", "wohl\u00b7ge\u00b7wapp\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["CARD", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat der heil'ge Geist erw\u00e4hlt,", "tokens": ["Hat", "der", "heil'\u00b7ge", "Geist", "er\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seinen Willen zu erf\u00fcllen,", "tokens": ["Sei\u00b7nen", "Wil\u00b7len", "zu", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und er hat sie muthbeseelt.", "tokens": ["Und", "er", "hat", "sie", "muth\u00b7be\u00b7seelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Ihre theuern Schwerdter blitzen,", "tokens": ["Ih\u00b7re", "theu\u00b7ern", "Schwerd\u00b7ter", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihre guten Banner weh'n!", "tokens": ["Ih\u00b7re", "gu\u00b7ten", "Ban\u00b7ner", "weh'n", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ei, du m\u00f6chtest wohl, mein Kindchen,", "tokens": ["Ei", ",", "du", "m\u00f6ch\u00b7test", "wohl", ",", "mein", "Kind\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Solche stolze Ritter seh'n?", "tokens": ["Sol\u00b7che", "stol\u00b7ze", "Rit\u00b7ter", "seh'n", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Nun, so schau' mich an, mein Kindchen,", "tokens": ["Nun", ",", "so", "schau'", "mich", "an", ",", "mein", "Kind\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fcsse mich und schaue dreist;", "tokens": ["K\u00fcs\u00b7se", "mich", "und", "schau\u00b7e", "dreist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn ich selber bin ein solcher", "tokens": ["Denn", "ich", "sel\u00b7ber", "bin", "ein", "sol\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV