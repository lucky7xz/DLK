{"dta.poem.9907": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wegen unser grossen zahl kan man uns unm\u00f6glich zehlen/", "tokens": ["We\u00b7gen", "un\u00b7ser", "gros\u00b7sen", "zahl", "kan", "man", "uns", "un\u00b7m\u00f6g\u00b7lich", "zeh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VMFIN", "PIS", "PPER", "ADJD", "VVFIN", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Wenn wir lang und dicke sind/ so will keiner uns verh\u00f6hlen.", "tokens": ["Wenn", "wir", "lang", "und", "di\u00b7cke", "sind", "/", "so", "will", "kei\u00b7ner", "uns", "ver\u00b7h\u00f6h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADJA", "VAFIN", "$(", "ADV", "VMFIN", "PIS", "PPER", "VVINF", "$."], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der/ so uns am sch\u00f6nsten thut/ l\u00e4sst uns j\u00e4m\u0303erlich beschneiden/", "tokens": ["Der", "/", "so", "uns", "am", "sch\u00f6ns\u00b7ten", "thut", "/", "l\u00e4sst", "uns", "j\u00e4m\u0303er\u00b7lich", "be\u00b7schnei\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ADV", "PPER", "APPRART", "ADJA", "VVFIN", "$(", "VVFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ja wir m\u00fcssen auch wol gar einen brand vom feuer leiden.", "tokens": ["Ja", "wir", "m\u00fcs\u00b7sen", "auch", "wol", "gar", "ei\u00b7nen", "brand", "vom", "feu\u00b7er", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VMFIN", "ADV", "ADV", "ADV", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}