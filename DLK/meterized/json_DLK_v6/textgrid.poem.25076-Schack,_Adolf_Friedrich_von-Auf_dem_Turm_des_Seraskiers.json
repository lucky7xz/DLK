{"textgrid.poem.25076": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Auf dem Turm des Seraskiers", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch Brausen um mich her? Mir ist, als wehte", "tokens": ["Welch", "Brau\u00b7sen", "um", "mich", "her", "?", "Mir", "ist", ",", "als", "weh\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "APPR", "PPER", "PTKVZ", "$.", "PPER", "VAFIN", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Sch\u00f6pfungsodem durch die Welt,", "tokens": ["Ein", "Sch\u00f6p\u00b7fun\u00b7gso\u00b7dem", "durch", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da unten sich die Kaiserin der St\u00e4dte", "tokens": ["Da", "un\u00b7ten", "sich", "die", "Kai\u00b7se\u00b7rin", "der", "St\u00e4d\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PRF", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im ersten Tagesstrahl erhellt.", "tokens": ["Im", "ers\u00b7ten", "Ta\u00b7ges\u00b7strahl", "er\u00b7hellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Herein durchs Klippenthor der Symplegaden", "tokens": ["Her\u00b7ein", "durchs", "Klip\u00b7pen\u00b7thor", "der", "Sym\u00b7ple\u00b7ga\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Melodisch saust der Bosporus", "tokens": ["Me\u00b7lo\u00b7disch", "saust", "der", "Bos\u00b7po\u00b7rus"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und giebt, aufsch\u00e4umend an den Felsgestaden,", "tokens": ["Und", "giebt", ",", "auf\u00b7sch\u00e4u\u00b7mend", "an", "den", "Fels\u00b7ge\u00b7sta\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zwei Welten seinen Wogenku\u00df.", "tokens": ["Zwei", "Wel\u00b7ten", "sei\u00b7nen", "Wo\u00b7gen\u00b7ku\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Morgenwinde jagen Segelboote", "tokens": ["Die", "Mor\u00b7gen\u00b7win\u00b7de", "ja\u00b7gen", "Se\u00b7gel\u00b7boo\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Heran vom blauen Hellespont;", "tokens": ["He\u00b7ran", "vom", "blau\u00b7en", "Hel\u00b7les\u00b7pont", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Fern strahlt das Schneehaupt des Olymp, vom Rote", "tokens": ["Fern", "strahlt", "das", "Schnee\u00b7haupt", "des", "O\u00b7lymp", ",", "vom", "Ro\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Des nahen Lichtgestirns besonnt;", "tokens": ["Des", "na\u00b7hen", "Licht\u00b7ges\u00b7tirns", "be\u00b7sonnt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und hoch und h\u00f6her leuchten auf die Dome,", "tokens": ["Und", "hoch", "und", "h\u00f6\u00b7her", "leuch\u00b7ten", "auf", "die", "Do\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wei\u00dfe Minarete gl\u00fchn,", "tokens": ["Und", "wei\u00b7\u00dfe", "Mi\u00b7na\u00b7re\u00b7te", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-----+", "measure": "dactylic.init"}, "line.3": {"text": "Friedh\u00f6fe, Brunnen, m\u00e4cht'ge Hippodrome", "tokens": ["Fried\u00b7h\u00f6\u00b7fe", ",", "Brun\u00b7nen", ",", "m\u00e4cht'\u00b7ge", "Hip\u00b7po\u00b7dro\u00b7me"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus dunkelndem Cypressengr\u00fcn.", "tokens": ["Aus", "dun\u00b7keln\u00b7dem", "Cyp\u00b7res\u00b7sen\u00b7gr\u00fcn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Doch \u00fcber all der Pracht mit tr\u00fcbem Blicke", "tokens": ["Doch", "\u00fc\u00b7ber", "all", "der", "Pracht", "mit", "tr\u00fc\u00b7bem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seh' ich am Horizonte schon", "tokens": ["Seh'", "ich", "am", "Ho\u00b7ri\u00b7zon\u00b7te", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die d\u00fcstre Wetterwolke der Geschicke,", "tokens": ["Die", "d\u00fcst\u00b7re", "Wet\u00b7ter\u00b7wol\u00b7ke", "der", "Ge\u00b7schi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schwer von der Zukunft Schrecken, drohn;", "tokens": ["Schwer", "von", "der", "Zu\u00b7kunft", "Schre\u00b7cken", ",", "drohn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Ja, seh' auf hochbesch\u00e4umten Wogenpfaden", "tokens": ["Ja", ",", "seh'", "auf", "hoch\u00b7be\u00b7sch\u00e4um\u00b7ten", "Wo\u00b7gen\u00b7pfa\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im weltverheerenden Orkan,", "tokens": ["Im", "welt\u00b7ver\u00b7hee\u00b7ren\u00b7den", "Or\u00b7kan", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mit Blitzen und mit Wirbelwind beladen,", "tokens": ["Mit", "Blit\u00b7zen", "und", "mit", "Wir\u00b7bel\u00b7wind", "be\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Flotten aller L\u00e4nder nahn.", "tokens": ["Die", "Flot\u00b7ten", "al\u00b7ler", "L\u00e4n\u00b7der", "nahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Schlachtdonner und Gekrach und Flammenzischen,", "tokens": ["Schlacht\u00b7don\u00b7ner", "und", "Ge\u00b7krach", "und", "Flam\u00b7men\u00b7zi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn Tod die Feuerschl\u00fcnde spein,", "tokens": ["Wenn", "Tod", "die", "Feu\u00b7er\u00b7schl\u00fcn\u00b7de", "spein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wird bald gen Himmel schallen, und dazwischen", "tokens": ["Wird", "bald", "gen", "Him\u00b7mel", "schal\u00b7len", ",", "und", "da\u00b7zwi\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "NN", "VVINF", "$,", "KON", "PAV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Sterbenden das Jammerschrein.", "tokens": ["Von", "Ster\u00b7ben\u00b7den", "das", "Jam\u00b7mer\u00b7schrein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wie bleich dort durch des Morgens Purpurd\u00e4mpfe", "tokens": ["Wie", "bleich", "dort", "durch", "des", "Mor\u00b7gens", "Pur\u00b7pur\u00b7d\u00e4mp\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADV", "APPR", "ART", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Halbmond \u00fcber Stambul blinkt!", "tokens": ["Der", "Halb\u00b7mond", "\u00fc\u00b7ber", "Stam\u00b7bul", "blinkt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Zeit der Wehen und der Todeskr\u00e4mpfe,", "tokens": ["O", "Zeit", "der", "We\u00b7hen", "und", "der", "To\u00b7des\u00b7kr\u00e4mp\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bevor er ganz hinuntersinkt!", "tokens": ["Be\u00b7vor", "er", "ganz", "hin\u00b7un\u00b7ter\u00b7sinkt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}