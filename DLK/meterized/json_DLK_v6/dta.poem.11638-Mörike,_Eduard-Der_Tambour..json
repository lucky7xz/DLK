{"dta.poem.11638": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Der Tambour.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Da m\u00fc\u00dft' sie mit dem Regiment", "tokens": ["Da", "m\u00fc\u00dft'", "sie", "mit", "dem", "Re\u00b7gi\u00b7ment"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach Frankreich, \u00fcberall mit hin,", "tokens": ["Nach", "Fran\u00b7kreich", ",", "\u00fc\u00b7be\u00b7rall", "mit", "hin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ADV", "APPR", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und w\u00e4r' die Markedenterin.", "tokens": ["Und", "w\u00e4r'", "die", "Mar\u00b7ke\u00b7den\u00b7te\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Lager, wohl um Mitternacht,", "tokens": ["Im", "La\u00b7ger", ",", "wohl", "um", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn Niemand auf ist als die Wacht,", "tokens": ["Wenn", "Nie\u00b7mand", "auf", "ist", "als", "die", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "VAFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Alles schnarchet, Ro\u00df und Mann,", "tokens": ["Und", "Al\u00b7les", "schnar\u00b7chet", ",", "Ro\u00df", "und", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vor meiner Trommel s\u00e4\u00df' ich dann:", "tokens": ["Vor", "mei\u00b7ner", "Trom\u00b7mel", "s\u00e4\u00df'", "ich", "dann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Trommel m\u00fc\u00dft' eine Sch\u00fcssel seyn,", "tokens": ["Die", "Trom\u00b7mel", "m\u00fc\u00dft'", "ei\u00b7ne", "Sch\u00fcs\u00b7sel", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Ein warmes Sauerkraut darein,", "tokens": ["Ein", "war\u00b7mes", "Sau\u00b7er\u00b7kraut", "da\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die Schlegel Messer und Gabel,", "tokens": ["Die", "Schle\u00b7gel", "Mes\u00b7ser", "und", "Ga\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Eine lange Wurst mein Sabel,", "tokens": ["Ei\u00b7ne", "lan\u00b7ge", "Wurst", "mein", "Sa\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Mein Tschako w\u00e4r' ein Humpen gut,", "tokens": ["Mein", "Tscha\u00b7ko", "w\u00e4r'", "ein", "Hum\u00b7pen", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Gef\u00fcllet mit Burgunderblut,", "tokens": ["Ge\u00b7f\u00fcl\u00b7let", "mit", "Bur\u00b7gun\u00b7der\u00b7blut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und weil es mir an Lichte fehlt,", "tokens": ["Und", "weil", "es", "mir", "an", "Lich\u00b7te", "fehlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Da scheint der Mond in mein Gezelt,", "tokens": ["Da", "scheint", "der", "Mond", "in", "mein", "Ge\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Scheint er auch auf Franz\u00f6'sch herein,", "tokens": ["Scheint", "er", "auch", "auf", "Fran\u00b7z\u00f6'sch", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Mir f\u00e4llt doch meine Liebste ein:", "tokens": ["Mir", "f\u00e4llt", "doch", "mei\u00b7ne", "Liebs\u00b7te", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Ach weh! jezt hat der Spa\u00df ein End'!", "tokens": ["Ach", "weh", "!", "jezt", "hat", "der", "Spa\u00df", "ein", "End'", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$.", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "\u2014 Wenn nur meine Mutter hexen k\u00f6nnt'!", "tokens": ["Wenn", "nur", "mei\u00b7ne", "Mut\u00b7ter", "he\u00b7xen", "k\u00f6nnt'", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ADV", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}