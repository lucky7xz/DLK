{"dta.poem.15925": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "130.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von Zeit zu Zeit ein Schlag dem \u00fcberm\u00fct'gen Knaben,", "tokens": ["Von", "Zeit", "zu", "Zeit", "ein", "Schlag", "dem", "\u00fc\u00b7ber\u00b7m\u00fct'\u00b7gen", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lehrt ihn besonnener gebrauchen seine Gaben.", "tokens": ["Lehrt", "ihn", "be\u00b7son\u00b7ne\u00b7ner", "ge\u00b7brau\u00b7chen", "sei\u00b7ne", "Ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "O danke Gott, da\u00df dir zur rechten Zeit von oben", "tokens": ["O", "dan\u00b7ke", "Gott", ",", "da\u00df", "dir", "zur", "rech\u00b7ten", "Zeit", "von", "o\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NN", "$,", "KOUS", "PPER", "APPRART", "ADJA", "NN", "APPR", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ward immer solch ein Wink, wann du dich \u00fcberhoben.", "tokens": ["Ward", "im\u00b7mer", "solch", "ein", "Wink", ",", "wann", "du", "dich", "\u00fc\u00b7berh\u00b7o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "ART", "NN", "$,", "PWAV", "PPER", "PRF", "VVINF