{"textgrid.poem.35299": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Toten schweigen, doch die innre Qual,", "tokens": ["Die", "To\u00b7ten", "schwei\u00b7gen", ",", "doch", "die", "inn\u00b7re", "Qual", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Selbstanklage hat dich heimgesucht,", "tokens": ["Die", "Selbs\u00b7tan\u00b7kla\u00b7ge", "hat", "dich", "heim\u00b7ge\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Santos Per\u00e9z, und dich verflucht, verflucht!", "tokens": ["San\u00b7tos", "Per\u00e9z", ",", "und", "dich", "ver\u00b7flucht", ",", "ver\u00b7flucht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$,", "KON", "PPER", "VVPP", "$,", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Man l\u00fcgt nicht vor dem eignen Tribunal,", "tokens": ["Man", "l\u00fcgt", "nicht", "vor", "dem", "eig\u00b7nen", "Tri\u00b7bu\u00b7nal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Man lacht nicht \u00fcber seiner Ehre Fetzen;", "tokens": ["Man", "lacht", "nicht", "\u00fc\u00b7ber", "sei\u00b7ner", "Eh\u00b7re", "Fet\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was du gethan, erf\u00fcllt dich mit Entsetzen;", "tokens": ["Was", "du", "ge\u00b7than", ",", "er\u00b7f\u00fcllt", "dich", "mit", "Ent\u00b7set\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Du h\u00f6rst das Flehn der armen Kreatur \u2013 \u2013", "tokens": ["Du", "h\u00f6rst", "das", "Flehn", "der", "ar\u00b7men", "Kre\u00b7a\u00b7tur", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "O Held der W\u00fcste! Kinder zu entleiben,", "tokens": ["O", "Held", "der", "W\u00fcs\u00b7te", "!", "Kin\u00b7der", "zu", "ent\u00b7lei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$.", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Das eines Mannes Pflicht? \u2013 Gef\u00fcrchtet bleiben", "tokens": ["Das", "ei\u00b7nes", "Man\u00b7nes", "Pflicht", "?", "\u2013", "Ge\u00b7f\u00fcrch\u00b7tet", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PDS", "ART", "NN", "NN", "$.", "$(", "VVPP", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Mag deine starke Faust, die grause Spur,", "tokens": ["Mag", "dei\u00b7ne", "star\u00b7ke", "Faust", ",", "die", "grau\u00b7se", "Spur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Das warme Blut ist nicht mehr wegzureiben.", "tokens": ["Das", "war\u00b7me", "Blut", "ist", "nicht", "mehr", "weg\u00b7zu\u00b7rei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das Schicksal gab dich der Verfolgung preis;", "tokens": ["Das", "Schick\u00b7sal", "gab", "dich", "der", "Ver\u00b7fol\u00b7gung", "preis", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du flohst durchs Land wie ein gehetztes Wild,", "tokens": ["Du", "flohst", "durchs", "Land", "wie", "ein", "ge\u00b7hetz\u00b7tes", "Wild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ach, vor den Augen stets dasselbe Bild,", "tokens": ["Ach", ",", "vor", "den", "Au\u00b7gen", "stets", "das\u00b7sel\u00b7be", "Bild", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "ART", "NN", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und du, so jung an Jahren, doch ein Greis,", "tokens": ["Und", "du", ",", "so", "jung", "an", "Jah\u00b7ren", ",", "doch", "ein", "Greis", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADV", "ADJD", "APPR", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gebeugt, verzehrt von unheilbarem Kummer,", "tokens": ["Ge\u00b7beugt", ",", "ver\u00b7zehrt", "von", "un\u00b7heil\u00b7ba\u00b7rem", "Kum\u00b7mer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Angstvollen Tagen, N\u00e4chten ohne Schlummer,", "tokens": ["Angst\u00b7vol\u00b7len", "Ta\u00b7gen", ",", "N\u00e4ch\u00b7ten", "oh\u00b7ne", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und im Gehirn die namenlose Glut!", "tokens": ["Und", "im", "Ge\u00b7hirn", "die", "na\u00b7men\u00b7lo\u00b7se", "Glut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Menschen Strafgericht ist ein gelindes;", "tokens": ["Der", "Men\u00b7schen", "Straf\u00b7ge\u00b7richt", "ist", "ein", "ge\u00b7lin\u00b7des", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Doch bei dem leisen Gru\u00df des Abendwindes,", "tokens": ["Doch", "bei", "dem", "lei\u00b7sen", "Gru\u00df", "des", "Ab\u00b7end\u00b7win\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Im Sonnenschein, wie durch des Sturmes Wut \u2013", "tokens": ["Im", "Son\u00b7nen\u00b7schein", ",", "wie", "durch", "des", "Stur\u00b7mes", "Wut", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Du h\u00f6rtest stets das Weinen jenes Kindes.", "tokens": ["Du", "h\u00f6r\u00b7test", "stets", "das", "Wei\u00b7nen", "je\u00b7nes", "Kin\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nach langen Monden fanden sie dich dort", "tokens": ["Nach", "lan\u00b7gen", "Mon\u00b7den", "fan\u00b7den", "sie", "dich", "dort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PRF", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Hochgebirge, schleppten dich herab,", "tokens": ["Im", "Hoch\u00b7ge\u00b7bir\u00b7ge", ",", "schlepp\u00b7ten", "dich", "her\u00b7ab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Buenos-Ayres brach den Richterstab.", "tokens": ["Und", "Bu\u00b7e\u00b7nos\u00b7Ay\u00b7res", "brach", "den", "Rich\u00b7ter\u00b7stab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sein Anathem, war das ein Schreckenswort?", "tokens": ["Sein", "A\u00b7nat\u00b7hem", ",", "war", "das", "ein", "Schre\u00b7ckens\u00b7wort", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VAFIN", "PDS", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nein! Denn vergiftet war dein Lebensbecher.", "tokens": ["Nein", "!", "Denn", "ver\u00b7gif\u00b7tet", "war", "dein", "Le\u00b7bens\u00b7be\u00b7cher", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "KON", "VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Du starbst nicht wie ein zitternder Verbrecher,", "tokens": ["Du", "starbst", "nicht", "wie", "ein", "zit\u00b7tern\u00b7der", "Ver\u00b7bre\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Als Triumphator stiegst du aufs Schaffot", "tokens": ["Als", "Tri\u00b7um\u00b7pha\u00b7tor", "stiegst", "du", "aufs", "Schaf\u00b7fot"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und blicktest auf das Volk, das rohe, feile,", "tokens": ["Und", "blick\u00b7test", "auf", "das", "Volk", ",", "das", "ro\u00b7he", ",", "fei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und botest stolz dein Haupt dem Richterbeile,", "tokens": ["Und", "bo\u00b7test", "stolz", "dein", "Haupt", "dem", "Rich\u00b7ter\u00b7bei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Denn eine Stimme rief: \u00bbIch bitte Gott,", "tokens": ["Denn", "ei\u00b7ne", "Stim\u00b7me", "rief", ":", "\u00bb", "Ich", "bit\u00b7te", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$.", "$(", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Da\u00df er auch deine Wunden wieder heile.\u00ab", "tokens": ["Da\u00df", "er", "auch", "dei\u00b7ne", "Wun\u00b7den", "wie\u00b7der", "hei\u00b7le", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "ADV", "ADJA", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}