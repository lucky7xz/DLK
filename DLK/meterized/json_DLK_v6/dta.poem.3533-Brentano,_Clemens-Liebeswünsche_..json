{"dta.poem.3533": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Liebesw\u00fcnsche .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf der Welt hab ich kein Freud,               ", "tokens": ["Auf", "der", "Welt", "hab", "ich", "kein", "Freud", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich hab ein Schatz und der ist weit;", "tokens": ["Ich", "hab", "ein", "Schatz", "und", "der", "ist", "weit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "ART", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn ich nur mit ihm reden k\u00f6nnt,", "tokens": ["Wenn", "ich", "nur", "mit", "ihm", "re\u00b7den", "k\u00f6nnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So w\u00e4r mein ganzes Herz gesund.", "tokens": ["So", "w\u00e4r", "mein", "gan\u00b7zes", "Herz", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Frau Nachtigall, Frau Nachtigall!", "tokens": ["Frau", "Nach\u00b7ti\u00b7gall", ",", "Frau", "Nach\u00b7ti\u00b7gall", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gr\u00fc\u00df meinen Schatz viel tausendmal;", "tokens": ["Gr\u00fc\u00df", "mei\u00b7nen", "Schatz", "viel", "tau\u00b7send\u00b7mal", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gr\u00fc\u00df ihn so h\u00fcbsch, gr\u00fc\u00df ihn so fein,", "tokens": ["Gr\u00fc\u00df", "ihn", "so", "h\u00fcbsch", ",", "gr\u00fc\u00df", "ihn", "so", "fein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADJD", "$,", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sag ihm er soll mein eigen seyn.", "tokens": ["Sag", "ihm", "er", "soll", "mein", "ei\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "VMFIN", "PPOSAT", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und komm ich vor ein Goldschmidtshaus,", "tokens": ["Und", "komm", "ich", "vor", "ein", "Gold\u00b7schmidts\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Goldschmidt schaut zum Fenster raus;", "tokens": ["Der", "Gold\u00b7schmidt", "schaut", "zum", "Fens\u00b7ter", "raus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach Goldschmidt, liebster Goldschmidt mein!", "tokens": ["Ach", "Gold\u00b7schmidt", ",", "liebs\u00b7ter", "Gold\u00b7schmidt", "mein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "ADJA", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schmied mir ein feines Ringelein.", "tokens": ["Schmied", "mir", "ein", "fei\u00b7nes", "Rin\u00b7ge\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Schmied's nicht zu gro\u00df, schmied's nicht zu klein,", "tokens": ["Schmie\u00b7d's", "nicht", "zu", "gro\u00df", ",", "schmie\u00b7d's", "nicht", "zu", "klein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "PTKA", "ADJD", "$,", "VVFIN", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Schmied's f\u00fcr ein sch\u00f6nes Fingerlein;", "tokens": ["Schmie\u00b7d's", "f\u00fcr", "ein", "sch\u00f6\u00b7nes", "Fin\u00b7ger\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Auch schmied mir meinen Namen dran,", "tokens": ["Auch", "schmied", "mir", "mei\u00b7nen", "Na\u00b7men", "dran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es solls mein Herzallerliebster han.", "tokens": ["Es", "solls", "mein", "Her\u00b7zal\u00b7ler\u00b7liebs\u00b7ter", "han", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "H\u00e4tt ich ein Schl\u00fcssel von rothem Gold,", "tokens": ["H\u00e4tt", "ich", "ein", "Schl\u00fcs\u00b7sel", "von", "ro\u00b7them", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mein Herz ich dir aufschliessen wollt,", "tokens": ["Mein", "Herz", "ich", "dir", "auf\u00b7schlies\u00b7sen", "wollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein sch\u00f6nes Bild das ist darein,", "tokens": ["Ein", "sch\u00f6\u00b7nes", "Bild", "das", "ist", "da\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PDS", "VAFIN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Schatz es mu\u00df dein eignes seyn.", "tokens": ["Mein", "Schatz", "es", "mu\u00df", "dein", "eig\u00b7nes", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VMFIN", "PPOSAT", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wenn ich nur ein klein Waldv\u00f6glein w\u00e4r,", "tokens": ["Wenn", "ich", "nur", "ein", "klein", "Wald\u00b7v\u00f6\u00b7glein", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJD", "NN", "VAFIN", "$,"], "meter": "-+---++-+", "measure": "dactylic.init"}, "line.2": {"text": "So s\u00e4\u00df ich auf dem gr\u00fcnen Zweig;", "tokens": ["So", "s\u00e4\u00df", "ich", "auf", "dem", "gr\u00fc\u00b7nen", "Zweig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn ich genug gepfiffen h\u00e4tt,", "tokens": ["Und", "wenn", "ich", "ge\u00b7nug", "ge\u00b7pfif\u00b7fen", "h\u00e4tt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Fl\u00f6g ich zu dir, mein Schatz ins Reich.", "tokens": ["Fl\u00f6g", "ich", "zu", "dir", ",", "mein", "Schatz", "ins", "Reich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPER", "$,", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wenn ich zwey Taubenfl\u00fcgel h\u00e4tt,", "tokens": ["Wenn", "ich", "zwey", "Tau\u00b7ben\u00b7fl\u00fc\u00b7gel", "h\u00e4tt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "CARD", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wollt fliegen \u00fcber die ganze Welt;", "tokens": ["Wollt", "flie\u00b7gen", "\u00fc\u00b7ber", "die", "gan\u00b7ze", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich wollt fliegen \u00fcber Berg und Thal,", "tokens": ["Ich", "wollt", "flie\u00b7gen", "\u00fc\u00b7ber", "Berg", "und", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hin wo mein Herzallerliebster w\u00e4r.", "tokens": ["Hin", "wo", "mein", "Her\u00b7zal\u00b7ler\u00b7liebs\u00b7ter", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWAV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Und wann ich endlich bey dir w\u00e4r,", "tokens": ["Und", "wann", "ich", "end\u00b7lich", "bey", "dir", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und du redst dann kein Wort mit mir;", "tokens": ["Und", "du", "redst", "dann", "kein", "Wort", "mit", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PIAT", "NN", "APPR", "PPER", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "M\u00fc\u00dft ich in Trauren wieder fort,", "tokens": ["M\u00fc\u00dft", "ich", "in", "Trau\u00b7ren", "wie\u00b7der", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Adje mein Schatz, adje von dir.", "tokens": ["Ad\u00b7je", "mein", "Schatz", ",", "ad\u00b7je", "von", "dir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}