{"textgrid.poem.55112": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "8", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Blick von deinen Augen in die meinen,", "tokens": ["Ein", "Blick", "von", "dei\u00b7nen", "Au\u00b7gen", "in", "die", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "APPR", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Ku\u00df von deinem Mund auf meinem Munde,", "tokens": ["Ein", "Ku\u00df", "von", "dei\u00b7nem", "Mund", "auf", "mei\u00b7nem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer davon hat, wie ich, gewisse Kunde,", "tokens": ["Wer", "da\u00b7von", "hat", ",", "wie", "ich", ",", "ge\u00b7wis\u00b7se", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PAV", "VAFIN", "$,", "PWAV", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mag dem was anders wohl erfreulich scheinen?", "tokens": ["Mag", "dem", "was", "an\u00b7ders", "wohl", "er\u00b7freu\u00b7lich", "schei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "PRELS", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Entfernt von dir, entfremdet von den Meinen,", "tokens": ["Ent\u00b7fernt", "von", "dir", ",", "ent\u00b7frem\u00b7det", "von", "den", "Mei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPER", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fchr ich stets die Gedanken in die Runde,", "tokens": ["F\u00fchr", "ich", "stets", "die", "Ge\u00b7dan\u00b7ken", "in", "die", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und immer treffen sie auf jene Stunde,", "tokens": ["Und", "im\u00b7mer", "tref\u00b7fen", "sie", "auf", "je\u00b7ne", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die einzige; da fang ich an zu weinen.", "tokens": ["Die", "ein\u00b7zi\u00b7ge", ";", "da", "fang", "ich", "an", "zu", "wei\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$.", "ADV", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Tr\u00e4ne trocknet wieder unversehens:", "tokens": ["Die", "Tr\u00e4\u00b7ne", "trock\u00b7net", "wie\u00b7der", "un\u00b7ver\u00b7se\u00b7hens", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er liebt ja, denk ich, her in diese Stille,", "tokens": ["Er", "liebt", "ja", ",", "denk", "ich", ",", "her", "in", "die\u00b7se", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "VVFIN", "PPER", "$,", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und solltest du nicht in die Ferne reichen?", "tokens": ["Und", "soll\u00b7test", "du", "nicht", "in", "die", "Fer\u00b7ne", "rei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Vernimm das Lispeln dieses Liebewehens;", "tokens": ["Ver\u00b7nimm", "das", "Lis\u00b7peln", "die\u00b7ses", "Lie\u00b7be\u00b7we\u00b7hens", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein einzig Gl\u00fcck auf Erden ist dein Wille,", "tokens": ["Mein", "ein\u00b7zig", "Gl\u00fcck", "auf", "Er\u00b7den", "ist", "dein", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "APPR", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein freundlicher zu mir; gib mir ein Zeichen!", "tokens": ["Dein", "freund\u00b7li\u00b7cher", "zu", "mir", ";", "gib", "mir", "ein", "Zei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "APPR", "PPER", "$.", "VVIMP", "PPER", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}