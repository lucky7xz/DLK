{"textgrid.poem.65810": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "84", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wer das meiste sagt \u2013 wer \u00fcberbietet", "tokens": ["Und", "wer", "das", "meis\u00b7te", "sagt", "\u2013", "wer", "\u00fc\u00b7berb\u00b7ie\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWS", "ART", "PIS", "VVFIN", "$(", "PWS", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In des Bezirk sich aller Wert umfriedet,", "tokens": ["In", "des", "Be\u00b7zirk", "sich", "al\u00b7ler", "Wert", "um\u00b7frie\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wonach zu messen wer dir gleich soll sein.", "tokens": ["Wo\u00b7nach", "zu", "mes\u00b7sen", "wer", "dir", "gleich", "soll", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "PWS", "PPER", "ADV", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Unfruchtbar d\u00fcrftig w\u00e4r f\u00fcrwahr der Mann,", "tokens": ["Un\u00b7frucht\u00b7bar", "d\u00fcrf\u00b7tig", "w\u00e4r", "f\u00fcr\u00b7wahr", "der", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dem nicht der kleinste Schmuck f\u00fcr seinen Stoff sich b\u00f6te:", "tokens": ["Dem", "nicht", "der", "kleins\u00b7te", "Schmuck", "f\u00fcr", "sei\u00b7nen", "Stoff", "sich", "b\u00f6\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch wer, von dir berichtend, sagen kann,", "tokens": ["Doch", "wer", ",", "von", "dir", "be\u00b7rich\u00b7tend", ",", "sa\u00b7gen", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "$,", "APPR", "PPER", "VVPP", "$,", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df du ", "tokens": ["Da\u00df", "du"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Er bilde nach nur, was an dir sich weist,", "tokens": ["Er", "bil\u00b7de", "nach", "nur", ",", "was", "an", "dir", "sich", "weist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "$,", "PRELS", "APPR", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Entkr\u00e4fte nicht der Sch\u00f6pfung sch\u00f6ne St\u00e4rke;", "tokens": ["Ent\u00b7kr\u00e4f\u00b7te", "nicht", "der", "Sch\u00f6p\u00b7fung", "sch\u00f6\u00b7ne", "St\u00e4r\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und solch ein Abbild lobt dann seinen Geist,", "tokens": ["Und", "solch", "ein", "Ab\u00b7bild", "lobt", "dann", "sei\u00b7nen", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ART", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Da\u00df alle Welt sich beugt vor seinem Werke.", "tokens": ["Da\u00df", "al\u00b7le", "Welt", "sich", "beugt", "vor", "sei\u00b7nem", "Wer\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PRF", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Du fluchst der Sch\u00f6nheit Segen, dir gef\u00e4llt", "tokens": ["Du", "fluchst", "der", "Sch\u00f6n\u00b7heit", "Se\u00b7gen", ",", "dir", "ge\u00b7f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,", "PPER", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Lob, das dein Lobenswertes nur entstellt.", "tokens": ["Lob", ",", "das", "dein", "Lo\u00b7bens\u00b7wer\u00b7tes", "nur", "ent\u00b7stellt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}