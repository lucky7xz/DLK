{"dta.poem.9639": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "8.  \n  Die vier bauern. Der erste bauer tr\u00f6stet sich  \n bey diesen schweren zeiten mit der guten  \n obrigkeit die wir haben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.57", "nl:0.42"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was wollen wir denn arme bauren/", "tokens": ["Was", "wol\u00b7len", "wir", "denn", "ar\u00b7me", "bau\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da alles heute sich ergetzt/", "tokens": ["Da", "al\u00b7les", "heu\u00b7te", "sich", "er\u00b7getzt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PRF", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Allein des krieges wegen trauren/", "tokens": ["Al\u00b7lein", "des", "krie\u00b7ges", "we\u00b7gen", "trau\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der uns in manche sorgen setzt?", "tokens": ["Der", "uns", "in", "man\u00b7che", "sor\u00b7gen", "setzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es sind ja freylich schwere zeiten/", "tokens": ["Es", "sind", "ja", "frey\u00b7lich", "schwe\u00b7re", "zei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Rhein ist voller hertzeleid;", "tokens": ["Der", "Rhein", "ist", "vol\u00b7ler", "hert\u00b7ze\u00b7leid", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch schauen wir es nur von weiten/", "tokens": ["Doch", "schau\u00b7en", "wir", "es", "nur", "von", "wei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "APPR", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Durch sorgfalt unsrer obrigkeit.", "tokens": ["Durch", "sorg\u00b7falt", "uns\u00b7rer", "ob\u00b7rig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wer wei\u00df nicht/ da\u00df ein Herr regieret/", "tokens": ["Wer", "wei\u00df", "nicht", "/", "da\u00df", "ein", "Herr", "re\u00b7gie\u00b7ret", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "$(", "KOUS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der uns an rechten vaters statt?", "tokens": ["Der", "uns", "an", "rech\u00b7ten", "va\u00b7ters", "statt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und der auch/ wie es ihm geb\u00fchret/", "tokens": ["Und", "der", "auch", "/", "wie", "es", "ihm", "ge\u00b7b\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "$(", "PWAV", "PPER", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die w\u00fcrdigste gemahlin hat?", "tokens": ["Die", "w\u00fcr\u00b7digs\u00b7te", "ge\u00b7mah\u00b7lin", "hat", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}