{"textgrid.poem.45476": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "An die N. Deutschen", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie haben mich nie verstanden", "tokens": ["Sie", "ha\u00b7ben", "mich", "nie", "ver\u00b7stan\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und verstehen mich jetzt noch nicht;", "tokens": ["Und", "ver\u00b7ste\u00b7hen", "mich", "jetzt", "noch", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "PTKNEG", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Es gibt in den deutschen Landen", "tokens": ["Es", "gibt", "in", "den", "deut\u00b7schen", "Lan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Viel Dichter \u2013 doch kein Gedicht.", "tokens": ["Viel", "Dich\u00b7ter", "\u2013", "doch", "kein", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$(", "ADV", "PIAT", "NN", "$."], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}}}}}