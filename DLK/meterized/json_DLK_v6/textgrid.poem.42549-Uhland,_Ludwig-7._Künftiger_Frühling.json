{"textgrid.poem.42549": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "7. K\u00fcnftiger Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl bl\u00fchet jedem Jahre", "tokens": ["Wohl", "bl\u00fc\u00b7het", "je\u00b7dem", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sein Fr\u00fchling, mild und licht,", "tokens": ["Sein", "Fr\u00fch\u00b7ling", ",", "mild", "und", "licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auch jener gro\u00dfe, klare \u2013", "tokens": ["Auch", "je\u00b7ner", "gro\u00b7\u00dfe", ",", "kla\u00b7re", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PDAT", "ADJA", "$,", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Getrost! er fehlt dir nicht;", "tokens": ["Ge\u00b7trost", "!", "er", "fehlt", "dir", "nicht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er ist dir noch beschieden", "tokens": ["Er", "ist", "dir", "noch", "be\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Am Ziele deiner Bahn,", "tokens": ["Am", "Zie\u00b7le", "dei\u00b7ner", "Bahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Du ahnest ihn hienieden,", "tokens": ["Du", "ah\u00b7nest", "ihn", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und droben bricht er an.", "tokens": ["Und", "dro\u00b7ben", "bricht", "er", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}