{"dta.poem.2520": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcckw\u00fcnschung auff das Nahmens-Licht Tit.  \n Fr. Barbara von Horn/ gebohrner Trachin  \n von Birckau/ rc. in einem andern Nahmen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nach schwartzgew\u00f6lckter Nacht und rauhem Donner-", "tokens": ["Nach", "schwartz\u00b7ge\u00b7w\u00f6lck\u00b7ter", "Nacht", "und", "rau\u00b7hem", "Don\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Knall/", "tokens": ["Knall", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Nach vieler Wetter-Sturm und ungeheurem Regen", "tokens": ["Nach", "vie\u00b7ler", "Wet\u00b7ter\u00b7Sturm", "und", "un\u00b7ge\u00b7heu\u00b7rem", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mu\u00df Aeol seinen Grimm/ und wildes Rasen legen;", "tokens": ["Mu\u00df", "A\u00b7e\u00b7ol", "sei\u00b7nen", "Grimm", "/", "und", "wil\u00b7des", "Ra\u00b7sen", "le\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "PPOSAT", "NE", "$(", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "Nach Wermuth-herbem Weh\u2019 und Gifft-vermischter Gall", "tokens": ["Nach", "Wer\u00b7muth\u00b7her\u00b7bem", "Weh'", "und", "Gifft\u00b7ver\u00b7mischter", "Gall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "NE"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Erfolgt ein Freuden-Kelch: Nach Schmertzen-reichen Stunden", "tokens": ["Er\u00b7folgt", "ein", "Freu\u00b7den\u00b7Kelch", ":", "Nach", "Schmert\u00b7zen\u00b7rei\u00b7chen", "Stun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Hat sich ein heller Blick de\u00df Himmels eingefunden.", "tokens": ["Hat", "sich", "ein", "hel\u00b7ler", "Blick", "de\u00df", "Him\u00b7mels", "ein\u00b7ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das ist das theure Licht ", "tokens": ["Das", "ist", "das", "theu\u00b7re", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Das Ph\u00f6bus wiederumb auff seinem g\u00f6ldnen Wagen", "tokens": ["Das", "Ph\u00f6\u00b7bus", "wie\u00b7de\u00b7rumb", "auff", "sei\u00b7nem", "g\u00f6ld\u00b7nen", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Durch die Saphyrne Burg de\u00df Himmels bringt getragen.", "tokens": ["Durch", "die", "Sa\u00b7phyr\u00b7ne", "Burg", "de\u00df", "Him\u00b7mels", "bringt", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "VVPP", "$."], "meter": "+-++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.11": {"text": "Es hat ihr Auge nun sich satsam au\u00dfgeweint/", "tokens": ["Es", "hat", "ihr", "Au\u00b7ge", "nun", "sich", "sat\u00b7sam", "au\u00df\u00b7ge\u00b7weint", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADV", "PRF", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Auff heute sey das Leid in Thetis Scho\u00df versencket/", "tokens": ["Auff", "heu\u00b7te", "sey", "das", "Leid", "in", "The\u00b7tis", "Scho\u00df", "ver\u00b7sen\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VAFIN", "ART", "NN", "APPR", "NE", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Was nutzt es da\u00df sie sich mit altem Kummer kr\u00e4ncket?", "tokens": ["Was", "nutzt", "es", "da\u00df", "sie", "sich", "mit", "al\u00b7tem", "Kum\u00b7mer", "kr\u00e4n\u00b7cket", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "KOUS", "PPER", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mir ist ihr edler Geist und hoher Sinn bekand/", "tokens": ["Mir", "ist", "ihr", "ed\u00b7ler", "Geist", "und", "ho\u00b7her", "Sinn", "be\u00b7kand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "KON", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Jhr Himmlisches Gem\u00fct hegt ungemeine Flammen", "tokens": ["Ihr", "Himm\u00b7li\u00b7sches", "Ge\u00b7m\u00fct", "hegt", "un\u00b7ge\u00b7mei\u00b7ne", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und pflegt die Niedrigkeit de\u00df P\u00f6fels zuverdammen/", "tokens": ["Und", "pflegt", "die", "Nied\u00b7rig\u00b7keit", "de\u00df", "P\u00f6\u00b7fels", "zu\u00b7ver\u00b7dam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Geht nur den Sternen nach/ mit denen es verwand/", "tokens": ["Geht", "nur", "den", "Ster\u00b7nen", "nach", "/", "mit", "de\u00b7nen", "es", "ver\u00b7wand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "$(", "APPR", "PRELS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und l\u00e4sset unter sich die feigen Seelen liegen/", "tokens": ["Und", "l\u00e4s\u00b7set", "un\u00b7ter", "sich", "die", "fei\u00b7gen", "See\u00b7len", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PRF", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Indem es ist gewohnt ob Zeit und Neid zusiegen.", "tokens": ["In\u00b7dem", "es", "ist", "ge\u00b7wohnt", "ob", "Zeit", "und", "Neid", "zu\u00b7sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "VVPP", "KOUS", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wem ist nicht diese Bahn der Sterbligkeit bewust?", "tokens": ["Wem", "ist", "nicht", "die\u00b7se", "Bahn", "der", "Ster\u00b7blig\u00b7keit", "be\u00b7wust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PTKNEG", "PDAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wir k\u00f6nnen nicht allzeit auff Lilg und Rosen gehen/", "tokens": ["Wir", "k\u00f6n\u00b7nen", "nicht", "all\u00b7zeit", "auff", "Lilg", "und", "Ro\u00b7sen", "ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "APPR", "NE", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Offt mu\u00df ein Helden-Muth bey Dorn und Disteln stehen:", "tokens": ["Offt", "mu\u00df", "ein", "Hel\u00b7den\u00b7Muth", "bey", "Dorn", "und", "Dis\u00b7teln", "ste\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ein Weichling zeiget nie den Feinden seine Brust:", "tokens": ["Ein", "Weich\u00b7ling", "zei\u00b7get", "nie", "den", "Fein\u00b7den", "sei\u00b7ne", "Brust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der Ehren Tempel steht auff hoher Felsen Spitzen/", "tokens": ["Der", "Eh\u00b7ren", "Tem\u00b7pel", "steht", "auff", "ho\u00b7her", "Fel\u00b7sen", "Spit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und was hochsch\u00e4tzbar ist/ erlangt M\u00fch/ Flei\u00df und Schwitzen.", "tokens": ["Und", "was", "hoch\u00b7sch\u00e4tz\u00b7bar", "ist", "/", "er\u00b7langt", "M\u00fch", "/", "Flei\u00df", "und", "Schwit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJD", "VAFIN", "$(", "VVFIN", "NE", "$(", "NN", "KON", "NN", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.26": {"text": "Die Proben die sie hat/ Amazonin/ gethan/", "tokens": ["Die", "Pro\u00b7ben", "die", "sie", "hat", "/", "A\u00b7ma\u00b7zo\u00b7nin", "/", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "VAFIN", "$(", "NE", "$(", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Sind w\u00fcrdig/ da\u00df man sie den Cedern einverleibe/", "tokens": ["Sind", "w\u00fcr\u00b7dig", "/", "da\u00df", "man", "sie", "den", "Ce\u00b7dern", "ein\u00b7ver\u00b7lei\u00b7be", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$(", "KOUS", "PIS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Da\u00df ihr Ged\u00e4chtn\u00fc\u00df man in Ertz und Marmel schreibe/", "tokens": ["Da\u00df", "ihr", "Ge\u00b7d\u00e4cht\u00b7n\u00fc\u00df", "man", "in", "Ertz", "und", "Mar\u00b7mel", "schrei\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PIS", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und k\u00fcndige ihr Lob der sp\u00e4ten Nachwelt an;", "tokens": ["Und", "k\u00fcn\u00b7di\u00b7ge", "ihr", "Lob", "der", "sp\u00e4\u00b7ten", "Nach\u00b7welt", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Nachdem durch Gottes-Schlu\u00df den Ehstand sie erwehlet/", "tokens": ["Nach\u00b7dem", "durch", "Got\u00b7tes\u00b7Schlu\u00df", "den", "Eh\u00b7stand", "sie", "er\u00b7weh\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "ART", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Hilff Gott! was hat sie nicht f\u00fcr Unheil da gezehlet;", "tokens": ["Hilff", "Gott", "!", "was", "hat", "sie", "nicht", "f\u00fcr", "Un\u00b7heil", "da", "ge\u00b7zeh\u00b7let", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWS", "VAFIN", "PPER", "PTKNEG", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Treu und Best\u00e4ndigkeit blieb ihrer Sinnen Ziel/", "tokens": ["Treu", "und", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "blieb", "ih\u00b7rer", "Sin\u00b7nen", "Ziel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.33": {"text": "Und hohe Tugend hie\u00df der Grund-Stein ihrer Liebe.", "tokens": ["Und", "ho\u00b7he", "Tu\u00b7gend", "hie\u00df", "der", "Grun\u00b7dStein", "ih\u00b7rer", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Es kam kein Tag so schwartz/ so neblich und so tr\u00fcbe", "tokens": ["Es", "kam", "kein", "Tag", "so", "schwartz", "/", "so", "neb\u00b7lich", "und", "so", "tr\u00fc\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADV", "ADJD", "$(", "ADV", "ADJD", "KON", "ADV", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Da\u00df nicht ihr Wahl-Spruch hie\u00df: ", "tokens": ["Da\u00df", "nicht", "ihr", "Wahl\u00b7Spruch", "hie\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.36": {"text": "Daher ihr Eh-Schatz auch in Zunder-reichen Flammen", "tokens": ["Da\u00b7her", "ihr", "Eh\u00b7Schatz", "auch", "in", "Zun\u00b7der\u00b7rei\u00b7chen", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPOSAT", "NN", "ADV", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Verkn\u00fcpffte Seel und Hertz in gleicher Treu zusammen.", "tokens": ["Ver\u00b7kn\u00fcpff\u00b7te", "Seel", "und", "Hertz", "in", "glei\u00b7cher", "Treu", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wie saur die Wirtschafft war/ wie schwer der Sorgen-Last/", "tokens": ["Wie", "saur", "die", "Wirt\u00b7schafft", "war", "/", "wie", "schwer", "der", "Sor\u00b7gen\u00b7Last", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VAFIN", "$(", "PWAV", "ADJD", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "So trat sie alles an dem Liebsten zugefallen/", "tokens": ["So", "trat", "sie", "al\u00b7les", "an", "dem", "Liebs\u00b7ten", "zu\u00b7ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So da\u00df der arge Neid auch seine Gifft und Gallen", "tokens": ["So", "da\u00df", "der", "ar\u00b7ge", "Neid", "auch", "sei\u00b7ne", "Gifft", "und", "Gal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "ADV", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Dar\u00fcber au\u00dfgespeyt. Doch was wol abgefast/", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "au\u00df\u00b7ge\u00b7speyt", ".", "Doch", "was", "wol", "ab\u00b7ge\u00b7fast", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVPP", "$.", "KON", "PWS", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und reifflich ist bedacht/ mu\u00df endlich doch gedeyen/", "tokens": ["Und", "reif\u00b7flich", "ist", "be\u00b7dacht", "/", "mu\u00df", "end\u00b7lich", "doch", "ge\u00b7de\u00b7yen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "VVPP", "$(", "VMFIN", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.43": {"text": "Man sahe Gl\u00fcck und Heil auff ihre Sorgen schneyen.", "tokens":