{"dta.poem.4297": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vergn\u00fcgliche  \n Betrachtung des Herbsts,  \n nebst  \n beygef\u00fcgten Dank-Gedanken  \n f\u00fcr  \n die erhaltene und gl\u00fccklich eingef\u00fchrte  \n Feld-Fr\u00fcchte,  \n  1737 .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer leugnen will, da\u00df nicht in jeder Jahres-Zeit", "tokens": ["Wer", "leug\u00b7nen", "will", ",", "da\u00df", "nicht", "in", "je\u00b7der", "Jah\u00b7res\u00b7Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVINF", "VMFIN", "$,", "KOUS", "PTKNEG", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Welt voll Schmuck und Zierlichkeit,", "tokens": ["Die", "Welt", "voll", "Schmuck", "und", "Zier\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der seh im fr\u00fchen Herbst, bey aufgekl\u00e4rtem Wetter,", "tokens": ["Der", "seh", "im", "fr\u00fc\u00b7hen", "Herbst", ",", "bey", "auf\u00b7ge\u00b7kl\u00e4r\u00b7tem", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bey dem durchstrahlten Gras und feuchten Glanz der", "tokens": ["Bey", "dem", "durch\u00b7strahl\u00b7ten", "Gras", "und", "feuch\u00b7ten", "Glanz", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "ADJA", "NN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Bl\u00e4tter,", "tokens": ["Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Der Felder leere Fl\u00e4chen an!", "tokens": ["Der", "Fel\u00b7der", "lee\u00b7re", "Fl\u00e4\u00b7chen", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die\u00df Leere selbst zeigt eine neue F\u00fclle", "tokens": ["Die\u00df", "Lee\u00b7re", "selbst", "zeigt", "ei\u00b7ne", "neu\u00b7e", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Von Lust und Anmuht, welche man", "tokens": ["Von", "Lust", "und", "An\u00b7muht", ",", "wel\u00b7che", "man"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "PRELS", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Fast f\u00fchlen mehr und seh\u2019n, als sie beschreiben, kann.", "tokens": ["Fast", "f\u00fch\u00b7len", "mehr", "und", "seh'n", ",", "als", "sie", "be\u00b7schrei\u00b7ben", ",", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "KON", "VVINF", "$,", "KOUS", "PPER", "VVPP", "$,", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es herrschet eine sanft\u2019 und angenehme Stille,", "tokens": ["Es", "herr\u00b7schet", "ei\u00b7ne", "sanft'", "und", "an\u00b7ge\u00b7neh\u00b7me", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So weit das Auge reicht. Das umgest\u00fcrzte Land,", "tokens": ["So", "weit", "das", "Au\u00b7ge", "reicht", ".", "Das", "um\u00b7ge\u00b7st\u00fcrz\u00b7te", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das vormahls h\u00f6her schien, erhoben,", "tokens": ["Das", "vor\u00b7mahls", "h\u00f6\u00b7her", "schien", ",", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und, durch der Aehren Fl\u00e4ch\u2019, als in die H\u00f6h\u2019 geschoben,", "tokens": ["Und", ",", "durch", "der", "A\u00b7eh\u00b7ren", "Fl\u00e4ch'", ",", "als", "in", "die", "H\u00f6h'", "ge\u00b7scho\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "NN", "NN", "$,", "KOUS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Den Augen gleichsam n\u00e4her stand,", "tokens": ["Den", "Au\u00b7gen", "gleich\u00b7sam", "n\u00e4\u00b7her", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Scheint itzo niedriger. Ein lieblich braun Gewand,", "tokens": ["Scheint", "it\u00b7zo", "nied\u00b7ri\u00b7ger", ".", "Ein", "lieb\u00b7lich", "braun", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$.", "ART", "ADJD", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mit langer Furchen dunklern Streifen,", "tokens": ["Mit", "lan\u00b7ger", "Fur\u00b7chen", "dunk\u00b7lern", "Strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die, durch den regen Pflug, itzt \u00fcberall sich h\u00e4ufen,", "tokens": ["Die", ",", "durch", "den", "re\u00b7gen", "Pflug", ",", "itzt", "\u00fc\u00b7be\u00b7rall", "sich", "h\u00e4u\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "ADV", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mit gr\u00fcnen Ranken doch noch hie und da bestreut,", "tokens": ["Mit", "gr\u00fc\u00b7nen", "Ran\u00b7ken", "doch", "noch", "hie", "und", "da", "be\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADV", "ADV", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ist itzt, in dieser holden Zeit,", "tokens": ["Ist", "itzt", ",", "in", "die\u00b7ser", "hol\u00b7den", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der milden Mutter ernsthaft Kleid.", "tokens": ["Der", "mil\u00b7den", "Mut\u00b7ter", "ernst\u00b7haft", "Kleid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Kl\u00f6sse heller Grau, zusammt den hellern H\u00f6hen", "tokens": ["Der", "Kl\u00f6s\u00b7se", "hel\u00b7ler", "Grau", ",", "zu\u00b7sammt", "den", "hel\u00b7lern", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Furchen, wovon wir auch kleine Schatten sehen,", "tokens": ["Der", "Fur\u00b7chen", ",", "wo\u00b7von", "wir", "auch", "klei\u00b7ne", "Schat\u00b7ten", "se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erheben, zieren noch und schm\u00fccken,", "tokens": ["Er\u00b7he\u00b7ben", ",", "zie\u00b7ren", "noch", "und", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was man sonst auf der Felder R\u00fccken", "tokens": ["Was", "man", "sonst", "auf", "der", "Fel\u00b7der", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PIS", "ADV", "APPR", "ART", "NN", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Zu einfach und zu allgemein,", "tokens": ["Zu", "ein\u00b7fach", "und", "zu", "all\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "KON", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In einer braunen Farb\u2019 allein,", "tokens": ["In", "ei\u00b7ner", "brau\u00b7nen", "Fa\u00b7rb'", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und ohn\u2019 Ver\u00e4ndrung w\u00fcrd\u2019 erblicken.", "tokens": ["Und", "ohn'", "Ver\u00b7\u00e4n\u00b7drung", "w\u00fcrd'", "er\u00b7bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nachdem der Sonnen-Strahl nun mehr und minder", "tokens": ["Nach\u00b7dem", "der", "Son\u00b7nen\u00b7Strahl", "nun", "mehr", "und", "min\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "ADV", "KON", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "hei\u00df,", "tokens": ["hei\u00df", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Wird auch die braune Farb\u2019 bald mehr, bald minder wei\u00df.", "tokens": ["Wird", "auch", "die", "brau\u00b7ne", "Fa\u00b7rb'", "bald", "mehr", ",", "bald", "min\u00b7der", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "ADV", "ADV", "$,", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Wie Grau und Braun sich nun bald scheiden, und bald", "tokens": ["Wie", "Grau", "und", "Braun", "sich", "nun", "bald", "schei\u00b7den", ",", "und", "bald"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "NN", "KON", "NE", "PRF", "ADV", "ADV", "VVINF", "$,", "KON", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.12": {"text": "mischen,", "tokens": ["mi\u00b7schen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "So sucht doch hie und dort", "tokens": ["So", "sucht", "doch", "hie", "und", "dort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "KON", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Manch lieblich gr\u00fcner Platz, an manchem Ort,", "tokens": ["Manch", "lieb\u00b7lich", "gr\u00fc\u00b7ner", "Platz", ",", "an", "man\u00b7chem", "Ort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "ADJA", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Vom Grase, so durchstrahlt, das Aug\u2019 uns zu erfrischen.", "tokens": ["Vom", "Gra\u00b7se", ",", "so", "durch\u00b7strahlt", ",", "das", "Aug'", "uns", "zu", "er\u00b7fri\u00b7schen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADV", "VVPP", "$,", "ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Zumahl bem\u00fcht der Schilf, den an den langen Graben", "tokens": ["Zu\u00b7mahl", "be\u00b7m\u00fcht", "der", "Schilf", ",", "den", "an", "den", "lan\u00b7gen", "Gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wir hin und her annoch zu sehen haben,", "tokens": ["Wir", "hin", "und", "her", "an\u00b7noch", "zu", "se\u00b7hen", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "KON", "ADV", "ADV", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Mit dem durchstrahlten holden Gr\u00fcnen,", "tokens": ["Mit", "dem", "durch\u00b7strahl\u00b7ten", "hol\u00b7den", "Gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Vom g\u00fcldnen Sonnen-Strahl oft durch-oft angeschie-", "tokens": ["Vom", "g\u00fcld\u00b7nen", "Son\u00b7nen\u00b7Strahl", "oft", "durch\u00b7oft", "an\u00b7ge\u00b7schie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "nen,", "tokens": ["nen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.21": {"text": "Uns sich, mit ihrem Schmuck, zur Augen-Lust zu dienen.", "tokens": ["Uns", "sich", ",", "mit", "ih\u00b7rem", "Schmuck", ",", "zur", "Au\u00b7gen\u00b7Lust", "zu", "die\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "$,", "APPR", "PPOSAT", "NN", "$,", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Indem ich also j\u00fcngst, zur fr\u00fchen Morgens-Zeit,", "tokens": ["In\u00b7dem", "ich", "al\u00b7so", "j\u00fcngst", ",", "zur", "fr\u00fc\u00b7hen", "Mor\u00b7gens\u00b7Zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In diesem weiten Feld, und seiner Lieblichkeit,", "tokens": ["In", "die\u00b7sem", "wei\u00b7ten", "Feld", ",", "und", "sei\u00b7ner", "Lieb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Fast blo\u00df in meinen Augen lebte,", "tokens": ["Fast", "blo\u00df", "in", "mei\u00b7nen", "Au\u00b7gen", "leb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und, beym entw\u00f6lkten Sonnen-Licht,", "tokens": ["Und", ",", "beym", "ent\u00b7w\u00f6lk\u00b7ten", "Son\u00b7nen\u00b7Licht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein auf das stille Feld gerichtetes Gesicht", "tokens": ["Mein", "auf", "das", "stil\u00b7le", "Feld", "ge\u00b7rich\u00b7te\u00b7tes", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "APPR", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sanft \u00fcber seine Fl\u00e4che schwebte,", "tokens": ["Sanft", "\u00fc\u00b7ber", "sei\u00b7ne", "Fl\u00e4\u00b7che", "schweb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Auch ich, mit frohem Ernst, mich recht bestrebte,", "tokens": ["Auch", "ich", ",", "mit", "fro\u00b7hem", "Ernst", ",", "mich", "recht", "be\u00b7streb\u00b7te", ","], "token_info": ["word", "