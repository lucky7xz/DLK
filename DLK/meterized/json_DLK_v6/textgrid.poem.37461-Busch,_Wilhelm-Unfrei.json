{"textgrid.poem.37461": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Unfrei", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ganz richtig, diese Welt ist nichtig.", "tokens": ["Ganz", "rich\u00b7tig", ",", "die\u00b7se", "Welt", "ist", "nich\u00b7tig", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PDAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch du, der in Person erscheint,", "tokens": ["Auch", "du", ",", "der", "in", "Per\u00b7son", "er\u00b7scheint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Bist ebenfalls nicht gar so wichtig,", "tokens": ["Bist", "e\u00b7ben\u00b7falls", "nicht", "gar", "so", "wich\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie deine Eitelkeit vermeint.", "tokens": ["Wie", "dei\u00b7ne", "Ei\u00b7tel\u00b7keit", "ver\u00b7meint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was hilft es dir, damit zu prahlen,", "tokens": ["Was", "hilft", "es", "dir", ",", "da\u00b7mit", "zu", "prah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "$,", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df du ein freies Menschenkind?", "tokens": ["Da\u00df", "du", "ein", "frei\u00b7es", "Men\u00b7schen\u00b7kind", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mu\u00dft du nicht p\u00fcnktlich Steuern zahlen,", "tokens": ["Mu\u00dft", "du", "nicht", "p\u00fcnkt\u00b7lich", "Steu\u00b7ern", "zah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Obwohl sie dir zuwider sind?", "tokens": ["Ob\u00b7wohl", "sie", "dir", "zu\u00b7wi\u00b7der", "sind", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "W\u00e4rst du vielleicht auch, sozusagen,", "tokens": ["W\u00e4rst", "du", "viel\u00b7leicht", "auch", ",", "so\u00b7zu\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "$,", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erhaben \u00fcber Gut und Schlecht,", "tokens": ["Er\u00b7ha\u00b7ben", "\u00fc\u00b7ber", "Gut", "und", "Schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Trotzdem behandelt dich dein Magen", "tokens": ["Trotz\u00b7dem", "be\u00b7han\u00b7delt", "dich", "dein", "Ma\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als ganz gemeinen Futterknecht.", "tokens": ["Als", "ganz", "ge\u00b7mei\u00b7nen", "Fut\u00b7ter\u00b7knecht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Lang bleibst du \u00fcberhaupt nicht munter.", "tokens": ["Lang", "bleibst", "du", "\u00fc\u00b7ber\u00b7haupt", "nicht", "mun\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Alter kommt und zieht dich krumm", "tokens": ["Das", "Al\u00b7ter", "kommt", "und", "zieht", "dich", "krumm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und st\u00f6\u00dft dich r\u00fccksichslos hinunter", "tokens": ["Und", "st\u00f6\u00dft", "dich", "r\u00fcck\u00b7sichs\u00b7los", "hin\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ins dunkle Sammelsurium.", "tokens": ["Ins", "dunk\u00b7le", "Sam\u00b7mel\u00b7su\u00b7ri\u00b7um", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Daselbst umf\u00e4ngt dich das Gewimmel", "tokens": ["Da\u00b7selbst", "um\u00b7f\u00e4ngt", "dich", "das", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Unsichtbaren, wie zuerst,", "tokens": ["Der", "Un\u00b7sicht\u00b7ba\u00b7ren", ",", "wie", "zu\u00b7erst", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Eh du erschienst, und nur der Himmel", "tokens": ["Eh", "du", "er\u00b7schienst", ",", "und", "nur", "der", "Him\u00b7mel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KON", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wei\u00df, ob und wann du wiederkehrst.", "tokens": ["Wei\u00df", ",", "ob", "und", "wann", "du", "wie\u00b7der\u00b7kehrst", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "KON", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}