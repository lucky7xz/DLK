{"textgrid.poem.56163": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O trotz Schicksal: die herrlichen \u00dcberfl\u00fcsse", "tokens": ["O", "trotz", "Schick\u00b7sal", ":", "die", "herr\u00b7li\u00b7chen", "\u00dc\u00b7berf\u00b7l\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "APPR", "NN", "$.", "ART", "ADJA", "NN"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "unseres Daseins, in Parken \u00fcbergesch\u00e4umt, \u2013", "tokens": ["un\u00b7se\u00b7res", "Da\u00b7seins", ",", "in", "Par\u00b7ken", "\u00fc\u00b7ber\u00b7ge\u00b7sch\u00e4umt", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "NN", "VVPP", "$,", "$("], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "oder als steinerne M\u00e4nner neben die Schl\u00fcsse", "tokens": ["o\u00b7der", "als", "stei\u00b7ner\u00b7ne", "M\u00e4n\u00b7ner", "ne\u00b7ben", "die", "Schl\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "hoher Portale, unter Balkone geb\u00e4umt!", "tokens": ["ho\u00b7her", "Por\u00b7ta\u00b7le", ",", "un\u00b7ter", "Bal\u00b7ko\u00b7ne", "ge\u00b7b\u00e4umt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "+--+-+-+---+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "O die eherne Glocke, die ihre Keule", "tokens": ["O", "die", "e\u00b7her\u00b7ne", "Glo\u00b7cke", ",", "die", "ih\u00b7re", "Keu\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "t\u00e4glich wider den stumpfen Alltag hebt.", "tokens": ["t\u00e4g\u00b7lich", "wi\u00b7der", "den", "stump\u00b7fen", "All\u00b7tag", "hebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Oder die ", "tokens": ["O\u00b7der", "die"], "token_info": ["word", "word"], "pos": ["KON", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.4": {"text": "die fast ewige Tempel \u00fcberlebt.", "tokens": ["die", "fast", "e\u00b7wi\u00b7ge", "Tem\u00b7pel", "\u00fc\u00b7ber\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Aber das Rasen zergeht und l\u00e4\u00dft keine Spuren.", "tokens": ["A\u00b7ber", "das", "Ra\u00b7sen", "zer\u00b7geht", "und", "l\u00e4\u00dft", "kei\u00b7ne", "Spu\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "VVFIN", "PIAT", "NN", "$."], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Kurven des Flugs durch die Luft und die, die sie fuhren,", "tokens": ["Kur\u00b7ven", "des", "Flugs", "durch", "die", "Luft", "und", "die", ",", "die", "sie", "fuh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "KON", "ART", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+--+--+-++-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "keine vielleicht ist umsonst. Doch nur wie gedacht.", "tokens": ["kei\u00b7ne", "viel\u00b7leicht", "ist", "um\u00b7sonst", ".", "Doch", "nur", "wie", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADV", "VAFIN", "ADV", "$.", "KON", "ADV", "KOKOM", "VVPP", "$."], "meter": "+--+-+---+-+", "measure": "iambic.penta.invert"}}}}}