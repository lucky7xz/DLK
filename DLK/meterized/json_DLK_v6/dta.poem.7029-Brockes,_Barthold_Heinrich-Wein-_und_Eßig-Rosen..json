{"dta.poem.7029": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wein- und E\u00dfig-Rosen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es suchen dem Geruch, und dem Gesicht,", "tokens": ["Es", "su\u00b7chen", "dem", "Ge\u00b7ruch", ",", "und", "dem", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die bunten Wein- und E\u00dfig-Rosen,", "tokens": ["Die", "bun\u00b7ten", "Wein", "und", "E\u00b7\u00dfig\u00b7Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sowol als jene, liebzukosen.", "tokens": ["So\u00b7wol", "als", "je\u00b7ne", ",", "lieb\u00b7zu\u00b7ko\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "KOKOM", "PDS", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Auge, da\u00df darauf gericht\u2019t,", "tokens": ["Ein", "Au\u00b7ge", ",", "da\u00df", "da\u00b7rauf", "ge\u00b7richt't", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Trifft auf denselbigen von neuen,", "tokens": ["Trifft", "auf", "den\u00b7sel\u00b7bi\u00b7gen", "von", "neu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDS", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ver\u00e4nderte besondre Wunder an,", "tokens": ["Ver\u00b7\u00e4n\u00b7der\u00b7te", "be\u00b7sond\u00b7re", "Wun\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die unser Hertz dnrchs Aug\u2019 erfreuen,", "tokens": ["Die", "un\u00b7ser", "Hertz", "dnrchs", "Aug'", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und die nach W\u00fcrden doch kein Kiel beschreiben kan.", "tokens": ["Und", "die", "nach", "W\u00fcr\u00b7den", "doch", "kein", "Kiel", "be\u00b7schrei\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "NN", "ADV", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}