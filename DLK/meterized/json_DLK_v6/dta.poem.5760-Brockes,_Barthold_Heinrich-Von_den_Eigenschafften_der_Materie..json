{"dta.poem.5760": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Eigenschafften der  \n Materie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein ausgespannter C\u00f6rper kan,", "tokens": ["Ein", "aus\u00b7ge\u00b7spann\u00b7ter", "C\u00f6r\u00b7per", "kan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man fange, wie man will, es an,", "tokens": ["Man", "fan\u00b7ge", ",", "wie", "man", "will", ",", "es", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWAV", "PIS", "VMFIN", "$,", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf keine Art denselben Ort,", "tokens": ["Auf", "kei\u00b7ne", "Art", "den\u00b7sel\u00b7ben", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den erst ein andrer f\u00fcllt, so lang er dort,", "tokens": ["Den", "erst", "ein", "an\u00b7drer", "f\u00fcllt", ",", "so", "lang", "er", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "VVFIN", "$,", "ADV", "ADJD", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Erf\u00fcllen.", "tokens": ["Er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Wofern ein Erster erst den Raum hat eingenommen;", "tokens": ["Wo\u00b7fern", "ein", "Ers\u00b7ter", "erst", "den", "Raum", "hat", "ein\u00b7ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So mu\u00df, um seinen Raum zu \u00fcberkommen,", "tokens": ["So", "mu\u00df", ",", "um", "sei\u00b7nen", "Raum", "zu", "\u00fc\u00b7ber\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Er erst vernichtig\u2019t seyn:", "tokens": ["Er", "erst", "ver\u00b7nich\u00b7tig't", "seyn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Man mag ihn pressen oder dr\u00fccken;", "tokens": ["Man", "mag", "ihn", "pres\u00b7sen", "o\u00b7der", "dr\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Kein andrer nimmt mit ihm zugleich die Stelle ein.", "tokens": ["Kein", "an\u00b7drer", "nimmt", "mit", "ihm", "zu\u00b7gleich", "die", "Stel\u00b7le", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "VVFIN", "APPR", "PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ist er gleich noch so klein;", "tokens": ["Ist", "er", "gleich", "noch", "so", "klein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Kan man ihn gleich nicht f\u00fchlen, nicht erblicken;", "tokens": ["Kan", "man", "ihn", "gleich", "nicht", "f\u00fch\u00b7len", ",", "nicht", "er\u00b7bli\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPER", "ADV", "PTKNEG", "VVINF", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Ist er doch ausgespannt, ist er doch undurchdringlich.", "tokens": ["Ist", "er", "doch", "aus\u00b7ge\u00b7spannt", ",", "ist", "er", "doch", "un\u00b7durch\u00b7dring\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$,", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Nimmt man die C\u00f6rper nun als ausgespannt;", "tokens": ["Nimmt", "man", "die", "C\u00f6r\u00b7per", "nun", "als", "aus\u00b7ge\u00b7spannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "ADV", "KOKOM", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "So k\u00f6nnen sie unm\u00f6glich sich bewegen,", "tokens": ["So", "k\u00f6n\u00b7nen", "sie", "un\u00b7m\u00f6g\u00b7lich", "sich", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Wofern sie nicht getheilt; und sich zu regen,", "tokens": ["Wo\u00b7fern", "sie", "nicht", "ge\u00b7theilt", ";", "und", "sich", "zu", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVPP", "$.", "KON", "PRF", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Jmgleichen", "tokens": ["Jm\u00b7glei\u00b7chen"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.18": {"text": "Geschickt gemachet sind einander auszuweichen.", "tokens": ["Ge\u00b7schickt", "ge\u00b7ma\u00b7chet", "sind", "ein\u00b7an\u00b7der", "aus\u00b7zu\u00b7wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAFIN", "PRF", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ist die Bewegung da, so zeigt sich gleich dabey,", "tokens": ["Ist", "die", "Be\u00b7we\u00b7gung", "da", ",", "so", "zeigt", "sich", "gleich", "da\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKVZ", "$,", "ADV", "VVFIN", "PRF", "ADV", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da\u00df die Materie nothwendig theilbar sey.", "tokens": ["Da\u00df", "die", "Ma\u00b7te\u00b7rie", "noth\u00b7wen\u00b7dig", "theil\u00b7bar", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}