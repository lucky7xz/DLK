{"textgrid.poem.33233": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein allerliebster Freund und werthester Herr Bruder,", "genre": "verse", "period": "N.A.", "pub_year": 1676, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein allerliebster Freund und werthester Herr Bruder,", "tokens": ["Mein", "al\u00b7ler\u00b7liebs\u00b7ter", "Freund", "und", "wert\u00b7hes\u00b7ter", "Herr", "Bru\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der du in Blumberg itzt versammelst deine Fuder,", "tokens": ["Der", "du", "in", "Blum\u00b7berg", "itzt", "ver\u00b7sam\u00b7melst", "dei\u00b7ne", "Fu\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der du, wie Tityrus, dort in dem Schatten liegst,", "tokens": ["Der", "du", ",", "wie", "Ti\u00b7ty\u00b7rus", ",", "dort", "in", "dem", "Schat\u00b7ten", "liegst", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PWAV", "NE", "$,", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und zehlest, was f\u00fcr Korn du in die Scheunen kriegst,", "tokens": ["Und", "zeh\u00b7lest", ",", "was", "f\u00fcr", "Korn", "du", "in", "die", "Scheu\u00b7nen", "kriegst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "APPR", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du d\u00fcrfftest dich f\u00fcrwahr so k\u00fcnstlich nicht bem\u00fchen,", "tokens": ["Du", "d\u00fcrff\u00b7test", "dich", "f\u00fcr\u00b7wahr", "so", "k\u00fcnst\u00b7lich", "nicht", "be\u00b7m\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "ADJD", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mich, durch ein sch\u00f6n Gedicht, aufs Land hinaus zu ziehen;", "tokens": ["Mich", ",", "durch", "ein", "sch\u00f6n", "Ge\u00b7dicht", ",", "aufs", "Land", "hin\u00b7aus", "zu", "zie\u00b7hen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ART", "ADJD", "NN", "$,", "APPRART", "NN", "APZR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es braucht, wilt du mich sehn, von dir ein eintzig Wort,", "tokens": ["Es", "braucht", ",", "wilt", "du", "mich", "sehn", ",", "von", "dir", "ein", "eint\u00b7zig", "Wort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VMFIN", "PPER", "PRF", "VVINF", "$,", "APPR", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dein Land-Gut ist f\u00fcr mich ein allzulieber Ort.", "tokens": ["Dein", "Lan\u00b7dGut", "ist", "f\u00fcr", "mich", "ein", "all\u00b7zu\u00b7lie\u00b7ber", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PPER", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich wei\u00df schon, wie man da die Stunden kan vertreiben;", "tokens": ["Ich", "wei\u00df", "schon", ",", "wie", "man", "da", "die", "Stun\u00b7den", "kan", "ver\u00b7trei\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "PIS", "ADV", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Feld-Lust h\u00e4ttest du nicht n\u00f6thig zu beschreiben,", "tokens": ["Die", "Feld\u00b7Lust", "h\u00e4t\u00b7test", "du", "nicht", "n\u00f6\u00b7thig", "zu", "be\u00b7schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dieweil mein freyer Geist, den Hof, zusammt der Stadt,", "tokens": ["Die\u00b7weil", "mein", "frey\u00b7er", "Geist", ",", "den", "Hof", ",", "zu\u00b7sammt", "der", "Stadt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mit Vorbedacht, wie du, schon offt vermieden hat.", "tokens": ["Mit", "Vor\u00b7be\u00b7dacht", ",", "wie", "du", ",", "schon", "offt", "ver\u00b7mie\u00b7den", "hat", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "PPER", "$,", "ADV", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Drum freut es mich recht sehr, da\u00df dieses stille Leben", "tokens": ["Drum", "freut", "es", "mich", "recht", "sehr", ",", "da\u00df", "die\u00b7ses", "stil\u00b7le", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ADV", "ADV", "$,", "KOUS", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dir eben so gef\u00e4llt, als ich ihm selbst ergeben;", "tokens": ["Dir", "e\u00b7ben", "so", "ge\u00b7f\u00e4llt", ",", "als", "ich", "ihm", "selbst", "er\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVPP", "$,", "KOUS", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und da wir beyderseits hierinn so gleich von Sinn,", "tokens": ["Und", "da", "wir", "bey\u00b7der\u00b7seits", "hie\u00b7rinn", "so", "gleich", "von", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Als eil ich desto mehr zu dir nach Blumberg hin,", "tokens": ["Als", "eil", "ich", "des\u00b7to", "mehr", "zu", "dir", "nach", "Blum\u00b7berg", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "ADV", "APPR", "PPER", "APPR", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Da wir auf eigne Hand uns k\u00f6nnen lustig machen,", "tokens": ["Da", "wir", "auf", "eig\u00b7ne", "Hand", "uns", "k\u00f6n\u00b7nen", "lus\u00b7tig", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PPER", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und, nebst der Eitelkeit, auch Welt und Hoff verlachen;", "tokens": ["Und", ",", "nebst", "der", "Ei\u00b7tel\u00b7keit", ",", "auch", "Welt", "und", "Hoff", "ver\u00b7la\u00b7chen", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "NN", "$,", "ADV", "NN", "KON", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da wir nicht so gepre\u00dft mit Schreiben auf die Post,", "tokens": ["Da", "wir", "nicht", "so", "ge\u00b7pre\u00dft", "mit", "Schrei\u00b7ben", "auf", "die", "Post", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVPP", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und da uns keiner jagt von unsrer Hau\u00dfmanns-Kost;", "tokens": ["Und", "da", "uns", "kei\u00b7ner", "jagt", "von", "uns\u00b7rer", "Hau\u00df\u00b7manns\u00b7Kost", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Da man, frey von dem Zwang bey grossen Potentaten,", "tokens": ["Da", "man", ",", "frey", "von", "dem", "Zwang", "bey", "gros\u00b7sen", "Po\u00b7ten\u00b7ta\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "ADJD", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "Sich satt fein friedlich i\u00dft von seinem eignen Braten;", "tokens": ["Sich", "satt", "fein", "fried\u00b7lich", "i\u00dft", "von", "sei\u00b7nem", "eig\u00b7nen", "Bra\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADJD", "ADJD", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Da keiner f\u00fcrchten darff Gewalt, Gefahr und List,", "tokens": ["Da", "kei\u00b7ner", "f\u00fcrch\u00b7ten", "darff", "Ge\u00b7walt", ",", "Ge\u00b7fahr", "und", "List", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVINF", "VMFIN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die einen grossen Han\u00df offt unversehens frist.", "tokens": ["Die", "ei\u00b7nen", "gros\u00b7sen", "Han\u00df", "offt", "un\u00b7ver\u00b7se\u00b7hens", "frist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ach! w\u00e4re mancher Held auch so daheim geblieben!", "tokens": ["Ach", "!", "w\u00e4\u00b7re", "man\u00b7cher", "Held", "auch", "so", "da\u00b7heim", "ge\u00b7blie\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VAFIN", "PIAT", "NN", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und h\u00e4tte nicht sein Gl\u00fcck so hoch hinausgetrieben,", "tokens": ["Und", "h\u00e4t\u00b7te", "nicht", "sein", "Gl\u00fcck", "so", "hoch", "hin\u00b7aus\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "PPOSAT", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "H\u00e4tt er sich nicht vergafft in Ehre, Macht und Geld,", "tokens": ["H\u00e4tt", "er", "sich", "nicht", "ver\u00b7gafft", "in", "Eh\u00b7re", ",", "Macht", "und", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "VVFIN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So w\u00fcrd er ietzo nicht vor solch Gericht gestellt.", "tokens": ["So", "w\u00fcrd", "er", "iet\u00b7zo", "nicht", "vor", "solch", "Ge\u00b7richt", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "Drum thun wir beyde wohl, dieweil wir uns bequemen,", "tokens": ["Drum", "thun", "wir", "bey\u00b7de", "wohl", ",", "die\u00b7weil", "wir", "uns", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PIS", "ADV", "$,", "KOUS", "PPER", "PRF", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Mit R\u00fcben, Kohl und Speck fein h\u00fcbsch vorlieb zu nehmen.", "tokens": ["Mit", "R\u00fc\u00b7ben", ",", "Kohl", "und", "Speck", "fein", "h\u00fcbsch", "vor\u00b7lieb", "zu", "neh\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "ADJD", "ADJD", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Bescheret uns dann Gott auch Wildpret oder Fisch,", "tokens": ["Be\u00b7sche\u00b7ret", "uns", "dann", "Gott", "auch", "Wild\u00b7pret", "o\u00b7der", "Fisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "ADV", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So sagen wir ihm Danck f\u00fcr solchen guten Tisch.", "tokens": ["So", "sa\u00b7gen", "wir", "ihm", "Danck", "f\u00fcr", "sol\u00b7chen", "gu\u00b7ten", "Tisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "NN", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ey nun! mein liebster Freund, in Hofnung, dich zu sprechen,", "tokens": ["Ey", "nun", "!", "mein", "liebs\u00b7ter", "Freund", ",", "in", "Hof\u00b7nung", ",", "dich", "zu", "spre\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$.", "PPOSAT", "ADJA", "NN", "$,", "APPR", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Will ich am Freytag fr\u00fch mit Sack und Pack aufbrechen.", "tokens": ["Will", "ich", "am", "Frey\u00b7tag", "fr\u00fch", "mit", "Sack", "und", "Pack", "auf\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "ADJD", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Mein Bruder kommt allein; Frau, Kinder bring ich mit;", "tokens": ["Mein", "Bru\u00b7der", "kommt", "al\u00b7lein", ";", "Frau", ",", "Kin\u00b7der", "bring", "ich", "mit", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$.", "NN", "$,", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der Pape wegen nur geh ich nicht einen Schritt.", "tokens": ["Der", "Pa\u00b7pe", "we\u00b7gen", "nur", "geh", "ich", "nicht", "ei\u00b7nen", "Schritt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "Ich wei\u00df gewi\u00dflich ihr sonst keinen Platz zu finden,", "tokens": ["Ich", "wei\u00df", "ge\u00b7wi\u00df\u00b7lich", "ihr", "sonst", "kei\u00b7nen", "Platz", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "ADV", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Als etwan hinten sie beym Bett-Sack aufzubinden;", "tokens": ["Als", "et\u00b7wan", "hin\u00b7ten", "sie", "beym", "Bet\u00b7tSack", "auf\u00b7zu\u00b7bin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "PPER", "APPRART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wann ihr nur sonst nicht was hier aus den Falten r\u00fcckt,", "tokens": ["Wann", "ihr", "nur", "sonst", "nicht", "was", "hier", "aus", "den", "Fal\u00b7ten", "r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "PTKNEG", "PRELS", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "An statt, da\u00df dort ihr Kopf im Wagen sich zerdr\u00fcckt.", "tokens": ["An", "statt", ",", "da\u00df", "dort", "ihr", "Kopf", "im", "Wa\u00b7gen", "sich", "zer\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "$,", "KOUS", "ADV", "PPOSAT", "NN", "APPRART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Es m\u00f6cht ihr auch dabey ein andrer Fall begegnen,", "tokens": ["Es", "m\u00f6cht", "ihr", "auch", "da\u00b7bey", "ein", "an\u00b7drer", "Fall", "be\u00b7geg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Da\u00df sie gar hinten k\u00f6nt ein Wolcken-Gu\u00df beregnen;", "tokens": ["Da\u00df", "sie", "gar", "hin\u00b7ten", "k\u00f6nt", "ein", "Wol\u00b7cken\u00b7Gu\u00df", "be\u00b7reg\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Alsdenn so h\u00fcllte sie sich gantz in Fuchspeltz ein,", "tokens": ["Als\u00b7denn", "so", "h\u00fcll\u00b7te", "sie", "sich", "gantz", "in", "Fuchs\u00b7peltz", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Und Pabgen k\u00f6nte so den Kindern Guckug! schreyn.", "tokens": ["Und", "Pab\u00b7gen", "k\u00f6n\u00b7te", "so", "den", "Kin\u00b7dern", "Gu\u00b7ckug", "!", "schreyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "ADV", "ART", "NN", "NE", "$.", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Herr Perband bittet sie in seinen hohen Wagen;", "tokens": ["Herr", "Per\u00b7band", "bit\u00b7tet", "sie", "in", "sei\u00b7nen", "ho\u00b7hen", "Wa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Allein, ich f\u00fcrchte sehr, sie m\u00f6chten sich da schlagen,", "tokens": ["Al\u00b7lein", ",", "ich", "f\u00fcrch\u00b7te", "sehr", ",", "sie", "m\u00f6ch\u00b7ten", "sich", "da", "schla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "ADV", "$,", "PPER", "VMFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Bi\u00df da\u00df die Federn gar von Pabgen alle fort,", "tokens": ["Bi\u00df", "da\u00df", "die", "Fe\u00b7dern", "gar", "von", "Pab\u00b7gen", "al\u00b7le", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "ADV", "APPR", "NN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Und keine mehr davon blieb an dem rechten Ort.", "tokens": ["Und", "kei\u00b7ne", "mehr", "da\u00b7von", "blieb", "an", "dem", "rech\u00b7ten", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PIS", "PAV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Sonst freu ich mich im Geist, wie du uns wirst empfangen,", "tokens": ["Sonst", "freu", "ich", "mich", "im", "Geist", ",", "wie", "du", "uns", "wirst", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,", "PWAV", "PPER", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Und fragen, wie es uns so lange Zeit ergangen?", "tokens": ["Und", "fra\u00b7gen", ",", "wie", "es", "uns", "so", "lan\u00b7ge", "Zeit", "er\u00b7gan\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PWAV", "PPER", "PPER", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Auch hast du hoffentlich zum Tisch in grosses Blat,", "tokens": ["Auch", "hast", "du", "hof\u00b7fent\u00b7lich", "zum", "Tisch", "in", "gros\u00b7ses", "Blat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Da man gem\u00e4chlich sitzt, bi\u00df Wirth und G\u00e4ste satt.", "tokens": ["Da", "man", "ge\u00b7m\u00e4ch\u00b7lich", "sitzt", ",", "bi\u00df", "Wirth", "und", "G\u00e4s\u00b7te", "satt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVFIN", "$,", "KOUS", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Nach diesem wirst du uns in deinen Garten f\u00fchren,", "tokens": ["Nach", "die\u00b7sem", "wirst", "du", "uns", "in", "dei\u00b7nen", "Gar\u00b7ten", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VAFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Und wir, im Gr\u00fcnen, da vergn\u00fcgt herumspatzieren.", "tokens": ["Und", "wir", ",", "im", "Gr\u00fc\u00b7nen", ",", "da", "ver\u00b7gn\u00fcgt", "her\u00b7um\u00b7spat\u00b7zie\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPRART", "NN", "$,", "ADV", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Weicht aber Ph\u00f6bus Glut aldann der k\u00fchlen Nacht,", "tokens": ["Weicht", "a\u00b7ber", "Ph\u00f6\u00b7bus", "Glut", "al\u00b7dann", "der", "k\u00fch\u00b7len", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NE", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "So ist f\u00fcr jeden schon ein sanfftes Bett gemacht.", "tokens": ["So", "ist", "f\u00fcr", "je\u00b7den", "schon", "ein", "sanff\u00b7tes", "Bett", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PIAT", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Werd ich, in meinem, nun zu Gustgen mich gesellen,", "tokens": ["Werd", "ich", ",", "in", "mei\u00b7nem", ",", "nun", "zu", "Gust\u00b7gen", "mich", "ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "APPR", "PPOSAT", "$,", "ADV", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "So thu de\u00dfgleichen auch bey deiner liebsten Drellen.", "tokens": ["So", "thu", "de\u00df\u00b7glei\u00b7chen", "auch", "bey", "dei\u00b7ner", "liebs\u00b7ten", "Drel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Ein Seegen macht vielleicht alsdann aus zweyen drey,", "tokens": ["Ein", "See\u00b7gen", "macht", "viel\u00b7leicht", "als\u00b7dann", "aus", "zwe\u00b7yen", "drey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "APPR", "VVFIN", "CARD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Da\u00df Blumberg ja so wohl als K\u00f6pnig fruchtbar sey.", "tokens": ["Da\u00df", "Blum\u00b7berg", "ja", "so", "wohl", "als", "K\u00f6p\u00b7nig", "frucht\u00b7bar", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ADV", "ADV", "KOUS", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "So geht es gut. Doch schlie\u00dft den Brief ein starckes: Aber!", "tokens": ["So", "geht", "es", "gut", ".", "Doch", "schlie\u00dft", "den", "Brief", "ein", "star\u00b7ckes", ":", "A\u00b7ber", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$.", "KON", "VVFIN", "ART", "NN", "ART", "ADJA", "$.", "KON", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Da\u00df vor die Pferde ja bereit sey Heu und Haber!", "tokens": ["Da\u00df", "vor", "die", "Pfer\u00b7de", "ja", "be\u00b7reit", "sey", "Heu", "und", "Ha\u00b7ber", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADV", "ADJD", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Dieweil ein t\u00fcchtig Ro\u00df auch gern was gutes frist,", "tokens": ["Die\u00b7weil", "ein", "t\u00fcch\u00b7tig", "Ro\u00df", "auch", "gern", "was", "gu\u00b7tes", "frist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "NN", "ADV", "ADV", "PWS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Wann es bey dir zu Gast mit mir gekommen ist.", "tokens": ["Wann", "es", "bey", "dir", "zu", "Gast", "mit", "mir", "ge\u00b7kom\u00b7men", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPER", "APPR", "NN", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.65": {"text": "Die Gelben mercken di\u00df, und fangen an zu prauschen.", "tokens": ["Die", "Gel\u00b7ben", "mer\u00b7cken", "di\u00df", ",", "und", "fan\u00b7gen", "an", "zu", "prau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "PDS", "$,", "KON", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Weil man uns gerne sieht, so la\u00dft die R\u00e4der rauschen!", "tokens": ["Weil", "man", "uns", "ger\u00b7ne", "sieht", ",", "so", "la\u00dft", "die", "R\u00e4\u00b7der", "rau\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Im \u00fcbrigen, so nimm mich auf f\u00fcr einen Gast,", "tokens": ["Im", "\u00fcb\u00b7ri\u00b7gen", ",", "so", "nimm", "mich", "auf", "f\u00fcr", "ei\u00b7nen", "Gast", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADV", "VVIMP", "PPER", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Dem du, als deinem Knecht, stets zu befehlen hast.", "tokens": ["Dem", "du", ",", "als", "dei\u00b7nem", "Knecht", ",", "stets", "zu", "be\u00b7feh\u00b7len", "hast", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "KOUS", "PPOSAT", "NN", "$,", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}