{"textgrid.poem.59569": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Kriegslied", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frischauf! Und ziehet die Schwerter aus!", "tokens": ["Frisc\u00b7hauf", "!", "Und", "zie\u00b7het", "die", "Schwer\u00b7ter", "aus", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und spornet die stampfenden Pferde!", "tokens": ["Und", "spor\u00b7net", "die", "stamp\u00b7fen\u00b7den", "Pfer\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Tyrannen erbr\u00fcsten sich \u00fcberaus", "tokens": ["Ty\u00b7ran\u00b7nen", "er\u00b7br\u00fcs\u00b7ten", "sich", "\u00fc\u00b7be\u00b7raus"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "ADV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und sch\u00e4nden mit Sklaven die Erde,", "tokens": ["Und", "sch\u00e4n\u00b7den", "mit", "Skla\u00b7ven", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Sie tilgen die Freiheit, sie l\u00f6schen das Licht,", "tokens": ["Sie", "til\u00b7gen", "die", "Frei\u00b7heit", ",", "sie", "l\u00f6\u00b7schen", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Wetzt ihr zur Erl\u00f6sung das Eisen nicht.", "tokens": ["Wetzt", "ihr", "zur", "Er\u00b7l\u00f6\u00b7sung", "das", "Ei\u00b7sen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "PTKNEG", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Wetzt ihr zur Rache nicht M\u00e4nnermut,", "tokens": ["Wetzt", "ihr", "zur", "Ra\u00b7che", "nicht", "M\u00e4n\u00b7ner\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKNEG", "NN", "$,"], "meter": "++-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Der sch\u00e4rfer noch schneidet als Eisen,", "tokens": ["Der", "sch\u00e4r\u00b7fer", "noch", "schnei\u00b7det", "als", "Ei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "VVFIN", "KOUS", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Blitzt ihr vom Auge nicht Feuerglut,", "tokens": ["Blitzt", "ihr", "vom", "Au\u00b7ge", "nicht", "Feu\u00b7er\u00b7glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKNEG", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Drin Tod und Verderben sich weisen,", "tokens": ["Drin", "Tod", "und", "Ver\u00b7der\u00b7ben", "sich", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "PRF", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Setzt ihr nicht den heiligen Schwur darein:", "tokens": ["Setzt", "ihr", "nicht", "den", "hei\u00b7li\u00b7gen", "Schwur", "da\u00b7rein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "VVFIN", "PAV", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Wir wollen heut frei oder Leichen sein.", "tokens": ["Wir", "wol\u00b7len", "heut", "frei", "o\u00b7der", "Lei\u00b7chen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "KON", "NN", "VAINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Frischauf! Und bindet die Herzen fest", "tokens": ["Frisc\u00b7hauf", "!", "Und", "bin\u00b7det", "die", "Her\u00b7zen", "fest"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "KON", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In dem gro\u00dfen, dem heiligen Bunde!", "tokens": ["In", "dem", "gro\u00b7\u00dfen", ",", "dem", "hei\u00b7li\u00b7gen", "Bun\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Vertilgt das tyrannische Schlangennest", "tokens": ["Ver\u00b7tilgt", "das", "ty\u00b7ran\u00b7ni\u00b7sche", "Schlan\u00b7gen\u00b7nest"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und schlaget wie w\u00fctende Hunde", "tokens": ["Und", "schla\u00b7get", "wie", "w\u00fc\u00b7ten\u00b7de", "Hun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Den Buben, der schalket, den Knecht, der dient,", "tokens": ["Den", "Bu\u00b7ben", ",", "der", "schal\u00b7ket", ",", "den", "Knecht", ",", "der", "dient", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$,", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Den Fremdling, der frech sich des Herrn erk\u00fchnt.", "tokens": ["Den", "Fremd\u00b7ling", ",", "der", "frech", "sich", "des", "Herrn", "er\u00b7k\u00fchnt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Sie wollen uns rauben das Vaterland,", "tokens": ["Sie", "wol\u00b7len", "uns", "rau\u00b7ben", "das", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Uns rauben die Ehre, die Treue,", "tokens": ["Uns", "rau\u00b7ben", "die", "Eh\u00b7re", ",", "die", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Uns wickeln franz\u00f6sisch in Lug und Tand,", "tokens": ["Uns", "wi\u00b7ckeln", "fran\u00b7z\u00f6\u00b7sisch", "in", "Lug", "und", "Tand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df Zwietracht die Starken entzweie,", "tokens": ["Da\u00df", "Zwiet\u00b7racht", "die", "Star\u00b7ken", "ent\u00b7zwei\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Sie wollen uns rauben das weidliche Wort,", "tokens": ["Sie", "wol\u00b7len", "uns", "rau\u00b7ben", "das", "weid\u00b7li\u00b7che", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Das sch\u00e4rfer als Pfeile die Schande durchbohrt.", "tokens": ["Das", "sch\u00e4r\u00b7fer", "als", "Pfei\u00b7le", "die", "Schan\u00b7de", "durch\u00b7bohrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KOUS", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Sie wollen uns rauben den ewigen Gott,", "tokens": ["Sie", "wol\u00b7len", "uns", "rau\u00b7ben", "den", "e\u00b7wi\u00b7gen", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Den Geist, den allm\u00e4chtigen Walter,", "tokens": ["Den", "Geist", ",", "den", "all\u00b7m\u00e4ch\u00b7ti\u00b7gen", "Wal\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die gaukelnden Affen \u2013 o Hohn und Spott!", "tokens": ["Die", "gau\u00b7keln\u00b7den", "Af\u00b7fen", "\u2013", "o", "Hohn", "und", "Spott", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "FM", "NN", "KON", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wir greifen zum Schwert, dem Erhalter,", "tokens": ["Wir", "grei\u00b7fen", "zum", "Schwert", ",", "dem", "Er\u00b7hal\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Wir waschen die Schanden uns ab in Blut:", "tokens": ["Wir", "wa\u00b7schen", "die", "Schan\u00b7den", "uns", "ab", "in", "Blut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "APPR", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Das blinkende Eisen macht alles gut.", "tokens": ["Das", "blin\u00b7ken\u00b7de", "Ei\u00b7sen", "macht", "al\u00b7les", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Drum frisch! Es walte das Gott der Herr,", "tokens": ["Drum", "frisch", "!", "Es", "wal\u00b7te", "das", "Gott", "der", "Herr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "$.", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Vater der Tapfern und Freien!", "tokens": ["Der", "Va\u00b7ter", "der", "Tap\u00b7fern", "und", "Frei\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sein hei\u00dft die R\u00fcstung und sein die Wehr,", "tokens": ["Sein", "hei\u00dft", "die", "R\u00fcs\u00b7tung", "und", "sein", "die", "Wehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "ART", "NN", "KON", "PPOSAT", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er kann uns den Sieg nur verleihen:", "tokens": ["Er", "kann", "uns", "den", "Sieg", "nur", "ver\u00b7lei\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Wer Gott sich vertrauet, der ist der Mann,", "tokens": ["Wer", "Gott", "sich", "ver\u00b7trau\u00b7et", ",", "der", "ist", "der", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PRF", "VVFIN", "$,", "PRELS", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Der fr\u00f6hlich sich schlagen und sterben kann.", "tokens": ["Der", "fr\u00f6h\u00b7lich", "sich", "schla\u00b7gen", "und", "ster\u00b7ben", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "VVINF", "KON", "VVINF", "VMFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}