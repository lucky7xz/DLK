{"textgrid.poem.67465": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die Birke \u00fcber dem Grabe", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fchlingsbirke, Du stehst hier \u00fcber dem Grabe der Schwester", "tokens": ["Fr\u00fch\u00b7lings\u00b7bir\u00b7ke", ",", "Du", "stehst", "hier", "\u00fc\u00b7ber", "dem", "Gra\u00b7be", "der", "Schwes\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Herbstlich einsam und streust Bl\u00e4tter und Thr\u00e4nen darauf.", "tokens": ["Herbst\u00b7lich", "ein\u00b7sam", "und", "streust", "Bl\u00e4t\u00b7ter", "und", "Thr\u00e4\u00b7nen", "da\u00b7rauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "NN", "KON", "NN", "PAV", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Deiner unschuldigen Brust will ich's vertrauen: Sie spro\u00dfte", "tokens": ["Dei\u00b7ner", "un\u00b7schul\u00b7di\u00b7gen", "Brust", "will", "ich's", "ver\u00b7trau\u00b7en", ":", "Sie", "spro\u00df\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "PIS", "VVINF", "$.", "PPER", "VVFIN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Ach, und vermochte nicht zu bestehn dem Sturme des Winters.", "tokens": ["Ach", ",", "und", "ver\u00b7moch\u00b7te", "nicht", "zu", "be\u00b7stehn", "dem", "Stur\u00b7me", "des", "Win\u00b7ters", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "VVFIN", "PTKNEG", "PTKZU", "VVINF", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.5": {"text": "S\u00e4usle, jungfr\u00e4ulicher Baum, s\u00e4usle der Schlafenden Ruh!", "tokens": ["S\u00e4us\u00b7le", ",", "jung\u00b7fr\u00e4u\u00b7li\u00b7cher", "Baum", ",", "s\u00e4us\u00b7le", "der", "Schla\u00b7fen\u00b7den", "Ruh", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}}}}}