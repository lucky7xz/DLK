{"textgrid.poem.61025": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So halt' ich endlich dich umfangen,", "tokens": ["So", "halt'", "ich", "end\u00b7lich", "dich", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In s\u00fc\u00dfes Schweigen starb das Wort,", "tokens": ["In", "s\u00fc\u00b7\u00dfes", "Schwei\u00b7gen", "starb", "das", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und meine trunknen Lippen hangen", "tokens": ["Und", "mei\u00b7ne", "trunk\u00b7nen", "Lip\u00b7pen", "han\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "An deinen Lippen fort und fort.", "tokens": ["An", "dei\u00b7nen", "Lip\u00b7pen", "fort", "und", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was nur das Gl\u00fcck vermag zu geben,", "tokens": ["Was", "nur", "das", "Gl\u00fcck", "ver\u00b7mag", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In sel'ger F\u00fclle ist es mein:", "tokens": ["In", "sel'\u00b7ger", "F\u00fcl\u00b7le", "ist", "es", "mein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich habe dich, geliebtes Leben,", "tokens": ["Ich", "ha\u00b7be", "dich", ",", "ge\u00b7lieb\u00b7tes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was braucht es mehr als dich allein?", "tokens": ["Was", "braucht", "es", "mehr", "als", "dich", "al\u00b7lein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "KOUS", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O, decke jetzt des Schicksals Wille", "tokens": ["O", ",", "de\u00b7cke", "jetzt", "des", "Schick\u00b7sals", "Wil\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Nacht die Welt und ihre Zier,", "tokens": ["Mit", "Nacht", "die", "Welt", "und", "ih\u00b7re", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nur dein Auge schwebe stille,", "tokens": ["Und", "nur", "dein", "Au\u00b7ge", "schwe\u00b7be", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein blauer Himmel, \u00fcber mir!", "tokens": ["Ein", "blau\u00b7er", "Him\u00b7mel", ",", "\u00fc\u00b7ber", "mir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}