{"textgrid.poem.58626": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "58.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wechselt alles ab, was ich in mir befinde,", "tokens": ["Es", "wech\u00b7selt", "al\u00b7les", "ab", ",", "was", "ich", "in", "mir", "be\u00b7fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PTKVZ", "$,", "PWS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bald ist mein Herz erfreut und bald in Leiden gar,", "tokens": ["Bald", "ist", "mein", "Herz", "er\u00b7freut", "und", "bald", "in", "Lei\u00b7den", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVFIN", "KON", "ADV", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es streiten Licht und Nacht, Gerechtigkeit und S\u00fcnde;", "tokens": ["Es", "strei\u00b7ten", "Licht", "und", "Nacht", ",", "Ge\u00b7rech\u00b7tig\u00b7keit", "und", "S\u00fcn\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wann werd' ich endlich stehn in Gott unwandelbar?", "tokens": ["Wann", "werd'", "ich", "end\u00b7lich", "stehn", "in", "Gott", "un\u00b7wan\u00b7del\u00b7bar", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "VVFIN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "W\u00e4r' Gott mein ew'ges Licht im Firmament der Seelen,", "tokens": ["W\u00e4r'", "Gott", "mein", "ew'\u00b7ges", "Licht", "im", "Fir\u00b7ma\u00b7ment", "der", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dies reine Wesen selbst mein Zierrat und mein Kleid,", "tokens": ["Dies", "rei\u00b7ne", "We\u00b7sen", "selbst", "mein", "Zier\u00b7rat", "und", "mein", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "ADV", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den Seelenkr\u00e4ften all' dann auch kein Licht w\u00fcrd' fehlen,", "tokens": ["Den", "See\u00b7len\u00b7kr\u00e4f\u00b7ten", "all'", "dann", "auch", "kein", "Licht", "w\u00fcrd'", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "ADV", "ADV", "PIAT", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So w\u00e4r' ich ganz gerecht und frei von allem Leid.", "tokens": ["So", "w\u00e4r'", "ich", "ganz", "ge\u00b7recht", "und", "frei", "von", "al\u00b7lem", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "APPR", "PIS", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}