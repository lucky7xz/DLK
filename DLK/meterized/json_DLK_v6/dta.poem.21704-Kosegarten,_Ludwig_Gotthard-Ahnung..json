{"dta.poem.21704": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Ahnung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.85", "id:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schau, das junge Jahr erlau't,", "tokens": ["Schau", ",", "das", "jun\u00b7ge", "Jahr", "er\u00b7lau't", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Str\u00f6me Grundeis thau't.", "tokens": ["Und", "der", "Str\u00f6\u00b7me", "Grun\u00b7deis", "thau'", "t."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "NE"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Dampfend stehn die Triften.", "tokens": ["Damp\u00b7fend", "stehn", "die", "Trif\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Horch, der Turteltauber girrt,", "tokens": ["Horch", ",", "der", "Tur\u00b7tel\u00b7tau\u00b7ber", "girrt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der Lerche Brautlied schwirrt", "tokens": ["Und", "der", "Ler\u00b7che", "Braut\u00b7lied", "schwirrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "In den mildern L\u00fcften.", "tokens": ["In", "den", "mil\u00b7dern", "L\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Bringt des Fr\u00fchlings Erstlinge,", "tokens": ["Bringt", "des", "Fr\u00fch\u00b7lings", "Erst\u00b7lin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Krokos mir und Glockenschnee,", "tokens": ["Kro\u00b7kos", "mir", "und", "Glo\u00b7cken\u00b7schnee", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dass mein Herz sich labe.", "tokens": ["Dass", "mein", "Herz", "sich", "la\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wenn das junge Jahr verbleicht,", "tokens": ["Wenn", "das", "jun\u00b7ge", "Jahr", "ver\u00b7bleicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Streift der Abendwind vielleicht", "tokens": ["Streift", "der", "Ab\u00b7end\u00b7wind", "viel\u00b7leicht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00dcber meinem Grabe.", "tokens": ["\u00dc\u00b7ber", "mei\u00b7nem", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Satt des langen Zwangs zerreisst", "tokens": ["Satt", "des", "lan\u00b7gen", "Zwangs", "zer\u00b7reisst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sein Gespinnst der ew'ge Geist \u2014", "tokens": ["Sein", "Ge\u00b7spinnst", "der", "ew'\u00b7ge", "Geist"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fleusst in lauen Wellen", "tokens": ["Fleusst", "in", "lau\u00b7en", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Nicht mein Leben purpurn fort?", "tokens": ["Nicht", "mein", "Le\u00b7ben", "pur\u00b7purn", "fort", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "NE", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00fchl' ich nicht zerschlitzt den Ort,", "tokens": ["F\u00fchl'", "ich", "nicht", "zer\u00b7schlitzt", "den", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Den die Seufzer schwellen?", "tokens": ["Den", "die", "Seuf\u00b7zer", "schwel\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Sonne, nie erlahmt dein Schwung,", "tokens": ["Son\u00b7ne", ",", "nie", "er\u00b7lahmt", "dein", "Schwung", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ewig frisch und ewig jung", "tokens": ["E\u00b7wig", "frisch", "und", "e\u00b7wig", "jung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gr\u00fcn'st du, sch\u00f6ne Erde.", "tokens": ["Gr\u00fcn'st", "du", ",", "sch\u00f6\u00b7ne", "Er\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "\u00dcber Land und Wasser schwebt", "tokens": ["\u00dc\u00b7ber", "Land", "und", "Was\u00b7ser", "schwebt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gottes Hauch. Das All belebt", "tokens": ["Got\u00b7tes", "Hauch", ".", "Das", "All", "be\u00b7lebt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$.", "ART", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sein erbarmend Werde.", "tokens": ["Sein", "er\u00b7bar\u00b7mend", "Wer\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "VVPP", "VAFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Wir nur, wir . . . gef\u00e4rbter Schaum . . .", "tokens": ["Wir", "nur", ",", "wir", ".", ".", ".", "ge\u00b7f\u00e4rb\u00b7ter", "Schaum", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "punct", "punct", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "ADV", "$,", "PPER", "$.", "$.", "$.", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Einer Lenznacht nicht'ger Traum . . .", "tokens": ["Ei\u00b7ner", "Lenz\u00b7nacht", "nicht'\u00b7ger", "Traum", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "$.", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kommen und verschwinden.", "tokens": ["Kom\u00b7men", "und", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Kaum vom schweren Schlummer wach,", "tokens": ["Kaum", "vom", "schwe\u00b7ren", "Schlum\u00b7mer", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schaun wir m\u00fchsam in den Tag,", "tokens": ["Schaun", "wir", "m\u00fch\u00b7sam", "in", "den", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Blinzeln und erblinden. \u2014 \u2014", "tokens": ["Blin\u00b7zeln", "und", "er\u00b7blin\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "KON", "VVINF", "$.", "$(", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Doch der dunkle Tropfe sinkt", "tokens": ["Doch", "der", "dunk\u00b7le", "Trop\u00b7fe", "sinkt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und des lautrern Aethers trinkt", "tokens": ["Und", "des", "laut\u00b7rern", "A\u00b7e\u00b7thers", "trinkt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der genes'ne Sieche.", "tokens": ["Der", "ge\u00b7nes'\u00b7ne", "Sie\u00b7che", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Los des d\u00fcstern Sarkophags,", "tokens": ["Los", "des", "d\u00fcs\u00b7tern", "Sar\u00b7ko\u00b7phags", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Freut sich des entflorten Tags", "tokens": ["Freut", "sich", "des", "ent\u00b7flor\u00b7ten", "Tags"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die erl\u00f6s'te Psyche. \u2014", "tokens": ["Die", "er\u00b7l\u00f6s'\u00b7te", "Psy\u00b7che", "."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Bringt des Jahres Erstlinge,", "tokens": ["Bringt", "des", "Jah\u00b7res", "Erst\u00b7lin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Krokos mir und Glockenschnee,", "tokens": ["Kro\u00b7kos", "mir", "und", "Glo\u00b7cken\u00b7schnee", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dass mein Herz sich labe!", "tokens": ["Dass", "mein", "Herz", "sich", "la\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Eh des Fr\u00fchlings Wang' verbleicht,", "tokens": ["Eh", "des", "Fr\u00fch\u00b7lings", "Wang'", "ver\u00b7bleicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Triumphirt erl\u00f6st vielleicht", "tokens": ["Tri\u00b7um\u00b7phirt", "er\u00b7l\u00f6st", "viel\u00b7leicht"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Psyche \u00fcberm Grabe!", "tokens": ["Psy\u00b7che", "\u00fc\u00b7berm", "Gra\u00b7be", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}