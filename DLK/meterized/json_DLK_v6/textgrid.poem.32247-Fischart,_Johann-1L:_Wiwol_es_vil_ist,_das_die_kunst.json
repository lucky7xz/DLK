{"textgrid.poem.32247": {"metadata": {"author": {"name": "Fischart, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wiwol es vil ist, das die kunst", "genre": "verse", "period": "N.A.", "pub_year": 1568, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wiwol es vil ist, das die kunst", "tokens": ["Wi\u00b7wol", "es", "vil", "ist", ",", "das", "die", "kunst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ir selber schaffet lib vnd gunst", "tokens": ["Ir", "sel\u00b7ber", "schaf\u00b7fet", "lib", "vnd", "gunst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "NE", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd anderen ergezlichait,", "tokens": ["Vnd", "an\u00b7de\u00b7ren", "er\u00b7gez\u00b7lic\u00b7hait", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch ist vil meh die nuzbarkait;", "tokens": ["Noch", "ist", "vil", "meh", "die", "nuz\u00b7bar\u00b7kait", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dan gm\u00e4l mag auch die thir ergezen", "tokens": ["Dan", "gm\u00e4l", "mag", "auch", "die", "thir", "er\u00b7ge\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VMFIN", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd sint drum nicht f\u00fcr meh zusch\u00e4tzen:", "tokens": ["Vnd", "sint", "drum", "nicht", "f\u00fcr", "meh", "zu\u00b7sch\u00e4t\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "PTKNEG", "APPR", "NE", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Aesopi wolf fr\u00e4ut auch ain bild,", "tokens": ["A\u00b7e\u00b7so\u00b7pi", "wolf", "fr\u00e4ut", "auch", "ain", "bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Plib doch ain Wild vnd wurd nie milt:", "tokens": ["Plib", "doch", "ain", "Wild", "vnd", "wurd", "nie", "milt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "NE", "NE", "KON", "VAFIN", "ADV", "VVPP", "$."], "meter": "+--+--++", "measure": "dactylic.di.plus"}, "line.9": {"text": "Ain kind sicht auch gern gmalte schilt,", "tokens": ["Ain", "kind", "sicht", "auch", "gern", "gmal\u00b7te", "schilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ADV", "ADV", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wiwol es nicht sein deitnus f\u00fclt:", "tokens": ["Wi\u00b7wol", "es", "nicht", "sein", "deit\u00b7nus", "f\u00fclt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VAINF", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "So sagt man, das gmainlich die g\u00f6tzen", "tokens": ["So", "sagt", "man", ",", "das", "gmain\u00b7lich", "die", "g\u00f6t\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PRELS", "ADJD", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.12": {"text": "Die g\u00f6tzen pflegen zuergetzen;", "tokens": ["Die", "g\u00f6t\u00b7zen", "pfle\u00b7gen", "zu\u00b7er\u00b7get\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Aber ain Weiser h\u00f6her sucht,", "tokens": ["A\u00b7ber", "ain", "Wei\u00b7ser", "h\u00f6\u00b7her", "sucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.14": {"text": "Acht nicht der schal, sonder der frucht,", "tokens": ["Acht", "nicht", "der", "schal", ",", "son\u00b7der", "der", "frucht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "PTKNEG", "ART", "ADJD", "$,", "KON", "ART", "NN", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.15": {"text": "Diweil er wais, das ehrlich k\u00fcnst", "tokens": ["Di\u00b7weil", "er", "wais", ",", "das", "ehr\u00b7lich", "k\u00fcnst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKVZ", "$,", "PRELS", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Sint gschaffen zu des menschen dinst.", "tokens": ["Sint", "gschaf\u00b7fen", "zu", "des", "men\u00b7schen", "dinst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.17": {"text": "Was solln aber f\u00fcr dinst dis haisen,", "tokens": ["Was", "solln", "a\u00b7ber", "f\u00fcr", "dinst", "dis", "hai\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "APPR", "NN", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Die nicht das gm\u00fct auch vnterweisen?", "tokens": ["Die", "nicht", "das", "gm\u00fct", "auch", "vn\u00b7ter\u00b7wei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "PDS", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Was solt ain weiser sich dran gn\u00fcgen,", "tokens": ["Was", "solt", "ain", "wei\u00b7ser", "sich", "dran", "gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "PRF", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Das Parrhasius kan betrigen", "tokens": ["Das", "Par\u00b7rha\u00b7si\u00b7us", "kan", "be\u00b7tri\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "VVINF"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.21": {"text": "Mit seinem sch\u00f6ngemalten trauben", "tokens": ["Mit", "sei\u00b7nem", "sch\u00f6n\u00b7ge\u00b7mal\u00b7ten", "trau\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Die ainfaltig gelustrig Dauben?", "tokens": ["Die", "ain\u00b7fal\u00b7tig", "ge\u00b7lust\u00b7rig", "Dau\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "NN", "$."], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Het er das kind, welchs den traub f\u00fchrt,", "tokens": ["Het", "er", "das", "kind", ",", "welchs", "den", "traub", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.24": {"text": "Recht gmalt, kain Daub hets nit ber\u00fcrt,", "tokens": ["Recht", "gmalt", ",", "kain", "Daub", "hets", "nit", "be\u00b7r\u00fcrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADJA", "NN", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Vnd wer er nicht vil thauber gwesen,", "tokens": ["Vnd", "wer", "er", "nicht", "vil", "thau\u00b7ber", "gwe\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PTKNEG", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Als alle dauben, die wir essen,", "tokens": ["Als", "al\u00b7le", "dau\u00b7ben", ",", "die", "wir", "es\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVINF", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Het er zerstosen nicht die hand,", "tokens": ["Het", "er", "zer\u00b7sto\u00b7sen", "nicht", "die", "hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.28": {"text": "Da er wolt zihen von der wand", "tokens": ["Da", "er", "wolt", "zi\u00b7hen", "von", "der", "wand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "VVINF", "APPR", "ART", "NN"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.29": {"text": "Den vmhang, auf das er beseh,", "tokens": ["Den", "vm\u00b7hang", ",", "auf", "das", "er", "be\u00b7seh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Was dahinden gemalet steh", "tokens": ["Was", "da\u00b7hin\u00b7den", "ge\u00b7ma\u00b7let", "steh"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PAV", "VVPP", "VVFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.31": {"text": "Was ists, das der fremd maler Dosse", "tokens": ["Was", "ists", ",", "das", "der", "fremd", "ma\u00b7ler", "Dos\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Malt etlich b\u00f6r so sch\u00f6n zum bossen,", "tokens": ["Malt", "et\u00b7lich", "b\u00f6r", "so", "sch\u00f6n", "zum", "bos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "ADV", "ADJD", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Das sie die Pfauen so zerbissen,", "tokens": ["Das", "sie", "die", "Pfau\u00b7en", "so", "zer\u00b7bis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Bis gar der Kalk ist abgerissen?", "tokens": ["Bis", "gar", "der", "Kalk", "ist", "ab\u00b7ge\u00b7ris\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Oder das ain baum ainer malt", "tokens": ["O\u00b7der", "das", "ain", "baum", "ai\u00b7ner", "malt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "In ain Kirch, so artlich gestalt,", "tokens": ["In", "ain", "Kirch", ",", "so", "art\u00b7lich", "ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.37": {"text": "Das vil V\u00f6gel, gar grob betrogen,", "tokens": ["Das", "vil", "V\u00f6\u00b7gel", ",", "gar", "grob", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.38": {"text": "Drauf zusitzen, sind zugeflogen?", "tokens": ["Drauf", "zu\u00b7sit\u00b7zen", ",", "sind", "zu\u00b7ge\u00b7flo\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVINF", "$,", "VAFIN", "VVPP", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.39": {"text": "Vnd das ainer so wol malt zigel", "tokens": ["Vnd", "das", "ai\u00b7ner", "so", "wol", "malt", "zi\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ADV", "ADV", "VVFIN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.40": {"text": "Auf thuch vnd gzelten, das manch gfl\u00fcgel", "tokens": ["Auf", "thuch", "vnd", "gzel\u00b7ten", ",", "das", "manch", "gfl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "VVFIN", "$,", "PRELS", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Zuflog vnd sich darauf wolt setzen,", "tokens": ["Zu\u00b7flog", "vnd", "sich", "da\u00b7rauf", "wolt", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PRF", "PAV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Seinen schnabel daran zuwetzen?", "tokens": ["Sei\u00b7nen", "schna\u00b7bel", "da\u00b7ran", "zu\u00b7wet\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "PAV", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.43": {"text": "Desgleichen, das ab gmalter schlang", "tokens": ["Des\u00b7glei\u00b7chen", ",", "das", "ab", "gmal\u00b7ter", "schlang"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.44": {"text": "Vil V\u00f6gel vergasen jr gsang,", "tokens": ["Vil", "V\u00f6\u00b7gel", "ver\u00b7ga\u00b7sen", "jr", "gsang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.45": {"text": "Vnd ain trostel also erschrak,", "tokens": ["Vnd", "ain", "tros\u00b7tel", "al\u00b7so", "er\u00b7schrak", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.46": {"text": "Das jr die Pfeif ful gar in sack?", "tokens": ["Das", "jr", "die", "Pfeif", "ful", "gar", "in", "sack", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "NE", "ADV", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Solch ding sint, wie man spricht, nur kizlig;", "tokens": ["Solch", "ding", "sint", ",", "wie", "man", "spricht", ",", "nur", "kiz\u00b7lig", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "PWAV", "PIS", "VVFIN", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Aber zur besserung nicht vil n\u00fczlich,", "tokens": ["A\u00b7ber", "zur", "bes\u00b7se\u00b7rung", "nicht", "vil", "n\u00fcz\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.49": {"text": "Vnd welchen solch schlecht ding erfreuen,", "tokens": ["Vnd", "wel\u00b7chen", "solch", "schlecht", "ding", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "PIAT", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.50": {"text": "M\u00f6cht lachen auch der Vogelscheuen", "tokens": ["M\u00f6cht", "la\u00b7chen", "auch", "der", "Vo\u00b7gel\u00b7scheu\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Auch auf der Vogelherd der fl\u00fccken,", "tokens": ["Auch", "auf", "der", "Vo\u00b7gel\u00b7herd", "der", "fl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Vnd wan V\u00f6gel in dfenster picken;", "tokens": ["Vnd", "wan", "V\u00f6\u00b7gel", "in", "dfens\u00b7ter", "pi\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "APPR", "ADJA", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.53": {"text": "Auch das der Esel scheucht sein schetten,", "tokens": ["Auch", "das", "der", "E\u00b7sel", "scheucht", "sein", "schet\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ART", "NN", "VVFIN", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "Vnd M\u00e4idlin gern vorm spigel betten