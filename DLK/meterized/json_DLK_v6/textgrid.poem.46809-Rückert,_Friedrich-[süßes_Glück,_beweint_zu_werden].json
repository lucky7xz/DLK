{"textgrid.poem.46809": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[s\u00fc\u00dfes Gl\u00fcck, beweint zu werden]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00dfes Gl\u00fcck, beweint zu werden,", "tokens": ["S\u00fc\u00b7\u00dfes", "Gl\u00fcck", ",", "be\u00b7weint", "zu", "wer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und zu weinen, s\u00fc\u00dfes Gl\u00fcck!", "tokens": ["Und", "zu", "wei\u00b7nen", ",", "s\u00fc\u00b7\u00dfes", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieses nahmt ihr mit zur Erden,", "tokens": ["Die\u00b7ses", "nahmt", "ihr", "mit", "zur", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieses la\u00dft ihr uns zur\u00fcck.", "tokens": ["Die\u00b7ses", "la\u00dft", "ihr", "uns", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer will uns dies Gl\u00fcck entwenden,", "tokens": ["Wer", "will", "uns", "dies", "Gl\u00fcck", "ent\u00b7wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PDS", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Diese Thr\u00e4nen euch zu spenden,", "tokens": ["Die\u00b7se", "Thr\u00e4\u00b7nen", "euch", "zu", "spen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Euch den Seufzer nachzusenden:", "tokens": ["Euch", "den", "Seuf\u00b7zer", "nach\u00b7zu\u00b7sen\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "S\u00fc\u00dfes Gl\u00fcck beweint zu werden,", "tokens": ["S\u00fc\u00b7\u00dfes", "Gl\u00fcck", "be\u00b7weint", "zu", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und zu weinen, s\u00fc\u00dfes Gl\u00fcck!", "tokens": ["Und", "zu", "wei\u00b7nen", ",", "s\u00fc\u00b7\u00dfes", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}