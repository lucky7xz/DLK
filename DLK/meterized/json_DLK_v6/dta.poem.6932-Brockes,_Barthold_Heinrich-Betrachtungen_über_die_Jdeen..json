{"dta.poem.6932": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber die Jdeen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der SCHOEPFFER l\u00e4sst Sein Werck an allen Orten sehn.", "tokens": ["Der", "ScHOE\u00b7PF\u00b7FER", "l\u00e4sst", "Sein", "Werck", "an", "al\u00b7len", "Or\u00b7ten", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es m\u00f6gen Vorw\u00fcrff\u2019 auch noch so verschiedlich seyn;", "tokens": ["Es", "m\u00f6\u00b7gen", "Vor\u00b7w\u00fcrff'", "auch", "noch", "so", "ver\u00b7schied\u00b7lich", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "ADV", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist es der K\u00fcnstler nicht allein,", "tokens": ["Ist", "es", "der", "K\u00fcnst\u00b7ler", "nicht", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der desfalls billig zu erh\u00f6hn?", "tokens": ["Der", "des\u00b7falls", "bil\u00b7lig", "zu", "er\u00b7h\u00f6hn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn ein Mechanieus, ein Steuermann,", "tokens": ["Wenn", "ein", "Me\u00b7cha\u00b7ni\u00b7eus", ",", "ein", "Steu\u00b7er\u00b7mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein Organist, und einer, dessen Hand", "tokens": ["Ein", "Or\u00b7ga\u00b7nist", ",", "und", "ei\u00b7ner", ",", "des\u00b7sen", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "PIS", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein k\u00fcnstlich Uhrwerck machen kan,", "tokens": ["Ein", "k\u00fcnst\u00b7lich", "Uhr\u00b7werck", "ma\u00b7chen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Durch Rad- und Feder-Werck, die ihr Verstand", "tokens": ["Durch", "Ra\u00b7d", "und", "Fe\u00b7der\u00b7\u00b7Werck", ",", "die", "ihr", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "TRUNC", "KON", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Zum n\u00fctzlichem Gebrauch erfand,", "tokens": ["Zum", "n\u00fctz\u00b7li\u00b7chem", "Ge\u00b7brauch", "er\u00b7fand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Metall, Lufft, Fluth und Winde zwingt;", "tokens": ["Me\u00b7tall", ",", "Lufft", ",", "Fluth", "und", "Win\u00b7de", "zwingt", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Mu\u00df man dieselbigen nicht loben,", "tokens": ["Mu\u00df", "man", "die\u00b7sel\u00b7bi\u00b7gen", "nicht", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PDS", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "Und wird nicht ihre Kunst mit Recht erhoben,", "tokens": ["Und", "wird", "nicht", "ih\u00b7re", "Kunst", "mit", "Recht", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Die die Materie in solche Ordnung bringt?", "tokens": ["Die", "die", "Ma\u00b7te\u00b7rie", "in", "sol\u00b7che", "Ord\u00b7nung", "bringt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.14": {"text": "In dem unwandelbar- und stetem Gang der Welt,", "tokens": ["In", "dem", "un\u00b7wan\u00b7del\u00b7ba\u00b7r", "und", "ste\u00b7tem", "Gang", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "TRUNC", "KON", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+---+-+-+-+", "measure": "unknown.measure.hexa"}, "line.15": {"text": "Wird GOTTES Macht und Kunst in allem vorgestellt.", "tokens": ["Wird", "GoT\u00b7TES", "Macht", "und", "Kunst", "in", "al\u00b7lem", "vor\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "NN", "KON", "NN", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da er die Massa, die so gro\u00df und allgemein,", "tokens": ["Da", "er", "die", "Mas\u00b7sa", ",", "die", "so", "gro\u00df", "und", "all\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "So wunderbar regieret;", "tokens": ["So", "wun\u00b7der\u00b7bar", "re\u00b7gie\u00b7ret", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Kan anders sonst kein Absehn seyn,", "tokens": ["Kan", "an\u00b7ders", "sonst", "kein", "Ab\u00b7sehn", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "PIAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Als da\u00df sein\u2019 Allmacht, Gr\u00f6ss\u2019 und Weisheit sey versp\u00fcret.", "tokens": ["Als", "da\u00df", "sein'", "All\u00b7macht", ",", "Gr\u00f6ss'", "und", "Weis\u00b7heit", "sey", "ver\u00b7sp\u00fc\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Er der das Triebwerck macht, versteht es zu verwalten,", "tokens": ["Er", "der", "das", "Trieb\u00b7werck", "macht", ",", "ver\u00b7steht", "es", "zu", "ver\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ein stetes Schaffen ist, dasselbe stets erhalten.", "tokens": ["Ein", "ste\u00b7tes", "Schaf\u00b7fen", "ist", ",", "das\u00b7sel\u00b7be", "stets", "er\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "PDAT", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}