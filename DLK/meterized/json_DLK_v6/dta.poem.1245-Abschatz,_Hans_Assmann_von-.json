{"dta.poem.1245": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Weicht von mir Freude/ Schertz und Lust/", "tokens": ["Weicht", "von", "mir", "Freu\u00b7de", "/", "Schertz", "und", "Lust", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Denn Celimen ist weggegangen.", "tokens": ["Denn", "Ce\u00b7li\u00b7men", "ist", "weg\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Furcht/ Zweifel/ Trauren und Verlangen", "tokens": ["Furcht", "/", "Zwei\u00b7fel", "/", "Trau\u00b7ren", "und", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00e4lt eure Stell in meiner Brust.", "tokens": ["H\u00e4lt", "eu\u00b7re", "Stell", "in", "mei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Geht hin/ zieht mit ihr auff und nieder/", "tokens": ["Geht", "hin", "/", "zieht", "mit", "ihr", "auff", "und", "nie\u00b7der", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$(", "VVFIN", "APPR", "PPOSAT", "APPR", "KON", "PTKVZ", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und kommet ohne sie nicht wieder.", "tokens": ["Und", "kom\u00b7met", "oh\u00b7ne", "sie", "nicht", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}