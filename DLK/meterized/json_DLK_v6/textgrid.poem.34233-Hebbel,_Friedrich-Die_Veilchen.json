{"textgrid.poem.34233": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Veilchen", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Veilchen hab' ich gepfl\u00fcckt, nun will ich zum Strau\u00dfe sie reihen,", "tokens": ["Veil\u00b7chen", "hab'", "ich", "ge\u00b7pfl\u00fcckt", ",", "nun", "will", "ich", "zum", "Strau\u00b7\u00dfe", "sie", "rei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "VVPP", "$,", "ADV", "VMFIN", "PPER", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Da entfallen sie mir, und es zerstreut sie der Wind.", "tokens": ["Da", "ent\u00b7fal\u00b7len", "sie", "mir", ",", "und", "es", "zer\u00b7streut", "sie", "der", "Wind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "KON", "PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Leichter pfl\u00fccke ich neue und frische, als da\u00df ich sie sammle,", "tokens": ["Leich\u00b7ter", "pfl\u00fc\u00b7cke", "ich", "neu\u00b7e", "und", "fri\u00b7sche", ",", "als", "da\u00df", "ich", "sie", "samm\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJA", "KON", "ADJA", "$,", "KOKOM", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+--+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Denn die Wiese ist reich, aber ich sammle sie doch.", "tokens": ["Denn", "die", "Wie\u00b7se", "ist", "reich", ",", "a\u00b7ber", "ich", "samm\u00b7le", "sie", "doch", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "$,", "KON", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}}}}