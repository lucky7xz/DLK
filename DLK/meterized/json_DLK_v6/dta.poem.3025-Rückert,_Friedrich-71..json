{"dta.poem.3025": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "71.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "An Kindern hab' ich oft bewundert, wie in Bildern", "tokens": ["An", "Kin\u00b7dern", "hab'", "ich", "oft", "be\u00b7wun\u00b7dert", ",", "wie", "in", "Bil\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,", "PWAV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie gleich den Gegenstand erkennen, den sie schildern.", "tokens": ["Sie", "gleich", "den", "Ge\u00b7gen\u00b7stand", "er\u00b7ken\u00b7nen", ",", "den", "sie", "schil\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVINF", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein nur gemaltes Pferd, ja gar ein nur in Strichen", "tokens": ["Ein", "nur", "ge\u00b7mal\u00b7tes", "Pferd", ",", "ja", "gar", "ein", "nur", "in", "Stri\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN", "$,", "ADV", "ADV", "ART", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gezeichnetes, worin hats einem Pferd geglichen?", "tokens": ["Ge\u00b7zeich\u00b7ne\u00b7tes", ",", "wo\u00b7rin", "hats", "ei\u00b7nem", "Pferd", "ge\u00b7gli\u00b7chen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So Gr\u00f6\u00df' als Umfang fehlt, so Leben als Bewegung;", "tokens": ["So", "Gr\u00f6\u00df'", "als", "Um\u00b7fang", "fehlt", ",", "so", "Le\u00b7ben", "als", "Be\u00b7we\u00b7gung", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOUS", "NN", "VVFIN", "$,", "ADV", "NN", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ist im Bilde denn zu des Begriffs Anregung?", "tokens": ["Was", "ist", "im", "Bil\u00b7de", "denn", "zu", "des", "Be\u00b7griffs", "An\u00b7re\u00b7gung", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPRART", "NN", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Geist mu\u00df innerlich voll seyn von solchen Bildern,", "tokens": ["Der", "Geist", "mu\u00df", "in\u00b7ner\u00b7lich", "voll", "seyn", "von", "sol\u00b7chen", "Bil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADJD", "ADJD", "VAINF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die dann nach ihrer Kunst die K\u00fcnstler au\u00dfen schildern.", "tokens": ["Die", "dann", "nach", "ih\u00b7rer", "Kunst", "die", "K\u00fcnst\u00b7ler", "au\u00b7\u00dfen", "schil\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und solche Bilder sind dem Kind schon eingeboren,", "tokens": ["Und", "sol\u00b7che", "Bil\u00b7der", "sind", "dem", "Kind", "schon", "ein\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie werdem ihm nicht erst durch Bildung anerkoren.", "tokens": ["Sie", "wer\u00b7dem", "ihm", "nicht", "erst", "durch", "Bil\u00b7dung", "an\u00b7er\u00b7ko\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ganz sinnlich scheint das Kind, und ist schon geistig ganz,", "tokens": ["Ganz", "sinn\u00b7lich", "scheint", "das", "Kind", ",", "und", "ist", "schon", "geis\u00b7tig", "ganz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "$,", "KON", "VAFIN", "ADV", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die Entwicklung streift nur H\u00fcllen ab vom Glanz.", "tokens": ["Und", "die", "Ent\u00b7wick\u00b7lung", "streift", "nur", "H\u00fcl\u00b7len", "ab", "vom", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "NN", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}