{"textgrid.poem.43341": {"metadata": {"author": {"name": "Leopardi, Giacomo", "birth": "N.A.", "death": "N.A."}, "title": "ViII. Hymnus an die Patriarchen", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von euch, der Menschheit hocherlauchten V\u00e4tern,", "tokens": ["Von", "euch", ",", "der", "Menschheit", "ho\u00b7cher\u00b7lauch\u00b7ten", "V\u00e4\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Soll der Gesang der schmerzgeweihten S\u00f6hne", "tokens": ["Soll", "der", "Ge\u00b7sang", "der", "schmerz\u00b7ge\u00b7weih\u00b7ten", "S\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Mit Preis ert\u00f6nen, die ihr so viel theurer", "tokens": ["Mit", "Preis", "er\u00b7t\u00f6\u00b7nen", ",", "die", "ihr", "so", "viel", "theu\u00b7rer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVINF", "$,", "PRELS", "PPER", "ADV", "ADV", "ADJD"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Dem Lenker der Gestirne war't und minder", "tokens": ["Dem", "Len\u00b7ker", "der", "Ge\u00b7stir\u00b7ne", "wa\u00b7r't", "und", "min\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "KON", "ADV"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Beweinenswerth, als wir, zum hehren Licht", "tokens": ["Be\u00b7wei\u00b7nens\u00b7werth", ",", "als", "wir", ",", "zum", "heh\u00b7ren", "Licht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Emporgeblickt. Unheilbar Ungl\u00fcck, das", "tokens": ["Em\u00b7por\u00b7ge\u00b7blickt", ".", "Un\u00b7heil\u00b7bar", "Un\u00b7gl\u00fcck", ",", "das"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["VVPP", "$.", "ADJD", "NN", "$,", "PRELS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die armen Menschen traf: geboren werden", "tokens": ["Die", "ar\u00b7men", "Men\u00b7schen", "traf", ":", "ge\u00b7bo\u00b7ren", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "VVPP", "VAINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zum Weinen und das Licht des \u00c4thers freudig", "tokens": ["Zum", "Wei\u00b7nen", "und", "das", "Licht", "des", "\u00c4\u00b7thers", "freu\u00b7dig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "ART", "NN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Vertauschen mit der ew'gen Grabesnacht, \u2013", "tokens": ["Ver\u00b7tau\u00b7schen", "mit", "der", "ew'\u00b7gen", "Gra\u00b7bes\u00b7nacht", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Nicht hat's die milde und gerechte Satzung", "tokens": ["Nicht", "hat's", "die", "mil\u00b7de", "und", "ge\u00b7rech\u00b7te", "Sat\u00b7zung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VAFIN", "ART", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Des Himmels euch verh\u00e4ngt. Wenn von der S\u00fcnde,", "tokens": ["Des", "Him\u00b7mels", "euch", "ver\u00b7h\u00e4ngt", ".", "Wenn", "von", "der", "S\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$.", "KOUS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Die alle Menschenkinder der Gewalt", "tokens": ["Die", "al\u00b7le", "Men\u00b7schen\u00b7kin\u00b7der", "der", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Der Seuchen und des Elends \u00fcberliefert,", "tokens": ["Der", "Seu\u00b7chen", "und", "des", "E\u00b7lends", "\u00fc\u00b7berl\u00b7ie\u00b7fert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.14": {"text": "Die alte Sage spricht: noch \u00e4rgre S\u00fcnden", "tokens": ["Die", "al\u00b7te", "Sa\u00b7ge", "spricht", ":", "noch", "\u00e4r\u00b7gre", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Der Sterblichen, ihr ruheloser Geist", "tokens": ["Der", "Sterb\u00b7li\u00b7chen", ",", "ihr", "ru\u00b7he\u00b7lo\u00b7ser", "Geist"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und schlimmrer Wahnsinn waffneten wider sie", "tokens": ["Und", "schlimm\u00b7rer", "Wahn\u00b7sinn", "waff\u00b7ne\u00b7ten", "wi\u00b7der", "sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Die Rache des Olympos und die Hand", "tokens": ["Die", "Ra\u00b7che", "des", "O\u00b7lym\u00b7pos", "und", "die", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Der lang vergessnen N\u00e4hrerin Natur.", "tokens": ["Der", "lang", "ver\u00b7gess\u00b7nen", "N\u00e4h\u00b7re\u00b7rin", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Da ward verleidet uns die Lebensflamme,", "tokens": ["Da", "ward", "ver\u00b7lei\u00b7det", "uns", "die", "Le\u00b7bens\u00b7flam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Verabscheut jede Neugeburt, und w\u00fcthend", "tokens": ["Ver\u00b7ab\u00b7scheut", "je\u00b7de", "Neu\u00b7ge\u00b7burt", ",", "und", "w\u00fct\u00b7hend"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "$,", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Brach in die Welt herein der Erebus.", "tokens": ["Brach", "in", "die", "Welt", "her\u00b7ein", "der", "E\u00b7re\u00b7bus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du sahst zuerst den Tag, die Purpurfackeln", "tokens": ["Du", "sahst", "zu\u00b7erst", "den", "Tag", ",", "die", "Pur\u00b7pur\u00b7fa\u00b7ckeln"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der kreisenden Gestirne und die jungen", "tokens": ["Der", "krei\u00b7sen\u00b7den", "Ge\u00b7stir\u00b7ne", "und", "die", "jun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Thiere des Feldes weiden, alter F\u00fchrer", "tokens": ["Thie\u00b7re", "des", "Fel\u00b7des", "wei\u00b7den", ",", "al\u00b7ter", "F\u00fch\u00b7rer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "VVINF", "$,", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und Vater du der menschlichen Familie,", "tokens": ["Und", "Va\u00b7ter", "du", "der", "menschli\u00b7chen", "Fa\u00b7mi\u00b7lie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sahst auf den frischen Au'n die L\u00fcfte spielen,", "tokens": ["Sahst", "auf", "den", "fri\u00b7schen", "Au'n", "die", "L\u00fcf\u00b7te", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und wie herniederst\u00fcrzend Alpenflut", "tokens": ["Und", "wie", "her\u00b7nie\u00b7ders\u00b7t\u00fcr\u00b7zend", "Al\u00b7pen\u00b7flut"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "VVPP", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "An Felsenw\u00e4nd' und \u00f6de Th\u00e4ler schlug", "tokens": ["An", "Fel\u00b7sen\u00b7w\u00e4nd'", "und", "\u00f6\u00b7de", "Th\u00e4\u00b7ler", "schlug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mit unerh\u00f6rtem Schall, wie auf den heitern", "tokens": ["Mit", "un\u00b7er\u00b7h\u00f6r\u00b7tem", "Schall", ",", "wie", "auf", "den", "hei\u00b7tern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Zuk\u00fcnft'gen St\u00e4tten hochber\u00fchmter V\u00f6lker", "tokens": ["Zu\u00b7k\u00fcnft'\u00b7gen", "St\u00e4t\u00b7ten", "hoch\u00b7be\u00b7r\u00fchm\u00b7ter", "V\u00f6l\u00b7ker"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und l\u00e4rmerf\u00fcllter St\u00e4dte noch ein tief", "tokens": ["Und", "l\u00e4r\u00b7mer\u00b7f\u00fcll\u00b7ter", "St\u00e4d\u00b7te", "noch", "ein", "tief"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ADV", "ART", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Verborgner Frieden herrscht' und stumm und einsam", "tokens": ["Ver\u00b7borg\u00b7ner", "Frie\u00b7den", "herrscht'", "und", "stumm", "und", "ein\u00b7sam"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "KON", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Der Strahl der Sonne und des goldnen Monds", "tokens": ["Der", "Strahl", "der", "Son\u00b7ne", "und", "des", "gold\u00b7nen", "Monds"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Erklomm die ungepfl\u00fcgten H\u00f6h'n. O sel'ge,", "tokens": ["Er\u00b7klomm", "die", "un\u00b7ge\u00b7pfl\u00fcg\u00b7ten", "H\u00f6h'", "n.", "O", "sel'\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "abbreviation", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "NE", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Von Schuld und finstrem Schicksal unber\u00fchrte", "tokens": ["Von", "Schuld", "und", "fins\u00b7trem", "Schick\u00b7sal", "un\u00b7be\u00b7r\u00fchr\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Welteinsamkeit! O wie viel bittres Leid,", "tokens": ["Welt\u00b7ein\u00b7sam\u00b7keit", "!", "O", "wie", "viel", "bitt\u00b7res", "Leid", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "KOKOM", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Welch ungeheure Kette von Geschicken", "tokens": ["Welch", "un\u00b7ge\u00b7heu\u00b7re", "Ket\u00b7te", "von", "Ge\u00b7schi\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Bereiten, armer Vater, deinen Kindern", "tokens": ["Be\u00b7rei\u00b7ten", ",", "ar\u00b7mer", "Va\u00b7ter", ",", "dei\u00b7nen", "Kin\u00b7dern"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Die ew'gen M\u00e4chte! Siehe, Blut besudelt", "tokens": ["Die", "ew'\u00b7gen", "M\u00e4ch\u00b7te", "!", "Sie\u00b7he", ",", "Blut", "be\u00b7su\u00b7delt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "VVIMP", "$,", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Und Brudergr\u00e4uel nun zum ersten Mal", "tokens": ["Und", "Bru\u00b7der\u00b7gr\u00e4u\u00b7el", "nun", "zum", "ers\u00b7ten", "Mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Die kargen Fluren, und die L\u00fcfte h\u00f6ren", "tokens": ["Die", "kar\u00b7gen", "Flu\u00b7ren", ",", "und", "die", "L\u00fcf\u00b7te", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Zuerst der Todesfittiche schaurig Schwirren.", "tokens": ["Zu\u00b7erst", "der", "To\u00b7des\u00b7fit\u00b7ti\u00b7che", "schau\u00b7rig", "Schwir\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Der Bruderm\u00f6rder, bebend, heimathlos,", "tokens": ["Der", "Bru\u00b7der\u00b7m\u00f6r\u00b7der", ",", "be\u00b7bend", ",", "hei\u00b7math\u00b7los", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Einsame Schatten meidend und der Winde", "tokens": ["Ein\u00b7sa\u00b7me", "Schat\u00b7ten", "mei\u00b7dend", "und", "der", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVPP", "KON", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.24": {"text": "Geheimes Grollen durch die tiefen W\u00e4lder,", "tokens": ["Ge\u00b7hei\u00b7mes", "Grol\u00b7len", "durch", "die", "tie\u00b7fen", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Erbaut zuerst Stadth\u00e4user, bleicher Sorgen", "tokens": ["Er\u00b7baut", "zu\u00b7erst", "Stadt\u00b7h\u00e4u\u00b7ser", ",", "blei\u00b7cher", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Wohnsitz und Herrschgebiet; zuerst vereinigt", "tokens": ["Wohn\u00b7sitz", "und", "Herrschge\u00b7biet", ";", "zu\u00b7erst", "ver\u00b7ei\u00b7nigt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NE", "$.", "ADV", "VVPP"], "meter": "-+-++-+-+-", "measure": "unknown.measure.penta"}, "line.27": {"text": "Verzweiflungsvolle Reue, krank und st\u00f6hnend,", "tokens": ["Ver\u00b7zwei\u00b7flungs\u00b7vol\u00b7le", "Reu\u00b7e", ",", "krank", "und", "st\u00f6h\u00b7nend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Die blinden Sterblichen und bietet ihnen", "tokens": ["Die", "blin\u00b7den", "Sterb\u00b7li\u00b7chen", "und", "bie\u00b7tet", "ih\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Gesellige Zufluchtsst\u00e4tten; nun verschm\u00e4ht", "tokens": ["Ge\u00b7sel\u00b7li\u00b7ge", "Zu\u00b7fluchts\u00b7st\u00e4t\u00b7ten", ";", "nun", "ver\u00b7schm\u00e4ht"], "tok