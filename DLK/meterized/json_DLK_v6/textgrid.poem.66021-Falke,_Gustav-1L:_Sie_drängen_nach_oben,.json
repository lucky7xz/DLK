{"textgrid.poem.66021": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie dr\u00e4ngen nach oben,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie dr\u00e4ngen nach oben,", "tokens": ["Sie", "dr\u00e4n\u00b7gen", "nach", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die lange geduckt,", "tokens": ["Die", "lan\u00b7ge", "ge\u00b7duckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Das Haupt erhoben", "tokens": ["Das", "Haupt", "er\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wird aufgemuckt;", "tokens": ["Wird", "auf\u00b7ge\u00b7muckt", ";"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wollen auch was haben", "tokens": ["Wol\u00b7len", "auch", "was", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PWS", "VAFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Von der Welt Gaben.", "tokens": ["Von", "der", "Welt", "Ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Habt lange genug allein gezecht,", "tokens": ["Habt", "lan\u00b7ge", "ge\u00b7nug", "al\u00b7lein", "ge\u00b7zecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Den Wein verteilt mehr schlecht als recht.", "tokens": ["Den", "Wein", "ver\u00b7teilt", "mehr", "schlecht", "als", "recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Z\u00f6gernd erst, doch m\u00e4hlich frecher", "tokens": ["Z\u00f6\u00b7gernd", "erst", ",", "doch", "m\u00e4h\u00b7lich", "fre\u00b7cher"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "ADV", "$,", "ADV", "ADJD", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Tappen sie nach eurem Becher,", "tokens": ["Tap\u00b7pen", "sie", "nach", "eu\u00b7rem", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Mit groben F\u00e4usten und wenig eben.", "tokens": ["Mit", "gro\u00b7ben", "F\u00e4us\u00b7ten", "und", "we\u00b7nig", "e\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADV", "ADV", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "H\u00e4ttet willig ihr gegeben,", "tokens": ["H\u00e4t\u00b7tet", "wil\u00b7lig", "ihr", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Das Tischtuch w\u00e4re geblieben rein.", "tokens": ["Das", "Tischtuch", "w\u00e4\u00b7re", "ge\u00b7blie\u00b7ben", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Nun wird versch\u00fcttet viel edler Wein,", "tokens": ["Nun", "wird", "ver\u00b7sch\u00fct\u00b7tet", "viel", "ed\u00b7ler", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Vieles verderbt,", "tokens": ["Vie\u00b7les", "ver\u00b7derbt", ","], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.16": {"text": "Wie Blut gef\u00e4rbt.", "tokens": ["Wie", "Blut", "ge\u00b7f\u00e4rbt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}