{"textgrid.poem.58689": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1. Gaben f\u00fcr alle", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So fass' ich dennoch Mut, weil Jesus triumphiert.", "tokens": ["So", "fass'", "ich", "den\u00b7noch", "Mut", ",", "weil", "Je\u00b7sus", "tri\u00b7um\u00b7phiert", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN", "$,", "KOUS", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Siegesgaben, die er ohne Ma\u00df bekommen,", "tokens": ["Die", "Sie\u00b7ges\u00b7ga\u00b7ben", ",", "die", "er", "oh\u00b7ne", "Ma\u00df", "be\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sind f\u00fcr uns Menschen doch und nicht nur f\u00fcr die Frommen;", "tokens": ["Sind", "f\u00fcr", "uns", "Men\u00b7schen", "doch", "und", "nicht", "nur", "f\u00fcr", "die", "From\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "NN", "ADV", "KON", "PTKNEG", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er hat was mit f\u00fcr mich, ob ich gleich rebelliert.", "tokens": ["Er", "hat", "was", "mit", "f\u00fcr", "mich", ",", "ob", "ich", "gleich", "re\u00b7bel\u00b7liert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "APPR", "APPR", "PPER", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}