{"textgrid.poem.61405": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Wie K\u00f6nig Sigurd Hochzeit hielt", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bei der Sigurdsflotte nicht weit vom Feld der Schlacht", "tokens": ["Bei", "der", "Si\u00b7gurds\u00b7flot\u00b7te", "nicht", "weit", "vom", "Feld", "der", "Schlacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKNEG", "ADJD", "APPRART", "NN", "ART", "NN"], "meter": "+---+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Lag ein Schiff ger\u00fcstet mit wundersamer Pracht,", "tokens": ["Lag", "ein", "Schiff", "ge\u00b7r\u00fcs\u00b7tet", "mit", "wun\u00b7der\u00b7sa\u00b7mer", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Die Masten und die Stangen gebaut aus edlem Holz,", "tokens": ["Die", "Mas\u00b7ten", "und", "die", "Stan\u00b7gen", "ge\u00b7baut", "aus", "ed\u00b7lem", "Holz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Dran sah man Wimpel prangen und Flaggen reich und stolz.", "tokens": ["Dran", "sah", "man", "Wim\u00b7pel", "pran\u00b7gen", "und", "Flag\u00b7gen", "reich", "und", "stolz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "NN", "VVFIN", "KON", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Von schneewei\u00dfem Linnen das Segel war zur Fahrt,", "tokens": ["Von", "schnee\u00b7wei\u00b7\u00dfem", "Lin\u00b7nen", "das", "Se\u00b7gel", "war", "zur", "Fahrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Man hatte an den Tauen der Seide nicht gespart,", "tokens": ["Man", "hat\u00b7te", "an", "den", "Tau\u00b7en", "der", "Sei\u00b7de", "nicht", "ge\u00b7spart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Silbern schien der Anker, es war des Steuers Griff", "tokens": ["Sil\u00b7bern", "schien", "der", "An\u00b7ker", ",", "es", "war", "des", "Steu\u00b7ers", "Griff"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "NN"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Aus blankem Erz getrieben. Das war das Hochzeitsschiff.", "tokens": ["Aus", "blan\u00b7kem", "Erz", "ge\u00b7trie\u00b7ben", ".", "Das", "war", "das", "Hoch\u00b7zeits\u00b7schiff", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$.", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Am Ufer bei dem Schiffe K\u00f6nig Sigurd stand;", "tokens": ["Am", "U\u00b7fer", "bei", "dem", "Schif\u00b7fe", "K\u00f6\u00b7nig", "Si\u00b7gurd", "stand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fr\u00f6hlich war sein Herze, und purpurn sein Gewand.", "tokens": ["Fr\u00f6h\u00b7lich", "war", "sein", "Her\u00b7ze", ",", "und", "pur\u00b7purn", "sein", "Ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "VVFIN", "$,", "KON", "NE", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Voll hei\u00dfer Inbrunst harrt' er der holdsel'gen Maid,", "tokens": ["Voll", "hei\u00b7\u00dfer", "In\u00b7brunst", "harrt'", "er", "der", "hold\u00b7sel'\u00b7gen", "Maid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Da\u00df Ragnar sie br\u00e4chte. Doch oft wird Lust verkehrt in Leid.", "tokens": ["Da\u00df", "Rag\u00b7nar", "sie", "br\u00e4ch\u00b7te", ".", "Doch", "oft", "wird", "Lust", "ver\u00b7kehrt", "in", "Leid", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "$.", "KON", "ADV", "VAFIN", "NN", "VVPP", "APPR", "NN", "$."], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Es kam des Wegs vom Schlosse daher der junge Held;", "tokens": ["Es", "kam", "des", "Wegs", "vom", "Schlos\u00b7se", "da\u00b7her", "der", "jun\u00b7ge", "Held", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "PAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "So hanget wohl ein Wetter d\u00fcster \u00fcberm Feld,", "tokens": ["So", "han\u00b7get", "wohl", "ein", "Wet\u00b7ter", "d\u00fcs\u00b7ter", "\u00fc\u00b7berm", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Eh' es tobend ausbricht in Blitz und Donnerschlag,", "tokens": ["Eh'", "es", "to\u00b7bend", "aus\u00b7bricht", "in", "Blitz", "und", "Don\u00b7ner\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Wie auf der Stirn des Knaben des Grames Wolke lag.", "tokens": ["Wie", "auf", "der", "Stirn", "des", "Kna\u00b7ben", "des", "Gra\u00b7mes", "Wol\u00b7ke", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Ihm folgten sieben Degen in Helm und Panzerring,", "tokens": ["Ihm", "folg\u00b7ten", "sie\u00b7ben", "De\u00b7gen", "in", "Helm", "und", "Pan\u00b7zer\u00b7ring", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sie trugen eine Bahre, darob ein Teppich hing.", "tokens": ["Sie", "tru\u00b7gen", "ei\u00b7ne", "Bah\u00b7re", ",", "da\u00b7rob", "ein", "Tep\u00b7pich", "hing", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Langsam schritten alle, mit Blicken trauervoll", "tokens": ["Lang\u00b7sam", "schrit\u00b7ten", "al\u00b7le", ",", "mit", "Bli\u00b7cken", "trau\u00b7er\u00b7voll"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PIS", "$,", "APPR", "NN", "ADJD"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Gr\u00fc\u00dften sie den K\u00f6nig, da\u00df bangend ihm die Seele schwoll.", "tokens": ["Gr\u00fc\u00df\u00b7ten", "sie", "den", "K\u00f6\u00b7nig", ",", "da\u00df", "ban\u00b7gend", "ihm", "die", "See\u00b7le", "schwoll", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "KOUS", "ADJD", "PPER", "ART", "NN", "ADJD", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.6": {"line.1": {"text": "Da sprach Ragnar der Junge: \u00bbIch habe schlechten Gru\u00df,", "tokens": ["Da", "sprach", "Rag\u00b7nar", "der", "Jun\u00b7ge", ":", "\u00bb", "Ich", "ha\u00b7be", "schlech\u00b7ten", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ART", "NN", "$.", "$(", "PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Eitel Rabenbotschaft ist, was ich k\u00fcnden mu\u00df.", "tokens": ["Ei\u00b7tel", "Ra\u00b7ben\u00b7bot\u00b7schaft", "ist", ",", "was", "ich", "k\u00fcn\u00b7den", "mu\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Wohl bring' ich dir Alfsonnen, wie dein Spruch gebot;", "tokens": ["Wohl", "bring'", "ich", "dir", "Alf\u00b7son\u00b7nen", ",", "wie", "dein", "Spruch", "ge\u00b7bot", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "NN", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch wirst du nie sie minnen, geminnt hat sie der bleiche Tod.\u00ab", "tokens": ["Doch", "wirst", "du", "nie", "sie", "min\u00b7nen", ",", "ge\u00b7minnt", "hat", "sie", "der", "blei\u00b7che", "Tod", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PPER", "VVINF", "$,", "VVPP", "VAFIN", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+---+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Er winkte den Genossen, da\u00df sie aus der Hand", "tokens": ["Er", "wink\u00b7te", "den", "Ge\u00b7nos\u00b7sen", ",", "da\u00df", "sie", "aus", "der", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die B\u00fcrde setzen m\u00f6chten. Dann schlug er das Gewand", "tokens": ["Die", "B\u00fcr\u00b7de", "set\u00b7zen", "m\u00f6ch\u00b7ten", ".", "Dann", "schlug", "er", "das", "Ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$.", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Zur\u00fcck von der Bahre, die faltig es bedeckt:", "tokens": ["Zu\u00b7r\u00fcck", "von", "der", "Bah\u00b7re", ",", "die", "fal\u00b7tig", "es", "be\u00b7deckt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVPP", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Da lag die sch\u00f6ne Jungfrau tot dahingestreckt.", "tokens": ["Da", "lag", "die", "sch\u00f6\u00b7ne", "Jung\u00b7frau", "tot", "da\u00b7hin\u00b7ge\u00b7streckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Sie lag in Mohn und Lilien als wie ein schlafend Bild,", "tokens": ["Sie", "lag", "in", "Mohn", "und", "Li\u00b7li\u00b7en", "als", "wie", "ein", "schla\u00b7fend", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NE", "KOUS", "KOKOM", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Zugedr\u00fcckt die Augen, verf\u00e4rbt die Wangen mild,", "tokens": ["Zu\u00b7ge\u00b7dr\u00fcckt", "die", "Au\u00b7gen", ",", "ver\u00b7f\u00e4rbt", "die", "Wan\u00b7gen", "mild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Im wei\u00dfen Linnenkleide, jeden Schmuckes bar,", "tokens": ["Im", "wei\u00b7\u00dfen", "Lin\u00b7nen\u00b7klei\u00b7de", ",", "je\u00b7den", "Schmu\u00b7ckes", "bar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihr einzig Goldgeschmeide das sonnig leuchtende Haar.", "tokens": ["Ihr", "ein\u00b7zig", "Gold\u00b7ge\u00b7schmei\u00b7de", "das", "son\u00b7nig", "leuch\u00b7ten\u00b7de", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Da sie der K\u00f6nig sahe, die schneeblasse Maid,", "tokens": ["Da", "sie", "der", "K\u00f6\u00b7nig", "sa\u00b7he", ",", "die", "schnee\u00b7blas\u00b7se", "Maid", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihm war's, als f\u00fchre pl\u00f6tzlich durch all sein Eingeweid'", "tokens": ["Ihm", "wa\u00b7r's", ",", "als", "f\u00fch\u00b7re", "pl\u00f6tz\u00b7lich", "durch", "all", "sein", "Ein\u00b7ge\u00b7weid'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "ADJD", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ein zweischneidig Eisen. Zum Himmel auf er schrie.", "tokens": ["Ein", "zwei\u00b7schnei\u00b7dig", "Ei\u00b7sen", ".", "Zum", "Him\u00b7mel", "auf", "er", "schrie", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$.", "APPRART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Er hatte nimmer Minne getragen hei\u00df wie die.", "tokens": ["Er", "hat\u00b7te", "nim\u00b7mer", "Min\u00b7ne", "ge\u00b7tra\u00b7gen", "hei\u00df", "wie", "die", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "VVPP", "ADJD", "KOKOM", "ART", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Keine Tr\u00e4ne weint' er; starr blieb er stehn", "tokens": ["Kei\u00b7ne", "Tr\u00e4\u00b7ne", "weint'", "er", ";", "starr", "blieb", "er", "stehn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$.", "ADJD", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Mit vorgesunknem Antlitz. Wer ihn da gesehn:", "tokens": ["Mit", "vor\u00b7ge\u00b7sun\u00b7knem", "Ant\u00b7litz", ".", "Wer", "ihn", "da", "ge\u00b7sehn", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "PWS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er h\u00e4tt' ihn wohl gehalten f\u00fcr ein Bild von Stein.", "tokens": ["Er", "h\u00e4tt'", "ihn", "wohl", "ge\u00b7hal\u00b7ten", "f\u00fcr", "ein", "Bild", "von", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da ward ein tiefes Schweigen durch aller K\u00e4mpen Reihn.", "tokens": ["Da", "ward", "ein", "tie\u00b7fes", "Schwei\u00b7gen", "durch", "al\u00b7ler", "K\u00e4m\u00b7pen", "Reihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "NE", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed