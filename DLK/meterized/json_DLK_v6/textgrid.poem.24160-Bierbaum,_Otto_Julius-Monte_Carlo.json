{"textgrid.poem.24160": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Monte Carlo", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meer und Sterne, Palmen und das Leuchten", "tokens": ["Meer", "und", "Ster\u00b7ne", ",", "Pal\u00b7men", "und", "das", "Leuch\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wei\u00dfen Steines hinter Lorbeerhecken,", "tokens": ["Wei\u00b7\u00dfen", "Stei\u00b7nes", "hin\u00b7ter", "Lor\u00b7beer\u00b7he\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und im Saale Klirr und Kling und Scharren", "tokens": ["Und", "im", "Saa\u00b7le", "Klirr", "und", "Kling", "und", "Schar\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "NN", "KON", "NN", "KON", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Und das Rascheln bunter Seidenschleppen.", "tokens": ["Und", "das", "Ra\u00b7scheln", "bun\u00b7ter", "Sei\u00b7den\u00b7schlep\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Gl\u00fche Augen und erhitzte Wangen,", "tokens": ["Gl\u00fc\u00b7he", "Au\u00b7gen", "und", "er\u00b7hitz\u00b7te", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Hei\u00dfe H\u00e4nde, zuckende Geb\u00e4rden,", "tokens": ["Hei\u00b7\u00dfe", "H\u00e4n\u00b7de", ",", "zu\u00b7cken\u00b7de", "Ge\u00b7b\u00e4r\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Und die Br\u00fcste sprengen fast die Mieder.", "tokens": ["Und", "die", "Br\u00fcs\u00b7te", "spren\u00b7gen", "fast", "die", "Mie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ach ihr Kinder! Ach ihr lieben Kinder!", "tokens": ["Ach", "ihr", "Kin\u00b7der", "!", "Ach", "ihr", "lie\u00b7ben", "Kin\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "NN", "$.", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Nein, der Glaube ist nicht ausgestorben,", "tokens": ["Nein", ",", "der", "Glau\u00b7be", "ist", "nicht", "aus\u00b7ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und die Menschen sind doch sehr sympathisch.", "tokens": ["Und", "die", "Men\u00b7schen", "sind", "doch", "sehr", "sym\u00b7pa\u00b7thisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Selten noch konnt ich so gut sie leiden,", "tokens": ["Sel\u00b7ten", "noch", "konnt", "ich", "so", "gut", "sie", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "ADV", "ADJD", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Als, da ich sie heut am gr\u00fcnen Tische", "tokens": ["Als", ",", "da", "ich", "sie", "heut", "am", "gr\u00fc\u00b7nen", "Ti\u00b7sche"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "PPER", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Spielen sah, wie Kinder spieln im Sande.", "tokens": ["Spie\u00b7len", "sah", ",", "wie", "Kin\u00b7der", "spieln", "im", "San\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PWAV", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}