{"textgrid.poem.57539": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Den Alltagsmenschen", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr, denen der Zufall die Krankheit versagt,", "tokens": ["Ihr", ",", "de\u00b7nen", "der", "Zu\u00b7fall", "die", "Krank\u00b7heit", "ver\u00b7sagt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die g\u00f6ttlich Genie man benennt,", "tokens": ["Die", "g\u00f6tt\u00b7lich", "Ge\u00b7nie", "man", "be\u00b7nennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PIS", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "La\u00dft fahren die Trauer und seid nicht verzagt,", "tokens": ["La\u00dft", "fah\u00b7ren", "die", "Trau\u00b7er", "und", "seid", "nicht", "ver\u00b7zagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "ART", "NN", "KON", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ihr ahnet ja nicht, wie das brennt,", "tokens": ["Ihr", "ah\u00b7net", "ja", "nicht", ",", "wie", "das", "brennt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$,", "PWAV", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Lebt ruhig nur fort in dem engen Gebiet,", "tokens": ["Lebt", "ru\u00b7hig", "nur", "fort", "in", "dem", "en\u00b7gen", "Ge\u00b7biet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Mit euch und dem Herrgott in Frieden,", "tokens": ["Mit", "euch", "und", "dem", "Herr\u00b7gott", "in", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Und preist euch gl\u00fccklich, da\u00df eurem Gem\u00fct", "tokens": ["Und", "preist", "euch", "gl\u00fcck\u00b7lich", ",", "da\u00df", "eu\u00b7rem", "Ge\u00b7m\u00fct"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Kein st\u00fcrmendes Ringen beschieden.", "tokens": ["Kein", "st\u00fcr\u00b7men\u00b7des", "Rin\u00b7gen", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "O k\u00f6nntet ins Herz jenen M\u00e4nnern ihr schaun,", "tokens": ["O", "k\u00f6nn\u00b7tet", "ins", "Herz", "je\u00b7nen", "M\u00e4n\u00b7nern", "ihr", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "APPRART", "NN", "PDAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ihr pralltet erschrocken zur\u00fcck:", "tokens": ["Ihr", "prall\u00b7tet", "er\u00b7schro\u00b7cken", "zu\u00b7r\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Bleichzuckende Flammen und nebliges Graun,", "tokens": ["Bleich\u00b7zu\u00b7cken\u00b7de", "Flam\u00b7men", "und", "neb\u00b7li\u00b7ges", "Graun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Doch nimmer und nimmer Gl\u00fcck;", "tokens": ["Doch", "nim\u00b7mer", "und", "nim\u00b7mer", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KON", "ADV", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Kaum einem noch wurde vom Zufall beschert", "tokens": ["Kaum", "ei\u00b7nem", "noch", "wur\u00b7de", "vom", "Zu\u00b7fall", "be\u00b7schert"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADV", "VAFIN", "APPRART", "NN", "VVPP"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Genie und zufriedenes Leben,", "tokens": ["Ge\u00b7nie", "und", "zu\u00b7frie\u00b7de\u00b7nes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Den meisten hat Leben und Lieben zerst\u00f6rt", "tokens": ["Den", "meis\u00b7ten", "hat", "Le\u00b7ben", "und", "Lie\u00b7ben", "zer\u00b7st\u00f6rt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VAFIN", "NN", "KON", "ADJA", "VVPP"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Das Ringen und K\u00e4mpfen und Streben.", "tokens": ["Das", "Rin\u00b7gen", "und", "K\u00e4mp\u00b7fen", "und", "Stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Kein Frieden bei Tage, kein Frieden bei Nacht,", "tokens": ["Kein", "Frie\u00b7den", "bei", "Ta\u00b7ge", ",", "kein", "Frie\u00b7den", "bei", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "$,", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Im Fieber von Abend bis Fr\u00fch \u2013", "tokens": ["Im", "Fie\u00b7ber", "von", "A\u00b7bend", "bis", "Fr\u00fch", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "APPR", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Das Los jeder Stirn, der in h\u00f6llischer Pracht", "tokens": ["Das", "Los", "je\u00b7der", "Stirn", ",", "der", "in", "h\u00f6l\u00b7li\u00b7scher", "Pracht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Den Kainsstempel gab das Genie,", "tokens": ["Den", "Kains\u00b7stem\u00b7pel", "gab", "das", "Ge\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Ein Hungern nach Ruhe, ein Dursten nach Gl\u00fcck,", "tokens": ["Ein", "Hun\u00b7gern", "nach", "Ru\u00b7he", ",", "ein", "Durs\u00b7ten", "nach", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Nach Schatten, die ewig verschwinden,", "tokens": ["Nach", "Schat\u00b7ten", ",", "die", "e\u00b7wig", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADJD", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Sie suchen und suchen mit trostlosem Blick", "tokens": ["Sie", "su\u00b7chen", "und", "su\u00b7chen", "mit", "trost\u00b7lo\u00b7sem", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Und glauben doch selbst nicht ans Finden.", "tokens": ["Und", "glau\u00b7ben", "doch", "selbst", "nicht", "ans", "Fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und schlie\u00dflich, wenn alles verbrannt und vergl\u00fcht,", "tokens": ["Und", "schlie\u00df\u00b7lich", ",", "wenn", "al\u00b7les", "ver\u00b7brannt", "und", "ver\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "PIS", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und jeglich Idol ist zernagt,", "tokens": ["Und", "jeg\u00b7lich", "I\u00b7dol", "ist", "zer\u00b7nagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn \u00f6de die Seele und kahl das Gem\u00fct,", "tokens": ["Wenn", "\u00f6\u00b7de", "die", "See\u00b7le", "und", "kahl", "das", "Ge\u00b7m\u00fct", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Verzweiflung die Elenden plagt \u2013", "tokens": ["Ver\u00b7zwei\u00b7flung", "die", "E\u00b7len\u00b7den", "plagt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Die Tr\u00e4ume zerplatzen, ins Weite sich schwingt", "tokens": ["Die", "Tr\u00e4u\u00b7me", "zer\u00b7plat\u00b7zen", ",", "ins", "Wei\u00b7te", "sich", "schwingt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "APPRART", "NN", "PRF", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Des Gl\u00fcckes verbla\u00dfte Erscheinung,", "tokens": ["Des", "Gl\u00fc\u00b7ckes", "ver\u00b7bla\u00df\u00b7te", "Er\u00b7schei\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Und h\u00f6hnisch im herzlosen Herzen nur klingt", "tokens": ["Und", "h\u00f6h\u00b7nisch", "im", "herz\u00b7lo\u00b7sen", "Her\u00b7zen", "nur", "klingt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "ADJA", "NN", "ADV", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Das schneidende Lied der Verneinung.", "tokens": ["Das", "schnei\u00b7den\u00b7de", "Lied", "der", "Ver\u00b7nei\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}