{"textgrid.poem.31278": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seit ich IHM vertr\u00e4ue/", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seit ich IHM vertr\u00e4ue/", "tokens": ["Seit", "ich", "IhM", "ver\u00b7tr\u00e4u\u00b7e", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "lebe ich auffs Neue/", "tokens": ["le\u00b7be", "ich", "auffs", "Neu\u00b7e", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "ob mein Leib gleich bricht.", "tokens": ["ob", "mein", "Leib", "gleich", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wann mein Hertz auch zittert/", "tokens": ["Wann", "mein", "Hertz", "auch", "zit\u00b7tert", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "da\u00df es fast zersplittert \u2013", "tokens": ["da\u00df", "es", "fast", "zer\u00b7split\u00b7tert", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Dodt/ du schrekkst mich nicht!", "tokens": ["Dodt", "/", "du", "schrekkst", "mich", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wirff nach mir die Hippe/", "tokens": ["Wirff", "nach", "mir", "die", "Hip\u00b7pe", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "altes Mord-Gerippe:", "tokens": ["al\u00b7tes", "Mord\u00b7Ge\u00b7rip\u00b7pe", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "inner kortzer Zeit", "tokens": ["in\u00b7ner", "kort\u00b7zer", "Zeit"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "wekkt au\u00df deinem Grauen", "tokens": ["wekkt", "au\u00df", "dei\u00b7nem", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "zu saffirnen Auen", "tokens": ["zu", "saf\u00b7fir\u00b7nen", "Au\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "mich die Ewigkeit!", "tokens": ["mich", "die", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Hihr so mu\u00df ich sizzen", "tokens": ["Hihr", "so", "mu\u00df", "ich", "siz\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "nur auff Dornen-Spizzen/", "tokens": ["nur", "auff", "Dor\u00b7nen\u00b7Spiz\u00b7zen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schorff d\u00e4kkt mich und Grind;", "tokens": ["Schorff", "d\u00e4kkt", "mich", "und", "Grind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Angst/ Geseufftz und Jammer", "tokens": ["Angst", "/", "Ge\u00b7seufftz", "und", "Jam\u00b7mer"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "f\u00fcllt schon meine Kammer/", "tokens": ["f\u00fcllt", "schon", "mei\u00b7ne", "Kam\u00b7mer", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "wenn das Fr\u00fch-Roht spinnt!", "tokens": ["wenn", "das", "Fr\u00fch\u00b7Roht", "spinnt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Dort in mein Erwachen", "tokens": ["Dort", "in", "mein", "Er\u00b7wa\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "herrlich reucht ihr Wehn!", "tokens": ["herr\u00b7lich", "reucht", "ihr", "Wehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wie \u00fcmb Mandel-Ruhten/", "tokens": ["Wie", "\u00fcmb", "Man\u00b7del\u00b7Ruh\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "die Rubine bluhten/", "tokens": ["die", "Ru\u00b7bi\u00b7ne", "bluh\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "sich die Sterne drehn!", "tokens": ["sich", "die", "Ster\u00b7ne", "drehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Engel in mein Singen", "tokens": ["En\u00b7gel", "in", "mein", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "nichts al\u00df Palmen schwingen/", "tokens": ["nichts", "al\u00df", "Pal\u00b7men", "schwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "NN", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "HeRR/ dan b\u00fcn ich Dein!", "tokens": ["HeRR", "/", "dan", "b\u00fcn", "ich", "Dein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ADV", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Erst wenn meine Knochen", "tokens": ["Erst", "wenn", "mei\u00b7ne", "Kno\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "gantz und gar zerbrochen/", "tokens": ["gantz", "und", "gar", "zer\u00b7bro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "VVPP", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "werd ich Sieger seyn!", "tokens": ["werd", "ich", "Sie\u00b7ger", "seyn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}