{"dta.poem.7422": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingslied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Noch ein Lied dem deutschen B\u00fcrger,", "tokens": ["Noch", "ein", "Lied", "dem", "deut\u00b7schen", "B\u00fcr\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch ein \u00e4chtes Maienlied!", "tokens": ["Noch", "ein", "\u00e4ch\u00b7tes", "Mai\u00b7en\u00b7lied", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fr\u00fchling sei es keinem W\u00fcrger,", "tokens": ["Fr\u00fch\u00b7ling", "sei", "es", "kei\u00b7nem", "W\u00fcr\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der sein Volk zum Staube zieht;", "tokens": ["Der", "sein", "Volk", "zum", "Stau\u00b7be", "zieht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fr\u00fchling Jedem bis zum Tod,", "tokens": ["Fr\u00fch\u00b7ling", "Je\u00b7dem", "bis", "zum", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Fr\u00fchling nie f\u00fcr den Despot!", "tokens": ["Fr\u00fch\u00b7ling", "nie", "f\u00fcr", "den", "Des\u00b7pot", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Selbst der Himmel, warm und rein,", "tokens": ["Selbst", "der", "Him\u00b7mel", ",", "warm", "und", "rein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Der des Freien Brust erweitert,", "tokens": ["Der", "des", "Frei\u00b7en", "Brust", "er\u00b7wei\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Eine Klippe, dran er scheitert,", "tokens": ["Ei\u00b7ne", "Klip\u00b7pe", ",", "dran", "er", "schei\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "M\u00f6g' er jedem W\u00fctrich sein.", "tokens": ["M\u00f6g'", "er", "je\u00b7dem", "W\u00fct\u00b7rich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Alle Blumen sollen fl\u00fcstern:", "tokens": ["Al\u00b7le", "Blu\u00b7men", "sol\u00b7len", "fl\u00fcs\u00b7tern", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u201eseht ihr, seht ihr den Tyrann?", "tokens": ["\u201e", "seht", "ihr", ",", "seht", "ihr", "den", "Ty\u00b7rann", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201ebleib' in Deinem Reich, dem d\u00fcstern,", "tokens": ["\u201e", "bleib'", "in", "Dei\u00b7nem", "Reich", ",", "dem", "d\u00fcs\u00b7tern", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u201ein der H\u00f6lle, finstrer Mann!", "tokens": ["\u201e", "in", "der", "H\u00f6l\u00b7le", ",", "finst\u00b7rer", "Mann", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u201ewillst Du noch des Weihrauchs mehr?", "tokens": ["\u201e", "willst", "Du", "noch", "des", "Weih\u00b7rauchs", "mehr", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "ADV", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "\u201eunser Kelch ist f\u00fcr Dich leer.", "tokens": ["\u201e", "un\u00b7ser", "Kelch", "ist", "f\u00fcr", "Dich", "leer", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "APPR", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "\u201efort, Du taugst nicht an das Licht!", "tokens": ["\u201e", "fort", ",", "Du", "taugst", "nicht", "an", "das", "Licht", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "PPER", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "\u201eweiche ferne, Du Verr\u00e4ther,", "tokens": ["\u201e", "wei\u00b7che", "fer\u00b7ne", ",", "Du", "Ver\u00b7r\u00e4\u00b7ther", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJA", "ADJA", "$,", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "\u201edu verstehst den freien Aether", "tokens": ["\u201e", "du", "ver\u00b7stehst", "den", "frei\u00b7en", "A\u00b7e\u00b7ther"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.10": {"text": "\u201eund die Fr\u00fchlingsfreiheit nicht!\u201c", "tokens": ["\u201e", "und", "die", "Fr\u00fch\u00b7lings\u00b7frei\u00b7heit", "nicht", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "ART", "NN", "PTKNEG", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jede Biene d\u00fcnk' Tarantel,", "tokens": ["Je\u00b7de", "Bie\u00b7ne", "d\u00fcnk'", "Ta\u00b7ran\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jeder Rose Purpurkleid", "tokens": ["Je\u00b7der", "Ro\u00b7se", "Pur\u00b7pur\u00b7kleid"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihm ein Carbonarimantel,", "tokens": ["Ihm", "ein", "Car\u00b7bo\u00b7na\u00b7ri\u00b7man\u00b7tel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Drin ein Dolch f\u00fcr ihn bereit!", "tokens": ["Drin", "ein", "Dolch", "f\u00fcr", "ihn", "be\u00b7reit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jeglich S\u00e4useln, das er h\u00f6rt,", "tokens": ["Jeg\u00b7lich", "S\u00e4u\u00b7seln", ",", "das", "er", "h\u00f6rt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihm sein Volk, das sich emp\u00f6rt;", "tokens": ["Ihm", "sein", "Volk", ",", "das", "sich", "em\u00b7p\u00f6rt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "PRELS", "PRF", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Keine Freude und kein Scherz,", "tokens": ["Kei\u00b7ne", "Freu\u00b7de", "und", "kein", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Keine Wonne soll ihm bl\u00fchen,", "tokens": ["Kei\u00b7ne", "Won\u00b7ne", "soll", "ihm", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und von keiner Sonne gl\u00fchen", "tokens": ["Und", "von", "kei\u00b7ner", "Son\u00b7ne", "gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Je ihm sein sibirisch Herz!", "tokens": ["Je", "ihm", "sein", "si\u00b7bi\u00b7risch", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPOSAT", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "N\u00e4chtlich mit Entsetzen dreh' er", "tokens": ["N\u00e4cht\u00b7lich", "mit", "Ent\u00b7set\u00b7zen", "dreh'", "er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich im sternenlosen Nichts,", "tokens": ["Sich", "im", "ster\u00b7nen\u00b7lo\u00b7sen", "Nichts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und von allen Engeln seh' er", "tokens": ["Und", "von", "al\u00b7len", "En\u00b7geln", "seh'", "er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur den Engel des Gerichts;", "tokens": ["Nur", "den", "En\u00b7gel", "des", "Ge\u00b7richts", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jeder Schlag der Nachtigall", "tokens": ["Je\u00b7der", "Schlag", "der", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Kling' ihm wie Posaunenschall,", "tokens": ["Kling'", "ihm", "wie", "Po\u00b7sau\u00b7nen\u00b7schall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "KOKOM", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der ihn vor den Ew'gen ruft;", "tokens": ["Der", "ihn", "vor", "den", "Ew'\u00b7gen", "ruft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und der Lerche jubelnd Schmettern,", "tokens": ["Und", "der", "Ler\u00b7che", "ju\u00b7belnd", "Schmet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wie der Blitz von tausend Wettern", "tokens": ["Wie", "der", "Blitz", "von", "tau\u00b7send", "Wet\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Treff' es ihn aus blauer Luft.", "tokens": ["Treff'", "es", "ihn", "aus", "blau\u00b7er", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Jeder Bl\u00fctenbaum am Wege", "tokens": ["Je\u00b7der", "Bl\u00fc\u00b7ten\u00b7baum", "am", "We\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Streu' auf's Haubt ihm Silberschnee,", "tokens": ["Streu'", "auf's", "Haubt", "ihm", "Sil\u00b7ber\u00b7schnee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einen eis'gen Panzer lege", "tokens": ["Ei\u00b7nen", "eis'\u00b7gen", "Pan\u00b7zer", "le\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Um sein Schiff ihm jeder See;", "tokens": ["Um", "sein", "Schiff", "ihm", "je\u00b7der", "See", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo er immer landen mag,", "tokens": ["Wo", "er", "im\u00b7mer", "lan\u00b7den", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Flieh' erschreckt der goldne Tag;", "tokens": ["Flieh'", "er\u00b7schreckt", "der", "gold\u00b7ne", "Tag", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In der \u00f6den kahlen Flur", "tokens": ["In", "der", "\u00f6\u00b7den", "kah\u00b7len", "Flur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Soll sich seine Seele spiegeln,", "tokens": ["Soll", "sich", "sei\u00b7ne", "See\u00b7le", "spie\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ihm ein Buch mit tausend Siegeln", "tokens": ["Ihm", "ein", "Buch", "mit", "tau\u00b7send", "Sie\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sei im Lenze die Natur.", "tokens": ["Sei", "im", "Len\u00b7ze", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ja, o Lenz, sei f\u00fcr die Dichter,", "tokens": ["Ja", ",", "o", "Lenz", ",", "sei", "f\u00fcr", "die", "Dich\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "FM", "NN", "$,", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcr die V\u00f6lker Lenz allein!", "tokens": ["F\u00fcr", "die", "V\u00f6l\u00b7ker", "Lenz", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcr Tyrannen sollst Du Richter,", "tokens": ["F\u00fcr", "Ty\u00b7ran\u00b7nen", "sollst", "Du", "Rich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr Tyrannen R\u00e4cher sein.", "tokens": ["F\u00fcr", "Ty\u00b7ran\u00b7nen", "R\u00e4\u00b7cher", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schreib' auf jedes gr\u00fcne Blatt:", "tokens": ["Schreib'", "auf", "je\u00b7des", "gr\u00fc\u00b7ne", "Blatt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich bin eurer herzlich satt,", "tokens": ["Ich", "bin", "eu\u00b7rer", "herz\u00b7lich", "satt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Eurer schn\u00f6den Tyrannei!", "tokens": ["Eu\u00b7rer", "schn\u00f6\u00b7den", "Ty\u00b7ran\u00b7nei", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Frei sind meiner Blumen D\u00fcfte,", "tokens": ["Frei", "sind", "mei\u00b7ner", "Blu\u00b7men", "D\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Meine Wolken, meine L\u00fcfte,", "tokens": ["Mei\u00b7ne", "Wol\u00b7ken", ",", "mei\u00b7ne", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Auch die Menschen seien frei!", "tokens": ["Auch", "die", "Men\u00b7schen", "sei\u00b7en", "frei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}