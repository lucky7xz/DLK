{"textgrid.poem.48926": {"metadata": {"author": {"name": "Kleist, Ewald Christian von", "birth": "N.A.", "death": "N.A."}, "title": "An Damon", "genre": "verse", "period": "N.A.", "pub_year": 1737, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "St\u00f6hrer deiner eignen Ruh,", "tokens": ["St\u00f6h\u00b7rer", "dei\u00b7ner", "eig\u00b7nen", "Ruh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Damon! warum trauerst du?", "tokens": ["Da\u00b7mon", "!", "wa\u00b7rum", "trau\u00b7erst", "du", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Soll dein Kummer ewig siegen,", "tokens": ["Soll", "dein", "Kum\u00b7mer", "e\u00b7wig", "sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dir Stirn und Wangen pfl\u00fcgen?", "tokens": ["Und", "dir", "Stirn", "und", "Wan\u00b7gen", "pfl\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie der Glanz von dunkelm Licht", "tokens": ["Wie", "der", "Glanz", "von", "dun\u00b7kelm", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwach aus Todtengr\u00fcften bricht;", "tokens": ["Schwach", "aus", "Tod\u00b7ten\u00b7gr\u00fcf\u00b7ten", "bricht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So blinkt deine tr\u00fcbe Sele", "tokens": ["So", "blinkt", "dei\u00b7ne", "tr\u00fc\u00b7be", "Se\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Aus des Leibes Trauerh\u00f6hle.", "tokens": ["Aus", "des", "Lei\u00b7bes", "Trau\u00b7er\u00b7h\u00f6h\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch die kluge Mine zeigt,", "tokens": ["Doch", "die", "klu\u00b7ge", "Mi\u00b7ne", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn dein blasser Mund gleich schweigt,", "tokens": ["Wenn", "dein", "blas\u00b7ser", "Mund", "gleich", "schweigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df die Weisheit, die dich qu\u00e4let,", "tokens": ["Da\u00df", "die", "Weis\u00b7heit", ",", "die", "dich", "qu\u00e4\u00b7let", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich f\u00fcr Sinnlichkeiten st\u00e4hlet.", "tokens": ["Dich", "f\u00fcr", "Sinn\u00b7lich\u00b7kei\u00b7ten", "st\u00e4h\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Weiser Damon! dessen Haupt", "tokens": ["Wei\u00b7ser", "Da\u00b7mon", "!", "des\u00b7sen", "Haupt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$.", "PRELAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lorber um und um belaubt,", "tokens": ["Lor\u00b7ber", "um", "und", "um", "be\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "KON", "APPR", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00df die Weisheit immer sitzen", "tokens": ["Mu\u00df", "die", "Weis\u00b7heit", "im\u00b7mer", "sit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das Haupt voll Unmuth st\u00fctzen?", "tokens": ["Und", "das", "Haupt", "voll", "Un\u00b7muth", "st\u00fct\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nimmer, nimmer sey ihr gut,", "tokens": ["Nim\u00b7mer", ",", "nim\u00b7mer", "sey", "ihr", "gut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn sie dieses jemahls thut!", "tokens": ["Wenn", "sie", "die\u00b7ses", "je\u00b7mahls", "thut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In den W\u00fcsten zwischen Eulen", "tokens": ["In", "den", "W\u00fcs\u00b7ten", "zwi\u00b7schen", "Eu\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Baue sie sich Ehrens\u00e4ulen!", "tokens": ["Bau\u00b7e", "sie", "sich", "Eh\u00b7ren\u00b7s\u00e4u\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Freund! in deiner Jahre Zahl", "tokens": ["Freund", "!", "in", "dei\u00b7ner", "Jah\u00b7re", "Zahl"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Rechnet dir der Tod einmal", "tokens": ["Rech\u00b7net", "dir", "der", "Tod", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nebst den freudenreichen Tagen,", "tokens": ["Nebst", "den", "freu\u00b7den\u00b7rei\u00b7chen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch die Tage voll von Plagen.", "tokens": ["Auch", "die", "Ta\u00b7ge", "voll", "von", "Pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Du schwimmst in der Zeiten Raum,", "tokens": ["Du", "schwimmst", "in", "der", "Zei\u00b7ten", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie auf Str\u00f6men leichter Schaum;", "tokens": ["Wie", "auf", "Str\u00f6\u00b7men", "leich\u00b7ter", "Schaum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kanst du nicht so schnell zur Erden,", "tokens": ["Kanst", "du", "nicht", "so", "schnell", "zur", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie der Schaum zu Wasser werden?", "tokens": ["Wie", "der", "Schaum", "zu", "Was\u00b7ser", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch itzt schmeckt noch Wein und Ku\u00df.", "tokens": ["Doch", "itzt", "schmeckt", "noch", "Wein", "und", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf! ertr\u00e4nke den Verdru\u00df.", "tokens": ["Auf", "!", "er\u00b7tr\u00e4n\u00b7ke", "den", "Ver\u00b7dru\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh, wie mir die Tropfen gleiten", "tokens": ["Sieh", ",", "wie", "mir", "die", "Trop\u00b7fen", "glei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "PPER", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob der K\u00fcrze dieser Zeiten.", "tokens": ["Ob", "der", "K\u00fcr\u00b7ze", "die\u00b7ser", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Zehnmal f\u00fcll ich schon die\u00df Glas", "tokens": ["Zehn\u00b7mal", "f\u00fcll", "ich", "schon", "die\u00df", "Glas"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PDS", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit der Trauben edlem Na\u00df;", "tokens": ["Mit", "der", "Trau\u00b7ben", "ed\u00b7lem", "Na\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch reitzt mich sein g\u00fcldnes Blinken,", "tokens": ["Noch", "reitzt", "mich", "sein", "g\u00fcld\u00b7nes", "Blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und die Freude w\u00e4chst im Trinken.", "tokens": ["Und", "die", "Freu\u00b7de", "w\u00e4chst", "im", "Trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "M\u00e4dchen! k\u00fc\u00df mich, mich \u2013 \u2013 ver \u2013 \u2013 langt,", "tokens": ["M\u00e4d\u00b7chen", "!", "k\u00fc\u00df", "mich", ",", "mich", "\u2013", "\u2013", "ver", "\u2013", "\u2013", "langt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "punct", "word", "punct", "punct", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "$,", "PPER", "$(", "$(", "NE", "$(", "$(", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "O! mein froher Schedel wankt.", "tokens": ["O", "!", "mein", "fro\u00b7her", "Sche\u00b7del", "wankt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df mich, unter Scherz und Lallen,", "tokens": ["La\u00df", "mich", ",", "un\u00b7ter", "Scherz", "und", "Lal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sanft dir in die Arme fallen.", "tokens": ["Sanft", "dir", "in", "die", "Ar\u00b7me", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Th\u00fcr und Teppich tanzt um mich,", "tokens": ["Th\u00fcr", "und", "Tep\u00b7pich", "tanzt", "um", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Erd und Himmel drehet sich;", "tokens": ["Erd", "und", "Him\u00b7mel", "dre\u00b7het", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seht die Fenster in den Zimmern,", "tokens": ["Seht", "die", "Fens\u00b7ter", "in", "den", "Zim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie sie an dem Boden schimmern!", "tokens": ["Wie", "sie", "an", "dem", "Bo\u00b7den", "schim\u00b7mern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Alles sieht mich l\u00e4chelnd an.", "tokens": ["Al\u00b7les", "sieht", "mich", "l\u00e4\u00b7chelnd", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "O Evoe, o Evan!", "tokens": ["O", "E\u00b7voe", ",", "o", "E\u00b7van", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "La\u00df sich nur zu deinen Ehren", "tokens": ["La\u00df", "sich", "nur", "zu", "dei\u00b7nen", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PRF", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Damon auch einmal bekehren.", "tokens": ["Da\u00b7mon", "auch", "ein\u00b7mal", "be\u00b7keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}