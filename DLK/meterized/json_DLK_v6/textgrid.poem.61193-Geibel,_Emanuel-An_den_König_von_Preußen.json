{"textgrid.poem.61193": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "An den K\u00f6nig von Preu\u00dfen", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe nie nach Gunst gerungen,", "tokens": ["Ich", "ha\u00b7be", "nie", "nach", "Gunst", "ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich sang allein, was ich gemu\u00dft;", "tokens": ["Ich", "sang", "al\u00b7lein", ",", "was", "ich", "ge\u00b7mu\u00dft", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Rosen, frisch dem Lenz entsprungen,", "tokens": ["Wie", "Ro\u00b7sen", ",", "frisch", "dem", "Lenz", "ent\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So brach's hervor aus meiner Brust.", "tokens": ["So", "brach's", "her\u00b7vor", "aus", "mei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und fr\u00f6hlich streut' ich in die Winde", "tokens": ["Und", "fr\u00f6h\u00b7lich", "streut'", "ich", "in", "die", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die leichte, reiche Blumenpracht;", "tokens": ["Die", "leich\u00b7te", ",", "rei\u00b7che", "Blu\u00b7men\u00b7pracht", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ob sie der Freund, der Tadler finde,", "tokens": ["Ob", "sie", "der", "Freund", ",", "der", "Tad\u00b7ler", "fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich hab' es nie zuvor bedacht.", "tokens": ["Ich", "hab'", "es", "nie", "zu\u00b7vor", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch dir, o F\u00fcrst aus edlem Stamme,", "tokens": ["Doch", "dir", ",", "o", "F\u00fcrst", "aus", "ed\u00b7lem", "Stam\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der treu vor Gott sein Volk regiert,", "tokens": ["Der", "treu", "vor", "Gott", "sein", "Volk", "re\u00b7giert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den sch\u00f6ner noch des Geistes Flamme", "tokens": ["Den", "sch\u00f6\u00b7ner", "noch", "des", "Geis\u00b7tes", "Flam\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als seiner V\u00e4ter Krone ziert,", "tokens": ["Als", "sei\u00b7ner", "V\u00e4\u00b7ter", "Kro\u00b7ne", "ziert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf den, wenn sich die Wolken schw\u00e4rzen,", "tokens": ["Auf", "den", ",", "wenn", "sich", "die", "Wol\u00b7ken", "schw\u00e4r\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "KOUS", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als Leuchtturm schauet Deutschlands Kern:", "tokens": ["Als", "Leucht\u00b7turm", "schau\u00b7et", "Deutschlands", "Kern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "NE", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Wie dank' ich dir aus tiefstem Herzen,", "tokens": ["Wie", "dank'", "ich", "dir", "aus", "tiefs\u00b7tem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie dank' ich alles dir so gern!", "tokens": ["Wie", "dank'", "ich", "al\u00b7les", "dir", "so", "gern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PIS", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was ich in unsrer W\u00e4lder Stille,", "tokens": ["Was", "ich", "in", "uns\u00b7rer", "W\u00e4l\u00b7der", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An Hellas' Strand umsonst begehrt,", "tokens": ["An", "Hel\u00b7las'", "Strand", "um\u00b7sonst", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das hat dein k\u00f6niglicher Wille", "tokens": ["Das", "hat", "dein", "k\u00f6\u00b7nig\u00b7li\u00b7cher", "Wil\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus freien Hulden mir gew\u00e4hrt:", "tokens": ["Aus", "frei\u00b7en", "Hul\u00b7den", "mir", "ge\u00b7w\u00e4hrt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du gabst ein Leben mir, vom Staube", "tokens": ["Du", "gabst", "ein", "Le\u00b7ben", "mir", ",", "vom", "Stau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des niedern Marktes unber\u00fchrt,", "tokens": ["Des", "nie\u00b7dern", "Mark\u00b7tes", "un\u00b7be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Leben, wie's im gr\u00fcnen Laube", "tokens": ["Ein", "Le\u00b7ben", ",", "wie's", "im", "gr\u00fc\u00b7nen", "Lau\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der freie Vogel singend f\u00fchrt.", "tokens": ["Der", "frei\u00b7e", "Vo\u00b7gel", "sin\u00b7gend", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So helfe Gott mir, da\u00df ich walte", "tokens": ["So", "hel\u00b7fe", "Gott", "mir", ",", "da\u00df", "ich", "wal\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "PPER", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Ernst des Pfundes, das mir ward,", "tokens": ["Mit", "Ernst", "des", "Pfun\u00b7des", ",", "das", "mir", "ward", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich getreu am Banner halte", "tokens": ["Da\u00df", "ich", "ge\u00b7treu", "am", "Ban\u00b7ner", "hal\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der deutschen Ehre, Zucht und Art.", "tokens": ["Der", "deut\u00b7schen", "Eh\u00b7re", ",", "Zucht", "und", "Art", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Fern von dem Schwarm, der unbesonnen", "tokens": ["Fern", "von", "dem", "Schwarm", ",", "der", "un\u00b7be\u00b7son\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$,", "ART", "ADJA"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Altar und Herz in Tr\u00fcmmern schl\u00e4gt,", "tokens": ["Al\u00b7tar", "und", "Herz", "in", "Tr\u00fcm\u00b7mern", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Quillt mir der Dichtung heil'ger Bronnen", "tokens": ["Quillt", "mir", "der", "Dich\u00b7tung", "heil'\u00b7ger", "Bron\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Am Felsen, der die Kirche tr\u00e4gt.", "tokens": ["Am", "Fel\u00b7sen", ",", "der", "die", "Kir\u00b7che", "tr\u00e4gt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nicht, da\u00df mir drum in Nacht versunken", "tokens": ["Nicht", ",", "da\u00df", "mir", "drum", "in", "Nacht", "ver\u00b7sun\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "$,", "KOUS", "PPER", "PAV", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Welt und ihre Sch\u00f6nheit sei,", "tokens": ["Die", "Welt", "und", "ih\u00b7re", "Sch\u00f6n\u00b7heit", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nein! Wer aus jenem Born getrunken,", "tokens": ["Nein", "!", "Wer", "aus", "je\u00b7nem", "Born", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PWS", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem ward erst ganz die Lippe frei.", "tokens": ["Dem", "ward", "erst", "ganz", "die", "Lip\u00b7pe", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sein ernster Mut mag fr\u00f6hlich scherzen,", "tokens": ["Sein", "erns\u00b7ter", "Mut", "mag", "fr\u00f6h\u00b7lich", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des Grundes, drauf er steht, bewu\u00dft;", "tokens": ["Des", "Grun\u00b7des", ",", "drauf", "er", "steht", ",", "be\u00b7wu\u00dft", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "PPER", "VVFIN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er tr\u00e4gt erbl\u00fcht im reinen Herzen", "tokens": ["Er", "tr\u00e4gt", "er\u00b7bl\u00fcht", "im", "rei\u00b7nen", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den Rosengarten jeder Lust.", "tokens": ["Den", "Ro\u00b7sen\u00b7gar\u00b7ten", "je\u00b7der", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und wo die grimmsten Qualen bluten,", "tokens": ["Und", "wo", "die", "grimms\u00b7ten", "Qua\u00b7len", "blu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In jeden Abgrund schaut er k\u00fchn,", "tokens": ["In", "je\u00b7den", "Ab\u00b7grund", "schaut", "er", "k\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sieht er doch ob den finstern Fluten", "tokens": ["Sieht", "er", "doch", "ob", "den", "fins\u00b7tern", "Flu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Bogen der Vers\u00f6hnung gl\u00fchn.", "tokens": ["Den", "Bo\u00b7gen", "der", "Ver\u00b7s\u00f6h\u00b7nung", "gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Fluch, den \u00d6dipus entsandte,", "tokens": ["Den", "Fluch", ",", "den", "\u00d6\u00b7di\u00b7pus", "ent\u00b7sand\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Er zeugt ihn neu aus ", "tokens": ["Er", "zeugt", "ihn", "neu", "aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Und schreitet unversehrt wie Dante", "tokens": ["Und", "schrei\u00b7tet", "un\u00b7ver\u00b7sehrt", "wie", "Dan\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "KOKOM", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Selbst durch der H\u00f6lle Flammen hin.", "tokens": ["Selbst", "durch", "der", "H\u00f6l\u00b7le", "Flam\u00b7men", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und so durch Wonn' und Jammer gehn!", "tokens": ["Und", "so", "durch", "Wonn'", "und", "Jam\u00b7mer", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein eitel Spielwerk ist mein Singen,", "tokens": ["Kein", "ei\u00b7tel", "Spiel\u00b7werk", "ist", "mein", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sp\u00fcr' in mir des Geistes Wehn.", "tokens": ["Ich", "sp\u00fcr'", "in", "mir", "des", "Geis\u00b7tes", "Wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ob auch der Vernichtung T\u00f6nen", "tokens": ["Und", "ob", "auch", "der", "Ver\u00b7nich\u00b7tung", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Haufe rasch entgegenflammt:", "tokens": ["Der", "Hau\u00b7fe", "rasch", "ent\u00b7ge\u00b7gen\u00b7flammt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu baun, zu bilden, zu vers\u00f6hnen,", "tokens": ["Zu", "baun", ",", "zu", "bil\u00b7den", ",", "zu", "ver\u00b7s\u00f6h\u00b7nen", ","], "token_info": ["word"