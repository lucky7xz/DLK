{"dta.poem.8480": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Das Wort .", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Wort gleicht dem beschwingten Pfeil,", "tokens": ["Das", "Wort", "gleicht", "dem", "be\u00b7schwing\u00b7ten", "Pfeil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ist es einmal deinem Bogen", "tokens": ["Und", "ist", "es", "ein\u00b7mal", "dei\u00b7nem", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In T\u00e4ndeln oder Ernst entflogen,", "tokens": ["In", "T\u00e4n\u00b7deln", "o\u00b7der", "Ernst", "ent\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erschrecken mu\u00df dich seine Eil\u2019.", "tokens": ["Er\u00b7schre\u00b7cken", "mu\u00df", "dich", "sei\u00b7ne", "Eil'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dem K\u00f6rnlein gleicht es, deiner Hand", "tokens": ["Dem", "K\u00f6rn\u00b7lein", "gleicht", "es", ",", "dei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entschl\u00fcpft; wer mag es wiederfinden?", "tokens": ["Ent\u00b7schl\u00fcpft", ";", "wer", "mag", "es", "wie\u00b7der\u00b7fin\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und dennoch wuchert\u2019s in den Gr\u00fcnden", "tokens": ["Und", "den\u00b7noch", "wu\u00b7chert's", "in", "den", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Und treibt die Wurzeln durch das Land.", "tokens": ["Und", "treibt", "die", "Wur\u00b7zeln", "durch", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Gleicht dem verlornen Funken, der", "tokens": ["Gleicht", "dem", "ver\u00b7lor\u00b7nen", "Fun\u00b7ken", ",", "der"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "PRELS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vielleicht erlischt am feuchten Tage,", "tokens": ["Viel\u00b7leicht", "er\u00b7lischt", "am", "feuch\u00b7ten", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vielleicht am milden glimmt im Haage,", "tokens": ["Viel\u00b7leicht", "am", "mil\u00b7den", "glimmt", "im", "Haa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Am d\u00fcrren schwillt zum Flammenmeer.", "tokens": ["Am", "d\u00fcr\u00b7ren", "schwillt", "zum", "Flam\u00b7men\u00b7meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und Worte sind es doch, die einst", "tokens": ["Und", "Wor\u00b7te", "sind", "es", "doch", ",", "die", "einst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "VAFIN", "PPER", "ADV", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So schwer in deine Schaale fallen,", "tokens": ["So", "schwer", "in", "dei\u00b7ne", "Schaa\u00b7le", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ist Keins ein Nichtiges von Allen,", "tokens": ["Ist", "Keins", "ein", "Nich\u00b7ti\u00b7ges", "von", "Al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um jedes hoffst du oder weinst.", "tokens": ["Um", "je\u00b7des", "hoffst", "du", "o\u00b7der", "weinst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "VVFIN", "PPER", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O einen Strahl der Himmelsau,", "tokens": ["O", "ei\u00b7nen", "Strahl", "der", "Him\u00b7mel\u00b7sau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Gott, dem Zagenden und Blinden!", "tokens": ["Mein", "Gott", ",", "dem", "Za\u00b7gen\u00b7den", "und", "Blin\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie soll er Ziel und Acker finden?", "tokens": ["Wie", "soll", "er", "Ziel", "und", "A\u00b7cker", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie L\u00fcfte messen und den Thau?", "tokens": ["Wie", "L\u00fcf\u00b7te", "mes\u00b7sen", "und", "den", "Thau", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Allm\u00e4cht\u2019ger, der das Wort geschenkt,", "tokens": ["All\u00b7m\u00e4cht'\u00b7ger", ",", "der", "das", "Wort", "ge\u00b7schenkt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch seine Zukunft uns verhalten,", "tokens": ["Doch", "sei\u00b7ne", "Zu\u00b7kunft", "uns", "ver\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Woll\u2019 selber deiner Gabe walten,", "tokens": ["Woll'", "sel\u00b7ber", "dei\u00b7ner", "Ga\u00b7be", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch deinen Hauch sei sie gelenkt!", "tokens": ["Durch", "dei\u00b7nen", "Hauch", "sei", "sie", "ge\u00b7lenkt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Richte den Pfeil dem Ziele zu,", "tokens": ["Rich\u00b7te", "den", "Pfeil", "dem", "Zie\u00b7le", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "N\u00e4hre das K\u00f6rnlein schlummertrunken!", "tokens": ["N\u00e4h\u00b7re", "das", "K\u00f6rn\u00b7lein", "schlum\u00b7mer\u00b7trun\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Erstick\u2019 ihn oder fach\u2019 den Funken!", "tokens": ["Er\u00b7stick'", "ihn", "o\u00b7der", "fach'", "den", "Fun\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn was da frommt, das wei\u00dft nur du.", "tokens": ["Denn", "was", "da", "frommt", ",", "das", "wei\u00dft", "nur", "du", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VVFIN", "$,", "PDS", "VVFIN", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}