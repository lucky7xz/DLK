{"textgrid.poem.47550": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[o D\u00fcsterni\u00df, o Trauerflor!]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbo D\u00fcsterni\u00df, o Trauerflor!", "tokens": ["\u00bb", "o", "D\u00fcs\u00b7ter\u00b7ni\u00df", ",", "o", "Trau\u00b7er\u00b7flor", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der ist f\u00fcrwahr der gr\u00f6\u00dfte Thor,", "tokens": ["Der", "ist", "f\u00fcr\u00b7wahr", "der", "gr\u00f6\u00df\u00b7te", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der sich f\u00fcr heller Sonne Strahl", "tokens": ["Der", "sich", "f\u00fcr", "hel\u00b7ler", "Son\u00b7ne", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Solch eine tiefe Nacht erkor.\u00ab", "tokens": ["Solch", "ei\u00b7ne", "tie\u00b7fe", "Nacht", "er\u00b7kor", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So rief ich aus, geradewegs", "tokens": ["So", "rief", "ich", "aus", ",", "ge\u00b7ra\u00b7de\u00b7wegs"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gekommen aus der Schenke Thor;", "tokens": ["Ge\u00b7kom\u00b7men", "aus", "der", "Schen\u00b7ke", "Thor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da kam dem Auge Klosterraum", "tokens": ["Da", "kam", "dem", "Au\u00b7ge", "Klos\u00b7ter\u00b7raum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Schule gar so dunkel vor.", "tokens": ["Und", "Schu\u00b7le", "gar", "so", "dun\u00b7kel", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}