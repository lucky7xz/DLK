{"dta.poem.16210": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "264.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In meiner Wohnung bin ich wohnlich eingewohnt,", "tokens": ["In", "mei\u00b7ner", "Woh\u00b7nung", "bin", "ich", "wohn\u00b7lich", "ein\u00b7ge\u00b7wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Ungewohnetem will ich da seyn verschont.", "tokens": ["Mit", "Un\u00b7ge\u00b7woh\u00b7ne\u00b7tem", "will", "ich", "da", "seyn", "ver\u00b7schont", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "ADV", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das Ungew\u00f6hnliche zu sehen geh' ich aus,", "tokens": ["Das", "Un\u00b7ge\u00b7w\u00f6hn\u00b7li\u00b7che", "zu", "se\u00b7hen", "geh'", "ich", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch zum Gew\u00f6hnlichen kehr' ich mit Lust nach Haus.", "tokens": ["Doch", "zum", "Ge\u00b7w\u00f6hn\u00b7li\u00b7chen", "kehr'", "ich", "mit", "Lust", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "APPR", "NN", "APPR", "NN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}, "stanza.3": {"line.1": {"text": "Gewohnheit, aber nur die \u00fcble, ist zu schelten,", "tokens": ["Ge\u00b7wohn\u00b7heit", ",", "a\u00b7ber", "nur", "die", "\u00fcb\u00b7le", ",", "ist", "zu", "schel\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADV", "ART", "ADJA", "$,", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gew\u00f6hnung bessere mu\u00df f\u00fcr das beste gelten.", "tokens": ["Ge\u00b7w\u00f6h\u00b7nung", "bes\u00b7se\u00b7re", "mu\u00df", "f\u00fcr", "das", "bes\u00b7te", "gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "VMFIN", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Denn Gutes, zur Natur geworden, haftet nur,", "tokens": ["Denn", "Gu\u00b7tes", ",", "zur", "Na\u00b7tur", "ge\u00b7wor\u00b7den", ",", "haf\u00b7tet", "nur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "APPRART", "NN", "VAPP", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gewohnheit aber wird zur anderen Natur.", "tokens": ["Ge\u00b7wohn\u00b7heit", "a\u00b7ber", "wird", "zur", "an\u00b7de\u00b7ren", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}