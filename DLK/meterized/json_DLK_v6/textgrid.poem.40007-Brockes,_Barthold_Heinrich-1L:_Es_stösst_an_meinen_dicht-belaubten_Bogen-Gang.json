{"textgrid.poem.40007": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es st\u00f6sst an meinen dicht-belaubten Bogen-Gang", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es st\u00f6sst an meinen dicht-belaubten Bogen-Gang", "tokens": ["Es", "st\u00f6sst", "an", "mei\u00b7nen", "dicht\u00b7be\u00b7laub\u00b7ten", "Bo\u00b7gen\u00b7Gang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Fisch-Teich, der, so breit, als lang,", "tokens": ["Ein", "FischTeich", ",", "der", ",", "so", "breit", ",", "als", "lang", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "$,", "ADV", "ADJD", "$,", "KOUS", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein Regel-rechtes Viereck zeiget.", "tokens": ["Ein", "Re\u00b7gel\u00b7rech\u00b7tes", "Vier\u00b7eck", "zei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Ufer deckt bebl\u00fchmtes Gras,", "tokens": ["Das", "U\u00b7fer", "deckt", "be\u00b7bl\u00fchm\u00b7tes", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und, weil es allgem\u00e4hlig steiget,", "tokens": ["Und", ",", "weil", "es", "all\u00b7ge\u00b7m\u00e4h\u00b7lig", "stei\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Scheint jede Seit' ein kleiner H\u00fcgel.", "tokens": ["Scheint", "je\u00b7de", "Seit'", "ein", "klei\u00b7ner", "H\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das glatte Wasser scheint ein Glas", "tokens": ["Das", "glat\u00b7te", "Was\u00b7ser", "scheint", "ein", "Glas"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von einem rein polirten Spiegel,", "tokens": ["Von", "ei\u00b7nem", "rein", "po\u00b7lir\u00b7ten", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der, an der Seiten, uns der Erden gr\u00fcne Zier,", "tokens": ["Der", ",", "an", "der", "Sei\u00b7ten", ",", "uns", "der", "Er\u00b7den", "gr\u00fc\u00b7ne", "Zier", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ART", "NN", "$,", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und, in der Mitte, gar den himmlischen Sapphir,", "tokens": ["Und", ",", "in", "der", "Mit\u00b7te", ",", "gar", "den", "himm\u00b7li\u00b7schen", "Sap\u00b7phir", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Des Tages voller Glantz, des Nachts voll Sterne, zeiget,", "tokens": ["Des", "Ta\u00b7ges", "vol\u00b7ler", "Glantz", ",", "des", "Nachts", "voll", "Ster\u00b7ne", ",", "zei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "ADV", "ADJD", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und so die sch\u00f6ne Pracht des Himmels und der Welt", "tokens": ["Und", "so", "die", "sch\u00f6\u00b7ne", "Pracht", "des", "Him\u00b7mels", "und", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Verdoppelt uns vor Augen stellt.", "tokens": ["Ver\u00b7dop\u00b7pelt", "uns", "vor", "Au\u00b7gen", "stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn ich der gr\u00fcnen Klarheit Grentzen,", "tokens": ["Wenn", "ich", "der", "gr\u00fc\u00b7nen", "Klar\u00b7heit", "Grent\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit aufmercksamem Blick, beschau';", "tokens": ["Mit", "auf\u00b7merck\u00b7sa\u00b7mem", "Blick", ",", "be\u00b7schau'", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seh ich des Himmerls funckelnd Blau", "tokens": ["Seh", "ich", "des", "Him\u00b7merls", "fun\u00b7ckelnd", "Blau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVPP", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Oft rein, oft hier und dort voll Wolcken-Silber gl\u00e4ntzen.", "tokens": ["Oft", "rein", ",", "oft", "hier", "und", "dort", "voll", "Wol\u00b7cken\u00b7Sil\u00b7ber", "gl\u00e4nt\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADV", "KON", "ADV", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man kann, wenn man's erweget, finden,", "tokens": ["Man", "kann", ",", "wenn", "man's", "er\u00b7we\u00b7get", ",", "fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VMFIN", "$,", "KOUS", "PIS", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie, voller Licht und Klarheit, hier", "tokens": ["Wie", ",", "vol\u00b7ler", "Licht", "und", "Klar\u00b7heit", ",", "hier"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "$,", "ADJA", "NN", "KON", "NN", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Des Himmels und der Erden Zier,", "tokens": ["Des", "Him\u00b7mels", "und", "der", "Er\u00b7den", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf einer Stelle, sich verbinden.", "tokens": ["Auf", "ei\u00b7ner", "Stel\u00b7le", ",", "sich", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, da\u00df man nicht den Sch\u00f6pfer preiset,", "tokens": ["Ach", ",", "da\u00df", "man", "nicht", "den", "Sch\u00f6p\u00b7fer", "prei\u00b7set", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PIS", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn man so holde Sch\u00f6nheit sieht,", "tokens": ["Wenn", "man", "so", "hol\u00b7de", "Sch\u00f6n\u00b7heit", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Womit sich die Natur, auf sein Gehei\u00df, bem\u00fcht,", "tokens": ["Wo\u00b7mit", "sich", "die", "Na\u00b7tur", ",", "auf", "sein", "Ge\u00b7hei\u00df", ",", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "(um es in's Aug' uns recht zu pr\u00e4gen)", "tokens": ["(", "um", "es", "in's", "Aug'", "uns", "recht", "zu", "pr\u00e4\u00b7gen", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUI", "PPER", "APPRART", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie uns gedoppelt vorzulegen!", "tokens": ["Sie", "uns", "ge\u00b7dop\u00b7pelt", "vor\u00b7zu\u00b7le\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Denn dencket nicht, als ob von ungefehr", "tokens": ["Denn", "den\u00b7cket", "nicht", ",", "als", "ob", "von", "un\u00b7ge\u00b7fehr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "KOKOM", "KOUS", "APPR", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Des Wassers Fl\u00e4che solche Gl\u00e4tte", "tokens": ["Des", "Was\u00b7sers", "Fl\u00e4\u00b7che", "sol\u00b7che", "Gl\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Empfangen h\u00e4tte.", "tokens": ["Emp\u00b7fan\u00b7gen", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Wie Alles, kommt auch die\u00df von Gottes Allmacht her.", "tokens": ["Wie", "Al\u00b7les", ",", "kommt", "auch", "die\u00df", "von", "Got\u00b7tes", "All\u00b7macht", "her", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "VVFIN", "ADV", "PDS", "APPR", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ach, da\u00df ich oft an diese Wahrheit d\u00e4chte!", "tokens": ["Ach", ",", "da\u00df", "ich", "oft", "an", "die\u00b7se", "Wahr\u00b7heit", "d\u00e4ch\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADV", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ach, da\u00df doch \u00f6fters mein Gem\u00fcthe", "tokens": ["Ach", ",", "da\u00df", "doch", "\u00f6f\u00b7ters", "mein", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Den Teich, von meines Sch\u00f6pfers G\u00fcte,", "tokens": ["Den", "Teich", ",", "von", "mei\u00b7nes", "Sch\u00f6p\u00b7fers", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Als einen Spiegel, brauchen m\u00f6chte!", "tokens": ["Als", "ei\u00b7nen", "Spie\u00b7gel", ",", "brau\u00b7chen", "m\u00f6ch\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Schatten hier, und dort der Wiederschein", "tokens": ["Der", "Schat\u00b7ten", "hier", ",", "und", "dort", "der", "Wie\u00b7der\u00b7schein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "KON", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von den geschornen Taxus-Hecken,", "tokens": ["Von", "den", "ge\u00b7schor\u00b7nen", "Ta\u00b7xus\u00b7He\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wodurch der Teich umfasst, bedecken,", "tokens": ["Wo\u00b7durch", "der", "Teich", "um\u00b7fasst", ",", "be\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In einer Anmuth-reichen Pracht,", "tokens": ["In", "ei\u00b7ner", "An\u00b7muth\u00b7rei\u00b7chen", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit gr\u00fcner D\u00e4mm'rung hier, dort einer gr\u00fcnen Nacht,", "tokens": ["Mit", "gr\u00fc\u00b7ner", "D\u00e4m\u00b7m'\u00b7rung", "hier", ",", "dort", "ei\u00b7ner", "gr\u00fc\u00b7nen", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Die unbewegte Fluth. So kr\u00e4ftig war das Gr\u00fcn,", "tokens": ["Die", "un\u00b7be\u00b7weg\u00b7te", "Fluth", ".", "So", "kr\u00e4f\u00b7tig", "war", "das", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ADV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df es an manchem Orte schien,", "tokens": ["Da\u00df", "es", "an", "man\u00b7chem", "Or\u00b7te", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Als n\u00e4hme wahres Schilf und Binsen,", "tokens": ["Als", "n\u00e4h\u00b7me", "wah\u00b7res", "Schilf", "und", "Bin\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Als n\u00e4hmen gr\u00fcne Wasser-Linsen", "tokens": ["Als", "n\u00e4h\u00b7men", "gr\u00fc\u00b7ne", "Was\u00b7ser\u00b7Lin\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Des Wassers Fl\u00e4che wircklich ein:", "tokens": ["Des", "Was\u00b7sers", "Fl\u00e4\u00b7che", "wir\u00b7ck\u00b7lich", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+----+", "measure": "unknown.measure.tri"}, "line.11": {"text": "Recht leiblich schien der Schein zu seyn.", "tokens": ["Recht", "leib\u00b7lich", "schien", "der", "Schein", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "So herrlich gl\u00e4ntzt, so lieblich prangt die Fluth,", "tokens": ["So", "herr\u00b7lich", "gl\u00e4ntzt", ",", "so", "lieb\u00b7lich", "prangt", "die", "Fluth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,", "ADV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "So lange sie, in glatter Stille, ruht.", "tokens": ["So", "lan\u00b7ge", "sie", ",", "in", "glat\u00b7ter", "Stil\u00b7le", ",", "ruht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Allein es sp\u00fcret unsre Brust", "tokens": ["Al\u00b7lein", "es", "sp\u00fc\u00b7ret", "uns\u00b7re", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch eine neue Lust,", "tokens": ["Noch", "ei\u00b7ne", "neu\u00b7e", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So bald von ungefehr", "tokens": ["So", "bald", "von", "un\u00b7ge\u00b7fehr"], "token