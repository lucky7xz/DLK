{"textgrid.poem.58591": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hast du auch die s\u00fc\u00dfe Stimm', liebe Seel', in dir vernommen,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hast du auch die s\u00fc\u00dfe Stimm', liebe Seel', in dir vernommen,", "tokens": ["Hast", "du", "auch", "die", "s\u00fc\u00b7\u00dfe", "Stimm'", ",", "lie\u00b7be", "Seel'", ",", "in", "dir", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,", "ADJA", "NN", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Dieses Evangelium, da\u00df Gott selbst ins Herz will kommen?", "tokens": ["Die\u00b7ses", "E\u00b7van\u00b7ge\u00b7li\u00b7um", ",", "da\u00df", "Gott", "selbst", "ins", "Herz", "will", "kom\u00b7men", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "KOUS", "NN", "ADV", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Bleib dann eingekehrt und still, bis es hei\u00dfet: Siehe da,", "tokens": ["Bleib", "dann", "ein\u00b7ge\u00b7kehrt", "und", "still", ",", "bis", "es", "hei\u00b7\u00dfet", ":", "Sie\u00b7he", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "KON", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$.", "VVIMP", "ADV", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Nun hast du den Schatz in dir, deinen Gott im Herzen nah!", "tokens": ["Nun", "hast", "du", "den", "Schatz", "in", "dir", ",", "dei\u00b7nen", "Gott", "im", "Her\u00b7zen", "nah", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "APPR", "PPER", "$,", "PPOSAT", "NN", "APPRART", "NN", "ADJD", "$."], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Da dient man aus Liebe nur und nicht an Vergelten denket,", "tokens": ["Da", "dient", "man", "aus", "Lie\u00b7be", "nur", "und", "nicht", "an", "Ver\u00b7gel\u00b7ten", "den\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "NN", "ADV", "KON", "PTKNEG", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Der hat Lohn und Himmel g'nug, wem das h\u00f6chste Gut sich schenket,", "tokens": ["Der", "hat", "Lohn", "und", "Him\u00b7mel", "g'\u00b7nug", ",", "wem", "das", "h\u00f6chs\u00b7te", "Gut", "sich", "schen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "KON", "NN", "ADV", "$,", "PWS", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Gib nur hin in seine Macht, was du hast und kannst und bist,", "tokens": ["Gib", "nur", "hin", "in", "sei\u00b7ne", "Macht", ",", "was", "du", "hast", "und", "kannst", "und", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,", "PWS", "PPER", "VAFIN", "KON", "VMFIN", "KON", "VAFIN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Gib ihm ein dein ganzes Herz; Gott mu\u00df herrschen, wo er ist!", "tokens": ["Gib", "ihm", "ein", "dein", "gan\u00b7zes", "Herz", ";", "Gott", "mu\u00df", "herr\u00b7schen", ",", "wo", "er", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "PPOSAT", "ADJA", "NN", "$.", "NN", "VMFIN", "VVINF", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "+-+-+-++-+-+-+", "measure": "unknown.measure.octa.plus"}}}}}