{"dta.poem.14272": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der junge Ehemann.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Jetzt neben meinem Liebchen", "tokens": ["Jetzt", "ne\u00b7ben", "mei\u00b7nem", "Lieb\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sitz' ich im Schatten k\u00fchl,", "tokens": ["Sitz'", "ich", "im", "Schat\u00b7ten", "k\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie wiegt ein muntres B\u00fcbchen,", "tokens": ["Sie", "wiegt", "ein", "mun\u00b7tres", "B\u00fcb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Th\u00e4ler schimmern schw\u00fcl,", "tokens": ["Die", "Th\u00e4\u00b7ler", "schim\u00b7mern", "schw\u00fcl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und unten im leisen Winde", "tokens": ["Und", "un\u00b7ten", "im", "lei\u00b7sen", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Regt sich das Kornfeld kaum", "tokens": ["Regt", "sich", "das", "Korn\u00b7feld", "kaum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und \u00fcber uns s\u00e4uselt die Linde \u2014", "tokens": ["Und", "\u00fc\u00b7ber", "uns", "s\u00e4u\u00b7selt", "die", "Lin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "NE", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Es ist mir noch wie ein Traum.", "tokens": ["Es", "ist", "mir", "noch", "wie", "ein", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "KOKOM", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}