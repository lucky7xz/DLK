{"dta.poem.12093": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Schwalb' ist eingethan in D\u00f6rfern nicht allein,", "tokens": ["Die", "Schwalb'", "ist", "ein\u00b7ge\u00b7than", "in", "D\u00f6r\u00b7fern", "nicht", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPR", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie wagt sich, scheuer zwar, auch in die St\u00e4dt' hinein.", "tokens": ["Sie", "wagt", "sich", ",", "scheu\u00b7er", "zwar", ",", "auch", "in", "die", "St\u00e4dt'", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "ADJD", "ADV", "$,", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "In gr\u00f6\u00dfern St\u00e4dten wol fliegt wilder nur und scheuer,", "tokens": ["In", "gr\u00f6\u00b7\u00dfern", "St\u00e4d\u00b7ten", "wol", "fliegt", "wil\u00b7der", "nur", "und", "scheu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVFIN", "ADJA", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kreischend ob dem Gekreisch, die Thurmschwalb' oder Steuer.", "tokens": ["Krei\u00b7schend", "ob", "dem", "Ge\u00b7kreisch", ",", "die", "Thurm\u00b7schwalb'", "o\u00b7der", "Steu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "In kleinern St\u00e4dten, die zur H\u00e4lfte l\u00e4ndlich sind,", "tokens": ["In", "klei\u00b7nern", "St\u00e4d\u00b7ten", ",", "die", "zur", "H\u00e4lf\u00b7te", "l\u00e4nd\u00b7lich", "sind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schwebt an der \u00e4u\u00dfern Wand der Mauerschwalbe Kind.", "tokens": ["Schwebt", "an", "der", "\u00e4u\u00b7\u00dfern", "Wand", "der", "Mau\u00b7er\u00b7schwal\u00b7be", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Hausschwalb' aber wohnt, Rauchschwalbe hei\u00dft sie auch,", "tokens": ["Die", "Haus\u00b7schwalb'", "a\u00b7ber", "wohnt", ",", "Rauch\u00b7schwal\u00b7be", "hei\u00dft", "sie", "auch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am liebsten auf dem Dorf in stiller H\u00fctten Rauch;", "tokens": ["Am", "liebs\u00b7ten", "auf", "dem", "Dorf", "in", "stil\u00b7ler", "H\u00fct\u00b7ten", "Rauch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wo sie sich abendlich versammeln auf dem Plan,", "tokens": ["Wo", "sie", "sich", "a\u00b7bend\u00b7lich", "ver\u00b7sam\u00b7meln", "auf", "dem", "Plan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sich erz\u00e4hlen, was sie in den H\u00e4usern sahn.", "tokens": ["Und", "sich", "er\u00b7z\u00e4h\u00b7len", ",", "was", "sie", "in", "den", "H\u00e4u\u00b7sern", "sahn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVINF", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Doch welche sah ich, die hoch im Gebirge schwirrten", "tokens": ["Doch", "wel\u00b7che", "sah", "ich", ",", "die", "hoch", "im", "Ge\u00b7bir\u00b7ge", "schwirr\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "PPER", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVFIN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ums einsame Geh\u00f6ft, bewohnt von armen Hirten;", "tokens": ["Ums", "ein\u00b7sa\u00b7me", "Ge\u00b7h\u00f6ft", ",", "be\u00b7wohnt", "von", "ar\u00b7men", "Hir\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "$,", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Die vor der Einsamkeit nicht schienen dort zu schaudern,", "tokens": ["Die", "vor", "der", "Ein\u00b7sam\u00b7keit", "nicht", "schie\u00b7nen", "dort", "zu", "schau\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PTKNEG", "VVFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo sie am Abend nicht mit Nachbarn k\u00f6nnen plaudern.", "tokens": ["Wo", "sie", "am", "A\u00b7bend", "nicht", "mit", "Nach\u00b7barn", "k\u00f6n\u00b7nen", "plau\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "PTKNEG", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Sie plaudern unter sich, das Paar mit seiner Brut,", "tokens": ["Sie", "plau\u00b7dern", "un\u00b7ter", "sich", ",", "das", "Paar", "mit", "sei\u00b7ner", "Brut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "$,", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mit dem Hirten, wann er heimkehrt von der Hut.", "tokens": ["Und", "mit", "dem", "Hir\u00b7ten", ",", "wann", "er", "heim\u00b7kehrt", "von", "der", "Hut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Wie traulichen Verkehr hier Mensch und Vogel pflegen,", "tokens": ["Wie", "trau\u00b7li\u00b7chen", "Ver\u00b7kehr", "hier", "Mensch", "und", "Vo\u00b7gel", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADV", "NN", "KON", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sah ich, als beim Geh\u00f6ft ich Obdach sucht' im Regen.", "tokens": ["Sah", "ich", ",", "als", "beim", "Ge\u00b7h\u00f6ft", "ich", "Ob\u00b7dach", "sucht'", "im", "Re\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "APPRART", "NN", "PPER", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.10": {"line.1": {"text": "Die Leute waren aus, die Th\u00fcr nicht zugemacht,", "tokens": ["Die", "Leu\u00b7te", "wa\u00b7ren", "aus", ",", "die", "Th\u00fcr", "nicht", "zu\u00b7ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "$,", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein Hund, der bellte, nur die Schwalben hielten Wacht.", "tokens": ["Kein", "Hund", ",", "der", "bell\u00b7te", ",", "nur", "die", "Schwal\u00b7ben", "hiel\u00b7ten", "Wacht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "VVFIN", "$,", "ADV", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Ich fand sie in der Stub', als ich hineingekommen,", "tokens": ["Ich", "fand", "sie", "in", "der", "Stub'", ",", "als", "ich", "hin\u00b7ein\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie hatten am Geb\u00e4lk der Mitte Sitz genommen.", "tokens": ["Sie", "hat\u00b7ten", "am", "Ge\u00b7b\u00e4lk", "der", "Mit\u00b7te", "Sitz", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Von hier die Th\u00fcre stand, von dort das Fenster auf,", "tokens": ["Von", "hier", "die", "Th\u00fc\u00b7re", "stand", ",", "von", "dort", "das", "Fens\u00b7ter", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "VVFIN", "$,", "APPR", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ungehemmt herein, hinaus ergieng' ihr Lauf.", "tokens": ["Da\u00df", "un\u00b7ge\u00b7hemmt", "her\u00b7ein", ",", "hin\u00b7aus", "er\u00b7gieng'", "ihr", "Lauf", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PTKVZ", "$,", "APZR", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Doch unbedachtsam st\u00f6rt' ich ihren freien Flug,", "tokens": ["Doch", "un\u00b7be\u00b7dacht\u00b7sam", "st\u00f6rt'", "ich", "ih\u00b7ren", "frei\u00b7en", "Flug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da ich das Fenster schlo\u00df, weil na\u00df mich fror im Zug.", "tokens": ["Da", "ich", "das", "Fens\u00b7ter", "schlo\u00df", ",", "weil", "na\u00df", "mich", "fror", "im", "Zug", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "KOUS", "ADJD", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Die Leute kamen dann, und fanden ausgeschlossen", "tokens": ["Die", "Leu\u00b7te", "ka\u00b7men", "dann", ",", "und", "fan\u00b7den", "aus\u00b7ge\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "KON", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom eingedrungnen Gast die alten Hausgenossen.", "tokens": ["Vom", "ein\u00b7ge\u00b7drung\u00b7nen", "Gast", "die", "al\u00b7ten", "Haus\u00b7ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Mit Pfeifen \u00f6ffnete das Fenster gleich ein Bube,", "tokens": ["Mit", "Pfei\u00b7fen", "\u00f6ff\u00b7ne\u00b7te", "das", "Fens\u00b7ter", "gleich", "ein", "Bu\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und eine Schwalbe kam geflogen in die Stube.", "tokens": ["Und", "ei\u00b7ne", "Schwal\u00b7be", "kam", "ge\u00b7flo\u00b7gen", "in", "die", "Stu\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "Die andre folgt' ihr bald, und vom Geb\u00e4lke nieder", "tokens": ["Die", "and\u00b7re", "folgt'", "ihr", "bald", ",", "und", "vom", "Ge\u00b7b\u00e4l\u00b7ke", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "PPER", "ADV", "$,", "KON", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Spr\u00fchten sie \u00fcber'n Tisch ihr triefendes Gefieder.", "tokens": ["Spr\u00fch\u00b7ten", "sie", "\u00fc\u00b7ber'n", "Tisch", "ihr", "trie\u00b7fen\u00b7des", "Ge\u00b7fie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.17": {"line.1": {"text": "Ich m\u00f6chte wissen, ob sie hier im Winter bleiben;", "tokens": ["Ich", "m\u00f6ch\u00b7te", "wis\u00b7sen", ",", "ob", "sie", "hier", "im", "Win\u00b7ter", "blei\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "KOUS", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom warmen Ofen kann sie doch kein Frost vertreiben.", "tokens": ["Vom", "war\u00b7men", "O\u00b7fen", "kann", "sie", "doch", "kein", "Frost", "ver\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VMFIN", "PPER", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.18": {"line.1": {"text": "Auch Nahrungslosigkeit wird hier sie nicht bekriegen;", "tokens": ["Auch", "Nah\u00b7rungs\u00b7lo\u00b7sig\u00b7keit", "wird", "hier", "sie", "nicht", "be\u00b7krie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "ADV", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum mindsten damals war die Stube voll von Fliegen.", "tokens": ["Zum", "minds\u00b7ten", "da\u00b7mals", "war", "die", "Stu\u00b7be", "voll", "von", "Flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADV", "VAFIN", "ART", "NN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}