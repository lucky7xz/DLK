{"dta.poem.8153": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "2 .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Und achtest meiner nicht;", "tokens": ["Und", "ach\u00b7test", "mei\u00b7ner", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nun schmerzt mich deine wei\u00dfe Hand,", "tokens": ["Nun", "schmerzt", "mich", "dei\u00b7ne", "wei\u00b7\u00dfe", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein s\u00fc\u00dfes Angesicht.", "tokens": ["Dein", "s\u00fc\u00b7\u00dfes", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O sprich wie sonst ein liebes Wort,", "tokens": ["O", "sprich", "wie", "sonst", "ein", "lie\u00b7bes", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KOKOM", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein einzig Wort mir zu!", "tokens": ["Ein", "ein\u00b7zig", "Wort", "mir", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Wunden bluten heimlich fort,", "tokens": ["Die", "Wun\u00b7den", "blu\u00b7ten", "heim\u00b7lich", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch du hast keine Ruh'.", "tokens": ["Auch", "du", "hast", "kei\u00b7ne", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Mund, der jetzt zu meiner Qual", "tokens": ["Der", "Mund", ",", "der", "jetzt", "zu", "mei\u00b7ner", "Qual"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich stumm vor mir verschlie\u00dft,", "tokens": ["Sich", "stumm", "vor", "mir", "ver\u00b7schlie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich hab' ihn ja so tausend mal,", "tokens": ["Ich", "hab'", "ihn", "ja", "so", "tau\u00b7send", "mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "CARD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Viel tausend mal gek\u00fc\u00dft.", "tokens": ["Viel", "tau\u00b7send", "mal", "ge\u00b7k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Was einst so \u00fcberselig war,", "tokens": ["Was", "einst", "so", "\u00fc\u00b7ber\u00b7se\u00b7lig", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bricht nun das Herz entzwei;", "tokens": ["Bricht", "nun", "das", "Herz", "ent\u00b7zwei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Aug', das meine Seele trank,", "tokens": ["Das", "Aug'", ",", "das", "mei\u00b7ne", "See\u00b7le", "trank", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sieht fremd an mir vorbei.", "tokens": ["Sieht", "fremd", "an", "mir", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}