{"textgrid.poem.35995": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "An Karoline H", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab ein Str\u00e4u\u00dflein funden,", "tokens": ["Ich", "hab", "ein", "Str\u00e4u\u00df\u00b7lein", "fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du liebe G\u00e4rtnerin,", "tokens": ["Du", "lie\u00b7be", "G\u00e4rt\u00b7ne\u00b7rin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das hast du frisch gebunden", "tokens": ["Das", "hast", "du", "frisch", "ge\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit zartem Blumen-Sinn.", "tokens": ["Mit", "zar\u00b7tem", "Blu\u00b7men\u00b7Sinn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Bl\u00fcmlein lieblich prangen", "tokens": ["Die", "Bl\u00fcm\u00b7lein", "lieb\u00b7lich", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im hellen Tr\u00e4nentau,", "tokens": ["Im", "hel\u00b7len", "Tr\u00e4\u00b7nen\u00b7tau", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und blicken mit Verlangen", "tokens": ["Und", "bli\u00b7cken", "mit", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hinauf zum Himmelsblau.", "tokens": ["Hin\u00b7auf", "zum", "Him\u00b7mels\u00b7blau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es haben milde L\u00fcfte", "tokens": ["Es", "ha\u00b7ben", "mil\u00b7de", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von deinem Blumenbeet", "tokens": ["Von", "dei\u00b7nem", "Blu\u00b7men\u00b7beet"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die wunders\u00fc\u00dfen D\u00fcfte", "tokens": ["Die", "wun\u00b7der\u00b7s\u00fc\u00b7\u00dfen", "D\u00fcf\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu mir herabgeweht.", "tokens": ["Zu", "mir", "her\u00b7ab\u00b7ge\u00b7weht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "In deinem Garten gl\u00fchen", "tokens": ["In", "dei\u00b7nem", "Gar\u00b7ten", "gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Rosen hell im Schein;", "tokens": ["Die", "Ro\u00b7sen", "hell", "im", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf meinen Fluren bl\u00fchen", "tokens": ["Auf", "mei\u00b7nen", "Flu\u00b7ren", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur kleine Bl\u00fcmelein.", "tokens": ["Nur", "klei\u00b7ne", "Bl\u00fc\u00b7me\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Auch meine Bl\u00fcmlein stehen", "tokens": ["Auch", "mei\u00b7ne", "Bl\u00fcm\u00b7lein", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im hellen Tr\u00e4nentau", "tokens": ["Im", "hel\u00b7len", "Tr\u00e4\u00b7nen\u00b7tau"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und \u00f6ffnen sich und sehen", "tokens": ["Und", "\u00f6ff\u00b7nen", "sich", "und", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hinauf zum Himmelblau.", "tokens": ["Hin\u00b7auf", "zum", "Him\u00b7mel\u00b7blau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wollst deine Anemonen,", "tokens": ["Wollst", "dei\u00b7ne", "A\u00b7ne\u00b7mo\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Rosen holden Schein,", "tokens": ["Der", "Ro\u00b7sen", "hol\u00b7den", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Violen, Kaiserkronen", "tokens": ["Vi\u00b7o\u00b7len", ",", "Kai\u00b7ser\u00b7kro\u00b7nen"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum vollen Kranze reih'n.", "tokens": ["Zum", "vol\u00b7len", "Kran\u00b7ze", "reih'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und auch von meinen Bl\u00fcten", "tokens": ["Und", "auch", "von", "mei\u00b7nen", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein zartes Denkemein", "tokens": ["Ein", "zar\u00b7tes", "Den\u00b7ke\u00b7mein"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der trauten Schwester bieten", "tokens": ["Der", "trau\u00b7ten", "Schwes\u00b7ter", "bie\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In deines Kranzes Reih'n.", "tokens": ["In", "dei\u00b7nes", "Kran\u00b7zes", "Reih'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "PPOSAT", "NN", "NN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}