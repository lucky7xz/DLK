{"textgrid.poem.45818": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ruhe! \u2013 die Gr\u00e4ber erbeben;", "genre": "verse", "period": "N.A.", "pub_year": 1799, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ruhe! \u2013 die Gr\u00e4ber erbeben;", "tokens": ["Ru\u00b7he", "!", "\u2013", "die", "Gr\u00e4\u00b7ber", "er\u00b7be\u00b7ben", ";"], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Ruhe! \u2013 und heftig hervor", "tokens": ["Ru\u00b7he", "!", "\u2013", "und", "hef\u00b7tig", "her\u00b7vor"], "token_info": ["word", "punct", "punct", "word", "word", "word"], "pos": ["NN", "$.", "$(", "KON", "ADJD", "PTKVZ"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "St\u00fcrzt aus der Ruhe das Leben,", "tokens": ["St\u00fcrzt", "aus", "der", "Ru\u00b7he", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Str\u00f6mt aus sich selbsten empor", "tokens": ["Str\u00f6mt", "aus", "sich", "selbs\u00b7ten", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PRF", "ADV", "PTKVZ"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Die Menge, vereinzelt im Chor.", "tokens": ["Die", "Men\u00b7ge", ",", "ver\u00b7ein\u00b7zelt", "im", "Chor", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Schaffend er\u00f6ffnet der Meister", "tokens": ["Schaf\u00b7fend", "er\u00b7\u00f6ff\u00b7net", "der", "Meis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Gr\u00e4ber \u2013 Geborener Tanz", "tokens": ["Gr\u00e4\u00b7ber", "\u2013", "Ge\u00b7bo\u00b7re\u00b7ner", "Tanz"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$(", "NN", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Schweben die t\u00f6nenden Geister;", "tokens": ["Schwe\u00b7ben", "die", "t\u00f6\u00b7nen\u00b7den", "Geis\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Schimmert im eigenen Glanz", "tokens": ["Schim\u00b7mert", "im", "ei\u00b7ge\u00b7nen", "Glanz"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Der T\u00f6ne bunt wechselnder Kranz.", "tokens": ["Der", "T\u00f6\u00b7ne", "bunt", "wech\u00b7seln\u00b7der", "Kranz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Alle in einem verschlungen,", "tokens": ["Al\u00b7le", "in", "ei\u00b7nem", "ver\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "ADJA", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Jeder im eigenen Klang,", "tokens": ["Je\u00b7der", "im", "ei\u00b7ge\u00b7nen", "Klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "M\u00e4chtig durchs Ganze geschwungen,", "tokens": ["M\u00e4ch\u00b7tig", "durchs", "Gan\u00b7ze", "ge\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Eilet der Geister Gesang", "tokens": ["Ei\u00b7let", "der", "Geis\u00b7ter", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Gestaltet die B\u00fchne entlang.", "tokens": ["Ge\u00b7stal\u00b7tet", "die", "B\u00fch\u00b7ne", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Heilige brausende Wogen,", "tokens": ["Hei\u00b7li\u00b7ge", "brau\u00b7sen\u00b7de", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Ernst und woll\u00fcstige Glut", "tokens": ["Ernst", "und", "wol\u00b7l\u00fcs\u00b7ti\u00b7ge", "Glut"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KON", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Str\u00f6met in schimmernden Bogen,", "tokens": ["Str\u00f6\u00b7met", "in", "schim\u00b7mern\u00b7den", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Spr\u00fchet in klingender Wut", "tokens": ["Spr\u00fc\u00b7het", "in", "klin\u00b7gen\u00b7der", "Wut"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Des Geistertanz silberne Flut.", "tokens": ["Des", "Geis\u00b7ter\u00b7tanz", "sil\u00b7ber\u00b7ne", "Flut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Alle in einem, erstanden,", "tokens": ["Al\u00b7le", "in", "ei\u00b7nem", ",", "er\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "APPR", "ART", "$,", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Sind sie sich selbst nicht bewu\u00dft", "tokens": ["Sind", "sie", "sich", "selbst", "nicht", "be\u00b7wu\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "PTKNEG", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sie sich einzeln verbanden;", "tokens": ["Da\u00df", "sie", "sich", "ein\u00b7zeln", "ver\u00b7ban\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "F\u00fchlt in der eigenen Brust", "tokens": ["F\u00fchlt", "in", "der", "ei\u00b7ge\u00b7nen", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Ein jeder vom Ganzen die Lust.", "tokens": ["Ein", "je\u00b7der", "vom", "Gan\u00b7zen", "die", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Aber im inneren Leben", "tokens": ["A\u00b7ber", "im", "in\u00b7ne\u00b7ren", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Fesselt der Meister das Sein;", "tokens": ["Fes\u00b7selt", "der", "Meis\u00b7ter", "das", "Sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "L\u00e4\u00dft sie dann ringen und streben;", "tokens": ["L\u00e4\u00dft", "sie", "dann", "rin\u00b7gen", "und", "stre\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Handelnd durcheilet die Reihn", "tokens": ["Han\u00b7delnd", "durc\u00b7hei\u00b7let", "die", "Reihn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Das Ganze im einzelnen Schein.", "tokens": ["Das", "Gan\u00b7ze", "im", "ein\u00b7zel\u00b7nen", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}