{"textgrid.poem.32345": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "22. An das Bild der Gerechtigkeit, in dem Hause", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gerechtigkeit! wie k\u00f6mmst du hier zu stehen?", "tokens": ["Ge\u00b7rech\u00b7tig\u00b7keit", "!", "wie", "k\u00f6mmst", "du", "hier", "zu", "ste\u00b7hen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hat dich dein Hausherr schon gesehen?", "tokens": ["Hat", "dich", "dein", "Haus\u00b7herr", "schon", "ge\u00b7se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbwie meinst du, Fremder, diese Frage?", "tokens": ["\u00bb", "wie", "meinst", "du", ",", "Frem\u00b7der", ",", "die\u00b7se", "Fra\u00b7ge", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "$,", "NN", "$,", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er sieht und \u00fcbersieht mich alle Tage.\u00ab", "tokens": ["Er", "sieht", "und", "\u00fc\u00b7ber\u00b7sieht", "mich", "al\u00b7le", "Ta\u00b7ge", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}