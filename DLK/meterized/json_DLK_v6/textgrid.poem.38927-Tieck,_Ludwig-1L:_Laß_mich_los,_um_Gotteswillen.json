{"textgrid.poem.38927": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: La\u00df mich los, um Gotteswillen", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df mich los, um Gotteswillen", "tokens": ["La\u00df", "mich", "los", ",", "um", "Got\u00b7tes\u00b7wil\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "KOUI", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gieb mich armen Sklaven frei,", "tokens": ["Gieb", "mich", "ar\u00b7men", "Skla\u00b7ven", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df die Augen dir verh\u00fcllen,", "tokens": ["La\u00df", "die", "Au\u00b7gen", "dir", "ver\u00b7h\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ihr Glanz nicht t\u00f6dlich sei.", "tokens": ["Da\u00df", "ihr", "Glanz", "nicht", "t\u00f6d\u00b7lich", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mu\u00dft du mich in Ketten schleifen", "tokens": ["Mu\u00dft", "du", "mich", "in", "Ket\u00b7ten", "schlei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00e4rker als von Demantstein?", "tokens": ["St\u00e4r\u00b7ker", "als", "von", "De\u00b7mant\u00b7stein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00df das Schicksal mich ergreifen,", "tokens": ["Mu\u00df", "das", "Schick\u00b7sal", "mich", "er\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich ihr Kriegsgefangner sein? \u2013", "tokens": ["Ich", "ihr", "Kriegs\u00b7ge\u00b7fang\u00b7ner", "sein", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VAINF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}