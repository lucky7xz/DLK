{"textgrid.poem.55736": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich habe gewartet von Tag zu Tag,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe gewartet von Tag zu Tag,", "tokens": ["Ich", "ha\u00b7be", "ge\u00b7war\u00b7tet", "von", "Tag", "zu", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ob nicht ein Zeichen mir werden mag;", "tokens": ["Ob", "nicht", "ein", "Zei\u00b7chen", "mir", "wer\u00b7den", "mag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "PPER", "VAINF", "VMFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich habe gewartet, gl\u00e4ubig und fromm,", "tokens": ["Ich", "ha\u00b7be", "ge\u00b7war\u00b7tet", ",", "gl\u00e4u\u00b7big", "und", "fromm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und habe gebetet: O komm, o komm!", "tokens": ["Und", "ha\u00b7be", "ge\u00b7be\u00b7tet", ":", "O", "komm", ",", "o", "komm", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "$.", "NE", "VVFIN", "$,", "FM", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch ein Tag zog nach dem andern vorbei, \u2013", "tokens": ["Doch", "ein", "Tag", "zog", "nach", "dem", "an\u00b7dern", "vor\u00b7bei", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "PTKVZ", "$,", "$("], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Vergeblich erklang meiner Sehnsucht Schrei ...", "tokens": ["Ver\u00b7geb\u00b7lich", "er\u00b7klang", "mei\u00b7ner", "Sehn\u00b7sucht", "Schrei", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}}, "stanza.4": {"line.1": {"text": "Das alte Leben von Neuem beginnt,", "tokens": ["Das", "al\u00b7te", "Le\u00b7ben", "von", "Neu\u00b7em", "be\u00b7ginnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Strom meiner Liebe \u2013 im Schmutze verrinnt.", "tokens": ["Der", "Strom", "mei\u00b7ner", "Lie\u00b7be", "\u2013", "im", "Schmut\u00b7ze", "ver\u00b7rinnt", "."], "token_info": ["word", "word", "word", "word", "pu