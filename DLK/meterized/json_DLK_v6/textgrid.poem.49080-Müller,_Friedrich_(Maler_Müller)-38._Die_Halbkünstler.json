{"textgrid.poem.49080": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "38. Die Halbk\u00fcnstler", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bis an das mittlere Thor des Tempels gelanget wol leichtlich", "tokens": ["Bis", "an", "das", "mitt\u00b7le\u00b7re", "Thor", "des", "Tem\u00b7pels", "ge\u00b7lan\u00b7get", "wol", "leicht\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "ADV", "ADJD"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Jeder, dem nicht die Natur himmlischen Funken versagt;", "tokens": ["Je\u00b7der", ",", "dem", "nicht", "die", "Na\u00b7tur", "himm\u00b7li\u00b7schen", "Fun\u00b7ken", "ver\u00b7sagt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PTKNEG", "ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Aber von dort an weiter zu schreiten, verhindert die Pforte,", "tokens": ["A\u00b7ber", "von", "dort", "an", "wei\u00b7ter", "zu", "schrei\u00b7ten", ",", "ver\u00b7hin\u00b7dert", "die", "Pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "APPR", "ADV", "PTKZU", "VVINF", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Die, aus dauerndem Erz, nur der Geweihte erschlie\u00dft,", "tokens": ["Die", ",", "aus", "dau\u00b7ern\u00b7dem", "Erz", ",", "nur", "der", "Ge\u00b7weih\u00b7te", "er\u00b7schlie\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ART", "NN", "$,", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Dem Apollo selber den G\u00f6tterku\u00df dr\u00fcckt' auf die Stirne,", "tokens": ["Dem", "A\u00b7pol\u00b7lo", "sel\u00b7ber", "den", "G\u00f6t\u00b7ter\u00b7ku\u00df", "dr\u00fcckt'", "auf", "die", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Dem er das Zauberwort lieh, das ihre Angelen sprengt.", "tokens": ["Dem", "er", "das", "Zau\u00b7ber\u00b7wort", "lieh", ",", "das", "ih\u00b7re", "An\u00b7ge\u00b7len", "sprengt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Trotz er\u00f6ffnet sie nicht, nicht j\u00e4mmerlich Klagen, nicht Sinon's", "tokens": ["Trotz", "er\u00b7\u00f6ff\u00b7net", "sie", "nicht", ",", "nicht", "j\u00e4m\u00b7mer\u00b7lich", "Kla\u00b7gen", ",", "nicht", "Si\u00b7non's"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "VVFIN", "PPER", "PTKNEG", "$,", "PTKNEG", "ADJD", "NN", "$,", "PTKNEG", "NN"], "meter": "+-+--+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Schwarzgeknoteter Trug, noch odyss\u00e4ische List.", "tokens": ["Schwarz\u00b7ge\u00b7kno\u00b7te\u00b7ter", "Trug", ",", "noch", "o\u00b7dys\u00b7s\u00e4i\u00b7sche", "List", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "ADJA", "NN", "$."], "meter": "+-+--+-+++-+", "measure": "trochaic.septa.relaxed"}}}}}