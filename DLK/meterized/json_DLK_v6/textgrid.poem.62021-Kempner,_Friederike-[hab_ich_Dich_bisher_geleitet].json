{"textgrid.poem.62021": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[hab ich Dich bisher geleitet]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hab ich Dich bisher geleitet,", "tokens": ["Hab", "ich", "Dich", "bis\u00b7her", "ge\u00b7lei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wanke nicht an meiner Hand,", "tokens": ["Wan\u00b7ke", "nicht", "an", "mei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh, der Teppich ist gebreitet,", "tokens": ["Sieh", ",", "der", "Tep\u00b7pich", "ist", "ge\u00b7brei\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es gr\u00fc\u00dft das Uferland.", "tokens": ["Und", "es", "gr\u00fc\u00dft", "das", "U\u00b7fer\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und es l\u00e4cheln alle Sterne,", "tokens": ["Und", "es", "l\u00e4\u00b7cheln", "al\u00b7le", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die sch\u00f6n're Sonne winkt, \u2013", "tokens": ["Und", "die", "sch\u00f6n'\u00b7re", "Son\u00b7ne", "winkt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In der N\u00e4he, in der Ferne", "tokens": ["In", "der", "N\u00e4\u00b7he", ",", "in", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich der Allmacht Arm umschlingt. \u2013", "tokens": ["Dich", "der", "All\u00b7macht", "Arm", "um\u00b7schlingt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ART", "NN", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}