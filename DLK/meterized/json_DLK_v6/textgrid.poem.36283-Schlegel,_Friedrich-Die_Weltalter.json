{"textgrid.poem.36283": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Weltalter", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Anfangs lebte der Mensch mit sich selber in heiliger Eintracht;", "tokens": ["An\u00b7fangs", "leb\u00b7te", "der", "Mensch", "mit", "sich", "sel\u00b7ber", "in", "hei\u00b7li\u00b7ger", "Ein\u00b7tracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PRF", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+--+--+-+++", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Denn es umwehete noch ihn der Anhauch himmlischen Geistes.", "tokens": ["Denn", "es", "um\u00b7we\u00b7he\u00b7te", "noch", "ihn", "der", "An\u00b7hauch", "himm\u00b7li\u00b7schen", "Geis\u00b7tes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Liebend besangen und dankbar die Kinder den ewigen Vater;", "tokens": ["Lie\u00b7bend", "be\u00b7san\u00b7gen", "und", "dank\u00b7bar", "die", "Kin\u00b7der", "den", "e\u00b7wi\u00b7gen", "Va\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "KON", "ADJD", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Blumen da brachte die Erde, und fromme Gebete die Menschen,", "tokens": ["Blu\u00b7men", "da", "brach\u00b7te", "die", "Er\u00b7de", ",", "und", "from\u00b7me", "Ge\u00b7be\u00b7te", "die", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "ART", "NN", "$,", "KON", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.5": {"text": "Jegliches brachte selber sich dar in Vertrauen und Demut.", "tokens": ["Jeg\u00b7li\u00b7ches", "brach\u00b7te", "sel\u00b7ber", "sich", "dar", "in", "Ver\u00b7trau\u00b7en", "und", "De\u00b7mut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PRF", "PTKVZ", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-+-+--+-", "measure": "iambic.septa.invert"}, "line.6": {"text": "Segen entquoll der heiligen Sonn' auf des Orients Fluren,", "tokens": ["Se\u00b7gen", "ent\u00b7quoll", "der", "hei\u00b7li\u00b7gen", "Sonn'", "auf", "des", "O\u00b7rients", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "ADJA", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+--+--+-+-", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Wo des Erleuchteten freudiger Gru\u00df zuerst sie bewillkommt.", "tokens": ["Wo", "des", "Er\u00b7leuch\u00b7te\u00b7ten", "freu\u00b7di\u00b7ger", "Gru\u00df", "zu\u00b7erst", "sie", "be\u00b7will\u00b7kommt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+--+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "Willig gew\u00e4hrte und folgsam der Boden die heiligen Fr\u00fcchte,", "tokens": ["Wil\u00b7lig", "ge\u00b7w\u00e4hr\u00b7te", "und", "folg\u00b7sam", "der", "Bo\u00b7den", "die", "hei\u00b7li\u00b7gen", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "KON", "ADJD", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.9": {"text": "Und dann teilten das l\u00e4ndliche Mahl mit dem Gotte die Menschen.", "tokens": ["Und", "dann", "teil\u00b7ten", "das", "l\u00e4nd\u00b7li\u00b7che", "Mahl", "mit", "dem", "Got\u00b7te", "die", "Men\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.10": {"text": "Da war golden das Leben, in goldenen Tr\u00e4umen beseligt,", "tokens": ["Da", "war", "gol\u00b7den", "das", "Le\u00b7ben", ",", "in", "gol\u00b7de\u00b7nen", "Tr\u00e4u\u00b7men", "be\u00b7se\u00b7ligt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "ART", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.11": {"text": "Schwebte sinnend das kindliche Herz, und gedachte des Glanzes,", "tokens": ["Schweb\u00b7te", "sin\u00b7nend", "das", "kind\u00b7li\u00b7che", "Herz", ",", "und", "ge\u00b7dach\u00b7te", "des", "Glan\u00b7zes", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Wo den K\u00f6nig des Himmels noch hellere Strahlen umscheinen.", "tokens": ["Wo", "den", "K\u00f6\u00b7nig", "des", "Him\u00b7mels", "noch", "hel\u00b7le\u00b7re", "Strah\u00b7len", "um\u00b7schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.13": {"text": "Denn oft stiegen hernieder zum erdegeborenen Menschen,", "tokens": ["Denn", "oft", "stie\u00b7gen", "her\u00b7nie\u00b7der", "zum", "er\u00b7de\u00b7ge\u00b7bo\u00b7re\u00b7nen", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-++--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Lieblich in lieber Gestalt noch, die seligen Kinder des Lichtes;", "tokens": ["Lieb\u00b7lich", "in", "lie\u00b7ber", "Ge\u00b7stalt", "noch", ",", "die", "se\u00b7li\u00b7gen", "Kin\u00b7der", "des", "Lich\u00b7tes", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "ADV", "$,", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.15": {"text": "So auch gingen die Menschen noch, leicht entschlummert zur Heimat,", "tokens": ["So", "auch", "gin\u00b7gen", "die", "Men\u00b7schen", "noch", ",", "leicht", "ent\u00b7schlum\u00b7mert", "zur", "Hei\u00b7mat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "ADV", "$,", "ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.16": {"text": "Blieben dort immer vereinigt wie gern wohl im heitersten Frieden,", "tokens": ["Blie\u00b7ben", "dort", "im\u00b7mer", "ver\u00b7ei\u00b7nigt", "wie", "gern", "wohl", "im", "hei\u00b7ters\u00b7ten", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "VVPP", "KOKOM", "ADV", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.17": {"text": "Kehrten doch willig verwandelt auch wieder zum Garten der Unschuld,", "tokens": ["Kehr\u00b7ten", "doch", "wil\u00b7lig", "ver\u00b7wan\u00b7delt", "auch", "wie\u00b7der", "zum", "Gar\u00b7ten", "der", "Un\u00b7schuld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "VVFIN", "ADV", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Wo die gr\u00fcnende Erde so sch\u00f6n dem Geliebten sich schm\u00fcckte.", "tokens": ["Wo", "die", "gr\u00fc\u00b7nen\u00b7de", "Er\u00b7de", "so", "sch\u00f6n", "dem", "Ge\u00b7lieb\u00b7ten", "sich", "schm\u00fcck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADV", "ADJD", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.19": {"text": "Aufw\u00e4rts stieg die Flamme des Lebens aus sehnenden Herzen,", "tokens": ["Auf\u00b7w\u00e4rts", "stieg", "die", "Flam\u00b7me", "des", "Le\u00b7bens", "aus", "seh\u00b7nen\u00b7den", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.20": {"text": "Welche der Treue Geheimnis, noch G\u00f6ttliches deutend, vereinigt;", "tokens": ["Wel\u00b7che", "der", "Treu\u00b7e", "Ge\u00b7heim\u00b7nis", ",", "noch", "G\u00f6tt\u00b7li\u00b7ches", "deu\u00b7tend", ",", "ver\u00b7ei\u00b7nigt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ART", "NN", "NN", "$,", "ADV", "NN", "VVPP", "$,", "VVPP", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.21": {"text": "Selber der Tod verband ja nur inniger noch die Umarmten.", "tokens": ["Sel\u00b7ber", "der", "Tod", "ver\u00b7band", "ja", "nur", "in\u00b7ni\u00b7ger", "noch", "die", "Um\u00b7arm\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "ADV", "ADV", "ADJD", "ADV", "ART", "NN", "$."], "meter": "+--+-+-+-+-++-+-", "measure": "iambic.octa.plus.invert"}, "line.22": {"text": "Ganz in Liebe verschlungen verwebten sich alle Gedanken;", "tokens": ["Ganz", "in", "Lie\u00b7be", "ver\u00b7schlun\u00b7gen", "ver\u00b7web\u00b7ten", "sich", "al\u00b7le", "Ge\u00b7dan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "VVFIN", "PRF", "PIAT", "NN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.23": {"text": "Auch noch waren die Zeiten vereint des Jahrs und des Tages.", "tokens": ["Auch", "noch", "wa\u00b7ren", "die", "Zei\u00b7ten", "ver\u00b7eint", "des", "Jahrs", "und", "des", "Ta\u00b7ges", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.24": {"text": "Abendk\u00fchlung umhauchte die Stirn, und es leuchtete Morgen,", "tokens": ["A\u00b7bend\u00b7k\u00fch\u00b7lung", "um\u00b7hauch\u00b7te", "die", "Stirn", ",", "und", "es", "leuch\u00b7te\u00b7te", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.25": {"text": "Gl\u00fchender Hoffnung Rose gesellt mit der Blume des Friedens;", "tokens": ["Gl\u00fc\u00b7hen\u00b7der", "Hoff\u00b7nung", "Ro\u00b7se", "ge\u00b7sellt", "mit", "der", "Blu\u00b7me", "des", "Frie\u00b7dens", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "VVPP", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.26": {"text": "Ja, auch des Tages strahlende Kraft und der dunkelen F\u00fclle", "tokens": ["Ja", ",", "auch", "des", "Ta\u00b7ges", "strah\u00b7len\u00b7de", "Kraft", "und", "der", "dun\u00b7ke\u00b7len", "F\u00fcl\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "ART", "NN", "ADJA", "NN", "KON", "ART", "ADJA", "NN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.27": {"text": "N\u00e4chtliches Segens Geheimnis, sie waren noch herrlich verbunden.", "tokens": ["N\u00e4cht\u00b7li\u00b7ches", "Se\u00b7gens", "Ge\u00b7heim\u00b7nis", ",", "sie", "wa\u00b7ren", "noch", "herr\u00b7lich", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.28": {"text": "Ewiger Fr\u00fchling umschlang den Blumeng\u00fcrtel der Erde,", "tokens": ["E\u00b7wi\u00b7ger", "Fr\u00fch\u00b7ling", "um\u00b7schlang", "den", "Blu\u00b7men\u00b7g\u00fcr\u00b7tel", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.29": {"text": "Und es alterten niemals der seligen Menschen Geschlechter.", "tokens": ["Und", "es", "al\u00b7ter\u00b7ten", "nie\u00b7mals", "der", "se\u00b7li\u00b7gen", "Men\u00b7schen", "Ge\u00b7schlech\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}}}}}