{"dta.poem.351": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der Entwurff einer Sch\u00f6nen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "An Sch\u00f6nheit kanst du wol der holden Venus gleichen/", "tokens": ["An", "Sch\u00f6n\u00b7heit", "kanst", "du", "wol", "der", "hol\u00b7den", "Ve\u00b7nus", "glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dein Verstand darf nicht der klugen ", "tokens": ["Und", "dein", "Ver\u00b7stand", "darf", "nicht", "der", "klu\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PTKNEG", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "weichen/", "tokens": ["wei\u00b7chen", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Junonens Augen sind vereinet mit der Pracht", "tokens": ["Ju\u00b7no\u00b7nens", "Au\u00b7gen", "sind", "ver\u00b7ei\u00b7net", "mit", "der", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "VAFIN", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die zweyer Sonnen-Glantz an deinem Stirnen macht.", "tokens": ["Die", "zwey\u00b7er", "Son\u00b7nen\u00b7Glantz", "an", "dei\u00b7nem", "Stir\u00b7nen", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Ja selbst der Himmel wird von deinem Glantz verd\u00fcstert/", "tokens": ["Ja", "selbst", "der", "Him\u00b7mel", "wird", "von", "dei\u00b7nem", "Glantz", "ver\u00b7d\u00fcs\u00b7tert", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dein sch\u00f6nes Wesen l\u00e4st nur Lieblichkeiten sehn/", "tokens": ["Dein", "sch\u00f6\u00b7nes", "We\u00b7sen", "l\u00e4st", "nur", "Lieb\u00b7lich\u00b7kei\u00b7ten", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und machest/ da\u00df du wirst noch tausend mahl so sch\u00f6n.", "tokens": ["Und", "ma\u00b7chest", "/", "da\u00df", "du", "wirst", "noch", "tau\u00b7send", "mahl", "so", "sch\u00f6n", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PPER", "VAFIN", "ADV", "CARD", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}