{"textgrid.poem.25931": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Der Liebsten Mund ist's Reiseziel", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein funkelnd B\u00e4chlein schiebt durchs Tal,", "tokens": ["Ein", "fun\u00b7kelnd", "B\u00e4ch\u00b7lein", "schiebt", "durchs", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und leise Melodie es gibt,", "tokens": ["Und", "lei\u00b7se", "Me\u00b7lo\u00b7die", "es", "gibt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir scheint, da\u00df es das Wandern liebt.", "tokens": ["Mir", "scheint", ",", "da\u00df", "es", "das", "Wan\u00b7dern", "liebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Tanne aufgepflanzt dransteht,", "tokens": ["Die", "Tan\u00b7ne", "auf\u00b7ge\u00b7pflanzt", "drans\u00b7teht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Wipfel gern im Himmel tanzt,", "tokens": ["Ihr", "Wip\u00b7fel", "gern", "im", "Him\u00b7mel", "tanzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Leben nicht vom Flecke geht.", "tokens": ["Ihr", "Le\u00b7ben", "nicht", "vom", "Fle\u00b7cke", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Gern wie der Bach ich wandern will,", "tokens": ["Gern", "wie", "der", "Bach", "ich", "wan\u00b7dern", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "PPER", "VVFIN", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Liebsten Mund ist's Reiseziel,", "tokens": ["Der", "Liebs\u00b7ten", "Mund", "ist's", "Rei\u00b7se\u00b7ziel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort steh' ich wie die Tanne still.", "tokens": ["Dort", "steh'", "ich", "wie", "die", "Tan\u00b7ne", "still", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}