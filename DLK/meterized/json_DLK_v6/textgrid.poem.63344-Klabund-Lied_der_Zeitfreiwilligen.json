{"textgrid.poem.63344": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Lied der Zeitfreiwilligen", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin ein Zeitfreiwilliger,", "tokens": ["Ich", "bin", "ein", "Zeit\u00b7frei\u00b7wil\u00b7li\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und stehle dem lieben Gott die Zeit.", "tokens": ["Und", "steh\u00b7le", "dem", "lie\u00b7ben", "Gott", "die", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es lebt sich billiger,", "tokens": ["Es", "lebt", "sich", "bil\u00b7li\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wenn man: Nieder mit den verfluchten Spartakisten schreit.", "tokens": ["Wenn", "man", ":", "Nie\u00b7der", "mit", "den", "ver\u00b7fluch\u00b7ten", "Spar\u00b7ta\u00b7kis\u00b7ten", "schreit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$.", "ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Fuffzehn M\u00e4rker den Tag. Daneben", "tokens": ["Fuff\u00b7zehn", "M\u00e4r\u00b7ker", "den", "Tag", ".", "Da\u00b7ne\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["CARD", "NN", "ART", "NN", "$.", "NN"], "meter": "+-+--+---", "measure": "trochaic.tri.relaxed"}, "line.6": {"text": "Allens frei.", "tokens": ["Al\u00b7lens", "frei", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Es ist ein herrliches Leben.", "tokens": ["Es", "ist", "ein", "herr\u00b7li\u00b7ches", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Juchhei.", "tokens": ["Juch\u00b7hei", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Ich verdiente mir meine Sporen", "tokens": ["Ich", "ver\u00b7dien\u00b7te", "mir", "mei\u00b7ne", "Spo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Bei Kapp.", "tokens": ["Bei", "Kapp", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Als dessen Sache verloren,", "tokens": ["Als", "des\u00b7sen", "Sa\u00b7che", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zog ich ab.", "tokens": ["Zog", "ich", "ab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Ich geh\u00f6re wieder zu den Regierungstreu'n", "tokens": ["Ich", "ge\u00b7h\u00f6\u00b7re", "wie\u00b7der", "zu", "den", "Re\u00b7gie\u00b7rungs\u00b7treu'n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Und habe den Schutz der Verfassung erkoren.", "tokens": ["Und", "ha\u00b7be", "den", "Schutz", "der", "Ver\u00b7fas\u00b7sung", "er\u00b7ko\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Ich breche alle Eide von acht bis neun,", "tokens": ["Ich", "bre\u00b7che", "al\u00b7le", "Ei\u00b7de", "von", "acht", "bis", "neun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "CARD", "APPR", "CARD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Die ich von sieben bis acht geschworen.", "tokens": ["Die", "ich", "von", "sie\u00b7ben", "bis", "acht", "ge\u00b7schwo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "CARD", "APPR", "CARD", "VVPP", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Neulich bei Mechterst\u00e4dt: Pst...", "tokens": ["Neu\u00b7lich", "bei", "Mech\u00b7ter\u00b7st\u00e4dt", ":", "Pst", "..."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "NE", "$.", "NN", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Zeigten wir's den Arbeiterlaffen.", "tokens": ["Zeig\u00b7ten", "wir's", "den", "Ar\u00b7bei\u00b7ter\u00b7laf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Falls es irgendwo ruhig ist,", "tokens": ["Falls", "es", "ir\u00b7gend\u00b7wo", "ru\u00b7hig", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Mu\u00df man eben k\u00fcnstlich Unruhe schaffen.", "tokens": ["Mu\u00df", "man", "e\u00b7ben", "k\u00fcnst\u00b7lich", "Un\u00b7ru\u00b7he", "schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "ADJD", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "La\u00dft die Maschinengewehre streichen!", "tokens": ["La\u00dft", "die", "Ma\u00b7schi\u00b7nen\u00b7ge\u00b7weh\u00b7re", "strei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Ins Kabuff.", "tokens": ["Ins", "Ka\u00b7buff", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Immer feste druff.", "tokens": ["Im\u00b7mer", "fes\u00b7te", "druff", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "PAV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Unsre Anatomie braucht Leichen.", "tokens": ["Uns\u00b7re", "A\u00b7nat\u00b7o\u00b7mie", "braucht", "Lei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}}}}