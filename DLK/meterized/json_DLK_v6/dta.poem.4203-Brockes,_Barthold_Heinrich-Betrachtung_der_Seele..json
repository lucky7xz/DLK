{"dta.poem.4203": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung der Seele.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Wunderwerk, das nimmer gnug zu achten, zu bewun-", "tokens": ["Ein", "Wun\u00b7der\u00b7werk", ",", "das", "nim\u00b7mer", "gnug", "zu", "ach\u00b7ten", ",", "zu", "be\u00b7wun"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADV", "PTKZU", "VVINF", "$,", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "dern ist,", "tokens": ["dern", "ist", ","], "token_info": ["word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Und worinn man des Sch\u00f6pfers Weisheit am \u00fcberschweng-", "tokens": ["Und", "wo\u00b7rinn", "man", "des", "Sch\u00f6p\u00b7fers", "Weis\u00b7heit", "am", "\u00fc\u00b7ber\u00b7schweng"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PIS", "ART", "NN", "NN", "APPRART", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "lichsten ermi\u00dft,", "tokens": ["lichs\u00b7ten", "er\u00b7mi\u00dft", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ist unsers Leibes und der Seele nie zu erforschender Ver-", "tokens": ["Ist", "un\u00b7sers", "Lei\u00b7bes", "und", "der", "See\u00b7le", "nie", "zu", "er\u00b7for\u00b7schen\u00b7der", "Ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "ART", "NN", "ADV", "APPR", "ADJA", "TRUNC"], "meter": "+--+-+-+-+--+--+", "measure": "iambic.septa.invert"}, "line.6": {"text": "band;", "tokens": ["band", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Je mehr man ihm sucht nachzuforschen, je mehr bleibt er uns", "tokens": ["Je", "mehr", "man", "ihm", "sucht", "nach\u00b7zu\u00b7for\u00b7schen", ",", "je", "mehr", "bleibt", "er", "uns"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIS", "PPER", "VVFIN", "VVIZU", "$,", "ADV", "ADV", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "unbekannt.", "tokens": ["un\u00b7be\u00b7kannt", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Ein jeder Theil, f\u00fcr sich betrachtet, hat nichts von allen", "tokens": ["Ein", "je\u00b7der", "Theil", ",", "f\u00fcr", "sich", "be\u00b7trach\u00b7tet", ",", "hat", "nichts", "von", "al\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "$,", "APPR", "PRF", "VVPP", "$,", "VAFIN", "PIS", "APPR", "PIAT"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Eigenschaften,", "tokens": ["Ei\u00b7gen\u00b7schaf\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Von keiner Wirkung, keiner Handlung, die an dem andern", "tokens": ["Von", "kei\u00b7ner", "Wir\u00b7kung", ",", "kei\u00b7ner", "Hand\u00b7lung", ",", "die", "an", "dem", "an\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PIAT", "NN", "$,", "PRELS", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Theile haften,", "tokens": ["Thei\u00b7le", "haf\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Und doch, da sie vereinet sind, wird, durch ein\u2019 unbekannte", "tokens": ["Und", "doch", ",", "da", "sie", "ver\u00b7ei\u00b7net", "sind", ",", "wird", ",", "durch", "ein'", "un\u00b7be\u00b7kann\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$,", "VAFIN", "$,", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Macht,", "tokens": ["Macht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Die Wirkung, welche jeder fehlte, verwunderlich hervor-", "tokens": ["Die", "Wir\u00b7kung", ",", "wel\u00b7che", "je\u00b7der", "fehl\u00b7te", ",", "ver\u00b7wun\u00b7der\u00b7lich", "her\u00b7vor"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,", "ADJD", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "gebracht.", "tokens": ["ge\u00b7bracht", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.10": {"text": "bereitet,", "tokens": ["be\u00b7rei\u00b7tet", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Und dennoch reget er sich nicht, wenn ihn die Seele nicht", "tokens": ["Und", "den\u00b7noch", "re\u00b7get", "er", "sich", "nicht", ",", "wenn", "ihn", "die", "See\u00b7le", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "PTKNEG", "$,", "KOUS", "PPER", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.12": {"text": "begleitet,", "tokens": ["be\u00b7glei\u00b7tet", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Die sich doch nicht bewegen kann.", "tokens": ["Die", "sich", "doch", "nicht", "be\u00b7we\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.15": {"text": "als Pein,", "tokens": ["als", "Pein", ","], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.16": {"text": "Die, sonder unsers C\u00f6rpers Beytrag, doch nicht von ihr", "tokens": ["Die", ",", "son\u00b7der", "un\u00b7sers", "C\u00f6r\u00b7pers", "Bey\u00b7trag", ",", "doch", "nicht", "von", "ihr"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "KON", "PPOSAT", "NN", "NN", "$,", "ADV", "PTKNEG", "APPR", "PPOSAT"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "empfunden seyn,", "tokens": ["emp\u00b7fun\u00b7den", "seyn", ","], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "Und doch besitzet sie kein C\u00f6rper. Die Seele sieht den C\u00f6rper", "tokens": ["Und", "doch", "be\u00b7sit\u00b7zet", "sie", "kein", "C\u00f6r\u00b7per", ".", "Die", "See\u00b7le", "sieht", "den", "C\u00f6r\u00b7per"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$.", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.19": {"text": "an,", "tokens": ["an", ","], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$,"], "meter": "+", "measure": "single.up"}}}}}