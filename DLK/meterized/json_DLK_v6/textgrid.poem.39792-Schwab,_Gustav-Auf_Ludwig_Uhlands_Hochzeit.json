{"textgrid.poem.39792": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Auf Ludwig Uhlands Hochzeit", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl dem, der das errungen,", "tokens": ["Wohl", "dem", ",", "der", "das", "er\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was unser Freund errang,", "tokens": ["Was", "un\u00b7ser", "Freund", "er\u00b7rang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem, wie ihm viel gelungen,", "tokens": ["Dem", ",", "wie", "ihm", "viel", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Leben auch gelang.", "tokens": ["Das", "Le\u00b7ben", "auch", "ge\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zum Kranz der B\u00fcrgertugend,", "tokens": ["Zum", "Kranz", "der", "B\u00fcr\u00b7ger\u00b7tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den ihm das Volk verlieh,", "tokens": ["Den", "ihm", "das", "Volk", "ver\u00b7lieh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zum Kranz der ew'gen Jugend,", "tokens": ["Zum", "Kranz", "der", "ew'\u00b7gen", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der s\u00fc\u00dfen Poesie,", "tokens": ["Der", "s\u00fc\u00b7\u00dfen", "Poe\u00b7sie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Reicht Sie den Kranz der Liebe,", "tokens": ["Reicht", "Sie", "den", "Kranz", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Stolz der treuen Brust,", "tokens": ["Im", "Stolz", "der", "treu\u00b7en", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die mit dem sch\u00f6nsten Triebe", "tokens": ["Die", "mit", "dem", "sch\u00f6ns\u00b7ten", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu schalten frei gewu\u00dft.", "tokens": ["Zu", "schal\u00b7ten", "frei", "ge\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Jetzt wird aus Ihrem Bilde", "tokens": ["Jetzt", "wird", "aus", "Ih\u00b7rem", "Bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Dichtkunst Born gen\u00e4hrt;", "tokens": ["Der", "Dicht\u00b7kunst", "Born", "ge\u00b7n\u00e4hrt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt wird von Lieb' und Milde", "tokens": ["Jetzt", "wird", "von", "Lieb'", "und", "Mil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das strenge Recht verkl\u00e4rt.", "tokens": ["Das", "stren\u00b7ge", "Recht", "ver\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Drum Heil dir, Paar! am Ziele,", "tokens": ["Drum", "Heil", "dir", ",", "Paar", "!", "am", "Zie\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "NN", "PPER", "$,", "NN", "$.", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Heil in dein neues Haus!", "tokens": ["Heil", "in", "dein", "neu\u00b7es", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ich wei\u00df, es sprechen Viele", "tokens": ["Ich", "wei\u00df", ",", "es", "spre\u00b7chen", "Vie\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit mir den Segen aus.", "tokens": ["Mit", "mir", "den", "Se\u00b7gen", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Viel Freunde sprechen's, echte,", "tokens": ["Viel", "Freun\u00b7de", "spre\u00b7chen's", ",", "ech\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die eure Wonne labt,", "tokens": ["Die", "eu\u00b7re", "Won\u00b7ne", "labt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es sprechen's die Geschlechte,", "tokens": ["Es", "spre\u00b7chen's", "die", "Ge\u00b7schlech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die ihr verbunden habt.", "tokens": ["Die", "ihr", "ver\u00b7bun\u00b7den", "habt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und wem des Mannes Ringen", "tokens": ["Und", "wem", "des", "Man\u00b7nes", "Rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den vaterl\u00e4nd'schen Mut", "tokens": ["Den", "va\u00b7ter\u00b7l\u00e4n\u00b7d'\u00b7schen", "Mut"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Gest\u00e4hlt; und wem sein Singen", "tokens": ["Ge\u00b7st\u00e4hlt", ";", "und", "wem", "sein", "Sin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$.", "KON", "PWS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Herzen trieb das Blut:", "tokens": ["Zum", "Her\u00b7zen", "trieb", "das", "Blut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Heil rufen sie dem Bunde,", "tokens": ["Heil", "ru\u00b7fen", "sie", "dem", "Bun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie jubeln alle drob:", "tokens": ["Sie", "ju\u00b7beln", "al\u00b7le", "drob", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drum g\u00f6nnt auch meinem Munde", "tokens": ["Drum", "g\u00f6nnt", "auch", "mei\u00b7nem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das ungest\u00fcme Lob.", "tokens": ["Das", "un\u00b7ge\u00b7st\u00fc\u00b7me", "Lob", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Die Liebe mag verdunkeln", "tokens": ["Die", "Lie\u00b7be", "mag", "ver\u00b7dun\u00b7keln"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Jedweden andern Stern,", "tokens": ["Jed\u00b7we\u00b7den", "an\u00b7dern", "Stern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch ihr zur Seite funkeln", "tokens": ["Doch", "ihr", "zur", "Sei\u00b7te", "fun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "L\u00e4\u00dft sie die Freundschaft gern. \u2013", "tokens": ["L\u00e4\u00dft", "sie", "die", "Freund\u00b7schaft", "gern", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}