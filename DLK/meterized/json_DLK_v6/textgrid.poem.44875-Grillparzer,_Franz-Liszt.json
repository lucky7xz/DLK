{"textgrid.poem.44875": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Liszt", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch st\u00fcrmt der Beifall, des Entz\u00fcckens Flug,", "tokens": ["Noch", "st\u00fcrmt", "der", "Bei\u00b7fall", ",", "des", "Ent\u00b7z\u00fc\u00b7ckens", "Flug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch l\u00e4\u00dft das Ma\u00df sich kaum noch mehr vergr\u00f6\u00dfern,", "tokens": ["Doch", "l\u00e4\u00dft", "das", "Ma\u00df", "sich", "kaum", "noch", "mehr", "ver\u00b7gr\u00f6\u00b7\u00dfern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PRF", "ADV", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drum seis, o Herr, der Trefflichkeit genug.", "tokens": ["Drum", "seis", ",", "o", "Herr", ",", "der", "Treff\u00b7lich\u00b7keit", "ge\u00b7nug", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "$,", "FM", "NN", "$,", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wir danken dir! doch send uns keinen Bessern.", "tokens": ["Wir", "dan\u00b7ken", "dir", "!", "doch", "send", "uns", "kei\u00b7nen", "Bes\u00b7sern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "ADV", "ADJD", "PPER", "PIAT", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}}}}