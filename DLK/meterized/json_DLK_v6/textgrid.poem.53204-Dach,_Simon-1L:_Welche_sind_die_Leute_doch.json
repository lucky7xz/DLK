{"textgrid.poem.53204": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Welche sind die Leute doch", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welche sind die Leute doch", "tokens": ["Wel\u00b7che", "sind", "die", "Leu\u00b7te", "doch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die aus blossem Kleinmuht sagen,", "tokens": ["Die", "aus", "blos\u00b7sem", "Klein\u00b7muht", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df gef\u00e4hrlich sey das Joch", "tokens": ["Da\u00df", "ge\u00b7f\u00e4hr\u00b7lich", "sey", "das", "Joch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00fcsser Heyraht jetzund tragen,", "tokens": ["S\u00fcs\u00b7ser", "Hey\u00b7raht", "je\u00b7tzund", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun sich der gesampte Nord", "tokens": ["Nun", "sich", "der", "ge\u00b7samp\u00b7te", "Nord"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PRF", "ART", "ADJA", "NN"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.2": {"text": "Grimmig wider uns emp\u00f6ret,", "tokens": ["Grim\u00b7mig", "wi\u00b7der", "uns", "em\u00b7p\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd man Waffen, Raub und Mord", "tokens": ["Vnd", "man", "Waf\u00b7fen", ",", "Raub", "und", "Mord"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nah umb unsre Grentzen h\u00f6ret?", "tokens": ["Nah", "umb", "uns\u00b7re", "Grent\u00b7zen", "h\u00f6\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Vnd was geben sie doch vor?", "tokens": ["Vnd", "was", "ge\u00b7ben", "sie", "doch", "vor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ja, man mu\u00df selbst ander fliehen", "tokens": ["Ja", ",", "man", "mu\u00df", "selbst", "an\u00b7der", "flie\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PIS", "VMFIN", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd zu schantzen vor das Thor", "tokens": ["Vnd", "zu", "schant\u00b7zen", "vor", "das", "Thor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKZU", "VVINF", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nebenst andrer Vnpflicht ziehen.", "tokens": ["Ne\u00b7benst", "an\u00b7drer", "Vnpflicht", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Die hergegen einsam sind", "tokens": ["Die", "her\u00b7ge\u00b7gen", "ein\u00b7sam", "sind"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "VAFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Haben hief\u00fcr nicht zu sorgen,", "tokens": ["Ha\u00b7ben", "hie\u00b7f\u00fcr", "nicht", "zu", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch wie es umb Weib und Kind", "tokens": ["Noch", "wie", "es", "umb", "Weib", "und", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Werde stehn heut oder morgen.", "tokens": ["Wer\u00b7de", "stehn", "heut", "o\u00b7der", "mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "ADV", "KON", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aber die bedencken nicht,", "tokens": ["A\u00b7ber", "die", "be\u00b7den\u00b7cken", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gottherrsch' auch mitin den Kriegen,", "tokens": ["Gott\u00b7herr\u00b7sch'", "auch", "mi\u00b7tin", "den", "Krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Noch daran was David spricht:", "tokens": ["Noch", "da\u00b7ran", "was", "Da\u00b7vid", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "PWS", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wirff auff Gott hin dein anliegen,", "tokens": ["Wirff", "auff", "Gott", "hin", "dein", "an\u00b7lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADV", "PPOSAT", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Hoff auff ihn zu allerzeit", "tokens": ["Hoff", "auff", "ihn", "zu", "al\u00b7ler\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vnd befiehl ihm deine Sachen,", "tokens": ["Vnd", "be\u00b7fiehl", "ihm", "dei\u00b7ne", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Er wird auch im wilden Streit", "tokens": ["Er", "wird", "auch", "im", "wil\u00b7den", "Streit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Alles wol und l\u00f6blich machen.", "tokens": ["Al\u00b7les", "wol", "und", "l\u00f6b\u00b7lich", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "KON", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wessen ist das Heyraht-Band?", "tokens": ["Wes\u00b7sen", "ist", "das", "Hey\u00b7raht\u00b7Band", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vnd von wem ist es entsprossen?", "tokens": ["Vnd", "von", "wem", "ist", "es", "ent\u00b7spros\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PWS", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "R\u00fchrt sie her von Menschen Hand?", "tokens": ["R\u00fchrt", "sie", "her", "von", "Men\u00b7schen", "Hand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nein, Gott selbst hat sie geschlossen.", "tokens": ["Nein", ",", "Gott", "selbst", "hat", "sie", "ge\u00b7schlos\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.8": {"line.1": {"text": "Stell' ich sein Werck billig ein,", "tokens": ["Stell'", "ich", "sein", "Werck", "bil\u00b7lig", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Darumb weil die Waffen walten?", "tokens": ["Da\u00b7rumb", "weil", "die", "Waf\u00b7fen", "wal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Kan Gott \u00fcber dem, was seyn,", "tokens": ["Kan", "Gott", "\u00fc\u00b7ber", "dem", ",", "was", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "NN", "APPR", "ART", "$,", "PRELS", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch nicht in dem Kriege halten?", "tokens": ["Auch", "nicht", "in", "dem", "Krie\u00b7ge", "hal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Die\u00df ist Kleinmuth, sag ich frey,", "tokens": ["Die\u00df", "ist", "Klein\u00b7muth", ",", "sag", "ich", "frey", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$,", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vnd dem Glauben gantz entgegen,", "tokens": ["Vnd", "dem", "Glau\u00b7ben", "gantz", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihr, Herr Br\u00e4utgam, fallt mir bey,", "tokens": ["Ihr", ",", "Herr", "Br\u00e4ut\u00b7gam", ",", "fallt", "mir", "bey", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "NE", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Denn ihr lasst euch nichts bewegen,", "tokens": ["Denn", "ihr", "lasst", "euch", "nichts", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Nicht der Waffen schwere Last,", "tokens": ["Nicht", "der", "Waf\u00b7fen", "schwe\u00b7re", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die in Pohlen sich l\u00e4sst schawen,", "tokens": ["Die", "in", "Poh\u00b7len", "sich", "l\u00e4sst", "scha\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "PRF", "VVFIN", "VVINF", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Nicht der Leute Flucht, die fast", "tokens": ["Nicht", "der", "Leu\u00b7te", "Flucht", ",", "die", "fast"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "NN", "$,", "PRELS", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00fcndlich kommen aus Littawen,", "tokens": ["St\u00fcnd\u00b7lich", "kom\u00b7men", "aus", "Lit\u00b7ta\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.11": {"line.1": {"text": "Noch den Vorwitz der ohn Ziel", "tokens": ["Noch", "den", "Vor\u00b7witz", "der", "ohn", "Ziel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich an ewre Liebe machet", "tokens": ["Sich", "an", "ew\u00b7re", "Lie\u00b7be", "ma\u00b7chet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd, als h\u00fclffe die\u00df ihn viel,", "tokens": ["Vnd", ",", "als", "h\u00fclf\u00b7fe", "die\u00df", "ihn", "viel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "VVFIN", "PDS", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie beredet, sie belachet.", "tokens": ["Sie", "be\u00b7re\u00b7det", ",", "sie", "be\u00b7la\u00b7chet", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Welcher Heyrath-Schlu\u00df wird nicht", "tokens": ["Wel\u00b7cher", "Hey\u00b7ra\u00b7th\u00b7Schlu\u00df", "wird", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAT", "NN", "VAFIN", "PTKNEG"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "In des H\u00f6chsten Raht gestiftet?", "tokens": ["In", "des", "H\u00f6chs\u00b7ten", "Raht", "ge\u00b7stif\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist es recht, da\u00df das Gericht", "tokens": ["Ist", "es", "recht", ",", "da\u00df", "das", "Ge\u00b7richt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vnter Menschen sich vergifftet?", "tokens": ["Vn\u00b7ter", "Men\u00b7schen", "sich", "ver\u00b7giff\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Wolte Gott, da\u00df jederman", "tokens": ["Wol\u00b7te", "Gott", ",", "da\u00df", "je\u00b7der\u00b7man"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VMFIN", "NN", "$,", "KOUS", "PIS"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Jetzt beredte seine S\u00fcnden,", "tokens": ["Jetzt", "be\u00b7red\u00b7te", "sei\u00b7ne", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die er wider Gott gethan,", "tokens": ["Die", "er", "wi\u00b7der", "Gott", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gnug w\u00fcrd' er zu straffen finden.", "tokens": ["Gnug", "w\u00fcrd'", "er", "zu", "straf\u00b7fen", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKZU", "VVINF", "VVINF", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}}, "stanza.14": {"line.1": {"text": "Diese Nachred w\u00fcrde Gott,", "tokens": ["Die\u00b7se", "Nach\u00b7red", "w\u00fcr\u00b7de", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hofft' ich, alsobald bewegen,", "tokens": ["Hofft'", "ich", ",", "al\u00b7so\u00b7bald", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df er diese Krieges-Noht", "tokens": ["Da\u00df", "er", "die\u00b7se", "Krie\u00b7ge\u00b7sNoht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eilends w\u00fcrde von uns legen.", "tokens": ["Ei\u00b7lends", "w\u00fcr\u00b7de", "von", "uns", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Kehrt euch nichts, als wie ihr thut,", "tokens": ["Kehrt", "euch", "nichts", ",", "als", "wie", "ihr", "thut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIS", "$,", "KOUS", "KOKOM", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "An das Volck, nichts an die Waffen,", "tokens": ["An", "das", "Volck", ",", "nichts", "an", "die", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihr sitzt fest in Gottes Hut,", "tokens": ["Ihr", "sitzt", "fest", "in", "Got\u00b7tes", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der weis alles wol zu schaffen.", "tokens": ["Der", "weis", "al\u00b7les", "wol", "zu", "schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKVZ", "PIS", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Ihr seyd frembd, drumb habet ihr", "tokens": ["Ihr", "seyd", "frembd", ",", "drumb", "ha\u00b7bet", "ihr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "PAV", "VAFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch der frembden war genommen,", "tokens": ["Auch", "der", "fremb\u00b7den", "war", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die mit aller Tugend Zier", "tokens": ["Die", "mit", "al\u00b7ler", "Tu\u00b7gend", "Zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PIAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun in ewer Hau\u00df mu\u00df kommen.", "tokens": ["Nun", "in", "e\u00b7wer", "Hau\u00df", "mu\u00df", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Seyd einander nur bekant,", "tokens": ["Seyd", "ein\u00b7an\u00b7der", "nur", "be\u00b7kant", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PRF", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lebt in hertzlichem Vertrawen,", "tokens": ["Lebt", "in", "hertz\u00b7li\u00b7chem", "Ver\u00b7tra\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie euch kn\u00fcpfft der Liebe Band,", "tokens": ["Wie", "euch", "kn\u00fcpfft", "der", "Lie\u00b7be", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr sollt allen Segen schawen.", "tokens": ["Ihr", "sollt", "al\u00b7len", "Se\u00b7gen", "scha\u00b7wen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}