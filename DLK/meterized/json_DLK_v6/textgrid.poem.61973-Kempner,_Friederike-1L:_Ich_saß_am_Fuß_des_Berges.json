{"textgrid.poem.61973": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich sa\u00df am Fu\u00df des Berges", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sa\u00df am Fu\u00df des Berges", "tokens": ["Ich", "sa\u00df", "am", "Fu\u00df", "des", "Ber\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und tr\u00e4umte mancherlei,", "tokens": ["Und", "tr\u00e4um\u00b7te", "man\u00b7cher\u00b7lei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die kleine, frische Elster,", "tokens": ["Die", "klei\u00b7ne", ",", "fri\u00b7sche", "Els\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie pl\u00e4tscherte vorbei.", "tokens": ["Sie", "pl\u00e4t\u00b7scher\u00b7te", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was hast Du zu erz\u00e4hlen,", "tokens": ["Was", "hast", "Du", "zu", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du schmales Silberband,", "tokens": ["Du", "schma\u00b7les", "Sil\u00b7ber\u00b7band", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was wir nicht schon geh\u00f6ret,", "tokens": ["Was", "wir", "nicht", "schon", "ge\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was uns nicht schon bekannt?", "tokens": ["Was", "uns", "nicht", "schon", "be\u00b7kannt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was Dir bekannt geworden", "tokens": ["Was", "Dir", "be\u00b7kannt", "ge\u00b7wor\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADJD", "VAPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch der Geschichte Mund,", "tokens": ["Durch", "der", "Ge\u00b7schich\u00b7te", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Das waren bunte Muscheln,", "tokens": ["Das", "wa\u00b7ren", "bun\u00b7te", "Mu\u00b7scheln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Doch Perlen beut mein Grund.", "tokens": ["Doch", "Per\u00b7len", "beut", "mein", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich sa\u00df am Fu\u00df des Berges,", "tokens": ["Ich", "sa\u00df", "am", "Fu\u00df", "des", "Ber\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und tr\u00e4umte mancherlei,", "tokens": ["Und", "tr\u00e4um\u00b7te", "man\u00b7cher\u00b7lei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die kleine, frische Elster,", "tokens": ["Die", "klei\u00b7ne", ",", "fri\u00b7sche", "Els\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie pl\u00e4tscherte vorbei.", "tokens": ["Sie", "pl\u00e4t\u00b7scher\u00b7te", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Komm her, la\u00df Dir erz\u00e4hlen,", "tokens": ["Komm", "her", ",", "la\u00df", "Dir", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du fremdes Menschenkind,", "tokens": ["Du", "frem\u00b7des", "Men\u00b7schen\u00b7kind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Einstmalen Ro\u00df und Reiter", "tokens": ["Einst\u00b7ma\u00b7len", "Ro\u00df", "und", "Rei\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In mir versunken sind.", "tokens": ["In", "mir", "ver\u00b7sun\u00b7ken", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Versteh' mich recht, dem Polen", "tokens": ["Ver\u00b7steh'", "mich", "recht", ",", "dem", "Po\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "ADJD", "$,", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Hoffnung g\u00e4nzlich sank;", "tokens": ["Die", "Hoff\u00b7nung", "g\u00e4nz\u00b7lich", "sank", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er st\u00fcrzt sich in die Tiefe, \u2013", "tokens": ["Er", "st\u00fcrzt", "sich", "in", "die", "Tie\u00b7fe", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es hie\u00df: da\u00df er ertrank;", "tokens": ["Es", "hie\u00df", ":", "da\u00df", "er", "er\u00b7trank", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Er lebt in meinen Fluten,", "tokens": ["Er", "lebt", "in", "mei\u00b7nen", "Flu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Singt n\u00e4chtlich einen Sang,", "tokens": ["Singt", "n\u00e4cht\u00b7lich", "ei\u00b7nen", "Sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie ein Gebet so klingt es,", "tokens": ["Wie", "ein", "Ge\u00b7bet", "so", "klingt", "es", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch traurig, weh und bang;", "tokens": ["Doch", "trau\u00b7rig", ",", "weh", "und", "bang", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Er lebt in meinen Fluten", "tokens": ["Er", "lebt", "in", "mei\u00b7nen", "Flu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und weint um's Vaterland,", "tokens": ["Und", "weint", "um's", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Tr\u00e4nen werden zu Perlen,", "tokens": ["Die", "Tr\u00e4\u00b7nen", "wer\u00b7den", "zu", "Per\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Man fischt sie an das Land.", "tokens": ["Man", "fischt", "sie", "an", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}