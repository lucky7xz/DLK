{"dta.poem.3307": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Der Winter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Die Erde dr\u00fcckt ein tiefer Schnee:", "tokens": ["Die", "Er\u00b7de", "dr\u00fcckt", "ein", "tie\u00b7fer", "Schnee", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es gl\u00e4nzt ein blendend Wei\u00df um ihre nackten", "tokens": ["Es", "gl\u00e4nzt", "ein", "blen\u00b7dend", "Wei\u00df", "um", "ih\u00b7re", "nack\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Glieder:", "tokens": ["Glie\u00b7der", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Es gl\u00e4nzen Wald, Gefild und See.", "tokens": ["Es", "gl\u00e4n\u00b7zen", "Wald", ",", "Ge\u00b7fild", "und", "See", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kein muntrer Vogel singt:", "tokens": ["Kein", "mun\u00b7trer", "Vo\u00b7gel", "singt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die tr\u00fcbe Schwermuth schwingt", "tokens": ["Die", "tr\u00fc\u00b7be", "Schwer\u00b7muth", "schwingt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Jhr trauriges Gefieder.", "tokens": ["Ihr", "trau\u00b7ri\u00b7ges", "Ge\u00b7fie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Weise bleibt sich immer gleich:", "tokens": ["Der", "Wei\u00b7se", "bleibt", "sich", "im\u00b7mer", "gleich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er ist in seiner Lust kein Sklave sch\u00f6ner Tage,", "tokens": ["Er", "ist", "in", "sei\u00b7ner", "Lust", "kein", "Skla\u00b7ve", "sch\u00f6\u00b7ner", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und stets an innrer Wollust reich.", "tokens": ["Und", "stets", "an", "inn\u00b7rer", "Wol\u00b7lust", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was Zephyrs Unbestand,", "tokens": ["Was", "Ze\u00b7phyrs", "Un\u00b7be\u00b7stand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was ihm die Zeit entwandt,", "tokens": ["Was", "ihm", "die", "Zeit", "ent\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Verliert er ohne Klage.", "tokens": ["Ver\u00b7liert", "er", "oh\u00b7ne", "Kla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wer euch, ihr s\u00fcssen Musen! liebt,", "tokens": ["Wer", "euch", ",", "ihr", "s\u00fcs\u00b7sen", "Mu\u00b7sen", "!", "liebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$.", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der scherzt an eurer Hand in bluhmenvollen Feldern,", "tokens": ["Der", "scherzt", "an", "eu\u00b7rer", "Hand", "in", "bluh\u00b7men\u00b7vol\u00b7len", "Fel\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wann Boreas die L\u00fcfte tr\u00fcbt.", "tokens": ["Wann", "Bo\u00b7re\u00b7as", "die", "L\u00fcf\u00b7te", "tr\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Fr\u00fchling mag verbl\u00fchn!", "tokens": ["Der", "Fr\u00fch\u00b7ling", "mag", "ver\u00b7bl\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Jhm lacht ein ewig Gr\u00fcn", "tokens": ["Jhm", "lacht", "ein", "e\u00b7wig", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In euern Lorbeer-W\u00e4ldern.", "tokens": ["In", "eu\u00b7ern", "Lor\u00b7beer\u00b7W\u00e4l\u00b7dern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und wie? Ly\u00e4us flieht ja nicht,", "tokens": ["Und", "wie", "?", "Ly\u00b7\u00e4us", "flieht", "ja", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "$.", "NE", "VVFIN", "ADV", "PTKNEG", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Um dessen Ephe\u00fc-Stab die leichten Scherze schweben!", "tokens": ["Um", "des\u00b7sen", "E\u00b7phe\u00b7\u00fc\u00b7Stab", "die", "leich\u00b7ten", "Scher\u00b7ze", "schwe\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Noch gl\u00fcht sein purpurnes Gesicht:", "tokens": ["Noch", "gl\u00fcht", "sein", "pur\u00b7pur\u00b7nes", "Ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Noch will er guten Muth", "tokens": ["Noch", "will", "er", "gu\u00b7ten", "Muth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und \u00e4chte Dichterglut,", "tokens": ["Und", "\u00e4ch\u00b7te", "Dich\u00b7ter\u00b7glut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Troz rauhen Froste, geben.", "tokens": ["Troz", "rau\u00b7hen", "Fros\u00b7te", ",", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dem Weingott ist es nie zu kalt,", "tokens": ["Dem", "Wein\u00b7gott", "ist", "es", "nie", "zu", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und auch der Liebe nicht, lockt Venus gleich nicht immer", "tokens": ["Und", "auch", "der", "Lie\u00b7be", "nicht", ",", "lockt", "Ve\u00b7nus", "gleich", "nicht", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "PTKNEG", "$,", "VVFIN", "NE", "ADV", "PTKNEG", "ADV"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "In einen gr\u00fcnbelaubten Wald.", "tokens": ["In", "ei\u00b7nen", "gr\u00fcn\u00b7be\u00b7laub\u00b7ten", "Wald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In B\u00fcschen rauscht kein Ku\u00df:", "tokens": ["In", "B\u00fc\u00b7schen", "rauscht", "kein", "Ku\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch Amors zarter Fu\u00df", "tokens": ["Doch", "A\u00b7mors", "zar\u00b7ter", "Fu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Entweicht in warme Zimmer.", "tokens": ["Ent\u00b7weicht", "in", "war\u00b7me", "Zim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Jhm dient ein weiches Canapee", "tokens": ["Jhm", "dient", "ein", "wei\u00b7ches", "Ca\u00b7na\u00b7pee"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So gut und besser noch, als im geheimen Hayne", "tokens": ["So", "gut", "und", "bes\u00b7ser", "noch", ",", "als", "im", "ge\u00b7hei\u00b7men", "Hay\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD", "ADV", "$,", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bebl\u00fchmtes Gras und sanfter Klee.", "tokens": ["Be\u00b7bl\u00fchm\u00b7tes", "Gras", "und", "sanf\u00b7ter", "Klee", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O welche Welt von Lust", "tokens": ["O", "wel\u00b7che", "Welt", "von", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PWAT", "NN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "An einer Phyllis Brust", "tokens": ["An", "ei\u00b7ner", "Phyl\u00b7lis", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NE", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und, Freund, bey altem Weine!", "tokens": ["Und", ",", "Freund", ",", "bey", "al\u00b7tem", "Wei\u00b7ne", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sto\u00df an! es leb' ein holdes Kind,", "tokens": ["Sto\u00df", "an", "!", "es", "leb'", "ein", "hol\u00b7des", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Grazien gepflegt, erzogen unter Musen", "tokens": ["Von", "Gra\u00b7zi\u00b7en", "ge\u00b7pflegt", ",", "er\u00b7zo\u00b7gen", "un\u00b7ter", "Mu\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$,", "VVPP", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und sch\u00e4tzbarer, als Phrynen sind,", "tokens": ["Und", "sch\u00e4tz\u00b7ba\u00b7rer", ",", "als", "Phry\u00b7nen", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Unschuld, klugen Scherz", "tokens": ["Durch", "Un\u00b7schuld", ",", "klu\u00b7gen", "Scherz"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und durch ein gutes Herz", "tokens": ["Und", "durch", "ein", "gu\u00b7tes", "Herz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In einem sch\u00f6nen Busen!", "tokens": ["In", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Bu\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}