{"textgrid.poem.50817": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Wanderlied", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun will ich gehn und wandern", "tokens": ["Nun", "will", "ich", "gehn", "und", "wan\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fr\u00fch bis zum Abend sp\u00e4t,", "tokens": ["Fr\u00fch", "bis", "zum", "A\u00b7bend", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "APPRART", "NN", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "So weit auf dieser Erde", "tokens": ["So", "weit", "auf", "die\u00b7ser", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Sonne mit mir geht!", "tokens": ["Die", "Son\u00b7ne", "mit", "mir", "geht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich nehme nichts mit als den Becher,", "tokens": ["Ich", "neh\u00b7me", "nichts", "mit", "als", "den", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein leichtes Saitenget\u00f6n;", "tokens": ["Mein", "leich\u00b7tes", "Sai\u00b7ten\u00b7ge\u00b7t\u00f6n", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich wundre mich \u00fcber die Ma\u00dfen,", "tokens": ["Ich", "wund\u00b7re", "mich", "\u00fc\u00b7ber", "die", "Ma\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wie's \u00fcberall so sch\u00f6n!", "tokens": ["Wie's", "\u00fc\u00b7be\u00b7rall", "so", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Ebne ist oft sch\u00f6ner", "tokens": ["Die", "Eb\u00b7ne", "ist", "oft", "sch\u00f6\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als meine Berge noch,", "tokens": ["Als", "mei\u00b7ne", "Ber\u00b7ge", "noch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wo kein blauer Himmel,", "tokens": ["Und", "wo", "kein", "blau\u00b7er", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gibt's rote Wolken doch.", "tokens": ["Gibt's", "ro\u00b7te", "Wol\u00b7ken", "doch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wo keine schmachtenden Lotos,", "tokens": ["Wo", "kei\u00b7ne", "schmach\u00b7ten\u00b7den", "Lo\u00b7tos", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "W\u00e4chst bl\u00fchendes Heidekraut,", "tokens": ["W\u00e4chst", "bl\u00fc\u00b7hen\u00b7des", "Hei\u00b7de\u00b7kraut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wo keine gotischen Dome,", "tokens": ["Wo", "kei\u00b7ne", "go\u00b7ti\u00b7schen", "Do\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sind jonische Tempel gebaut.", "tokens": ["Sind", "jo\u00b7ni\u00b7sche", "Tem\u00b7pel", "ge\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Und bin ich des Griechischen m\u00fcde,", "tokens": ["Und", "bin", "ich", "des", "Grie\u00b7chi\u00b7schen", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mich lockt die luft'ge Moschee:", "tokens": ["Mich", "lockt", "die", "luft'\u00b7ge", "Mo\u00b7schee", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich kleide in maurische Schn\u00f6rkel", "tokens": ["Ich", "klei\u00b7de", "in", "mau\u00b7ri\u00b7sche", "Schn\u00f6r\u00b7kel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mein europ\u00e4isches Weh!", "tokens": ["Mein", "eu\u00b7ro\u00b7p\u00e4\u00b7i\u00b7sches", "Weh", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Die mangle ich \u00fcberall,", "tokens": ["Die", "mang\u00b7le", "ich", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Von ", "tokens": ["Von"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Den reinen Silberschall.", "tokens": ["Den", "rei\u00b7nen", "Sil\u00b7ber\u00b7schall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Hallo, du muntrer J\u00e4ger!", "tokens": ["Hal\u00b7lo", ",", "du", "mun\u00b7trer", "J\u00e4\u00b7ger", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sag an, du Bergmann traut!", "tokens": ["Sag", "an", ",", "du", "Berg\u00b7mann", "traut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hast du, o stiller Fischer,", "tokens": ["Hast", "du", ",", "o", "stil\u00b7ler", "Fi\u00b7scher", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Liebchen nicht geschaut?", "tokens": ["Mein", "Lieb\u00b7chen", "nicht", "ge\u00b7schaut", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Mein Liebchen ist die Freiheit,", "tokens": ["Mein", "Lieb\u00b7chen", "ist", "die", "Frei\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich suche sie kreuz und quer \u2013", "tokens": ["Ich", "su\u00b7che", "sie", "kreuz", "und", "quer", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie ist doch nicht ertrunken", "tokens": ["Sie", "ist", "doch", "nicht", "er\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im alten falschen Meer?", "tokens": ["Im", "al\u00b7ten", "fal\u00b7schen", "Meer", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}