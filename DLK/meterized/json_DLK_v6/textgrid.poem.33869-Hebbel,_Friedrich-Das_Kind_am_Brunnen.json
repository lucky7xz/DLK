{"textgrid.poem.33869": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Kind am Brunnen", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frau Amme, Frau Amme, das Kind ist erwacht!", "tokens": ["Frau", "Am\u00b7me", ",", "Frau", "Am\u00b7me", ",", "das", "Kind", "ist", "er\u00b7wacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "NN", "NE", "$,", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Doch die liegt ruhig im Schlafe.", "tokens": ["Doch", "die", "liegt", "ru\u00b7hig", "im", "Schla\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die V\u00f6glein zwitschern, die Sonne lacht,", "tokens": ["Die", "V\u00f6\u00b7glein", "zwit\u00b7schern", ",", "die", "Son\u00b7ne", "lacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Am H\u00fcgel weiden die Schafe.", "tokens": ["Am", "H\u00fc\u00b7gel", "wei\u00b7den", "die", "Scha\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Frau Amme, Frau Amme, das Kind steht auf,", "tokens": ["Frau", "Am\u00b7me", ",", "Frau", "Am\u00b7me", ",", "das", "Kind", "steht", "auf", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "NN", "NE", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Es wagt sich weiter und weiter!", "tokens": ["Es", "wagt", "sich", "wei\u00b7ter", "und", "wei\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "KON", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hinab zum Brunnen nimmt es den Lauf,", "tokens": ["Hin\u00b7ab", "zum", "Brun\u00b7nen", "nimmt", "es", "den", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Da stehen Blumen und Kr\u00e4uter.", "tokens": ["Da", "ste\u00b7hen", "Blu\u00b7men", "und", "Kr\u00e4u\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Frau Amme, Frau Amme, der Brunnen ist tief!", "tokens": ["Frau", "Am\u00b7me", ",", "Frau", "Am\u00b7me", ",", "der", "Brun\u00b7nen", "ist", "tief", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "NN", "NE", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sie schl\u00e4ft, als l\u00e4ge sie d'rinnen!", "tokens": ["Sie", "schl\u00e4ft", ",", "als", "l\u00e4\u00b7ge", "sie", "d'\u00b7rin\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Kind l\u00e4uft schnell, wie es nie noch lief,", "tokens": ["Das", "Kind", "l\u00e4uft", "schnell", ",", "wie", "es", "nie", "noch", "lief", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "PWAV", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Die Blumen locken's von hinnen.", "tokens": ["Die", "Blu\u00b7men", "lo\u00b7cken's", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPR", "ADV", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Nun steht es am Brunnen, nun ist es am Ziel,", "tokens": ["Nun", "steht", "es", "am", "Brun\u00b7nen", ",", "nun", "ist", "es", "am", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,", "ADV", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Nun pfl\u00fcckt es die Blumen sich munter,", "tokens": ["Nun", "pfl\u00fcckt", "es", "die", "Blu\u00b7men", "sich", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Doch bald erm\u00fcdet das reizende Spiel,", "tokens": ["Doch", "bald", "er\u00b7m\u00fc\u00b7det", "das", "rei\u00b7zen\u00b7de", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da schaut's in die Tiefe hinunter.", "tokens": ["Da", "schaut's", "in", "die", "Tie\u00b7fe", "hin\u00b7un\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Und unten erblickt es ein holdes Gesicht,", "tokens": ["Und", "un\u00b7ten", "er\u00b7blickt", "es", "ein", "hol\u00b7des", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Mit Augen, so hell und so s\u00fc\u00dfe.", "tokens": ["Mit", "Au\u00b7gen", ",", "so", "hell", "und", "so", "s\u00fc\u00b7\u00dfe", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "ADJD", "KON", "ADV", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Es ist sein eig'nes, das wei\u00df es noch nicht,", "tokens": ["Es", "ist", "sein", "eig'\u00b7nes", ",", "das", "wei\u00df", "es", "noch", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "$,", "PDS", "VVFIN", "PPER", "ADV", "PTKNEG", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Viel stumme, freundliche Gr\u00fc\u00dfe!", "tokens": ["Viel", "stum\u00b7me", ",", "freund\u00b7li\u00b7che", "Gr\u00fc\u00b7\u00dfe", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Das Kindlein winkt, der Schatten geschwind", "tokens": ["Das", "Kin\u00b7dlein", "winkt", ",", "der", "Schat\u00b7ten", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVPP"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Winkt aus der Tiefe ihm wieder.", "tokens": ["Winkt", "aus", "der", "Tie\u00b7fe", "ihm", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "ADV", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Herauf! Herauf! So meint's das Kind:", "tokens": ["Her\u00b7auf", "!", "Her\u00b7auf", "!", "So", "meint's", "das", "Kind", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$.", "PTKVZ", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Schatten: Hernieder! Hernieder!", "tokens": ["Der", "Schat\u00b7ten", ":", "Her\u00b7nie\u00b7der", "!", "Her\u00b7nie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$.", "NN", "$.", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "Schon beugt es sich \u00fcber den Brunnenrand,", "tokens": ["Schon", "beugt", "es", "sich", "\u00fc\u00b7ber", "den", "Brun\u00b7nen\u00b7rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Frau Amme, du schl\u00e4fst noch immer!", "tokens": ["Frau", "Am\u00b7me", ",", "du", "schl\u00e4fst", "noch", "im\u00b7mer", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da fallen die Blumen ihm aus der Hand,", "tokens": ["Da", "fal\u00b7len", "die", "Blu\u00b7men", "ihm", "aus", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und tr\u00fcben den lockenden Schimmer.", "tokens": ["Und", "tr\u00fc\u00b7ben", "den", "lo\u00b7cken\u00b7den", "Schim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "Verschwunden ist sie, die s\u00fc\u00dfe Gestalt,", "tokens": ["Ver\u00b7schwun\u00b7den", "ist", "sie", ",", "die", "s\u00fc\u00b7\u00dfe", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Verschluckt von der h\u00fcpfenden Welle,", "tokens": ["Ver\u00b7schluckt", "von", "der", "h\u00fcp\u00b7fen\u00b7den", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Das Kind durchschauert's fremd und kalt,", "tokens": ["Das", "Kind", "durch\u00b7schau\u00b7ert's", "fremd", "und", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schnell enteilt es der Stelle.", "tokens": ["Und", "schnell", "ent\u00b7eilt", "es", "der", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}