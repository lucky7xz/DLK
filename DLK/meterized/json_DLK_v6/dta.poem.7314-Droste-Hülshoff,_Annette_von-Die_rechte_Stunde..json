{"dta.poem.7314": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Die rechte Stunde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Im heitren Saal beim Kerzenlicht,", "tokens": ["Im", "hei\u00b7tren", "Saal", "beim", "Ker\u00b7zen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn alle Lippen spr\u00fchen Funken,", "tokens": ["Wenn", "al\u00b7le", "Lip\u00b7pen", "spr\u00fc\u00b7hen", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und gar vom Sonnenscheine trunken,", "tokens": ["Und", "gar", "vom", "Son\u00b7nen\u00b7schei\u00b7ne", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn jeder Finger Blumen bricht,", "tokens": ["Wenn", "je\u00b7der", "Fin\u00b7ger", "Blu\u00b7men", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und vollends an geliebtem Munde,", "tokens": ["Und", "vol\u00b7lends", "an", "ge\u00b7lieb\u00b7tem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn die Natur in Flammen schwimmt, \u2014", "tokens": ["Wenn", "die", "Na\u00b7tur", "in", "Flam\u00b7men", "schwimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das ist sie nicht die rechte Stunde,", "tokens": ["Das", "ist", "sie", "nicht", "die", "rech\u00b7te", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die dir der Genius bestimmt.", "tokens": ["Die", "dir", "der", "Ge\u00b7nius", "be\u00b7stimmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Doch wenn so Tag als Lust versank,", "tokens": ["Doch", "wenn", "so", "Tag", "als", "Lust", "ver\u00b7sank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "NN", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann wirst du schon ein Pl\u00e4tzchen wissen,", "tokens": ["Dann", "wirst", "du", "schon", "ein", "Pl\u00e4tz\u00b7chen", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vielleicht in deines Sopha's Kissen,", "tokens": ["Viel\u00b7leicht", "in", "dei\u00b7nes", "So\u00b7pha's", "Kis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vielleicht auf einer Gartenbank:", "tokens": ["Viel\u00b7leicht", "auf", "ei\u00b7ner", "Gar\u00b7ten\u00b7bank", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann klingt's wie halb verstandne Weise,", "tokens": ["Dann", "klingt's", "wie", "halb", "ver\u00b7stand\u00b7ne", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie halb verwischter Farben Gu\u00df", "tokens": ["Wie", "halb", "ver\u00b7wischter", "Far\u00b7ben", "Gu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADJA", "NN", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Verrinnt's um dich, und leise, leise", "tokens": ["Ver\u00b7rinnt's", "um", "dich", ",", "und", "lei\u00b7se", ",", "lei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "APPR", "PPER", "$,", "KON", "ADJD", "$,", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ber\u00fchrt dich dann dein Genius.", "tokens": ["Be\u00b7r\u00fchrt", "dich", "dann", "dein", "Ge\u00b7nius", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}