{"dta.poem.8529": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Der Kaufmann.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ungl&#252;ckselig der Kaufmann ist", "tokens": ["Un\u00b7gl", "&#252;", "ck\u00b7se\u00b7lig", "der", "Kauf\u00b7mann", "ist"], "token_info": ["word", "XML_entity", "word", "word", "word", "word"], "pos": ["NE", "$(", "ADJD", "ART", "NN", "VAFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und ganz von Sorgen befangen,", "tokens": ["Und", "ganz", "von", "Sor\u00b7gen", "be\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "An den Wolken h\u00e4ngt sein Blick,", "tokens": ["An", "den", "Wol\u00b7ken", "h\u00e4ngt", "sein", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Am Flaume mi\u00dft er die Winde;", "tokens": ["Am", "Flau\u00b7me", "mi\u00dft", "er", "die", "Win\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Aber selig des R\u00e4ubers Loos,", "tokens": ["A\u00b7ber", "se\u00b7lig", "des", "R\u00e4u\u00b7bers", "Loos", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "NE", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Und herrlich lebt der Pirate!", "tokens": ["Und", "herr\u00b7lich", "lebt", "der", "Pi\u00b7ra\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der die Meere Gespielen nennt,", "tokens": ["Der", "die", "Mee\u00b7re", "Ge\u00b7spie\u00b7len", "nennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Die Windsbraut seine Geliebte;", "tokens": ["Die", "Winds\u00b7braut", "sei\u00b7ne", "Ge\u00b7lieb\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Lachend sieht er die Schiffe zieh\u2019n,", "tokens": ["La\u00b7chend", "sieht", "er", "die", "Schif\u00b7fe", "zieh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.10": {"text": "Die aller G\u00fcter beraubten.", "tokens": ["Die", "al\u00b7ler", "G\u00fc\u00b7ter", "be\u00b7raub\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "\u201efahret wohl, gr\u00fc\u00dft den Kaufmann mir,", "tokens": ["\u201e", "fah\u00b7ret", "wohl", ",", "gr\u00fc\u00dft", "den", "Kauf\u00b7mann", "mir", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "$,", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Der am Flaum gemessen die Winde!\u201c", "tokens": ["Der", "am", "Flaum", "ge\u00b7mes\u00b7sen", "die", "Win\u00b7de", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "APPRART", "NN", "VVPP", "ART", "NN", "$.", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}}}}