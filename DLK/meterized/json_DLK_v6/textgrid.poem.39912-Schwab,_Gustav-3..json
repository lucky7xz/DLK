 "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So la\u00dft mich bei den Schafen", "tokens": ["So", "la\u00dft", "mich", "bei", "den", "Scha\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zu Gutach fort und fort!", "tokens": ["Zu", "Gu\u00b7tach", "fort", "und", "fort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Wie will ich dort erz\u00e4hlen", "tokens": ["Wie", "will", "ich", "dort", "er\u00b7z\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den lieben, frommen Seelen,", "tokens": ["Den", "lie\u00b7ben", ",", "from\u00b7men", "See\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Will ihnen nicht verhehlen,", "tokens": ["Will", "ih\u00b7nen", "nicht", "ver\u00b7heh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df Ihr den Vogt nicht logt.", "tokens": ["Da\u00df", "Ihr", "den", "Vogt", "nicht", "logt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr seid, was Ihr gewesen,", "tokens": ["Ihr", "seid", ",", "was", "Ihr", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,