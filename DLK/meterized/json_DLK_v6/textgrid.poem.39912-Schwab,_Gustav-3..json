{"textgrid.poem.39912": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jetzt predigt er so milde,", "tokens": ["Jetzt", "pre\u00b7digt", "er", "so", "mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nach seines Meisters Bilde,", "tokens": ["Nach", "sei\u00b7nes", "Meis\u00b7ters", "Bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Wort in dem Gefilde", "tokens": ["Das", "Wort", "in", "dem", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wuchs unter seiner Hand.", "tokens": ["Wuchs", "un\u00b7ter", "sei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und Friedenstauben flogen,", "tokens": ["Und", "Frie\u00b7dens\u00b7tau\u00b7ben", "flo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und \u00fcber wilden Wogen", "tokens": ["Und", "\u00fc\u00b7ber", "wil\u00b7den", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Erschien der Regenbogen:", "tokens": ["Er\u00b7schien", "der", "Re\u00b7gen\u00b7bo\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Feind zog aus dem Land.", "tokens": ["Der", "Feind", "zog", "aus", "dem", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da trocknete die Z\u00e4hre,", "tokens": ["Da", "trock\u00b7ne\u00b7te", "die", "Z\u00e4h\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da reifte froh die Aehre,", "tokens": ["Da", "reif\u00b7te", "froh", "die", "A\u00b7eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da hub die gute Lehre", "tokens": ["Da", "hub", "die", "gu\u00b7te", "Leh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das m\u00fcde Haupt in Kraft.", "tokens": ["Das", "m\u00fc\u00b7de", "Haupt", "in", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den frommen Rat der Alten", "tokens": ["Den", "from\u00b7men", "Rat", "der", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sah man zu Stuttgart walten,", "tokens": ["Sah", "man", "zu", "Stutt\u00b7gart", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "NE", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Kirche sich entfalten", "tokens": ["Die", "Kir\u00b7che", "sich", "ent\u00b7fal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Befreit aus langer Haft.", "tokens": ["Be\u00b7freit", "aus", "lan\u00b7ger", "Haft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wo durch's Kriegestoben", "tokens": ["Und", "wo", "durch's", "Krie\u00b7ge\u00b7sto\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich eine Stimm' erhoben,", "tokens": ["Sich", "ei\u00b7ne", "Stimm'", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das lautre Wort zu loben,", "tokens": ["Das", "laut\u00b7re", "Wort", "zu", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die hallt in aller Ohr.", "tokens": ["Die", "hallt", "in", "al\u00b7ler", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Drum, wo wer unbeth\u00f6ret", "tokens": ["Drum", ",", "wo", "wer", "un\u00b7be\u00b7th\u00f6\u00b7ret"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PAV", "$,", "PWAV", "PWS", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In schwerer Zeit gelehret,", "tokens": ["In", "schwe\u00b7rer", "Zeit", "ge\u00b7leh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den rief man hochverehret", "tokens": ["Den", "rief", "man", "hoch\u00b7ver\u00b7eh\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PIS", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vor allem Volk hervor.", "tokens": ["Vor", "al\u00b7lem", "Volk", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da macht sich auf die Reise", "tokens": ["Da", "macht", "sich", "auf", "die", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu seines Amtes Preise,", "tokens": ["Zu", "sei\u00b7nes", "Am\u00b7tes", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Beschieden vor die Greise,", "tokens": ["Be\u00b7schie\u00b7den", "vor", "die", "Grei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Gutach unser Hirt:", "tokens": ["Von", "Gu\u00b7tach", "un\u00b7ser", "Hirt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df ihm gelohnet werde,", "tokens": ["Da\u00df", "ihm", "ge\u00b7loh\u00b7net", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Weil sich von seiner Heerde", "tokens": ["Weil", "sich", "von", "sei\u00b7ner", "Heer\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Trotz Jammer und Beschwerde", "tokens": ["Trotz", "Jam\u00b7mer", "und", "Be\u00b7schwer\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Kein Sch\u00e4flein hat verirrt.", "tokens": ["Kein", "Sch\u00e4f\u00b7lein", "hat", "ver\u00b7irrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er kommt mit Furcht und Beben;", "tokens": ["Er", "kommt", "mit", "Furcht", "und", "Be\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er hat in seinem Leben", "tokens": ["Er", "hat", "in", "sei\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nicht viel sich abgegeben", "tokens": ["Nicht", "viel", "sich", "ab\u00b7ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "PRF", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit hoher Obrigkeit.", "tokens": ["Mit", "ho\u00b7her", "Ob\u00b7rig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er will im Vorsaal bleiben,", "tokens": ["Er", "will", "im", "Vor\u00b7saal", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da sitzen viel und schreiben;", "tokens": ["Da", "sit\u00b7zen", "viel", "und", "schrei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Angst sich zu vertreiben", "tokens": ["Die", "Angst", "sich", "zu", "ver\u00b7trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Hat er da gute Zeit.", "tokens": ["Hat", "er", "da", "gu\u00b7te", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Diener lernt er kennen,", "tokens": ["Die", "Die\u00b7ner", "lernt", "er", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die hin und wieder rennen,", "tokens": ["Die", "hin", "und", "wie\u00b7der", "ren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKVZ", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt wagt er sich zu nennen,", "tokens": ["Jetzt", "wagt", "er", "sich", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er will gemeldet sein.", "tokens": ["Er", "will", "ge\u00b7mel\u00b7det", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbseid Ihr's? Euch kann's nicht fehlen!", "tokens": ["\u00bb", "seid", "Ih\u00b7r's", "?", "Euch", "kann's", "nicht", "feh\u00b7len", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PIS", "$.", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ja, Herr, Ihr d\u00fcrft nur w\u00e4hlen,", "tokens": ["Ja", ",", "Herr", ",", "Ihr", "d\u00fcrft", "nur", "w\u00e4h\u00b7len", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Euch steht, auf meine Seelen,", "tokens": ["Euch", "steht", ",", "auf", "mei\u00b7ne", "See\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bei'm ", "tokens": ["Bei'm"], "token_info": ["word"], "pos": ["XY"], "meter": "+", "measure": "single.up"}}, "stanza.7": {"line.1": {"text": "\u00bbbei'm Brenz? bei'm Probst und Rate?", "tokens": ["\u00bb", "bei'm", "Brenz", "?", "bei'm", "Probst", "und", "Ra\u00b7te", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "$.", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Kirche Hort im Staate,", "tokens": ["Der", "Kir\u00b7che", "Hort", "im", "Staa\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der drin in dem Senate", "tokens": ["Der", "drin", "in", "dem", "Se\u00b7na\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den hohen Vorsitz f\u00fchrt?", "tokens": ["Den", "ho\u00b7hen", "Vor\u00b7sitz", "f\u00fchrt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wann hat mich der gesehen?", "tokens": ["Wann", "hat", "mich", "der", "ge\u00b7se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie sollte das ergehen,", "tokens": ["Wie", "soll\u00b7te", "das", "er\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PDS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da seines Geistes Wehen", "tokens": ["Da", "sei\u00b7nes", "Geis\u00b7tes", "We\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein niedrig Haupt ber\u00fchrt?\u00ab", "tokens": ["Mein", "nied\u00b7rig", "Haupt", "be\u00b7r\u00fchrt", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ein geht er zu der Pforten", "tokens": ["Ein", "geht", "er", "zu", "der", "Pfor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit solchen Zweifelsworten;", "tokens": ["Mit", "sol\u00b7chen", "Zwei\u00b7fels\u00b7wor\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch wen erblickt er dorten?", "tokens": ["Doch", "wen", "er\u00b7blickt", "er", "dor\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist auch sein Auge klar?", "tokens": ["Ist", "auch", "sein", "Au\u00b7ge", "klar", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In Seide, Sammt und Spitzen", "tokens": ["In", "Sei\u00b7de", ",", "Sammt", "und", "Spit\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit goldnem Kreuze blitzen,", "tokens": ["Mit", "gold\u00b7nem", "Kreu\u00b7ze", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zu oberst steht er sitzen", "tokens": ["Zu", "o\u00b7berst", "steht", "er", "sit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}}, "stanza.9": {"line.1": {"text": "Der streckt mit Gru\u00df und Segen,", "tokens": ["Der", "streckt", "mit", "Gru\u00df", "und", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie alte Freunde pflegen,", "tokens": ["Wie", "al\u00b7te", "Freun\u00b7de", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die treue Hand entgegen:", "tokens": ["Die", "treu\u00b7e", "Hand", "ent\u00b7ge\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u00bbgelobt sei Jesus Christ!", "tokens": ["\u00bb", "ge\u00b7lobt", "sei", "Je\u00b7sus", "Christ", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "VAFIN", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr habt wohl unterdessen", "tokens": ["Ihr", "habt", "wohl", "un\u00b7ter\u00b7des\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Fl\u00fcchtling gar vergessen,", "tokens": ["Den", "Fl\u00fccht\u00b7ling", "gar", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der als ein Vogt gesessen", "tokens": ["Der", "als", "ein", "Vogt", "ge\u00b7ses\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "KOUS", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zu Euren F\u00fc\u00dfen ist?", "tokens": ["Zu", "Eu\u00b7ren", "F\u00fc\u00b7\u00dfen", "ist", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ihr aber seid mir theuer,", "tokens": ["Ihr", "a\u