{"textgrid.poem.32686": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schon locket der Mai", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon locket der Mai", "tokens": ["Schon", "lo\u00b7cket", "der", "Mai"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Die Schwalben herbei,", "tokens": ["Die", "Schwal\u00b7ben", "her\u00b7bei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und alles im D\u00f6rfchen ist heiter;", "tokens": ["Und", "al\u00b7les", "im", "D\u00f6rf\u00b7chen", "ist", "hei\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPRART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Auf luftigen H\u00f6hn", "tokens": ["Auf", "luf\u00b7ti\u00b7gen", "H\u00f6hn"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und Wiesen entstehn", "tokens": ["Und", "Wie\u00b7sen", "ent\u00b7stehn"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVINF"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Die lieblichsten Blumen und Kr\u00e4uter.", "tokens": ["Die", "lieb\u00b7lichs\u00b7ten", "Blu\u00b7men", "und", "Kr\u00e4u\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Sieh, R\u00f6schen, mein Feld", "tokens": ["Sieh", ",", "R\u00f6\u00b7schen", ",", "mein", "Feld"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "PPOSAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Ist herrlich bestellt;", "tokens": ["Ist", "herr\u00b7lich", "be\u00b7stellt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Schon schie\u00dfen die Halmen in \u00c4hren;", "tokens": ["Schon", "schie\u00b7\u00dfen", "die", "Hal\u00b7men", "in", "\u00c4h\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mit mutigem Klang", "tokens": ["Mit", "mu\u00b7ti\u00b7gem", "Klang"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "L\u00e4\u00dft Lerchengesang", "tokens": ["L\u00e4\u00dft", "Ler\u00b7chen\u00b7ge\u00b7sang"], "token_info": ["word", "word"], "pos": ["VVFIN", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Und Wachtel darinnen sich h\u00f6ren!", "tokens": ["Und", "Wach\u00b7tel", "da\u00b7rin\u00b7nen", "sich", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "PRF", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Sieh, unten am Bach,", "tokens": ["Sieh", ",", "un\u00b7ten", "am", "Bach", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Die Schafe gemach", "tokens": ["Die", "Scha\u00b7fe", "ge\u00b7mach"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Durch blumichte Gegenden ziehen;", "tokens": ["Durch", "blu\u00b7mich\u00b7te", "Ge\u00b7gen\u00b7den", "zie\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und, wei\u00dfer als Pflaum,", "tokens": ["Und", ",", "wei\u00b7\u00dfer", "als", "Pflaum", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJA", "KOUS", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Im Garten den Baum", "tokens": ["Im", "Gar\u00b7ten", "den", "Baum"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Von unten bis obenan bl\u00fchen!", "tokens": ["Von", "un\u00b7ten", "bis", "o\u00b7be\u00b7nan", "bl\u00fc\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ADV", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Dies alles ist mein,", "tokens": ["Dies", "al\u00b7les", "ist", "mein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "PPOSAT", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Und, R\u00f6schen, auch dein,", "tokens": ["Und", ",", "R\u00f6\u00b7schen", ",", "auch", "dein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "ADV", "PPOSAT", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Sobald wir uns ehlich verbinden;", "tokens": ["So\u00b7bald", "wir", "uns", "eh\u00b7lich", "ver\u00b7bin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Dann werden uns schnell,", "tokens": ["Dann", "wer\u00b7den", "uns", "schnell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wie Perlen im Quell,", "tokens": ["Wie", "Per\u00b7len", "im", "Quell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Die h\u00fcpfenden Tage verschwinden.", "tokens": ["Die", "h\u00fcp\u00b7fen\u00b7den", "Ta\u00b7ge", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}