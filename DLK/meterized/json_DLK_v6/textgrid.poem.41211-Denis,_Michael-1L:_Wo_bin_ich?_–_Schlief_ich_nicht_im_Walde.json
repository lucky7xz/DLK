{"textgrid.poem.41211": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo bin ich? \u2013 Schlief ich nicht im Walde", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo bin ich? \u2013 Schlief ich nicht im Walde", "tokens": ["Wo", "bin", "ich", "?", "\u2013", "Schlief", "ich", "nicht", "im", "Wal\u00b7de"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "$(", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Arbeiterm\u00fcdet ein?", "tokens": ["Ar\u00b7bei\u00b7ter\u00b7m\u00fc\u00b7det", "ein", "?"], "token_info": ["word", "word", "punct"], "pos": ["NE", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Walde, wo des Lenzes", "tokens": ["Im", "Wal\u00b7de", ",", "wo", "des", "Len\u00b7zes"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Tonvoller Vogel nicht nistet,", "tokens": ["Ton\u00b7vol\u00b7ler", "Vo\u00b7gel", "nicht", "nis\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKNEG", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Im Walde, wo kein Barde", "tokens": ["Im", "Wal\u00b7de", ",", "wo", "kein", "Bar\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Noch seine Harf' in die Schatten trug?", "tokens": ["Noch", "sei\u00b7ne", "Ha\u00b7rf'", "in", "die", "Schat\u00b7ten", "trug", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er ist es, wo ich entschlief,", "tokens": ["Er", "ist", "es", ",", "wo", "ich", "ent\u00b7schlief", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Wald voll br\u00fctender Schauer,", "tokens": ["Der", "Wald", "voll", "br\u00fc\u00b7ten\u00b7der", "Schau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als w\u00e4r' er hinter Helas", "tokens": ["Als", "w\u00e4r'", "er", "hin\u00b7ter", "He\u00b7las"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "APPR", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Grotte gegen Walhalla gepflanzt;", "tokens": ["Grot\u00b7te", "ge\u00b7gen", "Wal\u00b7hal\u00b7la", "ge\u00b7pflanzt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Denn wie vom Felde der Seligen", "tokens": ["Denn", "wie", "vom", "Fel\u00b7de", "der", "Se\u00b7li\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "T\u00f6nt mir durch seine Fichteng\u00e4nge", "tokens": ["T\u00f6nt", "mir", "durch", "sei\u00b7ne", "Fich\u00b7ten\u00b7g\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Bardenharfe Ger\u00e4usch.", "tokens": ["Der", "Bar\u00b7den\u00b7har\u00b7fe", "Ge\u00b7r\u00e4usch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Mich umwandelt der Geist der Lieder,", "tokens": ["Mich", "um\u00b7wan\u00b7delt", "der", "Geist", "der", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Wie die Seele der Br\u00fcnstiggeliebten", "tokens": ["Wie", "die", "See\u00b7le", "der", "Br\u00fcns\u00b7tig\u00b7ge\u00b7lieb\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.10": {"text": "Um den einsam trauernden J\u00fcngling schwebt.", "tokens": ["Um", "den", "ein\u00b7sam", "trau\u00b7ern\u00b7den", "J\u00fcng\u00b7ling", "schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "O sey du mir willkommen!", "tokens": ["O", "sey", "du", "mir", "will\u00b7kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ruft der Verzweifelnde.", "tokens": ["Ruft", "der", "Ver\u00b7zwei\u00b7feln\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.3": {"text": "O sey du mir willkommen!", "tokens": ["O", "sey", "du", "mir", "will\u00b7kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ruf' ich und rei\u00dfe mich auf,", "tokens": ["Ruf'", "ich", "und", "rei\u00b7\u00dfe", "mich", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Da\u00df die zweigigte Fichte schwankt,", "tokens": ["Da\u00df", "die", "zwei\u00b7gig\u00b7te", "Fich\u00b7te", "schwankt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und streife windschnell \u00fcber das Haidenkraut,", "tokens": ["Und", "strei\u00b7fe", "wind\u00b7schnell", "\u00fc\u00b7ber", "das", "Hai\u00b7den\u00b7kraut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Und eil' und fliege gegen den Harfenruf,", "tokens": ["Und", "eil'", "und", "flie\u00b7ge", "ge\u00b7gen", "den", "Har\u00b7fen\u00b7ruf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Der bei jedem von Felsen", "tokens": ["Der", "bei", "je\u00b7dem", "von", "Fel\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PIS", "APPR", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.9": {"text": "Zu Felsen gewagten Sprunge", "tokens": ["Zu", "Fel\u00b7sen", "ge\u00b7wag\u00b7ten", "Sprun\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Immer n\u00e4her und n\u00e4her t\u00f6nt.", "tokens": ["Im\u00b7mer", "n\u00e4\u00b7her", "und", "n\u00e4\u00b7her", "t\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Da rauschet mir gewaltig", "tokens": ["Da", "rau\u00b7schet", "mir", "ge\u00b7wal\u00b7tig"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Josephs Namen entgegen;", "tokens": ["Jo\u00b7se\u00b7phs", "Na\u00b7men", "ent\u00b7ge\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKVZ", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Es rufen dort oben die Felsen,", "tokens": ["Es", "ru\u00b7fen", "dort", "o\u00b7ben", "die", "Fel\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Dort unten die Fichten rufen", "tokens": ["Dort", "un\u00b7ten", "die", "Fich\u00b7ten", "ru\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Joseph's Namen zur\u00fcck.", "tokens": ["Jo\u00b7se\u00b7ph's", "Na\u00b7men", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Und hier sind Nachtigallen;", "tokens": ["Und", "hier", "sind", "Nach\u00b7ti\u00b7gal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Hier scherzt das k\u00fchle L\u00fcftchen", "tokens": ["Hier", "scherzt", "das", "k\u00fch\u00b7le", "L\u00fcft\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Um junge Wiesenblumen;", "tokens": ["Um", "jun\u00b7ge", "Wie\u00b7sen\u00b7blu\u00b7men", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Weidende Rehe h\u00fcpfen", "tokens": ["Wei\u00b7den\u00b7de", "Re\u00b7he", "h\u00fcp\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.10": {"text": "Fr\u00f6hlich am Bach.", "tokens": ["Fr\u00f6h\u00b7lich", "am", "Bach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Heil mir! nun bin ich am Ziel!", "tokens": ["Heil", "mir", "!", "nun", "bin", "ich", "am", "Ziel", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "ADV", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Heil mir! da ist der S\u00e4nger!", "tokens": ["Heil", "mir", "!", "da", "ist", "der", "S\u00e4n\u00b7ger", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "G\u00f6tter! da ist die Harfe! durstig", "tokens": ["G\u00f6t\u00b7ter", "!", "da", "ist", "die", "Har\u00b7fe", "!", "durs\u00b7tig"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "$.", "ADV", "VAFIN", "ART", "NN", "$.", "ADJD"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Trink' ich all' ihre T\u00f6ne auf.", "tokens": ["Trink'", "ich", "all'", "ih\u00b7re", "T\u00f6\u00b7ne", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Vergieb dem Bardensohne,", "tokens": ["Ver\u00b7gieb", "dem", "Bar\u00b7den\u00b7soh\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vergieb, du Bindengeschm\u00fcckter!", "tokens": ["Ver\u00b7gieb", ",", "du", "Bin\u00b7den\u00b7ge\u00b7schm\u00fcck\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PPER", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wer bist du?", "tokens": ["Wer", "bist", "du", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Druide mit der gold'nen Sichel", "tokens": ["Drui\u00b7de", "mit", "der", "gold'\u00b7nen", "Si\u00b7chel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In deinem Priesterg\u00fcrtel!", "tokens": ["In", "dei\u00b7nem", "Pries\u00b7ter\u00b7g\u00fcr\u00b7tel", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wer bist du? S\u00e4nger Josephs!", "tokens": ["Wer", "bist", "du", "?", "S\u00e4n\u00b7ger", "Jo\u00b7se\u00b7phs", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$.", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du l\u00e4chelst, theurer S\u00e4nger!", "tokens": ["Du", "l\u00e4\u00b7chelst", ",", "theu\u00b7rer", "S\u00e4n\u00b7ger", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aber ich kenne die Harfe;", "tokens": ["A\u00b7ber", "ich", "ken\u00b7ne", "die", "Har\u00b7fe", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Und nun kenn' ich dich, Sined!", "tokens": ["Und", "nun", "kenn'", "ich", "dich", ",", "Si\u00b7ned", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "$,", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Den Freund an Ossians Busen,", "tokens": ["Den", "Freund", "an", "Os\u00b7si\u00b7ans", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Dem er am Abend", "tokens": ["Dem", "er", "am", "A\u00b7bend"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Seiner Augen", "tokens": ["Sei\u00b7ner", "Au\u00b7gen"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.8": {"line.1": {"text": "O singe, singe", "tokens": ["O", "sin\u00b7ge", ",", "sin\u00b7ge"], "token_info": ["word", "word", "punct", "word"], "pos": ["NE", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Joseph den Fr\u00fchgeliebten,", "tokens": ["Jo\u00b7se\u00b7ph", "den", "Fr\u00fch\u00b7ge\u00b7lieb\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie er im Frieden gro\u00df", "tokens": ["Wie", "er", "im", "Frie\u00b7den", "gro\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Segen um sich, und \u00fcber sich hat!", "tokens": ["Se\u00b7gen", "um", "sich", ",", "und", "\u00fc\u00b7ber", "sich", "hat", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "$,", "KON", "APPR", "PRF", "VAFIN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "O singe, singe,", "tokens": ["O", "sin\u00b7ge", ",", "sin\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "So lange diesseit Walhallas", "tokens": ["So", "lan\u00b7ge", "dies\u00b7seit", "Wal\u00b7hal\u00b7las"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er seine Schritte verweilt,", "tokens": ["Er", "sei\u00b7ne", "Schrit\u00b7te", "ver\u00b7weilt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Joseph's Kriegsgesang nicht!", "tokens": ["Jo\u00b7se\u00b7ph's", "Kriegs\u00b7ge\u00b7sang", "nicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKNEG", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.9": {"line.1": {"text": "Zwar wie der Adler,", "tokens": ["Zwar", "wie", "der", "Ad\u00b7ler", ","], "