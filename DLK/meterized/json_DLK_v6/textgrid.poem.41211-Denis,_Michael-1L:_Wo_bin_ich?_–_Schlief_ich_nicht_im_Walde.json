, "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Aber dann singe von Joseph nicht!", "tokens": ["A\u00b7ber", "dann", "sin\u00b7ge", "von", "Jo\u00b7se\u00b7ph", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "NE", "PTKNEG", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Trage dein Harfenspiel tief in den Eichenwald;", "tokens": ["Tra\u00b7ge", "dein", "Har\u00b7fen\u00b7spiel", "tief", "in", "den", "Ei\u00b7chen\u00b7wald", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Geh' zu dem Grabe Dauns, dort, wo die Fahnen weh'n,", "tokens": ["Geh'", "zu", "dem", "Gra\u00b7be", "Dauns", ",", "dort", ",", "wo", "die", "Fah\u00b7nen", "weh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "NE", "$,", "ADV", "$,", "PWAV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dort, wo die Kriegesdonner harren,", "tokens": ["Dort", ",", "wo", "die", "Krie\u00b7ges\u00b7don\u00b7ner", "har\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Singe, dort singe den Ruhm, den er in mancher Schlacht", "tokens": ["Sin\u00b7ge", ",", "dort", "sin\u00b7ge", "den", "Ruhm", ",", "den", "er", "in", "man\u00b7cher", "Schlacht"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "V