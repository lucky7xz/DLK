{"textgrid.poem.38425": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Guten Appetit", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es regnet, Gott segnet,", "tokens": ["Es", "reg\u00b7net", ",", "Gott", "seg\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Sonne scheint,", "tokens": ["Die", "Son\u00b7ne", "scheint", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der Mond greint,", "tokens": ["Der", "Mond", "greint", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Der Pfaff sitzt aufm Laden,", "tokens": ["Der", "Pfaff", "sitzt", "aufm", "La\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Fri\u00dft all die Palisaden!", "tokens": ["Fri\u00dft", "all", "die", "Pa\u00b7li\u00b7sa\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Nonne geht ins Wirthshaus,", "tokens": ["Die", "Non\u00b7ne", "geht", "ins", "Wirths\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und trinkt die Gl\u00e4ser all, all aus.", "tokens": ["Und", "trinkt", "die", "Gl\u00e4\u00b7ser", "all", ",", "all", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PIAT", "$,", "PIAT", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}