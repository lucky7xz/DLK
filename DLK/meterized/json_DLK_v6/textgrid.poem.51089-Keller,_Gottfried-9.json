{"textgrid.poem.51089": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "9", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu loben ist der M\u00e4nner k\u00fchner Mut,", "tokens": ["Zu", "lo\u00b7ben", "ist", "der", "M\u00e4n\u00b7ner", "k\u00fch\u00b7ner", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Womit sie ringen, aus der Feuersglut", "tokens": ["Wo\u00b7mit", "sie", "rin\u00b7gen", ",", "aus", "der", "Feu\u00b7ers\u00b7glut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu retten, was man irgend retten kann;", "tokens": ["Zu", "ret\u00b7ten", ",", "was", "man", "ir\u00b7gend", "ret\u00b7ten", "kann", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PRELS", "PIS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch ist nicht redenswert, was man gewann.", "tokens": ["Doch", "ist", "nicht", "re\u00b7dens\u00b7wert", ",", "was", "man", "ge\u00b7wann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das Beste ist ein alter Totenkranz,", "tokens": ["Das", "Bes\u00b7te", "ist", "ein", "al\u00b7ter", "To\u00b7ten\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erinnerung an froher Jugend Glanz,", "tokens": ["E\u00b7rin\u00b7ne\u00b7rung", "an", "fro\u00b7her", "Ju\u00b7gend", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "An den, wie ein verstummter Harfenton,", "tokens": ["An", "den", ",", "wie", "ein", "ver\u00b7stumm\u00b7ter", "Har\u00b7fen\u00b7ton", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In voller Hoffnung fr\u00fch verblichnen Sohn.", "tokens": ["In", "vol\u00b7ler", "Hoff\u00b7nung", "fr\u00fch", "ver\u00b7blich\u00b7nen", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mit welken Bl\u00e4ttern liegt er in der Au,", "tokens": ["Mit", "wel\u00b7ken", "Bl\u00e4t\u00b7tern", "liegt", "er", "in", "der", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und auf ihn f\u00e4llt der k\u00fchle Maientau;", "tokens": ["Und", "auf", "ihn", "f\u00e4llt", "der", "k\u00fch\u00b7le", "Mai\u00b7en\u00b7tau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die blassen B\u00e4nder wehn im Morgenwind,", "tokens": ["Die", "blas\u00b7sen", "B\u00e4n\u00b7der", "wehn", "im", "Mor\u00b7gen\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Daneben fr\u00f6stelnd wacht ein schwaches Kind.", "tokens": ["Da\u00b7ne\u00b7ben", "fr\u00f6s\u00b7telnd", "wacht", "ein", "schwa\u00b7ches", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie leicht und d\u00fcrr der alte Kranz mag sein,", "tokens": ["Wie", "leicht", "und", "d\u00fcrr", "der", "al\u00b7te", "Kranz", "mag", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "ART", "ADJA", "NN", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Man wird ihm wieder eine Stelle weihn", "tokens": ["Man", "wird", "ihm", "wie\u00b7der", "ei\u00b7ne", "Stel\u00b7le", "weihn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im neuen Bau, hoch an der Stubenwand,", "tokens": ["Im", "neu\u00b7en", "Bau", ",", "hoch", "an", "der", "Stu\u00b7ben\u00b7wand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als des Vergangnen letztem, leichtem Pfand.", "tokens": ["Als", "des", "Ver\u00b7gang\u00b7nen", "letz\u00b7tem", ",", "leich\u00b7tem", "Pfand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Da wird er still aufs junge Leben sehn", "tokens": ["Da", "wird", "er", "still", "aufs", "jun\u00b7ge", "Le\u00b7ben", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPRART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und dieses ehrend ihm vor\u00fcbergehn,", "tokens": ["Und", "die\u00b7ses", "eh\u00b7rend", "ihm", "vor\u00b7\u00fc\u00b7ber\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bis, was einst gr\u00fcn war, endlich ganz zerstiebt", "tokens": ["Bis", ",", "was", "einst", "gr\u00fcn", "war", ",", "end\u00b7lich", "ganz", "zer\u00b7stiebt"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$,", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und man den nackten Reif dem Feuer gibt.", "tokens": ["Und", "man", "den", "nack\u00b7ten", "Reif", "dem", "Feu\u00b7er", "gibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}