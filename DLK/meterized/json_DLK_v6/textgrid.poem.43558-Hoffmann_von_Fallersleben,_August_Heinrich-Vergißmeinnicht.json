{"textgrid.poem.43558": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vergi\u00dfmeinnicht", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es bl\u00fcht ein sch\u00f6nes Bl\u00fcmchen", "tokens": ["Es", "bl\u00fcht", "ein", "sch\u00f6\u00b7nes", "Bl\u00fcm\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf unsrer gr\u00fcnen Au.", "tokens": ["Auf", "uns\u00b7rer", "gr\u00fc\u00b7nen", "Au", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Aug' ist wie der Himmel", "tokens": ["Sein", "Aug'", "ist", "wie", "der", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So heiter und so blau.", "tokens": ["So", "hei\u00b7ter", "und", "so", "blau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es wei\u00df nicht viel zu reden", "tokens": ["Es", "wei\u00df", "nicht", "viel", "zu", "re\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Alles, was es spricht,", "tokens": ["Und", "Al\u00b7les", ",", "was", "es", "spricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist immer nur dasselbe,", "tokens": ["Ist", "im\u00b7mer", "nur", "das\u00b7sel\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PDAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist nur: Vergi\u00dfmeinnicht.", "tokens": ["Ist", "nur", ":", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "$.", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wenn ich zwei Aeuglein sehe", "tokens": ["Wenn", "ich", "zwei", "A\u00b7e\u00b7u\u00b7glein", "se\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "CARD", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So heiter und so blau,", "tokens": ["So", "hei\u00b7ter", "und", "so", "blau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So denk' ich an mein Bl\u00fcmchen", "tokens": ["So", "denk'", "ich", "an", "mein", "Bl\u00fcm\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf unsrer gr\u00fcnen Au.", "tokens": ["Auf", "uns\u00b7rer", "gr\u00fc\u00b7nen", "Au", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da kann ich auch nicht reden", "tokens": ["Da", "kann", "ich", "auch", "nicht", "re\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und nur mein Herze spricht,", "tokens": ["Und", "nur", "mein", "Her\u00b7ze", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So bange nur, so leise,", "tokens": ["So", "ban\u00b7ge", "nur", ",", "so", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und nur: Vergi\u00dfmeinnicht.", "tokens": ["Und", "nur", ":", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "$.", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}