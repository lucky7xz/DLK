{"dta.poem.6659": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geh\u00f6r.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O unbegreifflicher Zusammenklang!", "tokens": ["O", "un\u00b7be\u00b7greif\u00b7fli\u00b7cher", "Zu\u00b7sam\u00b7men\u00b7klang", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+--+----+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der fast unendlich wunderbar.", "tokens": ["Der", "fast", "un\u00b7end\u00b7lich", "wun\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir werden offtermals gewahr,", "tokens": ["Wir", "wer\u00b7den", "off\u00b7ter\u00b7mals", "ge\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was auf der Menschen Geist ein Ton w\u00fcrckt, ein Gesang.", "tokens": ["Was", "auf", "der", "Men\u00b7schen", "Geist", "ein", "Ton", "w\u00fcrckt", ",", "ein", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "NN", "ART", "NN", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Durch T\u00f6ne binden und verm\u00e4hlen", "tokens": ["Durch", "T\u00f6\u00b7ne", "bin\u00b7den", "und", "ver\u00b7m\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVINF", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sich, mit dem C\u00f6rper, unsre Seelen,", "tokens": ["Sich", ",", "mit", "dem", "C\u00f6r\u00b7per", ",", "uns\u00b7re", "See\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "$,", "APPR", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Durch ein geheimes Band.", "tokens": ["Durch", "ein", "ge\u00b7hei\u00b7mes", "Band", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Auf! lasst dereinst den Jrrthum uns entziehen,", "tokens": ["Auf", "!", "lasst", "de\u00b7reinst", "den", "Jrr\u00b7thum", "uns", "ent\u00b7zie\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "ADV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und den Zusammenhang, der uns sonst unbekannt,", "tokens": ["Und", "den", "Zu\u00b7sam\u00b7men\u00b7hang", ",", "der", "uns", "sonst", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Zu unterscheiden uns bem\u00fchen.", "tokens": ["Zu", "un\u00b7ter\u00b7schei\u00b7den", "uns", "be\u00b7m\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Lasst uns des Vorurtheils Betr\u00fcgerey entfliehen,", "tokens": ["Lasst", "uns", "des", "Vor\u00b7urt\u00b7heils", "Be\u00b7tr\u00fc\u00b7ge\u00b7rey", "ent\u00b7flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}