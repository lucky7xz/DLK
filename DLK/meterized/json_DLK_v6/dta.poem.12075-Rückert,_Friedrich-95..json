{"dta.poem.12075": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "95.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn dir aus einem Buch, das heilig du benennst,", "tokens": ["Wenn", "dir", "aus", "ei\u00b7nem", "Buch", ",", "das", "hei\u00b7lig", "du", "be\u00b7nennst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wenn aus einem Spruch, den du f\u00fcr weis' erkennst,", "tokens": ["Und", "wenn", "aus", "ei\u00b7nem", "Spruch", ",", "den", "du", "f\u00fcr", "weis'", "er\u00b7kennst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Aus einem Lehrermund mehr Wahrheit dir wird kund,", "tokens": ["Aus", "ei\u00b7nem", "Leh\u00b7rer\u00b7mund", "mehr", "Wahr\u00b7heit", "dir", "wird", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIAT", "NN", "PPER", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als offenbaret selbst dir ist im Herzensgrund;", "tokens": ["Als", "of\u00b7fen\u00b7ba\u00b7ret", "selbst", "dir", "ist", "im", "Her\u00b7zens\u00b7grund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ADV", "PPER", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So magst du mit Vertraun auf die Belehrung baun,", "tokens": ["So", "magst", "du", "mit", "Ver\u00b7traun", "auf", "die", "Be\u00b7leh\u00b7rung", "baun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und, eigner Einsicht blind, in die Erleuchtung schaun.", "tokens": ["Und", ",", "eig\u00b7ner", "Ein\u00b7sicht", "blind", ",", "in", "die", "Er\u00b7leuch\u00b7tung", "schaun", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJA", "NN", "ADJD", "$,", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du bist entschuldigt, doch mu\u00dft du entschuldigen", "tokens": ["Du", "bist", "ent\u00b7schul\u00b7digt", ",", "doch", "mu\u00dft", "du", "ent\u00b7schul\u00b7di\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auch die dem Geist mehr als Buchstaben huldigen.", "tokens": ["Auch", "die", "dem", "Geist", "mehr", "als", "Buch\u00b7sta\u00b7ben", "hul\u00b7di\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ART", "NN", "ADV", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}