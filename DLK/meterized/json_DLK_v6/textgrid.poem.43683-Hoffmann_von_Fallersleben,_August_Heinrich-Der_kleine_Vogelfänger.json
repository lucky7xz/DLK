{"textgrid.poem.43683": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der kleine Vogelf\u00e4nger", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wart', V\u00f6glein, wart'! Jetzt bist du mein,", "tokens": ["Wart'", ",", "V\u00f6\u00b7glein", ",", "wart'", "!", "Jetzt", "bist", "du", "mein", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "NN", "$,", "VVFIN", "$.", "ADV", "VAFIN", "PPER", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jetzt hab' ich dich gefangen,", "tokens": ["Jetzt", "hab'", "ich", "dich", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In einem K\u00e4fig sollst du jetzt", "tokens": ["In", "ei\u00b7nem", "K\u00e4\u00b7fig", "sollst", "du", "jetzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An meinem Fenster hangen!", "tokens": ["An", "mei\u00b7nem", "Fens\u00b7ter", "han\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbach, lieber Bube, sag' mir doch,", "tokens": ["\u00bb", "ach", ",", "lie\u00b7ber", "Bu\u00b7be", ",", "sag'", "mir", "doch", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "ADV", "NN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was hab' ich denn begangen,", "tokens": ["Was", "hab'", "ich", "denn", "be\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df du mich armes V\u00f6gelein,", "tokens": ["Da\u00df", "du", "mich", "ar\u00b7mes", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df du mich hast gefangen?\u00ab \u2013", "tokens": ["Da\u00df", "du", "mich", "hast", "ge\u00b7fan\u00b7gen", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "VVPP", "$.", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich bin der Herr, du bist der Knecht:", "tokens": ["Ich", "bin", "der", "Herr", ",", "du", "bist", "der", "Knecht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Thiere, die da leben,", "tokens": ["Die", "Thie\u00b7re", ",", "die", "da", "le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die sind dem Menschen allzumal", "tokens": ["Die", "sind", "dem", "Men\u00b7schen", "all\u00b7zu\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mir auch untergeben.", "tokens": ["Und", "mir", "auch", "un\u00b7ter\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbdas, lieber Bube, glaub' ich nicht,", "tokens": ["\u00bb", "das", ",", "lie\u00b7ber", "Bu\u00b7be", ",", "glaub'", "ich", "nicht", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "$,", "ADV", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das sollst du mir beweisen!\u00ab \u2013", "tokens": ["Das", "sollst", "du", "mir", "be\u00b7wei\u00b7sen", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PPER", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schweig' still, schweig' still! sonst brat' ich dich", "tokens": ["Schweig'", "still", ",", "schweig'", "still", "!", "sonst", "brat'", "ich", "dich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und werde dich verspeisen! \u2013", "tokens": ["Und", "wer\u00b7de", "dich", "ver\u00b7spei\u00b7sen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Knabe rannte schnell nach Haus,", "tokens": ["Der", "Kna\u00b7be", "rann\u00b7te", "schnell", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da fiel er von der Stiegen.", "tokens": ["Da", "fiel", "er", "von", "der", "Stie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das V\u00f6glein flog zum Haus hinaus", "tokens": ["Das", "V\u00f6\u00b7glein", "flog", "zum", "Haus", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und lie\u00df das B\u00fcblein liegen.", "tokens": ["Und", "lie\u00df", "das", "B\u00fcb\u00b7lein", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}