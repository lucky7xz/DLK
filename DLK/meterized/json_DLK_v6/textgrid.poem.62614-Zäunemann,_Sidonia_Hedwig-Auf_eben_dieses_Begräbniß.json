{"textgrid.poem.62614": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Auf eben dieses Begr\u00e4bni\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Wort, kein Donnerknall ist so durchdringend stark;", "tokens": ["Kein", "Wort", ",", "kein", "Don\u00b7ner\u00b7knall", "ist", "so", "durch\u00b7drin\u00b7gend", "stark", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VAFIN", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nichts ist so f\u00fcrchterlich, und f\u00e4hrt durch Bein und Mark", "tokens": ["Nichts", "ist", "so", "f\u00fcrch\u00b7ter\u00b7lich", ",", "und", "f\u00e4hrt", "durch", "Bein", "und", "Mark"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "ADV", "ADJD", "$,", "KON", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als da die h\u00f6chste Macht im Paradie\u00df gesprochen:", "tokens": ["Als", "da", "die", "h\u00f6chs\u00b7te", "Macht", "im", "Pa\u00b7ra\u00b7die\u00df", "ge\u00b7spro\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dieweil ihr meinen Wort nicht eifrig nachgelebt,", "tokens": ["Die\u00b7weil", "ihr", "mei\u00b7nen", "Wort", "nicht", "eif\u00b7rig", "nach\u00b7ge\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und meinem heilgen Sinn so gr\u00f6blich wiederstrebt,", "tokens": ["Und", "mei\u00b7nem", "heil\u00b7gen", "Sinn", "so", "gr\u00f6b\u00b7lich", "wie\u00b7der\u00b7strebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So wird auch euer Bau des Leibes einst zerbrochen.", "tokens": ["So", "wird", "auch", "eu\u00b7er", "Bau", "des", "Lei\u00b7bes", "einst", "zer\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "O Schmerz, da\u00df Leib und Seel so pl\u00f6tzlich scheiden mu\u00df!", "tokens": ["O", "Schmerz", ",", "da\u00df", "Leib", "und", "Seel", "so", "pl\u00f6tz\u00b7lich", "schei\u00b7den", "mu\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "NN", "KON", "NN", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ach! da\u00df ihr festes Band nach kurzer Zeit auf Erden,", "tokens": ["Ach", "!", "da\u00df", "ihr", "fes\u00b7tes", "Band", "nach", "kur\u00b7zer", "Zeit", "auf", "Er\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Durch diesen Allmachts-Spruch und allzu festen Schlu\u00df", "tokens": ["Durch", "die\u00b7sen", "All\u00b7machts\u00b7Spruch", "und", "all\u00b7zu", "fes\u00b7ten", "Schlu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "KON", "PTKA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ohn Ansehn der Person mu\u00df so zerissen werden!", "tokens": ["Ohn", "An\u00b7sehn", "der", "Per\u00b7son", "mu\u00df", "so", "ze\u00b7ris\u00b7sen", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VMFIN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Tod schie\u00dft seinen Pfeil auf grosse F\u00fcrsten lo\u00df,", "tokens": ["Der", "Tod", "schie\u00dft", "sei\u00b7nen", "Pfeil", "auf", "gros\u00b7se", "F\u00fcrs\u00b7ten", "lo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und w\u00e4re ihre Macht und Herrschaft noch so gro\u00df,", "tokens": ["Und", "w\u00e4\u00b7re", "ih\u00b7re", "Macht", "und", "Herr\u00b7schaft", "noch", "so", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "KON", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So kan doch seiner Faust kein einger wiederstreben.", "tokens": ["So", "kan", "doch", "sei\u00b7ner", "Faust", "kein", "ein\u00b7ger", "wie\u00b7der\u00b7stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPOSAT", "NN", "PIAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Reicher, der durchs Geld fast alles zwingen kan,", "tokens": ["Ein", "Rei\u00b7cher", ",", "der", "durchs", "Geld", "fast", "al\u00b7les", "zwin\u00b7gen", "kan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "ADV", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wird doch o herber Schmerz! des Todes Unterthan,", "tokens": ["Wird", "doch", "o", "her\u00b7ber", "Schmerz", "!", "des", "To\u00b7des", "Un\u00b7ter\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "FM", "ADJA", "NN", "$.", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und mu\u00df ihm seinen Pracht auf ewig \u00fcbergeben.", "tokens": ["Und", "mu\u00df", "ihm", "sei\u00b7nen", "Pracht", "auf", "e\u00b7wig", "\u00fc\u00b7ber\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPOSAT", "NN", "APPR", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es wird der Adelstand auch nicht von ihm verschont,", "tokens": ["Es", "wird", "der", "A\u00b7del\u00b7stand", "auch", "nicht", "von", "ihm", "ver\u00b7schont", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "PTKNEG", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Gleichwie die Ritterschaft, die Stahl und Feind besieget,", "tokens": ["Gleich\u00b7wie", "die", "Rit\u00b7ter\u00b7schaft", ",", "die", "Stahl", "und", "Feind", "be\u00b7sie\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und unter Schwei\u00df und Blut die Bosheit abgelohnt,", "tokens": ["Und", "un\u00b7ter", "Schwei\u00df", "und", "Blut", "die", "Bos\u00b7heit", "ab\u00b7ge\u00b7lohnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Auch vor der Todes-Hand und ihren Schlag erlieget.", "tokens": ["Auch", "vor", "der", "To\u00b7des\u00b7Hand", "und", "ih\u00b7ren", "Schlag", "er\u00b7lie\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Da\u00df Helden sterblich sind, ist klar und ausgemacht.", "tokens": ["Da\u00df", "Hel\u00b7den", "sterb\u00b7lich", "sind", ",", "ist", "klar", "und", "aus\u00b7ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "VAFIN", "$,", "VAFIN", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allein man hat gar oft die Frage vorgebracht:", "tokens": ["Al\u00b7lein", "man", "hat", "gar", "oft", "die", "Fra\u00b7ge", "vor\u00b7ge\u00b7bracht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo doch ein k\u00fchner Held, am gl\u00fccklichsten verstirbet?", "tokens": ["Wo", "doch", "ein", "k\u00fch\u00b7ner", "Held", ",", "am", "gl\u00fcck\u00b7lichs\u00b7ten", "ver\u00b7stir\u00b7bet", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "$,", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Ob es die Wahlstadt sey; ob er in ofnen Feld,", "tokens": ["Ob", "es", "die", "Wahl\u00b7stadt", "sey", ";", "ob", "er", "in", "of\u00b7nen", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$.", "KOUS", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo Blut aus Wunden rinnt, wo Lager und Gezelt", "tokens": ["Wo", "Blut", "aus", "Wun\u00b7den", "rinnt", ",", "wo", "La\u00b7ger", "und", "Ge\u00b7zelt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "NN", "VVFIN", "$,", "PWAV", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Pulver Dampf durchzieht, ein s\u00fcsses Gl\u00fcck erwirbet?", "tokens": ["Der", "Pul\u00b7ver", "Dampf", "durch\u00b7zieht", ",", "ein", "s\u00fcs\u00b7ses", "Gl\u00fcck", "er\u00b7wir\u00b7bet", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie? oder ob er nicht in Ruhe, ohne Schwei\u00df", "tokens": ["Wie", "?", "o\u00b7der", "ob", "er", "nicht", "in", "Ru\u00b7he", ",", "oh\u00b7ne", "Schwei\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "$.", "KON", "KOUS", "PPER", "PTKNEG", "APPR", "NN", "$,", "KOUI", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und Pulver, Schwerd und Blut, auf einem weichen Bette", "tokens": ["Und", "Pul\u00b7ver", ",", "Schwerd", "und", "Blut", ",", "auf", "ei\u00b7nem", "wei\u00b7chen", "Bet\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "In seiner Freunde Arm von gr\u00f6\u00dfren Gl\u00fccke wei\u00df?", "tokens": ["In", "sei\u00b7ner", "Freun\u00b7de", "Arm", "von", "gr\u00f6\u00df\u00b7ren", "Gl\u00fc\u00b7cke", "wei\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und einen sch\u00f6nern Ruhm, dadurch zu hoffen h\u00e4tte?", "tokens": ["Und", "ei\u00b7nen", "sch\u00f6\u00b7nern", "Ruhm", ",", "da\u00b7durch", "zu", "hof\u00b7fen", "h\u00e4t\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PAV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Gustav Adolph der L\u00f6w und Held von Mitternacht", "tokens": ["Gus\u00b7tav", "A\u00b7dolph", "der", "L\u00f6w", "und", "Held", "von", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "ART", "NN", "KON", "NN", "APPR", "NN"], "meter": "+----+-+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Hat diese hochger\u00fchmt und sehr begl\u00fcckt gemacht,", "tokens": ["Hat", "die\u00b7se", "hoch\u00b7ge\u00b7r\u00fchmt", "und", "sehr", "be\u00b7gl\u00fcckt", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADJD", "KON", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die mit beherztem Muth auf Feld und Rasen sterben.", "tokens": ["Die", "mit", "be\u00b7herz\u00b7tem", "Muth", "auf", "Feld", "und", "Ra\u00b7sen", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die unter Blitz und Knall, durch Schu\u00df und Hieb und Stahl", "tokens": ["Die", "un\u00b7ter", "Blitz", "und", "Knall", ",", "durch", "Schu\u00df", "und", "Hieb", "und", "Stahl"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Getrost von hinnen gehn, und dieses Jammerthal", "tokens": ["Ge\u00b7trost", "von", "hin\u00b7nen", "gehn", ",", "und", "die\u00b7ses", "Jam\u00b7mer\u00b7thal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "ADV", "VVINF", "$,", "KON", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Ruh in jener Welt nach ihrem Kampf ererben.", "tokens": ["Die", "Ruh", "in", "je\u00b7ner", "Welt", "nach", "ih\u00b7rem", "Kampf", "er\u00b7er\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es ist ein grosser Ruhm vor Gottes heilge Ehr;", "tokens": ["Es", "ist", "ein", "gros\u00b7ser", "Ruhm", "vor", "Got\u00b7tes", "heil\u00b7ge", "Ehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und vor das Vaterland und vor des F\u00fcrsten Leben", "tokens": ["Und", "vor", "das", "Va\u00b7ter\u00b7land", "und", "vor", "des", "F\u00fcrs\u00b7ten", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und Wohlfahrt, Muth und Blut, und was dergleichen mehr,", "tokens": ["Und", "Wohl\u00b7fahrt", ",", "Muth", "und", "Blut", ",", "und", "was", "derg\u00b7lei\u00b7chen", "mehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN", "$,", "KON", "PWS", "PIS", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Im Feld und in der Schlacht gro\u00dfm\u00fcthig hinzugeben.", "tokens": ["Im", "Feld", "und", "in", "der", "Schlacht", "gro\u00df\u00b7m\u00fct\u00b7hig", "hin\u00b7zu\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "ART", "NN", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wie r\u00fchmlich l\u00e4\u00dft es nicht, wenn ein beherzter Held", "tokens": ["Wie", "r\u00fchm\u00b7lich", "l\u00e4\u00dft", "es", "nicht", ",", "wenn", "ein", "be\u00b7herz\u00b7ter", "Held"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit ausgestrecktem Arm ganz blutig niederf\u00e4llt,", "tokens": ["Mit", "aus\u00b7ge\u00b7streck\u00b7tem", "Arm", "ganz", "blu\u00b7tig", "nie\u00b7der\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und r\u00f6chlend auf dem Scho\u00df der nassen Erde lieget.", "tokens": ["Und", "r\u00f6ch\u00b7lend", "auf", "dem", "Scho\u00df", "der", "nas\u00b7sen", "Er\u00b7de", "lie\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Legt nicht die Tapferkeit ein r\u00fchmlich Zeugni\u00df ab,", "tokens": ["Legt", "nicht", "die", "Tap\u00b7fer\u00b7keit", "ein", "r\u00fchm\u00b7lich", "Zeug\u00b7ni\u00df", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "ART", "ADJD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn, ihr beherzter Sohn im Feld ein rothes Grab", "tokens": ["Wenn", ",", "ihr", "be\u00b7herz\u00b7ter", "Sohn", "im", "Feld", "ein", "ro\u00b7thes", "Grab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Auch mitten in dem Streit von seinem Feinde krieget.", "tokens": ["Auch", "mit\u00b7ten", "in", "dem", "Streit", "von", "sei\u00b7nem", "Fein\u00b7de", "krie\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Wahlstatt, so von Blut als wie von Ehre raucht,", "tokens": ["Die", "Wahl\u00b7statt", ",", "so", "von", "Blut", "als", "wie", "von", "Eh\u00b7re", "raucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "NN", "KOUS", "KOKOM", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Kan eine Helden-Brust nicht anders als erg\u00f6tzen.", "tokens": ["Kan", "ei\u00b7ne", "Hel\u00b7den\u00b7Brust", "nicht", "an\u00b7ders", "als", "er\u00b7g\u00f6t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PTKNEG", "ADV", "KOKOM", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "(hier wird das Sterbe-Kleid in Purpur eingetaucht.)", "tokens": ["(", "hier", "wird", "das", "Ster\u00b7be\u00b7Kleid", "in", "Pur\u00b7pur", "ein\u00b7ge\u00b7taucht", ".", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VAFIN", "ART", "NN", "APPR", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und ist dem Schau-Ger\u00fcst der F\u00fcrsten gleich zu sch\u00e4tzen.", "tokens": ["Und", "ist", "dem", "Schau\u00b7Ge\u00b7r\u00fcst", "der", "F\u00fcrs\u00b7ten", "gleich", "zu", "sch\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ob gleich das Ehren-Bett die blutge Wahlstatt heist,", "tokens": ["Ob", "gleich", "das", "Eh\u00b7ren\u00b7Bett", "die", "blut\u00b7ge", "Wahl\u00b7statt", "heist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wird das Kranken-Bett nichts minder hoch gepreist,", "tokens": ["So", "wird", "das", "Kran\u00b7ken\u00b7Bett", "nichts", "min\u00b7der", "hoch", "ge\u00b7preist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PIS", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und kan allhier ein Held begl\u00fcckt die Welt verlassen.", "tokens": ["Und", "kan", "all\u00b7hier", "ein", "Held", "be\u00b7gl\u00fcckt", "die", "Welt", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ART", "NN", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hier macht er sich mit Gott zu seiner Gruft bereit,", "tokens": ["Hier", "macht", "er", "sich", "mit", "Gott", "zu", "sei\u00b7ner", "Gruft", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bestellt ganz klug sein Haus, und lernt die Eitelkeit", "tokens": ["Be\u00b7stellt", "ganz", "klug", "sein", "Haus", ",", "und", "lernt", "die", "Ei\u00b7tel\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADJD", "PPOSAT", "NN", "$,", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und jene reine Welt in die Gedanken fassen,", "tokens": ["Und", "je\u00b7ne", "rei\u00b7ne", "Welt", "in", "die", "Ge\u00b7dan\u00b7ken", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er giebt den Seinigen wie Jacob dort die Hand,", "tokens": ["Er", "giebt", "den", "Sei\u00b7ni\u00b7gen", "wie", "Ja\u00b7cob", "dort", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PPOSS", "KOKOM", "NE", "ADV", "ART", "NN", "$,"], "meter": "-+-----++--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Und kan sie kr\u00e4ftiglich ermahnen, warnen, lehren.", "tokens": ["Und", "kan", "sie", "kr\u00e4f\u00b7tig\u00b7lich", "er\u00b7mah\u00b7nen", ",", "war\u00b7nen", ",", "leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "VVINF", "$,", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er nimmt in stiller Ruh des Glaubens Unterpfand.", "tokens": ["Er", "nimmt", "in", "stil\u00b7ler", "Ruh", "des", "Glau\u00b7bens", "Un\u00b7ter\u00b7pfand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wer wolte nicht den Tod dergleichen Helden ehren?", "tokens": ["Wer", "wol\u00b7te", "nicht", "den", "Tod", "derg\u00b7lei\u00b7chen", "Hel\u00b7den", "eh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PTKNEG", "ART", "NN", "PIS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "So sehr nun dieses Wort die Wahrheit in sich schlie\u00dft,", "tokens": ["So", "sehr", "nun", "die\u00b7ses", "Wort", "die", "Wahr\u00b7heit", "in", "sich", "schlie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PDAT", "NN", "ART", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So schmerzlich mir es doch jetzt zu vernehmen ist,", "tokens": ["So", "schmerz\u00b7lich", "mir", "es", "doch", "jetzt", "zu", "ver\u00b7neh\u00b7men", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dieweil mein liebster Freund den Satz durch sich beweiset.", "tokens": ["Die\u00b7weil", "mein", "liebs\u00b7ter", "Freund", "den", "Satz", "durch", "sich", "be\u00b7wei\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ART", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hier bricht mir fast das Herz, die Zunge wird mir schwach!", "tokens": ["Hier", "bricht", "mir", "fast", "das", "Herz", ",", "die", "Zun\u00b7ge", "wird", "mir", "schwach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,", "ART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und aus den Augen rinnt ein heiser Thr\u00e4hnen Bach.", "tokens": ["Und", "aus", "den", "Au\u00b7gen", "rinnt", "ein", "hei\u00b7ser", "Thr\u00b7\u00e4h\u00b7nen", "Bach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.6": {"text": "Ach! da\u00df des Todes Hand dich g\u00e4nzlich von mir reiset!", "tokens": ["Ach", "!", "da\u00df", "des", "To\u00b7des", "Hand", "dich", "g\u00e4nz\u00b7lich", "von", "mir", "rei\u00b7set", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "ART", "NN", "NN", "PPER", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich habe dich wie mich und meine Seel geliebt.", "tokens": ["Ich", "ha\u00b7be", "dich", "wie", "mich", "und", "mei\u00b7ne", "Seel", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PWAV", "PPER", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Herr Schwager! du bist stets mein Jonathan gewesen.", "tokens": ["Herr", "Schwa\u00b7ger", "!", "du", "bist", "stets", "mein", "Jo\u00b7na\u00b7than", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "PPER", "VAFIN", "ADV", "PPOSAT", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Drum macht mich auch dein Tod bis in den Tod betr\u00fcbt,", "tokens": ["Drum", "macht", "mich", "auch", "dein", "Tod", "bis", "in", "den", "Tod", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "APPR", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Man kan an mir den Schmerz aus Aug und Mienen lesen.", "tokens": ["Man", "kan", "an", "mir", "den", "Schmerz", "aus", "Aug", "und", "Mie\u00b7nen", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PPER", "ART", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ich wei\u00df fast nicht wo ich vor Schmerz und Wehmuth bin;", "tokens": ["Ich", "wei\u00df", "fast", "nicht", "wo", "ich", "vor", "Schmerz", "und", "Weh\u00b7muth", "bin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "PWAV", "PPER", "APPR", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Richt ich mein nasses Aug zu meiner Schwester hin,", "tokens": ["Richt", "ich", "mein", "nas\u00b7ses", "Aug", "zu", "mei\u00b7ner", "Schwes\u00b7ter", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So seh ich sie halb todt bey deinem Grabe liegen,", "tokens": ["So", "seh", "ich", "sie", "halb", "todt", "bey", "dei\u00b7nem", "Gra\u00b7be", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJD", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Freunde hat dein Tod zu hart, zu schwer gebeugt,", "tokens": ["Die", "Freun\u00b7de", "hat", "dein", "Tod", "zu", "hart", ",", "zu", "schwer", "ge\u00b7beugt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "PTKA", "ADJD", "$,", "PTKA", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein Herzog, so sein Herz dir gn\u00e4dig zugeneigt", "tokens": ["Dein", "Her\u00b7zog", ",", "so", "sein", "Herz", "dir", "gn\u00e4\u00b7dig", "zu\u00b7gen\u00b7eigt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADV", "PPOSAT", "NN", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "W\u00fcnscht, wenn es m\u00f6glich w\u00e4r, dich auf das neu zu kriegen.", "tokens": ["W\u00fcnscht", ",", "wenn", "es", "m\u00f6g\u00b7lich", "w\u00e4r", ",", "dich", "auf", "das", "neu", "zu", "krie\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "PRF", "APPR", "ART", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Ritterschaft beweint und netzt dein fr\u00fches Grab,", "tokens": ["Die", "Rit\u00b7ter\u00b7schaft", "be\u00b7weint", "und", "netzt", "dein", "fr\u00fc\u00b7hes", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dein Bischoffroda klagt mit \u00e4ngstlichen Geberden,", "tokens": ["Dein", "Bi\u00b7schoff\u00b7ro\u00b7da", "klagt", "mit", "\u00e4ngst\u00b7li\u00b7chen", "Ge\u00b7ber\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Eltern seufzen scharf um ihres Alters Stab,", "tokens": ["Die", "El\u00b7tern", "seuf\u00b7zen", "scharf", "um", "ih\u00b7res", "Al\u00b7ters", "Stab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und k\u00f6nnen fast durch nichts getr\u00f6st, gest\u00e4rket werden.", "tokens": ["Und", "k\u00f6n\u00b7nen", "fast", "durch", "nichts", "ge\u00b7tr\u00f6st", ",", "ge\u00b7st\u00e4r\u00b7ket", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "APPR", "PIS", "VVPP", "$,", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Dein Tod erschreckt mich zwar. Doch tr\u00f6stet mich, da\u00df du", "tokens": ["Dein", "Tod", "er\u00b7schreckt", "mich", "zwar", ".", "Doch", "tr\u00f6s\u00b7tet", "mich", ",", "da\u00df", "du"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$.", "KON", "VVFIN", "PPER", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als ein beherzter Held, durch Streiten, doch in Ruh", "tokens": ["Als", "ein", "be\u00b7herz\u00b7ter", "Held", ",", "durch", "Strei\u00b7ten", ",", "doch", "in", "Ruh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "APPR", "NN", "$,", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf deinem Kranken-Bett, so ritterlich gesieget.", "tokens": ["Auf", "dei\u00b7nem", "Kran\u00b7ken\u00b7Bett", ",", "so", "rit\u00b7ter\u00b7lich", "ge\u00b7sie\u00b7get", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du hast nach Helden Art gefochten und gek\u00e4mpft,", "tokens": ["Du", "hast", "nach", "Hel\u00b7den", "Art", "ge\u00b7foch\u00b7ten", "und", "ge\u00b7k\u00e4mpft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "S\u00fcnd, Teufel, H\u00f6ll und Tod mit deinem Gott ged\u00e4mpft,", "tokens": ["S\u00fcnd", ",", "Teu\u00b7fel", ",", "H\u00f6ll", "und", "Tod", "mit", "dei\u00b7nem", "Gott", "ge\u00b7d\u00e4mpft", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So, da\u00df sich ihre Macht vor deinen Glauben schmieget.", "tokens": ["So", ",", "da\u00df", "sich", "ih\u00b7re", "Macht", "vor", "dei\u00b7nen", "Glau\u00b7ben", "schmie\u00b7get", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PRF", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Drum mu\u00df dein Sterbe-Bett ber\u00fchmt und gl\u00fccklich seyn,", "tokens": ["Drum", "mu\u00df", "dein", "Ster\u00b7be\u00b7Bett", "be\u00b7r\u00fchmt", "und", "gl\u00fcck\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Wahlstatt war allhier mit viel Triumpf begleitet,", "tokens": ["Die", "Wahl\u00b7statt", "war", "all\u00b7hier", "mit", "viel", "Tri\u00b7umpf", "be\u00b7glei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du schliesst hier r\u00fchmlicher als auf der Wahlstatt ein.", "tokens": ["Du", "schliesst", "hier", "r\u00fchm\u00b7li\u00b7cher", "als", "auf", "der", "Wahl\u00b7statt", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KOKOM", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Gl\u00fcckselig ist der Held, der so wie Kreutzburg streitet.", "tokens": ["Gl\u00fcck\u00b7se\u00b7lig", 