{"textgrid.poem.56918": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was wirst du noch wollen,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was wirst du noch wollen,", "tokens": ["Was", "wirst", "du", "noch", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VMFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "du ewig begehrende,", "tokens": ["du", "e\u00b7wig", "be\u00b7geh\u00b7ren\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.3": {"text": "wohin du noch fliegen,", "tokens": ["wo\u00b7hin", "du", "noch", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "du sturmwindwilde!", "tokens": ["du", "sturm\u00b7wind\u00b7wil\u00b7de", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Die in Erkenntnis du", "tokens": ["Die", "in", "Er\u00b7kennt\u00b7nis", "du"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "rein dich badetest,", "tokens": ["rein", "dich", "ba\u00b7de\u00b7test", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "die du des Schaffens", "tokens": ["die", "du", "des", "Schaf\u00b7fens"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "heiligen Wahnsinn kostetest,", "tokens": ["hei\u00b7li\u00b7gen", "Wahn\u00b7sinn", "kos\u00b7te\u00b7test", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "die du der Macht", "tokens": ["die", "du", "der", "Macht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "\u00fcberweltliche Freuden ahnetest,", "tokens": ["\u00fc\u00b7ber\u00b7welt\u00b7li\u00b7che", "Freu\u00b7den", "ah\u00b7ne\u00b7test", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "die du von Str\u00f6men der Liebe", "tokens": ["die", "du", "von", "Str\u00f6\u00b7men", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "quollest und duftetest!", "tokens": ["quol\u00b7lest", "und", "duf\u00b7te\u00b7test", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.13": {"text": "War dir ein Lohn je genug?", "tokens": ["War", "dir", "ein", "Lohn", "je", "ge\u00b7nug", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Hielt dich ein Ziel je zur\u00fcck?", "tokens": ["Hielt", "dich", "ein", "Ziel", "je", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.15": {"text": "Gleich wie der Wind tagaus, nachtein", "tokens": ["Gleich", "wie", "der", "Wind", "ta\u00b7gaus", ",", "nach\u00b7tein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "PTKVZ", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "um den rollenden Ball", "tokens": ["um", "den", "rol\u00b7len\u00b7den", "Ball"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.17": {"text": "seine ruhlosen Fittiche regt,", "tokens": ["sei\u00b7ne", "ruh\u00b7lo\u00b7sen", "Fit\u00b7ti\u00b7che", "regt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.18": {"text": "nicht \u00fcber Meeren rastend,", "tokens": ["nicht", "\u00fc\u00b7ber", "Mee\u00b7ren", "ras\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "nicht auf der Berge Haupt,", "tokens": ["nicht", "auf", "der", "Ber\u00b7ge", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "ewig wechselnder Wolke", "tokens": ["e\u00b7wig", "wech\u00b7seln\u00b7der", "Wol\u00b7ke"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.21": {"text": "Former und Feger \u2013", "tokens": ["For\u00b7mer", "und", "Fe\u00b7ger", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "gleich wie sein Odem", "tokens": ["gleich", "wie", "sein", "O\u00b7dem"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.23": {"text": "des Pols und der W\u00fcste", "tokens": ["des", "Pols", "und", "der", "W\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.24": {"text": "streitende L\u00fcfte sind", "tokens": ["strei\u00b7ten\u00b7de", "L\u00fcf\u00b7te", "sind"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.25": {"text": "und der Blitze Herden", "tokens": ["und", "der", "Blit\u00b7ze", "Her\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.26": {"text": "ein Spiel seiner Lust \u2013", "tokens": ["ein", "Spiel", "sei\u00b7ner", "Lust", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.27": {"text": "so bist du, meine sturmwilde Seele,", "tokens": ["so", "bist", "du", ",", "mei\u00b7ne", "sturm\u00b7wil\u00b7de", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}, "line.28": {"text": "ein ewiger Odem,", "tokens": ["ein", "e\u00b7wi\u00b7ger", "O\u00b7dem", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.29": {"text": "ein schwangerer Weltwind,", "tokens": ["ein", "schwan\u00b7ge\u00b7rer", "Welt\u00b7wind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.30": {"text": "ein Scho\u00df von Gewittern!", "tokens": ["ein", "Scho\u00df", "von", "Ge\u00b7wit\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.31": {"text": "Oh du meine Seele,", "tokens": ["Oh", "du", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.32": {"text": "die du in tausend Herzblutquellen", "tokens": ["die", "du", "in", "tau\u00b7send", "Herz\u00b7blut\u00b7quel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "durch den Ring \u00e4onischer Alter", "tokens": ["durch", "den", "Ring", "\u00e4\u00b7o\u00b7nisc\u00b7her", "Al\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.34": {"text": "heran, herauf wuchsest bis zu mir,", "tokens": ["he\u00b7ran", ",", "her\u00b7auf", "wuch\u00b7sest", "bis", "zu", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "ADV", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.35": {"text": "du wie die Menschheit uralte Seele,", "tokens": ["du", "wie", "die", "Menschheit", "ur\u00b7al\u00b7te", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOKOM", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "du, deren zahllose Wurzeln", "tokens": ["du", ",", "de\u00b7ren", "zahl\u00b7lo\u00b7se", "Wur\u00b7zeln"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PRELAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "saugend die ganze Erde umklammern,", "tokens": ["sau\u00b7gend", "die", "gan\u00b7ze", "Er\u00b7de", "um\u00b7klam\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.38": {"text": "schwankend vor Gl\u00fcck", "tokens": ["schwan\u00b7kend", "vor", "Gl\u00fcck"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPR", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.39": {"text": "schreit' ich mit deiner lieben Last", "tokens": ["schreit'", "ich", "mit", "dei\u00b7ner", "lie\u00b7ben", "Last"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "und kann noch nicht fassen,", "tokens": ["und", "kann", "noch", "nicht", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.41": {"text": "da\u00df grade ich", "tokens": ["da\u00df", "gra\u00b7de", "ich"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADV", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.42": {"text": "dein Werk, deine Frucht.", "tokens": ["dein", "Werk", ",", "dei\u00b7ne", "Frucht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}