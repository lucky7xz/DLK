{"textgrid.poem.24482": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Uber einen Hochtrabenden Poeten", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Grillen waren hoch/ sie giengen in die Lufft/", "tokens": ["Die", "Gril\u00b7len", "wa\u00b7ren", "hoch", "/", "sie", "gien\u00b7gen", "in", "die", "Lufft", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$(", "PPER", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mit den Wolcken fort; denn kam ein b\u00f6ser Dufft;", "tokens": ["Und", "mit", "den", "Wol\u00b7cken", "fort", ";", "denn", "kam", "ein", "b\u00f6\u00b7ser", "Dufft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PTKVZ", "$.", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da wuchsen Raupen draus. Kam di\u00df von ohngefehr?", "tokens": ["Da", "wuch\u00b7sen", "Rau\u00b7pen", "draus", ".", "Kam", "di\u00df", "von", "ohn\u00b7ge\u00b7fehr", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PTKVZ", "$.", "NE", "PDS", "APPR", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer fraget noch/ wo k\u00f6mt das Ungezieffer her.", "tokens": ["Wer", "fra\u00b7get", "noch", "/", "wo", "k\u00f6mt", "das", "Un\u00b7ge\u00b7zief\u00b7fer", "her", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "$("