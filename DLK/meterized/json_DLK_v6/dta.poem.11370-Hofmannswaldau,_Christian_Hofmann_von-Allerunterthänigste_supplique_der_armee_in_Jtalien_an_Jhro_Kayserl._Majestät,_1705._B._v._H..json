{"dta.poem.11370": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Allerunterth\u00e4nigste  supplique  der armee  \n in Jtalien an Jhro Kayserl.  \n Majest\u00e4t, 1705.  \n B. v. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gro\u00dfm\u00e4chtigster Monarch! verzeih, wenn tausend schmertzen", "tokens": ["Gro\u00df\u00b7m\u00e4ch\u00b7tigs\u00b7ter", "Mon\u00b7arch", "!", "ver\u00b7zeih", ",", "wenn", "tau\u00b7send", "schmert\u00b7zen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "VVIMP", "$,", "KOUS", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die scufftzer mit gewalt mir pressen aus dem hertzen:", "tokens": ["Die", "scufft\u00b7zer", "mit", "ge\u00b7walt", "mir", "pres\u00b7sen", "aus", "dem", "hert\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "NN", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn bleiche d\u00fcrfftigkeit die hand mit zittern regt,", "tokens": ["Wenn", "blei\u00b7che", "d\u00fcr\u00b7ff\u00b7tig\u00b7keit", "die", "hand", "mit", "zit\u00b7tern", "regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ART", "NN", "APPR", "VVINF", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und nichts als lauter noth zu deinen f\u00fcssen legt!", "tokens": ["Und", "nichts", "als", "lau\u00b7ter", "noth", "zu", "dei\u00b7nen", "f\u00fcs\u00b7sen", "legt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KOKOM", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vergieb, wenn mein gesang vielleicht zu traurig klinget,", "tokens": ["Ver\u00b7gieb", ",", "wenn", "mein", "ge\u00b7sang", "viel\u00b7leicht", "zu", "trau\u00b7rig", "klin\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "PPOSAT", "ADJD", "ADV", "PTKA", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Jtzund da jedermann die sieges-lieder singet!", "tokens": ["Jt\u00b7zund", "da", "je\u00b7der\u00b7mann", "die", "sie\u00b7ges\u00b7lie\u00b7der", "sin\u00b7get", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Man siehet offtermahls, da\u00df da cypressen stehn,", "tokens": ["Man", "sie\u00b7het", "off\u00b7ter\u00b7mahls", ",", "da\u00df", "da", "cyp\u00b7res\u00b7sen", "stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "$,", "KOUS", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Allwo die lorbeer-b\u00e4um ihr gr\u00fcnes haupt erh\u00f6hn:", "tokens": ["All\u00b7wo", "die", "lor\u00b7beer\u00b7b\u00e4\u00b7um", "ihr", "gr\u00fc\u00b7nes", "haupt", "er\u00b7h\u00f6hn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Da\u00df auf den hellen klang der schallenden trompeten", "tokens": ["Da\u00df", "auf", "den", "hel\u00b7len", "klang", "der", "schal\u00b7len\u00b7den", "trom\u00b7pe\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Bisweilen uns gef\u00e4llt der thon der sanfften fl\u00f6ten.", "tokens": ["Bis\u00b7wei\u00b7len", "uns", "ge\u00b7f\u00e4llt", "der", "thon", "der", "sanff\u00b7ten", "fl\u00f6\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Begl\u00fcckter Leopold! wer dein verh\u00e4ngni\u00df kennt,", "tokens": ["Be\u00b7gl\u00fcck\u00b7ter", "Leo\u00b7pold", "!", "wer", "dein", "ver\u00b7h\u00e4ng\u00b7ni\u00df", "kennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "PWS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Mu\u00df sagen, da\u00df dich GOtt zum muster hat ernennt,", "tokens": ["Mu\u00df", "sa\u00b7gen", ",", "da\u00df", "dich", "Gott", "zum", "mus\u00b7ter", "hat", "er\u00b7nennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "KOUS", "PPER", "NN", "APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der lasterhafften welt in deinem gl\u00fcck zu zeigen:", "tokens": ["Der", "las\u00b7ter\u00b7haff\u00b7ten", "welt", "in", "dei\u00b7nem", "gl\u00fcck", "zu", "zei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df nur die tugend kan das ungl\u00fcck \u00fcbersteigen.", "tokens": ["Da\u00df", "nur", "die", "tu\u00b7gend", "kan", "das", "un\u00b7gl\u00fcck", "\u00fc\u00b7bers\u00b7tei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Durch diesen wunder-glantz, der deine seele ziert,", "tokens": ["Durch", "die\u00b7sen", "wun\u00b7der\u00b7glantz", ",", "der", "dei\u00b7ne", "see\u00b7le", "ziert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wird jedes hertz entz\u00fcndt, wird jeder geist ger\u00fchrt,", "tokens": ["Wird", "je\u00b7des", "hertz", "ent\u00b7z\u00fcndt", ",", "wird", "je\u00b7der", "geist", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVPP", "$,", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und dein beruffnes lob so weit und hoch getrieben,", "tokens": ["Und", "dein", "be\u00b7ruff\u00b7nes", "lob", "so", "weit", "und", "hoch", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ADV", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da\u00df man gezwungen ist, o Kayser! dich zu lieben.", "tokens": ["Da\u00df", "man", "ge\u00b7zwun\u00b7gen", "ist", ",", "o", "Kay\u00b7ser", "!", "dich", "zu", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVPP", "VAFIN", "$,", "FM", "NN", "$.", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wo w\u00e4r am Donau-strom ein Protestanten-heer,", "tokens": ["Wo", "w\u00e4r", "am", "Do\u00b7nau\u00b7strom", "ein", "Pro\u00b7tes\u00b7tan\u00b7ten\u00b7heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wenn diese liebe nicht sein nord-gestirne w\u00e4r?", "tokens": ["Wenn", "die\u00b7se", "lie\u00b7be", "nicht", "sein", "nord\u00b7ge\u00b7stir\u00b7ne", "w\u00e4r", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VVFIN", "PTKNEG", "PPOSAT", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Will ein verblendter f\u00fcrst der crone dich berauben,", "tokens": ["Will", "ein", "ver\u00b7blend\u00b7ter", "f\u00fcrst", "der", "cro\u00b7ne", "dich", "be\u00b7rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "ADV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So gilt kein unterschied, kein zancken mehr im glauben.", "tokens": ["So", "gilt", "kein", "un\u00b7ter\u00b7schied", ",", "kein", "zan\u00b7cken", "mehr", "im", "glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,", "PIAT", "NN", "ADV", "APPRART", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Hier stirbt der unterthan, dort der getreue freund", "tokens": ["Hier", "stirbt", "der", "un\u00b7ter\u00b7than", ",", "dort", "der", "ge\u00b7treu\u00b7e", "freund"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "$,", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Mit solcher lust vor dich, da\u00df der und jener meint:", "tokens": ["Mit", "sol\u00b7cher", "lust", "vor", "dich", ",", "da\u00df", "der", "und", "je\u00b7ner", "meint", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "PPER", "$,", "KOUS", "ART", "KON", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Er k\u00f6nne l\u00f6blicher sein leben nicht beschliessen.", "tokens": ["Er", "k\u00f6n\u00b7ne", "l\u00f6b\u00b7li\u00b7cher", "sein", "le\u00b7ben", "nicht", "be\u00b7schlies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VAINF", "VVFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ein jeder sucht den tod; so da\u00df man nicht kan wissen,", "tokens": ["Ein", "je\u00b7der", "sucht", "den", "tod", ";", "so", "da\u00df", "man", "nicht", "kan", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ART", "NN", "$.", "ADV", "KOUS", "PIS", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Was auf dem sieges-platz mehr zu bedauren sey:", "tokens": ["Was", "auf", "dem", "sie\u00b7ges\u00b7platz", "mehr", "zu", "be\u00b7dau\u00b7ren", "sey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ob deiner freunde lieb? ob deiner knechte treu?", "tokens": ["Ob", "dei\u00b7ner", "freun\u00b7de", "lieb", "?", "ob", "dei\u00b7ner", "knech\u00b7te", "treu", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "$.", "KOUS", "PPOSAT", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Was kan ich weiter mehr von dir, o Kayser! sagen?", "tokens": ["Was", "kan", "ich", "wei\u00b7ter", "mehr", "von", "dir", ",", "o", "Kay\u00b7ser", "!", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "$,", "FM", "NN", "$.", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Als da\u00df mein fremdes volck, durch tapfferkeit getragen,", "tokens": ["Als", "da\u00df", "mein", "frem\u00b7des", "volck", ",", "durch", "tapf\u00b7fer\u00b7keit", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPOSAT", "ADJA", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Von hundert meilen kommt, zu st\u00fctzen deinen thron,", "tokens": ["Von", "hun\u00b7dert", "mei\u00b7len", "kommt", ",", "zu", "st\u00fct\u00b7zen", "dei\u00b7nen", "thron", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "$,", "PTKZU", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und nichts als deine huld vor sich begehrt zum lohn.", "tokens": ["Und", "nichts", "als", "dei\u00b7ne", "huld", "vor", "sich", "be\u00b7gehrt", "zum", "lohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KOKOM", "PPOSAT", "NN", "APPR", "PRF", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Dort schreibt ein weiser mann: Da\u00df es nicht einen giebet,", "tokens": ["Dort", "schreibt", "ein", "wei\u00b7ser", "mann", ":", "Da\u00df", "es", "nicht", "ei\u00b7nen", "gie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "KOUS", "PPER", "PTKNEG", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Der sonder eigen-nutz allein die tugend liebet;", "tokens": ["Der", "son\u00b7der", "ei\u00b7gen\u00b7nutz", "al\u00b7lein", "die", "tu\u00b7gend", "lie\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und der im gegentheil den lastern widersteht,", "tokens": ["Und", "der", "im", "ge\u00b7gen\u00b7theil", "den", "las\u00b7tern", "wi\u00b7der\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPRART", "NN", "ART", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wenn ihr vergiffter lohn uns in der welt erh\u00f6ht.", "tokens": ["Wenn", "ihr", "ver\u00b7giff\u00b7ter", "lohn", "uns", "in", "der", "welt", "er\u00b7h\u00f6ht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ach! soltest du itzund, o kluger R\u00f6mer! leben!", "tokens": ["Ach", "!", "sol\u00b7test", "du", "it\u00b7zund", ",", "o", "klu\u00b7ger", "R\u00f6\u00b7mer", "!", "le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "VMFIN", "PPER", "ADV", "$,", "FM", "ADJA", "NN", "$.", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ich wei\u00df, du w\u00fcrdest selbst di\u00df sch\u00f6ne zeugni\u00df geben:", "tokens": ["Ich", "wei\u00df", ",", "du", "w\u00fcr\u00b7dest", "selbst", "di\u00df", "sch\u00f6\u00b7ne", "zeug\u00b7ni\u00df", "ge\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "PDS", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Da\u00df, wenn ein Leopold das kayserthum regiert,", "tokens": ["Da\u00df", ",", "wenn", "ein", "Leo\u00b7pold", "das", "kay\u00b7ser\u00b7thum", "re\u00b7giert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "ART", "NE", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "Die tugend nur allein uns zu der tugend f\u00fchrt.", "tokens": ["Die", "tu\u00b7gend", "nur", "al\u00b7lein", "uns", "zu", "der", "tu\u00b7gend", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wer aber ist einmahl zu deinem thron getreten,", "tokens": ["Wer", "a\u00b7ber", "ist", "ein\u00b7mahl", "zu", "dei\u00b7nem", "thron", "ge\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und hat vergebens dich, o Kayser! was gebeten?", "tokens": ["Und", "hat", "ver\u00b7ge\u00b7bens", "dich", ",", "o", "Kay\u00b7ser", "!", "was", "ge\u00b7be\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PPER", "$,", "FM", "NN", "$.", "PWS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Was man begehrt von dir, das wird uns gleich gew\u00e4hrt,", "tokens": ["Was", "man", "be\u00b7gehrt", "von", "dir", ",", "das", "wird", "uns", "gleich", "ge\u00b7w\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVPP", "APPR", "PPER", "$,", "PDS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Und offt noch mehr gew\u00e4hrt, als man von dir begehrt.", "tokens": ["Und", "offt", "noch", "mehr", "ge\u00b7w\u00e4hrt", ",", "als", "man", "von", "dir", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "VVPP", "$,", "KOUS", "PIS", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Du zeigest dich betr\u00fcbt, wenn eine stund verflossen,", "tokens": ["Du", "zei\u00b7gest", "dich", "be\u00b7tr\u00fcbt", ",", "wenn", "ei\u00b7ne", "stund", "ver\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "$,", "KOUS", "ART", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "In der dein unterthan nichts gutes hat genossen.", "tokens": ["In", "der", "dein", "un\u00b7ter\u00b7than", "nichts", "gu\u00b7tes", "hat", "ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "ADJD", "PIS", "ADJA", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Dein gros-gesinnter geist richt sich nach keiner zahl:", "tokens": ["Dein", "gros\u00b7ge\u00b7sinn\u00b7ter", "geist", "richt", "sich", "nach", "kei\u00b7ner", "zahl", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Er schencket ohne maas: Er giebet auf einmahl;", "tokens": ["Er", "schen\u00b7cket", "oh\u00b7ne", "maas", ":", "Er", "gie\u00b7bet", "auf", "ein\u00b7mahl", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$.", "PPER", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Drum mu\u00df man billich ihn der sonnen \u00e4hnlich preisen,", "tokens": ["Drum", "mu\u00df", "man", "bil\u00b7lich", "ihn", "der", "son\u00b7nen", "\u00e4hn\u00b7lich", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PIS", "ADJD", "PPER", "ART", "ADJA", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Die allzeit unzertheilt der welt sich pflegt zu weisen,", "tokens": ["Die", "all\u00b7zeit", "un\u00b7zer\u00b7theilt", "der", "welt", "sich", "pflegt", "zu", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ART", "NN", "PRF", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Und etwan ihren glantz nicht einem nur gew\u00e4hrt,", "tokens": ["Und", "et\u00b7wan", "ih\u00b7ren", "glantz", "nicht", "ei\u00b7nem", "nur", "ge\u00b7w\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "PTKNEG", "ART", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Nein, sondern ihre gunst auf alle menschen kehrt.", "tokens": ["Nein", ",", "son\u00b7dern", "ih\u00b7re", "gunst", "auf", "al\u00b7le", "men\u00b7schen", "kehrt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KON", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Auf alle? Doch was angst hat mir den sinn benommen!", "tokens": ["Auf", "al\u00b7le", "?", "Doch", "was", "angst", "hat", "mir", "den", "sinn", "be\u00b7nom\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$.", "KON", "PWS", "VVPP", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Ich will, und kan doch nicht, ich kan nicht weiter kommen.", "tokens": ["Ich", "will", ",", "und", "kan", "doch", "nicht", ",", "ich", "kan", "nicht", "wei\u00b7ter", "kom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KON", "VMFIN", "ADV", "PTKNEG", "$,", "PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Auf alle? Z\u00fcrne nicht, wenn die erinnrung mir", "tokens": ["Auf", "al\u00b7le", "?", "Z\u00fcr\u00b7ne", "nicht", ",", "wenn", "die", "e\u00b7rinn\u00b7rung", "mir"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "$.", "NN", "PTKNEG", "$,", "KOUS", "ART", "NN", "PPER"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.56": {"text": "Bey diesen worten itzt stellt meinen jammer f\u00fcr.", "tokens": ["Bey", "die\u00b7sen", "wor\u00b7ten", "itzt", "stellt", "mei\u00b7nen", "jam\u00b7mer", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Wenn ich vor deinem thron erblasset niederfalle,", "tokens": ["Wenn", "ich", "vor", "dei\u00b7nem", "thron", "er\u00b7blas\u00b7set", "nie\u00b7der\u00b7fal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Und kl\u00e4glich seufftzen mu\u00df: Auf uns nicht; sonst auf alle.", "tokens": ["Und", "kl\u00e4g\u00b7lich", "seufft\u00b7zen", "mu\u00df", ":", "Auf", "uns", "nicht", ";", "sonst", "auf", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "VMFIN", "$.", "APPR", "PPER", "PTKNEG", "$.", "ADV", "APPR", "PIS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Gerechter Leopold! ach! bist du allgemein:", "tokens": ["Ge\u00b7rech\u00b7ter", "Leo\u00b7pold", "!", "ach", "!", "bist", "du", "all\u00b7ge\u00b7mein", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "XY", "$.", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.60": {"text": "Wie kommt es denn, da\u00df wir von dir vergessen seyn?", "tokens": ["Wie", "kommt", "es", "denn", ",", "da\u00df", "wir", "von", "dir", "ver\u00b7ges\u00b7sen", "seyn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Doch schwartzer wolcken-dunst kan sonnen-klarheit schw\u00e4rtzen;", "tokens": ["Doch", "schwart\u00b7zer", "wol\u00b7cken\u00b7dunst", "kan", "son\u00b7nen\u00b7kla\u00b7rheit", "schw\u00e4rt\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Ich f\u00fcrchte gleiche noth bey deinem vater-hertzen:", "tokens": ["Ich", "f\u00fcrch\u00b7te", "glei\u00b7che", "noth", "bey", "dei\u00b7nem", "va\u00b7ter\u00b7hert\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Ich f\u00fcrchte, da\u00df der sturm des neides uns ber\u00fchrt,", "tokens": ["Ich", "f\u00fcrch\u00b7te", ",", "da\u00df", "der", "sturm", "des", "nei\u00b7des", "uns", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "ART", "ADJA", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Und unsre seufftzer weit von deinen ohren f\u00fchrt.", "tokens": ["Und", "uns\u00b7re", "seufft\u00b7zer", "weit", "von", "dei\u00b7nen", "oh\u00b7ren", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Drum, blasse schmertzen! kommt verhungert und zerrissen!", "tokens": ["Drum", ",", "blas\u00b7se", "schmert\u00b7zen", "!", "kommt", "ver\u00b7hun\u00b7gert", "und", "zer\u00b7ris\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "ADJA", "VVINF", "$.", "VVFIN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Begleitet mich itzund zu meines kaysers f\u00fcssen!", "tokens": ["Be\u00b7glei\u00b7tet", "mich", "it\u00b7zund", "zu", "mei\u00b7nes", "kay\u00b7sers", "f\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Schau, grosser Potentat! di\u00df ist die krieges-schaar,", "tokens": ["Schau", ",", "gros\u00b7ser", "Po\u00b7ten\u00b7tat", "!", "di\u00df", "ist", "die", "krie\u00b7ge\u00b7sschaar", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$.", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Die vor so gros, so sch\u00f6n, und so gef\u00fcrchtet war.", "tokens": ["Die", "vor", "so", "gros", ",", "so", "sch\u00f6n", ",", "und", "so", "ge\u00b7f\u00fcrch\u00b7tet", "war", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "KON", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Betrachte, was wir seynd, und was wir seynd gewesen!", "tokens": ["Be\u00b7trach\u00b7te", ",", "was", "wir", "seynd", ",", "und", "was", "wir", "seynd", "ge\u00b7we\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VAFIN", "$,", "KON", "PWS", "PPER", "VAFIN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "Ein volck, das C\u00e4sar selbst zum heer h\u00e4tt\u2019 auserlesen;", "tokens": ["Ein", "volck", ",", "das", "C\u00e4\u00b7sar", "selbst", "zum", "heer", "h\u00e4tt'", "au\u00b7ser\u00b7le\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "NE", "ADV", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "Jtzund ein schatten-bild, das um die gr\u00e4ber schwebt,", "tokens": ["Jt\u00b7zund", "ein", "schat\u00b7ten\u00b7bild", ",", "das", "um", "die", "gr\u00e4\u00b7ber", "schwebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "Und nur aus liebe noch zu dir ein wenig lebt.", "tokens": ["Und", "nur", "aus", "lie\u00b7be", "noch", "zu", "dir", "ein", "we\u00b7nig", "lebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "VVFIN", "ADV", "APPR", "PPER", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.73": {"text": "Di\u00df sind wir, wenn wir nichts als unser elend schauen:", "tokens": ["Di\u00df", "sind", "wir", ",", "wenn", "wir", "nichts", "als", "un\u00b7ser", "e\u00b7lend", "schau\u00b7en", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "KOUS", "PPER", "PIS", "KOKOM", "PPOSAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.74": {"text": "Wann, n\u00e4her uns zu sehn, die feinde sich nicht trauen;", "tokens": ["Wann", ",", "n\u00e4\u00b7her", "uns", "zu", "sehn", ",", "die", "fein\u00b7de", "sich", "nicht", "trau\u00b7en", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "ADJD", "PPER", "PTKZU", "VVINF", "$,", "ART", "NN", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.75": {"text": "Doch wenn man lermen schl\u00e4gt, und zu den waffen schreyt:", "tokens": ["Doch", "wenn", "man", "ler\u00b7men", "schl\u00e4gt", ",", "und", "zu", "den", "waf\u00b7fen", "schreyt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "VVINF", "VVFIN", "$,", "KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.76": {"text": "So sieht man durch die krafft der wahren tapfferkeit", "tokens": ["So", "sieht", "man", "durch", "die", "krafft", "der", "wah\u00b7ren", "tapf\u00b7fer\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.77": {"text": "Di\u00df matte todten-bild zum wunderwerck der erden,", "tokens": ["Di\u00df", "mat\u00b7te", "tod\u00b7ten\u00b7bild", "zum", "wun\u00b7der\u00b7werck", "der", "er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.78": {"text": "Ein unbesiegtes heer von lauter helden werden.", "tokens": ["Ein", "un\u00b7be\u00b7sieg\u00b7tes", "heer", "von", "lau\u00b7ter", "hel\u00b7den", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.79": {"text": "Ach! k\u00f6nte di\u00df dein aug, o Kayser! schauen an,", "tokens": ["Ach", "!", "k\u00f6n\u00b7te", "di\u00df", "dein", "aug", ",", "o", "Kay\u00b7ser", "!", "schau\u00b7en", "an", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "VMFIN", "PDS", "PPOSAT", "NN", "$,", "FM", "NN", "$.", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.80": {"text": "Was in derselben zeit die liebe vor dich kan!", "tokens": ["Was", "in", "der\u00b7sel\u00b7ben", "zeit", "die", "lie\u00b7be", "vor", "dich", "kan", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PDAT", "NN", "ART", "VVFIN", "APPR", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.81": {"text": "Der kaum den schwachen leib vor hunger konnt\u2019 ertragen,", "tokens": ["Der", "kaum", "den", "schwa\u00b7chen", "leib", "vor", "hun\u00b7ger", "konnt'", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.82": {"text": "Der laufft, der freuet sich, vor dich sein blut zu wagen;", "tokens": ["Der", "laufft", ",", "der", "freu\u00b7et", "sich", ",", "vor", "dich", "sein", "blut", "zu", "wa\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PRELS", "VVFIN", "PRF", "$,", "APPR", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.83": {"text": "Doch kommen wir zur\u00fcck, als sieger aus dem streit,", "tokens": ["Doch", "kom\u00b7men", "wir", "zu\u00b7r\u00fcck", ",", "als", "sie\u00b7ger", "aus", "dem", "streit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.84": {"text": "So m\u00fcssen wir an statt der s\u00fcssen fr\u00f6lichkeit,", "tokens": ["So", "m\u00fcs\u00b7sen", "wir", "an", "statt", "der", "s\u00fcs\u00b7sen", "fr\u00f6\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.85": {"text": "Die allezeit der lohn der \u00fcberwinder blieben,", "tokens": ["Die", "al\u00b7le\u00b7zeit", "der", "lohn", "der", "\u00fc\u00b7berw\u00b7in\u00b7der", "blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.86": {"text": "Beweinen, da\u00df uns nicht der tod hat aufgerieben.", "tokens": ["Be\u00b7wei\u00b7nen", ",", "da\u00df", "uns", "nicht", "der", "tod", "hat", "auf\u00b7ge\u00b7rie\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PTKNEG", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.87": {"text": "Hier lieget der soldat, durch hunger abgematt,", "tokens": ["Hier", "lie\u00b7get", "der", "sol\u00b7dat", ",", "durch", "hun\u00b7ger", "ab\u00b7ge\u00b7matt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.88": {"text": "Und stirbt, dieweil er nichts, den leib zu st\u00e4rcken, hat.", "tokens": ["Und", "stirbt", ",", "die\u00b7weil", "er", "nichts", ",", "den", "leib", "zu", "st\u00e4r\u00b7cken", ",", "hat", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PIS", "$,", "ART", "NN", "PTKZU", "VVINF", "$,", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.89": {"text": "Dort sitzt der officier, und tr\u00e4ncket sich mit z\u00e4hren,", "tokens": ["Dort", "sitzt", "der", "of\u00b7fi\u00b7cier", ",", "und", "tr\u00e4n\u00b7cket", "sich", "mit", "z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "$,", "KON", "VVFIN", "PRF", "APPR", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.90": {"text": "Dieweil er nicht mehr wei\u00df, wie er sich soll ern\u00e4hren.", "tokens": ["Die\u00b7weil", "er", "nicht", "mehr", "wei\u00df", ",", "wie", "er", "sich", "soll", "er\u00b7n\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "$,", "PWAV", "PPER", "PRF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.91": {"text": "Barmhertzger Leopold! beweget dich dann nicht", "tokens": ["Barm\u00b7hertz\u00b7ger", "Leo\u00b7pold", "!", "be\u00b7we\u00b7get", "dich", "dann", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "$.", "VVFIN", "PPER", "ADV", "PTKNEG"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.92": {"text": "Mein jammer, welcher itzt in diesen zeilen spricht?", "tokens": ["Mein", "jam\u00b7mer", ",", "wel\u00b7cher", "itzt", "in", "die\u00b7sen", "zei\u00b7len", "spricht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.93": {"text": "Kanst du wohl sehen zu, da\u00df die aus noth verderben,", "tokens": ["Kanst", "du", "wohl", "se\u00b7hen", "zu", ",", "da\u00df", "die", "aus", "noth", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "PTKVZ", "$,", "KOUS", "ART", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.94": {"text": "Die tausendmahl vor dich aus liebe wolten sterben?", "tokens": ["Die", "tau\u00b7send\u00b7mahl", "vor", "dich", "aus", "lie\u00b7be", "wol\u00b7ten", "ster\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PRF", "APPR", "ADJA", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.95": {"text": "Ich bitt, o Kayser! dich, um das getreue blut,", "tokens": ["Ich", "bitt", ",", "o", "Kay\u00b7ser", "!", "dich", ",", "um", "das", "ge\u00b7treu\u00b7e", "blut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "FM", "NN", "$.", "PPER", "$,", "KOUI", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.96": {"text": "So deine fiege cr\u00f6nt, um diese thr\u00e4nen-fluth,", "tokens": ["So", "dei\u00b7ne", "fie\u00b7ge", "cr\u00f6nt", ",", "um", "die\u00b7se", "thr\u00e4\u00b7nen\u00b7fluth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$,", "KOUI", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.97": {"text": "So meine wangen netzt; Ach! la\u00df uns einmahl schauen,", "tokens": ["So", "mei\u00b7ne", "wan\u00b7gen", "netzt", ";", "Ach", "!", "la\u00df", "uns", "ein\u00b7mahl", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$.", "ITJ", "$.", "VVIMP", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.98": {"text": "Da\u00df wir vergebens nicht auf deine g\u00fcte bauen:", "tokens": ["Da\u00df", "wir", "ver\u00b7ge\u00b7bens", "nicht", "auf", "dei\u00b7ne", "g\u00fc\u00b7te", "bau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.99": {"text": "Damit der schlu\u00df hinfort ein helles lust-geschrey,", "tokens": ["Da\u00b7mit", "der", "schlu\u00df", "hin\u00b7fort", "ein", "hel\u00b7les", "lust\u00b7ge\u00b7schrey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.100": {"text": "Und nicht so, wie itzund, ein bittres seufftzen sey!", "tokens": ["Und", "nicht", "so", ",", "wie", "it\u00b7zund", ",", "ein", "bitt\u00b7res", "seufft\u00b7zen", "sey", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "$,", "PWAV", "ADV", "$,", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}