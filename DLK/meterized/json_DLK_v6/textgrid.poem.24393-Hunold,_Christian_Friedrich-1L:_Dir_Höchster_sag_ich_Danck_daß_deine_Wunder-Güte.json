{"textgrid.poem.24393": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dir/ H\u00f6chster/ sag ich Danck/ da\u00df deine Wunder-G\u00fcte", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir/ H\u00f6chster/ sag ich Danck/ da\u00df deine Wunder-G\u00fcte", "tokens": ["Dir", "/", "H\u00f6chs\u00b7ter", "/", "sag", "ich", "Danck", "/", "da\u00df", "dei\u00b7ne", "Wun\u00b7der\u00b7G\u00fc\u00b7te"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$(", "NN", "$(", "VVIMP", "PPER", "NN", "$(", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vern\u00fcnfftig und gesund mich auf die Welt gebracht.", "tokens": ["Ver\u00b7n\u00fcnff\u00b7tig", "und", "ge\u00b7sund", "mich", "auf", "die", "Welt", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vergib mir aber auch/ wenn sonsten mein Gem\u00fcthe", "tokens": ["Ver\u00b7gib", "mir", "a\u00b7ber", "auch", "/", "wenn", "sons\u00b7ten", "mein", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "$(", "KOUS", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mehr an die Welt/ als dich in dieser Zeit gedacht.", "tokens": ["Mehr", "an", "die", "Welt", "/", "als", "dich", "in", "die\u00b7ser", "Zeit", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$(", "KOUS", "PPER", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Schaff Hertz und Seele neu. Die angeerbte Lust", "tokens": ["Schaff", "Hertz", "und", "See\u00b7le", "neu", ".", "Die", "an\u00b7ge\u00b7erb\u00b7te", "Lust"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "KON", "NN", "ADJD", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wasch' ab/ und la\u00df in mir/ die H\u00f6lle nicht mehr lachen.", "tokens": ["Wasch'", "ab", "/", "und", "la\u00df", "in", "mir", "/", "die", "H\u00f6l\u00b7le", "nicht", "mehr", "la\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$(", "KON", "VVIMP", "APPR", "PPER", "$(", "ART", "NN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mein Jesus/ Michael/ beschirme Seel und Brust/", "tokens": ["Mein", "Je\u00b7sus", "/", "Mic\u00b7hael", "/", "be\u00b7schir\u00b7me", "Seel", "und", "Brust", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "$(", "NE", "$(", "ADJA", "NN", "KON", "NN", "$("], "meter": "----+-+-+-+", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Ihr Engel siegt in mir von neuen \u00fcbern Drachen.", "tokens": ["Ihr", "En\u00b7gel", "siegt", "in", "mir", "von", "neu\u00b7en", "\u00fc\u00b7bern", "Dra\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}