{"textgrid.poem.40924": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bald denki, 's isch e b\u00f6si Zit,", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bald denki, 's isch e b\u00f6si Zit,", "tokens": ["Bald", "den\u00b7ki", ",", "'s", "isch", "e", "b\u00f6\u00b7si", "Zit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "$,", "PPER", "FM", "FM", "FM", "FM", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "und weger 's End isch n\u00fcmme wit;", "tokens": ["und", "we\u00b7ger", "'s", "End", "isch", "n\u00fcm\u00b7me", "wit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "NN", "FM", "FM", "FM", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "bald denki wieder: lo\u00df es goh,", "tokens": ["bald", "den\u00b7ki", "wie\u00b7der", ":", "lo\u00df", "es", "goh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$.", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wenn's gnug isch, wird's schon anderst cho.", "tokens": ["wenn's", "gnug", "isch", ",", "wird's", "schon", "an\u00b7derst", "cho", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "$,", "VAFIN", "ADV", "ADV", "NE", "$."], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.5": {"text": "Doch wenni n\u00e4umen ane gang", "tokens": ["Doch", "wen\u00b7ni", "n\u00e4u\u00b7men", "a\u00b7ne", "gang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und 's t\u00f6nt mer Lied und Vogelsang,", "tokens": ["und", "'s", "t\u00f6nt", "mer", "Lied", "und", "Vo\u00b7gel\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "so meini fast, i h\u00f6r e Stimm:", "tokens": ["so", "mei\u00b7ni", "fast", ",", "i", "h\u00f6r", "e", "Stimm", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$,", "FM", "FM", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbbis z'fride! 's isch jo nit so schlimm.\u00ab", "tokens": ["\u00bb", "bis", "z'\u00b7fri\u00b7de", "!", "'s", "isch", "jo", "nit", "so", "schlimm", ".", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "NE", "$.", "PPER", "ADJD", "NE", "PTKNEG", "ADV", "ADJD", "$.", "$("], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}}}}