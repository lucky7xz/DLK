{"textgrid.poem.44780": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "In das Stammbuch eines S\u00e4ngers", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbkunst sei nur ein heitrer Scherz\u00ab,", "tokens": ["\u00bb", "kunst", "sei", "nur", "ein", "hei\u00b7trer", "Scherz", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VAFIN", "ADV", "ART", "ADJA", "NN", "$(", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.2": {"text": "K\u00fcnstler sprachen dies!", "tokens": ["K\u00fcnst\u00b7ler", "spra\u00b7chen", "dies", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PDS", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Doch bei dir h\u00e4lt treu das Herz,", "tokens": ["Doch", "bei", "dir", "h\u00e4lt", "treu", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was der Mund verhie\u00df.", "tokens": ["Was", "der", "Mund", "ver\u00b7hie\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}