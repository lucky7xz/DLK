{"textgrid.poem.64834": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich liebte dich, doch meine Liebe,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich liebte dich, doch meine Liebe,", "tokens": ["Ich", "lieb\u00b7te", "dich", ",", "doch", "mei\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie hat dir Kummer nur gebracht,", "tokens": ["Sie", "hat", "dir", "Kum\u00b7mer", "nur", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dunkler Wetterwolken Tr\u00fcbe", "tokens": ["Und", "dunk\u00b7ler", "Wet\u00b7ter\u00b7wol\u00b7ken", "Tr\u00fc\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Deines Himmels Sonnenpracht.", "tokens": ["Auf", "Dei\u00b7nes", "Him\u00b7mels", "Son\u00b7nen\u00b7pracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Unf\u00e4hig, das Gef\u00fchl zu theilen,", "tokens": ["Un\u00b7f\u00e4\u00b7hig", ",", "das", "Ge\u00b7f\u00fchl", "zu", "thei\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Das mich empor zum Himmel trug,", "tokens": ["Das", "mich", "em\u00b7por", "zum", "Him\u00b7mel", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PTKVZ", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verfolgtest du mit Witzespfeilen", "tokens": ["Ver\u00b7folg\u00b7test", "du", "mit", "Wit\u00b7ze\u00b7spfei\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich auf dem sel'gen Heimathszug.", "tokens": ["Mich", "auf", "dem", "sel'\u00b7gen", "Hei\u00b7maths\u00b7zug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und solche scharfe Pfeile dringen", "tokens": ["Und", "sol\u00b7che", "schar\u00b7fe", "Pfei\u00b7le", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Selbst bis zum fernen Himmelsplan,", "tokens": ["Selbst", "bis", "zum", "fer\u00b7nen", "Him\u00b7mel\u00b7splan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df blutend mit gebrochnen Schwingen,", "tokens": ["Da\u00df", "blu\u00b7tend", "mit", "ge\u00b7broch\u00b7nen", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Man draus zu Boden st\u00fcrzen kann.", "tokens": ["Man", "draus", "zu", "Bo\u00b7den", "st\u00fcr\u00b7zen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PAV", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So st\u00fcrzte ich; \u2013 doch als du, J\u00e4ger!", "tokens": ["So", "st\u00fcrz\u00b7te", "ich", ";", "\u2013", "doch", "als", "du", ",", "J\u00e4\u00b7ger", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "ADV", "KOUS", "PPER", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sahst meiner Wunden Flammenpein,", "tokens": ["Sahst", "mei\u00b7ner", "Wun\u00b7den", "Flam\u00b7men\u00b7pein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da wolltest Heiler du und Pfleger", "tokens": ["Da", "woll\u00b7test", "Hei\u00b7ler", "du", "und", "Pfle\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "NE", "NE", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der von dir so Verletzten sein.", "tokens": ["Der", "von", "dir", "so", "Ver\u00b7letz\u00b7ten", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "ADV", "NN", "VAINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "La\u00df ab! Geh\u00f6r ich zu den Schwachen,", "tokens": ["La\u00df", "ab", "!", "Ge\u00b7h\u00f6r", "ich", "zu", "den", "Schwa\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$.", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die, schmeichelnd, man mit einem Wort", "tokens": ["Die", ",", "schmei\u00b7chelnd", ",", "man", "mit", "ei\u00b7nem", "Wort"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "ADJD", "$,", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf immer kann vergessen machen,", "tokens": ["Auf", "im\u00b7mer", "kann", "ver\u00b7ges\u00b7sen", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VMFIN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df man ihr Herz gequ\u00e4lt, durchbohrt?", "tokens": ["Da\u00df", "man", "ihr", "Herz", "ge\u00b7qu\u00e4lt", ",", "durch\u00b7bohrt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ein Blut, de\u00df K\u00f6n'ge sich nicht sch\u00e4men \u2013", "tokens": ["Ein", "Blut", ",", "de\u00df", "K\u00f6n'\u00b7ge", "sich", "nicht", "sch\u00e4\u00b7men", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PRF", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In meinen Adern sch\u00e4umt's und rollt's;", "tokens": ["In", "mei\u00b7nen", "A\u00b7dern", "sch\u00e4umt's", "und", "rollt's", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "KON", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Mitleids Gabe anzunehmen", "tokens": ["Des", "Mit\u00b7leids", "Ga\u00b7be", "an\u00b7zu\u00b7neh\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVIZU"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bin ich zu edel und zu stolz.", "tokens": ["Bin", "ich", "zu", "e\u00b7del", "und", "zu", "stolz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKA", "ADJD", "KON", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Mu\u00df ich doch meine W\u00fcrde wahren!", "tokens": ["Mu\u00df", "ich", "doch", "mei\u00b7ne", "W\u00fcr\u00b7de", "wah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es ziemt sich f\u00fcr ein K\u00f6nigsherz,", "tokens": ["Es", "ziemt", "sich", "f\u00fcr", "ein", "K\u00f6\u00b7nigs\u00b7herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das seine Gr\u00f6\u00df' will offenbaren,", "tokens": ["Das", "sei\u00b7ne", "Gr\u00f6\u00df'", "will", "of\u00b7fen\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur h\u00f6chste Lust, nur tiefster Schmerz.", "tokens": ["Nur", "h\u00f6chs\u00b7te", "Lust", ",", "nur", "tiefs\u00b7ter", "Schmerz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und f\u00fcr den Geist, den liederreichen,", "tokens": ["Und", "f\u00fcr", "den", "Geist", ",", "den", "lie\u00b7der\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ziemt sich nur blaue Himmelsluft;", "tokens": ["Ziemt", "sich", "nur", "blau\u00b7e", "Him\u00b7mel\u00b7sluft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADJA", "NN", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kann er die nicht mehr frei durchstreichen,", "tokens": ["Kann", "er", "die", "nicht", "mehr", "frei", "durch\u00b7strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "PTKNEG", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So sink' er lieber in die Gruft,", "tokens": ["So", "sink'", "er", "lie\u00b7ber", "in", "die", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Als, da\u00df mit klagender Geberde,", "tokens": ["Als", ",", "da\u00df", "mit", "kla\u00b7gen\u00b7der", "Ge\u00b7ber\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Entw\u00fcrdiget, ein l\u00e4st'ger Gast,", "tokens": ["Ent\u00b7w\u00fcr\u00b7di\u00b7get", ",", "ein", "l\u00e4st'\u00b7ger", "Gast", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Er auf der feindlich rauhen Erde", "tokens": ["Er", "auf", "der", "feind\u00b7lich", "rau\u00b7hen", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fortschleppe seines Lebens Last.", "tokens": ["Fort\u00b7schlep\u00b7pe", "sei\u00b7nes", "Le\u00b7bens", "Last", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}