{"textgrid.poem.31655": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schaff / ach schaff / O Herrscher / h\u00fclff / da\u00df man Herz-getrost kan lehren.", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schaff / ach schaff / O Herrscher / h\u00fclff / da\u00df man Herz-getrost kan lehren.", "tokens": ["Schaff", "/", "ach", "schaff", "/", "O", "Herr\u00b7scher", "/", "h\u00fclff", "/", "da\u00df", "man", "Her\u00b7zge\u00b7trost", "kan", "leh\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "XY", "XY", "$(", "NE", "NN", "$(", "NE", "$(", "KOUS", "PIS", "NN", "VMFIN", "VVINF", "$."], "meter": "----+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Feur-begeiste Flammen-Zungen gib' / in Andacht-kalter Zeit /", "tokens": ["Feur\u00b7be\u00b7geis\u00b7te", "Flam\u00b7men\u00b7Zun\u00b7gen", "gib'", "/", "in", "An\u00b7dacht\u00b7kal\u00b7ter", "Zeit", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$(", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "der verfelsten Menschen Sinn glauben-kr\u00e4fftig zu bekehren.", "tokens": ["der", "ver\u00b7fels\u00b7ten", "Men\u00b7schen", "Sinn", "glau\u00b7ben\u00b7kr\u00e4ff\u00b7tig", "zu", "be\u00b7keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wann der minste Allmacht-Strahle leucht in Herzen D\u00fcsterkeit /", "tokens": ["Wann", "der", "mins\u00b7te", "All\u00b7macht\u00b7Strah\u00b7le", "leucht", "in", "Her\u00b7zen", "D\u00fcs\u00b7ter\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "wird sie gleich der Nacht verjagt / und das Lehr-Liecht zu erlangen", "tokens": ["wird", "sie", "gleich", "der", "Nacht", "ver\u00b7jagt", "/", "und", "das", "Lehr\u00b7Liecht", "zu", "er\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$(", "KON", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+--++--+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "eine starke Herzen-Gier in der Seelen angefangen /", "tokens": ["ei\u00b7ne", "star\u00b7ke", "Her\u00b7zen\u00b7Gier", "in", "der", "See\u00b7len", "an\u00b7ge\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "die / als wie die schmachtig Erden / wann ein Regen in sie fliest /", "tokens": ["die", "/", "als", "wie", "die", "schmach\u00b7tig", "Er\u00b7den", "/", "wann", "ein", "Re\u00b7gen", "in", "sie", "fliest", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "KOUS", "KOKOM", "ART", "ADJD", "NN", "$(", "PWAV", "ART", "NN", "APPR", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "wahrer Warheit Vnterweisung / gleichsam Geisterdurst / geniest.", "tokens": ["wah\u00b7rer", "War\u00b7heit", "Vn\u00b7ter\u00b7wei\u00b7sung", "/", "gleich\u00b7sam", "Geis\u00b7ter\u00b7durst", "/", "ge\u00b7niest", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$(", "ADJD", "NN", "$(", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Schaffe nie-gef\u00fchlten Durst nach den s\u00fcssen Sions-Quellen /", "tokens": ["Schaf\u00b7fe", "nie\u00b7ge\u00b7f\u00fchl\u00b7ten", "Durst", "nach", "den", "s\u00fcs\u00b7sen", "Si\u00b7ons\u00b7Quel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "da\u00df man / wie erhitzte Hirschen / nach dem Wasser schreyen mu\u00df:", "tokens": ["da\u00df", "man", "/", "wie", "er\u00b7hitz\u00b7te", "Hir\u00b7schen", "/", "nach", "dem", "Was\u00b7ser", "schre\u00b7yen", "mu\u00df", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$(", "KOKOM", "ADJA", "NN", "$(", "APPR", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "schaff' auch Gnaden-Wasser-Str\u00f6m h\u00f6chst-erg\u00f6tzlich unsern Kehlen.", "tokens": ["schaff'", "auch", "Gna\u00b7den\u00b7Was\u00b7ser\u00b7Str\u00f6m", "h\u00f6chst\u00b7er\u00b7g\u00f6tz\u00b7lich", "un\u00b7sern", "Keh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Wollst im Durst / die m\u00e4ng' uns geben: und den Durst / im Vberflu\u00df.", "tokens": ["Wollst", "im", "Durst", "/", "die", "m\u00e4ng'", "uns", "ge\u00b7ben", ":", "und", "den", "Durst", "/", "im", "Vberf\u00b7lu\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$(", "ART", "NN", "PPER", "VVINF", "$.", "KON", "ART", "NN", "$(", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}}}}