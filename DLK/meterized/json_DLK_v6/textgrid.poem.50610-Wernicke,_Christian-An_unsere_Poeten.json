{"textgrid.poem.50610": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "An unsere Poeten", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Dichter, wenn ein Vers aus eurer Feder quillt,", "tokens": ["Ihr", "Dich\u00b7ter", ",", "wenn", "ein", "Vers", "aus", "eu\u00b7rer", "Fe\u00b7der", "quillt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um eure Phyllis zu bedienen,", "tokens": ["Um", "eu\u00b7re", "Phyl\u00b7lis", "zu", "be\u00b7die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So zeigt sich gleich ein \u00bbMarmorbild\u00ab,", "tokens": ["So", "zeigt", "sich", "gleich", "ein", "\u00bb", "Mar\u00b7mor\u00b7bild", "\u00ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ART", "$(", "NN", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Aug' ist von \u00bbAchat\u00ab, die Lippen sind \u00bbRubinen\u00ab,", "tokens": ["Ihr", "Aug'", "ist", "von", "\u00bb", "A\u00b7chat", "\u00ab", ",", "die", "Lip\u00b7pen", "sind", "\u00bb", "Ru\u00b7bi\u00b7nen", "\u00ab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "$(", "FM.la", "$(", "$,", "ART", "NN", "VAFIN", "$(", "NN", "$(", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Die Adern aus \u00bbSaphir\u00ab gemacht,", "tokens": ["Die", "A\u00b7dern", "aus", "\u00bb", "Sa\u00b7phir", "\u00ab", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "APPR", "$(", "NE", "$(", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und eure Buhlschaft wird, weil ihr sie preist, verlacht.", "tokens": ["Und", "eu\u00b7re", "Buhl\u00b7schaft", "wird", ",", "weil", "ihr", "sie", "preist", ",", "ver\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Welschen sind zwar auch nicht klug,", "tokens": ["Die", "Wel\u00b7schen", "sind", "zwar", "auch", "nicht", "klug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Weil sie in einem weiten Flug", "tokens": ["Weil", "sie", "in", "ei\u00b7nem", "wei\u00b7ten", "Flug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mit einer G\u00f6ttinn stets bis an die Sterne fliegen;", "tokens": ["Mit", "ei\u00b7ner", "G\u00f6t\u00b7tinn", "stets", "bis", "an", "die", "Ster\u00b7ne", "flie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "In Frankreich macht man sie von lauter Geist und Witz,", "tokens": ["In", "Fran\u00b7kreich", "macht", "man", "sie", "von", "lau\u00b7ter", "Geist", "und", "Witz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PIS", "PPER", "APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Freundschaft f\u00e4hig und verschwiegen,", "tokens": ["Der", "Freund\u00b7schaft", "f\u00e4\u00b7hig", "und", "ver\u00b7schwie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Kurz ein Gef\u00e4\u00df ohn' einen", "tokens": ["Kurz", "ein", "Ge\u00b7f\u00e4\u00df", "ohn'", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "APPR", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "In England, wo sie schalt- und walten,", "tokens": ["In", "En\u00b7gland", ",", "wo", "sie", "schal\u00b7t", "und", "wal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PWAV", "PPER", "TRUNC", "KON", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Da werden sie f\u00fcr nichts, als Fleisch und Blut, gehalten;", "tokens": ["Da", "wer\u00b7den", "sie", "f\u00fcr", "nichts", ",", "als", "Fleisch", "und", "Blut", ",", "ge\u00b7hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PIS", "$,", "KOUS", "NN", "KON", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ihr aber, umgekehrt, wollt all' Pygmalions sein,", "tokens": ["Ihr", "a\u00b7ber", ",", "um\u00b7ge\u00b7kehrt", ",", "wollt", "all'", "Pyg\u00b7ma\u00b7li\u00b7ons", "sein", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJD", "$,", "VMFIN", "PIAT", "NN", "VAINF", "$,"], "meter": "-+-+-+--++--+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Denn eure Phyllis ist ein ", "tokens": ["Denn", "eu\u00b7re", "Phyl\u00b7lis", "ist", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}