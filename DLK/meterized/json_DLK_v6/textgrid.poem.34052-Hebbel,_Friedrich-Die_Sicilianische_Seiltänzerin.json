{"textgrid.poem.34052": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Sicilianische Seilt\u00e4nzerin", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00dfes, reizendes M\u00e4dchen! Du tanzest drinnen, doch drau\u00dfen", "tokens": ["S\u00fc\u00b7\u00dfes", ",", "rei\u00b7zen\u00b7des", "M\u00e4d\u00b7chen", "!", "Du", "tan\u00b7zest", "drin\u00b7nen", ",", "doch", "drau\u00b7\u00dfen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$.", "PPER", "VVFIN", "ADV", "$,", "ADV", "ADV"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Schl\u00e4gst du die Becken zuvor, da\u00df sich die Bude dir f\u00fcllt.", "tokens": ["Schl\u00e4gst", "du", "die", "Be\u00b7cken", "zu\u00b7vor", ",", "da\u00df", "sich", "die", "Bu\u00b7de", "dir", "f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$,", "KOUS", "PRF", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Roth ist dein Kleid, und es stechen davon die wei\u00dfen Korallen", "tokens": ["Roth", "ist", "dein", "Kleid", ",", "und", "es", "ste\u00b7chen", "da\u00b7von", "die", "wei\u00b7\u00dfen", "Ko\u00b7ral\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,", "KON", "PPER", "VVFIN", "PAV", "ART", "ADJA", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Zierlich ab, die du fein dir um das H\u00e4lschen geh\u00e4ngt.", "tokens": ["Zier\u00b7lich", "ab", ",", "die", "du", "fein", "dir", "um", "das", "H\u00e4l\u00b7schen", "ge\u00b7h\u00e4ngt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "PRELS", "PPER", "ADJD", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Aber wehe! Du lie\u00dfest die Schellen zu m\u00e4chtig ert\u00f6nen", "tokens": ["A\u00b7ber", "we\u00b7he", "!", "Du", "lie\u00b7\u00dfest", "die", "Schel\u00b7len", "zu", "m\u00e4ch\u00b7tig", "er\u00b7t\u00f6\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$.", "PPER", "VVFIN", "ART", "NN", "PTKA", "ADJD", "VVINF"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Und zerquetschtest dabei leider ein Perlchen der Schnur.", "tokens": ["Und", "zer\u00b7quetschtest", "da\u00b7bei", "lei\u00b7der", "ein", "Perl\u00b7chen", "der", "Schnur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.7": {"text": "Traurig senkst du das K\u00f6pfchen und blickst zur \u00e4lteren Schwester", "tokens": ["Trau\u00b7rig", "senkst", "du", "das", "K\u00f6pf\u00b7chen", "und", "blickst", "zur", "\u00e4l\u00b7te\u00b7ren", "Schwes\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Still hin\u00fcber und flehst stumm um ihr Mitleid sie an.", "tokens": ["Still", "hin\u00b7\u00fc\u00b7ber", "und", "flehst", "stumm", "um", "ihr", "Mit\u00b7leid", "sie", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "KON", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.9": {"text": "Doch sie l\u00e4chelt ver\u00e4chtlich, und dreht dir den R\u00fccken, und wirft ihr", "tokens": ["Doch", "sie", "l\u00e4\u00b7chelt", "ver\u00b7\u00e4cht\u00b7lich", ",", "und", "dreht", "dir", "den", "R\u00fc\u00b7cken", ",", "und", "wirft", "ihr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "$,", "KON", "VVFIN", "PPER", "ART", "NN", "$,", "KON", "VVFIN", "PPER"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.10": {"text": "Tambourin so in die Luft, da\u00df es, gefangen, zerspringt.", "tokens": ["Tam\u00b7bo\u00b7u\u00b7rin", "so", "in", "die", "Luft", ",", "da\u00df", "es", ",", "ge\u00b7fan\u00b7gen", ",", "zer\u00b7springt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "$,", "VVPP", "$,", "VVFIN", "$."], "meter": "+--+-+-+-+-+--+", "measure": "iambic.septa.invert"}, "line.11": {"text": "Aermste, ich kann sie versteh'n! Sie hat schon Be\u00dfres verloren,", "tokens": ["A\u00b7erms\u00b7te", ",", "ich", "kann", "sie", "ver\u00b7steh'n", "!", "Sie", "hat", "schon", "Be\u00df\u00b7res", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$.", "PPER", "VAFIN", "ADV", "NN", "VVPP", "$,"], "meter": "-+--+--+-+-+--+-", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Und dein kindlicher Schmerz um den zerschmetterten Tand,", "tokens": ["Und", "dein", "kind\u00b7li\u00b7cher", "Schmerz", "um", "den", "zer\u00b7schmet\u00b7ter\u00b7ten", "Tand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Der die Reinheit der Seele, die fleckenloseste, spiegelt,", "tokens": ["Der", "die", "Rein\u00b7heit", "der", "See\u00b7le", ",", "die", "fle\u00b7cken\u00b7lo\u00b7ses\u00b7te", ",", "spie\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.14": {"text": "Mahnt sie an deinen Besitz, ach! und an ihren Verlust!", "tokens": ["Mahnt", "sie", "an", "dei\u00b7nen", "Be\u00b7sitz", ",", "ach", "!", "und", "an", "ih\u00b7ren", "Ver\u00b7lust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,", "ITJ", "$.", "KON", "APPR", "PPOSAT", "NN", "$."], "meter": "++-+--+--+-+-+", "measure": "trochaic.septa.relaxed"}}}}}