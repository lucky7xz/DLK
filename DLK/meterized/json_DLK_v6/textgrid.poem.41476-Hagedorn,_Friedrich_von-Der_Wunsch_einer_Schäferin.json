{"textgrid.poem.41476": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Wunsch einer Sch\u00e4ferin", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort, wo im Thal die schlanken Erlen stehn,", "tokens": ["Dort", ",", "wo", "im", "Thal", "die", "schlan\u00b7ken", "Er\u00b7len", "stehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "APPRART", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hielt mich mein Sch\u00e4fer an, bei jenen frischen Quellen,", "tokens": ["Hielt", "mich", "mein", "Sch\u00e4\u00b7fer", "an", ",", "bei", "je\u00b7nen", "fri\u00b7schen", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und sprach: Geb\u00f6test du, mich wieder einzustellen,", "tokens": ["Und", "sprach", ":", "Ge\u00b7b\u00f6\u00b7test", "du", ",", "mich", "wie\u00b7der", "ein\u00b7zu\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "VVFIN", "PPER", "$,", "PPER", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du w\u00fcrdest mich f\u00fcr Liebe sterben sehn.", "tokens": ["Du", "w\u00fcr\u00b7dest", "mich", "f\u00fcr", "Lie\u00b7be", "ster\u00b7ben", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ach Liebe! kostet es auch unser beider Leben;", "tokens": ["Ach", "Lie\u00b7be", "!", "kos\u00b7tet", "es", "auch", "un\u00b7ser", "bei\u00b7der", "Le\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "VVFIN", "PPER", "ADV", "PPOSAT", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So la\u00df', o la\u00df' ihn doch sich wieder herbegeben!", "tokens": ["So", "la\u00df'", ",", "o", "la\u00df'", "ihn", "doch", "sich", "wie\u00b7der", "her\u00b7be\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "FM", "VVIMP", "PPER", "ADV", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}