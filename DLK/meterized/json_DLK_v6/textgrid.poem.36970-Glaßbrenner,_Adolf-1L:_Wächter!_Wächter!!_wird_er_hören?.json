{"textgrid.poem.36970": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: W\u00e4chter! W\u00e4chter!! wird er h\u00f6ren?", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4chter! W\u00e4chter!! wird er h\u00f6ren?", "tokens": ["W\u00e4ch\u00b7ter", "!", "W\u00e4ch\u00b7ter", "!!", "wird", "er", "h\u00f6\u00b7ren", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "$.", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist er nicht mehr in den Gassen?", "tokens": ["Ist", "er", "nicht", "mehr", "in", "den", "Gas\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat er uns mit seinem Horne", "tokens": ["Hat", "er", "uns", "mit", "sei\u00b7nem", "Hor\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dem scharfen Spie\u00df verlassen?", "tokens": ["Und", "dem", "schar\u00b7fen", "Spie\u00df", "ver\u00b7las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jetzt, wo noch die Diebe schleichen,", "tokens": ["Jetzt", ",", "wo", "noch", "die", "Die\u00b7be", "schlei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4tt' er sich davon gemacht?", "tokens": ["H\u00e4tt'", "er", "sich", "da\u00b7von", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PAV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sah' er schon den Tag der Freiheit", "tokens": ["Sah'", "er", "schon", "den", "Tag", "der", "Frei\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mitten in der tr\u00fcben Nacht?", "tokens": ["Mit\u00b7ten", "in", "der", "tr\u00fc\u00b7ben", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "W\u00e4chter! W\u00e4chter!! Sicher schl\u00e4ft er,", "tokens": ["W\u00e4ch\u00b7ter", "!", "W\u00e4ch\u00b7ter", "!!", "Si\u00b7cher", "schl\u00e4ft", "er", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "$.", "ADV", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie's die W\u00e4chter alle pflegen;", "tokens": ["Wie's", "die", "W\u00e4ch\u00b7ter", "al\u00b7le", "pfle\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NE", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil ich ihn so liebte, weck' ich", "tokens": ["Weil", "ich", "ihn", "so", "lieb\u00b7te", ",", "weck'", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "$,", "VVIMP", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den blasirten Freund, den tr\u00e4gen.", "tokens": ["Den", "bla\u00b7sir\u00b7ten", "Freund", ",", "den", "tr\u00e4\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Feuer! Feuer! W\u00e4chter helfe!", "tokens": ["Feu\u00b7er", "!", "Feu\u00b7er", "!", "W\u00e4ch\u00b7ter", "hel\u00b7fe", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NE", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sto\u00dfe wieder in Dein Horn!", "tokens": ["Sto\u00b7\u00dfe", "wie\u00b7der", "in", "Dein", "Horn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bl\u00e4st du nicht, so wird man glauben,", "tokens": ["Bl\u00e4st", "du", "nicht", ",", "so", "wird", "man", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "ADV", "VAFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df Dir zwei gewachsen vorn!", "tokens": ["Da\u00df", "Dir", "zwei", "ge\u00b7wach\u00b7sen", "vorn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "CARD", "VAPP", "ADV", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}}}}}