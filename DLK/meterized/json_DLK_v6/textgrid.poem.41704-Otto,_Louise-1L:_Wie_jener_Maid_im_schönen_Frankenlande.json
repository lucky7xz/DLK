{"textgrid.poem.41704": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie jener Maid im sch\u00f6nen Frankenlande", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie jener Maid im sch\u00f6nen Frankenlande", "tokens": ["Wie", "je\u00b7ner", "Maid", "im", "sch\u00f6\u00b7nen", "Fran\u00b7ken\u00b7lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PDAT", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die heilge Jungfrau einstens ist erschienen", "tokens": ["Die", "heil\u00b7ge", "Jung\u00b7frau", "eins\u00b7tens", "ist", "er\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sie vermocht ihr ewig treu zu dienen,", "tokens": ["Und", "sie", "ver\u00b7mocht", "ihr", "e\u00b7wig", "treu", "zu", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein zartes Weib im kriegrischen Gewande:", "tokens": ["Ein", "zar\u00b7tes", "Weib", "im", "krie\u00b7gri\u00b7schen", "Ge\u00b7wan\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So trat zu mir befreit vom Erdenbande", "tokens": ["So", "trat", "zu", "mir", "be\u00b7freit", "vom", "Er\u00b7den\u00b7ban\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "VVPP", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Muse mit den g\u00f6tterselgen Mienen,", "tokens": ["Die", "Mu\u00b7se", "mit", "den", "g\u00f6t\u00b7ter\u00b7sel\u00b7gen", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hat mich vermocht ihr ewig treu zu dienen,", "tokens": ["Hat", "mich", "ver\u00b7mocht", "ihr", "e\u00b7wig", "treu", "zu", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "PPER", "ADJD", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gab mir den Weiheku\u00df zum Bundespfande.", "tokens": ["Gab", "mir", "den", "Wei\u00b7he\u00b7ku\u00df", "zum", "Bun\u00b7des\u00b7pfan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So will auch ich die heilge Fahne schwingen", "tokens": ["So", "will", "auch", "ich", "die", "heil\u00b7ge", "Fah\u00b7ne", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "PPER", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und der Begeistrung Oriflamme tragen,", "tokens": ["Und", "der", "Be\u00b7geis\u00b7trung", "O\u00b7ri\u00b7flam\u00b7me", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit Liederschwertern unsre Feinde schlagen!", "tokens": ["Mit", "Lie\u00b7der\u00b7schwer\u00b7tern", "uns\u00b7re", "Fein\u00b7de", "schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die reine Magd kann jegliches vollbringen:", "tokens": ["Die", "rei\u00b7ne", "Magd", "kann", "jeg\u00b7li\u00b7ches", "voll\u00b7brin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der h\u00f6chsten Kunst hab ich mich ganz ergeben,", "tokens": ["Der", "h\u00f6chs\u00b7ten", "Kunst", "hab", "ich", "mich", "ganz", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Treu bis zum Tode durch das ganze Leben! \u2013", "tokens": ["Treu", "bis", "zum", "To\u00b7de", "durch", "das", "gan\u00b7ze", "Le\u00b7ben", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}