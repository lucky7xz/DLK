{"textgrid.poem.43125": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Alter Text und alte Weise \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alter Text und alte Weise \u2013", "tokens": ["Al\u00b7ter", "Text", "und", "al\u00b7te", "Wei\u00b7se", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie das durch mein Leben zog,", "tokens": ["Wie", "das", "durch", "mein", "Le\u00b7ben", "zog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und so wehmuthsvoll und leise", "tokens": ["Und", "so", "weh\u00b7muths\u00b7voll", "und", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir den Himmel nieder log.", "tokens": ["Mir", "den", "Him\u00b7mel", "nie\u00b7der", "log", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PTKVZ", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fast vergessen pocht es wieder", "tokens": ["Fast", "ver\u00b7ges\u00b7sen", "pocht", "es", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "An das eingewiegte Herz,", "tokens": ["An", "das", "ein\u00b7ge\u00b7wieg\u00b7te", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der erste Ton ist wieder,", "tokens": ["Und", "der", "ers\u00b7te", "Ton", "ist", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "ADV", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "So wie einst, ein leiser Schmerz.", "tokens": ["So", "wie", "einst", ",", "ein", "lei\u00b7ser", "Schmerz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADV", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}