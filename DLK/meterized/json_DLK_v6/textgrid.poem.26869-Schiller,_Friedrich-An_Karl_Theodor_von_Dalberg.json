{"textgrid.poem.26869": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An Karl Theodor von Dalberg", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn rohe Kr\u00e4fte feindlich sich entzweien", "tokens": ["Wenn", "ro\u00b7he", "Kr\u00e4f\u00b7te", "feind\u00b7lich", "sich", "ent\u00b7zwei\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "ADJD", "PRF", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und blinde Wut die Kriegesflamme sch\u00fcrt,", "tokens": ["Und", "blin\u00b7de", "Wut", "die", "Krie\u00b7ges\u00b7flam\u00b7me", "sch\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn sich im Kampfe tobender Parteien", "tokens": ["Wenn", "sich", "im", "Kamp\u00b7fe", "to\u00b7ben\u00b7der", "Par\u00b7tei\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Stimme der Gerechtigkeit verliert,", "tokens": ["Die", "Stim\u00b7me", "der", "Ge\u00b7rech\u00b7tig\u00b7keit", "ver\u00b7liert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn alle Laster schamlos sich befreien,", "tokens": ["Wenn", "al\u00b7le", "Las\u00b7ter", "scham\u00b7los", "sich", "be\u00b7frei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADJD", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn freche Willk\u00fcr an das Heilge r\u00fchrt,", "tokens": ["Wenn", "fre\u00b7che", "Will\u00b7k\u00fcr", "an", "das", "Heil\u00b7ge", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Den Anker l\u00f6st, an dem die Staaten h\u00e4ngen,", "tokens": ["Den", "An\u00b7ker", "l\u00f6st", ",", "an", "dem", "die", "Staa\u00b7ten", "h\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "\u2013 Das ist kein Stoff zu freudigen Ges\u00e4ngen!", "tokens": ["\u2013", "Das", "ist", "kein", "Stoff", "zu", "freu\u00b7di\u00b7gen", "Ge\u00b7s\u00e4n\u00b7gen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "PIAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch wenn ein Volk, das fromm die Herden weidet,", "tokens": ["Doch", "wenn", "ein", "Volk", ",", "das", "fromm", "die", "Her\u00b7den", "wei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich selbst genug, nicht fremden Guts begehrt,", "tokens": ["Sich", "selbst", "ge\u00b7nug", ",", "nicht", "frem\u00b7den", "Guts", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "$,", "PTKNEG", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Zwang abwirft, den es unw\u00fcrdig leidet,", "tokens": ["Den", "Zwang", "ab\u00b7wirft", ",", "den", "es", "un\u00b7w\u00fcr\u00b7dig", "lei\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch selbst im Zorn die Menschlichkeit noch ehrt,", "tokens": ["Doch", "selbst", "im", "Zorn", "die", "Menschlich\u00b7keit", "noch", "ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Im Gl\u00fccke selbst, im Siege sich bescheidet,", "tokens": ["Im", "Gl\u00fc\u00b7cke", "selbst", ",", "im", "Sie\u00b7ge", "sich", "be\u00b7schei\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "APPRART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "\u2013 Das ist unsterblich und des Liedes wert.", "tokens": ["\u2013", "Das", "ist", "uns\u00b7terb\u00b7lich", "und", "des", "Lie\u00b7des", "wert", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "ADJD", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und solch ein Bild darf ich ", "tokens": ["Und", "solch", "ein", "Bild", "darf", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ART", "NN", "VMFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}