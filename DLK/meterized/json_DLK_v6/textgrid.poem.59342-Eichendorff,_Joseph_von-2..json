{"textgrid.poem.59342": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Welt ruht still im Hafen,", "tokens": ["Die", "Welt", "ruht", "still", "im", "Ha\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Liebchen, gute Nacht!", "tokens": ["Mein", "Lieb\u00b7chen", ",", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wann Wald und Berge schlafen,", "tokens": ["Wann", "Wald", "und", "Ber\u00b7ge", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Treu' Liebe einsam wacht.", "tokens": ["Treu'", "Lie\u00b7be", "ein\u00b7sam", "wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich bin so wach und lustig,", "tokens": ["Ich", "bin", "so", "wach", "und", "lus\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Seele ist so licht,", "tokens": ["Die", "See\u00b7le", "ist", "so", "licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und eh ich liebt, da wu\u00dft ich", "tokens": ["Und", "eh", "ich", "liebt", ",", "da", "wu\u00dft", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von solcher Freude nicht.", "tokens": ["Von", "sol\u00b7cher", "Freu\u00b7de", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich f\u00fchl mich so befreiet", "tokens": ["Ich", "f\u00fchl", "mich", "so", "be\u00b7frei\u00b7et"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von eitlem Trieb und Streit,", "tokens": ["Von", "eit\u00b7lem", "Trieb", "und", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nichts mehr das Herz zerstreuet", "tokens": ["Nichts", "mehr", "das", "Herz", "zer\u00b7streu\u00b7et"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In seiner Fr\u00f6hlichkeit.", "tokens": ["In", "sei\u00b7ner", "Fr\u00f6h\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mir ist, als m\u00fc\u00dft ich singen", "tokens": ["Mir", "ist", ",", "als", "m\u00fc\u00dft", "ich", "sin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So recht aus tiefster Lust", "tokens": ["So", "recht", "aus", "tiefs\u00b7ter", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von wunderbaren Dingen,", "tokens": ["Von", "wun\u00b7der\u00b7ba\u00b7ren", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was niemand sonst bewu\u00dft.", "tokens": ["Was", "nie\u00b7mand", "sonst", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O k\u00f6nnt ich alles sagen!", "tokens": ["O", "k\u00f6nnt", "ich", "al\u00b7les", "sa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O w\u00e4r ich recht geschickt!", "tokens": ["O", "w\u00e4r", "ich", "recht", "ge\u00b7schickt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So mu\u00df ich still ertragen,", "tokens": ["So", "mu\u00df", "ich", "still", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was mich so hoch begl\u00fcckt.", "tokens": ["Was", "mich", "so", "hoch", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}