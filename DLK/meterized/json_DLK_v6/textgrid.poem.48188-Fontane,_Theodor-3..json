{"textgrid.poem.48188": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Mutter aller Liebe und ihr Kind,", "tokens": ["Du", "Mut\u00b7ter", "al\u00b7ler", "Lie\u00b7be", "und", "ihr", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PIAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du einzig Pf\u00fchl, auf dem wir sorglos schlummern,", "tokens": ["Du", "ein\u00b7zig", "Pf\u00fchl", ",", "auf", "dem", "wir", "sorg\u00b7los", "schlum\u00b7mern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "$,", "APPR", "PRELS", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich rufe dich, kehr' wieder in dies Herz!", "tokens": ["Ich", "ru\u00b7fe", "dich", ",", "kehr'", "wie\u00b7der", "in", "dies", "Herz", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "VVFIN", "ADV", "APPR", "PDS", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es gibt kein Gl\u00fcck, wo du den R\u00fccken wandtest,", "tokens": ["Es", "gibt", "kein", "Gl\u00fcck", ",", "wo", "du", "den", "R\u00fc\u00b7cken", "wand\u00b7test", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es gibt kein Ungl\u00fcck, l\u00e4chelst du aufs neu;", "tokens": ["Es", "gibt", "kein", "Un\u00b7gl\u00fcck", ",", "l\u00e4\u00b7chelst", "du", "aufs", "neu", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "VVFIN", "PPER", "APPRART", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "La\u00df k\u00e4mpfen mich in deinem Spruch und Zeichen,", "tokens": ["La\u00df", "k\u00e4mp\u00b7fen", "mich", "in", "dei\u00b7nem", "Spruch", "und", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und wieder wird das Leben mir zum Sieg.\u00ab", "tokens": ["Und", "wie\u00b7der", "wird", "das", "Le\u00b7ben", "mir", "zum", "Sieg", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "PPER", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}