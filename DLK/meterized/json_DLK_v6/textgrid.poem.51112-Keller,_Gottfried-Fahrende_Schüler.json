{"textgrid.poem.51112": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Fahrende Sch\u00fcler", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fliehe nicht, du holde Maid,", "tokens": ["Flie\u00b7he", "nicht", ",", "du", "hol\u00b7de", "Maid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn wir dir vor\u00fcberkommen,", "tokens": ["Wenn", "wir", "dir", "vor\u00b7\u00fc\u00b7ber\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Leute, denen aus Wanderleid", "tokens": ["Leu\u00b7te", ",", "de\u00b7nen", "aus", "Wan\u00b7der\u00b7leid"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Ist ein guter Stern entglommen!", "tokens": ["Ist", "ein", "gu\u00b7ter", "Stern", "ent\u00b7glom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sind gebr\u00e4unt in Wetter und Wind", "tokens": ["Sind", "ge\u00b7br\u00e4unt", "in", "Wet\u00b7ter", "und", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "APPR", "NN", "KON", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und gereift an hei\u00dfen Sonne;", "tokens": ["Und", "ge\u00b7reift", "an", "hei\u00b7\u00dfen", "Son\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcber unsre Wangen sind", "tokens": ["\u00dc\u00b7ber", "uns\u00b7re", "Wan\u00b7gen", "sind"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein paar Tr\u00e4nen schon geronnen.", "tokens": ["Ein", "paar", "Tr\u00e4\u00b7nen", "schon", "ge\u00b7ron\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Treten jetzo fest einher,", "tokens": ["Tre\u00b7ten", "jet\u00b7zo", "fest", "ein\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fchlen unter uns die Erde,", "tokens": ["F\u00fch\u00b7len", "un\u00b7ter", "uns", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht von eitlem Hoffen schwer", "tokens": ["Nicht", "von", "eit\u00b7lem", "Hof\u00b7fen", "schwer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch verzagend vor Gef\u00e4hrde.", "tokens": ["Noch", "ver\u00b7za\u00b7gend", "vor", "Ge\u00b7f\u00e4hr\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Atmen froh das Morgenwehn,", "tokens": ["At\u00b7men", "froh", "das", "Mor\u00b7gen\u00b7wehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn wir durch die Lande schweifen;", "tokens": ["Wenn", "wir", "durch", "die", "Lan\u00b7de", "schwei\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Glauben nichts, als was wir sehn", "tokens": ["Glau\u00b7ben", "nichts", ",", "als", "was", "wir", "sehn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "PRELS", "PPER", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mit unsern Sinnen greifen!", "tokens": ["Und", "mit", "un\u00b7sern", "Sin\u00b7nen", "grei\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Halten nichts auf hohlen Dunst,", "tokens": ["Hal\u00b7ten", "nichts", "auf", "hoh\u00b7len", "Dunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00f6gen nichts auf Worte geben;", "tokens": ["M\u00f6\u00b7gen", "nichts", "auf", "Wor\u00b7te", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch verstehen wir die Kunst,", "tokens": ["Doch", "ver\u00b7ste\u00b7hen", "wir", "die", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie wir denken, auch zu leben.", "tokens": ["Wie", "wir", "den\u00b7ken", ",", "auch", "zu", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "$,", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Scheiden leicht von jedem Traum,", "tokens": ["Schei\u00b7den", "leicht", "von", "je\u00b7dem", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der sich nicht mit Wahrheit paarte;", "tokens": ["Der", "sich", "nicht", "mit", "Wahr\u00b7heit", "paar\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PTKNEG", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch hegt unser Busen Raum", "tokens": ["Doch", "hegt", "un\u00b7ser", "Bu\u00b7sen", "Raum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "F\u00fcr das Starke wie das Zarte.", "tokens": ["F\u00fcr", "das", "Star\u00b7ke", "wie", "das", "Zar\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ruhen heut im sonnigen Tal,", "tokens": ["Ru\u00b7hen", "heut", "im", "son\u00b7ni\u00b7gen", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Lauschend, wie die Knospen springen,", "tokens": ["Lau\u00b7schend", ",", "wie", "die", "Knos\u00b7pen", "sprin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stehen morgen im Wetterstrahl,", "tokens": ["Ste\u00b7hen", "mor\u00b7gen", "im", "Wet\u00b7ter\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Wo die St\u00fcrme die Wolken schwingen.", "tokens": ["Wo", "die", "St\u00fcr\u00b7me", "die", "Wol\u00b7ken", "schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.8": {"line.1": {"text": "Und es lobet unser Geist,", "tokens": ["Und", "es", "lo\u00b7bet", "un\u00b7ser", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was da lebt in Licht und Grauen;", "tokens": ["Was", "da", "lebt", "in", "Licht", "und", "Grau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch wir ehren noch zumeist,", "tokens": ["Doch", "wir", "eh\u00b7ren", "noch", "zu\u00b7meist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn sie gut sind, holde Frauen!", "tokens": ["Wenn", "sie", "gut", "sind", ",", "hol\u00b7de", "Frau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}