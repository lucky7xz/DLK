{"textgrid.poem.26472": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sieh', da will ein dorn'ger Zweig", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh', da will ein dorn'ger Zweig", "tokens": ["Sieh'", ",", "da", "will", "ein", "dorn'\u00b7ger", "Zweig"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "VMFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Uns den Pfad verwehren \u2013", "tokens": ["Uns", "den", "Pfad", "ver\u00b7weh\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sieh die Beeren \u00fcberreich,", "tokens": ["Sieh", "die", "Bee\u00b7ren", "\u00fc\u00b7berr\u00b7eich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die ihn sanft beschweren.", "tokens": ["Die", "ihn", "sanft", "be\u00b7schwe\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Beeren, schwarz und roth und gr\u00fcn \u2013", "tokens": ["Bee\u00b7ren", ",", "schwarz", "und", "roth", "und", "gr\u00fcn", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "KON", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie sie m\u00e4lig reifen", "tokens": ["Wie", "sie", "m\u00e4\u00b7lig", "rei\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Bei der Strahlen hei\u00dfem Gl\u00fch'n,", "tokens": ["Bei", "der", "Strah\u00b7len", "hei\u00b7\u00dfem", "Gl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die im Wald sie streifen.", "tokens": ["Die", "im", "Wald", "sie", "strei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "So viel W\u00fcnsche sonder Zahl", "tokens": ["So", "viel", "W\u00fcn\u00b7sche", "son\u00b7der", "Zahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Trag' ich tief im Herzen,", "tokens": ["Trag'", "ich", "tief", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Das dir schl\u00e4gt in holder Qual", "tokens": ["Das", "dir", "schl\u00e4gt", "in", "hol\u00b7der", "Qual"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und geliebten Schmerzen.", "tokens": ["Und", "ge\u00b7lieb\u00b7ten", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Mancher, schon verblutend, mag,", "tokens": ["Man\u00b7cher", ",", "schon", "ver\u00b7blu\u00b7tend", ",", "mag", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "$,", "ADV", "VVPP", "$,", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df er ward, bereuen \u2013", "tokens": ["Da\u00df", "er", "ward", ",", "be\u00b7reu\u00b7en", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "$,", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Doch es reift ein jeder Tag", "tokens": ["Doch", "es", "reift", "ein", "je\u00b7der", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Selig einen neuen!", "tokens": ["Se\u00b7lig", "ei\u00b7nen", "neu\u00b7en", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}