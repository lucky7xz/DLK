{"dta.poem.2242": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Uebersetzung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Wesen, das ihm selber nicht sein Wesen, Sin-", "tokens": ["Ein", "We\u00b7sen", ",", "das", "ihm", "sel\u00b7ber", "nicht", "sein", "We\u00b7sen", ",", "Sin"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "PTKNEG", "PPOSAT", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "nen, Leib und Leben,", "tokens": ["nen", ",", "Leib", "und", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Haut, Adern, Sehnen, Knochen, Fleisch; auch nicht", "tokens": ["Haut", ",", "A\u00b7dern", ",", "Seh\u00b7nen", ",", "Kno\u00b7chen", ",", "Fleisch", ";", "auch", "nicht"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$.", "ADV", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "der Seelen F\u00e4higkeit,", "tokens": ["der", "See\u00b7len", "F\u00e4\u00b7hig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Begriff, Ged\u00e4chtni\u00df; ja um reich zu werden nicht Ge-", "tokens": ["Be\u00b7griff", ",", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", ";", "ja", "um", "reich", "zu", "wer\u00b7den", "nicht", "Ge"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$.", "ADV", "APPR", "ADJD", "PTKZU", "VAINF", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "legenheit,", "tokens": ["le\u00b7gen\u00b7heit", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Und fast von allem, was er hat, auch das geringste", "tokens": ["Und", "fast", "von", "al\u00b7lem", ",", "was", "er", "hat", ",", "auch", "das", "ge\u00b7rings\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PIS", "$,", "PWS", "PPER", "VAFIN", "$,", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "nicht gegeben:", "tokens": ["nicht", "ge\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Wie kann doch solches \u00fcber andre mit einem Rechte sich", "tokens": ["Wie", "kann", "doch", "sol\u00b7ches", "\u00fc\u00b7ber", "and\u00b7re", "mit", "ei\u00b7nem", "Rech\u00b7te", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ADV", "PIS", "APPR", "PIS", "APPR", "ART", "NN", "PRF"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "erheben?", "tokens": ["er\u00b7he\u00b7ben", "?"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}