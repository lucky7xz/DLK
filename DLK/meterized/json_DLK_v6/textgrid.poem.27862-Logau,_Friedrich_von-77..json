{"textgrid.poem.27862": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "77.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auff dieser Welt ist nichts, das st\u00e4rcker wird getrieben,", "tokens": ["Auff", "die\u00b7ser", "Welt", "ist", "nichts", ",", "das", "st\u00e4r\u00b7cker", "wird", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "PIS", "$,", "PRELS", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auff dieser Welt ist nichts, das billich mehr w\u00e4r blieben,", "tokens": ["Auff", "die\u00b7ser", "Welt", "ist", "nichts", ",", "das", "bil\u00b7lich", "mehr", "w\u00e4r", "blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "PIS", "$,", "PRELS", "ADJD", "ADV", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als au\u00dfgezeumte Brunst und ungeordnet lieben.", "tokens": ["Als", "au\u00df\u00b7ge\u00b7zeum\u00b7te", "Brunst", "und", "un\u00b7ge\u00b7ord\u00b7net", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}