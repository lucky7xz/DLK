{"textgrid.poem.66936": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schwer ert\u00f6nen ernste Feierglocken,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwer ert\u00f6nen ernste Feierglocken,", "tokens": ["Schwer", "er\u00b7t\u00f6\u00b7nen", "erns\u00b7te", "Fei\u00b7er\u00b7glo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mahnend durch die L\u00fcfte zieht ihr Klang,", "tokens": ["Mah\u00b7nend", "durch", "die", "L\u00fcf\u00b7te", "zieht", "ihr", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Selbst das Herz des Starken m\u00f6chte stocken ...", "tokens": ["Selbst", "das", "Herz", "des", "Star\u00b7ken", "m\u00f6ch\u00b7te", "sto\u00b7cken", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Pfingstgesang", "tokens": ["Pfingst\u00b7ge\u00b7sang"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Wagt sich kaum zu Wipfeln hochzuschwingen,", "tokens": ["Wagt", "sich", "kaum", "zu", "Wip\u00b7feln", "hoch\u00b7zu\u00b7schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Heller Freude Stimme scheint erstickt \u2013", "tokens": ["Hel\u00b7ler", "Freu\u00b7de", "Stim\u00b7me", "scheint", "er\u00b7stickt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NN", "VVFIN", "VVPP", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wer kann Deutschland frohe Botschaft bringen?!\u2013", "tokens": ["Wer", "kann", "Deutschland", "fro\u00b7he", "Bot\u00b7schaft", "brin\u00b7gen", "?!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VMFIN", "NE", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Aufgeblickt!", "tokens": ["Auf\u00b7ge\u00b7blickt", "!"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Eine Botschaft, eine nur erschalle", "tokens": ["Ei\u00b7ne", "Bot\u00b7schaft", ",", "ei\u00b7ne", "nur", "er\u00b7schal\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADV", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Durch des Schicksals mitleidlosen Mund:", "tokens": ["Durch", "des", "Schick\u00b7sals", "mit\u00b7leid\u00b7lo\u00b7sen", "Mund", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wer sich selbst verschrieben dem Verfalle,", "tokens": ["Wer", "sich", "selbst", "ver\u00b7schrie\u00b7ben", "dem", "Ver\u00b7fal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Geht zu Grund.", "tokens": ["Geht", "zu", "Grund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Nein! La\u00dft aus den schuldverworrnen Zeiten,", "tokens": ["Nein", "!", "La\u00dft", "aus", "den", "schuld\u00b7ver\u00b7worr\u00b7nen", "Zei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "VVIMP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "La\u00dft mit unersch\u00fctterlichem Sinn", "tokens": ["La\u00dft", "mit", "un\u00b7er\u00b7sch\u00fct\u00b7ter\u00b7li\u00b7chem", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Uns gefa\u00dft in neue Zukunft schreiten!", "tokens": ["Uns", "ge\u00b7fa\u00dft", "in", "neu\u00b7e", "Zu\u00b7kunft", "schrei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Siegerin", "tokens": ["Sie\u00b7ge\u00b7rin"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.9": {"line.1": {"text": "Sei die Kraft, sich menschlich zu verbinden,", "tokens": ["Sei", "die", "Kraft", ",", "sich", "menschlich", "zu", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Sieger sei der eingeborne Mut,", "tokens": ["Sie\u00b7ger", "sei", "der", "ein\u00b7ge\u00b7bor\u00b7ne", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "W\u00fcrdig Not und Schmach zu \u00fcberwinden.", "tokens": ["W\u00fcr\u00b7dig", "Not", "und", "Schmach", "zu", "\u00fc\u00b7berw\u00b7in\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Schaffensglut", "tokens": ["Schaf\u00b7fens\u00b7glut"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.11": {"line.1": {"text": "Soll aus Wut und Wahnsinns tr\u00fcbem Schwelen", "tokens": ["Soll", "aus", "Wut", "und", "Wahn\u00b7sinns", "tr\u00fc\u00b7bem", "Schwe\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "NN", "KON", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aufw\u00e4rts lodern wie ein lautrer Brand,", "tokens": ["Auf\u00b7w\u00e4rts", "lo\u00b7dern", "wie", "ein", "laut\u00b7rer", "Brand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Hirn und Hand mit Heilsgewalt beseelen.", "tokens": ["Hirn", "und", "Hand", "mit", "Heils\u00b7ge\u00b7walt", "be\u00b7see\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Zwist verbannt!", "tokens": ["Zwist", "ver\u00b7bannt", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.13": {"line.1": {"text": "Was emporgew\u00fchlt in wildem Grollen,", "tokens": ["Was", "em\u00b7por\u00b7ge\u00b7w\u00fchlt", "in", "wil\u00b7dem", "Grol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Was des Krieges Weltverzweiflung schuf,", "tokens": ["Was", "des", "Krie\u00b7ges", "Welt\u00b7ver\u00b7zwei\u00b7flung", "schuf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sei wie Spuk in Grabesnacht verschollen!", "tokens": ["Sei", "wie", "Spuk", "in", "Gra\u00b7bes\u00b7nacht", "ver\u00b7schol\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.14": {"line.1": {"text": "Werderuf", "tokens": ["Wer\u00b7de\u00b7ruf"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.15": {"line.1": {"text": "Dr\u00f6hnt die Erde wie am ersten Tage.", "tokens": ["Dr\u00f6hnt", "die", "Er\u00b7de", "wie", "am", "ers\u00b7ten", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KOKOM", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aus der Scholle bricht die junge Saat,", "tokens": ["Aus", "der", "Schol\u00b7le", "bricht", "die", "jun\u00b7ge", "Saat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Durch der Herzen ungeheure Klage", "tokens": ["Durch", "der", "Her\u00b7zen", "un\u00b7ge\u00b7heu\u00b7re", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.16": {"line.1": {"text": "Schreit die Tat.", "tokens": ["Schreit", "die", "Tat", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.17": {"line.1": {"text": "Schwer ert\u00f6nen ernste Feierglocken,", "tokens": ["Schwer", "er\u00b7t\u00f6\u00b7nen", "erns\u00b7te", "Fei\u00b7er\u00b7glo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mahnend durch die L\u00fcfte zittern sie:", "tokens": ["Mah\u00b7nend", "durch", "die", "L\u00fcf\u00b7te", "zit\u00b7tern", "sie", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Deutsches Volk, in Schrecken unerschrocken,", "tokens": ["Deut\u00b7sches", "Volk", ",", "in", "Schre\u00b7cken", "un\u00b7er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.18": {"line.1": {"text": "Stahl ins Knie!", "tokens": ["Stahl", "ins", "Knie", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}