{"textgrid.poem.56957": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du jagtest durch den Saal auf leichten Knien", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du jagtest durch den Saal auf leichten Knien", "tokens": ["Du", "jag\u00b7test", "durch", "den", "Saal", "auf", "leich\u00b7ten", "Kni\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und warfst das Haar mit fordernder Geb\u00e4rde,", "tokens": ["und", "warfst", "das", "Haar", "mit", "for\u00b7dern\u00b7der", "Ge\u00b7b\u00e4r\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "du wolltest mich zu dir hinunter ziehn,", "tokens": ["du", "woll\u00b7test", "mich", "zu", "dir", "hin\u00b7un\u00b7ter", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "mich saugen, wie den Tropfen trockne Erde.", "tokens": ["mich", "sau\u00b7gen", ",", "wie", "den", "Trop\u00b7fen", "trock\u00b7ne", "Er\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In deines stumpfen T\u00e4nzers Arme sankst", "tokens": ["In", "dei\u00b7nes", "stump\u00b7fen", "T\u00e4n\u00b7zers", "Ar\u00b7me", "sankst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "du weit r\u00fcck\u00fcber und, nach mir gedreht,", "tokens": ["du", "weit", "r\u00fcck\u00b7\u00fc\u00b7ber", "und", ",", "nach", "mir", "ge\u00b7dreht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "KON", "$,", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "verschlangst du mich mit jedem Blick und trankst", "tokens": ["ver\u00b7schlangst", "du", "mich", "mit", "je\u00b7dem", "Blick", "und", "trankst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PIAT", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "mein fliegend Herzblut, Venus Aschthoreth!", "tokens": ["mein", "flie\u00b7gend", "Herz\u00b7blut", ",", "Ve\u00b7nus", "Ascht\u00b7ho\u00b7re\u00b7th", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "NE", "NE", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}}}}