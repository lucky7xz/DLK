{"textgrid.poem.33879": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Bubensonntag", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich einst, ein kleiner Bube,", "tokens": ["Wenn", "ich", "einst", ",", "ein", "klei\u00b7ner", "Bu\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sonntags fr\u00fch' im Bette lag,", "tokens": ["Sonn\u00b7tags", "fr\u00fch'", "im", "Bet\u00b7te", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die helle Kirchenglocke", "tokens": ["Und", "die", "hel\u00b7le", "Kir\u00b7chen\u00b7glo\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "All das Schweigen unterbrach:", "tokens": ["All", "das", "Schwei\u00b7gen", "un\u00b7ter\u00b7brach", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O, wie schl\u00fcpft' ich dann so hurtig", "tokens": ["O", ",", "wie", "schl\u00fcpft'", "ich", "dann", "so", "hur\u00b7tig"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "VVFIN", "PPER", "ADV", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus dem Bett in's Kleid hinein,", "tokens": ["Aus", "dem", "Bett", "in's", "Kleid", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wie gern lie\u00df ich das Fr\u00fchst\u00fcck,", "tokens": ["Und", "wie", "gern", "lie\u00df", "ich", "das", "Fr\u00fch\u00b7st\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Um zuerst bei Gott zu sein!", "tokens": ["Um", "zu\u00b7erst", "bei", "Gott", "zu", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "APPR", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein Gesangbuch unter'm Arme,", "tokens": ["Ein", "Ge\u00b7sang\u00b7buch", "un\u00b7ter'm", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eh' ich's Lesen noch verstand,", "tokens": ["Eh'", "ich's", "Le\u00b7sen", "noch", "ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ging ich fort, gebeugten Hauptes,", "tokens": ["Ging", "ich", "fort", ",", "ge\u00b7beug\u00b7ten", "Haup\u00b7tes", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fromm verschr\u00e4nkend Hand in Hand.", "tokens": ["Fromm", "ver\u00b7schr\u00e4n\u00b7kend", "Hand", "in", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Kam mein H\u00fcndchen froh gesprungen,", "tokens": ["Kam", "mein", "H\u00fcnd\u00b7chen", "froh", "ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schalt ich: komm mir nicht zu nah'!", "tokens": ["Schalt", "ich", ":", "komm", "mir", "nicht", "zu", "nah'", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "VVFIN", "PPER", "PTKNEG", "PTKZU", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kaum, da\u00df ich, zur Seite schielend,", "tokens": ["Kaum", ",", "da\u00df", "ich", ",", "zur", "Sei\u00b7te", "schie\u00b7lend", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach der Vogelfalle sah.", "tokens": ["Nach", "der", "Vo\u00b7gel\u00b7fal\u00b7le", "sah", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Fiel die Kirchenth\u00fcr nun knarrend", "tokens": ["Fiel", "die", "Kir\u00b7chent\u00b7h\u00fcr", "nun", "knar\u00b7rend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hinter meinem R\u00fccken zu,", "tokens": ["Hin\u00b7ter", "mei\u00b7nem", "R\u00fc\u00b7cken", "zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sprach ich furchtsam-zuversichtlich:", "tokens": ["Sprach", "ich", "furcht\u00b7sam\u00b7zu\u00b7ver\u00b7sicht\u00b7lich", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jetzt allein sind Gott und du!", "tokens": ["Jetzt", "al\u00b7lein", "sind", "Gott", "und", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "NN", "KON", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "L\u00e4ngst mit ganzem, vollem Herzen", "tokens": ["L\u00e4ngst", "mit", "gan\u00b7zem", ",", "vol\u00b7lem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hing ich ja an meinem Gott,", "tokens": ["Hing", "ich", "ja", "an", "mei\u00b7nem", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch, da\u00df Niemand ihn erblicke,", "tokens": ["Doch", ",", "da\u00df", "Nie\u00b7mand", "ihn", "er\u00b7bli\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hielt ich stets f\u00fcr eitel Spott.", "tokens": ["Hielt", "ich", "stets", "f\u00fcr", "ei\u00b7tel", "Spott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und so hofft' ich jeden Morgen,", "tokens": ["Und", "so", "hofft'", "ich", "je\u00b7den", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Endlich einmal ihn zu seh'n;", "tokens": ["End\u00b7lich", "ein\u00b7mal", "ihn", "zu", "seh'n", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "War's denn Nichts in meinen Jahren,", "tokens": ["Wa\u00b7r's", "denn", "Nichts", "in", "mei\u00b7nen", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stets um F\u00fcnfe aufzusteh'n?", "tokens": ["Stets", "um", "F\u00fcn\u00b7fe", "auf\u00b7zu\u00b7steh'n", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Auf dem hohen Thurm die Glocke", "tokens": ["Auf", "dem", "ho\u00b7hen", "Thurm", "die", "Glo\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War schon lange wieder stumm,", "tokens": ["War", "schon", "lan\u00b7ge", "wie\u00b7der", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Altar warf d\u00fcstre Schatten,", "tokens": ["Der", "Al\u00b7tar", "warf", "d\u00fcst\u00b7re", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gr\u00e4ber lagen rings herum.", "tokens": ["Gr\u00e4\u00b7ber", "la\u00b7gen", "rings", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Drang ein Schall zu mir her\u00fcber,", "tokens": ["Drang", "ein", "Schall", "zu", "mir", "her\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dacht' ich: jetzt wirst du ihn schau'n!", "tokens": ["Dacht'", "ich", ":", "jetzt", "wirst", "du", "ihn", "schau'n", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "ADV", "VAFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+--++-+", "measure": "dactylic.init"}, "line.3": {"text": "Aber meine Augen schlossen", "tokens": ["A\u00b7ber", "mei\u00b7ne", "Au\u00b7gen", "schlos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich zugleich vor Angst und Grau'n.", "tokens": ["Sich", "zu\u00b7gleich", "vor", "Angst", "und", "Grau'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PRF", "ADV", "APPR", "NN", "KON", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und dies Zittern, dies Erbangen,", "tokens": ["Und", "dies", "Zit\u00b7tern", ",", "dies", "Er\u00b7ban\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDS", "NN", "$,", "PDS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mein kalter Todesschwei\u00df \u2013", "tokens": ["Und", "mein", "kal\u00b7ter", "To\u00b7des\u00b7schwei\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df der Herr vorbei gewandelt,", "tokens": ["Da\u00df", "der", "Herr", "vor\u00b7bei", "ge\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Galt mir Alles f\u00fcr Beweis.", "tokens": ["Galt", "mir", "Al\u00b7les", "f\u00fcr", "Be\u00b7weis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIS", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Still und tr\u00e4umend dann zu Hause", "tokens": ["Still", "und", "tr\u00e4u\u00b7mend", "dann", "zu", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlich ich mich in s\u00fc\u00dfer Qual,", "tokens": ["Schlich", "ich", "mich", "in", "s\u00fc\u00b7\u00dfer", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mein klopfend Herz gelobte", "tokens": ["Und", "mein", "klop\u00b7fend", "Herz", "ge\u00b7lob\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJD", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich mehr Muth f\u00fcr's n\u00e4chste Mal.", "tokens": ["Sich", "mehr", "Muth", "f\u00fcr's", "n\u00e4chs\u00b7te", "Mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}