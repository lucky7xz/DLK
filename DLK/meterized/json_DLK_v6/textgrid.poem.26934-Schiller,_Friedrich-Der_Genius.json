{"textgrid.poem.26934": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Genius", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wiederholen zwar kann der Verstand, was da schon gewesen,", "tokens": ["Wie\u00b7der\u00b7ho\u00b7len", "zwar", "kann", "der", "Ver\u00b7stand", ",", "was", "da", "schon", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VMFIN", "ART", "NN", "$,", "PRELS", "ADV", "ADV", "VAPP", "$,"], "meter": "--+--+--+--+-+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Was die Natur gebaut, bauet er w\u00e4hlend ihr nach.", "tokens": ["Was", "die", "Na\u00b7tur", "ge\u00b7baut", ",", "bau\u00b7et", "er", "w\u00e4h\u00b7lend", "ihr", "nach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "$,", "VVFIN", "PPER", "KOUS", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "\u00dcber Natur hinaus baut die Vernunft, doch nur in das Leere,", "tokens": ["\u00dc\u00b7ber", "Na\u00b7tur", "hin\u00b7aus", "baut", "die", "Ver\u00b7nunft", ",", "doch", "nur", "in", "das", "Lee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APZR", "VVFIN", "ART", "NN", "$,", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+-++--+-+--+-", "measure": "iambic.septa.invert"}, "line.4": {"text": "Du nur, Genius, mehrst ", "tokens": ["Du", "nur", ",", "Ge\u00b7nius", ",", "mehrst"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "ADV", "$,", "NN", "$,", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}}}}}