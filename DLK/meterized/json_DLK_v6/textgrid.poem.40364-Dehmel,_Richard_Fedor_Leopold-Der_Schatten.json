{"textgrid.poem.40364": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Der Schatten", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab einen kleinen Schatten;", "tokens": ["Ich", "hab", "ei\u00b7nen", "klei\u00b7nen", "Schat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "der geht, wohin ich geh.", "tokens": ["der", "geht", ",", "wo\u00b7hin", "ich", "geh."], "token_info": ["word", "word", "punct", "word", "word", "abbreviation"], "pos": ["ART", "VVFIN", "$,", "PWAV", "PPER", "NE"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Aber wozu ich ihn habe,", "tokens": ["A\u00b7ber", "wo\u00b7zu", "ich", "ihn", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "ist mehr als ich versteh.", "tokens": ["ist", "mehr", "als", "ich", "ver\u00b7steh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er ist ganz ebenso wie ich,", "tokens": ["Er", "ist", "ganz", "e\u00b7ben\u00b7so", "wie", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "KOKOM", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "blos nicht ganz so schwer;", "tokens": ["blos", "nicht", "ganz", "so", "schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "und wenn ich in mein Bettchen h\u00fcpfe,", "tokens": ["und", "wenn", "ich", "in", "mein", "Bett\u00b7chen", "h\u00fcp\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "dann h\u00fcpft er hinterher.", "tokens": ["dann", "h\u00fcpft", "er", "hin\u00b7ter\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das Sonderbarste an ihm ist,", "tokens": ["Das", "Son\u00b7der\u00b7bars\u00b7te", "an", "ihm", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wie er sich anders macht;", "tokens": ["wie", "er", "sich", "an\u00b7ders", "macht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "garnicht wie artige Kinder tun,", "tokens": ["gar\u00b7nicht", "wie", "ar\u00b7ti\u00b7ge", "Kin\u00b7der", "tun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "h\u00fcbsch alles mit Bedacht.", "tokens": ["h\u00fcbsch", "al\u00b7les", "mit", "Be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nein, manchmal springt er schneller hoch", "tokens": ["Nein", ",", "manch\u00b7mal", "springt", "er", "schnel\u00b7ler", "hoch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "PPER", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "als mein Gummimann;", "tokens": ["als", "mein", "Gum\u00b7mi\u00b7mann", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "und manchmal macht er sich so klein,", "tokens": ["und", "manch\u00b7mal", "macht", "er", "sich", "so", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "da\u00df Keiner ihn finden kann.", "tokens": ["da\u00df", "Kei\u00b7ner", "ihn", "fin\u00b7den", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Neulich ganz fr\u00fch, da stand ich auf,", "tokens": ["Neu\u00b7lich", "ganz", "fr\u00fch", ",", "da", "stand", "ich", "auf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "noch eh die Sonne schien,", "tokens": ["noch", "eh", "die", "Son\u00b7ne", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "und ging spazieren durch den Tau,", "tokens": ["und", "ging", "spa\u00b7zie\u00b7ren", "durch", "den", "Tau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "im Gras, und suchte ihn.", "tokens": ["im", "Gras", ",", "und", "such\u00b7te", "ihn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Aber mein kleiner fauler Schatten,", "tokens": ["A\u00b7ber", "mein", "klei\u00b7ner", "fau\u00b7ler", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "als wenn er Schnupfen h\u00e4tt,", "tokens": ["als", "wenn", "er", "Schnup\u00b7fen", "h\u00e4tt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "lag wie ein altes Murmeltier", "tokens": ["lag", "wie", "ein", "al\u00b7tes", "Mur\u00b7mel\u00b7tier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "noch fest zu Bett.", "tokens": ["noch", "fest", "zu", "Bett", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}