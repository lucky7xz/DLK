{"textgrid.poem.39628": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An B. bei Gelegenheit eines mi\u00dfrathenen lateinischen Einladungs-Programmes", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weder gehauen, weder gestochen,", "tokens": ["We\u00b7der", "ge\u00b7hau\u00b7en", ",", "we\u00b7der", "ge\u00b7sto\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVPP", "$,", "KON", "VVPP", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Greuliches Rothwelschradegebrochen,", "tokens": ["Greu\u00b7li\u00b7ches", "Roth\u00b7wel\u00b7schra\u00b7de\u00b7ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "So ist des Naturrecht-Professors Abhandlung", "tokens": ["So", "ist", "des", "Na\u00b7tur\u00b7recht\u00b7Pro\u00b7fes\u00b7sors", "Ab\u00b7hand\u00b7lung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN"], "meter": "-+--+--+-+--", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Nach hegelscher neuer Begriffs-Umwandlung.", "tokens": ["Nach", "he\u00b7gel\u00b7scher", "neu\u00b7er", "Be\u00b7griffs\u00b7Um\u00b7wand\u00b7lung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.5": {"text": "Das soll, wie es aussieht, wohl gar Latein sein;", "tokens": ["Das", "soll", ",", "wie", "es", "aus\u00b7sieht", ",", "wohl", "gar", "La\u00b7tein", "sein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADV", "ADV", "NN", "VAINF", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Er schenkt uns aber Sp\u00fclicht f\u00fcr Wein ein.", "tokens": ["Er", "schenkt", "uns", "a\u00b7ber", "Sp\u00fc\u00b7licht", "f\u00fcr", "Wein", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Wie ist er aus Tertia nur entsprungen,", "tokens": ["Wie", "ist", "er", "aus", "Ter\u00b7tia", "nur", "ent\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "APPR", "NE", "ADV", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und hat sich so dreist auf's Katheder geschwungen?", "tokens": ["Und", "hat", "sich", "so", "dreist", "auf's", "Ka\u00b7the\u00b7der", "ge\u00b7schwun\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PRF", "ADV", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Nicht einmal die Grammatik hat er begriffen,", "tokens": ["Nicht", "ein\u00b7mal", "die", "Gram\u00b7ma\u00b7tik", "hat", "er", "be\u00b7grif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "Und will das Meer der Weisheit durchschiffen?", "tokens": ["Und", "will", "das", "Meer", "der", "Weis\u00b7heit", "durch\u00b7schif\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Ja, der Mann hat im Kopf einen Sparren!", "tokens": ["Ja", ",", "der", "Mann", "hat", "im", "Kopf", "ei\u00b7nen", "Spar\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.12": {"text": "\u00bbmit Kolben soll man lausen die Narren\u00ab;", "tokens": ["\u00bb", "mit", "Kol\u00b7ben", "soll", "man", "lau\u00b7sen", "die", "Nar\u00b7ren", "\u00ab", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "NN", "VMFIN", "PIS", "VVFIN", "ART", "NN", "$(", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "So sagten unsre ehrbaren Alten:", "tokens": ["So", "sag\u00b7ten", "uns\u00b7re", "ehr\u00b7ba\u00b7ren", "Al\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Wir wollen's mit ihm denn eben so halten.", "tokens": ["Wir", "wol\u00b7len's", "mit", "ihm", "denn", "e\u00b7ben", "so", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}