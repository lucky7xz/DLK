{"textgrid.poem.61276": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Wiedersehen", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich schritt mit meinem sch\u00f6nen Kinde", "tokens": ["Ich", "schritt", "mit", "mei\u00b7nem", "sch\u00f6\u00b7nen", "Kin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Flu\u00df hinab im Morgentau,", "tokens": ["Den", "Flu\u00df", "hin\u00b7ab", "im", "Mor\u00b7gen\u00b7tau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Schilfrohr wogte sacht im Winde,", "tokens": ["Das", "Schilf\u00b7rohr", "wog\u00b7te", "sacht", "im", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wasser gl\u00e4nzten still und blau.", "tokens": ["Die", "Was\u00b7ser", "gl\u00e4nz\u00b7ten", "still", "und", "blau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Erst gestern war aus weiter Ferne", "tokens": ["Erst", "ge\u00b7stern", "war", "aus", "wei\u00b7ter", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "APPR", "ADV", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Ich heimgekehrt nach manchem Jahr,", "tokens": ["Ich", "heim\u00b7ge\u00b7kehrt", "nach", "man\u00b7chem", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch war mit mir gleich einem Sterne", "tokens": ["Doch", "war", "mit", "mir", "gleich", "ei\u00b7nem", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Bild gezogen immerdar.", "tokens": ["Ihr", "Bild", "ge\u00b7zo\u00b7gen", "im\u00b7mer\u00b7dar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und ob im Lande der Zypressen", "tokens": ["Und", "ob", "im", "Lan\u00b7de", "der", "Zyp\u00b7res\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPRART", "NN", "ART", "NN"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Manch dunkles Auge mich gebannt;", "tokens": ["Manch", "dunk\u00b7les", "Au\u00b7ge", "mich", "ge\u00b7bannt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des blauen hatt' ich nie vergessen,", "tokens": ["Des", "blau\u00b7en", "hatt'", "ich", "nie", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das, als ich schied, in Tr\u00e4nen stand.", "tokens": ["Das", ",", "als", "ich", "schied", ",", "in", "Tr\u00e4\u00b7nen", "stand", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "PPER", "VVFIN", "$,", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und jetzt gedacht' ich's ihr zu sagen,", "tokens": ["Und", "jetzt", "ge\u00b7dacht'", "ich's", "ihr", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie lieb sie mir von Herzensgrund;", "tokens": ["Wie", "lieb", "sie", "mir", "von", "Her\u00b7zens\u00b7grund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Allein ein nie gekanntes Zagen", "tokens": ["Al\u00b7lein", "ein", "nie", "ge\u00b7kann\u00b7tes", "Za\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verschlo\u00df mir, wie ich ging, den Mund.", "tokens": ["Ver\u00b7schlo\u00df", "mir", ",", "wie", "ich", "ging", ",", "den", "Mund", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Auch sie lie\u00df stumm das K\u00f6pfchen hangen,", "tokens": ["Auch", "sie", "lie\u00df", "stumm", "das", "K\u00f6pf\u00b7chen", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das sonst so munter umgeschaut;", "tokens": ["Das", "sonst", "so", "mun\u00b7ter", "um\u00b7ge\u00b7schaut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch lag's wie Glut auf unsern Wangen,", "tokens": ["Doch", "lag's", "wie", "Glut", "auf", "un\u00b7sern", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und unsre Herzen pochten laut.", "tokens": ["Und", "uns\u00b7re", "Her\u00b7zen", "poch\u00b7ten", "laut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und als zum Lindenborn wir kamen,", "tokens": ["Und", "als", "zum", "Lin\u00b7den\u00b7born", "wir", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der unsrer Kindheit Spiel gekannt,", "tokens": ["Der", "uns\u00b7rer", "Kind\u00b7heit", "Spiel", "ge\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur leise nannt' ich ihren Namen", "tokens": ["Nur", "lei\u00b7se", "nannt'", "ich", "ih\u00b7ren", "Na\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und dr\u00fcckte fester ihre Hand.", "tokens": ["Und", "dr\u00fcck\u00b7te", "fes\u00b7ter", "ih\u00b7re", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Da \u00fcberkam sie's: all mein Sehnen", "tokens": ["Da", "\u00fc\u00b7ber\u00b7kam", "sie's", ":", "all", "mein", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "$.", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "War pl\u00f6tzlich wortlos ihr bewu\u00dft,", "tokens": ["War", "pl\u00f6tz\u00b7lich", "wort\u00b7los", "ihr", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJD", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und hei\u00df bestr\u00f6mt von sel'gen Tr\u00e4nen", "tokens": ["Und", "hei\u00df", "be\u00b7str\u00f6mt", "von", "sel'\u00b7gen", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Barg sie das Haupt an meiner Brust.", "tokens": ["Barg", "sie", "das", "Haupt", "an", "mei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der Fr\u00fchling lie\u00df Maiblumend\u00fcfte", "tokens": ["Der", "Fr\u00fch\u00b7ling", "lie\u00df", "Maib\u00b7lu\u00b7men\u00b7d\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Her\u00fcberwehn vom Waldeshang,", "tokens": ["Her\u00b7\u00fc\u00b7berw\u00b7ehn", "vom", "Wal\u00b7des\u00b7hang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und \u00fcber uns im Blau der L\u00fcfte", "tokens": ["Und", "\u00fc\u00b7ber", "uns", "im", "Blau", "der", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "War nichts als Glanz und Lerchensang.", "tokens": ["War", "nichts", "als", "Glanz", "und", "Ler\u00b7chen\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "KOKOM", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}