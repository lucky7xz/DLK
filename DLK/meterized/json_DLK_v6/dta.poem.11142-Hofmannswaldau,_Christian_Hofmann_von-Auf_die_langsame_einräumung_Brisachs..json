{"dta.poem.11142": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die langsame einr\u00e4umung Brisachs.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Franckreich giebt dich nicht zur\u00fccke, bi\u00df die br\u00fccke ruinirt;", "tokens": ["Fran\u00b7ck\u00b7reich", "giebt", "dich", "nicht", "zu\u00b7r\u00fc\u00b7cke", ",", "bi\u00df", "die", "br\u00fc\u00b7cke", "ru\u00b7i\u00b7nirt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,", "KOUS", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.invert"}, "line.2": {"text": "Doch es sind gar andre t\u00fccken, die man hier im schilde", "tokens": ["Doch", "es", "sind", "gar", "and\u00b7re", "t\u00fc\u00b7cken", ",", "die", "man", "hier", "im", "schil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "PIS", "VVINF", "$,", "PRELS", "PIS", "ADV", "APPRART", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "f\u00fchrt.", "tokens": ["f\u00fchrt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Wieviel monden wird man noch bi\u00df zu deiner freyheit zehlen?", "tokens": ["Wie\u00b7viel", "mon\u00b7den", "wird", "man", "noch", "bi\u00df", "zu", "dei\u00b7ner", "frey\u00b7heit", "zeh\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PIS", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "An den pf\u00e4hlen fehlt es nicht, nur an Ludewigs befehlen.", "tokens": ["An", "den", "pf\u00e4h\u00b7len", "fehlt", "es", "nicht", ",", "nur", "an", "Lu\u00b7de\u00b7wigs", "be\u00b7feh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "ADV", "APPR", "NE", "VVINF", "$."], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}