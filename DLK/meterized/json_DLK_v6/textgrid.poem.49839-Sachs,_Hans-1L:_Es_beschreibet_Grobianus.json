 "punct"], "pos": ["ADV", "VVFIN", "NE", "KON", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.89": {"text": "wenn man dann hebt das tischtuch auf,", "tokens": ["wenn", "man", "dann", "hebt", "das", "tischtuch", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.90": {"text": "sezt darnach ein hantwa\u00dfer drauf,", "tokens": ["sezt", "dar\u00b7nach", "ein", "hant\u00b7wa\u00b7\u00dfer", "drauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "ART", "NN", "PTKVZ", "$,"], "meter": "+------+", "measure": "dactylic.init"}, "line.91": {"text": "wolschmeckend mit kreuter und w\u00fcrzen,", "tokens": ["wol\u00b7schme\u00b7ckend", "mit", "kreu\u00b7ter", "und", "w\u00fcr\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "KON", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.92": {"text": "so tu beid hent ungst\u00fcm drein st\u00fcrzen", "tokens": ["so", "tu", "beid", "hent", "ungs\u00b7t\u00fcm", "drein", "st\u00fcr\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "FM", "FM", "FM", "FM", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.93": {"text": "und bespr\u00fctz all, die herumb sitzen;", "tokens": ["und", "be\u00b7spr\u00fctz", "all", ",", "die", "he\u00b7rumb", "sit\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PIAT", "$,", "PRELS", "ADV", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.94": {"text": "denn spricht iederman wol dein witzen", "tokens": ["denn", "spricht", "ie\u00b7der\u00b7man", "wol", "dein", "wit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIS", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.95": {"text": "und helt dich f\u00fcr ein ordensman", "tokens": ["und", "helt", "dich", "f\u00fcr", "ein", "or\u00b7dens\u00b7man"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.96": {"text": "in dem kloster sant Grobian,", "tokens": ["in", "dem", "klos\u00b7ter", "sant", "Gro\u00b7bi\u00b7an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.97": {"text": "drin man lert weder scham noch zucht,", "tokens": ["drin", "man", "lert", "we\u00b7der", "scham", "noch", "zucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "KON", "ADJD", "ADV", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.98": {"text": "der auch kein mensch mer bei dir sucht.", "tokens": ["der", "auch", "kein", "mensch", "mer", "bei", "dir", "sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.99": {"text": "doch bleibet dir f\u00fcrhin das lob,", "tokens": ["doch", "blei\u00b7bet", "dir", "f\u00fcr\u00b7hin", "das", "lob", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.100": {"text": "du seist unverstanden und grob,", "tokens": ["du", "seist", "un\u00b7ver\u00b7stan\u00b7den", "und", "grob", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.101": {"text": "on alle gut sitten und tugent;", "tokens": ["on", "al\u00b7le", "gut", "sit\u00b7ten", "und", "tu\u00b7gent", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADJD", "VVFIN", "KON", "NN", "$."], "meter": "-+--+-++-", "measure": "iambic.tetra.relaxed"}, "line.102": {"text": "so bleibst ins alter von deinr jugent", "tokens": ["so", "bleibst", "ins", "al\u00b7ter", "von", "deinr", "ju\u00b7gent"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.103": {"text": "samt deinen br\u00fcdern jenseits bachs", "tokens": ["samt", "dei\u00b7nen", "br\u00fc\u00b7dern", "jen\u00b7seits", "bachs"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.104": {"text": "sant Grobians, so spricht Hans Sachs.", "tokens": ["sant", "Gro\u00b7bi\u00b7ans", ",", "so", "spricht", "Hans", "Sachs", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADV", "VVFIN", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}