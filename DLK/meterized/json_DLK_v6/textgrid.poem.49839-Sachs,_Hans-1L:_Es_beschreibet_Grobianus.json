{"textgrid.poem.49839": {"metadata": {"author": {"name": "Sachs, Hans", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es beschreibet Grobianus", "genre": "verse", "period": "N.A.", "pub_year": 1563, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es beschreibet Grobianus", "tokens": ["Es", "be\u00b7schrei\u00b7bet", "Gro\u00b7bi\u00b7a\u00b7nus"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NE"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "in seinem buch Dildappibus,", "tokens": ["in", "sei\u00b7nem", "buch", "Dild\u00b7ap\u00b7pi\u00b7bus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "im drittn capitl Stulticia", "tokens": ["im", "drittn", "ca\u00b7pitl", "Stul\u00b7ti\u00b7cia"], "token_info": ["word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "seinen schulern und spricht allda:", "tokens": ["sei\u00b7nen", "schu\u00b7lern", "und", "spricht", "all\u00b7da", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "KON", "VVFIN", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "h\u00f6r, mensch, wen du zu gast wilt e\u00dfen,", "tokens": ["h\u00f6r", ",", "mensch", ",", "wen", "du", "zu", "gast", "wilt", "e\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "$,", "PWS", "PPER", "PTKZU", "VVFIN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "wasch dein hent nicht, e du bist gse\u00dfen,", "tokens": ["wasch", "dein", "hent", "nicht", ",", "e", "du", "bist", "gse\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "PTKNEG", "$,", "VVFIN", "PPER", "VAFIN", "VVINF", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "sint dein feust gleich des unlust vol,", "tokens": ["sint", "dein", "feust", "gleich", "des", "un\u00b7lust", "vol", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "lang negel zimen dir auch wol,", "tokens": ["lang", "ne\u00b7gel", "zi\u00b7men", "dir", "auch", "wol", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "und setz am tisch dich oben an,", "tokens": ["und", "setz", "am", "tisch", "dich", "o\u00b7ben", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKA", "ADJD", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "setzt gleich der wirt dich nicht hinan,", "tokens": ["setzt", "gleich", "der", "wirt", "dich", "nicht", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.11": {"text": "des benedicite auch vergi\u00df,", "tokens": ["des", "be\u00b7ne\u00b7di\u00b7ci\u00b7te", "auch", "ver\u00b7gi\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VVFIN", "$,"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.12": {"text": "sonder zuck den l\u00f6ffel und i\u00df", "tokens": ["son\u00b7der", "zuck", "den", "l\u00f6f\u00b7fel", "und", "i\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ART", "NN", "KON", "VVFIN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "und greif hinein vor andern alten;", "tokens": ["und", "greif", "hin\u00b7ein", "vor", "an\u00b7dern", "al\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPR", "ADJA", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "tu dich grob und \u00f6lperisch halten,", "tokens": ["tu", "dich", "grob", "und", "\u00f6l\u00b7pe\u00b7risch", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.15": {"text": "am tisch schnaude und seuisch schmatz,", "tokens": ["am", "tisch", "schnau\u00b7de", "und", "seu\u00b7isch", "schmatz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVFIN", "KON", "ADJD", "ADJD", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.16": {"text": "mit ungest\u00fcm nach dem brot platz,", "tokens": ["mit", "un\u00b7ge\u00b7st\u00fcm", "nach", "dem", "brot", "platz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "ein becher zwen umbsto\u00dfen tust,", "tokens": ["ein", "be\u00b7cher", "zwen", "umb\u00b7sto\u00b7\u00dfen", "tust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.18": {"text": "denn schneid das brot an deiner brust,", "tokens": ["denn", "schneid", "das", "brot", "an", "dei\u00b7ner", "brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "das gschnitten brot oder den weck", "tokens": ["das", "gschnit\u00b7ten", "brot", "o\u00b7der", "den", "weck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "fein mit der linken hant bedeck,", "tokens": ["fein", "mit", "der", "lin\u00b7ken", "hant", "be\u00b7deck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.21": {"text": "auf das kein schnitten dir entlauf,", "tokens": ["auf", "das", "kein", "schnit\u00b7ten", "dir", "ent\u00b7lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.22": {"text": "oder mans zel und merk dir drauf.", "tokens": ["o\u00b7der", "mans", "zel", "und", "merk", "dir", "drauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "brock darnach mit den zenen ein,", "tokens": ["brock", "dar\u00b7nach", "mit", "den", "ze\u00b7nen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.24": {"text": "und greif nicht f\u00fcr dein ort allein,", "tokens": ["und", "greif", "nicht", "f\u00fcr", "dein", "ort", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKNEG", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "sonder, sichst vor dem nechsten dort,", "tokens": ["son\u00b7der", ",", "sichst", "vor", "dem", "nechs\u00b7ten", "dort", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "APPR", "ART", "ADJA", "ADV", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.26": {"text": "der sitzet an eim andren ort,", "tokens": ["der", "sit\u00b7zet", "an", "eim", "an\u00b7dren", "ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "etwas ligen, des du lust hast,", "tokens": ["et\u00b7was", "li\u00b7gen", ",", "des", "du", "lust", "hast", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "so denk, ich bin doch auch ein gast,", "tokens": ["so", "denk", ",", "ich", "bin", "doch", "auch", "ein", "gast", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "ADV", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "tu schnell das schleckerbi\u00dflein zwacken", "tokens": ["tu", "schnell", "das", "schle\u00b7cker\u00b7bi\u00df\u00b7lein", "zwa\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "ART", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "und sp\u00fcl mit der zungen dein backen.", "tokens": ["und", "sp\u00fcl", "mit", "der", "zun\u00b7gen", "dein", "ba\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "PPOSAT", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.31": {"text": "tregt man ein eingebicktes her,", "tokens": ["tregt", "man", "ein", "ein\u00b7ge\u00b7bick\u00b7tes", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "PTKVZ", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.32": {"text": "des br\u00fc vol w\u00fcrz und zuckers wer,", "tokens": ["des", "br\u00fc", "vol", "w\u00fcrz", "und", "zu\u00b7ckers", "wer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "KON", "ADV", "PWS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "so nem den l\u00f6ffel allmal vol;", "tokens": ["so", "nem", "den", "l\u00f6f\u00b7fel", "all\u00b7mal", "vol", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.34": {"text": "wenn du dich gleich treifst, es stet wol,", "tokens": ["wenn", "du", "dich", "gleich", "treifst", ",", "es", "stet", "wol", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "das es gleich auf das tischtuch rin,", "tokens": ["das", "es", "gleich", "auf", "das", "tischtuch", "rin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "nimts doch das wa\u00dfer alles hin.", "tokens": ["nimts", "doch", "das", "wa\u00b7\u00dfer", "al\u00b7les", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "nach ander speis greif wider her,", "tokens": ["nach", "an\u00b7der", "speis", "greif", "wi\u00b7der", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJD", "ADJD", "PTKVZ", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "e dir dein munt sei worden ler,", "tokens": ["e", "dir", "dein", "munt", "sei", "wor\u00b7den", "ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPOSAT", "ADJD", "VAFIN", "VAPP", "ADJD", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.39": {"text": "als w\u00f6ll die sch\u00fc\u00dfel dir entlaufen.", "tokens": ["als", "w\u00f6ll", "die", "sch\u00fc\u00b7\u00dfel", "dir", "ent\u00b7lau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "scheub brocken ein mit gro\u00dfem haufen;", "tokens": ["scheub", "bro\u00b7cken", "ein", "mit", "gro\u00b7\u00dfem", "hau\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "mit vollem munt red, sei gefre\u00dfig,", "tokens": ["mit", "vol\u00b7lem", "munt", "red", ",", "sei", "ge\u00b7fre\u00b7\u00dfig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJD", "VVFIN", "$,", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "spr\u00fc umb dich, halt dich gar unme\u00dfig,", "tokens": ["spr\u00fc", "umb", "dich", ",", "halt", "dich", "gar", "un\u00b7me\u00b7\u00dfig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.43": {"text": "der lezt ob der speis sei am tisch,", "tokens": ["der", "lezt", "ob", "der", "speis", "sei", "am", "tisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "KOUS", "ART", "NN", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "zerbrich das fleisch und schneid die fisch.", "tokens": ["zer\u00b7brich", "das", "fleisch", "und", "schneid", "die", "fisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "KON", "VVFIN", "ART", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "kleine fischlein auch schmecken wol,", "tokens": ["klei\u00b7ne", "fisc\u00b7hlein", "auch", "schme\u00b7cken", "wol", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVFIN", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.46": {"text": "der nim auch gro\u00dfe schnitten vol,", "tokens": ["der", "nim", "auch", "gro\u00b7\u00dfe", "schnit\u00b7ten", "vol", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJA", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "wan sie sint klein, nim ir dest mer,", "tokens": ["wan", "sie", "sint", "klein", ",", "nim", "ir", "dest", "mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ADJD", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.48": {"text": "darmit erlangst du rum und er.", "tokens": ["dar\u00b7mit", "er\u00b7langst", "du", "rum", "und", "er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "NE", "KON", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "keu mit dem munde unverschlo\u00dfen,", "tokens": ["keu", "mit", "dem", "mun\u00b7de", "un\u00b7ver\u00b7schlo\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.50": {"text": "schnarch durch die nasen gleich den rossen,", "tokens": ["schnarch", "durch", "die", "na\u00b7sen", "gleich", "den", "ros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "und schlag dein zungen aus dem munt", "tokens": ["und", "schlag", "dein", "zun\u00b7gen", "aus", "dem", "munt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "eben gleich eim fleischhackerhunt,", "tokens": ["e\u00b7ben", "gleich", "eim", "fleischha\u00b7cker\u00b7hunt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.53": {"text": "und leck dich umb das maul herum,", "tokens": ["und", "leck", "dich", "umb", "das", "maul", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": 