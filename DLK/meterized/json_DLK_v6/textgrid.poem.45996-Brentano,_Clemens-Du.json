{"textgrid.poem.45996": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Du", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Erde war gestorben", "tokens": ["Die", "Er\u00b7de", "war", "ge\u00b7stor\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich lebte ganz allein", "tokens": ["Ich", "leb\u00b7te", "ganz", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Sonne war verdorben,", "tokens": ["Die", "Son\u00b7ne", "war", "ver\u00b7dor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bis auf die Augen dein.", "tokens": ["Bis", "auf", "die", "Au\u00b7gen", "dein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du bietest mir zu trinken", "tokens": ["Du", "bie\u00b7test", "mir", "zu", "trin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und blickest mich nicht an", "tokens": ["Und", "bli\u00b7ckest", "mich", "nicht", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "L\u00e4\u00dft du die Augen sinken", "tokens": ["L\u00e4\u00dft", "du", "die", "Au\u00b7gen", "sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So ist's um mich getan.", "tokens": ["So", "ist's", "um", "mich", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Fr\u00fchling regt die Schwingen", "tokens": ["Der", "Fr\u00fch\u00b7ling", "regt", "die", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Erde sehnet sich", "tokens": ["Die", "Er\u00b7de", "seh\u00b7net", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie kann nichts wiederbringen", "tokens": ["Sie", "kann", "nichts", "wie\u00b7der\u00b7brin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als dich, du Gute, dich.", "tokens": ["Als", "dich", ",", "du", "Gu\u00b7te", ",", "dich", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PPER", "NN", "$,", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}