{"dta.poem.10363": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An H. S.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Wie ich dein B\u00fcchlein hastig aufgeschlagen,", "tokens": ["Wie", "ich", "dein", "B\u00fcch\u00b7lein", "has\u00b7tig", "auf\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da gr\u00fc\u00dfen mir entgegen viel vertraute,", "tokens": ["Da", "gr\u00fc\u00b7\u00dfen", "mir", "ent\u00b7ge\u00b7gen", "viel", "ver\u00b7trau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Viel goldne Bilder, die ich weiland schaute", "tokens": ["Viel", "gold\u00b7ne", "Bil\u00b7der", ",", "die", "ich", "wei\u00b7land", "schau\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Knabentraum und in den Kindertagen.", "tokens": ["Im", "Kna\u00b7ben\u00b7traum", "und", "in", "den", "Kin\u00b7der\u00b7ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich sehe wieder stolz gen Himmel ragen", "tokens": ["Ich", "se\u00b7he", "wie\u00b7der", "stolz", "gen", "Him\u00b7mel", "ra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den frommen Dom, den deutscher Glaube baute,", "tokens": ["Den", "from\u00b7men", "Dom", ",", "den", "deut\u00b7scher", "Glau\u00b7be", "bau\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ich h\u00f6r' der Glocken und der Orgel Laute,", "tokens": ["Ich", "h\u00f6r'", "der", "Glo\u00b7cken", "und", "der", "Or\u00b7gel", "Lau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dazwischen klingt's wie s\u00fc\u00dfe Liebesklagen.", "tokens": ["Da\u00b7zwi\u00b7schen", "klingt's", "wie", "s\u00fc\u00b7\u00dfe", "Lie\u00b7bes\u00b7kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wohl seh' ich auch wie sie den Dom umklettern,", "tokens": ["Wohl", "seh'", "ich", "auch", "wie", "sie", "den", "Dom", "um\u00b7klet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KOKOM", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die flinken Zwerglein, die sich dort erfrechen", "tokens": ["Die", "flin\u00b7ken", "Zwerg\u00b7lein", ",", "die", "sich", "dort", "er\u00b7fre\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PRF", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Das h\u00fcbsche Blum- und Schnitzwerk abzubrechen.", "tokens": ["Das", "h\u00fcb\u00b7sche", "Blum", "und", "Schnitz\u00b7werk", "ab\u00b7zu\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "TRUNC", "KON", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Doch mag man immerhin die Eich' entbl\u00e4ttern.", "tokens": ["Doch", "mag", "man", "im\u00b7mer\u00b7hin", "die", "Eich'", "ent\u00b7bl\u00e4t\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und sie des gr\u00fcnen Schmuckes rings berauben, \u2014", "tokens": ["Und", "sie", "des", "gr\u00fc\u00b7nen", "Schmu\u00b7ckes", "rings", "be\u00b7rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "ADV", "VVINF", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Kommt neuer Lenz, wird sie sich neu belauben.", "tokens": ["Kommt", "neu\u00b7er", "Lenz", ",", "wird", "sie", "sich", "neu", "be\u00b7lau\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,", "VAFIN", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}