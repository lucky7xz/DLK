{"dta.poem.5277": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mancherley Vorw\u00fcrffe der Sinnen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jm Garten h\u00f6rt ich j\u00fcngst den s\u00fc\u00df'- und scharffen\nSchall", "tokens": ["Jm", "Gar\u00b7ten", "h\u00f6rt", "ich", "j\u00fcngst", "den", "s\u00fc\u00df'", "und", "scharf\u00b7fen", "Schall"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "ART", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der feurig schlagenden verliebten Nachtigall.", "tokens": ["Der", "feu\u00b7rig", "schla\u00b7gen\u00b7den", "ver\u00b7lieb\u00b7ten", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich ward dadurch ger\u00fchrt, gereitzt, ergetzet", "tokens": ["Ich", "ward", "da\u00b7durch", "ge\u00b7r\u00fchrt", ",", "ge\u00b7reitzt", ",", "er\u00b7get\u00b7zet"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "PAV", "VVPP", "$,", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und, durch den reinen Klang, fast aus mir selbst gesetzet.", "tokens": ["Und", ",", "durch", "den", "rei\u00b7nen", "Klang", ",", "fast", "aus", "mir", "selbst", "ge\u00b7set\u00b7zet", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich horcht\u2019 aufmercksam zu, wie lieblich, s\u00fc\u00df und hell,", "tokens": ["Ich", "horcht'", "auf\u00b7merck\u00b7sam", "zu", ",", "wie", "lieb\u00b7lich", ",", "s\u00fc\u00df", "und", "hell", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$,", "PWAV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie scharf, wie rein, wie rund, wie hohl, wie tief, wie", "tokens": ["Wie", "scharf", ",", "wie", "rein", ",", "wie", "rund", ",", "wie", "hohl", ",", "wie", "tief", ",", "wie"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "$,", "PWAV", "ADJD", "$,", "PWAV", "ADJD", "$,", "PWAV", "ADJD", "$,", "PWAV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sie Stimm\u2019 und Ton formirt, ver\u00e4ndert, theilet, f\u00fcgt,", "tokens": ["Sie", "Stimm'", "und", "Ton", "for\u00b7mirt", ",", "ver\u00b7\u00e4n\u00b7dert", ",", "thei\u00b7let", ",", "f\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "VVPP", "$,", "VVPP", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und, durch unz\u00e4hliche Ver\u00e4ndrung, uns vergn\u00fcgt.", "tokens": ["Und", ",", "durch", "un\u00b7z\u00e4h\u00b7li\u00b7che", "Ver\u00b7\u00e4n\u00b7drung", ",", "uns", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ADJA", "NN", "$,", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Oft wei\u00df sie Schnarren, Fl\u00f6ten, Zischen,", "tokens": ["Oft", "wei\u00df", "sie", "Schnar\u00b7ren", ",", "Fl\u00f6\u00b7ten", ",", "Zi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "In unbegreiflicher Geschwindigkeit, zu mischen.", "tokens": ["In", "un\u00b7be\u00b7greif\u00b7li\u00b7cher", "Ge\u00b7schwin\u00b7dig\u00b7keit", ",", "zu", "mi\u00b7schen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Oft f\u00e4ngt sie einen Ton mit hellem Fl\u00f6ten an,", "tokens": ["Oft", "f\u00e4ngt", "sie", "ei\u00b7nen", "Ton", "mit", "hel\u00b7lem", "Fl\u00f6\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "F\u00e4llt in ein Zwitschern, schl\u00e4gt, lockt, winselt, jauch-", "tokens": ["F\u00e4llt", "in", "ein", "Zwit\u00b7schern", ",", "schl\u00e4gt", ",", "lockt", ",", "win\u00b7selt", ",", "jauch"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und alles fast zugleich: oft bricht sie ihn, oft dehnt,", "tokens": ["Und", "al\u00b7les", "fast", "zu\u00b7gleich", ":", "oft", "bricht", "sie", "ihn", ",", "oft", "dehnt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "ADV", "$.", "ADV", "VVFIN", "PPER", "PPER", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Oft drehet sie den Ton, oft wirbelt sie den Klang,", "tokens": ["Oft", "dre\u00b7het", "sie", "den", "Ton", ",", "oft", "wir\u00b7belt", "sie", "den", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Und \u00e4ndert tausendfach den fr\u00f6lichen Gesang.", "tokens": ["Und", "\u00e4n\u00b7dert", "tau\u00b7send\u00b7fach", "den", "fr\u00f6\u00b7li\u00b7chen", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Indem ich nun, bey einer dicken Hecken,", "tokens": ["In\u00b7dem", "ich", "nun", ",", "bey", "ei\u00b7ner", "di\u00b7cken", "He\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Zu Ende der bewachsenen Allee,", "tokens": ["Zu", "En\u00b7de", "der", "be\u00b7wach\u00b7se\u00b7nen", "Al\u00b7lee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "In dem ", "tokens": ["In", "dem"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Erblick ich ungefehr an einer Ecken", "tokens": ["Er\u00b7blick", "ich", "un\u00b7ge\u00b7fehr", "an", "ei\u00b7ner", "E\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Ein gleichsam buntes Licht. Es legte mir,", "tokens": ["Ein", "gleich\u00b7sam", "bun\u00b7tes", "Licht", ".", "Es", "leg\u00b7te", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "In einer mehr als g\u00fcldnen Zier,", "tokens": ["In", "ei\u00b7ner", "mehr", "als", "g\u00fcld\u00b7nen", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Ja, die sich von Smaragd, Sapphier", "tokens": ["Ja", ",", "die", "sich", "von", "Sma\u00b7ragd", ",", "Sap\u00b7phier"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PTKANT", "$,", "PRELS", "PRF", "APPR", "NN", "$,", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.23": {"text": "Und anderm Edelstein kaum unterscheidet,", "tokens": ["Und", "an\u00b7derm", "E\u00b7del\u00b7stein", "kaum", "un\u00b7ter\u00b7schei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Ein \u00fcber-wunder-sch\u00f6ner Pfan,", "tokens": ["Ein", "\u00fc\u00b7ber\u00b7wun\u00b7der\u00b7sch\u00f6\u00b7ner", "Pfan", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "In pr\u00e4chtigen Talar von g\u00fcldnem St\u00fcck gekleidet,", "tokens": ["In", "pr\u00e4ch\u00b7ti\u00b7gen", "Ta\u00b7lar", "von", "g\u00fcld\u00b7nem", "St\u00fcck", "ge\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der Federn bunten Glantz und Herrlichkeit zur Schau.", "tokens": ["Der", "Fe\u00b7dern", "bun\u00b7ten", "Glantz", "und", "Herr\u00b7lich\u00b7keit", "zur", "Schau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich stutzt\u2019 und meine Seel\u2019 empfand, wie diese Pracht", "tokens": ["Ich", "stutzt'", "und", "mei\u00b7ne", "Seel'", "emp\u00b7fand", ",", "wie", "die\u00b7se", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "PPOSAT", "NN", "VVFIN", "$,", "PWAV", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie auch durchs ", "tokens": ["Sie", "auch", "durchs"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADV", "APPRART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "F\u00fcr Anmuth halb verwirrt, fiel mir hier\u00fcber bey:", "tokens": ["F\u00fcr", "An\u00b7muth", "halb", "ver\u00b7wirrt", ",", "fiel", "mir", "hier\u00b7\u00fc\u00b7ber", "bey", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ADJD", "$,", "VVFIN", "PPER", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie doch in der Natur so mancherley", "tokens": ["Wie", "doch", "in", "der", "Na\u00b7tur", "so", "man\u00b7cher\u00b7ley"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "ART", "NN", "ADV", "PIAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ver\u00e4nderung und Pracht, an Lust und Sch\u00f6nheit, sey.", "tokens": ["Ver\u00b7\u00e4n\u00b7de\u00b7rung", "und", "Pracht", ",", "an", "Lust", "und", "Sch\u00f6n\u00b7heit", ",", "sey", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$,", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Man sp\u00fcrt es sonderlich an diesem V\u00f6gel-Par.", "tokens": ["Man", "sp\u00fcrt", "es", "son\u00b7der\u00b7lich", "an", "die\u00b7sem", "V\u00f6\u00b7gel\u00b7Par", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein unsern Geist bezaubernd Singen", "tokens": ["Ein", "un\u00b7sern", "Geist", "be\u00b7zau\u00b7bernd", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "L\u00e4\u00dft von der gantzen V\u00f6gel-Schar", "tokens": ["L\u00e4\u00dft", "von", "der", "gant\u00b7zen", "V\u00f6\u00b7gel\u00b7Schar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Unansehnlichste, zu unsrer Lust, erklingen;", "tokens": ["Der", "Un\u00b7an\u00b7sehn\u00b7lichs\u00b7te", ",", "zu", "uns\u00b7rer", "Lust", ",", "er\u00b7klin\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und ein verdrie\u00dfliches und rauh Geschrey erschallt", "tokens": ["Und", "ein", "ver\u00b7drie\u00df\u00b7li\u00b7ches", "und", "rauh", "Ge\u00b7schrey", "er\u00b7schallt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "KON", "ADJD", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Aus eines Vogels Hals, der Himmlisch an Gestalt", "tokens": ["Aus", "ei\u00b7nes", "Vo\u00b7gels", "Hals", ",", "der", "Himm\u00b7lisch", "an", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "$,", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Fast mehr, als irdisch, ist. Di\u00df kan ein Beyspiel seyn,", "tokens": ["Fast", "mehr", ",", "als", "ir\u00b7disch", ",", "ist", ".", "Di\u00df", "kan", "ein", "Bey\u00b7spiel", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "ADJD", "$,", "VAFIN", "$.", "PDS", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dacht ich, da\u00df einer alle Gaben", "tokens": ["Dacht", "ich", ",", "da\u00df", "ei\u00b7ner", "al\u00b7le", "Ga\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Nicht leichtlich soll beysammen haben.", "tokens": ["Nicht", "leicht\u00b7lich", "soll", "bey\u00b7sam\u00b7men", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Kaum aber hatt ich die\u00df gedacht,", "tokens": ["Kaum", "a\u00b7ber", "hatt", "ich", "die\u00df", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PDS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Als mir, in Purpur-farbner Pracht,", "tokens": ["Als", "mir", ",", "in", "Pur\u00b7pur\u00b7fa\u00b7rb\u00b7ner", "Pracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.17": {"text": "Ein frischer Rosen-Busch schnell in die Augen fiel.", "tokens": ["Ein", "fri\u00b7scher", "Ro\u00b7sen\u00b7Busch", "schnell", "in", "die", "Au\u00b7gen", "fiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der aber ward nicht nur allein der ", "tokens": ["Der", "a\u00b7ber", "ward", "nicht", "nur", "al\u00b7lein", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VAFIN", "PTKNEG", "ADV", "ADV", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Er ward auch des ", "tokens": ["Er", "ward", "auch", "des"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.20": {"text": "Die beide sich daran recht zu ergetzen taugen.", "tokens": ["Die", "bei\u00b7de", "sich", "da\u00b7ran", "recht", "zu", "er\u00b7get\u00b7zen", "tau\u00b7gen",