{"textgrid.poem.37396": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es gr\u00fcnte allenthalben.", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es gr\u00fcnte allenthalben.", "tokens": ["Es", "gr\u00fcn\u00b7te", "al\u00b7len\u00b7thal\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Fr\u00fchling wurde wach.", "tokens": ["Der", "Fr\u00fch\u00b7ling", "wur\u00b7de", "wach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bald flogen auch die Schwalben", "tokens": ["Bald", "flo\u00b7gen", "auch", "die", "Schwal\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hell zwitschernd um das Dach.", "tokens": ["Hell", "zwit\u00b7schernd", "um", "das", "Dach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie sangen unerm\u00fcdlich", "tokens": ["Sie", "san\u00b7gen", "un\u00b7er\u00b7m\u00fcd\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und bauten au\u00dferdem", "tokens": ["Und", "bau\u00b7ten", "au\u00b7\u00dfer\u00b7dem"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PAV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am Giebel, rund und niedlich", "tokens": ["Am", "Gie\u00b7bel", ",", "rund", "und", "nied\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr Nest aus feuchtem Lehm.", "tokens": ["Ihr", "Nest", "aus", "feuch\u00b7tem", "Lehm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und als sie eine Woche", "tokens": ["Und", "als", "sie", "ei\u00b7ne", "Wo\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich redlich abgequ\u00e4lt,", "tokens": ["Sich", "red\u00b7lich", "ab\u00b7ge\u00b7qu\u00e4lt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hat nur am Eingangsloche", "tokens": ["Hat", "nur", "am", "Ein\u00b7gangs\u00b7lo\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein St\u00fcckchen noch gefehlt.", "tokens": ["Ein", "St\u00fcck\u00b7chen", "noch", "ge\u00b7fehlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da nahm der Spatz, der Schlingel,", "tokens": ["Da", "nahm", "der", "Spatz", ",", "der", "Schlin\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Wohnung in Besitz.", "tokens": ["Die", "Woh\u00b7nung", "in", "Be\u00b7sitz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt h\u00e4ngt ein Strohgekl\u00fcngel", "tokens": ["Jetzt", "h\u00e4ngt", "ein", "Stroh\u00b7ge\u00b7kl\u00fcn\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hervor aus ihrem Schlitz.", "tokens": ["Her\u00b7vor", "aus", "ih\u00b7rem", "Schlitz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nicht sch\u00f6n ist dies Gebahren", "tokens": ["Nicht", "sch\u00f6n", "ist", "dies", "Ge\u00b7bah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "VAFIN", "PDS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und wenig ehrenwert", "tokens": ["Und", "we\u00b7nig", "eh\u00b7ren\u00b7wert"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIS", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von einem, der seit Jahren", "tokens": ["Von", "ei\u00b7nem", ",", "der", "seit", "Jah\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Menschen viel verkehrt.", "tokens": ["Mit", "Men\u00b7schen", "viel", "ver\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}