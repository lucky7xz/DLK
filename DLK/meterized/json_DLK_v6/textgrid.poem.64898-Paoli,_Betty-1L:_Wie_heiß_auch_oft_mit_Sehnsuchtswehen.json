{"textgrid.poem.64898": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie hei\u00df auch oft mit Sehnsuchtswehen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie hei\u00df auch oft mit Sehnsuchtswehen", "tokens": ["Wie", "hei\u00df", "auch", "oft", "mit", "Sehn\u00b7suchts\u00b7we\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Bild die Seele mir durchzieht;", "tokens": ["Dein", "Bild", "die", "See\u00b7le", "mir", "durch\u00b7zieht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich hab dich nimmermehr gesehen", "tokens": ["Ich", "hab", "dich", "nim\u00b7mer\u00b7mehr", "ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Seit jenem Tage, der uns schied.", "tokens": ["Seit", "je\u00b7nem", "Ta\u00b7ge", ",", "der", "uns", "schied", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Noch lebt er fort in meinem Herzen", "tokens": ["Noch", "lebt", "er", "fort", "in", "mei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der finstre, sonnenlose Tag,", "tokens": ["Der", "finst\u00b7re", ",", "son\u00b7nen\u00b7lo\u00b7se", "Tag", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo ich im wilden Brand der Schmerzen", "tokens": ["Wo", "ich", "im", "wil\u00b7den", "Brand", "der", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu deinen F\u00fc\u00dfen jammernd lag!", "tokens": ["Zu", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen", "jam\u00b7mernd", "lag", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo ich dein Haupt, das sch\u00f6ne, bleiche,", "tokens": ["Wo", "ich", "dein", "Haupt", ",", "das", "sch\u00f6\u00b7ne", ",", "blei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zum letzten Mal gesegnet hab',", "tokens": ["Zum", "letz\u00b7ten", "Mal", "ge\u00b7seg\u00b7net", "hab'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dann mein innerst Selbst als Leiche", "tokens": ["Und", "dann", "mein", "in\u00b7nerst", "Selbst", "als", "Lei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPOSAT", "ADV", "ADV", "KOUS", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Bestattet in der Liebe Grab.", "tokens": ["Be\u00b7stat\u00b7tet", "in", "der", "Lie\u00b7be", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Seit jener nachtumflorten Stunde", "tokens": ["Seit", "je\u00b7ner", "nach\u00b7tum\u00b7flor\u00b7ten", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sah dich mein d\u00fcstres Auge nicht!", "tokens": ["Sah", "dich", "mein", "d\u00fcst\u00b7res", "Au\u00b7ge", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl ward durch Andre mir die Kunde,", "tokens": ["Wohl", "ward", "durch", "And\u00b7re", "mir", "die", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PIS", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df du noch wandelst hier im Licht;", "tokens": ["Da\u00df", "du", "noch", "wan\u00b7delst", "hier", "im", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Mir aber bist du abgeschieden,", "tokens": ["Mir", "a\u00b7ber", "bist", "du", "ab\u00b7ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Entr\u00fccket diesem ird'schen Ort,", "tokens": ["Ent\u00b7r\u00fc\u00b7cket", "die\u00b7sem", "ird'\u00b7schen", "Ort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und lebst in meiner Brust in Frieden", "tokens": ["Und", "lebst", "in", "mei\u00b7ner", "Brust", "in", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur der Erinn'rung Leben fort!", "tokens": ["Nur", "der", "Er\u00b7inn'\u00b7rung", "Le\u00b7ben", "fort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}