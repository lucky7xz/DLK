{"textgrid.poem.66659": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Willst du dich \u00f6ffnen,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du dich \u00f6ffnen,", "tokens": ["Willst", "du", "dich", "\u00f6ff\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Mein \u00fcbervolles Herz,", "tokens": ["Mein", "\u00fc\u00b7ber\u00b7vol\u00b7les", "Herz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Um auszusch\u00fctten", "tokens": ["Um", "aus\u00b7zu\u00b7sch\u00fct\u00b7ten"], "token_info": ["word", "word"], "pos": ["KOUI", "VVIZU"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Deines Segens Last?", "tokens": ["Dei\u00b7nes", "Se\u00b7gens", "Last", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "In dieses goldnen", "tokens": ["In", "die\u00b7ses", "gold\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Herbsttags F\u00fclle", "tokens": ["Herbst\u00b7tags", "F\u00fcl\u00b7le"], "token_info": ["word", "word"], "pos": ["ADV", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Lohnt es zu schenken", "tokens": ["Lohnt", "es", "zu", "schen\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Mit andern Fr\u00fcchten", "tokens": ["Mit", "an\u00b7dern", "Fr\u00fcch\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Der Seele Frucht.", "tokens": ["Der", "See\u00b7le", "Frucht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Gefunden hab' ich", "tokens": ["Ge\u00b7fun\u00b7den", "hab'", "ich"], "token_info": ["word", "word", "word"], "pos": ["NN", "VAFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Macht in mir selbst,", "tokens": ["Macht", "in", "mir", "selbst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Fest hier auf Erden", "tokens": ["Fest", "hier", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Nun steht mein Fu\u00df,", "tokens": ["Nun", "steht", "mein", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und alle Strudel,", "tokens": ["Und", "al\u00b7le", "Stru\u00b7del", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Drin ich gewirbelt,", "tokens": ["Drin", "ich", "ge\u00b7wir\u00b7belt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Sind abgeglitten", "tokens": ["Sind", "ab\u00b7ge\u00b7glit\u00b7ten"], "token_info": ["word", "word"], "pos": ["VAFIN", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Von meinem Haupt.", "tokens": ["Von", "mei\u00b7nem", "Haupt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Nun mag mir reifen", "tokens": ["Nun", "mag", "mir", "rei\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Des Lebens Saat,", "tokens": ["Des", "Le\u00b7bens", "Saat", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die Blitz und Hagel", "tokens": ["Die", "Blitz", "und", "Ha\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Doch nicht zerst\u00f6rt.", "tokens": ["Doch", "nicht", "zer\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ich darf erwarten", "tokens": ["Ich", "darf", "er\u00b7war\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Der Ernte Tag:", "tokens": ["Der", "Ern\u00b7te", "Tag", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Bin vor der Zeit nicht verdorben.", "tokens": ["Bin", "vor", "der", "Zeit", "nicht", "ver\u00b7dor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Wer f\u00fchlt sich Blume,", "tokens": ["Wer", "f\u00fchlt", "sich", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Wer f\u00fchlt sich Fackel,", "tokens": ["Wer", "f\u00fchlt", "sich", "Fa\u00b7ckel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Duftet und gl\u00fcht", "tokens": ["Duf\u00b7tet", "und", "gl\u00fcht"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "In Garten und Welt?", "tokens": ["In", "Gar\u00b7ten", "und", "Welt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "O meine unverwelkte Seele,", "tokens": ["O", "mei\u00b7ne", "un\u00b7ver\u00b7welk\u00b7te", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wes ist die Kraft,", "tokens": ["Wes", "ist", "die", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Die dich hebt und h\u00e4lt?", "tokens": ["Die", "dich", "hebt", "und", "h\u00e4lt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Und sank auch manches", "tokens": ["Und", "sank", "auch", "man\u00b7ches"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PIS"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Glaubens Blendwerk", "tokens": ["Glau\u00b7bens", "Blend\u00b7werk"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Vor deinem glanzbetrogenen Blick,", "tokens": ["Vor", "dei\u00b7nem", "glanz\u00b7be\u00b7tro\u00b7ge\u00b7nen", "Blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Dein tiefstes F\u00fchlen", "tokens": ["Dein", "tiefs\u00b7tes", "F\u00fch\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Lebt unverdunkelt,", "tokens": ["Lebt", "un\u00b7ver\u00b7dun\u00b7kelt", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Stern deiner Sterne", "tokens": ["Stern", "dei\u00b7ner", "Ster\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Ob allem Geschick.", "tokens": ["Ob", "al\u00b7lem", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Dich reizt zum Leben", "tokens": ["Dich", "reizt", "zum", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Der Seele Wollust,", "tokens": ["Der", "See\u00b7le", "Wol\u00b7lust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Sich aufzuschlie\u00dfen", "tokens": ["Sich", "auf\u00b7zu\u00b7schlie\u00b7\u00dfen"], "token_info": ["word", "word"], "pos": ["PRF", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Nach freier Wahl;", "tokens": ["Nach", "frei\u00b7er", "Wahl", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Du magst nicht schielen", "tokens": ["Du", "magst", "nicht", "schie\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Nach fremden Augen,", "tokens": ["Nach", "frem\u00b7den", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Dich lockt und leitet", "tokens": ["Dich", "lockt", "und", "lei\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Der eigne Strahl.", "tokens": ["Der", "eig\u00b7ne", "Strahl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Aus wilden Feuern,", "tokens": ["Aus", "wil\u00b7den", "Feu\u00b7ern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die lodernd sengten,", "tokens": ["Die", "lo\u00b7dernd", "seng\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Gegl\u00fcht sind Ringe,", "tokens": ["Ge\u00b7gl\u00fcht", "sind", "Rin\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Die dich umziehn.", "tokens": ["Die", "dich", "um\u00b7ziehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVIZU", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Du bist von kr\u00e4ftigen", "tokens": ["Du", "bist", "von", "kr\u00e4f\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Kreisen umh\u00fctet,", "tokens": ["Krei\u00b7sen", "um\u00b7h\u00fc\u00b7tet", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Davor die Schrecken", "tokens": ["Da\u00b7vor", "die", "Schre\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["PAV", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Des Daseins fliehn.", "tokens": ["Des", "Da\u00b7seins", "fliehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "In blaue Tiefe nun tauche,", "tokens": ["In", "blau\u00b7e", "Tie\u00b7fe", "nun", "tau\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mein Auge, sonnenverwandt!", "tokens": ["Mein", "Au\u00b7ge", ",", "son\u00b7nen\u00b7ver\u00b7wandt", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dankbar die Lippe hauche:", "tokens": ["Dank\u00b7bar", "die", "Lip\u00b7pe", "hau\u00b7che", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Erde, segnende Erde,", "tokens": ["Er\u00b7de", ",", "seg\u00b7nen\u00b7de", "Er\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Dich lieb' ich, m\u00fctterlich Land.", "tokens": ["Dich", "lieb'", "ich", ",", "m\u00fct\u00b7ter\u00b7lich", "Land", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJD", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}