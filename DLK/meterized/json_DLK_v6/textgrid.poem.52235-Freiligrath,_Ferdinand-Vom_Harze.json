{"textgrid.poem.52235": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Vom Harze", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O stille, graue Fr\u00fche!", "tokens": ["O", "stil\u00b7le", ",", "grau\u00b7e", "Fr\u00fc\u00b7he", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Bl\u00e4tter fl\u00fcstern sacht;", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "fl\u00fcs\u00b7tern", "sacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Hirsch hat seine K\u00fche", "tokens": ["Der", "Hirsch", "hat", "sei\u00b7ne", "K\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Waldrand schon gebracht.", "tokens": ["Zum", "Wald\u00b7rand", "schon", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zum Waldrand in die Saaten!", "tokens": ["Zum", "Wald\u00b7rand", "in", "die", "Saa\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da steht und stampft er schon!", "tokens": ["Da", "steht", "und", "stampft", "er", "schon", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Im Busch ruhn die Kossaten,", "tokens": ["Im", "Busch", "ruhn", "die", "Kos\u00b7sa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Vater und sein Sohn.", "tokens": ["Der", "Va\u00b7ter", "und", "sein", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Alte wiegt in H\u00e4nden", "tokens": ["Der", "Al\u00b7te", "wiegt", "in", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den rost'gen Flintenlauf.", "tokens": ["Den", "rost'\u00b7gen", "Flin\u00b7ten\u00b7lauf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbein Hirsch von vierzehn Enden!", "tokens": ["\u00bb", "ein", "Hirsch", "von", "vier\u00b7zehn", "En\u00b7den", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kerl, Schwerenot, halt drauf!\u00ab", "tokens": ["Kerl", ",", "Schwe\u00b7re\u00b7not", ",", "halt", "drauf", "!", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Junge dr\u00fcckt \u2013 ein Knallen!", "tokens": ["Der", "Jun\u00b7ge", "dr\u00fcckt", "\u2013", "ein", "Knal\u00b7len", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das hei\u00df' ich gute Pirsch!", "tokens": ["Das", "hei\u00df'", "ich", "gu\u00b7te", "Pirsch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie sehn zur Erde fallen", "tokens": ["Sie", "sehn", "zur", "Er\u00b7de", "fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den vierzehnd'gen Hirsch!", "tokens": ["Den", "vier\u00b7zehn\u00b7d'\u00b7gen", "Hirsch", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Fortstieben rings die K\u00fche \u2013", "tokens": ["Fort\u00b7stie\u00b7ben", "rings", "die", "K\u00fc\u00b7he", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Alte ruft: \u00bbO Gl\u00fcck!\u00ab", "tokens": ["Der", "Al\u00b7te", "ruft", ":", "\u00bb", "O", "Gl\u00fcck", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "NE", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "St\u00fcrzt vor und stemmt die Knie", "tokens": ["St\u00fcrzt", "vor", "und", "stemmt", "die", "Knie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Auf das erlegte St\u00fcck.", "tokens": ["Auf", "das", "er\u00b7leg\u00b7te", "St\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbei, Bursch, du zieltest wacker!", "tokens": ["\u00bb", "ei", ",", "Bursch", ",", "du", "ziel\u00b7test", "wa\u00b7cker", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "NN", "$,", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sieh selber \u2013 grad' aufs Blatt!", "tokens": ["Sieh", "sel\u00b7ber", "\u2013", "grad'", "aufs", "Blatt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$(", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gott segn' es unserm Acker \u2013", "tokens": ["Gott", "segn'", "es", "un\u00b7serm", "A\u00b7cker", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der fri\u00dft sich nicht mehr satt!", "tokens": ["Der", "fri\u00dft", "sich", "nicht", "mehr", "satt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dem ist kein Korn mehr n\u00fctze,", "tokens": ["Dem", "ist", "kein", "Korn", "mehr", "n\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der biegt kein H\u00e4lmlein mehr,", "tokens": ["Der", "biegt", "kein", "H\u00e4lm\u00b7lein", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der \u2013 nun, was gaffst du, Fritze?", "tokens": ["Der", "\u2013", "nun", ",", "was", "gaffst", "du", ",", "Frit\u00b7ze", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "$(", "ADV", "$,", "PWS", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Rasch! gib die Stricke her!", "tokens": ["Rasch", "!", "gib", "die", "Stri\u00b7cke", "her", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "VVIMP", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So \u2013 Fu\u00df an Fu\u00df gebunden!", "tokens": ["So", "\u2013", "Fu\u00df", "an", "Fu\u00df", "ge\u00b7bun\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fchl' doch, er wird schon kalt!\u00ab \u2013", "tokens": ["F\u00fchl'", "doch", ",", "er", "wird", "schon", "kalt", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da tritt mit Volk und Hunden", "tokens": ["Da", "tritt", "mit", "Volk", "und", "Hun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der F\u00f6rster aus dem Wald.", "tokens": ["Der", "F\u00f6rs\u00b7ter", "aus", "dem", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Hilf Gott, der kennt die Schliche!", "tokens": ["Hilf", "Gott", ",", "der", "kennt", "die", "Schli\u00b7che", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nun gilt's! Aufspringt das Paar,", "tokens": ["Nun", "gilt's", "!", "Auf\u00b7springt", "das", "Paar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Rei\u00dft aus und l\u00e4\u00dft im Stiche", "tokens": ["Rei\u00dft", "aus", "und", "l\u00e4\u00dft", "im", "Sti\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Doppell\u00e4ufe gar!", "tokens": ["Die", "Dop\u00b7pel\u00b7l\u00e4u\u00b7fe", "gar", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der F\u00f6rster bleibt nicht hinten,", "tokens": ["Der", "F\u00f6rs\u00b7ter", "bleibt", "nicht", "hin\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nachruft er: \u00bbSteh, Gez\u00fccht!", "tokens": ["Nach\u00b7ruft", "er", ":", "\u00bb", "Steh", ",", "Ge\u00b7z\u00fccht", "!"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "$(", "VVFIN", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Was helfen mir die Flinten,", "tokens": ["Was", "hel\u00b7fen", "mir", "die", "Flin\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Hab' ich die Sch\u00fctzen nicht?\u00ab", "tokens": ["Hab'", "ich", "die", "Sch\u00fct\u00b7zen", "nicht", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PTKNEG", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Umsonst! \u2013 Da rasch zur Wange", "tokens": ["Um\u00b7sonst", "!", "\u2013", "Da", "rasch", "zur", "Wan\u00b7ge"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$.", "$(", "ADV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hebr er der B\u00fcchse Wucht!", "tokens": ["Hebr", "er", "der", "B\u00fcch\u00b7se", "Wucht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Zielt \u2013 kalt und fest und lange!", "tokens": ["Zielt", "\u2013", "kalt", "und", "fest", "und", "lan\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "ADJD", "KON", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was \u2013 Menschen? \u2013 Auf der Flucht?", "tokens": ["Was", "\u2013", "Men\u00b7schen", "?", "\u2013", "Auf", "der", "Flucht", "?"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "$(", "NN", "$.", "$(", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gleichviel! Er dr\u00fcckt \u2013 ein Knallen!", "tokens": ["Gleich\u00b7viel", "!", "Er", "dr\u00fcckt", "\u2013", "ein", "Knal\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VVFIN", "$(", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hallo, das hei\u00df' ich Gl\u00fcck!", "tokens": ["Hal\u00b7lo", ",", "das", "hei\u00df'", "ich", "Gl\u00fcck", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PDS", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den Alten sieht er fallen \u2013", "tokens": ["Den", "Al\u00b7ten", "sieht", "er", "fal\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Er traf ihn ins Genick!", "tokens": ["Er", "traf", "ihn", "ins", "Ge\u00b7nick", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "In seiner eignen Gerste", "tokens": ["In", "sei\u00b7ner", "eig\u00b7nen", "Gers\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Daliegt der knochige Mann;", "tokens": ["Da\u00b7liegt", "der", "kno\u00b7chi\u00b7ge", "Mann", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Als ob das Herz ihm berste,", "tokens": ["Als", "ob", "das", "Herz", "ihm", "bers\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auffst\u00f6hnt er dann und wann!", "tokens": ["Auff\u00b7st\u00f6hnt", "er", "dann", "und", "wann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KON", "PWAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sein Blut, dem Wams entquollen,", "tokens": ["Sein", "Blut", ",", "dem", "Wams", "ent\u00b7quol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Rinnt ab in Furch' und Spur;", "tokens": ["Rinnt", "ab", "in", "Furch'", "und", "Spur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Warm sickert's durch die Schollen \u2013", "tokens": ["Warm", "si\u00b7ckert's", "durch", "die", "Schol\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Was denkt die Lerche nur?", "tokens": ["Was", "denkt", "die", "Ler\u00b7che", "nur", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sie sitzt im stillen Neste \u2013", "tokens": ["Sie", "sitzt", "im", "stil\u00b7len", "Nes\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da schie\u00dft das Blut herein!", "tokens": ["Da", "schie\u00dft", "das", "Blut", "her\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aufschwirrt sie gleich zur Feste,", "tokens": ["Auf\u00b7schwirrt", "sie", "gleich", "zur", "Fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Blut an den Fl\u00fcgelein!", "tokens": ["Blut", "an", "den", "Fl\u00fc\u00b7ge\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Sie l\u00e4\u00dft vor Gott es blitzen", "tokens": ["Sie", "l\u00e4\u00dft", "vor", "Gott", "es", "blit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im ersten Sonnenblick,", "tokens": ["Im", "ers\u00b7ten", "Son\u00b7nen\u00b7blick", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sprengt auf die Halmenspitzen", "tokens": ["Sprengt", "auf", "die", "Hal\u00b7men\u00b7spit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Es schmetternd dann zur\u00fcck!", "tokens": ["Es", "schmet\u00b7ternd", "dann", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Das ist ein kr\u00e4ftiger Regen,", "tokens": ["Das", "ist", "ein", "kr\u00e4f\u00b7ti\u00b7ger", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das ist ein kostbar Spr\u00fchn!", "tokens": ["Das", "ist", "ein", "kost\u00b7bar", "Spr\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das ist einLerchensegen,", "tokens": ["Das", "ist", "ein", "Ler\u00b7chen\u00b7se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der macht die Saaten gr\u00fcn!", "tokens": ["Der", "macht", "die", "Saa\u00b7ten", "gr\u00fcn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der tropft auch auf den Jungen,", "tokens": ["Der", "tropft", "auch", "auf", "den", "Jun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der hinrast \u00fcbers Feld", "tokens": ["Der", "hin\u00b7rast", "\u00fc\u00b7bers", "Feld"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und heulend dann umschlungen", "tokens": ["Und", "heu\u00b7lend", "dann", "um\u00b7schlun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den toten Vater h\u00e4lt!", "tokens": ["Den", "to\u00b7ten", "Va\u00b7ter", "h\u00e4lt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Fort, Bursch! Was noch umklammern", "tokens": ["Fort", ",", "Bursch", "!", "Was", "noch", "um\u00b7klam\u00b7mern"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NN", "$.", "PWS", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die starre Mannsgestalt!", "tokens": ["Die", "star\u00b7re", "Manns\u00b7ge\u00b7stalt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Fort nun, und la\u00df dein Jammern \u2013", "tokens": ["Fort", "nun", ",", "und", "la\u00df", "dein", "Jam\u00b7mern", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "KON", "VVIMP", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u00bbf\u00fchl' doch, er wird schon kalt!\u00ab", "tokens": ["\u00bb", "f\u00fchl'", "doch", ",", "er", "wird", "schon", "kalt", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zur\u00fcck vom blauen Munde", "tokens": ["Zu\u00b7r\u00fcck", "vom", "blau\u00b7en", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit deinem roten! \u2013 Sieh,", "tokens": ["Mit", "dei\u00b7nem", "ro\u00b7ten", "!", "\u2013", "Sieh", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$.", "$(", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ankeuchen schon die Hunde \u2013", "tokens": ["An\u00b7keu\u00b7chen", "schon", "die", "Hun\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Herr Gott, zum \u00bbHalali!\u00ab", "tokens": ["Herr", "Gott", ",", "zum", "\u00bb", "Ha\u00b7la\u00b7li", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "NN", "$,", "APPRART", "$(", "NE", "$.", "$("], "meter": "-+-++-", "measure": "unknown.measure.tri"}}, "stanza.11": {"line.1": {"text": "Stracks ruhn auf ", "tokens": ["Stracks", "ruhn", "auf"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVINF", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Der Hirsch und auch der Mann!", "tokens": ["Der", "Hirsch", "und", "auch", "der", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zum Not- und Schwarzwildscharren", "tokens": ["Zum", "Not", "und", "Schwarz\u00b7wild\u00b7schar\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "TRUNC", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fortgeht es durch den Tann!", "tokens": ["Fort\u00b7geht", "es", "durch", "den", "Tann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Fortgeht's in einer Hetze \u2013", "tokens": ["Fort\u00b7geht's", "in", "ei\u00b7ner", "Het\u00b7ze", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der F\u00f6rster pfeift und lacht!", "tokens": ["Der", "F\u00f6rs\u00b7ter", "pfeift", "und", "lacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Warum nicht? \u2013 Die Gesetze", "tokens": ["Wa\u00b7rum", "nicht", "?", "\u2013", "Die", "Ge\u00b7set\u00b7ze"], "token_info": ["word", "word", "punct", "punct", "word", "word"], "pos": ["PWAV", "PTKNEG", "$.", "$(", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vollstreckt er nur der Jagd!", "tokens": ["Voll\u00b7streckt", "er", "nur", "der", "Jagd", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Drum macht ihm keine Trauer", "tokens": ["Drum", "macht", "ihm", "kei\u00b7ne", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Jungen wild Geknirsch' \u2013", "tokens": ["Des", "Jun\u00b7gen", "wild", "Ge\u00b7knir\u00b7sch'", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihm selbst wird die Medaille \u2013", "tokens": ["Ihm", "selbst", "wird", "die", "Me\u00b7dail\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "$("], "meter": "-+---+-", "measure": "dactylic.init"}, "line.4": {"text": "Ja so, das fehlte noch! \u2013", "tokens": ["Ja", "so", ",", "das", "fehl\u00b7te", "noch", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "ADV", "$,", "PDS", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den Fritzen, die Kanaille,", "tokens": ["Den", "Frit\u00b7zen", ",", "die", "Ka\u00b7nail\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wirft man ins Hundeloch!", "tokens": ["Wirft", "man", "ins", "Hun\u00b7de\u00b7loch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Da starrt er tr\u00fcb durchs Gitter;", "tokens": ["Da", "starrt", "er", "tr\u00fcb", "durchs", "Git\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Leirer steht am Tor,", "tokens": ["Ein", "Lei\u00b7rer", "steht", "am", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der singt zu seiner Zither", "tokens": ["Der", "singt", "zu", "sei\u00b7ner", "Zi\u00b7ther"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Lied den Leuten vor:", "tokens": ["Ein", "Lied", "den", "Leu\u00b7ten", "vor", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbes lebe, was auf Erden", "tokens": ["\u00bb", "es", "le\u00b7be", ",", "was", "auf", "Er\u00b7den"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Stolziert in gr\u00fcner Tracht,", "tokens": ["Stol\u00b7ziert", "in", "gr\u00fc\u00b7ner", "Tracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die W\u00e4lder und die Felder,", "tokens": ["Die", "W\u00e4l\u00b7der", "und", "die", "Fel\u