{"dta.poem.10231": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Erinnerung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Die gro\u00dfen Feuer warfen ihren Schein           ", "tokens": ["Die", "gro\u00b7\u00dfen", "Feu\u00b7er", "war\u00b7fen", "ih\u00b7ren", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Helllodernd in ein lustig Biwaktreiben.", "tokens": ["Hell\u00b7lo\u00b7dernd", "in", "ein", "lus\u00b7tig", "Bi\u00b7wak\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir Offiziere sa\u00dfen um den Holzsto\u00df", "tokens": ["Wir", "Of\u00b7fi\u00b7zie\u00b7re", "sa\u00b7\u00dfen", "um", "den", "Holz\u00b7sto\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und tranken Gl\u00fchwein, sternen\u00fcberscheitelt.", "tokens": ["Und", "tran\u00b7ken", "Gl\u00fch\u00b7wein", ",", "ster\u00b7nen\u00b7\u00fc\u00b7bersc\u00b7hei\u00b7telt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So manches Wort, das in der Sommernacht", "tokens": ["So", "man\u00b7ches", "Wort", ",", "das", "in", "der", "Som\u00b7mer\u00b7nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Im Fl\u00fcstern oder laut gesprochen wird,", "tokens": ["Im", "Fl\u00fcs\u00b7tern", "o\u00b7der", "laut", "ge\u00b7spro\u00b7chen", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Verweht der Wind, begr\u00e4bt das stille Feld.", "tokens": ["Ver\u00b7weht", "der", "Wind", ",", "be\u00b7gr\u00e4bt", "das", "stil\u00b7le", "Feld", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die Musketiere sangen: \u201eStra \u2014 a \u2014 sburg,", "tokens": ["Die", "Mus\u00b7ke\u00b7tie\u00b7re", "san\u00b7gen", ":", "\u201e", "Stra", "a", "sburg", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "$(", "NE", "$(", "NE", "$(", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "O Stra \u2014 a \u2014 sburg\u201c \u2026 Da f\u00fchlt\u2019 ich eine Hand,", "tokens": ["O", "Stra", "a", "sburg", "\u201c", "\u2026", "Da", "f\u00fchlt'", "ich", "ei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "NE", "$(", "NE", "$(", "$(", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die leise sich auf meine Schultern legte.", "tokens": ["Die", "lei\u00b7se", "sich", "auf", "mei\u00b7ne", "Schul\u00b7tern", "leg\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ich wandte rasch den Kopf, und sah den Lehrer,", "tokens": ["Ich", "wand\u00b7te", "rasch", "den", "Kopf", ",", "und", "sah", "den", "Leh\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Bei dem ich, freundlich aufgenommen, gestern", "tokens": ["Bei", "dem", "ich", ",", "freund\u00b7lich", "auf\u00b7ge\u00b7nom\u00b7men", ",", "ge\u00b7stern"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "PRELS", "PPER", "$,", "ADJD", "VVPP", "$,", "ADJA"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.13": {"text": "Quartier gehabt; der nun, verabredet,", "tokens": ["Quar\u00b7tier", "ge\u00b7habt", ";", "der", "nun", ",", "ver\u00b7ab\u00b7re\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VAPP", "$.", "ART", "ADV", "$,", "VVPP", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.14": {"text": "Mit seinem T\u00f6chterchen gekommen war.", "tokens": ["Mit", "sei\u00b7nem", "T\u00f6ch\u00b7ter\u00b7chen", "ge\u00b7kom\u00b7men", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Ein M\u00e4del, jung gleich einer Apfelbl\u00fcte,", "tokens": ["Ein", "M\u00e4\u00b7del", ",", "jung", "gleich", "ei\u00b7ner", "Ap\u00b7fel\u00b7bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Die niemals noch der Morgenwind geschaukelt.", "tokens": ["Die", "nie\u00b7mals", "noch", "der", "Mor\u00b7gen\u00b7wind", "ge\u00b7schau\u00b7kelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Der Alte mu\u00dfte neben uns sich setzen,", "tokens": ["Der", "Al\u00b7te", "mu\u00df\u00b7te", "ne\u00b7ben", "uns", "sich", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Und w\u00e4hrend ihm das Glas die Freunde f\u00fcllten,", "tokens": ["Und", "w\u00e4h\u00b7rend", "ihm", "das", "Glas", "die", "Freun\u00b7de", "f\u00fcll\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "F\u00fchrt\u2019 ich, von Allem ihr Erkl\u00e4rung gebend,", "tokens": ["F\u00fchrt'", "ich", ",", "von", "Al\u00b7lem", "ihr", "Er\u00b7kl\u00e4\u00b7rung", "ge\u00b7bend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "PIS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Das M\u00e4dchen langsam durch die Lagerreihen.", "tokens": ["Das", "M\u00e4d\u00b7chen", "lang\u00b7sam", "durch", "die", "La\u00b7ger\u00b7rei\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Sie sprach kein Wort, doch lautlos sprach ihr Mund,", "tokens": ["Sie", "sprach", "kein", "Wort", ",", "doch", "laut\u00b7los", "sprach", "ihr", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Ihr L\u00e4cheln und ihr staunend gro\u00dfes Auge.", "tokens": ["Ihr", "L\u00e4\u00b7cheln", "und", "ihr", "stau\u00b7nend", "gro\u00b7\u00dfes", "Au\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPER", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Wie sch\u00f6n sie war, wenn sie beim Feuer stand,", "tokens": ["Wie", "sch\u00f6n", "sie", "war", ",", "wenn", "sie", "beim", "Feu\u00b7er", "stand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VAFIN", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Und rote Funken knisternd uns umtanzten.", "tokens": ["Und", "ro\u00b7te", "Fun\u00b7ken", "knis\u00b7ternd", "uns", "um\u00b7tanz\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Es hob sich die Gestalt vom dunklen Himmel", "tokens": ["Es", "hob", "sich", "die", "Ge\u00b7stalt", "vom", "dunk\u00b7len", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Scharf ausgeschnitten aus dem schwarzen Rahmen.", "tokens": ["Scharf", "aus\u00b7ge\u00b7schnit\u00b7ten", "aus", "dem", "schwar\u00b7zen", "Rah\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Und einmal, als Soldaten, die verkleidet", "tokens": ["Und", "ein\u00b7mal", ",", "als", "Sol\u00b7da\u00b7ten", ",", "die", "ver\u00b7klei\u00b7det"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "$,", "KOUS", "NN", "$,", "PRELS", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Als Storch und B\u00e4r, uns ihre K\u00fcnste zeigten,", "tokens": ["Als", "Storch", "und", "B\u00e4r", ",", "uns", "ih\u00b7re", "K\u00fcns\u00b7te", "zeig\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "$,", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Da lehnte fl\u00fcchtig sie, beinah erschrocken,", "tokens": ["Da", "lehn\u00b7te", "fl\u00fcch\u00b7tig", "sie", ",", "bei\u00b7nah", "er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPER", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "An meine Brust ihr frommes Kinderantlitz.", "tokens": ["An", "mei\u00b7ne", "Brust", "ihr", "from\u00b7mes", "Kin\u00b7de\u00b7rant\u00b7litz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Wir traten z\u00f6gernd dann den R\u00fcckweg an,", "tokens": ["Wir", "tra\u00b7ten", "z\u00f6\u00b7gernd", "dann", "den", "R\u00fcck\u00b7weg", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "\u2014 Es stahl der Mond sich eben in die B\u00e4ume,", "tokens": ["Es", "stahl", "der", "Mond", "sich", "e\u00b7ben", "in", "die", "B\u00e4u\u00b7me", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "PRF", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.33": {"text": "Und in der Ferne, bei den Doppelposten,", "tokens": ["Und", "in", "der", "Fer\u00b7ne", ",", "bei", "den", "Dop\u00b7pel\u00b7pos\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Fiel, dumpf verhallend durch den Wald, ein Schu\u00df. \u2014", "tokens": ["Fiel", ",", "dumpf", "ver\u00b7hal\u00b7lend", "durch", "den", "Wald", ",", "ein", "Schu\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "ADJD", "VVPP", "APPR", "ART", "NN", "$,", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Wir gingen Hand in Hand,", "tokens": ["Wir", "gin\u00b7gen", "Hand", "in", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.36": {"text": "Und so, halb stehend, halb im Weiterschreiten,", "tokens": ["Und", "so", ",", "halb", "ste\u00b7hend", ",", "halb", "im", "Wei\u00b7ter\u00b7schrei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADJD", "VVPP", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Bog ich mein Haupt hinunter zu dem ihren.", "tokens": ["Bog", "ich", "mein", "Haupt", "hin\u00b7un\u00b7ter", "zu", "dem", "ih\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPO", "APPR", "ART", "PPOSS", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Ich f\u00fchlte wie die jungen Lippen mir", "tokens": ["Ich", "f\u00fchl\u00b7te", "wie", "die", "jun\u00b7gen", "Lip\u00b7pen", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Entgegenkamen, und noch heute seh\u2019 ich", "tokens": ["Ent\u00b7ge\u00b7gen\u00b7ka\u00b7men", ",", "und", "noch", "heu\u00b7te", "seh'", "ich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "ADV", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Ihr dunkles Auge in die Sterne leuchten\u2026", "tokens": ["Ihr", "dunk\u00b7les", "Au\u00b7ge", "in", "die", "Ster\u00b7ne", "leuch\u00b7ten", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Als l\u00e4ngst der Alte mit ihr fortgegangen,", "tokens": ["Als", "l\u00e4ngst", "der", "Al\u00b7te", "mit", "ihr", "fort\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Sa\u00df ich im Kreise meiner Kameraden", "tokens": ["Sa\u00df", "ich", "im", "Krei\u00b7se", "mei\u00b7ner", "Ka\u00b7me\u00b7ra\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Und dachte sehnsuchtschmerzlich an das M\u00e4dchen,", "tokens": ["Und", "dach\u00b7te", "sehn\u00b7sucht\u00b7schmerz\u00b7lich", "an", "das", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Bis mir zuletzt die schweren Lider sanken.", "tokens": ["Bis", "mir", "zu\u00b7letzt", "die", "schwe\u00b7ren", "Li\u00b7der", "san\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Mein treuer Bursche trug mich in mein Zelt", "tokens": ["Mein", "treu\u00b7er", "Bur\u00b7sche", "trug", "mich", "in", "mein", "Zelt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Und deckte sorgsam mir den Mantel \u00fcber.", "tokens": ["Und", "deck\u00b7te", "sorg\u00b7sam", "mir", "den", "Man\u00b7tel", "\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPER", "ART", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "Seitdem bin ich durch manches Land gezogen,", "tokens": ["Seit\u00b7dem", "bin", "ich", "durch", "man\u00b7ches", "Land", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.48": {"text": "Doch unvergessen bleibt mir jene Nacht.", "tokens": ["Doch", "un\u00b7ver\u00b7ges\u00b7sen", "bleibt", "mir", "je\u00b7ne", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}