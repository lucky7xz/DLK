{"dta.poem.21454": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "An  * * *", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Zum ewigen Thron strebt ihr empor,", "tokens": ["Zum", "e\u00b7wi\u00b7gen", "Thron", "strebt", "ihr", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So trotzigstolz und l\u00f6wenk\u00fchn,", "tokens": ["So", "trot\u00b7zigs\u00b7tolz", "und", "l\u00f6\u00b7wen\u00b7k\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr sucht des Himmels Flammenthor,", "tokens": ["Ihr", "sucht", "des", "Him\u00b7mels", "Flam\u00b7men\u00b7thor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht eher kann Euch Frieden bl\u00fch\u2019n.", "tokens": ["Nicht", "e\u00b7her", "kann", "Euch", "Frie\u00b7den", "bl\u00fch'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PTKNEG", "ADV", "VMFIN", "PPER", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O nehmt mich auf in Euern Bund!", "tokens": ["O", "nehmt", "mich", "auf", "in", "Eu\u00b7ern", "Bund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bin so arm und leidesm\u00fcd\u2019,", "tokens": ["Ich", "bin", "so", "arm", "und", "lei\u00b7des\u00b7m\u00fcd'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Kampf mit Euch werd\u2019 ich gesund,", "tokens": ["Im", "Kampf", "mit", "Euch", "werd'", "ich", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPER", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vielleicht gelingt mir auch ein Lied.", "tokens": ["Viel\u00b7leicht", "ge\u00b7lingt", "mir", "auch", "ein", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein Lied, das meine Seele rei\u00dft", "tokens": ["Ein", "Lied", ",", "das", "mei\u00b7ne", "See\u00b7le", "rei\u00dft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Urm\u00e4chtig aus der Tr\u00fcbsal Nacht,", "tokens": ["Ur\u00b7m\u00e4ch\u00b7tig", "aus", "der", "Tr\u00fcb\u00b7sal", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df frei im Aether schwebt der Geist,", "tokens": ["Da\u00df", "frei", "im", "A\u00b7e\u00b7ther", "schwebt", "der", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In Blumenduft und Sonnenpracht.", "tokens": ["In", "Blu\u00b7men\u00b7duft", "und", "Son\u00b7nen\u00b7pracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein Lied, wie Offenbarungslaut,", "tokens": ["Ein", "Lied", ",", "wie", "Of\u00b7fen\u00b7ba\u00b7rungs\u00b7laut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Fr\u00fchlingssturm in Wald und Flur,", "tokens": ["Wie", "Fr\u00fch\u00b7lings\u00b7sturm", "in", "Wald", "und", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das nur im Licht die Gottheit schaut", "tokens": ["Das", "nur", "im", "Licht", "die", "Got\u00b7theit", "schaut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und h\u00fcllenlos die Allnatur.", "tokens": ["Und", "h\u00fcl\u00b7len\u00b7los", "die", "All\u00b7na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ein Lied, das wie die Thr\u00e4ne quillt,", "tokens": ["Ein", "Lied", ",", "das", "wie", "die", "Thr\u00e4\u00b7ne", "quillt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die uns erl\u00f6st aus tiefstem Schmerz,", "tokens": ["Die", "uns", "er\u00b7l\u00f6st", "aus", "tiefs\u00b7tem", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Lied, das jede Sehnsucht stillt,", "tokens": ["Ein", "Lied", ",", "das", "je\u00b7de", "Sehn\u00b7sucht", "stillt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und s\u00fc\u00dfer Trost f\u00fcr jedes Herz.", "tokens": ["Und", "s\u00fc\u00b7\u00dfer", "Trost", "f\u00fcr", "je\u00b7des", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ein Lied, das nicht mehr br\u00fcnstig fragt,", "tokens": ["Ein", "Lied", ",", "das", "nicht", "mehr", "br\u00fcns\u00b7tig", "fragt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Lied, das Alles selig kennt,", "tokens": ["Ein", "Lied", ",", "das", "Al\u00b7les", "se\u00b7lig", "kennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In dem der ewige Morgen tagt,", "tokens": ["In", "dem", "der", "e\u00b7wi\u00b7ge", "Mor\u00b7gen", "tagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In dem die ewige Sonne brennt.", "tokens": ["In", "dem", "die", "e\u00b7wi\u00b7ge", "Son\u00b7ne", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}