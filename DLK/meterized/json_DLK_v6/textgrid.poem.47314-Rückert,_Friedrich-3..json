{"textgrid.poem.47314": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Flammt empor in euren H\u00f6h'n, Morgensonnen, lobt den Herrn!", "tokens": ["Flammt", "em\u00b7por", "in", "eu\u00b7ren", "H\u00f6h'n", ",", "Mor\u00b7gen\u00b7son\u00b7nen", ",", "lobt", "den", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN", "$,", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Rauscht in euren Tiefen auf, Sch\u00f6pfungsbronnen, lobt den Herrn!", "tokens": ["Rauscht", "in", "eu\u00b7ren", "Tie\u00b7fen", "auf", ",", "Sch\u00f6p\u00b7fungs\u00b7bron\u00b7nen", ",", "lobt", "den", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Die ihr, ohne zu vergl\u00fchn, lang' geflammt vor seinem Blick,", "tokens": ["Die", "ihr", ",", "oh\u00b7ne", "zu", "ver\u00b7gl\u00fchn", ",", "lang'", "ge\u00b7flammt", "vor", "sei\u00b7nem", "Blick", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "KOUI", "PTKZU", "VVINF", "$,", "ADV", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Ohne zu verrinnen, lang' hingeronnen, lobt den Herrn!", "tokens": ["Oh\u00b7ne", "zu", "ver\u00b7rin\u00b7nen", ",", "lang'", "hin\u00b7ge\u00b7ron\u00b7nen", ",", "lobt", "den", "Herrn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKZU", "VVINF", "$,", "ADV", "VVINF", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Der ein mannigfaltiges Leben schaun will au\u00dfer sich;", "tokens": ["Der", "ein", "man\u00b7nig\u00b7fal\u00b7ti\u00b7ges", "Le\u00b7ben", "schaun", "will", "au\u00b7\u00dfer", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVINF", "VMFIN", "APPR", "PRF", "$."], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Alle, die ein Leben ihr habt gewonnen, lobt den Herrn!", "tokens": ["Al\u00b7le", ",", "die", "ein", "Le\u00b7ben", "ihr", "habt", "ge\u00b7won\u00b7nen", ",", "lobt", "den", "Herrn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "ART", "NN", "PPER", "VAFIN", "VVPP", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Alle Tropfen seiner Huld, die zu Perlen sich geformt,", "tokens": ["Al\u00b7le", "Trop\u00b7fen", "sei\u00b7ner", "Huld", ",", "die", "zu", "Per\u00b7len", "sich", "ge\u00b7formt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPOSAT", "NN", "$,", "PRELS", "APPR", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Funken Lichtes, die zu Gold sind geronnen, lobt den Herrn!", "tokens": ["Fun\u00b7ken", "Lich\u00b7tes", ",", "die", "zu", "Gold", "sind", "ge\u00b7ron\u00b7nen", ",", "lobt", "den", "Herrn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PRELS", "APPR", "NN", "VAFIN", "VVPP", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Soviel Halme von dem Tau seiner Gnade trunken sind,", "tokens": ["So\u00b7viel", "Hal\u00b7me", "von", "dem", "Tau", "sei\u00b7ner", "Gna\u00b7de", "trun\u00b7ken", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "PPOSAT", "NN", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Soviel sich an seinem Strahl Welten sonnen, lobt den Herrn!", "tokens": ["So\u00b7viel", "sich", "an", "sei\u00b7nem", "Strahl", "Wel\u00b7ten", "son\u00b7nen", ",", "lobt", "den", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "PRF", "APPR", "PPOSAT", "NN", "NN", "VVINF", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-++-+-+-+", "measure": "iambic.septa.relaxed"}, "line.11": {"text": "Ob vor seinem ew'gen Blick ihr des Lebens raschen Tanz", "tokens": ["Ob", "vor", "sei\u00b7nem", "ew'\u00b7gen", "Blick", "ihr", "des", "Le\u00b7bens", "ra\u00b7schen", "Tanz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "ADJA", "NN", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Jetzt vollendet oder jetzt habt begonnen, lobt den Herrn!", "tokens": ["Jetzt", "voll\u00b7en\u00b7det", "o\u00b7der", "jetzt", "habt", "be\u00b7gon\u00b7nen", ",", "lobt", "den", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KON", "ADV", "VAFIN", "VVPP", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Blumen, die der Fr\u00fchling weckt, Garben, die der Sommer d\u00f6rrt,", "tokens": ["Blu\u00b7men", ",", "die", "der", "Fr\u00fch\u00b7ling", "weckt", ",", "Gar\u00b7ben", ",", "die", "der", "Som\u00b7mer", "d\u00f6rrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Trauben, deren Blut der Herbst pre\u00dft in Tonnen, lobt den Herrn!", "tokens": ["Trau\u00b7ben", ",", "de\u00b7ren", "Blut", "der", "Herbst", "pre\u00dft", "in", "Ton\u00b7nen", ",", "lobt", "den", "Herrn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELAT", "NN", "ART", "NN", "VVFIN", "APPR", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.15": {"text": "Raupe, die das Blatt benagt, haftend an dem gr\u00fcnen Zweig,", "tokens": ["Rau\u00b7pe", ",", "die", "das", "Blatt", "be\u00b7nagt", ",", "haf\u00b7tend", "an", "dem", "gr\u00fc\u00b7nen", "Zweig", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.16": {"text": "Puppe, zur Verwandlung reif eingesponnen, lobt den Herrn!", "tokens": ["Pup\u00b7pe", ",", "zur", "Ver\u00b7wand\u00b7lung", "reif", "ein\u00b7ge\u00b7spon\u00b7nen", ",", "lobt", "den", "Herrn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "NN", "ADJD", "VVINF", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+---+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "Schmetterlinge, die ihr noch von dem Duft der Bl\u00fcten nascht,", "tokens": ["Schmet\u00b7ter\u00b7lin\u00b7ge", ",", "die", "ihr", "noch", "von", "dem", "Duft", "der", "Bl\u00fc\u00b7ten", "nascht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+---+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "Schmetterlinge, die ins Licht schon zerronnen, lobt den Herrn!", "tokens": ["Schmet\u00b7ter\u00b7lin\u00b7ge", ",", "die", "ins", "Licht", "schon", "zer\u00b7ron\u00b7nen", ",", "lobt", "den", "Herrn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN", "ADV", "VVINF", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.19": {"text": "Geister, eingeengt in Nacht oder aufgeflammt ins Licht,", "tokens": ["Geis\u00b7ter", ",", "ein\u00b7ge\u00b7engt", "in", "Nacht", "o\u00b7der", "auf\u00b7ge\u00b7flammt", "ins", "Licht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "NN", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.20": {"text": "Herzen, schmeckend Lebenslust, Todeswonnen, lobt den Herrn!", "tokens": ["Her\u00b7zen", ",", "schme\u00b7ckend", "Le\u00b7bens\u00b7lust", ",", "To\u00b7des\u00b7won\u00b7nen", ",", "lobt", "den", "Herrn", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "NN", "$,", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.21": {"text": "Die ihr mit dem Fl\u00fcgelschlag gl\u00fchender Begeistrung strebt,", "tokens": ["Die", "ihr", "mit", "dem", "Fl\u00fc\u00b7gel\u00b7schlag", "gl\u00fc\u00b7hen\u00b7der", "Be\u00b7geis\u00b7trung", "strebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.22": {"text": "Oder f\u00f6rdert euer Werk still besonnen, lobt den Herrn!", "tokens": ["O\u00b7der", "f\u00f6r\u00b7dert", "eu\u00b7er", "Werk", "still", "be\u00b7son\u00b7nen", ",", "lobt", "den", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADJD", "VVINF", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.23": {"text": "Lobt den Herrn, des Lichtgewand auch durch dunkle F\u00e4den w\u00e4chst,", "tokens": ["Lobt", "den", "Herrn", ",", "des", "Licht\u00b7ge\u00b7wand", "auch", "durch", "dunk\u00b7le", "F\u00e4\u00b7den", "w\u00e4chst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.24": {"text": "Die ein unscheinbarer Flei\u00df hat gesponnen, lobt den Herrn!", "tokens": ["Die", "ein", "un\u00b7schein\u00b7ba\u00b7rer", "Flei\u00df", "hat", "ge\u00b7spon\u00b7nen", ",", "lobt", "den", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VAFIN", "VVPP", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "Lobt den Herrn, des Angesicht l\u00e4chelnd in den Spiegel schaut", "tokens": ["Lobt", "den", "Herrn", ",", "des", "An\u00b7ge\u00b7sicht", "l\u00e4\u00b7chelnd", "in", "den", "Spie\u00b7gel", "schaut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.26": {"text": "Auch des Tropfens, der am Halm h\u00e4ngt geronnen, lobt den Herrn!", "tokens": ["Auch", "des", "Trop\u00b7fens", ",", "der", "am", "Halm", "h\u00e4ngt", "ge\u00b7ron\u00b7nen", ",", "lobt", "den", "Herrn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "VVPP", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+---+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.27": {"text": "Lobt den Herrn, der loben sich gern in allen Sprachen h\u00f6rt,", "tokens": ["Lobt", "den", "Herrn", ",", "der", "lo\u00b7ben", "sich", "gern", "in", "al\u00b7len", "Spra\u00b7chen", "h\u00f6rt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "PRF", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.28": {"text": "Die Bed\u00fcrfnis seines Lobs hat ersonnen, lobt den Herrn!", "tokens": ["Die", "Be\u00b7d\u00fcrf\u00b7nis", "sei\u00b7nes", "Lobs", "hat", "er\u00b7son\u00b7nen", ",", "lobt", "den", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "VVPP", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.29": {"text": "Ob das Blatt am Zweige rauscht, ob des Menschen Zunge t\u00f6nt,", "tokens": ["Ob", "das", "Blatt", "am", "Zwei\u00b7ge", "rauscht", ",", "ob", "des", "Men\u00b7schen", "Zun\u00b7ge", "t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,", "KOUS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.30": {"text": "Ob ein Engel h\u00f6hern Gru\u00df sich ersonnen, lobt den Herrn!", "tokens": ["Ob", "ein", "En\u00b7gel", "h\u00f6\u00b7hern", "Gru\u00df", "sich", "er\u00b7son\u00b7nen", ",", "lobt", "den", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "PRF", "VVINF", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.31": {"text": "Alle, die ihr euren Gott f\u00fchlet, ahnet, denket, schaut,", "tokens": ["Al\u00b7le", ",", "die", "ihr", "eu\u00b7ren", "Gott", "f\u00fch\u00b7let", ",", "ah\u00b7net", ",", "den\u00b7ket", ",", "schaut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.32": {"text": "Die ihr sinnt, was niemals wird ausgesonnen, lobt den Herrn!", "tokens": ["Die", "ihr", "sinnt", ",", "was", "nie\u00b7mals", "wird", "aus\u00b7ge\u00b7son\u00b7nen", ",", "lobt", "den", "Herrn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "PRELS", "ADV", "VAFIN", "VVINF", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.33": {"text": "Wenn in des Gem\u00fctes Nacht euch sein erster Schimmer brach,", "tokens": ["Wenn", "in", "des", "Ge\u00b7m\u00fc\u00b7tes", "Nacht", "euch", "sein", "ers\u00b7ter", "Schim\u00b7mer", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "Oder wenn ihr euch im Glanz habt versonnen, lobt den Herrn!", "tokens": ["O\u00b7der", "wenn", "ihr", "euch", "im", "Glanz", "habt", "ver\u00b7son\u00b7nen", ",", "lobt", "den", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "APPRART", "NN", "VAFIN", "VVINF", "$,", "VVFIN", "ART", "NN", "$."], "meter": "----+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Alle Sinne, die des Sangs Woge schwellet himmelan,", "tokens": ["Al\u00b7le", "Sin\u00b7ne", ",", "die", "des", "Sangs", "Wo\u00b7ge", "schwel\u00b7let", "him\u00b7me\u00b7lan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ART", "NN", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.36": {"text": "Lobt mit allen rauschenden Sch\u00f6pfungsbronnen, lobt den Herrn!", "tokens": ["Lobt", "mit", "al\u00b7len", "rau\u00b7schen\u00b7den", "Sch\u00f6p\u00b7fungs\u00b7bron\u00b7nen", ",", "lobt", "den", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "ADJA", "NN", "$,", 