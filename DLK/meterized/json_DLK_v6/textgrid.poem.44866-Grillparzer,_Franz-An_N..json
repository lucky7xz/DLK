{"textgrid.poem.44866": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "An N.", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Katholisch und Aszet, plagst du dich gern,", "tokens": ["Ka\u00b7tho\u00b7lisch", "und", "As\u00b7zet", ",", "plagst", "du", "dich", "gern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,", "VVFIN", "PPER", "PRF", "ADV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nur die Geliebte will sich nicht enthalten,", "tokens": ["Nur", "die", "Ge\u00b7lieb\u00b7te", "will", "sich", "nicht", "ent\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und, ketzerhaft, den Leib des Herrn", "tokens": ["Und", ",", "ket\u00b7zer\u00b7haft", ",", "den", "Leib", "des", "Herrn"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "ADJD", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Genie\u00dft sie unter doppelten Gestalten.", "tokens": ["Ge\u00b7nie\u00dft", "sie", "un\u00b7ter", "dop\u00b7pel\u00b7ten", "Ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}}}}}