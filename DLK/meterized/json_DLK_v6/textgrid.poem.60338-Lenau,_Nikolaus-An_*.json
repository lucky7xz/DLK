{"textgrid.poem.60338": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "An *", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach w\u00e4rst du mein, es war ein sch\u00f6nes Leben!", "tokens": ["Ach", "w\u00e4rst", "du", "mein", ",", "es", "war", "ein", "sch\u00f6\u00b7nes", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "PPOSAT", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So aber ists Entsagen nur und Trauern,", "tokens": ["So", "a\u00b7ber", "ists", "Ent\u00b7sa\u00b7gen", "nur", "und", "Trau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "NN", "ADV", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur ein verlornes Grollen und Bedauern;", "tokens": ["Nur", "ein", "ver\u00b7lor\u00b7nes", "Grol\u00b7len", "und", "Be\u00b7dau\u00b7ern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich kann es meinem Schicksal nicht vergeben.", "tokens": ["Ich", "kann", "es", "mei\u00b7nem", "Schick\u00b7sal", "nicht", "ver\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Undank tut wohl und jedes Leid der Erde;", "tokens": ["Un\u00b7dank", "tut", "wohl", "und", "je\u00b7des", "Leid", "der", "Er\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "KON", "PIAT", "NN", "ART", "NN", "$."], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Ja! meine Freund' in S\u00e4rgen, Leich an Leiche,", "tokens": ["Ja", "!", "mei\u00b7ne", "Freund'", "in", "S\u00e4r\u00b7gen", ",", "Leich", "an", "Lei\u00b7che", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPOSAT", "NN", "APPR", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sind ein gelinder Gram, wenn ichs vergleiche", "tokens": ["Sind", "ein", "ge\u00b7lin\u00b7der", "Gram", ",", "wenn", "ichs", "ver\u00b7glei\u00b7che"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,", "KOUS", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem Schmerz, da\u00df ich dich nie besitzen werde.", "tokens": ["Dem", "Schmerz", ",", "da\u00df", "ich", "dich", "nie", "be\u00b7sit\u00b7zen", "wer\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "PRF", "ADV", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}