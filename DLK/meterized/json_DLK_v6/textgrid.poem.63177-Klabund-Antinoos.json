{"textgrid.poem.63177": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Antinoos", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Memnons\u00e4ule,", "tokens": ["Du", "Mem\u00b7non\u00b7s\u00e4u\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NE", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Singend im Licht!", "tokens": ["Sin\u00b7gend", "im", "Licht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wenn du die Arme hebst,", "tokens": ["Wenn", "du", "die", "Ar\u00b7me", "hebst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "An den Himmel gekreuzigt,", "tokens": ["An", "den", "Him\u00b7mel", "ge\u00b7kreu\u00b7zigt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Sehnt sich der Blitz, in dich zu fahren,", "tokens": ["Sehnt", "sich", "der", "Blitz", ",", "in", "dich", "zu", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und der Donner grollt z\u00e4rtlich um deine Locken.", "tokens": ["Und", "der", "Don\u00b7ner", "grollt", "z\u00e4rt\u00b7lich", "um", "dei\u00b7ne", "Lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Wie bin ich voll deiner summenden Gedanken:", "tokens": ["Wie", "bin", "ich", "voll", "dei\u00b7ner", "sum\u00b7men\u00b7den", "Ge\u00b7dan\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "+---+-+---+-", "measure": "dactylic.init"}, "line.8": {"text": "Ein Bienenkorb,", "tokens": ["Ein", "Bie\u00b7nen\u00b7korb", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Und deine S\u00fcsse ist meine t\u00e4gliche Speise.", "tokens": ["Und", "dei\u00b7ne", "S\u00fcs\u00b7se", "ist", "mei\u00b7ne", "t\u00e4g\u00b7li\u00b7che", "Spei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Als ich mich \u00fcber dich bog in der Nacht,", "tokens": ["Als", "ich", "mich", "\u00fc\u00b7ber", "dich", "bog", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Sass ein Sperber auf deiner Brust,", "tokens": ["Sass", "ein", "Sper\u00b7ber", "auf", "dei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.12": {"text": "Den hatte Gott gesandt,", "tokens": ["Den", "hat\u00b7te", "Gott", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Deinen Traum zu bewachen.", "tokens": ["Dei\u00b7nen", "Traum", "zu", "be\u00b7wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.14": {"text": "Er sperrte den Schnabel gegen mich.", "tokens": ["Er", "sperr\u00b7te", "den", "Schna\u00b7bel", "ge\u00b7gen", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Du Weide am Strom,", "tokens": ["Du", "Wei\u00b7de", "am", "Strom", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.16": {"text": "In dem ich verfliesse!", "tokens": ["In", "dem", "ich", "ver\u00b7flies\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.17": {"text": "Halte mit deinen Zweigen,", "tokens": ["Hal\u00b7te", "mit", "dei\u00b7nen", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.18": {"text": "Mit deinen Armen den Freund,", "tokens": ["Mit", "dei\u00b7nen", "Ar\u00b7men", "den", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.19": {"text": "Der zu dir emporwallt", "tokens": ["Der", "zu", "dir", "em\u00b7por\u00b7wallt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "VVFIN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.20": {"text": "Wie die Woge des Meeres", "tokens": ["Wie", "die", "Wo\u00b7ge", "des", "Mee\u00b7res"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.21": {"text": "Im heiligen Sturm.", "tokens": ["Im", "hei\u00b7li\u00b7gen", "Sturm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}