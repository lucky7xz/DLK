{"dta.poem.20626": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Dem  \n Vater des Vaterlandes  \n  Friederich dem Gro\u00dfen,  \n bei triumphirender Zur\u00fcckkunft  \n gesungen  \n im Namen Seiner B\u00fcrger .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Du den Tempel Deines neuen Freundschaftbandes                 ", "tokens": ["Der", "Du", "den", "Tem\u00b7pel", "Dei\u00b7nes", "neu\u00b7en", "Freund\u00b7schaft\u00b7ban\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit diamantnen Bogen w\u00f6lbst;", "tokens": ["Mit", "di\u00b7a\u00b7mant\u00b7nen", "Bo\u00b7gen", "w\u00f6lbst", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O ", "tokens": ["O"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Landes!", "tokens": ["Lan\u00b7des", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Uns gegenw\u00e4rtig bist Du selbst.", "tokens": ["Uns", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "bist", "Du", "selbst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dich, mit vermehrten Siegeskr\u00e4nzen Ausgeschm\u00fcckter!", "tokens": ["Dich", ",", "mit", "ver\u00b7mehr\u00b7ten", "Sie\u00b7ges\u00b7kr\u00e4n\u00b7zen", "Aus\u00b7ge\u00b7schm\u00fcck\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Empf\u00e4ngt der j\u00fcngste Fr\u00fchlingswind,", "tokens": ["Emp\u00b7f\u00e4ngt", "der", "j\u00fcngs\u00b7te", "Fr\u00fch\u00b7lings\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erf\u00fcllt mit Jauchzen Deiner B\u00fcrger, die entz\u00fcckter", "tokens": ["Er\u00b7f\u00fcllt", "mit", "Jauch\u00b7zen", "Dei\u00b7ner", "B\u00fcr\u00b7ger", ",", "die", "ent\u00b7z\u00fcck\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "PPOSAT", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jetzt f\u00fchlen, da\u00df sie Menschen sind.", "tokens": ["Jetzt", "f\u00fch\u00b7len", ",", "da\u00df", "sie", "Men\u00b7schen", "sind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "KOUS", "PPER", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zu lange suchten Dich befl\u00fcgelte Gedanken,", "tokens": ["Zu", "lan\u00b7ge", "such\u00b7ten", "Dich", "be\u00b7fl\u00fc\u00b7gel\u00b7te", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Seufzer Deines Volkes dort,", "tokens": ["Und", "Seuf\u00b7zer", "Dei\u00b7nes", "Vol\u00b7kes", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo um das Schlachtfeld sich die Helden standhaft", "tokens": ["Wo", "um", "das", "Schlacht\u00b7feld", "sich", "die", "Hel\u00b7den", "stand\u00b7haft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "PRF", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "zanken,", "tokens": ["zan\u00b7ken", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Und Kriegesdonner ist ihr Wort.", "tokens": ["Und", "Krie\u00b7ges\u00b7don\u00b7ner", "ist", "ihr", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Zu lange bliebest Du, versteckt in schwarzen Wettern,", "tokens": ["Zu", "lan\u00b7ge", "blie\u00b7best", "Du", ",", "ver\u00b7steckt", "in", "schwar\u00b7zen", "Wet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Rund um Dich werfend Deinen Blitz,", "tokens": ["Rund", "um", "Dich", "wer\u00b7fend", "Dei\u00b7nen", "Blitz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Wir aber wankten gleich verwelkten Lindenbl\u00e4ttern,", "tokens": ["Wir", "a\u00b7ber", "wank\u00b7ten", "gleich", "ver\u00b7welk\u00b7ten", "Lin\u00b7den\u00b7bl\u00e4t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Um Deinen w\u00fcsten goldnen Sitz.", "tokens": ["Um", "Dei\u00b7nen", "w\u00fcs\u00b7ten", "gold\u00b7nen", "Sitz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Vor unsers nebelvollen Geistes Blicke schliefen", "tokens": ["Vor", "un\u00b7sers", "ne\u00b7bel\u00b7vol\u00b7len", "Geis\u00b7tes", "Bli\u00b7cke", "schlie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "VVINF"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Die Sch\u00f6pfung selbst und die Natur;", "tokens": ["Die", "Sch\u00f6p\u00b7fung", "selbst", "und", "die", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir f\u00fchlten nicht den Reiz der besten Welt; wir riefen", "tokens": ["Wir", "f\u00fchl\u00b7ten", "nicht", "den", "Reiz", "der", "bes\u00b7ten", "Welt", ";", "wir", "rie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dich aller Welten Wunder! nur.", "tokens": ["Dich", "al\u00b7ler", "Wel\u00b7ten", "Wun\u00b7der", "!", "nur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "NN", "$.", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Das Klaggeschrey, die Thr\u00e4nenstr\u00f6hme rauschten", "tokens": ["Das", "Klag\u00b7ge\u00b7schrey", ",", "die", "Thr\u00e4\u00b7nen\u00b7str\u00f6h\u00b7me", "rauschten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "m\u00e4chtig", "tokens": ["m\u00e4ch\u00b7tig"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Bis an den Himmel und zu Dir;", "tokens": ["Bis", "an", "den", "Him\u00b7mel", "und", "zu", "Dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "KON", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du kommst, und Dein Triumph ist mehr als R\u00f6misch", "tokens": ["Du", "kommst", ",", "und", "Dein", "Tri\u00b7umph", "ist", "mehr", "als", "R\u00f6\u00b7misch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPOSAT", "NN", "VAFIN", "ADV", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "pr\u00e4chtig:", "tokens": ["pr\u00e4ch\u00b7tig", ":"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Nicht \u00fcber Sclaven jauchzen wir;", "tokens": ["Nicht", "\u00fc\u00b7ber", "Scla\u00b7ven", "jauch\u00b7zen", "wir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nicht \u00fcber nachgef\u00fchrte fremde K\u00f6nigssch\u00e4tze", "tokens": ["Nicht", "\u00fc\u00b7ber", "nach\u00b7ge\u00b7f\u00fchr\u00b7te", "frem\u00b7de", "K\u00f6\u00b7nigs\u00b7sch\u00e4t\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Kronen, die der Sieger nahm;", "tokens": ["Und", "Kro\u00b7nen", ",", "die", "der", "Sie\u00b7ger", "nahm", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nein \u00fcber Dich, ", "tokens": ["Nein", "\u00fc\u00b7ber", "Dich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Besch\u00fctzer, glorreich wieder kam.", "tokens": ["Be\u00b7sch\u00fct\u00b7zer", ",", "glor\u00b7reich", "wie\u00b7der", "kam", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "In Deinen Augen ging aus tausend Mittern\u00e4chten", "tokens": ["In", "Dei\u00b7nen", "Au\u00b7gen", "ging", "aus", "tau\u00b7send", "Mit\u00b7ter\u00b7n\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein uns geschaffnes Sonnenlicht", "tokens": ["Ein", "uns", "ge\u00b7schaff\u00b7nes", "Son\u00b7nen\u00b7licht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hervor, und strahlet nun so lieblich Deinen", "tokens": ["Her\u00b7vor", ",", "und", "strah\u00b7let", "nun", "so", "lieb\u00b7lich", "Dei\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKVZ", "$,", "KON", "VVFIN", "ADV", "ADV", "ADJD", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Knechten,", "tokens": ["Knech\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Als Deines Gottes Angesicht,", "tokens": ["Als", "Dei\u00b7nes", "Got\u00b7tes", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Das \u00fcber Dir daher geleuchtet und gel\u00e4chelt,", "tokens": ["Das", "\u00fc\u00b7ber", "Dir", "da\u00b7her", "ge\u00b7leuch\u00b7tet", "und", "ge\u00b7l\u00e4\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPER", "PAV", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In undurchdringlicher Gefahr,", "tokens": ["In", "un\u00b7durch\u00b7dring\u00b7li\u00b7cher", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn oft das Vaterland wie Sterbende ger\u00f6chelt,", "tokens": ["Wenn", "oft", "das", "Va\u00b7ter\u00b7land", "wie", "Ster\u00b7ben\u00b7de", "ge\u00b7r\u00f6\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "KOKOM", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und zitternd f\u00fcr Dein Leben war.", "tokens": ["Und", "zit\u00b7ternd", "f\u00fcr", "Dein", "Le\u00b7ben", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "O! la\u00df Dein in der Schlacht nie wankend Knie umfassen,", "tokens": ["O", "!", "la\u00df", "Dein", "in", "der", "Schlacht", "nie", "wan\u00b7kend", "Knie", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVIMP", "PPOSAT", "APPR", "ART", "NN", "ADV", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du ", "tokens": ["Du"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Nicht mehr Dein bittend Land verwaiset zu verlassen.", "tokens": ["Nicht", "mehr", "Dein", "bit\u00b7tend", "Land", "ver\u00b7wai\u00b7set", "zu", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PPOSAT", "ADJD", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und fodern neue Feinde Dich;", "tokens": ["Und", "fo\u00b7dern", "neu\u00b7e", "Fein\u00b7de", "Dich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Dann gieb uns Waffen, la\u00df Dein Volk zu Felde ziehen,", "tokens": ["Dann", "gieb", "uns", "Waf\u00b7fen", ",", "la\u00df", "Dein", "Volk", "zu", "Fel\u00b7de", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "NN", "$,", "VVIMP", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du aber, unsre Wollust! bleib", "tokens": ["Du", "a\u00b7ber", ",", "uns\u00b7re", "Wol\u00b7lust", "!", "bleib"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "ADV", "$,", "PPOSAT", "NN", "$.", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "fliehen;", "tokens": ["flie\u00b7hen", ";"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Den t\u00f6dte sein beherztes Weib.", "tokens": ["Den", "t\u00f6d\u00b7te", "sein", "be\u00b7herz\u00b7tes", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}