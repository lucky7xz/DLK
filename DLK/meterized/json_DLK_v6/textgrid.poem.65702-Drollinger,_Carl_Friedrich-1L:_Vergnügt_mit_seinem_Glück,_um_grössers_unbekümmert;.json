{"textgrid.poem.65702": {"metadata": {"author": {"name": "Drollinger, Carl Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vergn\u00fcgt mit seinem Gl\u00fcck, um gr\u00f6ssers unbek\u00fcmmert;", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vergn\u00fcgt mit seinem Gl\u00fcck, um gr\u00f6ssers unbek\u00fcmmert;", "tokens": ["Ver\u00b7gn\u00fcgt", "mit", "sei\u00b7nem", "Gl\u00fcck", ",", "um", "gr\u00f6s\u00b7sers", "un\u00b7be\u00b7k\u00fcm\u00b7mert", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,", "KOUI", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von keinem Gut gereizt, das nur von ausen schimmert;", "tokens": ["Von", "kei\u00b7nem", "Gut", "ge\u00b7reizt", ",", "das", "nur", "von", "au\u00b7sen", "schim\u00b7mert", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "$,", "PRELS", "ADV", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch Armut nicht gedr\u00fcckt, mit Reichtum nicht verwirrt!", "tokens": ["Durch", "Ar\u00b7mut", "nicht", "ge\u00b7dr\u00fcckt", ",", "mit", "Reich\u00b7tum", "nicht", "ver\u00b7wirrt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "VVPP", "$,", "APPR", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Im n\u00f6htigen gelehrt, doch nicht zu weit verirrt;", "tokens": ["Im", "n\u00f6h\u00b7ti\u00b7gen", "ge\u00b7lehrt", ",", "doch", "nicht", "zu", "weit", "ver\u00b7irrt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVPP", "$,", "ADV", "PTKNEG", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bem\u00fcht in eignem Tuhn, mit fremdem nie bem\u00e4nget;", "tokens": ["Be\u00b7m\u00fcht", "in", "eig\u00b7nem", "Tuhn", ",", "mit", "frem\u00b7dem", "nie", "be\u00b7m\u00e4n\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein Feind von M\u00fcssiggang, doch nicht zu sehr gedr\u00e4nget;", "tokens": ["Ein", "Feind", "von", "M\u00fcs\u00b7sig\u00b7gang", ",", "doch", "nicht", "zu", "sehr", "ge\u00b7dr\u00e4n\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,", "ADV", "PTKNEG", "PTKA", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dem Kern der Weysheit hold, mehr als dem leeren Klang;", "tokens": ["Dem", "Kern", "der", "Weys\u00b7heit", "hold", ",", "mehr", "als", "dem", "lee\u00b7ren", "Klang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "$,", "PIAT", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Fromm ohne He\u00fcchelschein, und m\u00e4\u00dfig ohne Zwang;", "tokens": ["Fromm", "oh\u00b7ne", "He\u00fc\u00b7chel\u00b7schein", ",", "und", "m\u00e4\u00b7\u00dfig", "oh\u00b7ne", "Zwang", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "KON", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Im Christentum ge\u00fcbt, doch fern von Streit und Hassen;", "tokens": ["Im", "Chris\u00b7ten\u00b7tum", "ge\u00b7\u00fcbt", ",", "doch", "fern", "von", "Streit", "und", "Has\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "ADV", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "In sichrer Nidrigkeit dem Schicksahl stets gelassen:", "tokens": ["In", "sich\u00b7rer", "Nid\u00b7rig\u00b7keit", "dem", "Schick\u00b7sahl", "stets", "ge\u00b7las\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dies war einst Brothag hier. Dem Leben folgt der Lohn.", "tokens": ["Dies", "war", "einst", "Brot\u00b7hag", "hier", ".", "Dem", "Le\u00b7ben", "folgt", "der", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "NN", "ADV", "$.", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Hier trug er Ruh und Lust, den Himmel dort davon.", "tokens": ["Hier", "trug", "er", "Ruh", "und", "Lust", ",", "den", "Him\u00b7mel", "dort", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$,", "ART", "NN", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}