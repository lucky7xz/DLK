{"textgrid.poem.44047": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich weis noch wohl die liebe Zeit,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich weis noch wohl die liebe Zeit,", "tokens": ["Ich", "weis", "noch", "wohl", "die", "lie\u00b7be", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In der ich mich genug erfreut;", "tokens": ["In", "der", "ich", "mich", "ge\u00b7nug", "er\u00b7freut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was waren das vor s\u00fc\u00dfe Tage!", "tokens": ["Was", "wa\u00b7ren", "das", "vor", "s\u00fc\u00b7\u00dfe", "Ta\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Schl\u00e4fe trugen Blum und Glut", "tokens": ["Die", "Schl\u00e4\u00b7fe", "tru\u00b7gen", "Blum", "und", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NE", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und kannten weder Wuntsch noch Plage", "tokens": ["Und", "kann\u00b7ten", "we\u00b7der", "Wunt\u00b7sch", "noch", "Pla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "NN", "ADV", "NN"], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Noch was den Greisen bange thut.", "tokens": ["Noch", "was", "den", "Grei\u00b7sen", "ban\u00b7ge", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein Sorgen gieng auf Lust und Scherz;", "tokens": ["Mein", "Sor\u00b7gen", "gieng", "auf", "Lust", "und", "Scherz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Herz war Amianthens Herz,", "tokens": ["Mein", "Herz", "war", "A\u00b7mi\u00b7an\u00b7thens", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir zehlten weder Ku\u00df noch Stunden,", "tokens": ["Wir", "zehl\u00b7ten", "we\u00b7der", "Ku\u00df", "noch", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tanz, Schauplaz, G\u00e4rthe, Spiel und Wein", "tokens": ["Tanz", ",", "Schau\u00b7plaz", ",", "G\u00e4r\u00b7the", ",", "Spiel", "und", "Wein"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und aller Vortheil der Gesunden", "tokens": ["Und", "al\u00b7ler", "Vor\u00b7theil", "der", "Ge\u00b7sun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nahm Blut und Geist mit Wollust ein.", "tokens": ["Nahm", "Blut", "und", "Geist", "mit", "Wol\u00b7lust", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie? Was? Erzehl ich einen Traum?", "tokens": ["Wie", "?", "Was", "?", "Er\u00b7zehl", "ich", "ei\u00b7nen", "Traum", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PWS", "$.", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum wenigsten gedenckt mich's kaum,", "tokens": ["Zum", "we\u00b7nigs\u00b7ten", "ge\u00b7denckt", "mich's", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "PIS", "VVFIN", "PIS", "ADV", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Mein Gott, wie ist die Zeit entronnen!", "tokens": ["Mein", "Gott", ",", "wie", "ist", "die", "Zeit", "ent\u00b7ron\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was hastu, Herz, von aller Lust?", "tokens": ["Was", "has\u00b7tu", ",", "Herz", ",", "von", "al\u00b7ler", "Lust", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "NN", "$,", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dies, da\u00df du Reu und Leid gewonnen", "tokens": ["Dies", ",", "da\u00df", "du", "Reu", "und", "Leid", "ge\u00b7won\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "$,", "KOUS", "PPER", "NE", "KON", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und wi\u00dfen und entbehren must.", "tokens": ["Und", "wi\u00b7\u00dfen", "und", "ent\u00b7beh\u00b7ren", "must", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "KON", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihr, die ihr die Natur versteht", "tokens": ["Ihr", ",", "die", "ihr", "die", "Na\u00b7tur", "ver\u00b7steht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und durch die Kunst oft h\u00f6her geht,", "tokens": ["Und", "durch", "die", "Kunst", "oft", "h\u00f6\u00b7her", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr k\u00f6nt euch mir recht sehr verbinden:", "tokens": ["Ihr", "k\u00f6nt", "euch", "mir", "recht", "sehr", "ver\u00b7bin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ach sagt mir doch, ich fleh euch an,", "tokens": ["Ach", "sagt", "mir", "doch", ",", "ich", "fleh", "euch", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie soll ich die Maschine finden,", "tokens": ["Wie", "soll", "ich", "die", "Ma\u00b7schi\u00b7ne", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Zeit und Jugend hemmen kan?", "tokens": ["Die", "Zeit", "und", "Ju\u00b7gend", "hem\u00b7men", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}