{"textgrid.poem.23875": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Unschuld", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gieb, sch\u00f6nes Kind, mir deine Hand", "tokens": ["Gieb", ",", "sch\u00f6\u00b7nes", "Kind", ",", "mir", "dei\u00b7ne", "Hand"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "$,", "ADJA", "NN", "$,", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sieh mich an,", "tokens": ["Und", "sieh", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Den Reisenden aus Wehmutland", "tokens": ["Den", "Rei\u00b7sen\u00b7den", "aus", "Weh\u00b7mut\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und \u00e4rmsten Mann.", "tokens": ["Und", "\u00e4rms\u00b7ten", "Mann", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Schlag deine Augen nieder nicht;", "tokens": ["Schlag", "dei\u00b7ne", "Au\u00b7gen", "nie\u00b7der", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "PTKNEG", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sie sind so hold;", "tokens": ["Sie", "sind", "so", "hold", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Noch nicht voll Glut, doch voller Licht", "tokens": ["Noch", "nicht", "voll", "Glut", ",", "doch", "vol\u00b7ler", "Licht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "ADJD", "NN", "$,", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Unschuldsgold.", "tokens": ["Und", "Un\u00b7schulds\u00b7gold", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Das hat so innig milden Schein,", "tokens": ["Das", "hat", "so", "in\u00b7nig", "mil\u00b7den", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Oh s\u00fc\u00dfes Kind,", "tokens": ["Oh", "s\u00fc\u00b7\u00dfes", "Kind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Dass alle K\u00fcmmernisse mein", "tokens": ["Dass", "al\u00b7le", "K\u00fcm\u00b7mer\u00b7nis\u00b7se", "mein"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verflogen sind.", "tokens": ["Ver\u00b7flo\u00b7gen", "sind", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}