{"textgrid.poem.64551": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "175. Eingetroffene Prophezeyhung bey einer Ged\u00e4chtni\u00dfrede.", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil ihm ein schwarzes Kleid gebricht,", "tokens": ["Weil", "ihm", "ein", "schwar\u00b7zes", "Kleid", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So h\u00f6ret mich der Eine nicht.", "tokens": ["So", "h\u00f6\u00b7ret", "mich", "der", "Ei\u00b7ne", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Andre wird mich auch nicht h\u00f6ren,", "tokens": ["Der", "And\u00b7re", "wird", "mich", "auch", "nicht", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem wird's sein schwarzes Herz verwehren.", "tokens": ["Dem", "wird's", "sein", "schwar\u00b7zes", "Herz", "ver\u00b7weh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}