{"dta.poem.17112": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "213.  Die Wei\u00dfheit ist ein Qual.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Die Wei\u00dfheit ist ein Oual/ je mehr man au\u00df jhr trinkt/", "tokens": ["Die", "Wei\u00df\u00b7heit", "ist", "ein", "Ou\u00b7al", "/", "je", "mehr", "man", "au\u00df", "jhr", "trinkt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$(", "ADV", "ADV", "PIS", "APPR", "PPER", "VVFIN", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Je mehr und m\u00e4chtiger sie wieder treibt und springt.", "tokens": ["Je", "mehr", "und", "m\u00e4ch\u00b7ti\u00b7ger", "sie", "wie\u00b7der", "treibt", "und", "springt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADJA", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}