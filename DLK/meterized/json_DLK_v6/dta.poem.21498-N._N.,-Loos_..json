{"dta.poem.21498": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Loos .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hab\u2019 gegl\u00fcht in meinen Jugendtagen,", "tokens": ["Ich", "hab'", "ge\u00b7gl\u00fcht", "in", "mei\u00b7nen", "Ju\u00b7gend\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ganze Welt, ich w\u00e4hnte, sie sei mein;", "tokens": ["Die", "gan\u00b7ze", "Welt", ",", "ich", "w\u00e4hn\u00b7te", ",", "sie", "sei", "mein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da siecht\u2019 ich hin: ach tausendf\u00e4lt\u2019ge Plagen,", "tokens": ["Da", "siecht'", "ich", "hin", ":", "ach", "tau\u00b7send\u00b7f\u00e4lt'\u00b7ge", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie warfen h\u00e4misch auf mich Stein um Stein.", "tokens": ["Sie", "war\u00b7fen", "h\u00e4\u00b7misch", "auf", "mich", "Stein", "um", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPER", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich blutete aus tausend tiefen Wunden,", "tokens": ["Ich", "blu\u00b7te\u00b7te", "aus", "tau\u00b7send", "tie\u00b7fen", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Herz schrie auf: ich litt, litt ", "tokens": ["Mein", "Herz", "schrie", "auf", ":", "ich", "litt", ",", "litt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "$,", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und nie mehr hab\u2019 Genesung ich gefunden,", "tokens": ["Und", "nie", "mehr", "hab'", "Ge\u00b7ne\u00b7sung", "ich", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VAFIN", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich hab\u2019 verzichtet auf des Himmels Huld.", "tokens": ["Ich", "hab'", "ver\u00b7zich\u00b7tet", "auf", "des", "Him\u00b7mels", "Huld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Gelernt hab\u2019 ich\u2019s in vielen b\u00f6sen Jahren:", "tokens": ["Ge\u00b7lernt", "hab'", "ich's", "in", "vie\u00b7len", "b\u00f6\u00b7sen", "Jah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIS", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Entbehren hei\u00dft der Parze strenger Spruch;               ", "tokens": ["Ent\u00b7beh\u00b7ren", "hei\u00dft", "der", "Par\u00b7ze", "stren\u00b7ger", "Spruch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im Himmel auch, dem kalten, g\u00f6tterbaren,", "tokens": ["Im", "Him\u00b7mel", "auch", ",", "dem", "kal\u00b7ten", ",", "g\u00f6t\u00b7ter\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "ART", "ADJA", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich f\u00fcrder mir kein Vateraug\u2019 mehr such\u2019!", "tokens": ["Ich", "f\u00fcr\u00b7der", "mir", "kein", "Va\u00b7ter\u00b7aug'", "mehr", "such'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die graue Schwester hat in n\u00e4cht\u2019gem Fl\u00fcstern", "tokens": ["Die", "grau\u00b7e", "Schwes\u00b7ter", "hat", "in", "n\u00e4cht'\u00b7gem", "Fl\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir\u2019s zugeraunt mit z\u00e4hnelosem Mund,", "tokens": ["Mir's", "zu\u00b7ge\u00b7raunt", "mit", "z\u00e4h\u00b7ne\u00b7lo\u00b7sem", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und unterm Brausen hundertj\u00e4hr\u2019ger R\u00fcstern", "tokens": ["Und", "un\u00b7term", "Brau\u00b7sen", "hun\u00b7dert\u00b7j\u00e4hr'\u00b7ger", "R\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ward, schaudernd, mir ein tief Geheimni\u00df kund.", "tokens": ["Ward", ",", "schau\u00b7dernd", ",", "mir", "ein", "tief", "Ge\u00b7heim\u00b7ni\u00df", "kund", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ADJD", "$,", "PPER", "ART", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Seitdem mu\u00df schreiten ich erstarrt durch\u2019s Leben,", "tokens": ["Seit\u00b7dem", "mu\u00df", "schrei\u00b7ten", "ich", "er\u00b7starrt", "durch's", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "VVFIN", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und doch \u2014 o Widerspruch! \u2014 gef\u00fchllos nicht \u2014", "tokens": ["Und", "doch", "o", "Wi\u00b7der\u00b7spruch", "!", "ge\u00b7f\u00fchl\u00b7los", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "FM", "NN", "$.", "$(", "ADJD", "PTKNEG", "$("], "meter": "+-+---", "measure": "unknown.measure.di"}, "line.3": {"text": "Ich f\u00fcrcht\u2019 und suche nichts und mu\u00df doch beben,", "tokens": ["Ich", "f\u00fcrcht'", "und", "su\u00b7che", "nichts", "und", "mu\u00df", "doch", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PIS", "KON", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sobald Nachtdunkel durch die Wolken bricht.", "tokens": ["So\u00b7bald", "Nacht\u00b7dun\u00b7kel", "durch", "die", "Wol\u00b7ken", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wem ward zur Nacht das herbste Leid gegeben,", "tokens": ["Wem", "ward", "zur", "Nacht", "das", "herbs\u00b7te", "Leid", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPRART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der lernt entsagen jedem Menschenwahn,", "tokens": ["Der", "lernt", "ent\u00b7sa\u00b7gen", "je\u00b7dem", "Men\u00b7schen\u00b7wahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch sieht Erinnerung vorbei er schweben,", "tokens": ["Doch", "sieht", "E\u00b7rin\u00b7ne\u00b7rung", "vor\u00b7bei", "er", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Flucht er der Stunde, die\u2019s ihm angethan.", "tokens": ["Flucht", "er", "der", "Stun\u00b7de", ",", "die's", "ihm", "an\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}