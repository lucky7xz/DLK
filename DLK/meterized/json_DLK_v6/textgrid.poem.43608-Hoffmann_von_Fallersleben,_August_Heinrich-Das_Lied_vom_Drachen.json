{"textgrid.poem.43608": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Lied vom Drachen", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Steig, Drache, steig zum Himmel hinan!", "tokens": ["Steig", ",", "Dra\u00b7che", ",", "steig", "zum", "Him\u00b7mel", "hi\u00b7nan", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hoch oben sieh die weite Welt dir an!", "tokens": ["Hoch", "o\u00b7ben", "sieh", "die", "wei\u00b7te", "Welt", "dir", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sieh an dir die Berge, die Th\u00e4ler und Au'n!", "tokens": ["Sieh", "an", "dir", "die", "Ber\u00b7ge", ",", "die", "Th\u00e4\u00b7ler", "und", "Au'n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Du mu\u00dft dir Alles ganz genau beschau'n!", "tokens": ["Du", "mu\u00dft", "dir", "Al\u00b7les", "ganz", "ge\u00b7nau", "be\u00b7schau'n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Drache h\u00f6rt's und verl\u00e4\u00dft das Feld,", "tokens": ["Der", "Dra\u00b7che", "h\u00f6rt's", "und", "ver\u00b7l\u00e4\u00dft", "das", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er will sich anseh'n ganz genau die Welt.", "tokens": ["Er", "will", "sich", "an\u00b7seh'n", "ganz", "ge\u00b7nau", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVFIN", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Er hat sich noch lange nicht satt geseh'n,", "tokens": ["Er", "hat", "sich", "noch", "lan\u00b7ge", "nicht", "satt", "ge\u00b7seh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "ADV", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Jetzt bleibt er oben in den L\u00fcften steh'n.", "tokens": ["Jetzt", "bleibt", "er", "o\u00b7ben", "in", "den", "L\u00fcf\u00b7ten", "steh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Nun aber wendet er seinen Flug,", "tokens": ["Nun", "a\u00b7ber", "wen\u00b7det", "er", "sei\u00b7nen", "Flug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er kehret schon zur\u00fcck, er sah genug. \u2013", "tokens": ["Er", "keh\u00b7ret", "schon", "zu\u00b7r\u00fcck", ",", "er", "sah", "ge\u00b7nug", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "PPER", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und Mancher sieht manch' Land und Meer,", "tokens": ["Und", "Man\u00b7cher", "sieht", "man\u00b7ch'", "Land", "und", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und kommt zuletzt zur Heimat wieder her.", "tokens": ["Und", "kommt", "zu\u00b7letzt", "zur", "Hei\u00b7mat", "wie\u00b7der", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}