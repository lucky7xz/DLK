{"textgrid.poem.48646": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "9. Philyrille", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein gestirntes Paradeis,", "tokens": ["Mein", "ge\u00b7stirn\u00b7tes", "Pa\u00b7ra\u00b7deis", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "mein Licht, mein Mon, meine Sonne,", "tokens": ["mein", "Licht", ",", "mein", "Mon", ",", "mei\u00b7ne", "Son\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "mein ganz Himmelreich voll Wonne", "tokens": ["mein", "ganz", "Him\u00b7mel\u00b7reich", "voll", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADV", "ADJD", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und von was ein Gott sonst weist,", "tokens": ["und", "von", "was", "ein", "Gott", "sonst", "weist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRELS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "das ist ", "tokens": ["das", "ist"], "token_info": ["word", "word"], "pos": ["PDS", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "mir, der Erden unter ihr.", "tokens": ["mir", ",", "der", "Er\u00b7den", "un\u00b7ter", "ihr", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich verge\u00dfner Erdenkreis!", "tokens": ["Ich", "ver\u00b7ge\u00df\u00b7ner", "Er\u00b7den\u00b7kreis", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Heute tagts zum dritten Male,", "tokens": ["Heu\u00b7te", "tagts", "zum", "drit\u00b7ten", "Ma\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "da\u00df ich ganz von keinem Strale", "tokens": ["da\u00df", "ich", "ganz", "von", "kei\u00b7nem", "Stra\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "meiner lieben Sonnen wei\u00df.", "tokens": ["mei\u00b7ner", "lie\u00b7ben", "Son\u00b7nen", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Das betr\u00fcbte Land, das weint,", "tokens": ["Das", "be\u00b7tr\u00fcb\u00b7te", "Land", ",", "das", "weint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "weil sein Himmel ihm nicht scheint.", "tokens": ["weil", "sein", "Him\u00b7mel", "ihm", "nicht", "scheint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Du, o aller K\u00fcnste Kunst!", "tokens": ["Du", ",", "o", "al\u00b7ler", "K\u00fcns\u00b7te", "Kunst", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Himmel wird durch dich zur Erden.", "tokens": ["Him\u00b7mel", "wird", "durch", "dich", "zur", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df wir Irdnen himmlisch werden,", "tokens": ["Da\u00df", "wir", "Ird\u00b7nen", "himm\u00b7lisch", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "das schafft, Laute, deine Gunst.", "tokens": ["das", "schafft", ",", "Lau\u00b7te", ",", "dei\u00b7ne", "Gunst", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gieb doch, da\u00df mein Himmel sich", "tokens": ["Gieb", "doch", ",", "da\u00df", "mein", "Him\u00b7mel", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$,", "KOUS", "PPOSAT", "NN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "bald neig' auf sein' Erde, mich!", "tokens": ["bald", "neig'", "auf", "sein'", "Er\u00b7de", ",", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}