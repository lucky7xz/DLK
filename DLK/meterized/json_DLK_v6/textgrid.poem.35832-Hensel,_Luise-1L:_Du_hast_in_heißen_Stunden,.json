{"textgrid.poem.35832": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du hast in hei\u00dfen Stunden,", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast in hei\u00dfen Stunden,", "tokens": ["Du", "hast", "in", "hei\u00b7\u00dfen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn mich der Schmerz besiegt,", "tokens": ["Wenn", "mich", "der", "Schmerz", "be\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So oft in Deinen Wunden", "tokens": ["So", "oft", "in", "Dei\u00b7nen", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein armes Kind gewiegt.", "tokens": ["Dein", "ar\u00b7mes", "Kind", "ge\u00b7wiegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich sah in hellen Gluthen", "tokens": ["Ich", "sah", "in", "hel\u00b7len", "Glu\u00b7then"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein g\u00f6ttlich Herz ergl\u00fchn", "tokens": ["Dein", "g\u00f6tt\u00b7lich", "Herz", "er\u00b7gl\u00fchn"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sah in rothen Fluthen", "tokens": ["Und", "sah", "in", "ro\u00b7then", "Flut\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein s\u00fc\u00dfes Leben fliehn.", "tokens": ["Dein", "s\u00fc\u00b7\u00dfes", "Le\u00b7ben", "fliehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dann konnt' ich mich nicht halten,", "tokens": ["Dann", "konnt'", "ich", "mich", "nicht", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich rang in Todeswehn;", "tokens": ["Ich", "rang", "in", "To\u00b7des\u00b7wehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Dir wollt' ich erkalten,", "tokens": ["Mit", "Dir", "wollt'", "ich", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit Dir zum Grabe gehn.", "tokens": ["Mit", "Dir", "zum", "Gra\u00b7be", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dann hab' ich wohl empfunden,", "tokens": ["Dann", "hab'", "ich", "wohl", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie Du mir Alles bist,", "tokens": ["Wie", "Du", "mir", "Al\u00b7les", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "PIS", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wie aus Deinen Wunden", "tokens": ["Und", "wie", "aus", "Dei\u00b7nen", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir Tod und Leben flie\u00dft.", "tokens": ["Mir", "Tod", "und", "Le\u00b7ben", "flie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "In Deines Blutes Fluthen", "tokens": ["In", "Dei\u00b7nes", "Blu\u00b7tes", "Flut\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Taucht' ich die Seele mein", "tokens": ["Taucht'", "ich", "die", "See\u00b7le", "mein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und l\u00f6scht' in heil'gen Gluthen", "tokens": ["Und", "l\u00f6scht'", "in", "heil'\u00b7gen", "Glu\u00b7then"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Erden-Liebe Schein.", "tokens": ["Der", "Er\u00b7den\u00b7Lie\u00b7be", "Schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "In Dir sah ich verrinnen", "tokens": ["In", "Dir", "sah", "ich", "ver\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Der Erde Lust und Pracht,", "tokens": ["Der", "Er\u00b7de", "Lust", "und", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die meine eiteln Sinnen", "tokens": ["Die", "mei\u00b7ne", "ei\u00b7teln", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum G\u00f6tzenbild gemacht.", "tokens": ["Zum", "G\u00f6t\u00b7zen\u00b7bild", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und die im Flitterglanze", "tokens": ["Und", "die", "im", "Flit\u00b7ter\u00b7glan\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Eitelkeit geschwebt,", "tokens": ["Der", "Ei\u00b7tel\u00b7keit", "ge\u00b7schwebt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hat ernst im Dornenkranze", "tokens": ["Hat", "ernst", "im", "Dor\u00b7nen\u00b7kran\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach Deinem Kreuz gestrebt.", "tokens": ["Nach", "Dei\u00b7nem", "Kreuz", "ge\u00b7strebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und bald hast Du nach oben,", "tokens": ["Und", "bald", "hast", "Du", "nach", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "APPR", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du heilig Gotteslamm,", "tokens": ["Du", "hei\u00b7lig", "Got\u00b7tes\u00b7lamm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Seele mir gehoben", "tokens": ["Die", "See\u00b7le", "mir", "ge\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum treu'sten Br\u00e4utigam.", "tokens": ["Zum", "treu'\u00b7sten", "Br\u00e4u\u00b7ti\u00b7gam", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Nun trinkt beim Hochzeitsmahle", "tokens": ["Nun", "trinkt", "beim", "Hoch\u00b7zeits\u00b7mah\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie Deiner Liebe Wein,", "tokens": ["Sie", "Dei\u00b7ner", "Lie\u00b7be", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie tanzt im Sternensaale", "tokens": ["Sie", "tanzt", "im", "Ster\u00b7nen\u00b7saa\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor Dir den ew'gen Reih'n.", "tokens": ["Vor", "Dir", "den", "ew'\u00b7gen", "Reih'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Sie h\u00e4ngt an Deinem Munde,", "tokens": ["Sie", "h\u00e4ngt", "an", "Dei\u00b7nem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie ruht in Deinem Arm;", "tokens": ["Sie", "ruht", "in", "Dei\u00b7nem", "Arm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In Deiner Herzenswunde", "tokens": ["In", "Dei\u00b7ner", "Her\u00b7zens\u00b7wun\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Begr\u00e4bt sie Freud' und Harm.", "tokens": ["Be\u00b7gr\u00e4bt", "sie", "Freud'", "und", "Harm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Gieb mehr, o Gott! zu trinken,", "tokens": ["Gieb", "mehr", ",", "o", "Gott", "!", "zu", "trin\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "FM", "NN", "$.", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gieb mehr der s\u00fc\u00dfen Gluth", "tokens": ["Gieb", "mehr", "der", "s\u00fc\u00b7\u00dfen", "Gluth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und la\u00df mich ganz versinken", "tokens": ["Und", "la\u00df", "mich", "ganz", "ver\u00b7sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In jene Wonnefluth!", "tokens": ["In", "je\u00b7ne", "Won\u00b7ne\u00b7fluth", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}