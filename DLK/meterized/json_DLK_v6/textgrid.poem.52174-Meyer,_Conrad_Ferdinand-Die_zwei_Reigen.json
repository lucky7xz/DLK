{"textgrid.poem.52174": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die zwei Reigen", "genre": "verse", "period": "N.A.", "pub_year": 1861, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Cherub schritt das Tal empor", "tokens": ["Ein", "Che\u00b7rub", "schritt", "das", "Tal", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schlug das Volk mit Schwert und Pest,", "tokens": ["Und", "schlug", "das", "Volk", "mit", "Schwert", "und", "Pest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hinsank der halbe Jugendflor \u2013", "tokens": ["Hin\u00b7sank", "der", "hal\u00b7be", "Ju\u00b7gend\u00b7flor", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Schwalbe kehrt und baut das Nest.", "tokens": ["Die", "Schwal\u00b7be", "kehrt", "und", "baut", "das", "Nest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Brautf\u00fchrer will der Fr\u00fchling sein,", "tokens": ["Braut\u00b7f\u00fch\u00b7rer", "will", "der", "Fr\u00fch\u00b7ling", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wer das Lieb verloren hat,", "tokens": ["Und", "wer", "das", "Lieb", "ver\u00b7lo\u00b7ren", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem gibt mit einem bl\u00fchnden Mai'n", "tokens": ["Dem", "gibt", "mit", "ei\u00b7nem", "bl\u00fchn\u00b7den", "Mai'n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er eines an des toten statt.", "tokens": ["Er", "ei\u00b7nes", "an", "des", "to\u00b7ten", "statt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er f\u00fchrt auf schwellend gr\u00fcnen Plan", "tokens": ["Er", "f\u00fchrt", "auf", "schwel\u00b7lend", "gr\u00fc\u00b7nen", "Plan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Rest der Jugend, neu gepaart,", "tokens": ["Den", "Rest", "der", "Ju\u00b7gend", ",", "neu", "ge\u00b7paart", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und hebt ein m\u00e4chtig Fiedeln an", "tokens": ["Und", "hebt", "ein", "m\u00e4ch\u00b7tig", "Fie\u00b7deln", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJD", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Liebesgl\u00fcck und Minnefahrt.", "tokens": ["Von", "Lie\u00b7bes\u00b7gl\u00fcck", "und", "Min\u00b7ne\u00b7fahrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Paare fliegen rasch daher,", "tokens": ["Die", "Paa\u00b7re", "flie\u00b7gen", "rasch", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Lenzgesind, gejagt vom Wind,", "tokens": ["Ein", "Lenz\u00b7ge\u00b7sind", ",", "ge\u00b7jagt", "vom", "Wind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dabei wird manches Herze schwer,", "tokens": ["Da\u00b7bei", "wird", "man\u00b7ches", "Her\u00b7ze", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PIS", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das an die alte Liebe sinnt...", "tokens": ["Das", "an", "die", "al\u00b7te", "Lie\u00b7be", "sinnt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch Leben hat das Leben gern,", "tokens": ["Doch", "Le\u00b7ben", "hat", "das", "Le\u00b7ben", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und leicht gew\u00f6hnt sich Brust an Brust,", "tokens": ["Und", "leicht", "ge\u00b7w\u00f6hnt", "sich", "Brust", "an", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Toten liegen tief und fern", "tokens": ["Die", "To\u00b7ten", "lie\u00b7gen", "tief", "und", "fern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wissen nichts von unsrer Lust...", "tokens": ["Und", "wis\u00b7sen", "nichts", "von", "uns\u00b7rer", "Lust", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Sonne schwand. Hell scheint ins Land", "tokens": ["Die", "Son\u00b7ne", "schwand", ".", "Hell", "scheint", "ins", "Land"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "NE", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Mond und streut den Silberglanz,", "tokens": ["Der", "Mond", "und", "streut", "den", "Sil\u00b7ber\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Reigen dreht sich Hand in Hand", "tokens": ["Der", "Rei\u00b7gen", "dreht", "sich", "Hand", "in", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Mund an Mund und Kranz an Kranz...", "tokens": ["Und", "Mund", "an", "Mund", "und", "Kranz", "an", "Kranz", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "KON", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Da steigt es aus der Wiese leis", "tokens": ["Da", "steigt", "es", "aus", "der", "Wie\u00b7se", "leis"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und beut sich auch die H\u00e4nde sacht:", "tokens": ["Und", "beut", "sich", "auch", "die", "H\u00e4n\u00b7de", "sacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gen\u00fcber schwebt ein stiller Kreis", "tokens": ["Ge\u00b7n\u00fc\u00b7ber", "schwebt", "ein", "stil\u00b7ler", "Kreis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im blauen Duft der Lenzesnacht.", "tokens": ["Im", "blau\u00b7en", "Duft", "der", "Len\u00b7zes\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Es haucht ein sanfter Fl\u00f6tenlaut", "tokens": ["Es", "haucht", "ein", "sanf\u00b7ter", "Fl\u00f6\u00b7ten\u00b7laut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und toter J\u00fcngling, tote Maid", "tokens": ["Und", "to\u00b7ter", "J\u00fcng\u00b7ling", ",", "to\u00b7te", "Maid"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Umschlingen sich im Reigen traut", "tokens": ["Um\u00b7schlin\u00b7gen", "sich", "im", "Rei\u00b7gen", "traut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ohne Neid und ohne Leid.", "tokens": ["Und", "oh\u00b7ne", "Neid", "und", "oh\u00b7ne", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}