{"textgrid.poem.61765": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Der Ulan", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fchmorgens um vier, eh die H\u00e4hne noch kr\u00e4hn,", "tokens": ["Fr\u00fch\u00b7mor\u00b7gens", "um", "vier", ",", "eh", "die", "H\u00e4h\u00b7ne", "noch", "kr\u00e4hn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "CARD", "$,", "KOUS", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+---+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Da sattelt sein Ro\u00df der Ulan", "tokens": ["Da", "sat\u00b7telt", "sein", "Ro\u00df", "der", "U\u00b7lan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und reitet, den Feind und das Land zu ersp\u00e4hn,", "tokens": ["Und", "rei\u00b7tet", ",", "den", "Feind", "und", "das", "Land", "zu", "er\u00b7sp\u00e4hn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "KON", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Den Waffengenossen voran.", "tokens": ["Den", "Waf\u00b7fen\u00b7ge\u00b7nos\u00b7sen", "vo\u00b7ran", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Hinjagt er durchs Blachfeld und pirscht durch den Forst,", "tokens": ["Hin\u00b7jagt", "er", "durchs", "Blach\u00b7feld", "und", "pirscht", "durch", "den", "Forst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Hoch flattert sein F\u00e4hnlein im Wind,", "tokens": ["Hoch", "flat\u00b7tert", "sein", "F\u00e4hn\u00b7lein", "im", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und er lugt von der H\u00f6h' wie der Falke vom Horst", "tokens": ["Und", "er", "lugt", "von", "der", "H\u00f6h'", "wie", "der", "Fal\u00b7ke", "vom", "Horst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "KOKOM", "ART", "NN", "APPRART", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Und w\u00e4hlt sich die Stra\u00dfe geschwind.", "tokens": ["Und", "w\u00e4hlt", "sich", "die", "Stra\u00b7\u00dfe", "ge\u00b7schwind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "In das sonnige St\u00e4dtchen da sprengt er hinein,", "tokens": ["In", "das", "son\u00b7ni\u00b7ge", "St\u00e4dt\u00b7chen", "da", "sprengt", "er", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Am Rathaus h\u00e4lt er in Ruh':", "tokens": ["Am", "Rat\u00b7haus", "h\u00e4lt", "er", "in", "Ruh'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "\u00bbherr Maire, nun schenkt mir vom sch\u00e4umenden Wein", "tokens": ["\u00bb", "herr", "Mai\u00b7re", ",", "nun", "schenkt", "mir", "vom", "sch\u00e4u\u00b7men\u00b7den", "Wein"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "NN", "NE", "$,", "ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und ein Fr\u00fchst\u00fcck gebt mir dazu!", "tokens": ["Und", "ein", "Fr\u00fch\u00b7st\u00fcck", "gebt", "mir", "da\u00b7zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PAV", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Und schafft mir die pr\u00e4chtigen Rinder daher,", "tokens": ["Und", "schafft", "mir", "die", "pr\u00e4ch\u00b7ti\u00b7gen", "Rin\u00b7der", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "PAV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die am Tor auf den Weiden ich sah,", "tokens": ["Die", "am", "Tor", "auf", "den", "Wei\u00b7den", "ich", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Und Hafer f\u00fcr zwanzig Schwadronen, Herr Maire,", "tokens": ["Und", "Ha\u00b7fer", "f\u00fcr", "zwan\u00b7zig", "Schwad\u00b7ro\u00b7nen", ",", "Herr", "Mai\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "CARD", "NN", "$,", "NN", "NE", "$,"], "meter": "-+--+-++--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Denn die Preu\u00dfen, die Preu\u00dfen sind da.\u00ab", "tokens": ["Denn", "die", "Preu\u00b7\u00dfen", ",", "die", "Preu\u00b7\u00dfen", "sind", "da", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "VAFIN", "ADV", "$.", "$("], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.5": {"line.1": {"text": "Hei lustige Streife! Hei k\u00f6stlicher Scherz,", "tokens": ["Hei", "lus\u00b7ti\u00b7ge", "Strei\u00b7fe", "!", "Hei", "k\u00f6st\u00b7li\u00b7cher", "Scherz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "NE", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wenn der Maire seine B\u00fccklinge macht!", "tokens": ["Wenn", "der", "Mai\u00b7re", "sei\u00b7ne", "B\u00fcck\u00b7lin\u00b7ge", "macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Doch freudiger w\u00e4chst dem Ulanen das Herz,", "tokens": ["Doch", "freu\u00b7di\u00b7ger", "w\u00e4chst", "dem", "U\u00b7la\u00b7nen", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+---+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wenn die Schlacht durch die Ebene kracht;", "tokens": ["Wenn", "die", "Schlacht", "durch", "die", "E\u00b7be\u00b7ne", "kracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.6": {"line.1": {"text": "Wenn, die Z\u00fcgel verh\u00e4ngt und die Lanz' in der Faust,", "tokens": ["Wenn", ",", "die", "Z\u00fc\u00b7gel", "ver\u00b7h\u00e4ngt", "und", "die", "Lanz'", "in", "der", "Faust", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ART", "NN", "VVFIN", "KON", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Das Geschwader mit stiebendem Huf", "tokens": ["Das", "Ge\u00b7schwa\u00b7der", "mit", "stie\u00b7ben\u00b7dem", "Huf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Auf den eisernen Rechen des Fu\u00dfvolks braust", "tokens": ["Auf", "den", "ei\u00b7ser\u00b7nen", "Re\u00b7chen", "des", "Fu\u00df\u00b7volks", "braust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Unter schallendem Hurraruf.", "tokens": ["Un\u00b7ter", "schal\u00b7len\u00b7dem", "Hur\u00b7ra\u00b7ruf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Wohl spein die Haubitzen Verderben und Tod,", "tokens": ["Wohl", "spein", "die", "Hau\u00b7bit\u00b7zen", "Ver\u00b7der\u00b7ben", "und", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wohl deckt sich mit Leichen die Bahn,", "tokens": ["Wohl", "deckt", "sich", "mit", "Lei\u00b7chen", "die", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und die Luft wird wie Blei, und die Erde wird rot,", "tokens": ["Und", "die", "Luft", "wird", "wie", "Blei", ",", "und", "die", "Er\u00b7de", "wird", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "KOKOM", "NN", "$,", "KON", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Doch vorw\u00e4rts st\u00fcrmt der Ulan.", "tokens": ["Doch", "vor\u00b7w\u00e4rts", "st\u00fcrmt", "der", "U\u00b7lan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und rinnt auch das Blut von den Schl\u00e4fen ihm warm:", "tokens": ["Und", "rinnt", "auch", "das", "Blut", "von", "den", "Schl\u00e4\u00b7fen", "ihm", "warm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Durch Geknatter und Kugelgesaus", "tokens": ["Durch", "Ge\u00b7knat\u00b7ter", "und", "Ku\u00b7gel\u00b7ge\u00b7saus"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "K\u00fchn setzt er hinein in den dichtesten Schwarm", "tokens": ["K\u00fchn", "setzt", "er", "hin\u00b7ein", "in", "den", "dich\u00b7tes\u00b7ten", "Schwarm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und holt sich den Adler heraus.", "tokens": ["Und", "holt", "sich", "den", "Ad\u00b7ler", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.9": {"line.1": {"text": "Und \u00bbViktoria\u00ab schallt's durchs Get\u00fcmmel herauf,", "tokens": ["Und", "\u00bb", "Vik\u00b7to\u00b7ria", "\u00ab", "schallt's", "durchs", "Ge\u00b7t\u00fcm\u00b7mel", "her\u00b7auf", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "NE", "$(", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Schon wanken die feindlichen Reihn,", "tokens": ["Schon", "wan\u00b7ken", "die", "feind\u00b7li\u00b7chen", "Reihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und das Wanken wird Flucht, und die Flucht wird Lauf,", "tokens": ["Und", "das", "Wan\u00b7ken", "wird", "Flucht", ",", "und", "die", "Flucht", "wird", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "NN", "$,", "KON", "ART", "NN", "VAFIN", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Der Ulan, der Ulan hinterdrein.", "tokens": ["Der", "U\u00b7lan", ",", "der", "U\u00b7lan", "hin\u00b7ter\u00b7drein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Hinterdrein durch den Flu\u00df, wo die Br\u00fccke verbrannt,", "tokens": ["Hin\u00b7ter\u00b7drein", "durch", "den", "Flu\u00df", ",", "wo", "die", "Br\u00fc\u00b7cke", "ver\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Durch das Dorf, das der Bauer verlie\u00df,", "tokens": ["Durch", "das", "Dorf", ",", "das", "der", "Bau\u00b7er", "ver\u00b7lie\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Mit Gott f\u00fcr K\u00f6nig und Vaterland", "tokens": ["Mit", "Gott", "f\u00fcr", "K\u00f6\u00b7nig", "und", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hinterdrein, hinterdrein bis Paris.", "tokens": ["Hin\u00b7ter\u00b7drein", ",", "hin\u00b7ter\u00b7drein", "bis", "Pa\u00b7ris", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "APPR", "NE", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.11": {"line.1": {"text": "Dort gibt's einen Tanz noch im eisernen Feld,", "tokens": ["Dort", "gibt's", "ei\u00b7nen", "Tanz", "noch", "im", "ei\u00b7ser\u00b7nen", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Bis der Franzmann den Atem verliert,", "tokens": ["Bis", "der", "Franz\u00b7mann", "den", "A\u00b7tem", "ver\u00b7liert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Und Wilhelm der Sieger, der eisgraue Held,", "tokens": ["Und", "Wil\u00b7helm", "der", "Sie\u00b7ger", ",", "der", "eis\u00b7grau\u00b7e", "Held", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Im Louvre den Frieden diktiert.", "tokens": ["Im", "Louv\u00b7re", "den", "Frie\u00b7den", "dik\u00b7tiert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.12": {"line.1": {"text": "Doch wenn dann die blutige Arbeit getan,", "tokens": ["Doch", "wenn", "dann", "die", "blu\u00b7ti\u00b7ge", "Ar\u00b7beit", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und die Stunde der Heimkehr erschien,", "tokens": ["Und", "die", "Stun\u00b7de", "der", "Heim\u00b7kehr", "er\u00b7schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Wie reitet so stattlich im Glied der Ulan", "tokens": ["Wie", "rei\u00b7tet", "so", "statt\u00b7lich", "im", "Glied", "der", "U\u00b7lan"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "ADJD", "APPRART", "NN", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Am Einzugstag in Berlin!", "tokens": ["Am", "Ein\u00b7zugs\u00b7tag", "in", "Ber\u00b7lin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.13": {"line.1": {"text": "Da steht an den Linden die rosigste Dirn',", "tokens": ["Da", "steht", "an", "den", "Lin\u00b7den", "die", "ro\u00b7sigs\u00b7te", "Dirn'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NE", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und sie jubelt vor Stolz und vor Lust:", "tokens": ["Und", "sie", "ju\u00b7belt", "vor", "Stolz", "und", "vor", "Lust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "\u00bbo wie lieb' ich dich erst um die Narb' auf der Stirn", "tokens": ["\u00bb", "o", "wie", "lieb'", "ich", "dich", "erst", "um", "die", "Na\u00b7rb'", "auf", "der", "Stirn"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "FM", "KOKOM", "VVFIN", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+--+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Und das Eiserne Kreuz auf der Brust!\u00ab", "tokens": ["Und", "das", "Ei\u00b7ser\u00b7ne", "Kreuz", "auf", "der", "Brust", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}}}}