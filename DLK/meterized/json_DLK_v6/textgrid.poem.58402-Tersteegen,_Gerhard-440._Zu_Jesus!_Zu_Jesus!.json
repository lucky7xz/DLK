{"textgrid.poem.58402": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "440. Zu Jesus! Zu Jesus!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Str\u00e4hlchen geht vorbei, kehr dein Gesicht zur Sonnen,", "tokens": ["Ein", "Str\u00e4hl\u00b7chen", "geht", "vor\u00b7bei", ",", "kehr", "dein", "Ge\u00b7sicht", "zur", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Tr\u00f6pflein macht nicht satt, lauf selber hin zum Bronnen,", "tokens": ["Ein", "Tr\u00f6pf\u00b7lein", "macht", "nicht", "satt", ",", "lauf", "sel\u00b7ber", "hin", "zum", "Bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADJD", "$,", "ADV", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du findst die Lebensquell in deinem Herzen nur;", "tokens": ["Du", "findst", "die", "Le\u00b7bens\u00b7quell", "in", "dei\u00b7nem", "Her\u00b7zen", "nur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kehr deinen Durst hinein von aller Kreatur!", "tokens": ["Kehr", "dei\u00b7nen", "Durst", "hin\u00b7ein", "von", "al\u00b7ler", "Kre\u00b7a\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}