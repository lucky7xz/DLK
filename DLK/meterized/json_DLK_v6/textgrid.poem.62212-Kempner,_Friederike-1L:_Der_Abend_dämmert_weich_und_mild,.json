{"textgrid.poem.62212": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Abend d\u00e4mmert weich und mild,", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Abend d\u00e4mmert weich und mild,", "tokens": ["Der", "A\u00b7bend", "d\u00e4m\u00b7mert", "weich", "und", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nichts st\u00f6rt des Schweigens Stille,", "tokens": ["Nichts", "st\u00f6rt", "des", "Schwei\u00b7gens", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da tritt der Mond hervor aus seiner H\u00fclle,", "tokens": ["Da", "tritt", "der", "Mond", "her\u00b7vor", "aus", "sei\u00b7ner", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Beleuchtend ein erhabenes Bild.", "tokens": ["Be\u00b7leuch\u00b7tend", "ein", "er\u00b7ha\u00b7be\u00b7nes", "Bild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Die Kokospalme bl\u00fcht und der Granatbaum brennt", "tokens": ["Die", "Ko\u00b7ko\u00b7spal\u00b7me", "bl\u00fcht", "und", "der", "Gra\u00b7nat\u00b7baum", "brennt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Im frischen menschenhohen Grase,", "tokens": ["Im", "fri\u00b7schen", "men\u00b7schen\u00b7ho\u00b7hen", "Gra\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ist dies die menschliche Oase,", "tokens": ["Ist", "dies", "die", "menschli\u00b7che", "O\u00b7a\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo man nicht Ha\u00df, noch Liebe kennt?", "tokens": ["Wo", "man", "nicht", "Ha\u00df", ",", "noch", "Lie\u00b7be", "kennt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PTKNEG", "NN", "$,", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Im Schatten eines Palmenhains,", "tokens": ["Im", "Schat\u00b7ten", "ei\u00b7nes", "Pal\u00b7men\u00b7hains", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im wei\u00dfen Kleid mit langen Haaren", "tokens": ["Im", "wei\u00b7\u00dfen", "Kleid", "mit", "lan\u00b7gen", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da kniet die Priesterin von achtzehn Jahren,", "tokens": ["Da", "kniet", "die", "Pries\u00b7te\u00b7rin", "von", "acht\u00b7zehn", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bestrahlt vom Licht des Mondenscheins.", "tokens": ["Be\u00b7strahlt", "vom", "Licht", "des", "Mon\u00b7den\u00b7scheins", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie spricht ein wunderbar Gebet,", "tokens": ["Sie", "spricht", "ein", "wun\u00b7der\u00b7bar", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+----", "measure": "unknown.measure.di"}, "line.2": {"text": "Horch, was sie leise innig fleht:", "tokens": ["Horch", ",", "was", "sie", "lei\u00b7se", "in\u00b7nig", "fleht", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "ADJD", "ADJD", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}}}}}