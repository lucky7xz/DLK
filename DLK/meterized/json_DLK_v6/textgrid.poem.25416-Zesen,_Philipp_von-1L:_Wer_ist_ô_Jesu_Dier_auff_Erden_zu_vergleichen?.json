{"textgrid.poem.25416": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer ist/ \u00f4 Jesu/ Dier auff Erden zu vergleichen?", "genre": "verse", "period": "N.A.", "pub_year": 1654, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer ist/ \u00f4 Jesu/ Dier auff Erden zu vergleichen?", "tokens": ["Wer", "ist", "/", "\u00f4", "Je\u00b7su", "/", "Dier", "auff", "Er\u00b7den", "zu", "ver\u00b7glei\u00b7chen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$(", "FM", "FM", "$(", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann du ", "tokens": ["Dann", "du"], "token_info": ["word", "word"], "pos": ["ADV", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Die Jericho gezeugt/ die immer wohlgeriet;", "tokens": ["Die", "Je\u00b7ri\u00b7cho", "ge\u00b7zeugt", "/", "die", "im\u00b7mer", "wohl\u00b7ge\u00b7riet", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVPP", "$(", "ART", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den lieblichen Geruch kann Balsam nicht erreichen/", "tokens": ["Den", "lieb\u00b7li\u00b7chen", "Ge\u00b7ruch", "kann", "Bal\u00b7sam", "nicht", "er\u00b7rei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "NE", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es mu\u00df Dier Libanon und auch Egypten weichen/", "tokens": ["Es", "mu\u00df", "Dier", "Li\u00b7ba\u00b7non", "und", "auch", "E\u00b7gyp\u00b7ten", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "NE", "KON", "ADV", "NE", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dann Du ", "tokens": ["Dann", "Du"], "token_info": ["word", "word"], "pos": ["ADV", "PPER"], "meter": "++", "measure": "spondeus"}, "line.7": {"text": "So deine starcke Krafft empfindt und nach sich ziht/", "tokens": ["So", "dei\u00b7ne", "star\u00b7cke", "Krafft", "emp\u00b7findt", "und", "nach", "sich", "ziht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVPP", "KON", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wann Du den s\u00fc\u00dfen West von dier l\u00e4st auff mich streichen.", "tokens": ["Wann", "Du", "den", "s\u00fc\u00b7\u00dfen", "West", "von", "dier", "l\u00e4st", "auff", "mich", "strei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du sauberst mich durchau\u00df und gibst mir solche krafft/", "tokens": ["Du", "sau\u00b7berst", "mich", "durch\u00b7au\u00df", "und", "gibst", "mir", "sol\u00b7che", "krafft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "die niemahls hat erweckt der wohlgeschmackste safft;", "tokens": ["die", "nie\u00b7mahls", "hat", "er\u00b7weckt", "der", "wohl\u00b7ge\u00b7schmacks\u00b7te", "safft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "VVPP", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dein lieblicher Geruch kann dieses alles machen.", "tokens": ["Dein", "lieb\u00b7li\u00b7cher", "Ge\u00b7ruch", "kann", "die\u00b7ses", "al\u00b7les", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "PDS", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ach! weiche nicht von mir/ sonst wird die Lust zergehn/", "tokens": ["Ach", "!", "wei\u00b7che", "nicht", "von", "mir", "/", "sonst", "wird", "die", "Lust", "zer\u00b7gehn", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADJA", "PTKNEG", "APPR", "PPER", "$(", "ADV", "VAFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wann auff mich hauchet zu des Satans wilder Rachen;", "tokens": ["Wann", "auff", "mich", "hau\u00b7chet", "zu", "des", "Sa\u00b7tans", "wil\u00b7der", "Ra\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPER", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Entzeuch nicht deine Krafft/ denn Du ", "tokens": ["Ent\u00b7zeuch", "nicht", "dei\u00b7ne", "Krafft", "/", "denn", "Du"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PTKNEG", "PPOSAT", "NN", "$(", "KON", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}