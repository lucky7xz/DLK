{"dta.poem.8897": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Und k\u00f6nnt euch nicht im Duft der Rose baden;", "tokens": ["Und", "k\u00f6nnt", "euch", "nicht", "im", "Duft", "der", "Ro\u00b7se", "ba\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch was ihr saht an blumigern Gestaden", "tokens": ["Doch", "was", "ihr", "saht", "an", "blu\u00b7mi\u00b7gern", "Ge\u00b7sta\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verge\u00dft ihr hier und w\u00fcnscht es kaum zu schauen.", "tokens": ["Ver\u00b7ge\u00dft", "ihr", "hier", "und", "w\u00fcnscht", "es", "kaum", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KON", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die stern'ge Nacht beginnt gemach zu thauen,", "tokens": ["Die", "stern'\u00b7ge", "Nacht", "be\u00b7ginnt", "ge\u00b7mach", "zu", "thau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um auf den Markus Alles einzuladen:", "tokens": ["Um", "auf", "den", "Mar\u00b7kus", "Al\u00b7les", "ein\u00b7zu\u00b7la\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "NN", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da sitzen unter herrlichen Arkaden,", "tokens": ["Da", "sit\u00b7zen", "un\u00b7ter", "herr\u00b7li\u00b7chen", "Ar\u00b7ka\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In langen Reih'n, Venedigs sch\u00f6nste Frauen.", "tokens": ["In", "lan\u00b7gen", "Reih'n", ",", "Ve\u00b7ne\u00b7digs", "sch\u00f6ns\u00b7te", "Frau\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NE", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Doch auf des Platzes Mitte treibt geschwinde,", "tokens": ["Doch", "auf", "des", "Plat\u00b7zes", "Mit\u00b7te", "treibt", "ge\u00b7schwin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Canaletto das versucht zu malen,", "tokens": ["Wie", "Ca\u00b7na\u00b7let\u00b7to", "das", "ver\u00b7sucht", "zu", "ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sich Schaar an Schaar, Musik verhallt gelinde.", "tokens": ["Sich", "Schaar", "an", "Schaar", ",", "Mu\u00b7sik", "ver\u00b7hallt", "ge\u00b7lin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "APPR", "NN", "$,", "NN", "VVFIN", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Indessen wehn, auf ehrnen Piedestalen,", "tokens": ["In\u00b7des\u00b7sen", "wehn", ",", "auf", "ehr\u00b7nen", "Pie\u00b7de\u00b7sta\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Flaggen dreyer Monarchien im Winde,", "tokens": ["Die", "Flag\u00b7gen", "drey\u00b7er", "Mon\u00b7ar\u00b7chi\u00b7en", "im", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die von Venedigs altem Ruhme stralen.", "tokens": ["Die", "von", "Ve\u00b7ne\u00b7digs", "al\u00b7tem", "Ruh\u00b7me", "stra\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}}}}