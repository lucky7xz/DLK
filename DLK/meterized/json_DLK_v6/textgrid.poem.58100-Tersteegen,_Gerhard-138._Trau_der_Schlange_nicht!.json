{"textgrid.poem.58100": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "138. Trau der Schlange nicht!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fcnd' ist ein listig Ding: sie macht sich anfangs klein,", "tokens": ["S\u00fcnd'", "ist", "ein", "lis\u00b7tig", "Ding", ":", "sie", "macht", "sich", "an\u00b7fangs", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJD", "NN", "$.", "PPER", "VVFIN", "PRF", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie schmeichelt sanft und s\u00fc\u00df und kommt in sch\u00f6nem Schein,", "tokens": ["Sie", "schmei\u00b7chelt", "sanft", "und", "s\u00fc\u00df", "und", "kommt", "in", "sch\u00f6\u00b7nem", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hernach da wird sie gro\u00df und greulich im Gesicht;", "tokens": ["Her\u00b7nach", "da", "wird", "sie", "gro\u00df", "und", "greu\u00b7lich", "im", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vergaff dich nicht, o Seel', la\u00df ihr den Willen nicht!", "tokens": ["Ver\u00b7gaff", "dich", "nicht", ",", "o", "Seel'", ",", "la\u00df", "ihr", "den", "Wil\u00b7len", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "$,", "FM", "NN", "$,", "VVIMP", "PPER", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}