{"dta.poem.12935": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie graue haare bekam, und  \n doch noch sch\u00f6n war.  \n Aus dem welschen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist kein wunder nicht,", "tokens": ["Es", "ist", "kein", "wun\u00b7der", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df bey dir der kalte winter andrer sch\u00f6nen lentz absticht,", "tokens": ["Da\u00df", "bey", "dir", "der", "kal\u00b7te", "win\u00b7ter", "an\u00b7drer", "sch\u00f6\u00b7nen", "lentz", "ab\u00b7sticht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ART", "ADJA", "NN", "ADJA", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+--++", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Weil offt die ros\u2019 im Jenner bl\u00fchet,", "tokens": ["Weil", "offt", "die", "ros'", "im", "Jen\u00b7ner", "bl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da man noch schnee in dem Aprille siehet.", "tokens": ["Da", "man", "noch", "schnee", "in", "dem", "Ap\u00b7ril\u00b7le", "sie\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVFIN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das aber ist ein werck, das etwas h\u00f6her steigt,", "tokens": ["Das", "a\u00b7ber", "ist", "ein", "werck", ",", "das", "et\u00b7was", "h\u00f6\u00b7her", "steigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "ART", "NN", "$,", "PRELS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und der verwundrung werth zu achten,", "tokens": ["Und", "der", "ver\u00b7wund\u00b7rung", "werth", "zu", "ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn wir den Jenner itzt in deinem haar betrachten,", "tokens": ["Wenn", "wir", "den", "Jen\u00b7ner", "itzt", "in", "dei\u00b7nem", "haar", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da der April sich im gesichte zeiget.", "tokens": ["Da", "der", "Ap\u00b7ril", "sich", "im", "ge\u00b7sich\u00b7te", "zei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}