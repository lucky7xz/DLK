{"dta.poem.11185": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine he\u00dfliche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer dich sieht und deine magd, der f\u00e4llt mir gar gerne bey,", "tokens": ["Wer", "dich", "sieht", "und", "dei\u00b7ne", "magd", ",", "der", "f\u00e4llt", "mir", "gar", "ger\u00b7ne", "bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "KON", "PPOSAT", "NN", "$,", "PRELS", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.2": {"text": "Da\u00df die dienerin viel sch\u00f6ner, als die jungfer selber sey.", "tokens": ["Da\u00df", "die", "die\u00b7ne\u00b7rin", "viel", "sch\u00f6\u00b7ner", ",", "als", "die", "jung\u00b7fer", "sel\u00b7ber", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PIAT", "ADJA", "$,", "KOUS", "ART", "ADJA", "ADV", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Schaffe du dein m\u00e4gdgen weg, die dir solchen schatten giebet;", "tokens": ["Schaf\u00b7fe", "du", "dein", "m\u00e4gd\u00b7gen", "weg", ",", "die", "dir", "sol\u00b7chen", "schat\u00b7ten", "gie\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,", "PRELS", "PPER", "PIAT", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Sonsten wird der umstand mehr, als die sache selbst, geliebet.", "tokens": ["Sons\u00b7ten", 