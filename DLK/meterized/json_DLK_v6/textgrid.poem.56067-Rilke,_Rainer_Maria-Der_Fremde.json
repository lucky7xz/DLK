{"textgrid.poem.56067": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Der Fremde", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ohne Sorgfalt, was die N\u00e4chsten d\u00e4chten,", "tokens": ["Oh\u00b7ne", "Sorg\u00b7falt", ",", "was", "die", "N\u00e4chs\u00b7ten", "d\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "die er m\u00fcde nichtmehr fragen hie\u00df,", "tokens": ["die", "er", "m\u00fc\u00b7de", "nicht\u00b7mehr", "fra\u00b7gen", "hie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "ADJD", "PIS", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "ging er wieder fort; verlor, verlie\u00df \u2013.", "tokens": ["ging", "er", "wie\u00b7der", "fort", ";", "ver\u00b7lor", ",", "ver\u00b7lie\u00df", "\u2013", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "$.", "VVFIN", "$,", "VVFIN", "$(", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Denn er hing an solchen Reisen\u00e4chten", "tokens": ["Denn", "er", "hing", "an", "sol\u00b7chen", "Rei\u00b7se\u00b7n\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "anders als an jeder Liebesnacht.", "tokens": ["an\u00b7ders", "als", "an", "je\u00b7der", "Lie\u00b7bes\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wunderbare hatte er durchwacht,", "tokens": ["Wun\u00b7der\u00b7ba\u00b7re", "hat\u00b7te", "er", "durch\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "die mit starken Sternen \u00fcberzogen", "tokens": ["die", "mit", "star\u00b7ken", "Ster\u00b7nen", "\u00fc\u00b7berz\u00b7o\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "enge Fernen auseinanderbogen", "tokens": ["en\u00b7ge", "Fer\u00b7nen", "aus\u00b7ein\u00b7an\u00b7der\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "und sich wandelten wie eine Schlacht;", "tokens": ["und", "sich", "wan\u00b7del\u00b7ten", "wie", "ei\u00b7ne", "Schlacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "andre, die mit in den Mond gestreuten", "tokens": ["and\u00b7re", ",", "die", "mit", "in", "den", "Mond", "ge\u00b7streu\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "$,", "PRELS", "APPR", "APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "D\u00f6rfern, wie mit hingehaltnen Beuten,", "tokens": ["D\u00f6r\u00b7fern", ",", "wie", "mit", "hin\u00b7ge\u00b7halt\u00b7nen", "Beu\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "sich ergaben, oder durch geschonte", "tokens": ["sich", "er\u00b7ga\u00b7ben", ",", "o\u00b7der", "durch", "ge\u00b7schon\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "VVINF", "$,", "KON", "APPR", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Parke graue Edelsitze zeigten,", "tokens": ["Par\u00b7ke", "grau\u00b7e", "E\u00b7del\u00b7sit\u00b7ze", "zeig\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "die er gerne in dem hingeneigten", "tokens": ["die", "er", "ger\u00b7ne", "in", "dem", "hin\u00b7ge\u00b7neig\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRELS", "PPER", "ADV", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Haupte einen Augenblick bewohnte,", "tokens": ["Haup\u00b7te", "ei\u00b7nen", "Au\u00b7gen\u00b7blick", "be\u00b7wohn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "tiefer wissend, da\u00df man nirgends bleibt;", "tokens": ["tie\u00b7fer", "wis\u00b7send", ",", "da\u00df", "man", "nir\u00b7gends", "bleibt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "KOUS", "PIS", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "und schon sah er bei dem n\u00e4chsten Biegen", "tokens": ["und", "schon", "sah", "er", "bei", "dem", "n\u00e4chs\u00b7ten", "Bie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "wieder Wege, Br\u00fccken, L\u00e4nder liegen", "tokens": ["wie\u00b7der", "We\u00b7ge", ",", "Br\u00fc\u00b7cken", ",", "L\u00e4n\u00b7der", "lie\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "NN", "$,", "NN", "$,", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "bis an St\u00e4dte, die man \u00fcbertreibt.", "tokens": ["bis", "an", "St\u00e4d\u00b7te", ",", "die", "man", "\u00fc\u00b7bert\u00b7reibt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Und dies alles immer unbegehrend", "tokens": ["Und", "dies", "al\u00b7les", "im\u00b7mer", "un\u00b7be\u00b7geh\u00b7rend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "PIS", "ADV", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "hinzulassen, schien ihm mehr als seines", "tokens": ["hin\u00b7zu\u00b7las\u00b7sen", ",", "schien", "ihm", "mehr", "als", "sei\u00b7nes"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "VVFIN", "PPER", "PIAT", "KOKOM", "PPOSAT"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Lebens Lust, Besitz und Ruhm.", "tokens": ["Le\u00b7bens", "Lust", ",", "Be\u00b7sitz", "und", "Ruhm", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch auf fremden Pl\u00e4tzen war ihm eines", "tokens": ["Doch", "auf", "frem\u00b7den", "Pl\u00e4t\u00b7zen", "war", "ihm", "ei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VAFIN", "PPER", "ART"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "t\u00e4glich ausgetretnen Brunnensteines", "tokens": ["t\u00e4g\u00b7lich", "aus\u00b7ge\u00b7tret\u00b7nen", "Brun\u00b7nen\u00b7stei\u00b7nes"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Mulde manchmal wie ein Eigentum.", "tokens": ["Mul\u00b7de", "manch\u00b7mal", "wie", "ein", "Ei\u00b7gen\u00b7tum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}