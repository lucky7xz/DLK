{"dta.poem.10311": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "( Im Marschgarten. )", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Nach Osten beugt sich Baum und Beerenflur,", "tokens": ["Nach", "Os\u00b7ten", "beugt", "sich", "Baum", "und", "Bee\u00b7ren\u00b7flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PRF", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Denn ewig zerrt der West in Sturm und Regen.", "tokens": ["Denn", "e\u00b7wig", "zerrt", "der", "West", "in", "Sturm", "und", "Re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein d\u00fcrftig Birnenb\u00e4umchen stemmt sich nur", "tokens": ["Ein", "d\u00fcrf\u00b7tig", "Bir\u00b7nen\u00b7b\u00e4um\u00b7chen", "stemmt", "sich", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PRF", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit aller Macht dem b\u00f6sen Wind entgegen.", "tokens": ["Mit", "al\u00b7ler", "Macht", "dem", "b\u00f6\u00b7sen", "Wind", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Des umgeklappten Regenschirms Figur,", "tokens": ["Des", "um\u00b7ge\u00b7klapp\u00b7ten", "Re\u00b7gen\u00b7schirms", "Fi\u00b7gur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Streckt es die \u00c4rmchen aus wie strittige Degen.", "tokens": ["Streckt", "es", "die", "\u00c4rm\u00b7chen", "aus", "wie", "strit\u00b7ti\u00b7ge", "De\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "KOKOM", "ADJA", "NN", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Neulich, bei dir, that ich den Fahnenschwur:", "tokens": ["Neu\u00b7lich", ",", "bei", "dir", ",", "that", "ich", "den", "Fah\u00b7nen\u00b7schwur", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "PPER", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Trotzig wie du lass\u2019 ich die Stirn mir fegen.", "tokens": ["Trot\u00b7zig", "wie", "du", "lass'", "ich", "die", "Stirn", "mir", "fe\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "PPER", "VVFIN", "PPER", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}}}}}