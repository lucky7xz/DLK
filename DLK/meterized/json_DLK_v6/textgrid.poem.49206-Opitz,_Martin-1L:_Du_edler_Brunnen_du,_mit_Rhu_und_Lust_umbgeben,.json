{"textgrid.poem.49206": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du edler Brunnen du, mit Rhu und Lust umbgeben,", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du edler Brunnen du, mit Rhu und Lust umbgeben,", "tokens": ["Du", "ed\u00b7ler", "Brun\u00b7nen", "du", ",", "mit", "Rhu", "und", "Lust", "umb\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPER", "$,", "APPR", "NE", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Bergen hier und da als einer Burg umbringt,", "tokens": ["Mit", "Ber\u00b7gen", "hier", "und", "da", "als", "ei\u00b7ner", "Burg", "um\u00b7bringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "KON", "ADV", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Printz aller sch\u00f6nen Quell', au\u00df welchen Wasser dringt,", "tokens": ["Printz", "al\u00b7ler", "sch\u00f6\u00b7nen", "Quell'", ",", "au\u00df", "wel\u00b7chen", "Was\u00b7ser", "dringt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "$,", "APPR", "PWAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Anmutiger dann Milch, und k\u00f6stlicher dann Reben,", "tokens": ["An\u00b7mu\u00b7ti\u00b7ger", "dann", "Milch", ",", "und", "k\u00f6st\u00b7li\u00b7cher", "dann", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "$,", "KON", "ADJD", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da unsers Landes Kron' und H\u00e4upt mit seinem Leben,", "tokens": ["Da", "un\u00b7sers", "Lan\u00b7des", "Kron'", "und", "H\u00e4upt", "mit", "sei\u00b7nem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NE", "KON", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der werthen Nymph', offt selbst die lange Zeit verbringt,", "tokens": ["Der", "wert\u00b7hen", "Nym\u00b7ph'", ",", "offt", "selbst", "die", "lan\u00b7ge", "Zeit", "ver\u00b7bringt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da das Gefl\u00fcgel ihr zu Ehren lieblich singt,", "tokens": ["Da", "das", "Ge\u00b7fl\u00fc\u00b7gel", "ihr", "zu", "Eh\u00b7ren", "lieb\u00b7lich", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "APPR", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da nur Ergetzlichkeit und keusche Wollust schweben,", "tokens": ["Da", "nur", "Er\u00b7getz\u00b7lich\u00b7keit", "und", "keu\u00b7sche", "Wol\u00b7lust", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Vergeblich bist du nicht in dieses gr\u00fcne Thal", "tokens": ["Ver\u00b7geb\u00b7lich", "bist", "du", "nicht", "in", "die\u00b7ses", "gr\u00fc\u00b7ne", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "PTKNEG", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beschlossen von Gebirg' und Klippen \u00fcberall:", "tokens": ["Be\u00b7schlos\u00b7sen", "von", "Ge\u00b7bir\u00b7g'", "und", "Klip\u00b7pen", "\u00fc\u00b7be\u00b7rall", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die k\u00fcnstliche Natur hat darumb dich umbfangen", "tokens": ["Die", "k\u00fcnst\u00b7li\u00b7che", "Na\u00b7tur", "hat", "da\u00b7rumb", "dich", "umb\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PAV", "PPER", "VVINF"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Mit Felsen und Gep\u00fcsch', auff da\u00df man wissen soll,", "tokens": ["Mit", "Fel\u00b7sen", "und", "Gep\u00fcsch'", ",", "auff", "da\u00df", "man", "wis\u00b7sen", "soll", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "KOUS", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da\u00df alle Fr\u00f6ligkeit sey M\u00fch' und Arbeit voll,", "tokens": ["Da\u00df", "al\u00b7le", "Fr\u00f6\u00b7lig\u00b7keit", "sey", "M\u00fch'", "und", "Ar\u00b7beit", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VAFIN", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und da\u00df auch nichts so sch\u00f6n, es sey schwer zu erlangen.", "tokens": ["Und", "da\u00df", "auch", "nichts", "so", "sch\u00f6n", ",", "es", "sey", "schwer", "zu", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PIS", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}