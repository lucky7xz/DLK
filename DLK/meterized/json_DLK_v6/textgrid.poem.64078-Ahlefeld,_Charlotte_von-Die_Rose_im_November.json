{"textgrid.poem.64078": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Die Rose im November", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herbstlich rauh ver\u00f6det sind die Fluren,", "tokens": ["Herbst\u00b7lich", "rauh", "ver\u00b7\u00f6\u00b7det", "sind", "die", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und verschwunden ist des Sommers Glanz;", "tokens": ["Und", "ver\u00b7schwun\u00b7den", "ist", "des", "Som\u00b7mers", "Glanz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dennoch reich' ich eine seiner Spuren", "tokens": ["Den\u00b7noch", "reich'", "ich", "ei\u00b7ne", "sei\u00b7ner", "Spu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Gl\u00fchend Dir zum jugendlichen Kranz.", "tokens": ["Gl\u00fc\u00b7hend", "Dir", "zum", "ju\u00b7gend\u00b7li\u00b7chen", "Kranz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Diese Rose, die sich sp\u00e4t erschlossen,", "tokens": ["Die\u00b7se", "Ro\u00b7se", ",", "die", "sich", "sp\u00e4t", "er\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "PRF", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dufte Lenzgef\u00fchl Dir in die Brust. \u2013", "tokens": ["Duf\u00b7te", "Lenz\u00b7ge\u00b7f\u00fchl", "Dir", "in", "die", "Brust", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ach sie hat die Sonne nicht genossen,", "tokens": ["Ach", "sie", "hat", "die", "Son\u00b7ne", "nicht", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VAFIN", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nicht der milderen Entwicklung Lust.", "tokens": ["Nicht", "der", "mil\u00b7de\u00b7ren", "Ent\u00b7wick\u00b7lung", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Feuchte L\u00fcfte haben sie erzogen,", "tokens": ["Feuch\u00b7te", "L\u00fcf\u00b7te", "ha\u00b7ben", "sie", "er\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dennoch trotzte sie mit innrer Kraft,", "tokens": ["Den\u00b7noch", "trotz\u00b7te", "sie", "mit", "inn\u00b7rer", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Still und schweigend kalten Nebelwogen,", "tokens": ["Still", "und", "schwei\u00b7gend", "kal\u00b7ten", "Ne\u00b7bel\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und des Sturmes wilder Leidenschaft.", "tokens": ["Und", "des", "Stur\u00b7mes", "wil\u00b7der", "Lei\u00b7den\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Dir, der tiefe Innigkeit und G\u00fcte", "tokens": ["Dir", ",", "der", "tie\u00b7fe", "In\u00b7nig\u00b7keit", "und", "G\u00fc\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mitgef\u00fchl f\u00fcr stumme Schmerzen reicht,", "tokens": ["Mit\u00b7ge\u00b7f\u00fchl", "f\u00fcr", "stum\u00b7me", "Schmer\u00b7zen", "reicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sage ihre bald verwelkte Bl\u00fcthe", "tokens": ["Sa\u00b7ge", "ih\u00b7re", "bald", "ver\u00b7welk\u00b7te", "Bl\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Welchem Loos ihr tr\u00fcbes Schicksal gleicht.", "tokens": ["Wel\u00b7chem", "Loos", "ihr", "tr\u00fc\u00b7bes", "Schick\u00b7sal", "gleicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}