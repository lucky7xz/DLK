{"textgrid.poem.61785": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Ferien", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Waldhang \u00fcberm Wiesengrunde", "tokens": ["Am", "Wald\u00b7hang", "\u00fc\u00b7berm", "Wie\u00b7sen\u00b7grun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ruht sich's gut zur Mittagstunde,", "tokens": ["Wie", "ruht", "sich's", "gut", "zur", "Mit\u00b7tags\u00b7tun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn nur mit sanftem Hauch der Wind", "tokens": ["Wenn", "nur", "mit", "sanf\u00b7tem", "Hauch", "der", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durchs Laub der Wipfel fl\u00fcsternd rinnt!", "tokens": ["Durchs", "Laub", "der", "Wip\u00b7fel", "fl\u00fcs\u00b7ternd", "rinnt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Hier, vor der Welt und ihren Sorgen", "tokens": ["Hier", ",", "vor", "der", "Welt", "und", "ih\u00b7ren", "Sor\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Scho\u00df der Einsamkeit geborgen,", "tokens": ["Im", "Scho\u00df", "der", "Ein\u00b7sam\u00b7keit", "ge\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Genie\u00df' ich endlich frei von Zwang", "tokens": ["Ge\u00b7ni\u00b7e\u00df'", "ich", "end\u00b7lich", "frei", "von", "Zwang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADJD", "APPR", "NN"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Den langentbehrten M\u00fc\u00dfiggang.", "tokens": ["Den", "lan\u00b7gent\u00b7behr\u00b7ten", "M\u00fc\u00b7\u00dfig\u00b7gang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da saugt mein Leib aus Luft und Sonne", "tokens": ["Da", "saugt", "mein", "Leib", "aus", "Luft", "und", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Daseins reinste Pflanzenwonne,", "tokens": ["Des", "Da\u00b7seins", "reins\u00b7te", "Pflan\u00b7zen\u00b7won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Indes der Geist zu freiem Spiel", "tokens": ["In\u00b7des", "der", "Geist", "zu", "frei\u00b7em", "Spiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ins Blaue flattert ohne Ziel.", "tokens": ["Ins", "Blau\u00b7e", "flat\u00b7tert", "oh\u00b7ne", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch tr\u00e4um' ich nicht von Ruhmeskr\u00e4nzen,", "tokens": ["Doch", "tr\u00e4um'", "ich", "nicht", "von", "Ruh\u00b7mes\u00b7kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Sternen mehr, die t\u00e4uschend gl\u00e4nzen;", "tokens": ["Von", "Ster\u00b7nen", "mehr", ",", "die", "t\u00e4u\u00b7schend", "gl\u00e4n\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "$,", "PRELS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den J\u00fcngling lockten solche H\u00f6hn;", "tokens": ["Den", "J\u00fcng\u00b7ling", "lock\u00b7ten", "sol\u00b7che", "H\u00f6hn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Alten deucht das N\u00e4chste sch\u00f6n.", "tokens": ["Dem", "Al\u00b7ten", "deucht", "das", "N\u00e4chs\u00b7te", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich h\u00f6r' im Forst den J\u00e4ger blasen,", "tokens": ["Ich", "h\u00f6r'", "im", "Forst", "den", "J\u00e4\u00b7ger", "bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich sehe, wie die Rinder grasen,", "tokens": ["Ich", "se\u00b7he", ",", "wie", "die", "Rin\u00b7der", "gra\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Storch durchs Ried hochbeinig stelzt,", "tokens": ["Der", "Storch", "durchs", "Ried", "hoch\u00b7bei\u00b7nig", "stelzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schimmernd sich das M\u00fchlrad w\u00e4lzt.", "tokens": ["Und", "schim\u00b7mernd", "sich", "das", "M\u00fchl\u00b7rad", "w\u00e4lzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Auch kommt mir bei der Wipfel Wogen", "tokens": ["Auch", "kommt", "mir", "bei", "der", "Wip\u00b7fel", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bisweilen noch ein Reim geflogen,", "tokens": ["Bis\u00b7wei\u00b7len", "noch", "ein", "Reim", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der, wie die Seele schweift und sinnt,", "tokens": ["Der", ",", "wie", "die", "See\u00b7le", "schweift", "und", "sinnt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Liede still sich weiter spinnt.", "tokens": ["Zum", "Lie\u00b7de", "still", "sich", "wei\u00b7ter", "spinnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch nur f\u00fcr mich. Im Marktgedr\u00e4nge", "tokens": ["Doch", "nur", "f\u00fcr", "mich", ".", "Im", "Markt\u00b7ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPER", "$.", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer horcht' auch auf die leisen Kl\u00e4nge?", "tokens": ["Wer", "horcht'", "auch", "auf", "die", "lei\u00b7sen", "Kl\u00e4n\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Bestes gab ich; g\u00f6nnt mir's nun,", "tokens": ["Mein", "Bes\u00b7tes", "gab", "ich", ";", "g\u00f6nnt", "mir's", "nun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$.", "VVFIN", "NE", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Gr\u00fcnen spielend auszuruhn.", "tokens": ["Im", "Gr\u00fc\u00b7nen", "spie\u00b7lend", "aus\u00b7zu\u00b7ruhn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}