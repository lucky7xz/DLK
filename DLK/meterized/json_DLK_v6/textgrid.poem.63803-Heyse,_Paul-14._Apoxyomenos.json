{"textgrid.poem.63803": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "14. Apoxyomenos", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So hat Mutter Natur in reingeschwungenem Gleichma\u00df", "tokens": ["So", "hat", "Mut\u00b7ter", "Na\u00b7tur", "in", "rein\u00b7ge\u00b7schwun\u00b7ge\u00b7nem", "Gleich\u00b7ma\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NN", "NN", "APPR", "ADJA", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Sich ihr Lieblingsgesch\u00f6pf, so sich den Menschen getr\u00e4umt,", "tokens": ["Sich", "ihr", "Lieb\u00b7lings\u00b7ge\u00b7sch\u00f6pf", ",", "so", "sich", "den", "Men\u00b7schen", "ge\u00b7tr\u00e4umt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "$,", "ADV", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Ehe der Vater, der Geist, mit dem \u00dcberma\u00df des Gedankens", "tokens": ["E\u00b7he", "der", "Va\u00b7ter", ",", "der", "Geist", ",", "mit", "dem", "\u00dc\u00b7berm\u00b7a\u00df", "des", "Ge\u00b7dan\u00b7kens"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "$,", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Herrisch von oben herab ihre Gebilde verpfuscht.", "tokens": ["Her\u00b7risch", "von", "o\u00b7ben", "her\u00b7ab", "ih\u00b7re", "Ge\u00b7bil\u00b7de", "ver\u00b7pfuscht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "---+--+-+-+--+", "measure": "iambic.penta.relaxed"}}}}}