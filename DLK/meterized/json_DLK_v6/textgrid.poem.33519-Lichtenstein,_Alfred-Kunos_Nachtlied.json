{"textgrid.poem.33519": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Kunos Nachtlied", "genre": "verse", "period": "N.A.", "pub_year": 1913, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "T\u00e4glich, wenn es so sehr dunkelt,", "tokens": ["T\u00e4g\u00b7lich", ",", "wenn", "es", "so", "sehr", "dun\u00b7kelt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich nicht mehr lesen kann,", "tokens": ["Da\u00df", "ich", "nicht", "mehr", "le\u00b7sen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lauf ich singend auf die Stra\u00dfe,", "tokens": ["Lauf", "ich", "sin\u00b7gend", "auf", "die", "Stra\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sehe jedes M\u00e4dchen an ...", "tokens": ["Se\u00b7he", "je\u00b7des", "M\u00e4d\u00b7chen", "an", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ob vielleicht \u2013 wer will das wissen \u2013", "tokens": ["Ob", "viel\u00b7leicht", "\u2013", "wer", "will", "das", "wis\u00b7sen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$(", "PWS", "VMFIN", "PDS", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gerade heut ein Wunder sei:", "tokens": ["Ge\u00b7ra\u00b7de", "heut", "ein", "Wun\u00b7der", "sei", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich als Erl\u00f6ster heimkehr,", "tokens": ["Da\u00df", "ich", "als", "Er\u00b7l\u00f6s\u00b7ter", "heim\u00b7kehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Friedlich und f\u00fcr ewig frei ...", "tokens": ["Fried\u00b7lich", "und", "f\u00fcr", "e\u00b7wig", "frei", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "APPR", "ADJD", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Komme ich vom vielen Suchen", "tokens": ["Kom\u00b7me", "ich", "vom", "vie\u00b7len", "Su\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00fcde und verwirrt nach Haus,", "tokens": ["M\u00fc\u00b7de", "und", "ver\u00b7wirrt", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wei\u00df ich ein geheimes Mittel,", "tokens": ["Wei\u00df", "ich", "ein", "ge\u00b7hei\u00b7mes", "Mit\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das l\u00f6scht alle Leiden aus \u2013", "tokens": ["Das", "l\u00f6scht", "al\u00b7le", "Lei\u00b7den", "aus", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "APPR", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}