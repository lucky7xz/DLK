{"dta.poem.11852": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer noch im Schlafe liegt, sei daraus aufgeweckt,", "tokens": ["Wer", "noch", "im", "Schla\u00b7fe", "liegt", ",", "sei", "da\u00b7raus", "auf\u00b7ge\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPRART", "NN", "VVFIN", "$,", "VAFIN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und liegt er fest darin, so sei er aufgeschreckt.", "tokens": ["Und", "liegt", "er", "fest", "da\u00b7rin", ",", "so", "sei", "er", "auf\u00b7ge\u00b7schreckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PAV", "$,", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer aber schon erwacht, doch noch nicht aufgebrochen", "tokens": ["Wer", "a\u00b7ber", "schon", "er\u00b7wacht", ",", "doch", "noch", "nicht", "auf\u00b7ge\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADV", "VVPP", "$,", "ADV", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zur Reise, sei dazu gespornt und angestochen.", "tokens": ["Zur", "Rei\u00b7se", ",", "sei", "da\u00b7zu", "ge\u00b7spornt", "und", "an\u00b7ge\u00b7sto\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VAFIN", "PAV", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wer vom Verlangen schon gespornt ist, doch den Weg", "tokens": ["Wer", "vom", "Ver\u00b7lan\u00b7gen", "schon", "ge\u00b7spornt", "ist", ",", "doch", "den", "Weg"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "APPRART", "NN", "ADV", "VVPP", "VAFIN", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dahin nicht kennt, dem sei gezeiget Weg und Steg.", "tokens": ["Da\u00b7hin", "nicht", "kennt", ",", "dem", "sei", "ge\u00b7zei\u00b7get", "Weg", "und", "Steg", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKNEG", "VVFIN", "$,", "PDS", "VAFIN", "VVPP", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wer aber kennt den Weg, und ihn nicht gehen mag,", "tokens": ["Wer", "a\u00b7ber", "kennt", "den", "Weg", ",", "und", "ihn", "nicht", "ge\u00b7hen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der hat auf immer nun vers\u00e4umt den Reisetag;", "tokens": ["Der", "hat", "auf", "im\u00b7mer", "nun", "ver\u00b7s\u00e4umt", "den", "Rei\u00b7se\u00b7tag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ADV", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und wieder leg' er sich zum Schlaf hin, wie er lag!", "tokens": ["Und", "wie\u00b7der", "leg'", "er", "sich", "zum", "Schlaf", "hin", ",", "wie", "er", "lag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "PTKVZ", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}