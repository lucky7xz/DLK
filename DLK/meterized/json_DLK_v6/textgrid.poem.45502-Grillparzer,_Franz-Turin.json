{"textgrid.poem.45502": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Turin", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo manch geschlagner General,", "tokens": ["Wo", "manch", "ge\u00b7schlag\u00b7ner", "Ge\u00b7ne\u00b7ral", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vermehrt nun noch der Schreier Zahl", "tokens": ["Ver\u00b7mehrt", "nun", "noch", "der", "Schrei\u00b7er", "Zahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein jungfr\u00e4ulicher Admiral.", "tokens": ["Ein", "jung\u00b7fr\u00e4u\u00b7li\u00b7cher", "Ad\u00b7mi\u00b7ral", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Sie waren oft im Wasser,", "tokens": ["Sie", "wa\u00b7ren", "oft", "im", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er nicht ein einzig Mal.", "tokens": ["Er", "nicht", "ein", "ein\u00b7zig", "Mal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}