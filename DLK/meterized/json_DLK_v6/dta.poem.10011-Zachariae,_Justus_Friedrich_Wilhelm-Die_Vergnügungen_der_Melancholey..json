{"dta.poem.10011": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Vergn\u00fcgungen  \n  der Melancholey.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20674-3", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df mich auch oft das erleuchtete Chor in der hei-", "tokens": ["La\u00df", "mich", "auch", "oft", "das", "er\u00b7leuch\u00b7te\u00b7te", "Chor", "in", "der", "hei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "APPR", "ART", "TRUNC"], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Gebets besuchen, wenn majest\u00e4tisch die Orgel", "tokens": ["Des", "Ge\u00b7bets", "be\u00b7su\u00b7chen", ",", "wenn", "ma\u00b7jes\u00b7t\u00e4\u00b7tisch", "die", "Or\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "KOUS", "ADJD", "ART", "NN"], "meter": "--+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In der Andacht Gesang von der H\u00f6h vielstimmig erschallet,", "tokens": ["In", "der", "An\u00b7dacht", "Ge\u00b7sang", "von", "der", "H\u00f6h", "viel\u00b7stim\u00b7mig", "er\u00b7schal\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "APPR", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Bis die Seele sich ausser sich rei\u00dft, und zum Himmel", "tokens": ["Bis", "die", "See\u00b7le", "sich", "aus\u00b7ser", "sich", "rei\u00dft", ",", "und", "zum", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PRF", "APPR", "PRF", "VVFIN", "$,", "KON", "APPRART", "NN"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "La\u00df mich auch oft im inneren Dom, im einsamen Stuhle,", "tokens": ["La\u00df", "mich", "auch", "oft", "im", "in\u00b7ne\u00b7ren", "Dom", ",", "im", "ein\u00b7sa\u00b7men", "Stuh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "APPRART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Heilige T\u00f6ne vernehmen, die feyerlichlangsam und", "tokens": ["Hei\u00b7li\u00b7ge", "T\u00f6\u00b7ne", "ver\u00b7neh\u00b7men", ",", "die", "fey\u00b7er\u00b7lich\u00b7lang\u00b7sam", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "VVINF", "$,", "PRELS", "ADJD", "KON"], "meter": "+--+--+--+--+-+", "measure": "diphilius"}, "line.7": {"text": "Durch die gothschen Gew\u00f6lbe sich winden, und in der", "tokens": ["Durch", "die", "goth\u00b7schen", "Ge\u00b7w\u00f6l\u00b7be", "sich", "win\u00b7den", ",", "und", "in", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PRF", "VVINF", "$,", "KON", "APPR", "ART"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Mein hinhorchendes Ohr mit hohem Gemurmel erreichen.", "tokens": ["Mein", "hin\u00b7hor\u00b7chen\u00b7des", "Ohr", "mit", "ho\u00b7hem", "Ge\u00b7mur\u00b7mel", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "La\u00df mich auch dann nicht zu bleiben vergessen, wenn ietzo", "tokens": ["La\u00df", "mich", "auch", "dann", "nicht", "zu", "blei\u00b7ben", "ver\u00b7ges\u00b7sen", ",", "wenn", "iet\u00b7zo"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "PTKNEG", "PTKZU", "VVINF", "VVPP", "$,", "KOUS", "ADV"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Unter den Schatten verl\u00f6scht, und einsame Stille zu-", "tokens": ["Un\u00b7ter", "den", "Schat\u00b7ten", "ver\u00b7l\u00f6scht", ",", "und", "ein\u00b7sa\u00b7me", "Stil\u00b7le", "zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "KON", "ADJA", "NN", "TRUNC"], "meter": "+--+--+-+--+-+", "measure": "dactylic.di.plus"}, "line.11": {"text": "La\u00df mich alsdann die schreckenden Schl\u00e4ge der Glocke", "tokens": ["La\u00df", "mich", "als\u00b7dann", "die", "schre\u00b7cken\u00b7den", "Schl\u00e4\u00b7ge", "der", "Glo\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ART", "ADJA", "NN", "ART", "NN"], "meter": "---+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Welche mit zitternder Zunge die fliehenden Stunden", "tokens": ["Wel\u00b7che", "mit", "zit\u00b7tern\u00b7der", "Zun\u00b7ge", "die", "flie\u00b7hen\u00b7den", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAT", "APPR", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}}}}}