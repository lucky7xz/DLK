{"textgrid.poem.33952": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das alte Haus", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Maurer schreitet frisch heraus,", "tokens": ["Der", "Mau\u00b7rer", "schrei\u00b7tet", "frisch", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er soll dich niederbrechen;", "tokens": ["Er", "soll", "dich", "nie\u00b7der\u00b7bre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da ist es mir, du altes Haus,", "tokens": ["Da", "ist", "es", "mir", ",", "du", "al\u00b7tes", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als h\u00f6rte ich dich sprechen:", "tokens": ["Als", "h\u00f6r\u00b7te", "ich", "dich", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbwie magst du mich, das lange Jahr'", "tokens": ["\u00bb", "wie", "magst", "du", "mich", ",", "das", "lan\u00b7ge", "Jahr'"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PWAV", "VMFIN", "PPER", "PRF", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Lieb' und Eintracht Tempel war,", "tokens": ["Der", "Lieb'", "und", "Ein\u00b7tracht", "Tem\u00b7pel", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie magst du mich zerst\u00f6ren?", "tokens": ["Wie", "magst", "du", "mich", "zer\u00b7st\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dein Ahnherr hat mich einst erbaut", "tokens": ["Dein", "Ahn\u00b7herr", "hat", "mich", "einst", "er\u00b7baut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und unter frommem Beten", "tokens": ["Und", "un\u00b7ter", "from\u00b7mem", "Be\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit seiner sch\u00f6nen, stillen Braut", "tokens": ["Mit", "sei\u00b7ner", "sch\u00f6\u00b7nen", ",", "stil\u00b7len", "Braut"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich dann zuerst betreten.", "tokens": ["Mich", "dann", "zu\u00b7erst", "be\u00b7tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich wei\u00df um Alles wohl Bescheid,", "tokens": ["Ich", "wei\u00df", "um", "Al\u00b7les", "wohl", "Be\u00b7scheid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIS", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Um jede Luft, um jedes Leid,", "tokens": ["Um", "je\u00b7de", "Luft", ",", "um", "je\u00b7des", "Leid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "NN", "$,", "KOUI", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was ihnen widerfahren.", "tokens": ["Was", "ih\u00b7nen", "wi\u00b7der\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dein Vater ward geboren hier,", "tokens": ["Dein", "Va\u00b7ter", "ward", "ge\u00b7bo\u00b7ren", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In der gebr\u00e4unten Stube,", "tokens": ["In", "der", "ge\u00b7br\u00e4un\u00b7ten", "Stu\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die ersten Blicke gab er mir,", "tokens": ["Die", "ers\u00b7ten", "Bli\u00b7cke", "gab", "er", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der munt're, kr\u00e4ft'ge Bube.", "tokens": ["Der", "munt'\u00b7re", ",", "kr\u00e4ft'\u00b7ge", "Bu\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er schaute auf die Engelein,", "tokens": ["Er", "schau\u00b7te", "auf", "die", "En\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die gaukeln in der Fenster Schein,", "tokens": ["Die", "gau\u00b7keln", "in", "der", "Fens\u00b7ter", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dann erst auf seine Mutter.", "tokens": ["Dann", "erst", "auf", "sei\u00b7ne", "Mut\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und als er traurig schlich am Stab", "tokens": ["Und", "als", "er", "trau\u00b7rig", "schlich", "am", "Stab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach manchen sch\u00f6nen Jahren,", "tokens": ["Nach", "man\u00b7chen", "sch\u00f6\u00b7nen", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da hat er schon, wie still ein Grab,", "tokens": ["Da", "hat", "er", "schon", ",", "wie", "still", "ein", "Grab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "PWAV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In meinem Schoo\u00df erfahren;", "tokens": ["In", "mei\u00b7nem", "Schoo\u00df", "er\u00b7fah\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In jener Ecke sa\u00df er da,", "tokens": ["In", "je\u00b7ner", "E\u00b7cke", "sa\u00df", "er", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und stumm und h\u00e4ndefaltend sah", "tokens": ["Und", "stumm", "und", "h\u00e4n\u00b7de\u00b7fal\u00b7tend", "sah"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er sehnlich auf zum Himmel.", "tokens": ["Er", "sehn\u00b7lich", "auf", "zum", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du selbst \u2013 doch nein, das sag' ich nicht,", "tokens": ["Du", "selbst", "\u2013", "doch", "nein", ",", "das", "sag'", "ich", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$(", "ADV", "PTKANT", "$,", "PDS", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich will von dir nicht sprechen,", "tokens": ["Ich", "will", "von", "dir", "nicht", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hat dieses Alles kein Gewicht,", "tokens": ["Hat", "die\u00b7ses", "Al\u00b7les", "kein", "Ge\u00b7wicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "PIS", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So la\u00df nur immer brechen.", "tokens": ["So", "la\u00df", "nur", "im\u00b7mer", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Gl\u00fcck zog mit dem Ahnherrn ein,", "tokens": ["Das", "Gl\u00fcck", "zog", "mit", "dem", "Ahn\u00b7herrn", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zerst\u00f6re du den Tempel sein,", "tokens": ["Zer\u00b7st\u00f6\u00b7re", "du", "den", "Tem\u00b7pel", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Damit es endlich weiche.", "tokens": ["Da\u00b7mit", "es", "end\u00b7lich", "wei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Noch lange Jahre kann ich steh'n,", "tokens": ["Noch", "lan\u00b7ge", "Jah\u00b7re", "kann", "ich", "steh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bin fest genug gegr\u00fcndet,", "tokens": ["Bin", "fest", "ge\u00b7nug", "ge\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ob sich mit der St\u00fcrme Weh'n", "tokens": ["Und", "ob", "sich", "mit", "der", "St\u00fcr\u00b7me", "Weh'n"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PRF", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Wolkenbruch verb\u00fcndet;", "tokens": ["Ein", "Wol\u00b7ken\u00b7bruch", "ver\u00b7b\u00fcn\u00b7det", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "K\u00fchn rag' ich, wie ein Fels, empor,", "tokens": ["K\u00fchn", "rag'", "ich", ",", "wie", "ein", "Fels", ",", "em\u00b7por", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und was ich auch an Schmuck verlor,", "tokens": ["Und", "was", "ich", "auch", "an", "Schmuck", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gewann ich's nicht an W\u00fcrde?", "tokens": ["Ge\u00b7wann", "ich's", "nicht", "an", "W\u00fcr\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und hab' ich denn nicht manchen Saal", "tokens": ["Und", "hab'", "ich", "denn", "nicht", "man\u00b7chen", "Saal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PTKNEG", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und manch ger\u00e4umig Zimmer?", "tokens": ["Und", "manch", "ge\u00b7r\u00e4u\u00b7mig", "Zim\u00b7mer", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und gl\u00e4nzt nicht festlich mein Portal", "tokens": ["Und", "gl\u00e4nzt", "nicht", "fest\u00b7lich", "mein", "Por\u00b7tal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In alter Pracht noch immer?", "tokens": ["In", "al\u00b7ter", "Pracht", "noch", "im\u00b7mer", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "-+-+-++", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Noch Jedem hat's in mir behagt,", "tokens": ["Noch", "Je\u00b7dem", "hat's", "in", "mir", "be\u00b7hagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "VAFIN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kein Gl\u00fccklicher hat sich beklagt,", "tokens": ["Kein", "Gl\u00fcck\u00b7li\u00b7cher", "hat", "sich", "be\u00b7klagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PRF", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Ich sei zu klein gewesen.", "tokens": ["Ich", "sei", "zu", "klein", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und, wenn es einst zum Letzten geht,", "tokens": ["Und", ",", "wenn", "es", "einst", "zum", "Letz\u00b7ten", "geht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wenn das warme Leben", "tokens": ["Und", "wenn", "das", "war\u00b7me", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In deinen Adern stille steht,", "tokens": ["In", "dei\u00b7nen", "A\u00b7dern", "stil\u00b7le", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird die\u00df dich nicht erheben,", "tokens": ["Wird", "die\u00df", "dich", "nicht", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dort, wo dein Vater sterbend lag,", "tokens": ["Dort", ",", "wo", "dein", "Va\u00b7ter", "ster\u00b7bend", "lag", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo deiner Mutter Auge brach,", "tokens": ["Wo", "dei\u00b7ner", "Mut\u00b7ter", "Au\u00b7ge", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den letzten Kampf zu streiten?\u00ab", "tokens": ["Den", "letz\u00b7ten", "Kampf", "zu", "strei\u00b7ten", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Nun schweigt es still, das alte Haus,", "tokens": ["Nun", "schweigt", "es", "still", ",", "das", "al\u00b7te", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir aber ist's, als schritten", "tokens": ["Mir", "a\u00b7ber", "ist's", ",", "als", "schrit\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "$,", "KOUS", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die todten V\u00e4ter all' heraus,", "tokens": ["Die", "tod\u00b7ten", "V\u00e4\u00b7ter", "all'", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um f\u00fcr ihr Haus zu bitten,", "tokens": ["Um", "f\u00fcr", "ihr", "Haus", "zu", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und auch in meiner eig'nen Brust,", "tokens": ["Und", "auch", "in", "mei\u00b7ner", "eig'\u00b7nen", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie ruft so manche Kinder-Lust:", "tokens": ["Wie", "ruft", "so", "man\u00b7che", "Kin\u00b7der\u00b7Lust", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "La\u00df steh'n das Haus, la\u00df stehen!", "tokens": ["La\u00df", "steh'n", "das", "Haus", ",", "la\u00df", "ste\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "ART", "NN", "$,", "VVIMP", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Indessen ist der Mauermann", "tokens": ["In\u00b7des\u00b7sen", "ist", "der", "Mau\u00b7er\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schon in's Geb\u00e4lk gestiegen,", "tokens": ["Schon", "in's", "Ge\u00b7b\u00e4lk", "ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er f\u00e4ngt mit Macht zu brechen an,", "tokens": ["Er", "f\u00e4ngt", "mit", "Macht", "zu", "bre\u00b7chen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PTKZU", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Stein' und Ziegel fliegen.", "tokens": ["Und", "Stein'", "und", "Zie\u00b7gel", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Still, lieber Meister, geh von hier,", "tokens": ["Still", ",", "lie\u00b7ber", "Meis\u00b7ter", ",", "geh", "von", "hier", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "NN", "$,", "VVFIN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gern zahle ich den Taglohn dir,", "tokens": ["Gern", "zah\u00b7le", "ich", "den", "Tag\u00b7lohn", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Allein das Haus bleibt stehen.", "tokens": ["Al\u00b7lein", "das", "Haus", "bleibt", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "