{"textgrid.poem.40391": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Ein Seemann", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kann ein Seemann werden,", "tokens": ["Ich", "kann", "ein", "See\u00b7mann", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kapit\u00e4n oder Steuermann.", "tokens": ["Ka\u00b7pi\u00b7t\u00e4n", "o\u00b7der", "Steu\u00b7er\u00b7mann", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Den macht sein Steuerrad so stark,", "tokens": ["Den", "macht", "sein", "Steu\u00b7er\u00b7rad", "so", "stark", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wie der Pflug den Ackersmann;", "tokens": ["wie", "der", "Pflug", "den", "A\u00b7ckers\u00b7mann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "kommt nur, ihr Wolken und Wellen!", "tokens": ["kommt", "nur", ",", "ihr", "Wol\u00b7ken", "und", "Wel\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Der Wind pfl\u00fcgt tausend Furchen", "tokens": ["Der", "Wind", "pfl\u00fcgt", "tau\u00b7send", "Fur\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "von einem zum andern Strand.", "tokens": ["von", "ei\u00b7nem", "zum", "an\u00b7dern", "Strand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nur Eine Furche pfl\u00fcgt mein Schiff:", "tokens": ["Nur", "Ei\u00b7ne", "Fur\u00b7che", "pfl\u00fcgt", "mein", "Schiff", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "die bricht unserm Vaterland", "tokens": ["die", "bricht", "un\u00b7serm", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "nach allen Erdteilen Bahn.", "tokens": ["nach", "al\u00b7len", "Erd\u00b7tei\u00b7len", "Bahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Ob noch so undurchdringlich", "tokens": ["Ob", "noch", "so", "un\u00b7durch\u00b7dring\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "ringsum der Nebel graut,", "tokens": ["ring\u00b7sum", "der", "Ne\u00b7bel", "graut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "da\u00df selbst die Sonne durch den Dunst", "tokens": ["da\u00df", "selbst", "die", "Son\u00b7ne", "durch", "den", "Dunst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wie'n blindes Auge schaut:", "tokens": ["wie'n", "blin\u00b7des", "Au\u00b7ge", "schaut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "unser Kompa\u00df kennt den Weg.", "tokens": ["un\u00b7ser", "Kom\u00b7pa\u00df", "kennt", "den", "Weg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn wir die Flagge hissen,", "tokens": ["Wenn", "wir", "die", "Flag\u00b7ge", "his\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "du fremde Hafenstadt,", "tokens": ["du", "frem\u00b7de", "Ha\u00b7fen\u00b7stadt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "soll jeder Matrose wissen,", "tokens": ["soll", "je\u00b7der", "Mat\u00b7ro\u00b7se", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "der Ehre im Leibe hat:", "tokens": ["der", "Eh\u00b7re", "im", "Lei\u00b7be", "hat", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "dir naht ein kleiner Held!", "tokens": ["dir", "naht", "ein", "klei\u00b7ner", "Held", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}