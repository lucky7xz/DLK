{"textgrid.poem.66774": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tief getaucht in Sonnengluten", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tief getaucht in Sonnengluten", "tokens": ["Tief", "ge\u00b7taucht", "in", "Son\u00b7nen\u00b7glu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ragt des Berges Haupt empor,", "tokens": ["Ragt", "des", "Ber\u00b7ges", "Haupt", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lichtgewirkte Schleier fluten,", "tokens": ["Licht\u00b7ge\u00b7wirk\u00b7te", "Schlei\u00b7er", "flu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Niederwallt der Silberflor.", "tokens": ["Nie\u00b7der\u00b7wallt", "der", "Sil\u00b7ber\u00b7flor", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hoch am Nacken seh ich schwellen", "tokens": ["Hoch", "am", "Na\u00b7cken", "seh", "ich", "schwel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00fc\u00dfer Trauben Perlenreihn,", "tokens": ["S\u00fc\u00b7\u00dfer", "Trau\u00b7ben", "Per\u00b7len\u00b7reihn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus den Br\u00fcsten seh ich quellen", "tokens": ["Aus", "den", "Br\u00fcs\u00b7ten", "seh", "ich", "quel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Milden, kraftgezeugten Wein.", "tokens": ["Mil\u00b7den", ",", "kraft\u00b7ge\u00b7zeug\u00b7ten", "Wein", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "B\u00e4ume schatten fr\u00fcchteprangend,", "tokens": ["B\u00e4u\u00b7me", "schat\u00b7ten", "fr\u00fcch\u00b7te\u00b7pran\u00b7gend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vollbelastet deinen Fu\u00df,", "tokens": ["Voll\u00b7be\u00b7las\u00b7tet", "dei\u00b7nen", "Fu\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Frohen Blickes dich umfangend", "tokens": ["Fro\u00b7hen", "Bli\u00b7ckes", "dich", "um\u00b7fan\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Biet' ich dir den Morgengru\u00df.", "tokens": ["Biet'", "ich", "dir", "den", "Mor\u00b7gen\u00b7gru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sei gegr\u00fc\u00dft, vom Morgenstrahle", "tokens": ["Sei", "ge\u00b7gr\u00fc\u00dft", ",", "vom", "Mor\u00b7gen\u00b7strah\u00b7le"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VAFIN", "VVPP", "$,", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Glanzumwobnes Vaterland!", "tokens": ["Glan\u00b7zum\u00b7wob\u00b7nes", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Leuchtest auf mit einem Male,", "tokens": ["Leuch\u00b7test", "auf", "mit", "ei\u00b7nem", "Ma\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seit der D\u00e4mmrung Schatten schwand.", "tokens": ["Seit", "der", "D\u00e4mm\u00b7rung", "Schat\u00b7ten", "schwand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "M\u00e4chtig w\u00e4chst dein Haupt, erhoben", "tokens": ["M\u00e4ch\u00b7tig", "w\u00e4chst", "dein", "Haupt", ",", "er\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$,", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu des Lichtes Wunderquell,", "tokens": ["Zu", "des", "Lich\u00b7tes", "Wun\u00b7der\u00b7quell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Reicher Segen str\u00f6mt von oben,", "tokens": ["Rei\u00b7cher", "Se\u00b7gen", "str\u00f6mt", "von", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Fr\u00fcchte reifen schnell.", "tokens": ["Und", "die", "Fr\u00fcch\u00b7te", "rei\u00b7fen", "schnell", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie der Seele bittres Leiden", "tokens": ["Wie", "der", "See\u00b7le", "bitt\u00b7res", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine Herrlichkeit vers\u00fc\u00dft!", "tokens": ["Dei\u00b7ne", "Herr\u00b7lich\u00b7keit", "ver\u00b7s\u00fc\u00dft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zukunftsahnend la\u00dft mich scheiden \u2013", "tokens": ["Zu\u00b7kunft\u00b7sah\u00b7nend", "la\u00dft", "mich", "schei\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVIMP", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deutschland, Mutter, sei gegr\u00fc\u00dft!", "tokens": ["Deutschland", ",", "Mut\u00b7ter", ",", "sei", "ge\u00b7gr\u00fc\u00dft", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}