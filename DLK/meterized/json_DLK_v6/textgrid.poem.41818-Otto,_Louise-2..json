{"textgrid.poem.41818": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin bereit! \u2013 wohl war es sch\u00f6n hienieden,", "tokens": ["Ich", "bin", "be\u00b7reit", "!", "\u2013", "wohl", "war", "es", "sch\u00f6n", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$.", "$(", "ADV", "VAFIN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als mir bei Veilchenduft und Lerchenschlag,", "tokens": ["Als", "mir", "bei", "Veil\u00b7chen\u00b7duft", "und", "Ler\u00b7chen\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "An manchen sonnenhaften Fr\u00fchlingstag", "tokens": ["An", "man\u00b7chen", "son\u00b7nen\u00b7haf\u00b7ten", "Fr\u00fch\u00b7lings\u00b7tag"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein innig Lied zu singen selbst beschieden.", "tokens": ["Ein", "in\u00b7nig", "Lied", "zu", "sin\u00b7gen", "selbst", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PTKZU", "VVFIN", "ADV", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Und sch\u00f6n auch, wenn nach St\u00fcrmen, nie gemieden,", "tokens": ["Und", "sch\u00f6n", "auch", ",", "wenn", "nach", "St\u00fcr\u00b7men", ",", "nie", "ge\u00b7mie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$,", "KOUS", "APPR", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach einem Wetter, das in Blitzen sprach,", "tokens": ["Nach", "ei\u00b7nem", "Wet\u00b7ter", ",", "das", "in", "Blit\u00b7zen", "sprach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hervor ein bunter Regenbogen brach,", "tokens": ["Her\u00b7vor", "ein", "bun\u00b7ter", "Re\u00b7gen\u00b7bo\u00b7gen", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verk\u00fcndend einen neuen Sabbatfrieden.", "tokens": ["Ver\u00b7k\u00fcn\u00b7dend", "ei\u00b7nen", "neu\u00b7en", "Sab\u00b7bat\u00b7frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6nwar's, wenn bei des Mondes mildem Leuchten,", "tokens": ["Sch\u00f6n\u00b7wa\u00b7r's", ",", "wenn", "bei", "des", "Mon\u00b7des", "mil\u00b7dem", "Leuch\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Und bei der Nachtigallen s\u00fc\u00dfem Sang,", "tokens": ["Und", "bei", "der", "Nach\u00b7ti\u00b7gal\u00b7len", "s\u00fc\u00b7\u00dfem", "Sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In tiefer Sehnsucht sich die Augen feuchten.", "tokens": ["In", "tie\u00b7fer", "Sehn\u00b7sucht", "sich", "die", "Au\u00b7gen", "feuch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und sch\u00f6ner, wenn in der Begeistrung Drang", "tokens": ["Und", "sch\u00f6\u00b7ner", ",", "wenn", "in", "der", "Be\u00b7geis\u00b7trung", "Drang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sich Herz und Seel' dem Ew'gen nahe deuchten \u2013", "tokens": ["Sich", "Herz", "und", "Seel'", "dem", "Ew'\u00b7gen", "na\u00b7he", "deuch\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "KON", "NN", "ART", "NN", "APPR", "ADJA", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch \u2013 wenn dies sterbend nun erst ganz gelang?", "tokens": ["Doch", "\u2013", "wenn", "dies", "ster\u00b7bend", "nun", "erst", "ganz", "ge\u00b7lang", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "KOUS", "PDS", "ADJD", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}