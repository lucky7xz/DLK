{"textgrid.poem.37672": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Herr da wird verkommen,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Herr da wird verkommen,", "tokens": ["Der", "Herr", "da", "wird", "ver\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er blo\u00dfen M\u00fc\u00dfiggang treibt,", "tokens": ["Er", "blo\u00b7\u00dfen", "M\u00fc\u00b7\u00dfig\u00b7gang", "treibt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hat nichts sich vorgenommen,", "tokens": ["Hat", "nichts", "sich", "vor\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht weibt.", "tokens": ["Nicht", "weibt", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Er sitzt auf seinem Hofe", "tokens": ["Er", "sitzt", "auf", "sei\u00b7nem", "Ho\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den ausgeschlagnen Tag,", "tokens": ["Den", "aus\u00b7ge\u00b7schlag\u00b7nen", "Tag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sieht nicht, was er bei Hofe", "tokens": ["Sieht", "nicht", ",", "was", "er", "bei", "Ho\u00b7fe"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$,", "PWS", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vermag.", "tokens": ["Ver\u00b7mag", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Er steigt nicht gerne Stufen,", "tokens": ["Er", "steigt", "nicht", "ger\u00b7ne", "Stu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hoffahrt will haben den Zwang;", "tokens": ["Hof\u00b7fahrt", "will", "ha\u00b7ben", "den", "Zwang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VMFIN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wer braucht ihn? der wird rufen:", "tokens": ["Wer", "braucht", "ihn", "?", "der", "wird", "ru\u00b7fen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "ART", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht lang.", "tokens": ["Nicht", "lang", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.4": {"line.1": {"text": "Er wartet auf ein Rufen", "tokens": ["Er", "war\u00b7tet", "auf", "ein", "Ru\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus seiner innern Brust,", "tokens": ["Aus", "sei\u00b7ner", "in\u00b7nern", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was Sch\u00f6nes je wir schufen,", "tokens": ["Was", "Sch\u00f6\u00b7nes", "je", "wir", "schu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schafft Lust.", "tokens": ["Schafft", "Lust", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.5": {"line.1": {"text": "Ihm tr\u00e4umt einst Morgens fr\u00fche", "tokens": ["Ihm", "tr\u00e4umt", "einst", "Mor\u00b7gens", "fr\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er sehe von dem Altan,", "tokens": ["Er", "se\u00b7he", "von", "dem", "Al\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Als trieb ein Hirsch so gl\u00fche", "tokens": ["Als", "trieb", "ein", "Hirsch", "so", "gl\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "ART", "NN", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bergan.", "tokens": ["Ber\u00b7gan", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.6": {"line.1": {"text": "Geweih, die sind verguldet,", "tokens": ["Ge\u00b7weih", ",", "die", "sind", "ver\u00b7gul\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es zieht zum Schlo\u00df ihn so nach,", "tokens": ["Es", "zieht", "zum", "Schlo\u00df", "ihn", "so", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPER", "ADV", "APPR", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Zur F\u00fcrstin unverschuldet! \u2013", "tokens": ["Zur", "F\u00fcrs\u00b7tin", "un\u00b7ver\u00b7schul\u00b7det", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wird wach!", "tokens": ["Wird", "wach", "!"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.7": {"line.1": {"text": "Am Brunnen er sie sahe,", "tokens": ["Am", "Brun\u00b7nen", "er", "sie", "sa\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ganz trocken war ihm sein Mund,", "tokens": ["Ganz", "tro\u00b7cken", "war", "ihm", "sein", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sprang hin zum Fenster nahe", "tokens": ["Sprang", "hin", "zum", "Fens\u00b7ter", "na\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zur Stund.", "tokens": ["Zur", "Stund", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.8": {"line.1": {"text": "Da sieht er, sieht er eilen", "tokens": ["Da", "sieht", "er", ",", "sieht", "er", "ei\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Hirsch mit g\u00fcldnem Geweih,", "tokens": ["Den", "Hirsch", "mit", "g\u00fcld\u00b7nem", "Ge\u00b7weih", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Er springt ihn zu ereilen", "tokens": ["Er", "springt", "ihn", "zu", "er\u00b7ei\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Herbei.", "tokens": ["Her\u00b7bei", "."], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.9": {"line.1": {"text": "Der Hirsch zieht hin zum Garten", "tokens": ["Der", "Hirsch", "zieht", "hin", "zum", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der F\u00fcrstin droben im Land,", "tokens": ["Der", "F\u00fcrs\u00b7tin", "dro\u00b7ben", "im", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dem J\u00e4ger, dem erstarrten,", "tokens": ["Dem", "J\u00e4\u00b7ger", ",", "dem", "er\u00b7starr\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verschwand.", "tokens": ["Ver\u00b7schwand", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.10": {"line.1": {"text": "Er sieht die F\u00fcrstin stehen", "tokens": ["Er", "sieht", "die", "F\u00fcrs\u00b7tin", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am Bronn wie dorten im Traum,", "tokens": ["Am", "Bronn", "wie", "dor\u00b7ten", "im", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KOKOM", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Noch matt von Kindes-Wehen,", "tokens": ["Noch", "matt", "von", "Kin\u00b7des\u00b7We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Glaubt's kaum.", "tokens": ["Glaubt's", "kaum", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "ADV", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.11": {"line.1": {"text": "Sie w\u00e4scht ihr Kindlein kleine", "tokens": ["Sie", "w\u00e4scht", "ihr", "Kin\u00b7dlein", "klei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am kalten Bronnen, da\u00df es schreit,", "tokens": ["Am", "kal\u00b7ten", "Bron\u00b7nen", ",", "da\u00df", "es", "schreit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Graf steht wie von Steine", "tokens": ["Der", "Graf", "steht", "wie", "von", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht weit.", "tokens": ["Nicht", "weit", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.12": {"line.1": {"text": "Ihr Haar wie Strahlen streuet", "tokens": ["Ihr", "Haar", "wie", "Strah\u00b7len", "streu\u00b7et"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KOKOM", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Wind, die Kron' da liegt,", "tokens": ["Der", "Wind", ",", "die", "Kron'", "da", "liegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Kindlein, das sich freuet,", "tokens": ["Das", "Kin\u00b7dlein", ",", "das", "sich", "freu\u00b7et", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Drein wiegt.", "tokens": ["Drein", "wiegt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.13": {"line.1": {"text": "\u00bbach Kindlein, du mu\u00dft sterben,\u00ab", "tokens": ["\u00bb", "ach", "Kin\u00b7dlein", ",", "du", "mu\u00dft", "ster\u00b7ben", ",", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "XY", "NN", "$,", "PPER", "VMFIN", "VVINF", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die F\u00fcrstin weinend da sagt,", "tokens": ["Die", "F\u00fcrs\u00b7tin", "wei\u00b7nend", "da", "sagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "ADV", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "\u00bbmein alter Mann sich Erben", "tokens": ["\u00bb", "mein", "al\u00b7ter", "Mann", "sich", "Er\u00b7ben"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "PRF", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Versagt.\u00ab", "tokens": ["Ver\u00b7sagt", ".", "\u00ab"], "token_info": ["word", "punct", "punct"], "pos": ["VVPP", "$.", "$("], "meter": "-+", "measure": "iambic.single"}}, "stanza.14": {"line.1": {"text": "Der Herr beugt seine Kniee,", "tokens": ["Der", "Herr", "beugt", "sei\u00b7ne", "Kni\u00b7ee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nimmt still das Kindlein auf:", "tokens": ["Nimmt", "still", "das", "Kin\u00b7dlein", "auf", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nimm auch die Krone, fliehe", "tokens": ["Nimm", "auch", "die", "Kro\u00b7ne", ",", "flie\u00b7he"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NE", "ADV", "ART", "NN", "$,", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Berg auf.", "tokens": ["Berg", "auf", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.15": {"line.1": {"text": "Er zieht das Kind auf Bergen,", "tokens": ["Er", "zieht", "das", "Kind", "auf", "Ber\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nach Hause darf er nun nicht,", "tokens": ["Nach", "Hau\u00b7se", "darf", "er", "nun", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "ADV", "PTKNEG", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Im Wald kann er's verbergen,", "tokens": ["Im", "Wald", "kann", "er's", "ver\u00b7ber\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da nicht.", "tokens": ["Da", "nicht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKNEG", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.16": {"line.1": {"text": "Doch als die Glocken schallen,", "tokens": ["Doch", "als", "die", "Glo\u00b7cken", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der F\u00fcrst gestorben da ist,", "tokens": ["Der", "F\u00fcrst", "ge\u00b7stor\u00b7ben", "da", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "ADV", "VAFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Zeigt Kron' und Kind er allen", "tokens": ["Zeigt", "Kron'", "und", "Kind", "er", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN", "PPER", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit List.", "tokens": ["Mit", "List", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.17": {"line.1": {"text": "Die F\u00fcrstin hoch ihn preiset,", "tokens": ["Die", "F\u00fcrs\u00b7tin", "hoch", "ihn", "prei\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie kr\u00f6nt ihr Kindelein zart.", "tokens": ["Sie", "kr\u00f6nt", "ihr", "Kin\u00b7de\u00b7lein", "zart", "."], "token_info": ["word", "word", "word",