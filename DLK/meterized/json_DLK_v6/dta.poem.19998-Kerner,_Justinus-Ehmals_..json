{"dta.poem.19998": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Ehmals .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl hab' ich manches Lied erdacht", "tokens": ["Wohl", "hab'", "ich", "man\u00b7ches", "Lied", "er\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Waldes D\u00e4mmerungen,", "tokens": ["In", "Wal\u00b7des", "D\u00e4m\u00b7me\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die V\u00f6gel haben's mitgemacht,", "tokens": ["Die", "V\u00f6\u00b7gel", "ha\u00b7ben's", "mit\u00b7ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Bach hat drein geklungen,", "tokens": ["Der", "Bach", "hat", "drein", "ge\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den laugen Weg, die fels'ge Bahn,", "tokens": ["Den", "lau\u00b7gen", "Weg", ",", "die", "fels'\u00b7ge", "Bahn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gieng ich ein seel'ger Wandersmann.", "tokens": ["Gieng", "ich", "ein", "seel'\u00b7ger", "Wan\u00b7ders\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun aber es mir nicht mehr gl\u00fcckt,", "tokens": ["Nun", "a\u00b7ber", "es", "mir", "nicht", "mehr", "gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPER", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch Bach und V\u00f6gel singen,", "tokens": ["Noch", "Bach", "und", "V\u00f6\u00b7gel", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich gehe traurend und geb\u00fcckt,", "tokens": ["Ich", "ge\u00b7he", "trau\u00b7rend", "und", "ge\u00b7b\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tr\u00e4um' von verlornen Dingen,", "tokens": ["Tr\u00e4um'", "von", "ver\u00b7lor\u00b7nen", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den langen Weg, die fels'ge Bahn", "tokens": ["Den", "lan\u00b7gen", "Weg", ",", "die", "fels'\u00b7ge", "Bahn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sieht man mir im Gesichte an.", "tokens": ["Sieht", "man", "mir", "im", "Ge\u00b7sich\u00b7te", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O armer Sohn der Arzeney!", "tokens": ["O", "ar\u00b7mer", "Sohn", "der", "Ar\u00b7ze\u00b7ney", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bist selbst erkrankt im Herzen.", "tokens": ["Bist", "selbst", "er\u00b7krankt", "im", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kennst der Heilkr\u00e4uter mancherley,", "tokens": ["Kennst", "der", "Heil\u00b7kr\u00e4u\u00b7ter", "man\u00b7cher\u00b7ley", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PIS", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Such' eins f\u00fcr eigne Schmerzen!", "tokens": ["Such'", "eins", "f\u00fcr", "eig\u00b7ne", "Schmer\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Welt, da\u00df ich's finde, la\u00df mich los!", "tokens": ["Welt", ",", "da\u00df", "ich's", "fin\u00b7de", ",", "la\u00df", "mich", "los", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PIS", "VVFIN", "$,", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Mich heilt nur meines Grabes Moos.", "tokens": ["Mich", "heilt", "nur", "mei\u00b7nes", "Gra\u00b7bes", "Moos", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}