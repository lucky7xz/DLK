{"textgrid.poem.42137": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Pennal", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "L\u00e4nger kann mein Herz ich nicht bez\u00e4hmen \u2013", "tokens": ["L\u00e4n\u00b7ger", "kann", "mein", "Herz", "ich", "nicht", "be\u00b7z\u00e4h\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "NN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ach du lieber Gott, ich tat es nie! \u2013", "tokens": ["Ach", "du", "lie\u00b7ber", "Gott", ",", "ich", "tat", "es", "nie", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "PPER", "ADV", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Doch Sie d\u00fcrfen es nicht \u00fcbelnehmen,", "tokens": ["Doch", "Sie", "d\u00fcr\u00b7fen", "es", "nicht", "\u00fc\u00b7bel\u00b7neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Aber ich gesteh's, ich liebe Sie.", "tokens": ["A\u00b7ber", "ich", "ge\u00b7steh's", ",", "ich", "lie\u00b7be", "Sie", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NE", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und wenn ich Sie auf der Stra\u00dfe sehe,", "tokens": ["Und", "wenn", "ich", "Sie", "auf", "der", "Stra\u00b7\u00dfe", "se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Dann ergreift es mich, ich wei\u00df nicht wie;", "tokens": ["Dann", "er\u00b7greift", "es", "mich", ",", "ich", "wei\u00df", "nicht", "wie", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "PPER", "VVFIN", "PTKNEG", "KOKOM", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Dann wird es mir klar und ich gestehe", "tokens": ["Dann", "wird", "es", "mir", "klar", "und", "ich", "ge\u00b7ste\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADJD", "KON", "PPER", "VVFIN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Ihnen noch einmal: Ich liebe Sie.", "tokens": ["Ih\u00b7nen", "noch", "ein\u00b7mal", ":", "Ich", "lie\u00b7be", "Sie", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ob ich gehe, stehe, liege, sitze,", "tokens": ["Ob", "ich", "ge\u00b7he", ",", "ste\u00b7he", ",", "lie\u00b7ge", ",", "sit\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ob ich meinen Aufsatz schreiben soll,", "tokens": ["Ob", "ich", "mei\u00b7nen", "Auf\u00b7satz", "schrei\u00b7ben", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ob ich \u00fcber der Grammatik schwitze,", "tokens": ["Ob", "ich", "\u00fc\u00b7ber", "der", "Gram\u00b7ma\u00b7tik", "schwit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Stets erscheint Ihr Bild verhei\u00dfungsvoll.", "tokens": ["Stets", "er\u00b7scheint", "Ihr", "Bild", "ver\u00b7hei\u00b7\u00dfungs\u00b7voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und wenn Sie mir nicht zu schreiben denken,", "tokens": ["Und", "wenn", "Sie", "mir", "nicht", "zu", "schrei\u00b7ben", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "PTKNEG", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Dann soll ein verhei\u00dfungsvoller Blick,", "tokens": ["Dann", "soll", "ein", "ver\u00b7hei\u00b7\u00dfungs\u00b7vol\u00b7ler", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Den Sie im Vor\u00fcbergehn mir schenken,", "tokens": ["Den", "Sie", "im", "Vor\u00b7\u00fc\u00b7ber\u00b7gehn", "mir", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Bote sein von meinem gr\u00f6\u00dften Gl\u00fcck.", "tokens": ["Bo\u00b7te", "sein", "von", "mei\u00b7nem", "gr\u00f6\u00df\u00b7ten", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Aber wenn mein Herz zu k\u00fchn gewesen,", "tokens": ["A\u00b7ber", "wenn", "mein", "Herz", "zu", "k\u00fchn", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "PTKA", "ADJD", "VAPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn sich Ihre Blicke wenden ab,", "tokens": ["Wenn", "sich", "Ih\u00b7re", "Bli\u00b7cke", "wen\u00b7den", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Werden Sie vielleicht im Tagblatt lesen,", "tokens": ["Wer\u00b7den", "Sie", "viel\u00b7leicht", "im", "Tag\u00b7blatt", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Wo ein Lebensm\u00fcder fand sein Grab.", "tokens": ["Wo", "ein", "Le\u00b7bens\u00b7m\u00fc\u00b7der", "fand", "sein", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "So, Sie kennen nun mein Liebesfeuer;", "tokens": ["So", ",", "Sie", "ken\u00b7nen", "nun", "mein", "Lie\u00b7bes\u00b7feu\u00b7er", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Winkt mir heitres, winkt mir d\u00fcstres Los?", "tokens": ["Winkt", "mir", "heit\u00b7res", ",", "winkt", "mir", "d\u00fcst\u00b7res", "Los", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "$,", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Meine Freude w\u00e4re ungeheuer;", "tokens": ["Mei\u00b7ne", "Freu\u00b7de", "w\u00e4\u00b7re", "un\u00b7ge\u00b7heu\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Meine Schmerzen w\u00e4ren riesengro\u00df.", "tokens": ["Mei\u00b7ne", "Schmer\u00b7zen", "w\u00e4\u00b7ren", "rie\u00b7sen\u00b7gro\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}