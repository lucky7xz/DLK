{"textgrid.poem.46929": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Deutschen von dem Flutenbett des Rheines,", "tokens": ["Ihr", "Deut\u00b7schen", "von", "dem", "Flu\u00b7ten\u00b7bett", "des", "Rhei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "ART", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bis wo die Elbe sich ins Nordmeer gie\u00dfet,", "tokens": ["Bis", "wo", "die", "El\u00b7be", "sich", "ins", "Nord\u00b7meer", "gie\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAV", "ART", "NE", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die ihr vordem ein Volk, ein gro\u00dfes, hie\u00dfet,", "tokens": ["Die", "ihr", "vor\u00b7dem", "ein", "Volk", ",", "ein", "gro\u00b7\u00dfes", ",", "hie\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was habt ihr denn, um noch zu hei\u00dfen eines?", "tokens": ["Was", "habt", "ihr", "denn", ",", "um", "noch", "zu", "hei\u00b7\u00dfen", "ei\u00b7nes", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "$,", "KOUI", "ADV", "PTKZU", "VVINF", "PIS", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was habt ihr denn noch gro\u00dfes allgemeines?", "tokens": ["Was", "habt", "ihr", "denn", "noch", "gro\u00b7\u00dfes", "all\u00b7ge\u00b7mei\u00b7nes", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "ADV", "ADJA", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Welch Band, das euch als Volk zusammenschlie\u00dfet?", "tokens": ["Welch", "Band", ",", "das", "euch", "als", "Volk", "zu\u00b7sam\u00b7men\u00b7schlie\u00b7\u00dfet", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Seit ihr den Kaiserszepter brechen lie\u00dfet,", "tokens": ["Seit", "ihr", "den", "Kai\u00b7sers\u00b7zep\u00b7ter", "bre\u00b7chen", "lie\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und euer Reich zerspalten, habt ihr keines.", "tokens": ["Und", "eu\u00b7er", "Reich", "zer\u00b7spal\u00b7ten", ",", "habt", "ihr", "kei\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,", "VAFIN", "PPER", "PIS", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Nur noch ein einziges Band ist euch geblieben,", "tokens": ["Nur", "noch", "ein", "ein\u00b7zi\u00b7ges", "Band", "ist", "euch", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "Das ist die Sprache, die ihr sonst verachtet;", "tokens": ["Das", "ist", "die", "Spra\u00b7che", ",", "die", "ihr", "sonst", "ver\u00b7ach\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Jetzt m\u00fc\u00dft ihr sie als euer einziges lieben.", "tokens": ["Jetzt", "m\u00fc\u00dft", "ihr", "sie", "als", "eu\u00b7er", "ein\u00b7zi\u00b7ges", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "KOUS", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Sie ist noch eu'r, ihr selber seid verpachtet;", "tokens": ["Sie", "ist", "noch", "eu'r", ",", "ihr", "sel\u00b7ber", "seid", "ver\u00b7pach\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NE", "$,", "PPER", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Sie haltet fest, wenn alles wird zerrieben,", "tokens": ["Sie", "hal\u00b7tet", "fest", ",", "wenn", "al\u00b7les", "wird", "zer\u00b7rie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KOUS", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Da\u00df ihr doch klagen k\u00f6nnt, wie ihr verschmachtet.", "tokens": ["Da\u00df", "ihr", "doch", "kla\u00b7gen", "k\u00f6nnt", ",", "wie", "ihr", "ver\u00b7schmach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VVFIN", "$,", "PWAV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}