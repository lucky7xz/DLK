{"textgrid.poem.31451": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Solt sichs noch tausendmal unm\u00f6glicher anlassen /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Solt sichs noch tausendmal unm\u00f6glicher anlassen /", "tokens": ["Solt", "sichs", "noch", "tau\u00b7send\u00b7mal", "un\u00b7m\u00f6g\u00b7li\u00b7cher", "an\u00b7las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ja ich und alls vergehn: noch gleichwol glaub' ich fort.", "tokens": ["ja", "ich", "und", "alls", "ver\u00b7gehn", ":", "noch", "gleich\u00b7wol", "glaub'", "ich", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "KON", "ADV", "VVINF", "$.", "ADV", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich bind den Hoffnungs Stamm' ans unverg\u00e4nglich Wort /", "tokens": ["Ich", "bind", "den", "Hoff\u00b7nungs", "Stamm'", "ans", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "Wort", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "APPRART", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "da\u00df wird mein Glaub' im Tod / ja gar im Grab umfassen.", "tokens": ["da\u00df", "wird", "mein", "Glaub'", "im", "Tod", "/", "ja", "gar", "im", "Grab", "um\u00b7fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPOSAT", "NN", "APPRART", "NN", "$(", "ADV", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es hat die g\u00fcte sich verstricket solcher massen /", "tokens": ["Es", "hat", "die", "g\u00fc\u00b7te", "sich", "ver\u00b7stri\u00b7cket", "sol\u00b7cher", "mas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PRF", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "da\u00df auch die Allmacht selbst findt kein entrinnungs Ort.", "tokens": ["da\u00df", "auch", "die", "All\u00b7macht", "selbst", "findt", "kein", "ent\u00b7rin\u00b7nungs", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Ja / sie ist selbst viel mehr / der Gnad'-und Warheit Port /", "tokens": ["Ja", "/", "sie", "ist", "selbst", "viel", "mehr", "/", "der", "Gna\u00b7d'\u00b7\u00b7und", "War\u00b7heit", "Port", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PPER", "VAFIN", "ADV", "ADV", "ADV", "$(", "ART", "NN", "NN", "NE", "$("], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.8": {"text": "und ihrer Hoffnung frucht / die wir im Geist offt a\u00dfen.", "tokens": ["und", "ih\u00b7rer", "Hoff\u00b7nung", "frucht", "/", "die", "wir", "im", "Geist", "offt", "a\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$(", "PRELS", "PPER", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie ist vollzieherin des Gn\u00e4den-Wunder Schlu\u00df:", "tokens": ["Sie", "ist", "voll\u00b7zie\u00b7he\u00b7rin", "des", "Gn\u00e4\u00b7den\u00b7Wun\u00b7der", "Schlu\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "und wie die G\u00fct der Grund / so wenig dieser / weichet /", "tokens": ["und", "wie", "die", "G\u00fct", "der", "Grund", "/", "so", "we\u00b7nig", "die\u00b7ser", "/", "wei\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ART", "NN", "$(", "ADV", "PIS", "PDAT", "$(", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "so n\u00f6thig sie dem Pfad der Warheit folgen mu\u00df.", "tokens": ["so", "n\u00f6\u00b7thig", "sie", "dem", "Pfad", "der", "War\u00b7heit", "fol\u00b7gen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ob die Regirung schon ob allen urtheiln streichet /", "tokens": ["Ob", "die", "Re\u00b7gi\u00b7rung", "schon", "ob", "al\u00b7len", "ur\u00b7theiln", "strei\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "KOUS", "PIS", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "dermassen wundert / da\u00df sie alle Ziel zerreist;", "tokens": ["der\u00b7mas\u00b7sen", "wun\u00b7dert", "/", "da\u00df", "sie", "al\u00b7le", "Ziel", "zer\u00b7reist", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "KOUS", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "so siegt aufs herrlichst doch die Warheit / in den Geist.", "tokens": ["so", "siegt", "aufs", "herr\u00b7lichst", "doch", "die", "War\u00b7heit", "/", "in", "den", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "VVFIN", "ADV", "ART", "NN", "$(", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}