{"textgrid.poem.27851": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "66.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hat Gott durch sein Wort di\u00df runde Haus gebauet,", "tokens": ["Es", "hat", "Gott", "durch", "sein", "Wort", "di\u00df", "run\u00b7de", "Haus", "ge\u00b7bau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "APPR", "PPOSAT", "NN", "PDS", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was man drinnen merckt, und was man aussen schauet.", "tokens": ["Und", "was", "man", "drin\u00b7nen", "merckt", ",", "und", "was", "man", "aus\u00b7sen", "schau\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIS", "ADV", "VVFIN", "$,", "KON", "PWS", "PIS", "VVIZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Teuffel hat ein Wort, dadurch er F\u00fcrsatz hat", "tokens": ["Der", "Teuf\u00b7fel", "hat", "ein", "Wort", ",", "da\u00b7durch", "er", "F\u00fcr\u00b7satz", "hat"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "PAV", "PPER", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu tilgen, was Gott schuff, und dieses heist: Soldat.", "tokens": ["Zu", "til\u00b7gen", ",", "was", "Gott", "schuff", ",", "und", "die\u00b7ses", "heist", ":", "Sol\u00b7dat", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PRELS", "NN", "PTKVZ", "$,", "KON", "PDS", "VAFIN", "$.", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}