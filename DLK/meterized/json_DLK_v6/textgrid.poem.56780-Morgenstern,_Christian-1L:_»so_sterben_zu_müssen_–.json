{"textgrid.poem.56780": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbso sterben zu m\u00fcssen \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbso sterben zu m\u00fcssen \u2013", "tokens": ["\u00bb", "so", "ster\u00b7ben", "zu", "m\u00fcs\u00b7sen", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVINF", "PTKZU", "VMINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "auf einer elenden Kerze!", "tokens": ["auf", "ei\u00b7ner", "e\u00b7len\u00b7den", "Ker\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "tatenlos, ruhmlos", "tokens": ["ta\u00b7ten\u00b7los", ",", "ruh\u00b7mlos"], "token_info": ["word", "punct", "word"], "pos": ["ADJD", "$,", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "im Atemchen", "tokens": ["im", "A\u00b7tem\u00b7chen"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+--", "measure": "dactylic.init"}, "line.5": {"text": "eines Menschleins", "tokens": ["ei\u00b7nes", "Mensc\u00b7hleins"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "zu enden! ...", "tokens": ["zu", "en\u00b7den", "!", "..."], "token_info": ["word", "word", "punct", "punct"], "pos": ["PTKZU", "VVINF", "$.", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Diese Kraft,", "tokens": ["Die\u00b7se", "Kraft", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "die ihr alle nicht kennt \u2013", "tokens": ["die", "ihr", "al\u00b7le", "nicht", "kennt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIS", "PTKNEG", "VVFIN", "$("], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.9": {"text": "diese grenzenlose Kraft!", "tokens": ["die\u00b7se", "gren\u00b7zen\u00b7lo\u00b7se", "Kraft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Ihr Nichtse! ...", "tokens": ["Ihr", "Nicht\u00b7se", "!", "..."], "token_info": ["word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$.", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Komm doch n\u00e4her,", "tokens": ["Komm", "doch", "n\u00e4\u00b7her", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "du schlafender Kopf!", "tokens": ["du", "schla\u00b7fen\u00b7der", "Kopf", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Schlummer,", "tokens": ["Schlum\u00b7mer", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.14": {"text": "der du ihn niederwarfst \u2013", "tokens": ["der", "du", "ihn", "nie\u00b7der\u00b7warfst", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "ruf doch dein Br\u00fcderlein Tod \u2013", "tokens": ["ruf", "doch", "dein", "Br\u00fc\u00b7derl\u00b7ein", "Tod", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "NN", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.16": {"text": "er soll ihn mir zuschieben \u2013", "tokens": ["er", "soll", "ihn", "mir", "zu\u00b7schie\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPER", "ADV", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "den Lockenkopf \u2013", "tokens": ["den", "Lo\u00b7cken\u00b7kopf", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "ich will ihn haben \u2013 haben!", "tokens": ["ich", "will", "ihn", "ha\u00b7ben", "\u2013", "ha\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VAFIN", "$(", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Sieh,", "tokens": ["Sieh", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+", "measure": "single.up"}, "line.20": {"text": "wie ich ihm entgegenhungre!", "tokens": ["wie", "ich", "ihm", "ent\u00b7ge\u00b7gen\u00b7hung\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "Ich renke mir alle Glieder", "tokens": ["Ich", "ren\u00b7ke", "mir", "al\u00b7le", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.22": {"text": "nach ihm aus ...", "tokens": ["nach", "ihm", "aus", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.23": {"text": "Ein wenig noch n\u00e4her \u2013", "tokens": ["Ein", "we\u00b7nig", "noch", "n\u00e4\u00b7her", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "n\u00e4her \u2013", "tokens": ["n\u00e4\u00b7her", "\u2013"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.25": {"text": "ein wenig \u2013", "tokens": ["ein", "we\u00b7nig", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ART", "PIS", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.26": {"text": "so \u2013", "tokens": ["so", "\u2013"], "token_info": ["word", "punct"], "pos": ["ADV", "$("], "meter": "-", "measure": "single.down"}, "line.27": {"text": "jetzt vielleicht \u2013", "tokens": ["jetzt", "viel\u00b7leicht", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.28": {"text": "wenn's gl\u00fcckt \u2013", "tokens": ["wenn's", "gl\u00fcckt", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVFIN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.29": {"text": "ah! du Hund!", "tokens": ["ah", "!", "du", "Hund", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.30": {"text": "Er will erwachen?", "tokens": ["Er", "will", "er\u00b7wa\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.31": {"text": "still \u2013", "tokens": ["still", "\u2013"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "+", "measure": "single.up"}, "line.32": {"text": "still \u2013", "tokens": ["still", "\u2013"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "+", "measure": "single.up"}, "line.33": {"text": "so ist's noch besser!", "tokens": ["so", "ist's", "noch", "bes\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.34": {"text": "Der Pelz am Mantel \u2013", "tokens": ["Der", "Pelz", "am", "Man\u00b7tel", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.35": {"text": "Der Pelz \u2013 der Pelz \u2013", "tokens": ["Der", "Pelz", "\u2013", "der", "Pelz", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.36": {"text": "hin\u00fcber \u2013 hin\u00fcber \u2013", "tokens": ["hin\u00b7\u00fc\u00b7ber", "\u2013", "hin\u00b7\u00fc\u00b7ber", "\u2013"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$(", "ADV", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.37": {"text": "ahhh! fa\u00df ich dich \u2013 hab ich dich \u2013", "tokens": ["ahhh", "!", "fa\u00df", "ich", "dich", "\u2013", "hab", "ich", "dich", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "PRF", "$(", "VAFIN", "PPER", "PRF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "hab ich dich, Br\u00fcderchen \u2013", "tokens": ["hab", "ich", "dich", ",", "Br\u00fc\u00b7der\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "$,", "NN", "$("], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.39": {"text": "Pelzbr\u00fcderchen, hab ich dich \u2013 ahhh!", "tokens": ["Pelz\u00b7br\u00fc\u00b7der\u00b7chen", ",", "hab", "ich", "dich", "\u2013", "ahhh", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "PRF", "$(", "ITJ", "$."], "meter": "++--+--+", "measure": "prosodiakos"}, "line.40": {"text": "Hilft dir nichts \u2013", "tokens": ["Hilft", "dir", "nichts", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.41": {"text": "wehr dich nicht mehr!", "tokens": ["wehr", "dich", "nicht", "mehr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.42": {"text": "Mein bist du jetzt \u2013", "tokens": ["Mein", "bist", "du", "jetzt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PPER", "ADV", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.43": {"text": "Hand weg!", "tokens": ["Hand", "weg", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.44": {"text": "Wasser weg!", "tokens": ["Was\u00b7ser", "weg", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.45": {"text": "Mein bist du jetzt!", "tokens": ["Mein", "bist", "du", "jetzt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.46": {"text": "Wasser weg!", "tokens": ["Was\u00b7ser", "weg", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.47": {"text": "Wart', da dr\u00fcben ist", "tokens": ["Wart'", ",", "da", "dr\u00fc\u00b7ben", "ist"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VAFIN", "$,", "KOUS", "ADV", "VAFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.48": {"text": "auch noch f\u00fcr mich \u2013", "tokens": ["auch", "noch", "f\u00fcr", "mich", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.49": {"text": "so \u2013", "tokens": ["so", "\u2013"], "token_info": ["word", "punct"], "pos": ["ADV", "$("], "meter": "-", "measure": "single.down"}, "line.50": {"text": "den Vorhang hinauf \u2013", "tokens": ["den", "Vor\u00b7hang", "hin\u00b7auf", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.51": {"text": "f\u00e4ngst mich nicht mehr \u2013", "tokens": ["f\u00e4ngst", "mich", "nicht", "mehr", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.52": {"text": "Tuch \u2013 Tuch \u2013", "tokens": ["Tuch", "\u2013", "Tuch", "\u2013"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$(", "NN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.53": {"text": "jetzt bin ich Herr!", "tokens": ["jetzt", "bin", "ich", "Herr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.54": {"text": "Siehst du, jetzt breit' ich mich", "tokens": ["Siehst", "du", ",", "jetzt", "breit'", "ich", "mich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.55": {"text": "ganz gem\u00e4chlich im Zimmer aus \u2013", "tokens": ["ganz", "ge\u00b7m\u00e4ch\u00b7lich", "im", "Zim\u00b7mer", "aus", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "PTKVZ", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.56": {"text": "la\u00df doch den Wasserkrug!", "tokens": ["la\u00df", "doch", "den", "Was\u00b7ser\u00b7krug", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.57": {"text": "La\u00df doch das H\u00fclfgeschrei!", "tokens": ["La\u00df", "doch", "das", "H\u00fclf\u00b7ge\u00b7schrei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.58": {"text": "Bis sie kommen", "tokens": ["Bis", "sie", "kom\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.59": {"text": "bin ich schon l\u00e4ngst", "tokens": ["bin", "ich", "schon", "l\u00e4ngst"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV"], "meter": "---+", "measure": "unknown.measure.single"}, "line.60": {"text": "in den Betten und Schr\u00e4nken \u2013", "tokens": ["in", "den", "Bet\u00b7ten", "und", "Schr\u00e4n\u00b7ken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.61": {"text": "und dann k\u00f6nnt ihr nicht mehr herein \u2013", "tokens": ["und", "dann", "k\u00f6nnt", "ihr", "nicht", "mehr", "her\u00b7ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.62": {"text": "und ich bei\u00df' in die Balken der Decke \u2013", "tokens": ["und", "ich", "bei\u00df'", "in", "die", "Bal\u00b7ken", "der", "De\u00b7cke", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "APPR", "ART", "NN", "ART", "NN", "$("], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.63": {"text": "die dicken, langen, braunen Balken \u2013", "tokens": ["die", "di\u00b7cken", ",", "lan\u00b7gen", ",", "brau\u00b7nen", "Bal\u00b7ken", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.64": {"text": "und steig' in den Dachstuhl \u2013", "tokens": ["und", "steig'", "in", "den", "Dach\u00b7stuhl", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.65": {"text": "und vom einen Dachstuhl", "tokens": ["und", "vom", "ei\u00b7nen", "Dach\u00b7stuhl"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.66": {"text": "zum andern Dachstuhl \u2013", "tokens": ["zum", "an\u00b7dern", "Dach\u00b7stuhl", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.67": {"text": "und irgendwo", "tokens": ["und", "ir\u00b7gend\u00b7wo"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.68": {"text": "werd' ich wohl Stroh finden,", "tokens": ["werd'", "ich", "wohl", "Stroh", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.69": {"text": "und \u00d6l finden,", "tokens": ["und", "\u00d6l", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,"], "meter": "-++-", "measure": "unknown.measure.di"}, "line.70": {"text": "und Pulver finden \u2013", "tokens": ["und", "Pul\u00b7ver", "fin\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.71": {"text": "das wird eine Lust werden!", "tokens": ["das", "wird", "ei\u00b7ne", "Lust", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VAINF", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.72": {"text": "Das wird ein Fest werden!", "tokens": ["Das", "wird", "ein", "Fest", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.73": {"text": "Und wenn ich die H\u00e4user alle zernichtet \u2013", "tokens": ["Und", "wenn", "ich", "die", "H\u00e4u\u00b7ser", "al\u00b7le", "zer\u00b7nich\u00b7tet", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "PIS", "VVPP", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.74": {"text": "dann wollen wir mit W\u00e4ldern", "tokens": ["dann", "wol\u00b7len", "wir", "mit", "W\u00e4l\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.75": {"text": "die Fische in den Fl\u00fcssen kochen \u2013", "tokens": ["die", "Fi\u00b7sche", "in", "den", "Fl\u00fcs\u00b7sen", "ko\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.76": {"text": "und ich will euch hinauftreiben", "tokens": ["und", "ich", "will", "euch", "hin\u00b7auf\u00b7trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.77": {"text": "auf die k\u00e4ltesten Berge \u2013", "tokens": ["auf", "die", "k\u00e4l\u00b7tes\u00b7ten", "Ber\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.78": {"text": "und da droben", "tokens": ["und", "da", "dro\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.79": {"text": "sollt auch ihr meine Opfer werden,", "tokens": ["sollt", "auch", "ihr", "mei\u00b7ne", "Op\u00b7fer", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.80": {"text": "sollt ihr meine Todesfackeln werden \u2013", "tokens": ["sollt", "ihr", "mei\u00b7ne", "To\u00b7des\u00b7fa\u00b7ckeln", "wer\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VAINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.81": {"text": "und dann wird alles still sein \u2013", "tokens": ["und", "dann", "wird", "al\u00b7les", "still", "sein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIS", "ADJD", "VAINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.82": {"text": "und dann \u2013", "tokens": ["und", "dann", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADV", "$("], "meter": "-+", "measure": "iambic.single"}}}}}