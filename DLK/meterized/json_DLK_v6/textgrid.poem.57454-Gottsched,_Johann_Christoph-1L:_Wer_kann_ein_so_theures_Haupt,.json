{"textgrid.poem.57454": {"metadata": {"author": {"name": "Gottsched, Johann Christoph", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer kann ein so theures Haupt,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer kann ein so theures Haupt,", "tokens": ["Wer", "kann", "ein", "so", "theu\u00b7res", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wer kann solche seltne Gaben,", "tokens": ["Wer", "kann", "sol\u00b7che", "selt\u00b7ne", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sonder Schmerz und Gram begraben,", "tokens": ["Son\u00b7der", "Schmerz", "und", "Gram", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als der Tod uns hier geraubt?", "tokens": ["Als", "der", "Tod", "uns", "hier", "ge\u00b7raubt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Flie\u00dft ihr h\u00f6chstgerechten Z\u00e4hren!", "tokens": ["Flie\u00dft", "ihr", "h\u00f6chst\u00b7ge\u00b7rech\u00b7ten", "Z\u00e4h\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eurer sch\u00e4mt sich niemand nicht,", "tokens": ["Eu\u00b7rer", "sch\u00e4mt", "sich", "nie\u00b7mand", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PRF", "PIS", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Euren Strom wird niemand wehren,", "tokens": ["Eu\u00b7ren", "Strom", "wird", "nie\u00b7mand", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der aus Antrieb seiner Pflicht,", "tokens": ["Der", "aus", "An\u00b7trieb", "sei\u00b7ner", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Die es treu und redlich meynet,", "tokens": ["Die", "es", "treu", "und", "red\u00b7lich", "mey\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Voll von zarter Regung weinet.", "tokens": ["Voll", "von", "zar\u00b7ter", "Re\u00b7gung", "wei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O du thr\u00e4nenwerthes Grab,", "tokens": ["O", "du", "thr\u00e4\u00b7nen\u00b7wert\u00b7hes", "Grab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und du m\u00fctterliche Leiche!", "tokens": ["Und", "du", "m\u00fct\u00b7ter\u00b7li\u00b7che", "Lei\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie entsetzlich sind die Streiche,", "tokens": ["Wie", "ent\u00b7setz\u00b7lich", "sind", "die", "Strei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die dein fr\u00fcher Fall uns gab!", "tokens": ["Die", "dein", "fr\u00fc\u00b7her", "Fall", "uns", "gab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mitten in den sch\u00f6nsten Tagen,", "tokens": ["Mit\u00b7ten", "in", "den", "sch\u00f6ns\u00b7ten", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mitten unter Gl\u00fcck und Ruh,", "tokens": ["Mit\u00b7ten", "un\u00b7ter", "Gl\u00fcck", "und", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "H\u00f6rt man dich von Krankheit klagen,", "tokens": ["H\u00f6rt", "man", "dich", "von", "Krank\u00b7heit", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dr\u00fcckt man dir die Augen zu;", "tokens": ["Dr\u00fcckt", "man", "dir", "die", "Au\u00b7gen", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Mu\u00df dein matter Leib erblassen,", "tokens": ["Mu\u00df", "dein", "mat\u00b7ter", "Leib", "er\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und der Geist den K\u00f6rper lassen.", "tokens": ["Und", "der", "Geist", "den", "K\u00f6r\u00b7per", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Ach was soll, was kann man thun,", "tokens": ["Ach", "was", "soll", ",", "was", "kann", "man", "thun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VMFIN", "$,", "PWS", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Theure Mutter! dich zu retten?", "tokens": ["Theu\u00b7re", "Mut\u00b7ter", "!", "dich", "zu", "ret\u00b7ten", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Sollst du k\u00fcnftig in den Ketten", "tokens": ["Sollst", "du", "k\u00fcnf\u00b7tig", "in", "den", "Ket\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Dieses Schlummers ewig ruhn?", "tokens": ["Die\u00b7ses", "Schlum\u00b7mers", "e\u00b7wig", "ruhn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Zucht, Gerechtigkeit und Treue,", "tokens": ["Zucht", ",", "Ge\u00b7rech\u00b7tig\u00b7keit", "und", "Treu\u00b7e", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Unverf\u00e4lschte Redlichkeit", "tokens": ["Un\u00b7ver\u00b7f\u00e4lschte", "Red\u00b7lich\u00b7keit"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.17": {"text": "R\u00fchmten sonst, und hier aufs neue,", "tokens": ["R\u00fchm\u00b7ten", "sonst", ",", "und", "hier", "aufs", "neu\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KON", "ADV", "APPRART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Da\u00df dein Herz sich jederzeit,", "tokens": ["Da\u00df", "dein", "Herz", "sich", "je\u00b7der\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Durch ein tugendhaftes Leben,", "tokens": ["Durch", "ein", "tu\u00b7gend\u00b7haf\u00b7tes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Ihrer aller Dienst ergeben.", "tokens": ["Ih\u00b7rer", "al\u00b7ler", "Dienst", "er\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Komm doch wieder, sch\u00f6ner Tag!", "tokens": ["Komm", "doch", "wie\u00b7der", ",", "sch\u00f6\u00b7ner", "Tag", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da ihr m\u00fctterlicher Segen,", "tokens": ["Da", "ihr", "m\u00fct\u00b7ter\u00b7li\u00b7cher", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsrer Hochzeitfackeln wegen,", "tokens": ["Uns\u00b7rer", "Hoch\u00b7zeit\u00b7fac\u00b7keln", "we\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Beyden auf der Scheitel lag;", "tokens": ["Bey\u00b7den", "auf", "der", "Schei\u00b7tel", "lag", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da ein treues H\u00e4ndek\u00fcssen", "tokens": ["Da", "ein", "treu\u00b7es", "H\u00e4n\u00b7de\u00b7k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unsrer Ehrfurcht Opfer war.", "tokens": ["Uns\u00b7rer", "Ehr\u00b7furcht", "Op\u00b7fer", "war", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "O wie viel ist uns entrissen!", "tokens": ["O", "wie", "viel", "ist", "uns", "ent\u00b7ris\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "O wie sehr wirds offenbar!", "tokens": ["O", "wie", "sehr", "wirds", "of\u00b7fen\u00b7bar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADV", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Keine Lust kann lange dauren;", "tokens": ["Kei\u00b7ne", "Lust", "kann", "lan\u00b7ge", "dau\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Auf die Freude folgt das Trauren.", "tokens": ["Auf", "die", "Freu\u00b7de", "folgt", "das", "Trau\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Du geliebtes Ehrenhayn!", "tokens": ["Du", "ge\u00b7lieb\u00b7tes", "Eh\u00b7ren\u00b7hayn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dein Vergn\u00fcgen, dein Ergetzen,", "tokens": ["Dein", "Ver\u00b7gn\u00fc\u00b7gen", ",", "dein", "Er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist dem Gram weit nachzusetzen,", "tokens": ["Ist", "dem", "Gram", "weit", "nach\u00b7zu\u00b7set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dem wir unterworfen seyn.", "tokens": ["Dem", "wir", "un\u00b7ter\u00b7wor\u00b7fen", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deine Sch\u00f6nheit wird zur W\u00fcsten,", "tokens": ["Dei\u00b7ne", "Sch\u00f6n\u00b7heit", "wird", "zur", "W\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deine Flur ein Todtenhaus;", "tokens": ["Dei\u00b7ne", "Flur", "ein", "Tod\u00b7ten\u00b7haus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "O da\u00df wir nicht sagen m\u00fc\u00dften:", "tokens": ["O", "da\u00df", "wir", "nicht", "sa\u00b7gen", "m\u00fc\u00df\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ehrenhayn f\u00fcllt Angst und Graus!", "tokens": ["Eh\u00b7ren\u00b7hayn", "f\u00fcllt", "Angst", "und", "Graus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Ist, wo wir die Wahrheit kennen,", "tokens": ["Ist", ",", "wo", "wir", "die", "Wahr\u00b7heit", "ken\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWAV", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Besser, Jammerhayn zu nennen.", "tokens": ["Bes\u00b7ser", ",", "Jam\u00b7mer\u00b7hayn", "zu", "nen\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Nennt es k\u00fcnftig anders nicht,", "tokens": ["Nennt", "es", "k\u00fcnf\u00b7tig", "an\u00b7ders", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Wenn ihr sein gedenken sollet,", "tokens": ["Wenn", "ihr", "sein", "ge\u00b7den\u00b7ken", "sol\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Die ihr uns nicht kr\u00e4nken wollet,", "tokens": ["Die", "ihr", "uns", "nicht", "kr\u00e4n\u00b7ken", "wol\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "In Erf\u00fcllung unsrer Pflicht.", "tokens": ["In", "Er\u00b7f\u00fcl\u00b7lung", "uns\u00b7rer", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Seine Felder, seine Wiesen", "tokens": ["Sei\u00b7ne", "Fel\u00b7der", ",", "sei\u00b7ne", "Wie\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Reizen uns zum Kummer an;", "tokens": ["Rei\u00b7zen", "uns", "zum", "Kum\u00b7mer", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Jeder Ort, der uns vor diesem", "tokens": ["Je\u00b7der", "Ort", ",", "der", "uns", "vor", "die\u00b7sem"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "APPR", "PDAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Manchen frohen Dienst gethan,", "tokens": ["Man\u00b7chen", "fro\u00b7hen", "Dienst", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.