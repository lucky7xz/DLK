{"dta.poem.3776": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1892", "urn": "urn:nbn:de:kobv:b4-200905191524", "language": ["de:0.99"], "booktitle": "George, Stefan: Algabal. Paris u. a., 1892."}, "poem": {"stanza.1": {"line.1": {"text": "Wo dem grossen Zeus zu fr\u00f6hnen", "tokens": ["Wo", "dem", "gros\u00b7sen", "Zeus", "zu", "fr\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Toller wunder fremde schau", "tokens": ["Tol\u00b7ler", "wun\u00b7der", "frem\u00b7de", "schau"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die w\u00fcrde sich vers\u00f6hnen", "tokens": ["Und", "die", "w\u00fcr\u00b7de", "sich", "ver\u00b7s\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "T\u00e4nzer \u00f6ffnen das geleit", "tokens": ["T\u00e4n\u00b7zer", "\u00f6ff\u00b7nen", "das", "ge\u00b7leit"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In verf\u00fchrenden gew\u00e4ndern \u00b7", "tokens": ["In", "ver\u00b7f\u00fch\u00b7ren\u00b7den", "ge\u00b7w\u00e4n\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "XY"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Knaben die ein opfer feit", "tokens": ["Kna\u00b7ben", "die", "ein", "op\u00b7fer", "feit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In den sonnenschlaffen l\u00e4ndern", "tokens": ["In", "den", "son\u00b7nen\u00b7schlaf\u00b7fen", "l\u00e4n\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Macht aus \u00f6l- und palmenlaub", "tokens": ["Macht", "aus", "\u00f6l", "und", "pal\u00b7men\u00b7laub"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "TRUNC", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Vor des priesters fuss ein kissen", "tokens": ["Vor", "des", "pries\u00b7ters", "fuss", "ein", "kis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Streuet sand und silberstaub", "tokens": ["Streu\u00b7et", "sand", "und", "sil\u00b7ber\u00b7staub"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Tote liljen und narzissen", "tokens": ["To\u00b7te", "lil\u00b7jen", "und", "nar\u00b7zis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVINF", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "An der schwelle haltet rast", "tokens": ["An", "der", "schwel\u00b7le", "hal\u00b7tet", "rast"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo das heilge bild entschleiert", "tokens": ["Wo", "das", "heil\u00b7ge", "bild", "ent\u00b7schlei\u00b7ert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur sich giebt dem einen gast", "tokens": ["Nur", "sich", "giebt", "dem", "ei\u00b7nen", "gast"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PRF", "VVFIN", "ART", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der es oft und innig feiert", "tokens": ["Der", "es", "oft", "und", "in\u00b7nig", "fei\u00b7ert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "KON", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Eines mund gebete lallt", "tokens": ["Ei\u00b7nes", "mund", "ge\u00b7be\u00b7te", "lallt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Auch kein bruder sei zugegen", "tokens": ["Auch", "kein", "bru\u00b7der", "sei", "zu\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Spricht des gottes zwiegestalt", "tokens": ["Spricht", "des", "got\u00b7tes", "zwie\u00b7ge\u00b7stalt"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Seinen immergleichen segen", "tokens": ["Sei\u00b7nen", "im\u00b7mer\u00b7glei\u00b7chen", "se\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Junge stimmen \u00b7 ferner hall \u00b7", "tokens": ["Jun\u00b7ge", "stim\u00b7men", "fer\u00b7ner", "hall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "XY", "XY", "XY", "XY"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Narden die verfl\u00fcchtet irren", "tokens": ["Nar\u00b7den", "die", "ver\u00b7fl\u00fcch\u00b7tet", "ir\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch der r\u00e4uche strengen quall", "tokens": ["Durch", "der", "r\u00e4u\u00b7che", "stren\u00b7gen", "quall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu dem kuss der s\u00fcssen mirren", "tokens": ["Zu", "dem", "kuss", "der", "s\u00fcs\u00b7sen", "mir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}