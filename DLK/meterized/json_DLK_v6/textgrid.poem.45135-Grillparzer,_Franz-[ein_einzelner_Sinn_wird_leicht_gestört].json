{"textgrid.poem.45135": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[ein einzelner Sinn wird leicht gest\u00f6rt]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein einzelner Sinn wird leicht gest\u00f6rt,", "tokens": ["Ein", "ein\u00b7zel\u00b7ner", "Sinn", "wird", "leicht", "ge\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Sie m\u00fcssen mitsammen gehen.", "tokens": ["Sie", "m\u00fcs\u00b7sen", "mit\u00b7sam\u00b7men", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nun hab ich genug von der Freiheit geh\u00f6rt,", "tokens": ["Nun", "hab", "ich", "ge\u00b7nug", "von", "der", "Frei\u00b7heit", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "M\u00f6cht einmal von ihr was sehen.", "tokens": ["M\u00f6cht", "ein\u00b7mal", "von", "ihr", "was", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}