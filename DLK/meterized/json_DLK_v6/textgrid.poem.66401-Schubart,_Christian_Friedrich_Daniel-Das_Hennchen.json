{"textgrid.poem.66401": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Das Hennchen", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Hennchen schlang, von Winterfrost gedrungen,", "tokens": ["Ein", "Henn\u00b7chen", "schlang", ",", "von", "Win\u00b7ter\u00b7frost", "ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Fl\u00fcgel fest um ihre Jungen,", "tokens": ["Die", "Fl\u00fc\u00b7gel", "fest", "um", "ih\u00b7re", "Jun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wonnevoll erfror das gute Thier:", "tokens": ["Und", "won\u00b7ne\u00b7voll", "er\u00b7fror", "das", "gu\u00b7te", "Thier", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Denn ihre Brut stack sicher im Gefieder.", "tokens": ["Denn", "ih\u00b7re", "Brut", "stack", "si\u00b7cher", "im", "Ge\u00b7fie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Medeen, Prognen, lernet hier,", "tokens": ["Me\u00b7de\u00b7en", ",", "Prog\u00b7nen", ",", "ler\u00b7net", "hier", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Und schaut besch\u00e4mt zur Erde nieder!", "tokens": ["Und", "schaut", "be\u00b7sch\u00e4mt", "zur", "Er\u00b7de", "nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}