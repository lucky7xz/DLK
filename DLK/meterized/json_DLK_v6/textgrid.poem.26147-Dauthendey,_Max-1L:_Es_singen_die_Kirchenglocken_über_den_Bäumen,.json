{"textgrid.poem.26147": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es singen die Kirchenglocken \u00fcber den B\u00e4umen,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es singen die Kirchenglocken \u00fcber den B\u00e4umen,", "tokens": ["Es", "sin\u00b7gen", "die", "Kir\u00b7chen\u00b7glo\u00b7cken", "\u00fc\u00b7ber", "den", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Als ob Fabelv\u00f6gel mit erzenen Schwingen", "tokens": ["Als", "ob", "Fa\u00b7bel\u00b7v\u00f6\u00b7gel", "mit", "er\u00b7ze\u00b7nen", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Aufrauschend durch die Wolken dringen.", "tokens": ["Auf\u00b7rau\u00b7schend", "durch", "die", "Wol\u00b7ken", "drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es h\u00e4ngt der Goldregen seinen Tr\u00e4umen nach,", "tokens": ["Es", "h\u00e4ngt", "der", "Gold\u00b7re\u00b7gen", "sei\u00b7nen", "Tr\u00e4u\u00b7men", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Kastanienkerzen leuchten an den dunkelgr\u00fcnen Wegen", "tokens": ["Die", "Kas\u00b7ta\u00b7ni\u00b7en\u00b7ker\u00b7zen", "leuch\u00b7ten", "an", "den", "dun\u00b7kel\u00b7gr\u00fc\u00b7nen", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.3": {"text": "Und Maiwolken stehen wie fliegende G\u00e4rten \u00fcber dem Dach.", "tokens": ["Und", "Mai\u00b7wol\u00b7ken", "ste\u00b7hen", "wie", "flie\u00b7gen\u00b7de", "G\u00e4r\u00b7ten", "\u00fc\u00b7ber", "dem", "Dach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "KOKOM", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+-+--+", "measure": "amphibrach.tetra.plus"}}, "stanza.3": {"line.1": {"text": "Nun wollen die Menschen nicht nur vor sich hingehen.", "tokens": ["Nun", "wol\u00b7len", "die", "Men\u00b7schen", "nicht", "nur", "vor", "sich", "hin\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PTKNEG", "ADV", "APPR", "PRF", "VVINF", "$."], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie nehmen sich Fl\u00fcgel, wo sie sie finden,", "tokens": ["Sie", "neh\u00b7men", "sich", "Fl\u00fc\u00b7gel", ",", "wo", "sie", "sie", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "NN", "$,", "PWAV", "PPER", "PPER", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und lassen sich gern beim Fliegen die Augen verbinden.", "tokens": ["Und", "las\u00b7sen", "sich", "gern", "beim", "Flie\u00b7gen", "die", "Au\u00b7gen", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}}}}}