{"textgrid.poem.67274": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tr\u00fcb und immer tr\u00fcber ziehet", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tr\u00fcb und immer tr\u00fcber ziehet", "tokens": ["Tr\u00fcb", "und", "im\u00b7mer", "tr\u00fc\u00b7ber", "zie\u00b7het"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADV", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wolk' auf Wolke schwer empor,", "tokens": ["Wol\u00b7k'", "auf", "Wol\u00b7ke", "schwer", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Bleicher Abendschimmer fliehet", "tokens": ["Blei\u00b7cher", "A\u00b7bend\u00b7schim\u00b7mer", "flie\u00b7het"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und kein Sternlein blickt hervor.", "tokens": ["Und", "kein", "Stern\u00b7lein", "blickt", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf des Lebens Ocean", "tokens": ["Auf", "des", "Le\u00b7bens", "O\u00b7cean"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Schwanket einsam hin mein Kahn,", "tokens": ["Schwan\u00b7ket", "ein\u00b7sam", "hin", "mein", "Kahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sorge sitzt am Steu'r, im Wider-", "tokens": ["Sor\u00b7ge", "sitzt", "am", "Steu'r", ",", "im", "Wi\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,", "APPRART", "TRUNC"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Winde rauscht das Segel nieder.", "tokens": ["Win\u00b7de", "rauscht", "das", "Se\u00b7gel", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hoch und immer h\u00f6her th\u00fcrmen", "tokens": ["Hoch", "und", "im\u00b7mer", "h\u00f6\u00b7her", "th\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich die Wogen, tiefer klafft", "tokens": ["Sich", "die", "Wo\u00b7gen", ",", "tie\u00b7fer", "klafft"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PRF", "ART", "NN", "$,", "ADJD", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meeresschlund, in wilden St\u00fcrmen", "tokens": ["Mee\u00b7res\u00b7schlund", ",", "in", "wil\u00b7den", "St\u00fcr\u00b7men"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aufgel\u00f6s't sinkt meine Kraft! \u2013 \u2013 \u2013 \u2013", "tokens": ["Auf\u00b7ge\u00b7l\u00f6s't", "sinkt", "mei\u00b7ne", "Kraft", "!", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$.", "$(", "$(", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zucke, Schmetternder, herab,", "tokens": ["Zu\u00b7cke", ",", "Schmet\u00b7tern\u00b7der", ",", "her\u00b7ab", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Flamm' und Abgrund sei mein Grab,", "tokens": ["Flamm'", "und", "Ab\u00b7grund", "sei", "mein", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Unter Blitz und Donner-Krachen", "tokens": ["Un\u00b7ter", "Blitz", "und", "Don\u00b7ner\u00b7Kra\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf! zerschelle du, mein Nachen!", "tokens": ["Auf", "!", "zer\u00b7schel\u00b7le", "du", ",", "mein", "Na\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Fern die Schmach! des Menschen W\u00fcrde", "tokens": ["Fern", "die", "Schmach", "!", "des", "Men\u00b7schen", "W\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trotzet siegreich dem Geschick,", "tokens": ["Trot\u00b7zet", "sieg\u00b7reich", "dem", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schleudert ab die Tr\u00fcbsalsb\u00fcrde,", "tokens": ["Schleu\u00b7dert", "ab", "die", "Tr\u00fcb\u00b7sals\u00b7b\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schaut mit Hohn hinab auf's Gl\u00fcck;", "tokens": ["Schaut", "mit", "Hohn", "hin\u00b7ab", "auf's", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unsers welken Fittigs Band", "tokens": ["Un\u00b7sers", "wel\u00b7ken", "Fit\u00b7tigs", "Band"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "L\u00f6set nur die eigne Hand,", "tokens": ["L\u00f6\u00b7set", "nur", "die", "eig\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur im innern Herzensspiegel", "tokens": ["Nur", "im", "in\u00b7nern", "Her\u00b7zens\u00b7spie\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Pranget unsrer Hoheit Siegel. \u2013 \u2013 \u2013 \u2013", "tokens": ["Pran\u00b7get", "uns\u00b7rer", "Ho\u00b7heit", "Sie\u00b7gel", ".", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$.", "$(", "$(", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Gr\u00e4uel, Schauder! schone, schone,", "tokens": ["Gr\u00e4u\u00b7el", ",", "Schau\u00b7der", "!", "scho\u00b7ne", ",", "scho\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$.", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blick in's nackte Menschenherz!", "tokens": ["Blick", "in's", "nack\u00b7te", "Men\u00b7schen\u00b7herz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, alsbald vom D\u00fcnkelthrone", "tokens": ["Ach", ",", "als\u00b7bald", "vom", "D\u00fcn\u00b7kelt\u00b7hro\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00fcrzt er uns in Schaam und Schmerz!", "tokens": ["St\u00fcrzt", "er", "uns", "in", "Schaam", "und", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Trotzig und verzagt bist du,", "tokens": ["Trot\u00b7zig", "und", "ver\u00b7zagt", "bist", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVPP", "VAFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Herz, des Menschen! Trost und Ruh'", "tokens": ["Herz", ",", "des", "Men\u00b7schen", "!", "Trost", "und", "Ruh'"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ART", "NN", "$.", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Kann, des Jammers Sturm zu stillen,", "tokens": ["Kann", ",", "des", "Jam\u00b7mers", "Sturm", "zu", "stil\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nicht aus deinem Schlamme quillen!", "tokens": ["Nicht", "aus", "dei\u00b7nem", "Schlam\u00b7me", "quil\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Rettung! wo? \u2013 \u2013 \u2013 Erreicht wohl Klage", "tokens": ["Ret\u00b7tung", "!", "wo", "?", "\u2013", "\u2013", "\u2013", "Er\u00b7reicht", "wohl", "Kla\u00b7ge"], "token_info": ["word", "punct", "word", "punct", "punct", "punct", "punct", "word", "word", "word"], "pos": ["NN", "$.", "PWAV", "$.", "$(", "$(", "$(", "NN", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jenes hohen Weltgeists Ohr,", "tokens": ["Je\u00b7nes", "ho\u00b7hen", "Welt\u00b7geists", "Ohr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der, so lehrt der Schule Sage,", "tokens": ["Der", ",", "so", "lehrt", "der", "Schu\u00b7le", "Sa\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kreiset in der Sph\u00e4ren Chor,", "tokens": ["Krei\u00b7set", "in", "der", "Sph\u00e4\u00b7ren", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Spr\u00fcht im Funken, gr\u00fcnt im Baum,", "tokens": ["Spr\u00fcht", "im", "Fun\u00b7ken", ",", "gr\u00fcnt", "im", "Baum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wiegt sich in der Welle Schaum? \u2013 \u2013 \u2013", "tokens": ["Wiegt", "sich", "in", "der", "Wel\u00b7le", "Schaum", "?", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$.", "$(", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nein! Es beut des Uralls Oede", "tokens": ["Nein", "!", "Es", "beut", "des", "Ur\u00b7alls", "O\u00b7e\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Meinem Anker keine Rhede! \u2013 \u2013 \u2013 \u2013", "tokens": ["Mei\u00b7nem", "An\u00b7ker", "kei\u00b7ne", "Rhe\u00b7de", "!", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct", "punct"], "pos": ["PPOSAT", "NN", "PIAT", "NN", "$.", "$(", "$(", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Heil mir! Wohl an wen ich glaube", "tokens": ["Heil", "mir", "!", "Wohl", "an", "wen", "ich", "glau\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "ADV", "APPR", "PWS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wei\u00df ich, Truggestalten weicht!", "tokens": ["Wei\u00df", "ich", ",", "Trug\u00b7ge\u00b7stal\u00b7ten", "weicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kenn' Ihn, den, wenn ich im Staube", "tokens": ["Kenn'", "Ihn", ",", "den", ",", "wenn", "ich", "im", "Stau\u00b7be"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ART", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flehe, mein Gebet erreicht,", "tokens": ["Fle\u00b7he", ",", "mein", "Ge\u00b7bet", "er\u00b7reicht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihn, der Liebe Urquell, Ihn,", "tokens": ["Ihn", ",", "der", "Lie\u00b7be", "Ur\u00b7quell", ",", "Ihn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "NE", "$,", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Vater ist sein Nam', o Ihn,", "tokens": ["Va\u00b7ter", "ist", "sein", "Nam'", ",", "o", "Ihn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,", "FM", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der im Vaterherzen heget,", "tokens": ["Der", "im", "Va\u00b7ter\u00b7her\u00b7zen", "he\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Was im Sch\u00f6pfungsraum sich reget!", "tokens": ["Was", "im", "Sch\u00f6p\u00b7fungs\u00b7raum", "sich", "re\u00b7get", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Allbarmherzig Er, es bl\u00fche", "tokens": ["All\u00b7barm\u00b7her\u00b7zig", "Er", ",", "es", "bl\u00fc\u00b7he"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJD", "PPER", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ros' und Myrt' auf meinem Pfad,", "tokens": ["Ros'", "und", "Myrt'", "auf", "mei\u00b7nem", "Pfad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder ach in Tr\u00fcbsals M\u00fche", "tokens": ["O\u00b7der", "ach", "in", "Tr\u00fcb\u00b7sals", "M\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schleich' er durch die Distelsaat.", "tokens": ["Schleich'", "er", "durch", "die", "Dis\u00b7tel\u00b7saat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wohl ein selig Loos mir fiel", "tokens": ["Wohl", "ein", "se\u00b7lig", "Loos", "mir", "fiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJD", "NN", "PPER", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "F\u00fchrt es sich'rer mich zum Ziel,", "tokens": ["F\u00fchrt", "es", "sich'\u00b7rer", "mich", "zum", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schweb' ich aus dem L\u00e4ut'rungstiegel", "tokens": ["Schweb'", "ich", "aus", "dem", "L\u00e4ut'\u00b7rungs\u00b7tie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Leichter hin zum Grabes H\u00fcgel!", "tokens": ["Leich\u00b7ter", "hin", "zum", "Gra\u00b7bes", "H\u00fc\u00b7gel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}