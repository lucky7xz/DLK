{"textgrid.poem.38010": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Reiterlied", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach Reitersbrauch ich reite", "tokens": ["Nach", "Rei\u00b7ters\u00b7brauch", "ich", "rei\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein R\u00f6\u00dflein in das Feld,", "tokens": ["Mein", "R\u00f6\u00df\u00b7lein", "in", "das", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Tumml das auf gr\u00fcner Heide", "tokens": ["Tumml", "das", "auf", "gr\u00fc\u00b7ner", "Hei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "APPR", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Werfs rumm auf alle Seiten,", "tokens": ["Werfs", "rumm", "auf", "al\u00b7le", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Mit Spornstreich mach' ich's springreich,", "tokens": ["Mit", "Sporn\u00b7streich", "mach'", "ich's", "sprin\u00b7greich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Das mir dann wol gef\u00e4llt.", "tokens": ["Das", "mir", "dann", "wol", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wann es h\u00f6flich thut traben,", "tokens": ["Wann", "es", "h\u00f6f\u00b7lich", "thut", "tra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Lacht mir das Herze mein,", "tokens": ["Lacht", "mir", "das", "Her\u00b7ze", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "VVFIN", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Artlich Tugend und Gaben", "tokens": ["Art\u00b7lich", "Tu\u00b7gend", "und", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Mein Ro\u00df an sich thut haben,", "tokens": ["Mein", "Ro\u00df", "an", "sich", "thut", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PRF", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auf alle Weis' erlangt es Prei\u00df,", "tokens": ["Auf", "al\u00b7le", "Weis'", "er\u00b7langt", "es", "Prei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zierlich sein Spr\u00fcnge sein.", "tokens": ["Zier\u00b7lich", "sein", "Spr\u00fcn\u00b7ge", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VAINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Im Rennen nicht seines gleichen,", "tokens": ["Im", "Ren\u00b7nen", "nicht", "sei\u00b7nes", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "PPOSAT", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schnell l\u00e4uft es wie der Wind,", "tokens": ["Schnell", "l\u00e4uft", "es", "wie", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "M\u00e4nnlich sichs thut erzeigen,", "tokens": ["M\u00e4nn\u00b7lich", "sichs", "thut", "er\u00b7zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Mit Schlagen und mit Beissen;", "tokens": ["Mit", "Schla\u00b7gen", "und", "mit", "Beis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gegen sein Feind ich sage heint,", "tokens": ["Ge\u00b7gen", "sein", "Feind", "ich", "sa\u00b7ge", "heint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVFIN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Seinesgleichen man nicht findt.", "tokens": ["Sei\u00b7nes\u00b7glei\u00b7chen", "man", "nicht", "findt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn ich bin ans heimreiten,", "tokens": ["Wenn", "ich", "bin", "ans", "heim\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "APPRART", "ADJA", "$,"], "meter": "--+-++-", "measure": "anapaest.init"}, "line.2": {"text": "Schenk ich meim Buhl ein Trab,", "tokens": ["Schenk", "ich", "meim", "Buhl", "ein", "Trab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NE", "NE", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann wirfts den Kopf auf die Seiten,", "tokens": ["Dann", "wirfts", "den", "Kopf", "auf", "die", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Trit auf mit engem Schreiten,", "tokens": ["Trit", "auf", "mit", "en\u00b7gem", "Schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und trabet vor meins Buhlens Th\u00fcr,", "tokens": ["Und", "tra\u00b7bet", "vor", "meins", "Buh\u00b7lens", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie schaut zum Fenster r'aus.", "tokens": ["Sie", "schaut", "zum", "Fens\u00b7ter", "r'\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Thut mich freundlich anlachen,", "tokens": ["Thut", "mich", "freund\u00b7lich", "an\u00b7la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "W\u00fcnscht mir einen guten Tag,", "tokens": ["W\u00fcnscht", "mir", "ei\u00b7nen", "gu\u00b7ten", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was sollt mich in den Sachen", "tokens": ["Was", "sollt", "mich", "in", "den", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Denn dies fr\u00f6licher machen.", "tokens": ["Denn", "dies", "fr\u00f6\u00b7li\u00b7cher", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADJD", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Mein Ro\u00df und Schatz bei mir han Platz,", "tokens": ["Mein", "Ro\u00df", "und", "Schatz", "bei", "mir", "han", "Platz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "APPR", "PPER", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ohn die ich nicht seyn mag.", "tokens": ["Ohn", "die", "ich", "nicht", "seyn", "mag."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "PRELS", "PPER", "PTKNEG", "VAINF", "VMFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}