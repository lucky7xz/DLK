{"textgrid.poem.53619": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Sentimentales Lied", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das war in Kurland, in leuchtender Luft \u2013", "tokens": ["Das", "war", "in", "Kur\u00b7land", ",", "in", "leuch\u00b7ten\u00b7der", "Luft", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "$,", "APPR", "ADJA", "NN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "weit hinten in stiller Etappe,", "tokens": ["weit", "hin\u00b7ten", "in", "stil\u00b7ler", "E\u00b7tap\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "(der Muschkot in alter, verwitterter Kluft,", "tokens": ["(", "der", "Muschkot", "in", "al\u00b7ter", ",", "ver\u00b7wit\u00b7ter\u00b7ter", "Kluft", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "der Leutnant mit gro\u00dfer Klappe \u2013).", "tokens": ["der", "Leut\u00b7nant", "mit", "gro\u00b7\u00dfer", "Klap\u00b7pe", "\u2013", ")", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$(", "$(", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Kommandowechsel. Wir feierten ihn", "tokens": ["Kom\u00b7man\u00b7do\u00b7wech\u00b7sel", ".", "Wir", "fei\u00b7er\u00b7ten", "ihn"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPER"], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "beim Inspektor vom Proviantmagazin.", "tokens": ["beim", "Ins\u00b7pek\u00b7tor", "vom", "Pro\u00b7vi\u00b7ant\u00b7ma\u00b7ga\u00b7zin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Die Dame vom Hilfsdienst, die Tag und Nacht", "tokens": ["Die", "Da\u00b7me", "vom", "Hilfs\u00b7dienst", ",", "die", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "ART", "NN", "KON", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "dem Inspektor den Dienst ertr\u00e4glicher macht,", "tokens": ["dem", "Ins\u00b7pek\u00b7tor", "den", "Dienst", "er\u00b7tr\u00e4g\u00b7li\u00b7cher", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "stand fett am Klavier. Ich spielte dazu:", "tokens": ["stand", "fett", "am", "Kla\u00b7vier", ".", "Ich", "spiel\u00b7te", "da\u00b7zu", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "$.", "PPER", "VVFIN", "PAV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "\u00bbservus du!\u00ab . . .", "tokens": ["\u00bb", "ser\u00b7vus", "du", "!", "\u00ab", ".", ".", "."], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "VAFIN", "PPER", "$.", "$(", "$.", "$.", "$."], "meter": "--+", "measure": "anapaest.init"}, "line.11": {"text": "Und dann ein Lied voll Schmalz und Dramatik,", "tokens": ["Und", "dann", "ein", "Lied", "voll", "Schmalz", "und", "Dra\u00b7ma\u00b7tik", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "voll wilden Geschehens, voll Seelenbatik \u2013", "tokens": ["voll", "wil\u00b7den", "Ge\u00b7sche\u00b7hens", ",", "voll", "See\u00b7len\u00b7ba\u00b7tik", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "noch klingt mir, was sie da gesungen hat:", "tokens": ["noch", "klingt", "mir", ",", "was", "sie", "da", "ge\u00b7sun\u00b7gen", "hat", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "\u00bbim Sumpfe der Gro\u00dfstadt \u2013 ein sinkendes Blatt!\u00ab", "tokens": ["\u00bb", "im", "Sump\u00b7fe", "der", "Gro\u00df\u00b7stadt", "\u2013", "ein", "sin\u00b7ken\u00b7des", "Blatt", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPRART", "NN", "ART", "NN", "$(", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Es leuchten die Lampen. Es summt in der Bar,", "tokens": ["Es", "leuch\u00b7ten", "die", "Lam\u00b7pen", ".", "Es", "summt", "in", "der", "Bar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "grad so, wies im vorigen Frieden war.", "tokens": ["grad", "so", ",", "wies", "im", "vo\u00b7ri\u00b7gen", "Frie\u00b7den", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "VVFIN", "APPRART", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Zigeuner fiedeln im gelben Licht", "tokens": ["Zi\u00b7geu\u00b7ner", "fie\u00b7deln", "im", "gel\u00b7ben", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "(eigentlich sinds gar keine Zigeuner nicht,", "tokens": ["(", "ei\u00b7gent\u00b7lich", "sinds", "gar", "kei\u00b7ne", "Zi\u00b7geu\u00b7ner", "nicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "ADV", "PIAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "sondern Musiker mit dem Reichstarif).", "tokens": ["son\u00b7dern", "Mu\u00b7si\u00b7ker", "mit", "dem", "Reichs\u00b7ta\u00b7rif", ")", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$(", "$."], "meter": "+----+-+-+", "measure": "dactylic.init"}, "line.6": {"text": "Eine blonde Dame lacht sich schief.", "tokens": ["Ei\u00b7ne", "blon\u00b7de", "Da\u00b7me", "lacht", "sich", "schief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Sie kommt vom Film und geht ins Bett", "tokens": ["Sie", "kommt", "vom", "Film", "und", "geht", "ins", "Bett"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "und steht heute mittag in der ", "tokens": ["und", "steht", "heu\u00b7te", "mit\u00b7tag", "in", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPR", "ART"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Gestern war eine Filmpremiere . . .", "tokens": ["Ge\u00b7stern", "war", "ei\u00b7ne", "Film\u00b7pre\u00b7mie\u00b7re", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Alle am Tisch hatten schon die Ehre . . .", "tokens": ["Al\u00b7le", "am", "Tisch", "hat\u00b7ten", "schon", "die", "Eh\u00b7re", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PIAT", "APPRART", "NN", "VAFIN", "ADV", "ART", "NN", "$.", "$.", "$."], "meter": "+--++-+-+-", "measure": "dactylic.init"}, "line.11": {"text": "Sie verdient auf der einen Leinwand im Spiel", "tokens": ["Sie", "ver\u00b7dient", "auf", "der", "ei\u00b7nen", "Lein\u00b7wand", "im", "Spiel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ART", "NN", "APPRART", "NN"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.12": {"text": "t\u00e4glich tausend \u2013", "tokens": ["t\u00e4g\u00b7lich", "tau\u00b7send", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "und auf der andern beinah ebensoviel.", "tokens": ["und", "auf", "der", "an\u00b7dern", "bei\u00b7nah", "e\u00b7ben\u00b7so\u00b7viel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "ADV", "ADV", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.14": {"text": "Der Papa war Portier. Ihre Perle gl\u00e4nzt matt.", "tokens": ["Der", "Pa\u00b7pa", "war", "Por\u00b7tier", ".", "Ih\u00b7re", "Per\u00b7le", "gl\u00e4nzt", "matt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$.", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "\u00bbim Sumpfe der Gro\u00dfstadt \u2013 ein sinkendes Blatt!\u00ab", "tokens": ["\u00bb", "im", "Sump\u00b7fe", "der", "Gro\u00df\u00b7stadt", "\u2013", "ein", "sin\u00b7ken\u00b7des", "Blatt", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPRART", "NN", "ART", "NN", "$(", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Im Zeitungsviertel die Redaktion", "tokens": ["Im", "Zei\u00b7tungs\u00b7vier\u00b7tel", "die", "Re\u00b7dak\u00b7ti\u00b7on"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ist aufgeregt seit dem Morgen schon.", "tokens": ["ist", "auf\u00b7ge\u00b7regt", "seit", "dem", "Mor\u00b7gen", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Gesinnung? Gesinnung? Alles gestorben.", "tokens": ["Ge\u00b7sin\u00b7nung", "?", "Ge\u00b7sin\u00b7nung", "?", "Al\u00b7les", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "PIS", "VVPP", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Blatt wurde gestern nachmittag erworben", "tokens": ["Das", "Blatt", "wur\u00b7de", "ge\u00b7stern", "nach\u00b7mit\u00b7tag", "er\u00b7wor\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "VVPP"], "meter": "-+---+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "von einem Industriesyndikat,", "tokens": ["von", "ei\u00b7nem", "In\u00b7dust\u00b7rie\u00b7syn\u00b7di\u00b7kat", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "das furchtbar viele Millionen hat,", "tokens": ["das", "furcht\u00b7bar", "vie\u00b7le", "Mil\u00b7lion\u00b7en", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Bis heute: gem\u00e4\u00dfigt reaktion\u00e4r.", "tokens": ["Bis", "heu\u00b7te", ":", "ge\u00b7m\u00e4\u00b7\u00dfigt", "re\u00b7ak\u00b7ti\u00b7o\u00b7n\u00e4r", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADV", "$.", "VVPP", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Von heute an: national \u2013 aber sehr!", "tokens": ["Von", "heu\u00b7te", "an", ":", "na\u00b7ti\u00b7o\u00b7nal", "\u2013", "a\u00b7ber", "sehr", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADV", "PTKVZ", "$.", "ADV", "$(", "ADV", "ADV", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Ansprache des Chefs: \u00bbWer nicht mitwill, fliegt!\u00ab", "tokens": ["An\u00b7spra\u00b7che", "des", "Chefs", ":", "\u00bb", "Wer", "nicht", "mit\u00b7will", ",", "fliegt", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "$.", "$(", "PWS", "PTKNEG", "PTKVZ", "$,", "VVFIN", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Jeder das K\u00f6pfchen zweifelnd wiegt . . .", "tokens": ["Je\u00b7der", "das", "K\u00f6pf\u00b7chen", "zwei\u00b7felnd", "wiegt", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PIS", "ART", "NN", "ADJD", "VVFIN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Eine Frau, vier Kinder, tausend Mark \u2013", "tokens": ["Ei\u00b7ne", "Frau", ",", "vier", "Kin\u00b7der", ",", "tau\u00b7send", "Mark", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "CARD", "NN", "$,", "CARD", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "wer bleibt da stark?", "tokens": ["wer", "bleibt", "da", "stark", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ein Engel fliegt leise durch den Raum.", "tokens": ["Ein", "En\u00b7gel", "fliegt", "lei\u00b7se", "durch", "den", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Stinnes Traum . . .", "tokens": ["Stin\u00b7nes", "Traum", ".", ".", "."], "token_info": ["word", "word", "punct", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$.", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Das ist so im menschlichen Leben und Treiben:", "tokens": ["Das", "ist", "so", "im", "menschli\u00b7chen", "Le\u00b7ben", "und", "Trei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "APPRART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die meisten bleiben, die meisten schreiben.", "tokens": ["Die", "meis\u00b7ten", "blei\u00b7ben", ",", "die", "meis\u00b7ten", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVINF", "$,", "PRELS", "PIS", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Keiner merkt was. Die Abonnenten", "tokens": ["Kei\u00b7ner", "merkt", "was", ".", "Die", "A\u00b7bon\u00b7nen\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "PIS", "$.", "ART", "NN"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.8": {"text": "pennen, so wie sie von jeher pennten.", "tokens": ["pen\u00b7nen", ",", "so", "wie", "sie", "von", "je\u00b7her", "penn\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "KOKOM", "PPER", "APPR", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Wie sind die gl\u00fccklich! Nicht jeder hat", "tokens": ["Wie", "sind", "die", "gl\u00fcck\u00b7lich", "!", "Nicht", "je\u00b7der", "hat"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "ADJD", "$.", "PTKNEG", "PIS", "VAFIN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.10": {"text": "im Sumpfe der Gro\u00dfstadt \u2013 ein sinkendes Blatt!", "tokens": ["im", "Sump\u00b7fe", "der", "Gro\u00df\u00b7stadt", "\u2013", "ein", "sin\u00b7ken\u00b7des", "Blatt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}