{"textgrid.poem.43598": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Soldatenlied", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein scheckiges Pferd,", "tokens": ["Ein", "sche\u00b7cki\u00b7ges", "Pferd", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Ein blankes Gewehr", "tokens": ["Ein", "blan\u00b7kes", "Ge\u00b7wehr"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und ein h\u00f6lzernes Schwert,", "tokens": ["Und", "ein", "h\u00f6l\u00b7zer\u00b7nes", "Schwert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Was braucht man denn mehr?", "tokens": ["Was", "braucht", "man", "denn", "mehr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIS", "ADV", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Ich bin ein Soldat,", "tokens": ["Ich", "bin", "ein", "Sol\u00b7dat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Man sieht's mir wohl an,", "tokens": ["Man", "sieht's", "mir", "wohl", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ich maschire schon grad',", "tokens": ["Ich", "ma\u00b7schi\u00b7re", "schon", "grad'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+---+", "measure": "dactylic.init"}, "line.4": {"text": "Halt' Schritt wie ein Mann.", "tokens": ["Halt'", "Schritt", "wie", "ein", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Mit trotzigem Muth", "tokens": ["Mit", "trot\u00b7zi\u00b7gem", "Muth"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Zieh' Morgens ich aus,", "tokens": ["Zieh'", "Mor\u00b7gens", "ich", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPER", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Kehre freundlich und gut", "tokens": ["Keh\u00b7re", "freund\u00b7lich", "und", "gut"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "ADJD"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Um Mittag nach Haus.", "tokens": ["Um", "Mit\u00b7tag", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "APPR", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "So wird exercirt", "tokens": ["So", "wird", "ex\u00b7er\u00b7cirt"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "VVPP"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Zum Abend noch spat,", "tokens": ["Zum", "A\u00b7bend", "noch", "spat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Bis der Schlaf commandirt:", "tokens": ["Bis", "der", "Schlaf", "com\u00b7man\u00b7dirt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "\u00bbzu Bett, Kamerad!\u00ab", "tokens": ["\u00bb", "zu", "Bett", ",", "Ka\u00b7me\u00b7rad", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "APPR", "NN", "$,", "NN", "$.", "$("], "meter": "-++-+", "measure": "unknown.measure.tri"}}}}}