{"textgrid.poem.45867": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[ein Ritter an dem Rheine ritt]", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Ritter an dem Rheine ritt", "tokens": ["Ein", "Rit\u00b7ter", "an", "dem", "Rhei\u00b7ne", "ritt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In dunkler Nacht dahin,", "tokens": ["In", "dunk\u00b7ler", "Nacht", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Ritterlein, das reitet mit", "tokens": ["Ein", "Rit\u00b7ter\u00b7lein", ",", "das", "rei\u00b7tet", "mit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PDS", "VVFIN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und fragt: wohin dein Sinn?", "tokens": ["Und", "fragt", ":", "wo\u00b7hin", "dein", "Sinn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PWAV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mein Sinn, der steht nach Minnen,", "tokens": ["Mein", "Sinn", ",", "der", "steht", "nach", "Min\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich hab' mich rumgeschlagen,", "tokens": ["Ich", "hab'", "mich", "rum\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und konnt' doch nichts gewinnen,", "tokens": ["Und", "konnt'", "doch", "nichts", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und mu\u00dft' das Leben wagen.", "tokens": ["Und", "mu\u00dft'", "das", "Le\u00b7ben", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ei hast du nicht die Ehr' davon?", "tokens": ["Ei", "hast", "du", "nicht", "die", "Ehr'", "da\u00b7von", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Ehr' ist hohes Gut \u2013", "tokens": ["Die", "Ehr'", "ist", "ho\u00b7hes", "Gut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich h\u00e4tt' die liebe Zeit davon,", "tokens": ["Ich", "h\u00e4tt'", "die", "lie\u00b7be", "Zeit", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Ehr' ist mir kein Gut. \u2013", "tokens": ["Die", "Ehr'", "ist", "mir", "kein", "Gut", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mein Blut ist hingeflossen", "tokens": ["Mein", "Blut", "ist", "hin\u00b7ge\u00b7flos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Rot zu der Erde nieder,", "tokens": ["Rot", "zu", "der", "Er\u00b7de", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "So warm ich es vergossen,", "tokens": ["So", "warm", "ich", "es", "ver\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Giebt mir's die Ehr' nicht wieder.", "tokens": ["Giebt", "mir's", "die", "Ehr'", "nicht", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da sprach das kleine Ritterlein:", "tokens": ["Da", "sprach", "das", "klei\u00b7ne", "Rit\u00b7ter\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df Gott sich dein erbarm'!", "tokens": ["Da\u00df", "Gott", "sich", "dein", "er\u00b7barm'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du mu\u00dft ein schlechter Ritter sein,", "tokens": ["Du", "mu\u00dft", "ein", "schlech\u00b7ter", "Rit\u00b7ter", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil deine Ehr' so arm. \u2013", "tokens": ["Weil", "dei\u00b7ne", "Ehr'", "so", "arm", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich will nun mit dir rechten,", "tokens": ["Ich", "will", "nun", "mit", "dir", "rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPER", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weil du nicht ehrst die Ehre;", "tokens": ["Weil", "du", "nicht", "ehrst", "die", "Eh\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein Ehr' will ich verfechten,", "tokens": ["Mein", "Ehr'", "will", "ich", "ver\u00b7fech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Setz deine nur zur Wehre.", "tokens": ["Setz", "dei\u00b7ne", "nur", "zur", "Weh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Des Ritters Unwill war sehr gro\u00df,", "tokens": ["Des", "Rit\u00b7ters", "Un\u00b7will", "war", "sehr", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum er vom Rosse sprang,", "tokens": ["Drum", "er", "vom", "Ros\u00b7se", "sprang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auch machet sich der kleine los", "tokens": ["Auch", "ma\u00b7chet", "sich", "der", "klei\u00b7ne", "los"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sich zur Erde schwang. \u2013", "tokens": ["Und", "sich", "zur", "Er\u00b7de", "schwang", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PRF", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Da f\u00fchlt sich der Geselle", "tokens": ["Da", "f\u00fchlt", "sich", "der", "Ge\u00b7sel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von hinten fest umwinden,", "tokens": ["Von", "hin\u00b7ten", "fest", "um\u00b7win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es ist die Nacht nicht helle,", "tokens": ["Es", "ist", "die", "Nacht", "nicht", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKNEG", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie streiten wie die Blinden.", "tokens": ["Sie", "strei\u00b7ten", "wie", "die", "Blin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und sinken beide in den Klee \u2013", "tokens": ["Und", "sin\u00b7ken", "bei\u00b7de", "in", "den", "Klee", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ei sprich! wer hat gesiegt!", "tokens": ["Ei", "sprich", "!", "wer", "hat", "ge\u00b7siegt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$.", "PWS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Ritter ohne Ach und Weh \u2013", "tokens": ["Der", "Rit\u00b7ter", "oh\u00b7ne", "Ach", "und", "Weh", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei einer Jungfrau liegt.", "tokens": ["Bei", "ei\u00b7ner", "Jung\u00b7frau", "liegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ei hast du nicht die Ehr' davon?", "tokens": ["Ei", "hast", "du", "nicht", "die", "Ehr'", "da\u00b7von", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Ehr' ist hohes Gut \u2013", "tokens": ["Die", "Ehr'", "ist", "ho\u00b7hes", "Gut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich h\u00e4tt' die liebe Zeit davon,", "tokens": ["Ich", "h\u00e4tt'", "die", "lie\u00b7be", "Zeit", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Ehr' ist mir kein Gut. \u2013", "tokens": ["Die", "Ehr'", "ist", "mir", "kein", "Gut", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}