{"textgrid.poem.47908": {"metadata": {"author": {"name": "Klaj, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gustav/ das L\u00f6wenhertz/ das Himmelab war kommen/", "genre": "verse", "period": "N.A.", "pub_year": 1636, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gustav/ das L\u00f6wenhertz/ das Himmelab war kommen/", "tokens": ["Gus\u00b7tav", "/", "das", "L\u00f6\u00b7wen\u00b7hertz", "/", "das", "Him\u00b7mel\u00b7ab", "war", "kom\u00b7men", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ART", "NN", "$(", "ART", "NN", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ist Himmel aufgenommen/", "tokens": ["ist", "Him\u00b7mel", "auf\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "nun gl\u00e4ntzt Er feuerrot in Sternenblancker Au/", "tokens": ["nun", "gl\u00e4ntzt", "Er", "feu\u00b7er\u00b7rot", "in", "Ster\u00b7nen\u00b7blan\u00b7cker", "Au", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "lebt in dem G\u00f6tterbau.", "tokens": ["lebt", "in", "dem", "G\u00f6t\u00b7ter\u00b7bau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Orion der ist k\u00fchn/ k\u00fchn/ mannhafft und verwegen/", "tokens": ["O\u00b7rion", "der", "ist", "k\u00fchn", "/", "k\u00fchn", "/", "mann\u00b7hafft", "und", "ver\u00b7we\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "VAFIN", "ADJD", "$(", "ADJD", "$(", "VVFIN", "KON", "ADJD", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "ficht nun mit zweyen Degen/", "tokens": ["ficht", "nun", "mit", "zwe\u00b7yen", "De\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "VVFIN", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "weil Erden/ den Gustav im Sterben abgelegt/", "tokens": ["weil", "Er\u00b7den", "/", "den", "Gus\u00b7tav", "im", "Ster\u00b7ben", "ab\u00b7ge\u00b7legt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$(", "ART", "NE", "APPRART", "NN", "VVPP", "$("], "meter": "-+--++-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "an seiner Seite tr\u00e4gt.", "tokens": ["an", "sei\u00b7ner", "Sei\u00b7te", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Christina flinckt und blinckt/ die helle Jungfer\u00e4hre/", "tokens": ["Chris\u00b7ti\u00b7na", "flinckt", "und", "blinckt", "/", "die", "hel\u00b7le", "Jung\u00b7fe\u00b7r\u00e4h\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "$(", "ART", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.10": {"text": "beleuchtet sich im Meere/", "tokens": ["be\u00b7leuch\u00b7tet", "sich", "im", "Mee\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "umh\u00fcllt das braune Kleid de\u00df Nordenlandes Nacht/", "tokens": ["um\u00b7h\u00fcllt", "das", "brau\u00b7ne", "Kleid", "de\u