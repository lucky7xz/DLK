 "iambic.tri"}, "line.17": {"text": "Wie bey de\u00df Ochsen Haubt sechs nachterleuchter Kertzen", "tokens": ["Wie", "bey", "de\u00df", "Och\u00b7sen", "Haubt", "sechs", "nach\u00b7ter\u00b7leuch\u00b7ter", "Kert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "sch\u00f6n miteinander schertzen;", "tokens": ["sch\u00f6n", "mi\u00b7tein\u00b7an\u00b7der", "schert\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.19": {"text": "Doch leuchtet allen vor das Aug/ das an der Stirn", "tokens": ["Doch", "leuch\u00b7tet", "al\u00b7len", "vor", "das", "Aug", "/", "das", "an", "der", "Stirn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIS", "APPR", "ART", "NN", "$(", "ART", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "begl\u00e4ntzet sein Gehirn:", "tokens": ["be\u00b7gl\u00e4nt\u00b7zet", "sein", "Ge\u00b7hirn", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "So/ Hochgeborner Herr/ f\u00fchrt Euer hoch Geschlechte", "tokens": ["So", "/", "Hoch\u00b7ge\u00b7bor\u00b7ner", "Herr", "/", "f\u00fchrt", "Eu\u00b7er", "hoch", "Ge\u00b7schlech\u00b7te"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$(", "ADJA", "NN", "$(", "VVFIN", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "den hohen Ruhm mit Rechte/", "tokens": ["den", "ho\u00b7hen", "Ruhm", "mit", "Rech\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "da\u00df es de\u00df Nordreichs sey Sternabgeschenckte Zier", "tokens": ["da\u00df", "es", "de\u00df", "Nord\u00b7reichs", "sey", "Ster\u00b7nab\u00b7ge\u00b7schenck\u00b7te", "Zier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "und de\u00df Geschlechtes Ihr;", "tokens": ["und", "de\u00df", "Ge\u00b7schlech\u00b7tes", "Ihr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Wann Er die Kriegesnacht/ die Nebelna\u00df befeuchtet/", "tokens": ["Wann", "Er", "die", "Krie\u00b7ges\u00b7nacht", "/", "die", "Ne\u00b7bel\u00b7na\u00df", "be\u00b7feuch\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "$(", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "mit seinem Tag erleuchtet/", "tokens": ["mit", "sei\u00b7nem", "Tag", "er\u00b7leuch\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "weil seine wache Stirn gesetzet Meer und Land", "tokens": ["weil", "sei\u00b7ne", "wa\u00b7che", "Stirn", "ge\u00b7set\u00b7zet", "Meer", "und", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVPP", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "in vollen Friedestand.", "tokens": ["in", "vol\u00b7len", "Frie\u00b7des\