{"textgrid.poem.49723": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "S\u00fcdtirol", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Tirol die Kapuziner", "tokens": ["In", "Ti\u00b7rol", "die", "Ka\u00b7pu\u00b7zi\u00b7ner"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Haben hochgew\u00f6lbte B\u00e4uche,", "tokens": ["Ha\u00b7ben", "hoch\u00b7ge\u00b7w\u00f6lb\u00b7te", "B\u00e4u\u00b7che", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind des Herren fromme Diener", "tokens": ["Sind", "des", "Her\u00b7ren", "from\u00b7me", "Die\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mit Wein gef\u00fcllte Schl\u00e4uche.", "tokens": ["Und", "mit", "Wein", "ge\u00b7f\u00fcll\u00b7te", "Schl\u00e4u\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In Tirol die Jesuiten", "tokens": ["In", "Ti\u00b7rol", "die", "Je\u00b7su\u00b7i\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Haben runde Angesichter,", "tokens": ["Ha\u00b7ben", "run\u00b7de", "An\u00b7ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch die Patres Karmeliter", "tokens": ["Auch", "die", "Pat\u00b7res", "Kar\u00b7me\u00b7li\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind gar fette Kirchenlichter.", "tokens": ["Sind", "gar", "fet\u00b7te", "Kir\u00b7chen\u00b7lich\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Reichlich gibt der Kirchenfiskus,", "tokens": ["Reich\u00b7lich", "gibt", "der", "Kir\u00b7chen\u00b7fis\u00b7kus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und gef\u00fcllt sind seine Spinde.", "tokens": ["Und", "ge\u00b7f\u00fcllt", "sind", "sei\u00b7ne", "Spin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch die J\u00fcnger vom Franziskus", "tokens": ["Auch", "die", "J\u00fcn\u00b7ger", "vom", "Fran\u00b7zis\u00b7kus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind wie runde Fa\u00dfgebinde.", "tokens": ["Sind", "wie", "run\u00b7de", "Fa\u00df\u00b7ge\u00b7bin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn sie so in milder G\u00fcte", "tokens": ["Wenn", "sie", "so", "in", "mil\u00b7der", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Wohlgef\u00fcllt vor\u00fcbergehen,", "tokens": ["Wohl\u00b7ge\u00b7f\u00fcllt", "vor\u00b7\u00fc\u00b7ber\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zieht das brave Volk die H\u00fcte,", "tokens": ["Zieht", "das", "bra\u00b7ve", "Volk", "die", "H\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleibt in tiefer Ehrfurcht stehen.", "tokens": ["Bleibt", "in", "tie\u00b7fer", "Ehr\u00b7furcht", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach! Man sieht im Volke leider", "tokens": ["Ach", "!", "Man", "sieht", "im", "Vol\u00b7ke", "lei\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PIS", "VVFIN", "APPRART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur die magersten Gestalten;", "tokens": ["Nur", "die", "ma\u00b7gers\u00b7ten", "Ge\u00b7stal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Schlotternd sitzen alle Kleider,", "tokens": ["Schlot\u00b7ternd", "sit\u00b7zen", "al\u00b7le", "Klei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn der Leib kann sie nicht halten.", "tokens": ["Denn", "der", "Leib", "kann", "sie", "nicht", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Braves Volk, das so verachtet", "tokens": ["Bra\u00b7ves", "Volk", ",", "das", "so", "ver\u00b7ach\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diese schalen Erdeng\u00fcter", "tokens": ["Die\u00b7se", "scha\u00b7len", "Er\u00b7den\u00b7g\u00fc\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den Platz im Himmel pachtet", "tokens": ["Und", "den", "Platz", "im", "Him\u00b7mel", "pach\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die dicken Seelenh\u00fcter!", "tokens": ["Durch", "die", "di\u00b7cken", "See\u00b7len\u00b7h\u00fc\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}