{"textgrid.poem.58949": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "168. Kauf die Zeit aus!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Gott am liebsten einsam sei,", "tokens": ["Mit", "Gott", "am", "liebs\u00b7ten", "ein\u00b7sam", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKA", "ADJD", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei Menschen wird die Zeit gestohlen!", "tokens": ["Bei", "Men\u00b7schen", "wird", "die", "Zeit", "ge\u00b7stoh\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein St\u00fcndchen geht so bald vorbei,", "tokens": ["Ein", "St\u00fcnd\u00b7chen", "geht", "so", "bald", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Man kann's hernach nicht wieder holen.", "tokens": ["Man", "kann's", "her\u00b7nach", "nicht", "wie\u00b7der", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}