{"dta.poem.4033": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Lebenslauf .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Gr\u00f6\u00dferes wolltest auch du, aber die Liebe zwingt", "tokens": ["Gr\u00f6\u00b7\u00dfe\u00b7res", "woll\u00b7test", "auch", "du", ",", "a\u00b7ber", "die", "Lie\u00b7be", "zwingt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "ADV", "PPER", "$,", "ADV", "ART", "NN", "VVFIN"], "meter": "+--+--++--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "All' uns nieder, das Leid beuget gewaltiger,", "tokens": ["All'", "uns", "nie\u00b7der", ",", "das", "Leid", "beu\u00b7get", "ge\u00b7wal\u00b7ti\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "PTKVZ", "$,", "ART", "NN", "VVFIN", "ADJA", "$,"], "meter": "+-+--++--+--", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und es kehret umsonst nicht", "tokens": ["Und", "es", "keh\u00b7ret", "um\u00b7sonst", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unser Bogen, woher er kommt.", "tokens": ["Un\u00b7ser", "Bo\u00b7gen", ",", "wo\u00b7her", "er", "kommt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Aufw\u00e4rts oder hinab! wehet in heil'ger Nacht, \u2012", "tokens": ["Auf\u00b7w\u00e4rts", "o\u00b7der", "hin\u00b7ab", "!", "we\u00b7het", "in", "heil'\u00b7ger", "Nacht", ",", "\u2012"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "KON", "PTKVZ", "$.", "VVFIN", "APPR", "ADJA", "NN", "$,", "$("], "meter": "+-+--+---+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wo die stumme Natur werdende Tage sinnt,", "tokens": ["Wo", "die", "stum\u00b7me", "Na\u00b7tur", "wer\u00b7den\u00b7de", "Ta\u00b7ge", "sinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Weht im n\u00fcchternen Orkus", "tokens": ["Weht", "im", "n\u00fcch\u00b7ter\u00b7nen", "Or\u00b7kus"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Nicht ein liebender Athem auch?", "tokens": ["Nicht", "ein", "lie\u00b7ben\u00b7der", "A\u00b7them", "auch", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Die\u00df erfuhr ich. Denn nie, sterblichen Meistern", "tokens": ["Die\u00df", "er\u00b7fuhr", "ich", ".", "Denn", "nie", ",", "sterb\u00b7li\u00b7chen", "Meis\u00b7tern"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "$.", "KON", "ADV", "$,", "ADJA", "NN"], "meter": "+-+---+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "gleich,", "tokens": ["gleich", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Habt ihr Himmlischen, ihr Alleserhaltenden,", "tokens": ["Habt", "ihr", "Himm\u00b7li\u00b7schen", ",", "ihr", "Al\u00b7le\u00b7ser\u00b7hal\u00b7ten\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Da\u00df ich w\u00fc\u00dfte, mit Vorsicht,", "tokens": ["Da\u00df", "ich", "w\u00fc\u00df\u00b7te", ",", "mit", "Vor\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "APPR", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Mich des ebenen Pfads gef\u00fchrt.", "tokens": ["Mich", "des", "e\u00b7be\u00b7nen", "Pfads", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Alles pr\u00fcfe der Mensch, sagen die Himmlischen,", "tokens": ["Al\u00b7les", "pr\u00fc\u00b7fe", "der", "Mensch", ",", "sa\u00b7gen", "die", "Himm\u00b7li\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Da\u00df er, kr\u00e4ftig gen\u00e4hrt, danken f\u00fcr Alles lern',", "tokens": ["Da\u00df", "er", ",", "kr\u00e4f\u00b7tig", "ge\u00b7n\u00e4hrt", ",", "dan\u00b7ken", "f\u00fcr", "Al\u00b7les", "lern'", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJD", "VVPP", "$,", "VVFIN", "APPR", "PIS", "VVFIN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und verstehe die Freiheit,", "tokens": ["Und", "ver\u00b7ste\u00b7he", "die", "Frei\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Aufzubrechen, wohin er will.", "tokens": ["Auf\u00b7zu\u00b7bre\u00b7chen", ",", "wo\u00b7hin", "er", "will", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}