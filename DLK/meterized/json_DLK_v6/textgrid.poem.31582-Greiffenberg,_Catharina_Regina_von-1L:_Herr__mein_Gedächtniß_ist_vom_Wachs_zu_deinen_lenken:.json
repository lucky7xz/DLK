{"textgrid.poem.31582": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr / mein Ged\u00e4chtni\u00df ist vom Wachs zu deinen lenken:", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr / mein Ged\u00e4chtni\u00df ist vom Wachs zu deinen lenken:", "tokens": ["Herr", "/", "mein", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "ist", "vom", "Wachs", "zu", "dei\u00b7nen", "len\u00b7ken", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPOSAT", "NN", "VAFIN", "APPRART", "NN", "APPR", "PPOSAT", "VVINF", "$."], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "pr\u00e4g deines Willensbild / dein Wunderkrafft / hinein:", "tokens": ["pr\u00e4g", "dei\u00b7nes", "Wil\u00b7lens\u00b7bild", "/", "dein", "Wun\u00b7der\u00b7krafft", "/", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$(", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es soll / als wie ein Stahl / ganz unaustilglich seyn.", "tokens": ["Es", "soll", "/", "als", "wie", "ein", "Stahl", "/", "ganz", "un\u00b7au\u00b7stilg\u00b7lich", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "KOUS", "KOKOM", "ART", "NN", "$(", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wollst / H\u00f6chster / mir dein Blut an Goldesstatt / einsenken.", "tokens": ["Wollst", "/", "H\u00f6chs\u00b7ter", "/", "mir", "dein", "Blut", "an", "Gol\u00b7des\u00b7statt", "/", "ein\u00b7sen\u00b7ken", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$(", "NN", "$(", "PPER", "PPOSAT", "NN", "APPR", "NN", "$(", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auch meinen Willen will ich willig dir / Herr / schenken:", "tokens": ["Auch", "mei\u00b7nen", "Wil\u00b7len", "will", "ich", "wil\u00b7lig", "dir", "/", "Herr", "/", "schen\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "PPER", "ADJD", "PPER", "$(", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nur deines Worts Befehl er folgen soll allein:", "tokens": ["Nur", "dei\u00b7nes", "Worts", "Be\u00b7fehl", "er", "fol\u00b7gen", "soll", "al\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "PPER", "VVINF", "VMFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "in ihm dein Gnadenbild und Wunder-Art erschein'!", "tokens": ["in", "ihm", "dein", "Gna\u00b7den\u00b7bild", "und", "Wun\u00b7der\u00b7Art", "er\u00b7schein'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "er diene dir mit Lust / wie sie mit st\u00e4tem denken!", "tokens": ["er", "die\u00b7ne", "dir", "mit", "Lust", "/", "wie", "sie", "mit", "st\u00e4\u00b7tem", "den\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$(", "PWAV", "PPER", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "ingleichen der Verstand / soll ob der Sonnen H\u00fctten /", "tokens": ["in\u00b7glei\u00b7chen", "der", "Ver\u00b7stand", "/", "soll", "ob", "der", "Son\u00b7nen", "H\u00fct\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$(", "VMFIN", "KOUS", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "ja ob Saturnus Krei\u00df / mit seiner Sch\u00e4rffe gehn!", "tokens": ["ja", "ob", "Sa\u00b7tur\u00b7nus", "Krei\u00df", "/", "mit", "sei\u00b7ner", "Sch\u00e4rf\u00b7fe", "gehn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NE", "NN", "$(", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "doch / werd' ich es vor gut zu deiner Ehr ersehn /", "tokens": ["doch", "/", "werd'", "ich", "es", "vor", "gut", "zu", "dei\u00b7ner", "Ehr", "er\u00b7sehn", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "VAFIN", "PPER", "PPER", "APPR", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So leg ich ihn in Staub / wie weit er auch geschritten.", "tokens": ["So", "leg", "ich", "ihn", "in", "Staub", "/", "wie", "weit", "er", "auch", "ge\u00b7schrit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "PPER", "PPER", "APPR", "NN", "$(", "PWAV", "ADJD", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "kurz / all mein dichten soll zu deiner Ehr geschehn.", "tokens": ["kurz", "/", "all", "mein", "dich\u00b7ten", "soll", "zu", "dei\u00b7ner", "Ehr", "ge\u00b7schehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$(", "PIAT", "PPOSAT", "ADJA", "VMFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "La\u00df mich die Krafft hierzu / Allkrafft von dir erbitten.", "tokens": ["La\u00df", "mich", "die", "Krafft", "hier\u00b7zu", "/", "All\u00b7krafft", "von", "dir", "er\u00b7bit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "PAV", "$(", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}