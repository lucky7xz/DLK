{"textgrid.poem.42582": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "6. Am 18. Oktober 1816", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn heut ein Geist herniederstiege,", "tokens": ["Wenn", "heut", "ein", "Geist", "her\u00b7nie\u00b7ders\u00b7tie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zugleich ein S\u00e4nger und ein Held,", "tokens": ["Zu\u00b7gleich", "ein", "S\u00e4n\u00b7ger", "und", "ein", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein solcher, der im heil'gen Kriege", "tokens": ["Ein", "sol\u00b7cher", ",", "der", "im", "heil'\u00b7gen", "Krie\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gefallen auf dem Siegesfeld,", "tokens": ["Ge\u00b7fal\u00b7len", "auf", "dem", "Sie\u00b7ges\u00b7feld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der s\u00e4nge wohl auf deutscher Erde", "tokens": ["Der", "s\u00e4n\u00b7ge", "wohl", "auf", "deut\u00b7scher", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein scharfes Lied, wie Schwertesstreich,", "tokens": ["Ein", "schar\u00b7fes", "Lied", ",", "wie", "Schwer\u00b7tes\u00b7streich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht so, wie ich es k\u00fcnden werde,", "tokens": ["Nicht", "so", ",", "wie", "ich", "es", "k\u00fcn\u00b7den", "wer\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "PWAV", "PPER", "PPER", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nein! himmelskr\u00e4ftig, donnergleich:", "tokens": ["Nein", "!", "him\u00b7mels\u00b7kr\u00e4f\u00b7tig", ",", "don\u00b7ner\u00b7gleich", ":"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["PTKANT", "$.", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbman sprach einmal von Festgel\u00e4ute,", "tokens": ["\u00bb", "man", "sprach", "ein\u00b7mal", "von", "Fest\u00b7ge\u00b7l\u00e4u\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Man sprach von einem Feuermeer,", "tokens": ["Man", "sprach", "von", "ei\u00b7nem", "Feu\u00b7er\u00b7meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch was das gro\u00dfe Fest bedeute,", "tokens": ["Doch", "was", "das", "gro\u00b7\u00dfe", "Fest", "be\u00b7deu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wei\u00df es denn jetzt noch irgendwer?", "tokens": ["Wei\u00df", "es", "denn", "jetzt", "noch", "ir\u00b7gend\u00b7wer", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wohl m\u00fcssen Geister niedersteigen,", "tokens": ["Wohl", "m\u00fcs\u00b7sen", "Geis\u00b7ter", "nie\u00b7ders\u00b7tei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von heil'gem Eifer aufgeregt,", "tokens": ["Von", "heil'\u00b7gem", "Ei\u00b7fer", "auf\u00b7ge\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und ihre Wundenmale zeigen,", "tokens": ["Und", "ih\u00b7re", "Wun\u00b7den\u00b7ma\u00b7le", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df ihr darein die Finger legt.", "tokens": ["Da\u00df", "ihr", "da\u00b7rein", "die", "Fin\u00b7ger", "legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihr F\u00fcrsten! seid zuerst befraget:", "tokens": ["Ihr", "F\u00fcrs\u00b7ten", "!", "seid", "zu\u00b7erst", "be\u00b7fra\u00b7get", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verga\u00dft ihr jenen Tag der Schlacht,", "tokens": ["Ver\u00b7ga\u00dft", "ihr", "je\u00b7nen", "Tag", "der", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An dem ihr auf den Knieen laget", "tokens": ["An", "dem", "ihr", "auf", "den", "Kni\u00b7e\u00b7en", "la\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Und huldigtet der h\u00f6hern Macht?", "tokens": ["Und", "hul\u00b7dig\u00b7tet", "der", "h\u00f6\u00b7hern", "Macht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn eure Schmach die V\u00f6lker l\u00f6sten,", "tokens": ["Wenn", "eu\u00b7re", "Schmach", "die", "V\u00f6l\u00b7ker", "l\u00f6s\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn ihre Treue sie erprobt,", "tokens": ["Wenn", "ih\u00b7re", "Treu\u00b7e", "sie", "er\u00b7probt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So ist's an euch, nicht zu vertr\u00f6sten,", "tokens": ["So", "ist's", "an", "euch", ",", "nicht", "zu", "ver\u00b7tr\u00f6s\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "$,", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Zu ", "tokens": ["Zu"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}}, "stanza.4": {"line.1": {"text": "Ihr V\u00f6lker! die ihr ", "tokens": ["Ihr", "V\u00f6l\u00b7ker", "!", "die", "ihr"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PRELS", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Verga\u00dft auch ihr den schw\u00fclen Tag?", "tokens": ["Ver\u00b7ga\u00dft", "auch", "ihr", "den", "schw\u00fc\u00b7len", "Tag", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Herrlichste, was ihr erstritten,", "tokens": ["Das", "Herr\u00b7lichs\u00b7te", ",", "was", "ihr", "er\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie kommt's, da\u00df es nicht frommen mag?", "tokens": ["Wie", "kommt's", ",", "da\u00df", "es", "nicht", "from\u00b7men", "mag", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zermalmt habt ihr die fremden Horden,", "tokens": ["Zer\u00b7malmt", "habt", "ihr", "die", "frem\u00b7den", "Hor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch innen hat sich nichts gehellt,", "tokens": ["Doch", "in\u00b7nen", "hat", "sich", "nichts", "ge\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PRF", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und Freie seid ihr nicht geworden,", "tokens": ["Und", "Frei\u00b7e", "seid", "ihr", "nicht", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "PTKNEG", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn ihr das Recht nicht festgestellt.", "tokens": ["Wenn", "ihr", "das", "Recht", "nicht", "fest\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ihr Weisen! mu\u00df man euch berichten,", "tokens": ["Ihr", "Wei\u00b7sen", "!", "mu\u00df", "man", "euch", "be\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VMFIN", "PIS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihr doch alles wissen wollt,", "tokens": ["Die", "ihr", "doch", "al\u00b7les", "wis\u00b7sen", "wollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie die Einf\u00e4ltigen und Schlichten", "tokens": ["Wie", "die", "Ein\u00b7f\u00e4l\u00b7ti\u00b7gen", "und", "Schlich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr klares Recht ihr Blut gezollt?", "tokens": ["F\u00fcr", "kla\u00b7res", "Recht", "ihr", "Blut", "ge\u00b7zollt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Meint ihr, da\u00df in den hei\u00dfen Gluten", "tokens": ["Meint", "ihr", ",", "da\u00df", "in", "den", "hei\u00b7\u00dfen", "Glu\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Zeit, ein Ph\u00f6nix, sich erneut,", "tokens": ["Die", "Zeit", ",", "ein", "Ph\u00f6\u00b7nix", ",", "sich", "er\u00b7neut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nur um die Eier auszubruten,", "tokens": ["Nur", "um", "die", "Ei\u00b7er", "aus\u00b7zu\u00b7bru\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die ihr gesch\u00e4ftig unterstreut?", "tokens": ["Die", "ihr", "ge\u00b7sch\u00e4f\u00b7tig", "un\u00b7ter\u00b7streut", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ihr F\u00fcrstenr\u00e4t' und Hofmarsch\u00e4lle", "tokens": ["Ihr", "F\u00fcrs\u00b7ten\u00b7r\u00e4t'", "und", "Hof\u00b7marsc\u00b7h\u00e4l\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit tr\u00fcbem Stern auf kalter Brust,", "tokens": ["Mit", "tr\u00fc\u00b7bem", "Stern", "auf", "kal\u00b7ter", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die ihr vom Kampf um Leipzigs W\u00e4lle", "tokens": ["Die", "ihr", "vom", "Kampf", "um", "Leip\u00b7zigs", "W\u00e4l\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPRART", "NN", "APPR", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl gar bis heute nichts gewu\u00dft,", "tokens": ["Wohl", "gar", "bis", "heu\u00b7te", "nichts", "ge\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADV", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vernehmt! an diesem heut'gen Tage", "tokens": ["Ver\u00b7nehmt", "!", "an", "die\u00b7sem", "heut'\u00b7gen", "Ta\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hielt Gott der Herr ein gro\u00df Gericht.", "tokens": ["Hielt", "Gott", "der", "Herr", "ein", "gro\u00df", "Ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "NN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u2013 Ihr aber h\u00f6rt nicht, was ich sage,", "tokens": ["\u2013", "Ihr", "a\u00b7ber", "h\u00f6rt", "nicht", ",", "was", "ich", "sa\u00b7ge", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADV", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Ihr glaubt an Geisterstimmen nicht.", "tokens": ["Ihr", "glaubt", "an", "Geis\u00b7ters\u00b7tim\u00b7men", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Was ich gesollt, hab ich gesungen,", "tokens": ["Was", "ich", "ge\u00b7sollt", ",", "hab", "ich", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wieder schwing ich mich empor,", "tokens": ["Und", "wie\u00b7der", "schwing", "ich", "mich", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was meinem Blick sich aufgedrungen,", "tokens": ["Was", "mei\u00b7nem", "Blick", "sich", "auf\u00b7ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PRF", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verk\u00fcnd ich dort dem sel'gen Chor:", "tokens": ["Ver\u00b7k\u00fcnd", "ich", "dort", "dem", "sel'\u00b7gen", "Chor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht r\u00fchmen kann ich, nicht verdammen,", "tokens": ["Nicht", "r\u00fch\u00b7men", "kann", "ich", ",", "nicht", "ver\u00b7dam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "PPER", "$,", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Untr\u00f6stlich ist's noch allerw\u00e4rts,", "tokens": ["Un\u00b7tr\u00f6st\u00b7lich", "ist's", "noch", "al\u00b7ler\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch sah