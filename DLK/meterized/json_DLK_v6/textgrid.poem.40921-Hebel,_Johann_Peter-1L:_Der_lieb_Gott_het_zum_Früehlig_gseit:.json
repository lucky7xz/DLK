{"textgrid.poem.40921": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der lieb Gott het zum Fr\u00fcehlig gseit:", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der lieb Gott het zum Fr\u00fcehlig gseit:", "tokens": ["Der", "lieb", "Gott", "het", "zum", "Fr\u00fceh\u00b7lig", "gseit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "APPRART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "\u00bbgang, deck im W\u00fcrmli au si Tisch!\u00ab", "tokens": ["\u00bb", "gang", ",", "deck", "im", "W\u00fcrm\u00b7li", "au", "si", "Tisch", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$,", "PRELS", "APPRART", "NE", "NE", "NE", "NN", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Druf het der Chriesbaum Bl\u00e4tter treit,", "tokens": ["Druf", "het", "der", "Chries\u00b7baum", "Bl\u00e4t\u00b7ter", "treit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "vil tausig Bl\u00e4tter gr\u00fcn und frisch.", "tokens": ["vil", "tau\u00b7sig", "Bl\u00e4t\u00b7ter", "gr\u00fcn", "und", "frisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und 's W\u00fcrmli us em Ei verwacht's,", "tokens": ["Und", "'s", "W\u00fcrm\u00b7li", "us", "em", "Ei", "ver\u00b7wacht's", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "s het gschlofen in sim Winterhus.", "tokens": ["s", "het", "gschlo\u00b7fen", "in", "sim", "Win\u00b7ter\u00b7hus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Es streckt si, und sperrt 's M\u00fc\u00fcli uf", "tokens": ["Es", "streckt", "si", ",", "und", "sperrt", "'s", "M\u00fc\u00fc\u00b7li", "uf"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "$,", "KON", "VVFIN", "PPER", "NE", "NE"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "und ribt die bl\u00f6den Augen us.", "tokens": ["und", "ribt", "die", "bl\u00f6\u00b7den", "Au\u00b7gen", "us", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und druf se het's mit stillem Zahn", "tokens": ["Und", "druf", "se", "het's", "mit", "stil\u00b7lem", "Zahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "NE", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "am Bl\u00e4ttli gnagt enanderno", "tokens": ["am", "Bl\u00e4tt\u00b7li", "gnagt", "en\u00b7an\u00b7der\u00b7no"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "und gseit: \u00bbWie isch das Gm\u00fces so guet!", "tokens": ["und", "gseit", ":", "\u00bb", "Wie", "isch", "das", "Gm\u00fces", "so", "guet", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$.", "$(", "PWAV", "ADJD", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Me chunnt schier nimme weg dervo.\u00ab", "tokens": ["Me", "chunnt", "schier", "nim\u00b7me", "weg", "der\u00b7vo", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und wieder het der lieb Gott gseit:", "tokens": ["Und", "wie\u00b7der", "het", "der", "lieb", "Gott", "gseit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJD", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "\u00bbdeck jez im Immli au si Tisch.\u00ab", "tokens": ["\u00bb", "deck", "jez", "im", "I\u00b7mmli", "au", "si", "Tisch", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Druf het der Chriesbaum Bl\u00fcte treit,", "tokens": ["Druf", "het", "der", "Chries\u00b7baum", "Bl\u00fc\u00b7te", "treit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.16": {"text": "viel tausig Bl\u00fcte wii\u00df und frisch.", "tokens": ["viel", "tau\u00b7sig", "Bl\u00fc\u00b7te", "wii\u00df", "und", "frisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und 's Immli sieht's und fliegt druf los,", "tokens": ["Und", "'s", "I\u00b7mmli", "sieht's", "und", "fliegt", "druf", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NE", "NE", "KON", "VVFIN", "PAV", "PTKVZ", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.18": {"text": "fr\u00fceih in der Sunne Morgeschin.", "tokens": ["fr\u00fc\u00b7eih", "in", "der", "Sun\u00b7ne", "Mor\u00b7ge\u00b7schin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.19": {"text": "Er denkt: \u00bbDas wird mi Caffi si;", "tokens": ["Er", "denkt", ":", "\u00bb", "Das", "wird", "mi", "Caf\u00b7fi", "si", ";"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PDS", "VAFIN", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "si henn doch chosper Porzelin.", "tokens": ["si", "henn", "doch", "chos\u00b7per", "Por\u00b7ze\u00b7lin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Wie sufer sin die Ch\u00e4cheli gschwenkt\u00ab", "tokens": ["Wie", "su\u00b7fer", "sin", "die", "Ch\u00e4\u00b7che\u00b7li", "gschwenkt", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.22": {"text": "Es streckt si troche Z\u00fcngli dri.", "tokens": ["Es", "streckt", "si", "tro\u00b7che", "Z\u00fcng\u00b7li", "dri", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "FM.it", "FM.it", "FM.it", "FM.it", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Es trinkt und seit: \u00bbWie schmeckt's so s\u00fc\u00df,", "tokens": ["Es", "trinkt", "und", "seit", ":", "\u00bb", "Wie", "schmeckt's", "so", "s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "NN", "$.", "$(", "PWAV", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "do mu\u00df der Zucker wohlfel si.\u00ab", "tokens": ["do", "mu\u00df", "der", "Zu\u00b7cker", "wohl\u00b7fel", "si", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "NE", "NE", "$.", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.25": {"text": "Der lieb Gott het zum Summer gseit:", "tokens": ["Der", "lieb", "Gott", "het", "zum", "Sum\u00b7mer", "gseit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "APPRART", "NN", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.26": {"text": "\u00bbgang, deck im Sp\u00e4tzli au si Tisch!\u00ab", "tokens": ["\u00bb", "gang", ",", "deck", "im", "Sp\u00e4tz\u00b7li", "au", "si", "Tisch", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.27": {"text": "Druf het der Chriesbaum Fr\u00fcchte treit,", "tokens": ["Druf", "het", "der", "Chries\u00b7baum", "Fr\u00fcch\u00b7te", "treit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.28": {"text": "viel tausig Chriesi rot und frisch.", "tokens": ["viel", "tau\u00b7sig", "Chrie\u00b7si", "rot", "und", "frisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NE", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Und 's Sp\u00e4tzli seit: \u00bbIsch das der Bricht?", "tokens": ["Und", "'s", "Sp\u00e4tz\u00b7li", "seit", ":", "\u00bb", "Isch", "das", "der", "Bricht", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "APPR", "$.", "$(", "ADJD", "ART", "ART", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.30": {"text": "Do sitzt me zu, und frogt nit lang.", "tokens": ["Do", "sitzt", "me", "zu", ",", "und", "frogt", "nit", "lang", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "KON", "VVFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Das git mer Chraft in Mark und Bei", "tokens": ["Das", "git", "mer", "Chraft", "in", "Mark", "und", "Bei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJA", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "und st\u00e4rcht mer d'Stimm zum neue Gsang.\u00ab", "tokens": ["und", "st\u00e4rcht", "mer", "d'\u00b7Stimm", "zum", "neu\u00b7e", "Gsang", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.33": {"text": "Der lieb Gott het zum Sp\u00f6tlig gseit:", "tokens": ["Der", "lieb", "Gott", "het", "zum", "Sp\u00f6t\u00b7lig", "gseit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "APPRART", "NN", "NN", "$."], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.34": {"text": "\u00bbruum ab, sie hen jez alli gha!\u00ab", "tokens": ["\u00bb", "ruum", "ab", ",", "sie", "hen", "jez", "al\u00b7li", "gha", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "PTKVZ", "$,", "FM", "FM", "FM", "FM", "FM", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Druf het e ch\u00fcele Bergluft gweiht,", "tokens": ["Druf", "het", "e", "ch\u00fce\u00b7le", "Berg\u00b7luft", "gweiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "NE", "NE", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.36": {"text": "und 's het scho chleini Rife gha.", "tokens": ["und", "'s", "het", "scho", "chlei\u00b7ni", "Ri\u00b7fe", "gha", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "FM", "FM", "FM", "FM", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.37": {"text": "Und d'Bl\u00e4ttli werde gel und rot", "tokens": ["Und", "d'\u00b7Bl\u00e4tt\u00b7li", "wer\u00b7de", "gel", "und", "rot"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "NN", "KON", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.38": {"text": "und fallen eis im andere no,", "tokens": ["und", "fal\u00b7len", "eis", "im", "an\u00b7de\u00b7re", "no", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "ADJA", "NE", "$,"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.39": {"text": "und was vom Boden obsi chunnt,", "tokens": ["und", "was", "vom", "Bo\u00b7den", "ob\u00b7si", "chunnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "mue\u00df au zum Bode nidsi go.", "tokens": ["mu\u00b7e\u00df", "au", "zum", "Bo\u00b7de", "nid\u00b7si", "go", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "APPRART", "NN", "FM", "FM", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.41": {"text": "Der lieb Gott het zum Winter gseit:", "tokens": ["Der", "lieb", "Gott", "het", "zum", "Win\u00b7ter", "gseit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "APPRART", "NN", "NN", "$."], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.42": {"text": "\u00bbdeck weidli zu, was \u00fcbrig ist.\u00ab", "tokens": ["\u00bb", "deck", "weid\u00b7li", "zu", ",", "was", "\u00fcb\u00b7rig", "ist", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "$,", "PRELS", "ADJD", "VAFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Druf het der Winter Flocke gstreut \u2013", "tokens": ["Druf", "het", "der", "Win\u00b7ter", "Flo\u00b7cke", "gs\u00b7treut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "NN", "VVFIN", "$("], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}}}}