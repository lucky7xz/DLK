{"textgrid.poem.52008": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr gebet unser Volck bey Euern Herren an,", "tokens": ["Ihr", "ge\u00b7bet", "un\u00b7ser", "Volck", "bey", "Eu\u00b7ern", "Her\u00b7ren", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das an den Steuren sich l\u00e4ngst arm und kranck gegeben,", "tokens": ["Das", "an", "den", "Steu\u00b7ren", "sich", "l\u00e4ngst", "arm", "und", "kranck", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "PRF", "ADV", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wann ihr nichts weiters w\u00fcst bey Ihnen zu erheben,", "tokens": ["Wann", "ihr", "nichts", "wei\u00b7ters", "w\u00fcst", "bey", "Ih\u00b7nen", "zu", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIS", "PIS", "VVFIN", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als ob es ie Gewalt der Majest\u00e4t gethan.", "tokens": ["Als", "ob", "es", "ie", "Ge\u00b7walt", "der", "Ma\u00b7jes\u00b7t\u00e4t", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wann ihr die Obrigkeit dann so verhast gemacht,", "tokens": ["Wann", "ihr", "die", "Ob\u00b7rig\u00b7keit", "dann", "so", "ver\u00b7hast", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df Sie ohn Schrecken nicht die Unterthanen nennen,", "tokens": ["Da\u00df", "Sie", "ohn", "Schre\u00b7cken", "nicht", "die", "Un\u00b7ter\u00b7tha\u00b7nen", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erweckt ihr \u00e4rgre Noth als die mit St\u00f6hrn und Brennen", "tokens": ["Er\u00b7weckt", "ihr", "\u00e4r\u00b7gre", "Noth", "als", "die", "mit", "St\u00f6hrn", "und", "Bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "KOKOM", "ART", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihr eignes Land durchaus verheert und umgebracht.", "tokens": ["Ihr", "eig\u00b7nes", "Land", "durc\u00b7haus", "ver\u00b7heert", "und", "um\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ihr F\u00fcrsten, denen Gott das heilge Schwerd befiehlt,", "tokens": ["Ihr", "F\u00fcrs\u00b7ten", ",", "de\u00b7nen", "Gott", "das", "heil\u00b7ge", "Schwerd", "be\u00b7fiehlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ach last die Leute gehn, die ohne Degen schlaffen;", "tokens": ["Ach", "last", "die", "Leu\u00b7te", "gehn", ",", "die", "oh\u00b7ne", "De\u00b7gen", "schlaf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ART", "NN", "VVINF", "$,", "PRELS", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nehmt solche Diener f\u00fcr, die so blo\u00df dra\u00fcn zu straffen,", "tokens": ["Nehmt", "sol\u00b7che", "Die\u00b7ner", "f\u00fcr", ",", "die", "so", "blo\u00df", "dra\u00fcn", "zu", "straf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "$,", "PRELS", "ADV", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Denn er beleidigt auch, der euch die Hertzen stiehlt.", "tokens": ["Denn", "er", "be\u00b7lei\u00b7digt", "auch", ",", "der", "euch", "die", "Hert\u00b7zen", "stiehlt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}