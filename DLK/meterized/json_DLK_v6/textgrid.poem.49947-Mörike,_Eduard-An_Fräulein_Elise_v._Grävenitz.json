{"textgrid.poem.49947": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "An Fr\u00e4ulein Elise v. Gr\u00e4venitz", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der jungen Rose fiel es ein,", "tokens": ["Der", "jun\u00b7gen", "Ro\u00b7se", "fiel", "es", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf einem Blumen-Maskenballe", "tokens": ["Auf", "ei\u00b7nem", "Blu\u00b7men\u00b7Mas\u00b7ken\u00b7bal\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In jener Feengartenhalle", "tokens": ["In", "je\u00b7ner", "Fe\u00b7en\u00b7gar\u00b7ten\u00b7hal\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bescheiden eine Distel zu sein.", "tokens": ["Be\u00b7schei\u00b7den", "ei\u00b7ne", "Dis\u00b7tel", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Get\u00e4uscht von der Metamorphose,", "tokens": ["Ge\u00b7t\u00e4uscht", "von", "der", "Me\u00b7ta\u00b7mor\u00b7pho\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Macht sich ein Herrchen gleich herbei,", "tokens": ["Macht", "sich", "ein", "Herr\u00b7chen", "gleich", "her\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im gr\u00fcnen Frack und gelber Hose,", "tokens": ["Im", "gr\u00fc\u00b7nen", "Frack", "und", "gel\u00b7ber", "Ho\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein ganzer Esel, meiner Treu!", "tokens": ["Ein", "gan\u00b7zer", "E\u00b7sel", ",", "mei\u00b7ner", "Treu", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Seht nur die wunderbaren Gesten,", "tokens": ["Seht", "nur", "die", "wun\u00b7der\u00b7ba\u00b7ren", "Ges\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie ihm das Herz im Leibe lacht!", "tokens": ["Wie", "ihm", "das", "Herz", "im", "Lei\u00b7be", "lacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Sch\u00f6ne denkt, den hab ich nun zum besten!", "tokens": ["Die", "Sch\u00f6\u00b7ne", "denkt", ",", "den", "hab", "ich", "nun", "zum", "bes\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "VAFIN", "PPER", "ADV", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und h\u00e4tte sich beinah zu gr\u00fcn gemacht.", "tokens": ["Und", "h\u00e4t\u00b7te", "sich", "bei\u00b7nah", "zu", "gr\u00fcn", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PRF", "ADV", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "\u2013 Auf einmal stutzt er, schn\u00fcffelt in die Luft:", "tokens": ["\u2013", "Auf", "ein\u00b7mal", "stutzt", "er", ",", "schn\u00fcf\u00b7felt", "in", "die", "Luft", ":"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ADV", "VVFIN", "PPER", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Er wittert wahrlich Rosenduft.", "tokens": ["Er", "wit\u00b7tert", "wahr\u00b7lich", "Ro\u00b7sen\u00b7duft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Gebt acht, nun schleicht er traurig sich beiseite,", "tokens": ["Gebt", "acht", ",", "nun", "schleicht", "er", "trau\u00b7rig", "sich", "bei\u00b7sei\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "CARD", "$,", "ADV", "VVFIN", "PPER", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "F\u00fcr seinesgleichen ist das schlechte Weide.", "tokens": ["F\u00fcr", "sei\u00b7nes\u00b7glei\u00b7chen", "ist", "das", "schlech\u00b7te", "Wei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "\u2013 Doch nein, er weilt entz\u00fcckt, seht her!", "tokens": ["\u2013", "Doch", "nein", ",", "er", "weilt", "ent\u00b7z\u00fcckt", ",", "seht", "her", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "KON", "PTKANT", "$,", "PPER", "VVFIN", "VVPP", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Der hat Verstand, trotz seiner langen Ohren!", "tokens": ["Der", "hat", "Ver\u00b7stand", ",", "trotz", "sei\u00b7ner", "lan\u00b7gen", "Oh\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Und hat er morgen keinen mehr,", "tokens": ["Und", "hat", "er", "mor\u00b7gen", "kei\u00b7nen", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PIAT", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Begreif ich's, wie er ihn verloren.", "tokens": ["Be\u00b7greif", "ich's", ",", "wie", "er", "ihn", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "$,", "PWAV", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}