{"textgrid.poem.63978": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "Wellenspiel", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heiteres Leuchten im braunen Gesicht,", "tokens": ["Hei\u00b7te\u00b7res", "Leuch\u00b7ten", "im", "brau\u00b7nen", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "W\u00e4hlig der Himmel hinrollendes Licht", "tokens": ["W\u00e4h\u00b7lig", "der", "Him\u00b7mel", "hin\u00b7rol\u00b7len\u00b7des", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Pr\u00e4chtige Bl\u00e4ue so unten, so oben", "tokens": ["Pr\u00e4ch\u00b7ti\u00b7ge", "Bl\u00e4u\u00b7e", "so", "un\u00b7ten", ",", "so", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "ADV", "ADV", "$,", "ADV", "ADV"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Singender Jubel, freudiges Toben.", "tokens": ["Sin\u00b7gen\u00b7der", "Ju\u00b7bel", ",", "freu\u00b7di\u00b7ges", "To\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Greifende Arme ins tolle Gemisch", "tokens": ["Grei\u00b7fen\u00b7de", "Ar\u00b7me", "ins", "tol\u00b7le", "Ge\u00b7misch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Kinder mit Flossen, zappelnder Fisch", "tokens": ["Kin\u00b7der", "mit", "Flos\u00b7sen", ",", "zap\u00b7peln\u00b7der", "Fisch"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "NN", "$,", "ADJA", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Fassen und fliehen, kr\u00e4hen und haschen,", "tokens": ["Fas\u00b7sen", "und", "flie\u00b7hen", ",", "kr\u00e4\u00b7hen", "und", "ha\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$,", "ADJA", "KON", "VVINF", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Taumeln und tauchen, spritzen und waschen.", "tokens": ["Tau\u00b7meln", "und", "tau\u00b7chen", ",", "sprit\u00b7zen", "und", "wa\u00b7schen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,", "VVFIN", "KON", "VVINF", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Siehe der V\u00e4ter verwunderlich Treiben", "tokens": ["Sie\u00b7he", "der", "V\u00e4\u00b7ter", "ver\u00b7wun\u00b7der\u00b7lich", "Trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "ADJD", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Wissen vor Freude nirgends zu bleiben", "tokens": ["Wis\u00b7sen", "vor", "Freu\u00b7de", "nir\u00b7gends", "zu", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "ADV", "PTKZU", "VVINF"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Greifende Arme ins tolle Gemisch", "tokens": ["Grei\u00b7fen\u00b7de", "Ar\u00b7me", "ins", "tol\u00b7le", "Ge\u00b7misch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Fassen die Kinder, fassen den Fisch.", "tokens": ["Fas\u00b7sen", "die", "Kin\u00b7der", ",", "fas\u00b7sen", "den", "Fisch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Schauen ihr lachendes Weltwunder an", "tokens": ["Schau\u00b7en", "ihr", "la\u00b7chen\u00b7des", "Welt\u00b7wun\u00b7der", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "APPR"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ja, so ein Vater, das ist euch ein Mann.", "tokens": ["Ja", ",", "so", "ein", "Va\u00b7ter", ",", "das", "ist", "euch", "ein", "Mann", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ART", "NN", "$,", "PDS", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In seinem Kinde ist nochmal sein Leben,", "tokens": ["In", "sei\u00b7nem", "Kin\u00b7de", "ist", "noch\u00b7mal", "sein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kann sich nun selber ja schwingen und heben.", "tokens": ["Kann", "sich", "nun", "sel\u00b7ber", "ja", "schwin\u00b7gen", "und", "he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADV", "ADV", "ADV", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Wie eine Sonne die selber sich scheint", "tokens": ["Wie", "ei\u00b7ne", "Son\u00b7ne", "die", "sel\u00b7ber", "sich", "scheint"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "ADV", "PRF", "VVFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Einmal rosig, das andere gebr\u00e4unt", "tokens": ["Ein\u00b7mal", "ro\u00b7sig", ",", "das", "an\u00b7de\u00b7re", "ge\u00b7br\u00e4unt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "PRELS", "PIS", "VVPP"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wirft an das Licht sein fliegendes Wunder,", "tokens": ["Wirft", "an", "das", "Licht", "sein", "flie\u00b7gen\u00b7des", "Wun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Das an der Brust h\u00e4lt glattzackigen Flunder.", "tokens": ["Das", "an", "der", "Brust", "h\u00e4lt", "glatt\u00b7za\u00b7cki\u00b7gen", "Flun\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Auf gr\u00fcnem Gestein rotflossige Hand", "tokens": ["Auf", "gr\u00fc\u00b7nem", "Ge\u00b7stein", "rot\u00b7flos\u00b7si\u00b7ge", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gold\u00fcberrollt ins verschwimmende Land", "tokens": ["Gold\u00b7\u00fc\u00b7berr\u00b7ollt", "ins", "ver\u00b7schwim\u00b7men\u00b7de", "Land"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Schauen zwei Augen,", "tokens": ["Schau\u00b7en", "zwei", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Sterne stiller Freude", "tokens": ["Ster\u00b7ne", "stil\u00b7ler", "Freu\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ins verschwindende Weite.", "tokens": ["Ins", "ver\u00b7schwin\u00b7den\u00b7de", "Wei\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.7": {"line.1": {"text": "Lustige V\u00e4ter, junge berauscht", "tokens": ["Lus\u00b7ti\u00b7ge", "V\u00e4\u00b7ter", ",", "jun\u00b7ge", "be\u00b7rauscht"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Schleudern mit Flossen ausspannender Hand", "tokens": ["Schleu\u00b7dern", "mit", "Flos\u00b7sen", "aus\u00b7span\u00b7nen\u00b7der", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Schuppenumglitzerte Kinder kr\u00e4hend ans Land \u2013", "tokens": ["Schup\u00b7pe\u00b7num\u00b7glit\u00b7zer\u00b7te", "Kin\u00b7der", "kr\u00e4\u00b7hend", "ans", "Land", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "APPRART", "NN", "$("], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Mutter lauscht.", "tokens": ["Mut\u00b7ter", "lauscht", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "So ist es, da\u00df die Erden", "tokens": ["So", "ist", "es", ",", "da\u00df", "die", "Er\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von allem Wachsen sch\u00f6ner werden.", "tokens": ["Von", "al\u00b7lem", "Wach\u00b7sen", "sch\u00f6\u00b7ner", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}