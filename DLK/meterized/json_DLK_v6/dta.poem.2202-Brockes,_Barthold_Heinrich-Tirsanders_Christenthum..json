{"dta.poem.2202": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Tirsanders Christenthum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jm Pabstthum sieht man hin und wieder die gr\u00f6\u00dften", "tokens": ["Jm", "Pab\u00b7stthum", "sieht", "man", "hin", "und", "wie\u00b7der", "die", "gr\u00f6\u00df\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "PTKVZ", "KON", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Laster oft begehen,", "tokens": ["Las\u00b7ter", "oft", "be\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Um, eben durch diefelbigen, in einem Stande sich zu sehen,", "tokens": ["Um", ",", "e\u00b7ben", "durch", "die\u00b7fel\u00b7bi\u00b7gen", ",", "in", "ei\u00b7nem", "Stan\u00b7de", "sich", "zu", "se\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "$,", "ADV", "APPR", "PDAT", "$,", "APPR", "ART", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.4": {"text": "Sich die Vergebung zu erlaufen. Wo dieses in der That", "tokens": ["Sich", "die", "Ver\u00b7ge\u00b7bung", "zu", "er\u00b7lau\u00b7fen", ".", "Wo", "die\u00b7ses", "in", "der", "That"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PRF", "ART", "NN", "PTKZU", "VVINF", "$.", "PWAV", "PDAT", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.5": {"text": "gescheh,", "tokens": ["ge\u00b7scheh", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "So gliche ja das Paradies fast jenem Thurm der Danae.", "tokens": ["So", "gli\u00b7che", "ja", "das", "Pa\u00b7ra\u00b7dies", "fast", "je\u00b7nem", "Thurm", "der", "Da\u00b7nae", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ADV", "PDAT", "NN", "ART", "NE", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}