{"dta.poem.4157": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Zephyr.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Rosenblatt war meine Wiege,", "tokens": ["Ein", "Ro\u00b7sen\u00b7blatt", "war", "mei\u00b7ne", "Wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Rosenblatt ist einst mein Grab.", "tokens": ["Ein", "Ro\u00b7sen\u00b7blatt", "ist", "einst", "mein", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich schlafe, wann der Winter tobt,", "tokens": ["Ich", "schla\u00b7fe", ",", "wann", "der", "Win\u00b7ter", "tobt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und mit dem Lenze werd' ich munter,", "tokens": ["Und", "mit", "dem", "Len\u00b7ze", "werd'", "ich", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und n\u00e4hre mich von Duft und Ku\u00df.", "tokens": ["Und", "n\u00e4h\u00b7re", "mich", "von", "Duft", "und", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du armer, stolzer Herr der Welt,", "tokens": ["Du", "ar\u00b7mer", ",", "stol\u00b7zer", "Herr", "der", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du keuchst einher mit deiner Krone,", "tokens": ["Du", "keuchst", "ein\u00b7her", "mit", "dei\u00b7ner", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und dienstbar trockn' ich deinen Schwei\u00df!", "tokens": ["Und", "dienst\u00b7bar", "trockn'", "ich", "dei\u00b7nen", "Schwei\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}