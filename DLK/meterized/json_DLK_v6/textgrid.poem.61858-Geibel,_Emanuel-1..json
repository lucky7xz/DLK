{"textgrid.poem.61858": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit geheimnisvollen D\u00fcften", "tokens": ["Mit", "ge\u00b7heim\u00b7nis\u00b7vol\u00b7len", "D\u00fcf\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gr\u00fc\u00dft vom Hang der Wald mich schon,", "tokens": ["Gr\u00fc\u00dft", "vom", "Hang", "der", "Wald", "mich", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcber mir in hohen L\u00fcften", "tokens": ["\u00dc\u00b7ber", "mir", "in", "ho\u00b7hen", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwebt der erste Lerchenton.", "tokens": ["Schwebt", "der", "ers\u00b7te", "Ler\u00b7chen\u00b7ton", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In den s\u00fc\u00dfen Laut versunken", "tokens": ["In", "den", "s\u00fc\u00b7\u00dfen", "Laut", "ver\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "APPR", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wall' ich hin durchs Saatgefild,", "tokens": ["Wall'", "ich", "hin", "durchs", "Saat\u00b7ge\u00b7fild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das noch halb von Schlummer trunken", "tokens": ["Das", "noch", "halb", "von", "Schlum\u00b7mer", "trun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ADJD", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sanft dem Licht entgegenschwillt.", "tokens": ["Sanft", "dem", "Licht", "ent\u00b7ge\u00b7gen\u00b7schwillt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Welch ein Sehnen! Welch ein Tr\u00e4umen!", "tokens": ["Welch", "ein", "Seh\u00b7nen", "!", "Welch", "ein", "Tr\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "$.", "PIAT", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, du m\u00f6chtest vorm Vergl\u00fchn", "tokens": ["Ach", ",", "du", "m\u00f6ch\u00b7test", "vorm", "Ver\u00b7gl\u00fchn"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VMFIN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit den Blumen, mit den B\u00e4umen,", "tokens": ["Mit", "den", "Blu\u00b7men", ",", "mit", "den", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Altes Herz, noch einmal bl\u00fchn.", "tokens": ["Al\u00b7tes", "Herz", ",", "noch", "ein\u00b7mal", "bl\u00fchn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}