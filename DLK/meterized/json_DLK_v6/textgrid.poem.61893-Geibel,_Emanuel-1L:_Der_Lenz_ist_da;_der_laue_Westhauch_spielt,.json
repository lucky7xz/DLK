{"textgrid.poem.61893": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Lenz ist da; der laue Westhauch spielt,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Lenz ist da; der laue Westhauch spielt,", "tokens": ["Der", "Lenz", "ist", "da", ";", "der", "lau\u00b7e", "West\u00b7hauch", "spielt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$.", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Fenster, die der Frost verschlossen hielt,", "tokens": ["Die", "Fens\u00b7ter", ",", "die", "der", "Frost", "ver\u00b7schlos\u00b7sen", "hielt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00d6ffnen sich rings mit frohem L\u00e4rmen;", "tokens": ["\u00d6ff\u00b7nen", "sich", "rings", "mit", "fro\u00b7hem", "L\u00e4r\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Es bricht ein Strom herein von Duft und Licht", "tokens": ["Es", "bricht", "ein", "Strom", "her\u00b7ein", "von", "Duft", "und", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und lockt unwiderstehlich. H\u00f6rst du nicht", "tokens": ["Und", "lockt", "un\u00b7wi\u00b7der\u00b7steh\u00b7lich", ".", "H\u00f6rst", "du", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "$.", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Kinder auf den Gassen schw\u00e4rmen?", "tokens": ["Die", "Kin\u00b7der", "auf", "den", "Gas\u00b7sen", "schw\u00e4r\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Lenz ist da; er ruft auch mich zum Fest;", "tokens": ["Der", "Lenz", "ist", "da", ";", "er", "ruft", "auch", "mich", "zum", "Fest", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$.", "PPER", "VVFIN", "ADV", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Am Nachbarhause die Kastanie l\u00e4\u00dft", "tokens": ["Am", "Nach\u00b7bar\u00b7hau\u00b7se", "die", "Kas\u00b7ta\u00b7nie", "l\u00e4\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Bl\u00fctenfederb\u00fcsche wallen;", "tokens": ["Die", "Bl\u00fc\u00b7ten\u00b7fe\u00b7der\u00b7b\u00fc\u00b7sche", "wal\u00b7len", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Tor gleich bunt entpuppten Faltern zieht", "tokens": ["Zum", "Tor", "gleich", "bunt", "ent\u00b7pupp\u00b7ten", "Fal\u00b7tern", "zieht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "ADJD", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Schwarm von M\u00e4dchen, der am ersten Lied", "tokens": ["Ein", "Schwarm", "von", "M\u00e4d\u00b7chen", ",", "der", "am", "ers\u00b7ten", "Lied"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sich freuen will der Nachtigallen.", "tokens": ["Sich", "freu\u00b7en", "will", "der", "Nach\u00b7ti\u00b7gal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "VMFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Froh sinnend folg' ich nach, die Br\u00fcck' entlang;", "tokens": ["Froh", "sin\u00b7nend", "fol\u00b7g'", "ich", "nach", ",", "die", "Br\u00fcck", "ent\u00b7lang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "APPO", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vom Flusse schallt Gel\u00e4chter und Gesang;", "tokens": ["Vom", "Flus\u00b7se", "schallt", "Ge\u00b7l\u00e4ch\u00b7ter", "und", "Ge\u00b7sang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die G\u00e4rten tun sich auf im Kranze;", "tokens": ["Die", "G\u00e4r\u00b7ten", "tun", "sich", "auf", "im", "Kran\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie labt den Blick des Rasens gr\u00fcner Samt,", "tokens": ["Wie", "labt", "den", "Blick", "des", "Ra\u00b7sens", "gr\u00fc\u00b7ner", "Samt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gestickt mit Perlen Taus! Wie wogt und flammt", "tokens": ["Ge\u00b7stickt", "mit", "Per\u00b7len", "Taus", "!", "Wie", "wogt", "und", "flammt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "NN", "NN", "$.", "PWAV", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das Tulpenbeet im Sonnenglanze!", "tokens": ["Das", "Tul\u00b7pen\u00b7beet", "im", "Son\u00b7nen\u00b7glan\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nun winkt das Dorf. Im Turme l\u00e4utet's, horch!", "tokens": ["Nun", "winkt", "das", "Dorf", ".", "Im", "Tur\u00b7me", "l\u00e4u\u00b7tet's", ",", "horch", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "APPRART", "NN", "VVFIN", "$,", "VVIMP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vom hohen Strohdach \u00fcberschaut der Storch", "tokens": ["Vom", "ho\u00b7hen", "Stroh\u00b7dach", "\u00fc\u00b7bersc\u00b7haut", "der", "Storch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ernst klappernd seines Weichbilds Grenzen;", "tokens": ["Ernst", "klap\u00b7pernd", "sei\u00b7nes", "Weich\u00b7bilds", "Gren\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dazwischen schallt's vom Krug wie Geigenstrich,", "tokens": ["Da\u00b7zwi\u00b7schen", "schallt's", "vom", "Krug", "wie", "Gei\u00b7gen\u00b7strich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPRART", "NN", "KOKOM", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und unterm bl\u00fchnden Birnbaum tummelt sich", "tokens": ["Und", "un\u00b7term", "bl\u00fchn\u00b7den", "Birn\u00b7baum", "tum\u00b7melt", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das Volk in l\u00e4ndlich schlichten T\u00e4nzen.", "tokens": ["Das", "Volk", "in", "l\u00e4nd\u00b7lich", "schlich\u00b7ten", "T\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich aber wandle still, bis tief im Wald", "tokens": ["Ich", "a\u00b7ber", "wand\u00b7le", "still", ",", "bis", "tief", "im", "Wald"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PTKVZ", "$,", "KOUS", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Reigens Jubel hinter mir verhallt;", "tokens": ["Des", "Rei\u00b7gens", "Ju\u00b7bel", "hin\u00b7ter", "mir", "ver\u00b7hallt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da pocht mein Herz in raschern Schl\u00e4gen,", "tokens": ["Da", "pocht", "mein", "Herz", "in", "ra\u00b7schern", "Schl\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn aus den B\u00fcschen tritt, den Blick voll Glanz,", "tokens": ["Denn", "aus", "den", "B\u00fc\u00b7schen", "tritt", ",", "den", "Blick", "voll", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Im goldnen Haar den jungen Veilchenkranz,", "tokens": ["Im", "gold\u00b7nen", "Haar", "den", "jun\u00b7gen", "Veil\u00b7chen\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Muse l\u00e4chelnd mir entgegen.", "tokens": ["Die", "Mu\u00b7se", "l\u00e4\u00b7chelnd", "mir", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}