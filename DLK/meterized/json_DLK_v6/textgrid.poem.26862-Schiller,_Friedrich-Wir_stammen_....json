{"textgrid.poem.26862": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Wir stammen ...", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir stammen, unsrer sechs Geschwister,", "tokens": ["Wir", "stam\u00b7men", ",", "uns\u00b7rer", "sechs", "Ge\u00b7schwis\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von einem wundersamen Paar,", "tokens": ["Von", "ei\u00b7nem", "wun\u00b7der\u00b7sa\u00b7men", "Paar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Mutter ewig ernst und d\u00fcster,", "tokens": ["Die", "Mut\u00b7ter", "e\u00b7wig", "ernst", "und", "d\u00fcs\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Vater fr\u00f6hlich immerdar.", "tokens": ["Der", "Va\u00b7ter", "fr\u00f6h\u00b7lich", "im\u00b7mer\u00b7dar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Von beiden erbten wir die Tugend,", "tokens": ["Von", "bei\u00b7den", "erb\u00b7ten", "wir", "die", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von ", "tokens": ["Von"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "So drehn wir uns in ewger Jugend", "tokens": ["So", "drehn", "wir", "uns", "in", "ew\u00b7ger", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "CARD", "PPER", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um dich herum im Zirkeltanz.", "tokens": ["Um", "dich", "he\u00b7rum", "im", "Zir\u00b7kel\u00b7tanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "APZR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Gern meiden wir die schwarzen H\u00f6hlen", "tokens": ["Gern", "mei\u00b7den", "wir", "die", "schwar\u00b7zen", "H\u00f6h\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und lieben uns den heitern Tag,", "tokens": ["Und", "lie\u00b7ben", "uns", "den", "hei\u00b7tern", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir sind es, die die Welt beseelen", "tokens": ["Wir", "sind", "es", ",", "die", "die", "Welt", "be\u00b7see\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit unsers Lebens Zauberschlag.", "tokens": ["Mit", "un\u00b7sers", "Le\u00b7bens", "Zau\u00b7ber\u00b7schlag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wir sind des Fr\u00fchlings lustge Boten", "tokens": ["Wir", "sind", "des", "Fr\u00fch\u00b7lings", "lust\u00b7ge", "Bo\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und f\u00fchren seinen muntern Reihn,", "tokens": ["Und", "f\u00fch\u00b7ren", "sei\u00b7nen", "mun\u00b7tern", "Reihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drum fliehen wir das Haus der Toten,", "tokens": ["Drum", "flie\u00b7hen", "wir", "das", "Haus", "der", "To\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn um uns her mu\u00df Leben sein.", "tokens": ["Denn", "um", "uns", "her", "mu\u00df", "Le\u00b7ben", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUI", "PRF", "APZR", "VMFIN", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Uns mag kein Gl\u00fccklicher entbehren,", "tokens": ["Uns", "mag", "kein", "Gl\u00fcck\u00b7li\u00b7cher", "ent\u00b7beh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir sind dabei, wo man sich freut,", "tokens": ["Wir", "sind", "da\u00b7bei", ",", "wo", "man", "sich", "freut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "$,", "PWAV", "PIS", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und l\u00e4\u00dft der Kaiser sich verehren,", "tokens": ["Und", "l\u00e4\u00dft", "der", "Kai\u00b7ser", "sich", "ver\u00b7eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die sechs Geschwister, die freundlichen Wesen,", "tokens": ["Die", "sechs", "Ge\u00b7schwis\u00b7ter", ",", "die", "freund\u00b7li\u00b7chen", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die mit des Vaters feuriger Gewalt", "tokens": ["Die", "mit", "des", "Va\u00b7ters", "feu\u00b7ri\u00b7ger", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Mutter sanften Sinn verm\u00e4hlen,", "tokens": ["Der", "Mut\u00b7ter", "sanf\u00b7ten", "Sinn", "ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die alle Welt mit Lust beseelen,", "tokens": ["Die", "al\u00b7le", "Welt", "mit", "Lust", "be\u00b7see\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die gern der Freude dienen und der Pracht", "tokens": ["Die", "gern", "der", "Freu\u00b7de", "die\u00b7nen", "und", "der", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "VVINF", "KON", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und sich nicht zeigen in dem Haus der Klagen \u2013", "tokens": ["Und", "sich", "nicht", "zei\u00b7gen", "in", "dem", "Haus", "der", "Kla\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PTKNEG", "VVINF", "APPR", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Farben sinds, des Lichtes Kinder und der Nacht.", "tokens": ["Die", "Far\u00b7ben", "sinds", ",", "des", "Lich\u00b7tes", "Kin\u00b7der", "und", "der", "Nacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "ART", "ADJA", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}