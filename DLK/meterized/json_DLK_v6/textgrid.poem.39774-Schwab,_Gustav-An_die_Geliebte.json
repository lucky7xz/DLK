{"textgrid.poem.39774": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "An die Geliebte", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie fassen nicht den ew'gen Schimmer,", "tokens": ["Sie", "fas\u00b7sen", "nicht", "den", "ew'\u00b7gen", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der dir aus deinen Augen geht,", "tokens": ["Der", "dir", "aus", "dei\u00b7nen", "Au\u00b7gen", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So wie des Mondes heil'gen Flimmer", "tokens": ["So", "wie", "des", "Mon\u00b7des", "heil'\u00b7gen", "Flim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kein irdiches Gem\u00fct versteht.", "tokens": ["Kein", "ir\u00b7di\u00b7ches", "Ge\u00b7m\u00fct", "ver\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hell mu\u00df es, wie die Sonne, blenden,", "tokens": ["Hell", "mu\u00df", "es", ",", "wie", "die", "Son\u00b7ne", ",", "blen\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "$,", "PWAV", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was dieser Welt gef\u00e4llt und lacht,", "tokens": ["Was", "die\u00b7ser", "Welt", "ge\u00b7f\u00e4llt", "und", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mu\u00df alles mit dem Tage enden,", "tokens": ["Mu\u00df", "al\u00b7les", "mit", "dem", "Ta\u00b7ge", "en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Denn f\u00fcr den Schlaf ist ihre Nacht.", "tokens": ["Denn", "f\u00fcr", "den", "Schlaf", "ist", "ih\u00b7re", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mir wird dein Leben erst entfaltet,", "tokens": ["Mir", "wird", "dein", "Le\u00b7ben", "erst", "ent\u00b7fal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wann alles rings in Schatten f\u00e4llt;", "tokens": ["Wann", "al\u00b7les", "rings", "in", "Schat\u00b7ten", "f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wei\u00df, so lang die Sonne waltet,", "tokens": ["Ich", "wei\u00df", ",", "so", "lang", "die", "Son\u00b7ne", "wal\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von dir kein Gleichni\u00df auf der Welt.", "tokens": ["Von", "dir", "kein", "Gleich\u00b7ni\u00df", "auf", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du gehst in unbemerkter F\u00fclle", "tokens": ["Du", "gehst", "in", "un\u00b7be\u00b7merk\u00b7ter", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Einsam vor\u00fcber und verwirrt,", "tokens": ["Ein\u00b7sam", "vor\u00b7\u00fc\u00b7ber", "und", "ver\u00b7wirrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Ein Stern, der sich aus Nacht und Stille", "tokens": ["Ein", "Stern", ",", "der", "sich", "aus", "Nacht", "und", "Stil\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In dieses fremde Licht verirrt.", "tokens": ["In", "die\u00b7ses", "frem\u00b7de", "Licht", "ver\u00b7irrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O dann erst, wann der Abend dichter", "tokens": ["O", "dann", "erst", ",", "wann", "der", "A\u00b7bend", "dich\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "ADV", "$,", "PWAV", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich um die stille Erde schlie\u00dft,", "tokens": ["Sich", "um", "die", "stil\u00b7le", "Er\u00b7de", "schlie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wann der Schein verwandter Lichter", "tokens": ["Und", "wann", "der", "Schein", "ver\u00b7wand\u00b7ter", "Lich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf dich vom blauen Himmel flie\u00dft;", "tokens": ["Auf", "dich", "vom", "blau\u00b7en", "Him\u00b7mel", "flie\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann erst, du namenloses Wesen,", "tokens": ["Dann", "erst", ",", "du", "na\u00b7men\u00b7lo\u00b7ses", "We\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du Stern des Himmels, fass' ich dich,", "tokens": ["Du", "Stern", "des", "Him\u00b7mels", ",", "fass'", "ich", "dich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und mein' in deinem Blick zu lesen,", "tokens": ["Und", "mein'", "in", "dei\u00b7nem", "Blick", "zu", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Beim Stral des Monds, du liebest mich.", "tokens": ["Beim", "Stral", "des", "Monds", ",", "du", "lie\u00b7best", "mich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}