{"dta.poem.11929": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "90.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Horch, das Gewitter braust, des Donners Scheltwort rollt", "tokens": ["Horch", ",", "das", "Ge\u00b7wit\u00b7ter", "braust", ",", "des", "Don\u00b7ners", "Schelt\u00b7wort", "rollt"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "$,", "ART", "NN", "NN", "VVFIN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Dem rothen Blitz nach, der ein Blick des Zornes grollt.", "tokens": ["Dem", "ro\u00b7then", "Blitz", "nach", ",", "der", "ein", "Blick", "des", "Zor\u00b7nes", "grollt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "PRELS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So fr\u00fch im Jahr, eh neu zum Leben sich verb\u00fcndet", "tokens": ["So", "fr\u00fch", "im", "Jahr", ",", "eh", "neu", "zum", "Le\u00b7ben", "sich", "ver\u00b7b\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$,", "KOUS", "ADJD", "APPRART", "NN", "PRF", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Elemente Kraft, ist schon ihr Kampf entz\u00fcndet.", "tokens": ["Der", "E\u00b7le\u00b7men\u00b7te", "Kraft", ",", "ist", "schon", "ihr", "Kampf", "ent\u00b7z\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Was in der G\u00e4hrung sonst der Sommerglut erwacht,", "tokens": ["Was", "in", "der", "G\u00e4h\u00b7rung", "sonst", "der", "Som\u00b7mer\u00b7glut", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist nun im schwellenden Lenz\u00e4ther angefacht.", "tokens": ["Ist", "nun", "im", "schwel\u00b7len\u00b7den", "Len\u00b7z\u00e4\u00b7ther", "an\u00b7ge\u00b7facht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Vor\u00fcber fahren sie vor deinem Aug' und Ohr,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "fah\u00b7ren", "sie", "vor", "dei\u00b7nem", "Aug'", "und", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das sie erschreckt vernahm, dann spurlos sie verlor.", "tokens": ["Das", "sie", "er\u00b7schreckt", "ver\u00b7nahm", ",", "dann", "spur\u00b7los", "sie", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVPP", "VVFIN", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und also meinst du wol, da\u00df sie auch ohne Spur", "tokens": ["Und", "al\u00b7so", "meinst", "du", "wol", ",", "da\u00df", "sie", "auch", "oh\u00b7ne", "Spur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor\u00fcberfahren der erwachenden Natur.", "tokens": ["Vor\u00b7\u00fc\u00b7berf\u00b7ah\u00b7ren", "der", "er\u00b7wa\u00b7chen\u00b7den", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Doch eine Spur davon, und ich will sie dir deuten,", "tokens": ["Doch", "ei\u00b7ne", "Spur", "da\u00b7von", ",", "und", "ich", "will", "sie", "dir", "deu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PAV", "$,", "KON", "PPER", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wird bleiben, die bemerkt nicht wird von vielen Leuten.", "tokens": ["Wird", "blei\u00b7ben", ",", "die", "be\u00b7merkt", "nicht", "wird", "von", "vie\u00b7len", "Leu\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVINF", "$,", "PRELS", "VVFIN", "PTKNEG", "VAFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Der Kukuk, der den Sang jetzt r\u00fcstet, um zu locken", "tokens": ["Der", "Ku\u00b7kuk", ",", "der", "den", "Sang", "jetzt", "r\u00fcs\u00b7tet", ",", "um", "zu", "lo\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "ADV", "VVFIN", "$,", "KOUI", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Bl\u00e4tter aus dem Wald, h\u00f6rt und verstummt erschrocken.", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "aus", "dem", "Wald", ",", "h\u00f6rt", "und", "ver\u00b7stummt", "er\u00b7schro\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "VVFIN", "KON", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Und der Verst\u00f6rung wird er diesen Lenz nicht frei,", "tokens": ["Und", "der", "Ver\u00b7st\u00f6\u00b7rung", "wird", "er", "die\u00b7sen", "Lenz", "nicht", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "PDAT", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und seines vollen Klangs entbehrt der bl\u00fch'nde Mai.", "tokens": ["Und", "sei\u00b7nes", "vol\u00b7len", "Klangs", "ent\u00b7behrt", "der", "bl\u00fch'n\u00b7de", "Mai", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Doch die Kastanie, die eben sich erkeckte,", "tokens": ["Doch", "die", "Kas\u00b7ta\u00b7nie", ",", "die", "e\u00b7ben", "sich", "er\u00b7keck\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADV", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Und eine Bl\u00fctenkerz' auf ihren Leuchter steckte,", "tokens": ["Und", "ei\u00b7ne", "Bl\u00fc\u00b7ten\u00b7ker\u00b7z'", "auf", "ih\u00b7ren", "Leuch\u00b7ter", "steck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "In der verschmolzen bl\u00fchn soll Wei\u00df und Gelb und Roth,", "tokens": ["In", "der", "ver\u00b7schmol\u00b7zen", "bl\u00fchn", "soll", "Wei\u00df", "und", "Gelb", "und", "Roth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "VMFIN", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erbla\u00dft vor Furcht, wie sie der rothe Blitz umloht.", "tokens": ["Er\u00b7bla\u00dft", "vor", "Furcht", ",", "wie", "sie", "der", "ro\u00b7the", "Blitz", "um\u00b7loht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "PWAV", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Wenn nun die Kerz' erbl\u00fcht, so scheint sie dir dieselbe,", "tokens": ["Wenn", "nun", "die", "Ker\u00b7z'", "er\u00b7bl\u00fcht", ",", "so", "scheint", "sie", "dir", "die\u00b7sel\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVPP", "$,", "ADV", "VVFIN", "PPER", "PPER", "PDAT", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ich aber seh', es fehlt das Roth im Wei\u00df und Gelbe.", "tokens": ["Ich", "a\u00b7ber", "seh'", ",", "es", "fehlt", "das", "Roth", "im", "Wei\u00df", "und", "Gel\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}