{"dta.poem.20094": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Unser Friede.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Sommertag, wo man zu tiefer", "tokens": ["Ein", "Som\u00b7mer\u00b7tag", ",", "wo", "man", "zu", "tie\u00b7fer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "PIS", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Siesta sich verpflichtet h\u00e4lt,", "tokens": ["Sies\u00b7ta", "sich", "ver\u00b7pflich\u00b7tet", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "VVPP", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wo M\u00fccken nur und Ungeziefer", "tokens": ["Wo", "M\u00fc\u00b7cken", "nur", "und", "Un\u00b7ge\u00b7zie\u00b7fer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So recht lebendig in der Welt,", "tokens": ["So", "recht", "le\u00b7ben\u00b7dig", "in", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo giftger Pesthauch auf zum Himmel", "tokens": ["Wo", "gift\u00b7ger", "Pest\u00b7hauch", "auf", "zum", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Aus stehenden Gew\u00e4ssern steigt,", "tokens": ["Aus", "ste\u00b7hen\u00b7den", "Ge\u00b7w\u00e4s\u00b7sern", "steigt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In deren Schlamm sich das Gewimmel", "tokens": ["In", "de\u00b7ren", "Schlamm", "sich", "das", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vielbeinigen Gew\u00fcrmes zeigt:", "tokens": ["Viel\u00b7bei\u00b7ni\u00b7gen", "Ge\u00b7w\u00fcr\u00b7mes", "zeigt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das ist der Friede, der uns schlimmer", "tokens": ["Das", "ist", "der", "Frie\u00b7de", ",", "der", "uns", "schlim\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als je ein Krieg zu werden droht,", "tokens": ["Als", "je", "ein", "Krieg", "zu", "wer\u00b7den", "droht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PTKZU", "VAINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als je ein Krieg, der uns noch immer", "tokens": ["Als", "je", "ein", "Krieg", ",", "der", "uns", "noch", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein offen Feld f\u00fcr Thaten bot;", "tokens": ["Ein", "of\u00b7fen", "Feld", "f\u00fcr", "Tha\u00b7ten", "bot", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gen\u00fcssler hegt jetzt unsre Jugend,", "tokens": ["Ge\u00b7n\u00fcss\u00b7ler", "hegt", "jetzt", "uns\u00b7re", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und Stockgelehrte allenfalls,", "tokens": ["Und", "Stock\u00b7ge\u00b7lehr\u00b7te", "al\u00b7len\u00b7falls", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch jeder Kraft und M\u00e4nnertugend", "tokens": ["Doch", "je\u00b7der", "Kraft", "und", "M\u00e4n\u00b7ner\u00b7tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Brach dieser Friede schon den Hals. \u2014", "tokens": ["Brach", "die\u00b7ser", "Frie\u00b7de", "schon", "den", "Hals", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PDAT", "NN", "ADV", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch wird die Sonn\u2019 erst unertr\u00e4glich,", "tokens": ["Doch", "wird", "die", "Sonn'", "erst", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und d\u00f6rrt den Wald, und sengt die Flur,", "tokens": ["Und", "d\u00f6rrt", "den", "Wald", ",", "und", "sengt", "die", "Flur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da hilft sich, auf gut-sommert\u00e4glich,", "tokens": ["Da", "hilft", "sich", ",", "auf", "gut\u00b7som\u00b7mer\u00b7t\u00e4g\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "$,", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit einem Schlage die Natur:", "tokens": ["Mit", "ei\u00b7nem", "Schla\u00b7ge", "die", "Na\u00b7tur", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Donnerwolke blitzt und wettert,", "tokens": ["Die", "Don\u00b7ner\u00b7wol\u00b7ke", "blitzt", "und", "wet\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und nimmt der Luft den giftgen Hauch,", "tokens": ["Und", "nimmt", "der", "Luft", "den", "gift\u00b7gen", "Hauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wird auch mancher Baum zerschmettert,", "tokens": ["Und", "wird", "auch", "man\u00b7cher", "Baum", "zer\u00b7schmet\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In faule S\u00fcmpfe schl\u00e4gt es auch.", "tokens": ["In", "fau\u00b7le", "S\u00fcmp\u00b7fe", "schl\u00e4gt", "es", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Welch Friede ", "tokens": ["Welch", "Frie\u00b7de"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Die Sonn\u2019 im Westen untergeht,", "tokens": ["Die", "Sonn'", "im", "Wes\u00b7ten", "un\u00b7ter\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dunkle Purpurrosen malend,", "tokens": ["Und", "dunk\u00b7le", "Pur\u00b7pur\u00b7ro\u00b7sen", "ma\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Himmel wie in Flammen steht!", "tokens": ["Der", "Him\u00b7mel", "wie", "in", "Flam\u00b7men", "steht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir baden uns im Hauch der Frische,", "tokens": ["Wir", "ba\u00b7den", "uns", "im", "Hauch", "der", "Fri\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie neugeboren ist das All,", "tokens": ["Wie", "neu\u00b7ge\u00b7bo\u00b7ren", "ist", "das", "All", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und in des Baumes Bl\u00e4tternische", "tokens": ["Und", "in", "des", "Bau\u00b7mes", "Bl\u00e4t\u00b7ter\u00b7ni\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Schl\u00e4gt lieblicher die Nachtigall.", "tokens": ["Schl\u00e4gt", "lieb\u00b7li\u00b7cher", "die", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}