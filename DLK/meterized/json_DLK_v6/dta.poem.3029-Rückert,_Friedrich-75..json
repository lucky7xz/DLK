{"dta.poem.3029": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "75.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist wahr oder falsch an innrer Offenbarung?", "tokens": ["Was", "ist", "wahr", "o\u00b7der", "falsch", "an", "inn\u00b7rer", "Of\u00b7fen\u00b7ba\u00b7rung", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJD", "KON", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ist damit alswie mit \u00e4u\u00dferer Gewahrung.", "tokens": ["Es", "ist", "da\u00b7mit", "als\u00b7wie", "mit", "\u00e4u\u00b7\u00dfe\u00b7rer", "Ge\u00b7wah\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "KOKOM", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was deine Augen sehn, was deine Ohren h\u00f6ren,", "tokens": ["Was", "dei\u00b7ne", "Au\u00b7gen", "sehn", ",", "was", "dei\u00b7ne", "Oh\u00b7ren", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVINF", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das glaubest du, daran wird dich kein Zweifel st\u00f6ren.", "tokens": ["Das", "glau\u00b7best", "du", ",", "da\u00b7ran", "wird", "dich", "kein", "Zwei\u00b7fel", "st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "PAV", "VAFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und wozu dir versagt sind Augen oder Ohren,", "tokens": ["Und", "wo\u00b7zu", "dir", "ver\u00b7sagt", "sind", "Au\u00b7gen", "o\u00b7der", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVPP", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sei es f\u00fcr andre da, f\u00fcr dich ist es verloren.", "tokens": ["Sei", "es", "f\u00fcr", "and\u00b7re", "da", ",", "f\u00fcr", "dich", "ist", "es", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PIS", "ADV", "$,", "APPR", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "So offenbart auch das der Geist dem Geiste nur,", "tokens": ["So", "of\u00b7fen\u00b7bart", "auch", "das", "der", "Geist", "dem", "Geis\u00b7te", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wof\u00fcr empf\u00e4nglich ist die geistige Natur.", "tokens": ["Wo\u00b7f\u00fcr", "emp\u00b7f\u00e4ng\u00b7lich", "ist", "die", "geis\u00b7ti\u00b7ge", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Er glaubt daran und schw\u00f6rt, er hats gesehn, geh\u00f6rt;", "tokens": ["Er", "glaubt", "da\u00b7ran", "und", "schw\u00f6rt", ",", "er", "hats", "ge\u00b7sehn", ",", "ge\u00b7h\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "KON", "VVFIN", "$,", "PPER", "VAFIN", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Warum nun glaubest du, da\u00df ihn ein Wahn beth\u00f6rt?", "tokens": ["Wa\u00b7rum", "nun", "glau\u00b7best", "du", ",", "da\u00df", "ihn", "ein", "Wahn", "be\u00b7th\u00f6rt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Gott hat nur anders ihn als dich es sehen lassen;", "tokens": ["Gott", "hat", "nur", "an\u00b7ders", "ihn", "als", "dich", "es", "se\u00b7hen", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADV", "PPER", "KOUS", "PPER", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wei\u00dft du, auf wieviel Art sich Gott l\u00e4\u00dft sehn und fassen?", "tokens": ["Wei\u00dft", "du", ",", "auf", "wie\u00b7viel", "Art", "sich", "Gott", "l\u00e4\u00dft", "sehn", "und", "fas\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "PWAT", "NN", "PRF", "NN", "VVFIN", "VVINF", "KON", "VVINF", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Fass' ihn auf deine Art, fass' ihn auf deine recht!", "tokens": ["Fass'", "ihn", "auf", "dei\u00b7ne", "Art", ",", "fass'", "ihn", "auf", "dei\u00b7ne", "recht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So gut als solchen Herrn kann fassen solch ein Knecht.", "tokens": ["So", "gut", "als", "sol\u00b7chen", "Herrn", "kann", "fas\u00b7sen", "solch", "ein", "Knecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PIAT", "NN", "VMFIN", "ADV", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Und dank' ihm, da\u00df ins Aug' ihn jeder fassen darf,", "tokens": ["Und", "dank'", "ihm", ",", "da\u00df", "ins", "Aug'", "ihn", "je\u00b7der", "fas\u00b7sen", "darf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "APPRART", "NN", "PPER", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob scharf ob bl\u00f6d' es sei, was ist hier bl\u00f6d' und scharf?", "tokens": ["Ob", "scharf", "ob", "bl\u00f6d'", "es", "sei", ",", "was", "ist", "hier", "bl\u00f6d'", "und", "scharf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KOUS", "VVFIN", "PPER", "VAFIN", "$,", "PWS", "VAFIN", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "In wessen Auge sich ein Stral vom Herren spiegelt,", "tokens": ["In", "wes\u00b7sen", "Au\u00b7ge", "sich", "ein", "Stral", "vom", "Her\u00b7ren", "spie\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der dient dem Herrn, sein Dienst ist ihm vom Herrn besiegelt.", "tokens": ["Der", "dient", "dem", "Herrn", ",", "sein", "Dienst", "ist", "ihm", "vom", "Herrn", "be\u00b7sie\u00b7gelt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}