{"textgrid.poem.38776": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[nie hat die Eitelkeit mein Herz betrogen]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nie hat die Eitelkeit mein Herz betrogen,", "tokens": ["Nie", "hat", "die", "Ei\u00b7tel\u00b7keit", "mein", "Herz", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um, leeren Sinn's, mit Liebe nur zu spielen,", "tokens": ["Um", ",", "lee\u00b7ren", "Sinn's", ",", "mit", "Lie\u00b7be", "nur", "zu", "spie\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "$,", "ADJA", "NN", "$,", "APPR", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wollten sch\u00f6ne Augen nach mir zielen", "tokens": ["Und", "woll\u00b7ten", "sch\u00f6\u00b7ne", "Au\u00b7gen", "nach", "mir", "zie\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADJA", "NN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hat bl\u00f6de sich mein Sinn zur\u00fcck gezogen:", "tokens": ["Hat", "bl\u00f6\u00b7de", "sich", "mein", "Sinn", "zu\u00b7r\u00fcck", "ge\u00b7zo\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "PRF", "PPOSAT", "NN", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nie hab' ich Lust, nie Schmerzen mir gelogen,", "tokens": ["Nie", "hab'", "ich", "Lust", ",", "nie", "Schmer\u00b7zen", "mir", "ge\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "ADV", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Ahndung und Gedicht mich selbst zu f\u00fchlen,", "tokens": ["In", "Ahn\u00b7dung", "und", "Ge\u00b7dicht", "mich", "selbst", "zu", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein frommer Zweifel l\u00f6schte mit dem k\u00fchlen", "tokens": ["Ein", "from\u00b7mer", "Zwei\u00b7fel", "l\u00f6schte", "mit", "dem", "k\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gew\u00e4sser jeden Brand mit sanften Wogen.", "tokens": ["Ge\u00b7w\u00e4s\u00b7ser", "je\u00b7den", "Brand", "mit", "sanf\u00b7ten", "Wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zuerst mu\u00df ich das Wort mir selber sagen,", "tokens": ["Zu\u00b7erst", "mu\u00df", "ich", "das", "Wort", "mir", "sel\u00b7ber", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jetzt wei\u00df ich ohne Trug, ich leb' und liebe,", "tokens": ["Jetzt", "wei\u00df", "ich", "oh\u00b7ne", "Trug", ",", "ich", "leb'", "und", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dies eine nur sey Gl\u00fcck mir und Verderben.", "tokens": ["Dies", "ei\u00b7ne", "nur", "sey", "Gl\u00fcck", "mir", "und", "Ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADV", "VAFIN", "NN", "PPER", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Empfind' es, Herz, verschlie\u00df, o Mund, die Klagen;", "tokens": ["Emp\u00b7find'", "es", ",", "Herz", ",", "ver\u00b7schlie\u00df", ",", "o", "Mund", ",", "die", "Kla\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "VVFIN", "$,", "FM", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Begl\u00fcckt, wenn ich auch unverstanden bliebe,", "tokens": ["Be\u00b7gl\u00fcckt", ",", "wenn", "ich", "auch", "un\u00b7ver\u00b7stan\u00b7den", "blie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gern will ich doch der einzgen Liebe sterben.", "tokens": ["Gern", "will", "ich", "doch", "der", "einz\u00b7gen", "Lie\u00b7be", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}