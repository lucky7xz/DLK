{"textgrid.poem.56468": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es schattet dunkler noch des Wassers Scho\u00df,", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es schattet dunkler noch des Wassers Scho\u00df,", "tokens": ["Es", "schat\u00b7tet", "dunk\u00b7ler", "noch", "des", "Was\u00b7sers", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Tief unten brennt ein Licht, ein rotes Mal", "tokens": ["Tief", "un\u00b7ten", "brennt", "ein", "Licht", ",", "ein", "ro\u00b7tes", "Mal"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ADV", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Am schwarzen Leib der Nacht, wo bodenlos", "tokens": ["Am", "schwar\u00b7zen", "Leib", "der", "Nacht", ",", "wo", "bo\u00b7den\u00b7los"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,", "PWAV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Tiefe sinkt. Und auf dem dunklen Tal,", "tokens": ["Die", "Tie\u00b7fe", "sinkt", ".", "Und", "auf", "dem", "dunk\u00b7len", "Tal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mit gr\u00fcnem Fittich auf der dunklen Flut", "tokens": ["Mit", "gr\u00fc\u00b7nem", "Fit\u00b7tich", "auf", "der", "dunk\u00b7len", "Flut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Flattert der Schlaf, der Schnabel dunkelrot,", "tokens": ["Flat\u00b7tert", "der", "Schlaf", ",", "der", "Schna\u00b7bel", "dun\u00b7kel\u00b7rot", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Drin eine Lilie welkt, der Nacht Salut,", "tokens": ["Drin", "ei\u00b7ne", "Li\u00b7lie", "welkt", ",", "der", "Nacht", "Sa\u00b7lut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den Kopf von einem Greise gelb und tot.", "tokens": ["Den", "Kopf", "von", "ei\u00b7nem", "Grei\u00b7se", "gelb", "und", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Er sch\u00fcttelt seine Federn wie ein Pfau.", "tokens": ["Er", "sch\u00fct\u00b7telt", "sei\u00b7ne", "Fe\u00b7dern", "wie", "ein", "Pfau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Tr\u00e4ume wandern wie ein lila Hauch", "tokens": ["Die", "Tr\u00e4u\u00b7me", "wan\u00b7dern", "wie", "ein", "li\u00b7la", "Hauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Um seine Schwinge, wie ein blasser Tau.", "tokens": ["Um", "sei\u00b7ne", "Schwin\u00b7ge", ",", "wie", "ein", "blas\u00b7ser", "Tau", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In ihre Wolke taucht er, in den Rauch.", "tokens": ["In", "ih\u00b7re", "Wol\u00b7ke", "taucht", "er", ",", "in", "den", "Rauch", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die gro\u00dfen B\u00e4ume wandern durch die Nacht", "tokens": ["Die", "gro\u00b7\u00dfen", "B\u00e4u\u00b7me", "wan\u00b7dern", "durch", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit langem Schatten, der hin\u00fcber l\u00e4uft", "tokens": ["Mit", "lan\u00b7gem", "Schat\u00b7ten", ",", "der", "hin\u00b7\u00fc\u00b7ber", "l\u00e4uft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ins wei\u00dfe Herz der Schl\u00e4fer, die bewacht", "tokens": ["Ins", "wei\u00b7\u00dfe", "Herz", "der", "Schl\u00e4\u00b7fer", ",", "die", "be\u00b7wacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der kalte Mond, der seine Gifte tr\u00e4uft", "tokens": ["Der", "kal\u00b7te", "Mond", ",", "der", "sei\u00b7ne", "Gif\u00b7te", "tr\u00e4uft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wie ein erfahrner Arzt tief in ihr Blut.", "tokens": ["Wie", "ein", "er\u00b7fahr\u00b7ner", "Arzt", "tief", "in", "ihr", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie liegen fremd einander, stumm, im Ha\u00df", "tokens": ["Sie", "lie\u00b7gen", "fremd", "ein\u00b7an\u00b7der", ",", "stumm", ",", "im", "Ha\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "$,", "ADJD", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der dunklen Tr\u00e4ume, in verborgner Wut.", "tokens": ["Der", "dunk\u00b7len", "Tr\u00e4u\u00b7me", ",", "in", "ver\u00b7borg\u00b7ner", "Wut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und ihre Stirn wird von den Giften bla\u00df.", "tokens": ["Und", "ih\u00b7re", "Stirn", "wird", "von", "den", "Gif\u00b7ten", "bla\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Der Baum von Schatten klammert um ihr Herz", "tokens": ["Der", "Baum", "von", "Schat\u00b7ten", "klam\u00b7mert", "um", "ihr", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und senkt die Wurzeln ein. Er steigt empor", "tokens": ["Und", "senkt", "die", "Wur\u00b7zeln", "ein", ".", "Er", "steigt", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und saugt sie aus. Sie st\u00f6hnen auf vor Schmerz.", "tokens": ["Und", "saugt", "sie", "aus", ".", "Sie", "st\u00f6h\u00b7nen", "auf", "vor", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er ragt herauf, am Turm der Nacht, am Tor", "tokens": ["Er", "ragt", "her\u00b7auf", ",", "am", "Turm", "der", "Nacht", ",", "am", "Tor"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "APPRART", "NN", "ART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Der blinden Stille. In die Zweige fliegt", "tokens": ["Der", "blin\u00b7den", "Stil\u00b7le", ".", "In", "die", "Zwei\u00b7ge", "fliegt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Schlaf. Und seine kalte Schwinge streift", "tokens": ["Der", "Schlaf", ".", "Und", "sei\u00b7ne", "kal\u00b7te", "Schwin\u00b7ge", "streift"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "KON", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die schwere Nacht, die auf den Schl\u00e4fern liegt", "tokens": ["Die", "schwe\u00b7re", "Nacht", ",", "die", "auf", "den", "Schl\u00e4\u00b7fern", "liegt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und ihre Stirn mit Qualen wei\u00df bereift.", "tokens": ["Und", "ih\u00b7re", "Stirn", "mit", "Qua\u00b7len", "wei\u00df", "be\u00b7reift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Er singt. Ein Ton von krankem Violett", "tokens": ["Er", "singt", ".", "Ein", "Ton", "von", "kran\u00b7kem", "Vi\u00b7o\u00b7lett"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "St\u00f6\u00dft an den Raum. Der Tod geht. Manches Haar", "tokens": ["St\u00f6\u00dft", "an", "den", "Raum", ".", "Der", "Tod", "geht", ".", "Man\u00b7ches", "Haar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "$.", "ART", "NN", "VVFIN", "$.", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Streicht er zur\u00fcck. Ein Kreuz, Asche und Fett,", "tokens": ["Streicht", "er", "zu\u00b7r\u00fcck", ".", "Ein", "Kreuz", ",", "A\u00b7sche", "und", "Fett", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "ART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "So malt er seine Frucht im welken Jahr.", "tokens": ["So", "malt", "er", "sei\u00b7ne", "Frucht", "im", "wel\u00b7ken", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}