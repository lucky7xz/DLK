{"dta.poem.14968": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Zueignung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1814", "urn": "urn:nbn:de:kobv:b4-200905193258", "language": ["de:0.99"], "booktitle": "K\u00f6rner, Theodor: Leyer und Schwerdt. Berlin, 1814."}, "poem": {"stanza.1": {"line.1": {"text": "An den verwegnen Zitherspieler denkt,", "tokens": ["An", "den", "ver\u00b7weg\u00b7nen", "Zi\u00b7ther\u00b7spie\u00b7ler", "denkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und deren Bild, so oft ich es erneue,", "tokens": ["Und", "de\u00b7ren", "Bild", ",", "so", "oft", "ich", "es", "er\u00b7neu\u00b7e", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "$,", "ADV", "ADV", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir stillen Frieden in die Seele senkt,", "tokens": ["Mir", "stil\u00b7len", "Frie\u00b7den", "in", "die", "See\u00b7le", "senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zwar hat ", "tokens": ["Zwar", "hat"], "token_info": ["word", "word"], "pos": ["ADV", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Hat st\u00fcrmisch manche Stunde ", "tokens": ["Hat", "st\u00fcr\u00b7misch", "man\u00b7che", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "So bleibt mir hold! \u2014 des Vaterlandes Fahnen,", "tokens": ["So", "bleibt", "mir", "hold", "!", "des", "Va\u00b7ter\u00b7lan\u00b7des", "Fah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$.", "$(", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Hoch flattern sie am deutschen Freiheitsport.", "tokens": ["Hoch", "flat\u00b7tern", "sie", "am", "deut\u00b7schen", "Frei\u00b7heits\u00b7port", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es ruft die heil'ge Sprache unsrer Ahnen:", "tokens": ["Es", "ruft", "die", "heil'\u00b7ge", "Spra\u00b7che", "uns\u00b7rer", "Ah\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u201eihr S\u00e4nger, vor! und sch\u00fctzt das deutsche Wort!\u201c", "tokens": ["\u201e", "ihr", "S\u00e4n\u00b7ger", ",", "vor", "!", "und", "sch\u00fctzt", "das", "deut\u00b7sche", "Wort", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PTKVZ", "$.", "KON", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das k\u00fchne Herz l\u00e4\u00dft sich nicht l\u00e4nger mahnen,", "tokens": ["Das", "k\u00fch\u00b7ne", "Herz", "l\u00e4\u00dft", "sich", "nicht", "l\u00e4n\u00b7ger", "mah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Der Sturm der Schlachten tr\u00e4gt es brausend fort,", "tokens": ["Der", "Sturm", "der", "Schlach\u00b7ten", "tr\u00e4gt", "es", "brau\u00b7send", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Leyer schweigt, die blanken Schwerdter klingen,", "tokens": ["Die", "Le\u00b7yer", "schweigt", ",", "die", "blan\u00b7ken", "Schwerd\u00b7ter", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Heraus, mein Schwerdt! Magst auch dein Liedchen singen.", "tokens": ["He\u00b7raus", ",", "mein", "Schwerdt", "!", "Magst", "auch", "dein", "Lied\u00b7chen", "sin\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$.", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Laut tobt der Kampf! \u2014 lebt wohl, ", "tokens": ["Laut", "tobt", "der", "Kampf", "!", "lebt", "wohl", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "$.", "$(", "VVFIN", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Es mag ", "tokens": ["Es", "mag"], "token_info": ["word", "word"], "pos": ["PPER", "VMFIN"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Es trage sanft sein Bild vor Euren Blick. \u2014", "tokens": ["Es", "tra\u00b7ge", "sanft", "sein", "Bild", "vor", "Eu\u00b7ren", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und sollt' ich einst im Siegesheimzug fehlen, \u2014", "tokens": ["Und", "sollt'", "ich", "einst", "im", "Sie\u00b7ges\u00b7heim\u00b7zug", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Weint nicht um mich, beneidet mir mein Gl\u00fcck,", "tokens": ["Weint", "nicht", "um", "mich", ",", "be\u00b7nei\u00b7det", "mir", "mein", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "PPER", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Denn was berauscht, die ", "tokens": ["Denn", "was", "be\u00b7rauscht", ",", "die"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KON", "PWS", "VVFIN", "$,", "PRELS"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Das hat des ", "tokens": ["Das", "hat", "des"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}}}}