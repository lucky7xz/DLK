{"textgrid.poem.25636": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "Die goldene Leyer", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Maz reimet kein Gedicht,", "tokens": ["Maz", "rei\u00b7met", "kein", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Worin er nicht", "tokens": ["Wo\u00b7rin", "er", "nicht"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPER", "PTKNEG"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Von seiner goldnen Leyer spricht.", "tokens": ["Von", "sei\u00b7ner", "gold\u00b7nen", "Le\u00b7yer", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du Narr, der immer Hunger hat,", "tokens": ["Du", "Narr", ",", "der", "im\u00b7mer", "Hun\u00b7ger", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "ADV", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verkaufe sie, und i\u00df dich satt.", "tokens": ["Ver\u00b7kau\u00b7fe", "sie", ",", "und", "i\u00df", "dich", "satt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}