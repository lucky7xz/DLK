{"dta.poem.3194": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "45.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gemeinverst\u00e4ndlich sei ein Buch, das zur Erbauung", "tokens": ["Ge\u00b7mein\u00b7ver\u00b7st\u00e4nd\u00b7lich", "sei", "ein", "Buch", ",", "das", "zur", "Er\u00b7bau\u00b7ung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Volk hat in der Hand, zu t\u00e4glicher Beschauung.", "tokens": ["Das", "Volk", "hat", "in", "der", "Hand", ",", "zu", "t\u00e4g\u00b7li\u00b7cher", "Be\u00b7schau\u00b7ung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch etwas darf darin und soll seyn unverst\u00e4ndlich,", "tokens": ["Doch", "et\u00b7was", "darf", "da\u00b7rin", "und", "soll", "seyn", "un\u00b7ver\u00b7st\u00e4nd\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "PAV", "KON", "VMFIN", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Damit die Andacht sich daran erbau' unendlich.", "tokens": ["Da\u00b7mit", "die", "An\u00b7dacht", "sich", "da\u00b7ran", "er\u00b7bau'", "un\u00b7end\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PRF", "PAV", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn ein Verst\u00e4ndliches ist endlich auszubeuten,", "tokens": ["Denn", "ein", "Ver\u00b7st\u00e4nd\u00b7li\u00b7ches", "ist", "end\u00b7lich", "aus\u00b7zu\u00b7beu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VAFIN", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Unverst\u00e4ndliches unendlich umzudeuten.", "tokens": ["Ein", "Un\u00b7ver\u00b7st\u00e4nd\u00b7li\u00b7ches", "un\u00b7end\u00b7lich", "um\u00b7zu\u00b7deu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}