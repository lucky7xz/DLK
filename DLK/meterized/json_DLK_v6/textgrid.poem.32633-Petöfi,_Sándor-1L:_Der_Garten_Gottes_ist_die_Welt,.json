{"textgrid.poem.32633": {"metadata": {"author": {"name": "Pet\u00f6fi, S\u00e1ndor", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Garten Gottes ist die Welt,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Garten Gottes ist die Welt,", "tokens": ["Der", "Gar\u00b7ten", "Got\u00b7tes", "ist", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Blumen oder Unkraut seid", "tokens": ["Und", "Blu\u00b7men", "o\u00b7der", "Un\u00b7kraut", "seid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Menschen drin.", "tokens": ["Ihr", "Men\u00b7schen", "drin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ich aber bin", "tokens": ["Ich", "a\u00b7ber", "bin"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ein Saatkorn nur. Vielleicht gedeiht", "tokens": ["Ein", "Saat\u00b7korn", "nur", ".", "Viel\u00b7leicht", "ge\u00b7deiht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dies K\u00f6rnlein, wenn es Gott gef\u00e4llt.", "tokens": ["Dies", "K\u00f6rn\u00b7lein", ",", "wenn", "es", "Gott", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "KOUS", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Tiefe meiner Brust ist rein,", "tokens": ["Die", "Tie\u00b7fe", "mei\u00b7ner", "Brust", "ist", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Himmel selber go\u00df hinein", "tokens": ["Der", "Him\u00b7mel", "sel\u00b7ber", "go\u00df", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Flammenmeer,", "tokens": ["Ein", "Flam\u00b7men\u00b7meer", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Das keusch und hehr", "tokens": ["Das", "keusch", "und", "hehr"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ADJD", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Auf dem Altar der Tugend brennt \u2013", "tokens": ["Auf", "dem", "Al\u00b7tar", "der", "Tu\u00b7gend", "brennt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil noch das Herz nicht B\u00f6ses kennt.", "tokens": ["Weil", "noch", "das", "Herz", "nicht", "B\u00f6\u00b7ses", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PTKNEG", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn mir das Schicksal Gunst verwehrt,", "tokens": ["Wenn", "mir", "das", "Schick\u00b7sal", "Gunst", "ver\u00b7wehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich trage, was es mir beschert,", "tokens": ["Ich", "tra\u00b7ge", ",", "was", "es", "mir", "be\u00b7schert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie's just bestimmt,", "tokens": ["Wie's", "just", "be\u00b7stimmt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Es gibt, es nimmt,", "tokens": ["Es", "gibt", ",", "es", "nimmt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Gar oft ist's nicht mit Gl\u00fcck gepaart,", "tokens": ["Gar", "oft", "ist's", "nicht", "mit", "Gl\u00fcck", "ge\u00b7paart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PTKNEG", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wandelbar ist seine Art.", "tokens": ["Und", "wan\u00b7del\u00b7bar", "ist", "sei\u00b7ne", "Art", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und wie die Ebne, grad und frei,", "tokens": ["Und", "wie", "die", "Eb\u00b7ne", ",", "grad", "und", "frei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo ich geboren, \u2013 wahr und treu", "tokens": ["Wo", "ich", "ge\u00b7bo\u00b7ren", ",", "\u2013", "wahr", "und", "treu"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVPP", "$,", "$(", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist meine Art", "tokens": ["Ist", "mei\u00b7ne", "Art"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Mir stets gewahrt,", "tokens": ["Mir", "stets", "ge\u00b7wahrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Mein Mund spricht das nur, was ich f\u00fchl'", "tokens": ["Mein", "Mund", "spricht", "das", "nur", ",", "was", "ich", "f\u00fchl'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PDS", "ADV", "$,", "PWS", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und aufrecht schreit' ich auf mein Ziel.", "tokens": ["Und", "auf\u00b7recht", "schreit'", "ich", "auf", "mein", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Gott hat mir in das Herz gelegt", "tokens": ["Gott", "hat", "mir", "in", "das", "Herz", "ge\u00b7legt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Keim der Liebe. Wurzeln schl\u00e4gt", "tokens": ["Den", "Keim", "der", "Lie\u00b7be", ".", "Wur\u00b7zeln", "schl\u00e4gt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$.", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er da, und bl\u00fcht,", "tokens": ["Er", "da", ",", "und", "bl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Mein Herz ergl\u00fcht,", "tokens": ["Mein", "Herz", "er\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die Blumen windet es zum Kranz \u2013", "tokens": ["Die", "Blu\u00b7men", "win\u00b7det", "es", "zum", "Kranz", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zum Preis und Heil des Vaterlands!", "tokens": ["Zum", "Preis", "und", "Heil", "des", "Va\u00b7ter\u00b7lands", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}