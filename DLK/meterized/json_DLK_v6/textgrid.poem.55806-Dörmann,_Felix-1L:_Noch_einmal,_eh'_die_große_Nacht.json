{"textgrid.poem.55806": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Noch einmal, eh' die gro\u00dfe Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch einmal, eh' die gro\u00dfe Nacht", "tokens": ["Noch", "ein\u00b7mal", ",", "eh'", "die", "gro\u00b7\u00dfe", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erdr\u00fcckend mich umf\u00e4ngt,", "tokens": ["Er\u00b7dr\u00fc\u00b7ckend", "mich", "um\u00b7f\u00e4ngt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hat eines Auges Sonnenpracht", "tokens": ["Hat", "ei\u00b7nes", "Au\u00b7ges", "Son\u00b7nen\u00b7pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir einen Blick geschenkt.", "tokens": ["Mir", "ei\u00b7nen", "Blick", "ge\u00b7schenkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es traf ein lichter Funkenstrahl", "tokens": ["Es", "traf", "ein", "lich\u00b7ter", "Fun\u00b7ken\u00b7strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Dornendiadem,", "tokens": ["Mein", "Dor\u00b7nen\u00b7di\u00b7a\u00b7dem", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ich m\u00f6chte gern ein letztesmal", "tokens": ["Ich", "m\u00f6ch\u00b7te", "gern", "ein", "letz\u00b7tes\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ART", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch beten! \u2013 doch zu wem?", "tokens": ["Noch", "be\u00b7ten", "!", "\u2013", "doch", "zu", "wem", "?"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$.", "$(", "ADV", "APPR", "PWS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}