{"textgrid.poem.62605": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Der 143. Psalm", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "HeRR! des Himmels und der Erde,", "tokens": ["HeRR", "!", "des", "Him\u00b7mels", "und", "der", "Er\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieh! ich wende mich zu dir,", "tokens": ["Sieh", "!", "ich", "wen\u00b7de", "mich", "zu", "dir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "PRF", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Damit ich getr\u00f6stet werde,", "tokens": ["Da\u00b7mit", "ich", "ge\u00b7tr\u00f6s\u00b7tet", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn mein Geist ist ausser mir.", "tokens": ["Denn", "mein", "Geist", "ist", "aus\u00b7ser", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Herr, mein Sch\u00f6pfer! komm und kehre", "tokens": ["Herr", ",", "mein", "Sch\u00f6p\u00b7fer", "!", "komm", "und", "keh\u00b7re"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "$.", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dich zu meinem Klag-Geschrey", "tokens": ["Dich", "zu", "mei\u00b7nem", "Klag\u00b7Ge\u00b7schrey"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach! vernimm doch! ach! erh\u00f6re", "tokens": ["Ach", "!", "ver\u00b7nimm", "doch", "!", "ach", "!", "er\u00b7h\u00f6\u00b7re"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["ITJ", "$.", "VVIMP", "ADV", "$.", "XY", "$.", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mein Gebeth, und steh mir bey.", "tokens": ["Mein", "Ge\u00b7beth", ",", "und", "steh", "mir", "bey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "HeRR! um deiner Wahrheit willen,", "tokens": ["HeRR", "!", "um", "dei\u00b7ner", "Wahr\u00b7heit", "wil\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da du Beter h\u00f6ren wilt,", "tokens": ["Da", "du", "Be\u00b7ter", "h\u00f6\u00b7ren", "wilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollest du dein Wort erf\u00fcllen,", "tokens": ["Wol\u00b7lest", "du", "dein", "Wort", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welches meinen Jammer stillt!", "tokens": ["Wel\u00b7ches", "mei\u00b7nen", "Jam\u00b7mer", "stillt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Herr! du bist gerecht, drum h\u00f6re,", "tokens": ["Herr", "!", "du", "bist", "ge\u00b7recht", ",", "drum", "h\u00f6\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "ADJD", "$,", "PAV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meine Seufzer gn\u00e4dig an.", "tokens": ["Mei\u00b7ne", "Seuf\u00b7zer", "gn\u00e4\u00b7dig", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Es vermehrt ja deine Ehre,", "tokens": ["Es", "ver\u00b7mehrt", "ja", "dei\u00b7ne", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn der Mensch dich preisen kan,", "tokens": ["Wenn", "der", "Mensch", "dich", "prei\u00b7sen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Meiner S\u00fcnden grosse Menge", "tokens": ["Mei\u00b7ner", "S\u00fcn\u00b7den", "gros\u00b7se", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schreyen Rache \u00fcber mich;", "tokens": ["Schre\u00b7yen", "Ra\u00b7che", "\u00fc\u00b7ber", "mich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch verfahr nicht nach der Strenge,", "tokens": ["Doch", "ver\u00b7fahr", "nicht", "nach", "der", "Stren\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Herr! Ach! schone gn\u00e4diglich.", "tokens": ["Herr", "!", "Ach", "!", "scho\u00b7ne", "gn\u00e4\u00b7di\u00b7glich", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "ITJ", "$.", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Geh mit mir nicht ins Gerichte:", "tokens": ["Geh", "mit", "mir", "nicht", "ins", "Ge\u00b7rich\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "PTKNEG", "APPRART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Denn ich kan ja nicht bestehn.", "tokens": ["Denn", "ich", "kan", "ja", "nicht", "be\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Zeigst du mir dein Zorn-Gesichte,", "tokens": ["Zeigst", "du", "mir", "dein", "Zorn\u00b7Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "O! so mu\u00df ich untergehn.", "tokens": ["O", "!", "so", "mu\u00df", "ich", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sieh; der Feind verfolgt mein Leben;", "tokens": ["Sieh", ";", "der", "Feind", "ver\u00b7folgt", "mein", "Le\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Seele dr\u00fcckt er scharf;", "tokens": ["Mei\u00b7ne", "See\u00b7le", "dr\u00fcckt", "er", "scharf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich mu\u00df an der Erde kleben,", "tokens": ["Ich", "mu\u00df", "an", "der", "Er\u00b7de", "kle\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Weil er mich darnieder warf.", "tokens": ["Weil", "er", "mich", "dar\u00b7nie\u00b7der", "warf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PAV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sieh! er leget mich in Schatten,", "tokens": ["Sieh", "!", "er", "le\u00b7get", "mich", "in", "Schat\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie die Todten in der Welt.", "tokens": ["Wie", "die", "Tod\u00b7ten", "in", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Komm, mein Sch\u00f6pfer! hilf mir Matten,", "tokens": ["Komm", ",", "mein", "Sch\u00f6p\u00b7fer", "!", "hilf", "mir", "Mat\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$.", "VVIMP", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df mein Geist den Sieg erh\u00e4lt.", "tokens": ["Da\u00df", "mein", "Geist", "den", "Sieg", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sieh! wie ich ge\u00e4ngstget werde;", "tokens": ["Sieh", "!", "wie", "ich", "ge\u00b7\u00e4ngst\u00b7get", "wer\u00b7de", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mein Geist gequ\u00e4let wird,", "tokens": ["Und", "mein", "Geist", "ge\u00b7qu\u00e4\u00b7let", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie betr\u00fcbt ich mich geberde,", "tokens": ["Wie", "be\u00b7tr\u00fcbt", "ich", "mich", "ge\u00b7ber\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mein Herz nach H\u00fclffe girrt.", "tokens": ["Und", "mein", "Herz", "nach", "H\u00fclf\u00b7fe", "girrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Es ist ganz in mir verzehret,", "tokens": ["Es", "ist", "ganz", "in", "mir", "ver\u00b7zeh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Und von vielen Seufzen schwach.", "tokens": ["Und", "von", "vie\u00b7len", "Seuf\u00b7zen", "schwach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach wenn werd ich doch erh\u00f6ret!", "tokens": ["Ach", "wenn", "werd", "ich", "doch", "er\u00b7h\u00f6\u00b7ret", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "VAFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hemme meinen Thr\u00e4nen-Bach.", "tokens": ["Hem\u00b7me", "mei\u00b7nen", "Thr\u00e4\u00b7nen\u00b7Bach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich gedenk der ersten Tage,", "tokens": ["Ich", "ge\u00b7denk", "der", "ers\u00b7ten", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da du mich so oft erh\u00f6rt,", "tokens": ["Da", "du", "mich", "so", "oft", "er\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da du manche Noth und Plage", "tokens": ["Da", "du", "man\u00b7che", "Noth", "und", "Pla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "NN", "KON", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Gn\u00e4dig von mir abgekehrt.", "tokens": ["Gn\u00e4\u00b7dig", "von", "mir", "ab\u00b7ge\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deine Thaten, deine Werke", "tokens": ["Dei\u00b7ne", "Tha\u00b7ten", ",", "dei\u00b7ne", "Wer\u00b7ke"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hab ich oftermahls gesehn.", "tokens": ["Hab", "ich", "of\u00b7ter\u00b7mahls", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Darum mich auch jetzo st\u00e4rke,", "tokens": ["Da\u00b7rum", "mich", "auch", "jet\u00b7zo", "st\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df ich kan dein Lob erh\u00f6hn.", "tokens": ["Da\u00df", "ich", "kan", "dein", "Lob", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sieh! wie ich so sehnlich bethe,", "tokens": ["Sieh", "!", "wie", "ich", "so", "sehn\u00b7lich", "be\u00b7the", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ich win\u00dfle, klag und fleh!", "tokens": ["Wie", "ich", "win\u00df\u00b7le", ",", "klag", "und", "fleh", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ich glaubig vor dich trete!", "tokens": ["Wie", "ich", "glau\u00b7big", "vor", "dich", "tre\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Herz und Hand reicht in die H\u00f6h.", "tokens": ["Herz", "und", "Hand", "reicht", "in", "die", "H\u00f6h", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Meine Seele d\u00fcrst nach Seegen", "tokens": ["Mei\u00b7ne", "See\u00b7le", "d\u00fcrst", "nach", "See\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gleich wie ein verdorrtes Land", "tokens": ["Gleich", "wie", "ein", "ver\u00b7dorr\u00b7tes", "Land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nach den k\u00fchlen Abend-Regen.", "tokens": ["Nach", "den", "k\u00fch\u00b7len", "A\u00b7ben\u00b7dRe\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hilf! der Schmerz nimmt \u00fcberhand.", "tokens": ["Hilf", "!", "der", "Schmerz", "nimmt", "\u00fc\u00b7ber\u00b7hand", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "HeRR! erh\u00f6re mich beyzeiten;", "tokens": ["HeRR", "!", "er\u00b7h\u00f6\u00b7re", "mich", "bey\u00b7zei\u00b7t