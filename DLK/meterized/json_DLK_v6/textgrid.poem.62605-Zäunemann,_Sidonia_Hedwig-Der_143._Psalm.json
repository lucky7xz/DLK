, "mich", "nach", "dei\u00b7nen", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wort und Wohlgefallen thun:", "tokens": ["Wort", "und", "Wohl\u00b7ge\u00b7fal\u00b7len", "thun", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn mein W\u00fcnschen und Erf\u00fcllen,", "tokens": ["Denn", "mein", "W\u00fcn\u00b7schen", "und", "Er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll auf deinen Wink beruhn.", "tokens": ["Soll", "auf", "dei\u00b7nen", "Wink", "be\u00b7ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Du mein Gott! und meine Freude!", "tokens": ["Du", "mein", "Gott", "!", "und", "mei\u00b7ne", "Freu\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$.", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gieb mir deinen guten Geist,", "tokens": ["Gieb", "mir", "dei\u00b7nen", "gu\u00b7ten", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df ich alle Wege meide,", "tokens": ["Da\u00df", "ich", "al\u00b7le", "We\u00b7ge", "mei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die du mir nicht gehen heist.", "tokens": ["Die", "du", "mir", "nicht", "ge\u00b7hen", "heist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "PTKNEG", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Deines grossen Namens wegen,", "tokens": ["Dei\u00b7nes", "gros\u00b7sen", "Na\u00b7mens", "we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00e4rke und erquicke mich!", "tokens": ["St\u00e4r\u00b7ke", "und", "er\u00b7qui\u00b7cke", "mich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Komme mir mit Trost entgegen;", "tokens": ["Kom\u00b7me", "mir", "mit", "Trost", "ent\u00b7ge\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und erh\u00f6r mich gn\u00e4diglich;", "tokens": ["Und", "er\u00b7h\u00f6r", "mich", "gn\u00e4\u00b7di\u00b7glich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Herr! du bist gerecht in allen,", "tokens": ["Herr", "!", "du", "bist", "ge\u00b7recht", "in", "al\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "ADJD", "APPR", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Darum f\u00fchr mich aus der Noth.", "tokens": ["Da\u00b7rum", "f\u00fchr", "mich", "aus", "der", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "La\u00df nicht meine Seele fallen,", "tokens": ["La\u00df", "nicht", "mei\u00b7ne", "See\u00b7le", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sondern f\u00fchr sie aus dem Tod.", "tokens": ["Son\u00b7dern", "f\u00fchr", "sie", "aus", "dem", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Herr! erzeig mir deine Gnade,", "tokens": ["Herr", "!", "er\u00b7zeig", "mir", "dei\u00b7ne", "Gna\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und zerst\u00f6hr der Feinde Schwarm.", "tokens": ["Und", "zer\u00b7st\u00f6hr", "der", "Fein\u00b7de", "Schwarm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df mir ihre List nicht schade;", "tokens": ["Da\u00df", "mir", "ih\u00b7re", "List", "nicht", "scha\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00fcrze sie mit deinem Arm.", "tokens": ["St\u00fcr\u00b7ze", "sie", "mit", "dei\u00b7nem", "Arm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denen die mich \u00e4ngstgen, qu\u00e4len,", "tokens": ["De\u00b7nen", "die", "mich", "\u00e4ngst\u00b7gen", ",", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "PRELS", "PPER", "VVINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und mir stets zuwieder sind,", "tokens": ["Und", "mir", "stets", "zu\u00b7wie\u00b7der", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "La\u00df den b\u00f6sen Anschlag fehlen,", "tokens": ["La\u00df", "den", "b\u00f6\u00b7sen", "An\u00b7schlag", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Denn ich bin dein treues Kind.", "tokens": ["Denn", "ich", "bin", "dein", "treu\u00b7es", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}