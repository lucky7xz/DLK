{"textgrid.poem.36060": {"metadata": {"author": {"name": "Bechstein, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "XxI. Der Warner.", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es t\u00f6nt von heilger Freundschaft die Sage wundervoll,", "tokens": ["Es", "t\u00f6nt", "von", "heil\u00b7ger", "Freund\u00b7schaft", "die", "Sa\u00b7ge", "wun\u00b7der\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Von Freundschaft, die treusorgsam und rettend walten soll.", "tokens": ["Von", "Freund\u00b7schaft", ",", "die", "treu\u00b7sorg\u00b7sam", "und", "ret\u00b7tend", "wal\u00b7ten", "soll", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+--++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Es giebt ein sch\u00f6nes Spr\u00fcchwort; Heil, dem man's sagen kann:", "tokens": ["Es", "giebt", "ein", "sch\u00f6\u00b7nes", "Spr\u00fcch\u00b7wort", ";", "Heil", ",", "dem", "man's", "sa\u00b7gen", "kann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$.", "NN", "$,", "PRELS", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "\u00bbdu bist der treue Eckart, Du warnest Jedermann!\u00ab", "tokens": ["\u00bb", "du", "bist", "der", "treu\u00b7e", "E\u00b7ckart", ",", "Du", "war\u00b7nest", "Je\u00b7der\u00b7mann", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PPER", "VAFIN", "PIS", "$.", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Der treue Eckart folgte dem Herrn in Lust und Pein,", "tokens": ["Der", "treu\u00b7e", "E\u00b7ckart", "folg\u00b7te", "dem", "Herrn", "in", "Lust", "und", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Er ging mit dem Tanh\u00e4user zum Venusberg hinein.", "tokens": ["Er", "ging", "mit", "dem", "Tan\u00b7h\u00e4u\u00b7ser", "zum", "Ve\u00b7nus\u00b7berg", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+--+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Der treue Eckart sitzt noch am Zauberbergesschacht,", "tokens": ["Der", "treu\u00b7e", "E\u00b7ckart", "sitzt", "noch", "am", "Zau\u00b7ber\u00b7ber\u00b7ges\u00b7schacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und warnet wer hinein will, und h\u00e4lt gar ernste Wacht.", "tokens": ["Und", "war\u00b7net", "wer", "hin\u00b7ein", "will", ",", "und", "h\u00e4lt", "gar", "erns\u00b7te", "Wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "ADV", "VMFIN", "$,", "KON", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Der treue Eckart wandelt dem wilden Heer voran,", "tokens": ["Der", "treu\u00b7e", "E\u00b7ckart", "wan\u00b7delt", "dem", "wil\u00b7den", "Heer", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das \u00fcber W\u00e4lder hinbraust auf luftger Wolkenbahn.", "tokens": ["Das", "\u00fc\u00b7ber", "W\u00e4l\u00b7der", "hin\u00b7braust", "auf", "luft\u00b7ger", "Wol\u00b7ken\u00b7bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wer immer mag im Weg sein, wo's Heer vor\u00fcberwogt,", "tokens": ["Wer", "im\u00b7mer", "mag", "im", "Weg", "sein", ",", "wo's", "Heer", "vor\u00b7\u00fc\u00b7ber\u00b7wogt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "APPRART", "NN", "VAINF", "$,", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Zur Seite treten heisst ihn warnend der treue Vogt.", "tokens": ["Zur", "Sei\u00b7te", "tre\u00b7ten", "heisst", "ihn", "war\u00b7nend", "der", "treu\u00b7e", "Vogt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "VVFIN", "PPER", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Zu Faustus, der verd\u00fcstert, die Stirne faltenkraus,", "tokens": ["Zu", "Faus\u00b7tus", ",", "der", "ver\u00b7d\u00fcs\u00b7tert", ",", "die", "Stir\u00b7ne", "fal\u00b7ten\u00b7kraus", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "VVPP", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der Mordthat nachsinnt, schreitet ein fremder Mann ins Hans.", "tokens": ["Der", "Mord\u00b7that", "nach\u00b7sinnt", ",", "schrei\u00b7tet", "ein", "frem\u00b7der", "Mann", "ins", "Hans", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NE", "$."], "meter": "-+--++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ein Greis, am Stabe schleichend, umlockt vom Silberhaar,", "tokens": ["Ein", "Greis", ",", "am", "Sta\u00b7be", "schlei\u00b7chend", ",", "um\u00b7lockt", "vom", "Sil\u00b7ber\u00b7haar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "ADJD", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und glaubhaft will uns d\u00fcnken, dass er der Eckart war.", "tokens": ["Und", "glaub\u00b7haft", "will", "uns", "d\u00fcn\u00b7ken", ",", "dass", "er", "der", "E\u00b7ckart", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbo Faustus, Hochgelahrter, erlaubt ein ernstes Wort,", "tokens": ["\u00bb", "o", "Faus\u00b7tus", ",", "Hoch\u00b7ge\u00b7lahr\u00b7ter", ",", "er\u00b7laubt", "ein", "erns\u00b7tes", "Wort", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "$,", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Auf unheilvollem Pfade hineilt Ihr fort und fort.", "tokens": ["Auf", "un\u00b7heil\u00b7vol\u00b7lem", "Pfa\u00b7de", "hi\u00b7neilt", "Ihr", "fort", "und", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ein Wandrer, dem in W\u00fcsten des Trostes Quell versiegt,", "tokens": ["Ein", "Wand\u00b7rer", ",", "dem", "in", "W\u00fcs\u00b7ten", "des", "Tros\u00b7tes", "Quell", "ver\u00b7siegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ein Pilger, der gefesselt im S\u00fcnden-Kerker liegt.\u00ab", "tokens": ["Ein", "Pil\u00b7ger", ",", "der", "ge\u00b7fes\u00b7selt", "im", "S\u00fcn\u00b7den\u00b7Ker\u00b7ker", "liegt", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbblickt nicht auf mich so z\u00fcrnend, der solche Rede wagt!", "tokens": ["\u00bb", "blickt", "nicht", "auf", "mich", "so", "z\u00fcr\u00b7nend", ",", "der", "sol\u00b7che", "Re\u00b7de", "wagt", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKNEG", "APPR", "PPER", "ADV", "VVPP", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ich bin kein strenger Mahner, der Euch bei Gott verklagt,", "tokens": ["Ich", "bin", "kein", "stren\u00b7ger", "Mah\u00b7ner", ",", "der", "Euch", "bei", "Gott", "ver\u00b7klagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ich bin ein Mann des Friedens, der freundlich Euch erscheint,", "tokens": ["Ich", "bin", "ein", "Mann", "des", "Frie\u00b7dens", ",", "der", "freund\u00b7lich", "Euch", "er\u00b7scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der Eure Kraft betrauert, und Euern Fall beweint.\u00ab", "tokens": ["Der", "Eu\u00b7re", "Kraft", "be\u00b7trau\u00b7ert", ",", "und", "Eu\u00b7ern", "Fall", "be\u00b7weint", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$,", "KON", "PPOSAT", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bbo Faustus, nicht so finster rollt Euer Aug' nach mir!", "tokens": ["\u00bb", "o", "Faus\u00b7tus", ",", "nicht", "so", "fins\u00b7ter", "rollt", "Eu\u00b7er", "Aug'", "nach", "mir", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "$,", "PTKNEG", "ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Denkt, Eures Vaters Schatten der st\u00e4nde trauernd hier.", "tokens": ["Denkt", ",", "Eu\u00b7res", "Va\u00b7ters", "Schat\u00b7ten", "der", "st\u00e4n\u00b7de", "trau\u00b7ernd", "hier", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "NN", "ART", "ADJA", "ADJD", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Denkt, Eurer Mutter Stimme sie t\u00f6ne niederw\u00e4rts", "tokens": ["Denkt", ",", "Eu\u00b7rer", "Mut\u00b7ter", "Stim\u00b7me", "sie", "t\u00f6\u00b7ne", "nie\u00b7der\u00b7w\u00e4rts"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "NN", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Vom Himmel, und sie schmeichle sich z\u00e4rtlich Euch ins Herz!\u00ab", "tokens": ["Vom", "Him\u00b7mel", ",", "und", "sie", "schmeich\u00b7le", "sich", "z\u00e4rt\u00b7lich", "Euch", "ins", "Herz", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "PPER", "VVFIN", "PRF", "ADJD", "PPER", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "\u00bbdenkt, Tiefgesunkner, Armer, ach, so beklagenswerth,", "tokens": ["\u00bb", "denkt", ",", "Tief\u00b7ge\u00b7sun\u00b7kner", ",", "Ar\u00b7mer", ",", "ach", ",", "so", "be\u00b7kla\u00b7gens\u00b7werth", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "$,", "NN", "$,", "ITJ", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es w\u00e4re der Erbarmer beim S\u00fcnder eingekehrt,", "tokens": ["Es", "w\u00e4\u00b7re", "der", "Er\u00b7bar\u00b7mer", "beim", "S\u00fcn\u00b7der", "ein\u00b7ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und woll' Euch liebend heben aus der Versunkenheit,", "tokens": ["Und", "woll'", "Euch", "lie\u00b7bend", "he\u00b7ben", "aus", "der", "Ver\u00b7sun\u00b7ken\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und woll' Euch Gnade geben, sofern Ihr nur bereut!\u00ab", "tokens": ["Und", "woll'", "Euch", "Gna\u00b7de", "ge\u00b7ben", ",", "so\u00b7fern", "Ihr", "nur", "be\u00b7reut", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VMFIN", "PPER", "NN", "VVINF", "$,", "KON", "PPER", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "\u00bbund w\u00e4r' es so, mein Faustus, und k\u00e4m' Euch solches Gl\u00fcck,", "tokens": ["\u00bb", "und", "w\u00e4r'", "es", "so", ",", "mein", "Faus\u00b7tus", ",", "und", "k\u00e4m'", "Euch", "sol\u00b7ches", "Gl\u00fcck", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VAFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Nicht wahr, Ihr kehrtet wieder zum rechten Pfad zur\u00fcck?", "tokens": ["Nicht", "wahr", ",", "Ihr", "kehr\u00b7tet", "wie\u00b7der", "zum", "rech\u00b7ten", "Pfad", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ihr liesset ab vom B\u00f6sen, vom Zauberwerk und Trug?", "tokens": ["Ihr", "lies\u00b7set", "ab", "vom", "B\u00f6\u00b7sen", ",", "vom", "Zau\u00b7ber\u00b7werk", "und", "Trug", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPRART", "NN", "$,", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Liesst von den H\u00f6llengeistern \u2013 und diesen ihren Fluch?\u00ab", "tokens": ["Liesst", "von", "den", "H\u00f6l\u00b7len\u00b7geis\u00b7tern", "\u2013", "und", "die\u00b7sen", "ih\u00b7ren", "Fluch", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$(", "KON", "PDS", "PPOSAT", "NN", "$.", "$("], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.10": {"line.1": {"text": "\u00bbo Faustus, dem als J\u00fcngling ein Gott das Herz durchflammt,", "tokens": ["\u00bb", "o", "Faus\u00b7tus", ",", "dem", "als", "J\u00fcng\u00b7ling", "ein", "Gott", "das", "Herz", "durch\u00b7flammt", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "$,", "PRELS", "KOUS", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ihr habt in Euch ert\u00f6dtet, was doch vom Himmel stammt!", "tokens": ["Ihr", "habt", "in", "Euch", "er\u00b7t\u00f6d\u00b7tet", ",", "was", "doch", "vom", "Him\u00b7mel", "stammt", "!"], "token_info": ["wo