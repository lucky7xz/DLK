"line.1": {"text": "\u00bbo Traum von bessrer Zukunft, Dein Nahen fass' ich kaum!\u00ab", "tokens": ["\u00bb", "o", "Traum", "von", "bess\u00b7rer", "Zu\u00b7kunft", ",", "Dein", "Na\u00b7hen", "fass'", "ich", "kaum", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "NN", "APPR", "ADJA", "NN", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u203awenn Du bereuest, Faustus, dann siehst Du mehr, als Traum!\u2039", "tokens": ["\u203a", "wenn", "Du", "be\u00b7re\u00b7u\u00b7est", ",", "Faus\u00b7tus", ",", "dann", "siehst", "Du", "mehr", ",", "als", "Traum", "!", "\u2039"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "PPER", "VVFIN", "$,", "NE", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "NN", "$.", "$("], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "\u00bbo nenne mir die Quelle, aus der Vergebung fliesst!\u00ab", "tokens": ["\u00bb", "o", "nen\u00b7ne", "mir", "die", "Quel\u00b7le", ",", "aus", "der", "Ver\u00b7ge\u00b7bung", "fliesst", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "VVFIN", "PPER", "ART", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "\u203ader Urborn ew'ger Gnade sich in die Herzen giesst.\u2039", "tokens": ["\u203a", "der", "Ur\u00b7born", "ew'\u00b7ger", "Gna\u00b7de", "sich", "in", "die", "Her\u00b7zen", "giesst", ".", "\u2039"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.19": {"line.1": {"text": "\u00bbdank, Dank Dir, treuer Warner, wer Du auch immer bist,", "tokens": ["\u00bb", "dank", ",", "Dank", "Dir", ",", "treu\u00b7er", "War\u00b7ner", ",", "wer", "Du", "auch", "im\u00b7mer", "bist", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "APPR", "PPER", "$,", "ADJA", "NN", "$,", "PWS", "PPER", "ADV", "ADV", "VAFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der mir ein Stern des Trostes in Nacht erschienen ist.", "tokens": ["Der", "mir", "ein", "Stern", "des", "Tros\u00b7tes", "in", "Nacht", "er\u00b7schie\u00b7nen", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ART", "NN", "APPR", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der mit Prophetenstimme mein starres Herz ger\u00fchrt,", "tokens": ["Der", "mit", "Pro\u00b7phe\u00b7ten\u00b7stim\u00b7me", "mein", "star\u00b7res", "Herz", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der auf die Bahn der Reinen mich liebreich wieder f\u00fchrt!\u00ab", "tokens": ["Der", "auf", "die", "Bahn", "der", "Rei\u00b7nen", "mich", "lieb\u00b7reich", "wie\u00b7der", "f\u00fchrt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "PPER", "ADJD", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.20": {"line.1": {"text": "\u203aheil mir, wenn ich erweckte, was schon erstorben lag!", "tokens": ["\u203a", "heil", "mir", ",", "wenn", "ich", "er\u00b7weck\u00b7te", ",", "was", "schon", "ers\u00b7tor\u00b7ben", "lag", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,", "PRELS", "ADV", "VVPP", "VVFIN", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Heil Dir, wenn Deinem Innern aufgl\u00fcht ein neuer Tag!", "tokens": ["Heil", "Dir", ",", "wenn", "Dei\u00b7nem", "In\u00b7nern", "auf\u00b7gl\u00fcht", "ein", "neu\u00b7er", "Tag", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Gedenke mein \u2013 ich scheide froh mit erf\u00fcllter Pflicht,", "tokens": ["Ge\u00b7den\u00b7ke", "mein", "\u2013", "ich", "schei\u00b7de", "froh", "mit", "er\u00b7f\u00fcll\u00b7ter", "Pflicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "$(", "PPER", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und willst Du Gott vers\u00f6hnen, vergiss die Deine nicht!\u2039", "tokens": ["Und", "willst", "Du", "Gott", "ver\u00b7s\u00f6h\u00b7nen", ",", "ver\u00b7giss", "die", "Dei\u00b7ne", "nicht", "!", "\u2039"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VMFIN", "PPER", "NN", "VVINF", "$,", "VVFIN", "ART", "PPOSAT", "PTKNEG", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.21": {"line.1": {"text": "Er geht, an seiner Stelle Mephisto schleicht herbei,", "tokens": ["Er", "geht", ",", "an", "sei\u00b7ner", "Stel\u00b7le", "Me\u00b7phis\u00b7to", "schleicht", "her\u00b7bei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "NE", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und f\u00fchrt in Faustus Zelle schwarz die Melancholei.", "tokens": ["Und", "f\u00fchrt", "in", "Faus\u00b7tus", "Zel\u00b7le", "schwarz", "die", "Me\u00b7lan\u00b7cho\u00b7lei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "NN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Mephisto sieht dem Greis nach, und lacht so laut er kann:", "tokens": ["Me\u00b7phis\u00b7to", "sieht", "dem", "Greis", "nach", ",", "und", "lacht", "so", "laut", "er", "kann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "ADV", "ADJD", "PPER", "VMFIN", "$."], "meter": "---+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "\u00bbdu bist der treue Eckart, Du warnest Jedermann!\u00ab", "tokens": ["\u00bb", "du", "bist", "der", "treu\u00b7e", "E\u00b7ckart", ",", "Du", "war\u00b7nest", "Je\u00b7der\u00b7mann", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PPER", "VAFIN", "PIS", "$.", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}}}}