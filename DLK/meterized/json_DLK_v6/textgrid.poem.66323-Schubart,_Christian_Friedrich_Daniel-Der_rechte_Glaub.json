{"textgrid.poem.66323": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Der rechte Glaub", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst wandelten zur Himmelspfort'", "tokens": ["Einst", "wan\u00b7del\u00b7ten", "zur", "Him\u00b7mels\u00b7pfort'"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drei abgeschiedne Seelen fort.", "tokens": ["Drei", "ab\u00b7ge\u00b7schied\u00b7ne", "See\u00b7len", "fort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Stuhl von Rom verehrte der", "tokens": ["Den", "Stuhl", "von", "Rom", "ver\u00b7ehr\u00b7te", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE", "VVFIN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und zwei Kalvins und Luthers Lehr'.", "tokens": ["Und", "zwei", "Kal\u00b7vins", "und", "Lu\u00b7thers", "Lehr'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "KON", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der P\u00e4pstler wollt' der erste sein,", "tokens": ["Der", "P\u00e4pst\u00b7ler", "wollt'", "der", "ers\u00b7te", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "ADJA", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und schrie: \u00bbSankt Peter, la\u00df mich 'nein!\u00ab", "tokens": ["Und", "schrie", ":", "\u00bb", "Sankt", "Pe\u00b7ter", ",", "la\u00df", "mich", "'n\u00b7ein", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "VVFIN", "NE", "$,", "VVIMP", "PPER", "NN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Der Eiferer vom Lutherthum", "tokens": ["Der", "Ei\u00b7fe\u00b7rer", "vom", "Lu\u00b7ther\u00b7thum"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sprach: \u00bbGeh ins Purgatorium!", "tokens": ["Sprach", ":", "\u00bb", "Geh", "ins", "Pur\u00b7ga\u00b7to\u00b7ri\u00b7um", "!"], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Lutherus hat die Stra\u00dfe ", "tokens": ["Lu\u00b7the\u00b7rus", "hat", "die", "Stra\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Geebnet zu des Himmels Th\u00fcr!\u00ab \u2013", "tokens": ["Geeb\u00b7net", "zu", "des", "Him\u00b7mels", "Th\u00fcr", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Darauf entstand gar gro\u00dfer Streit", "tokens": ["Da\u00b7rauf", "ent\u00b7stand", "gar", "gro\u00b7\u00dfer", "Streit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Von Himmel, H\u00f6ll' und Seligkeit,", "tokens": ["Von", "Him\u00b7mel", ",", "H\u00f6ll'", "und", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Von Christen- und von Heidenlohn,", "tokens": ["Von", "Chris\u00b7ten", "und", "von", "Hei\u00b7den\u00b7lohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Von Transsubstantiation,", "tokens": ["Von", "Trans\u00b7subs\u00b7tan\u00b7ti\u00b7a\u00b7ti\u00b7on", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Vom Glauben und von Werken gut", "tokens": ["Vom", "Glau\u00b7ben", "und", "von", "Wer\u00b7ken", "gut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "War gar ein schrecklicher Disput.", "tokens": ["War", "gar", "ein", "schreck\u00b7li\u00b7cher", "Dis\u00b7put", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Der Kalvinist sprach: Schweigt einmal,", "tokens": ["Der", "Kal\u00b7vi\u00b7nist", "sprach", ":", "Schweigt", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Ich halt' mich an die Gnadenwahl!", "tokens": ["Ich", "halt'", "mich", "an", "die", "Gna\u00b7den\u00b7wahl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Drauf hub der Streit von neuem an,", "tokens": ["Drauf", "hub", "der", "Streit", "von", "neu\u00b7em", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "APPR", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und noch ward's Thor nicht aufgethan;", "tokens": ["Und", "noch", "ward's", "Thor", "nicht", "auf\u00b7ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Doch endlich war Sankt Peter m\u00fcd", "tokens": ["Doch", "end\u00b7lich", "war", "Sankt", "Pe\u00b7ter", "m\u00fcd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "VVFIN", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Des L\u00e4rms und bot den Z\u00e4nkern Fried';", "tokens": ["Des", "L\u00e4rms", "und", "bot", "den", "Z\u00e4n\u00b7kern", "Fried'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Guckt durchs halb offne Thor und spricht:", "tokens": ["Guckt", "durchs", "halb", "off\u00b7ne", "Thor", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJD", "ADJA", "NN", "KON", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.24": {"text": "F\u00fcr Z\u00e4nker ist der Himmel nicht!", "tokens": ["F\u00fcr", "Z\u00e4n\u00b7ker", "ist", "der", "Him\u00b7mel", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Und schlug die goldne Pforte zu.", "tokens": ["Und", "schlug", "die", "gold\u00b7ne", "Pfor\u00b7te", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Der Z\u00e4nkerhauf' gab sich zur Ruh,", "tokens": ["Der", "Z\u00e4n\u00b7ker\u00b7hauf'", "gab", "sich", "zur", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.27": {"text": "Setzt sich auf eine Wolkenbank", "tokens": ["Setzt", "sich", "auf", "ei\u00b7ne", "Wol\u00b7ken\u00b7bank"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Und dacht: 's ist wahr, was n\u00fctzt der Zank,", "tokens": ["Und", "dacht", ":", "'s", "ist", "wahr", ",", "was", "n\u00fctzt", "der", "Zank", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VAFIN", "ADJD", "$,", "PWS", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.29": {"text": "Das Ketzermachen und der Spott?", "tokens": ["Das", "Ket\u00b7zer\u00b7ma\u00b7chen", "und", "der", "Spott", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "So sangen sie mit gro\u00dfem Schall.", "tokens": ["So", "san\u00b7gen", "sie", "mit", "gro\u00b7\u00dfem", "Schall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Die Engel h\u00f6rten's allzumal", "tokens": ["Die", "En\u00b7gel", "h\u00f6r\u00b7ten's", "all\u00b7zu\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Und waren alle hoch erfreut,", "tokens": ["Und", "wa\u00b7ren", "al\u00b7le", "hoch", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Ob ihrer Lieb und Einigkeit. \u2013", "tokens": ["Ob", "ih\u00b7rer", "Lieb", "und", "Ei\u00b7nig\u00b7keit", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Sankt Peter kam und machte drauf", "tokens": ["Sankt", "Pe\u00b7ter", "kam", "und", "mach\u00b7te", "drauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "VVFIN", "KON", "VVFIN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Die Himmelspforte wieder auf,", "tokens": ["Die", "Him\u00b7mel\u00b7spfor\u00b7te", "wie\u00b7der", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Und sprach in Freud: Die\u00df h\u00f6r' ich gern;", "tokens": ["Und", "sprach", "in", "Freud", ":", "Die\u00df", "h\u00f6r'", "ich", "gern", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$.", "PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Kommt ihr Gesegneten des Herrn!", "tokens": ["Kommt", "ihr", "Ge\u00b7se\u00b7gne\u00b7ten", "des", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.38": {"text": "Die Dreie eilten Hand in Hand", "tokens": ["Die", "Drei\u00b7e", "eil\u00b7ten", "Hand", "in", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "CARD", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Nun in ihr himmlisch Vaterland.", "tokens": ["Nun", "in", "ihr", "himm\u00b7lisch", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}