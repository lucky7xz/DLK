{"textgrid.poem.24224": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Adoration", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich strecke meine H\u00e4nde aus nach dir,", "tokens": ["Ich", "stre\u00b7cke", "mei\u00b7ne", "H\u00e4n\u00b7de", "aus", "nach", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie Beterh\u00e4nde sich zum Antlitz strecken", "tokens": ["Wie", "Be\u00b7ter\u00b7h\u00e4n\u00b7de", "sich", "zum", "Ant\u00b7litz", "stre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "PRF", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der gnadenreichen Mutter, die im Arm", "tokens": ["Der", "gna\u00b7den\u00b7rei\u00b7chen", "Mut\u00b7ter", ",", "die", "im", "Arm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das liebevolle ernste Kind umf\u00e4ngt.", "tokens": ["Das", "lie\u00b7be\u00b7vol\u00b7le", "erns\u00b7te", "Kind", "um\u00b7f\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So, o Madonna, m\u00f6cht ich, da\u00df mein Herz", "tokens": ["So", ",", "o", "Ma\u00b7don\u00b7na", ",", "m\u00f6cht", "ich", ",", "da\u00df", "mein", "Herz"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "FM", "FM", "$,", "VMFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Umfangen w\u00e4r von deinem Herzen; so,", "tokens": ["Um\u00b7fan\u00b7gen", "w\u00e4r", "von", "dei\u00b7nem", "Her\u00b7zen", ";", "so", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPR", "PPOSAT", "NN", "$.", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So, o Madonna, m\u00f6cht ich, da\u00df dein Blick", "tokens": ["So", ",", "o", "Ma\u00b7don\u00b7na", ",", "m\u00f6cht", "ich", ",", "da\u00df", "dein", "Blick"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "FM", "FM", "$,", "VMFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der heitere mich \u00fcberstrahlte. Sieh,", "tokens": ["Der", "hei\u00b7te\u00b7re", "mich", "\u00fc\u00b7bers\u00b7trahl\u00b7te", ".", "Sieh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "PPER", "VVFIN", "$.", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Madonna, sieh, wie ich voll Glaubens bin,", "tokens": ["Ma\u00b7don\u00b7na", ",", "sieh", ",", "wie", "ich", "voll", "Glau\u00b7bens", "bin", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "PWAV", "PPER", "ADJD", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Versunken ganz in deine G\u00fcte, ganz,", "tokens": ["Ver\u00b7sun\u00b7ken", "ganz", "in", "dei\u00b7ne", "G\u00fc\u00b7te", ",", "ganz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$,", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ganz fromm und selig, wie ein armes Herz,", "tokens": ["Ganz", "fromm", "und", "se\u00b7lig", ",", "wie", "ein", "ar\u00b7mes", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Das schon gestorben war in Graus und Gram", "tokens": ["Das", "schon", "ge\u00b7stor\u00b7ben", "war", "in", "Graus", "und", "Gram"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "VVPP", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und zur Madonna seine Zuflucht nahm", "tokens": ["Und", "zur", "Ma\u00b7don\u00b7na", "sei\u00b7ne", "Zu\u00b7flucht", "nahm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und stark in Liebe ward, neu lebend, frei,", "tokens": ["Und", "stark", "in", "Lie\u00b7be", "ward", ",", "neu", "le\u00b7bend", ",", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "VAFIN", "$,", "ADJD", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Da\u00df es vor Gl\u00fccke zuckt und bebt und bangt", "tokens": ["Da\u00df", "es", "vor", "Gl\u00fc\u00b7cke", "zuckt", "und", "bebt", "und", "bangt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und nichts mehr, nichts von dieser Welt verlangt,", "tokens": ["Und", "nichts", "mehr", ",", "nichts", "von", "die\u00b7ser", "Welt", "ver\u00b7langt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "$,", "PIS", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Als da\u00df es stets im Glanz Mariens sei.", "tokens": ["Als", "da\u00df", "es", "stets", "im", "Glanz", "Ma\u00b7ri\u00b7ens", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "APPRART", "NN", "NE", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}