{"textgrid.poem.25260": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf sandiger Heide am Kiefernforst", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf sandiger Heide am Kiefernforst", "tokens": ["Auf", "san\u00b7di\u00b7ger", "Hei\u00b7de", "am", "Kie\u00b7fern\u00b7forst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Kauert ein H\u00e4uschen gedr\u00fcckt,", "tokens": ["Kau\u00b7ert", "ein", "H\u00e4usc\u00b7hen", "ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "An Fenster, Dach und Lehmgewand", "tokens": ["An", "Fens\u00b7ter", ",", "Dach", "und", "Lehm\u00b7ge\u00b7wand"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verwahrlost und zerst\u00fcckt.", "tokens": ["Ver\u00b7wahr\u00b7lost", "und", "zer\u00b7st\u00fcckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Des bretternen Stalles T\u00fcre klafft;", "tokens": ["Des", "bret\u00b7ter\u00b7nen", "Stal\u00b7les", "T\u00fc\u00b7re", "klafft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Verloren sind Schafe und Ziegen.", "tokens": ["Ver\u00b7lo\u00b7ren", "sind", "Scha\u00b7fe", "und", "Zie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Im D\u00fcnger ein letztes H\u00fchnchen scharrt,", "tokens": ["Im", "D\u00fcn\u00b7ger", "ein", "letz\u00b7tes", "H\u00fchn\u00b7chen", "scharrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "M\u00fcrrisch brummen die Fliegen.", "tokens": ["M\u00fcr\u00b7risch", "brum\u00b7men", "die", "Flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Und in der Stube da quarrt das Kind,", "tokens": ["Und", "in", "der", "Stu\u00b7be", "da", "quarrt", "das", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das Weib, das zornige, schilt,", "tokens": ["Das", "Weib", ",", "das", "zor\u00b7ni\u00b7ge", ",", "schilt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Des H\u00e4uslers Stimme vom Trunke rauh", "tokens": ["Des", "H\u00e4us\u00b7lers", "Stim\u00b7me", "vom", "Trun\u00b7ke", "rauh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "L\u00e4stert dazwischen wild ....", "tokens": ["L\u00e4s\u00b7tert", "da\u00b7zwi\u00b7schen", "wild", "...."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Am Fenster die schlanke Sonnenblume", "tokens": ["Am", "Fens\u00b7ter", "die", "schlan\u00b7ke", "Son\u00b7nen\u00b7blu\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Erbebt in heimlichem Leid.", "tokens": ["Er\u00b7bebt", "in", "heim\u00b7li\u00b7chem", "Leid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Aus Schutt und Unkraut strebt sie scheu", "tokens": ["Aus", "Schutt", "und", "Un\u00b7kraut", "strebt", "sie", "scheu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und starrt in die Ferne weit.", "tokens": ["Und", "starrt", "in", "die", "Fer\u00b7ne", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Dort hinter vergilbtem Kartoffelkraut", "tokens": ["Dort", "hin\u00b7ter", "ver\u00b7gilb\u00b7tem", "Kar\u00b7tof\u00b7fel\u00b7kraut"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und blondem Stoppelhaar", "tokens": ["Und", "blon\u00b7dem", "Stop\u00b7pel\u00b7haar"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ergl\u00e4nzt der Himmel wie mattes Gold,", "tokens": ["Er\u00b7gl\u00e4nzt", "der", "Him\u00b7mel", "wie", "mat\u00b7tes", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie Feiergesang so klar.", "tokens": ["Wie", "Fei\u00b7er\u00b7ge\u00b7sang", "so", "klar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Dort loht aus \u00fcberirdischem Licht", "tokens": ["Dort", "loht", "aus", "\u00fc\u00b7be\u00b7rir\u00b7di\u00b7schem", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Eine andere Blume: die Abendsonne.", "tokens": ["Ei\u00b7ne", "an\u00b7de\u00b7re", "Blu\u00b7me", ":", "die", "A\u00b7bend\u00b7son\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "NN", "$."], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Sie neigt sich zu Grab. Wer die heilige liebt,", "tokens": ["Sie", "neigt", "sich", "zu", "Grab", ".", "Wer", "die", "hei\u00b7li\u00b7ge", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$.", "PWS", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sauge noch einmal einzige Wonne!", "tokens": ["Sau\u00b7ge", "noch", "ein\u00b7mal", "ein\u00b7zi\u00b7ge", "Won\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Und die Sonnenblume, am Glutenball", "tokens": ["Und", "die", "Son\u00b7nen\u00b7blu\u00b7me", ",", "am", "Glu\u00b7ten\u00b7ball"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "APPRART", "NN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "H\u00e4ngt schw\u00e4rmerisch starr ihr Angesicht,", "tokens": ["H\u00e4ngt", "schw\u00e4r\u00b7me\u00b7risch", "starr", "ihr", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ihr gelbumkr\u00e4nztes Tr\u00e4umergesicht,", "tokens": ["Ihr", "gel\u00b7bum\u00b7kr\u00e4nz\u00b7tes", "Tr\u00e4u\u00b7mer\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Selig ertrunken im Purpurlicht.", "tokens": ["Se\u00b7lig", "er\u00b7trun\u00b7ken", "im", "Pur\u00b7pur\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "So steht sie, bei Nesseln an w\u00fcster Mauer,", "tokens": ["So", "steht", "sie", ",", "bei", "Nes\u00b7seln", "an", "w\u00fcs\u00b7ter", "Mau\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wie bebende Arme die Bl\u00e4tter gebreitet ...", "tokens": ["Wie", "be\u00b7ben\u00b7de", "Ar\u00b7me", "die", "Bl\u00e4t\u00b7ter", "ge\u00b7brei\u00b7tet", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "----+--+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Versunken die Sonne ... Hinterdrein gleitet", "tokens": ["Ver\u00b7sun\u00b7ken", "die", "Son\u00b7ne", "...", "Hin\u00b7ter\u00b7drein", "glei\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$(", "NN", "VVFIN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein Schmachten hinunter mit Todesschauer.", "tokens": ["Ein", "Schmach\u00b7ten", "hin\u00b7un\u00b7ter", "mit", "To\u00b7des\u00b7schau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPO", "APPR", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}}}}