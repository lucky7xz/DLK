{"dta.poem.21292": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "XiV.  \n  Burgunder-Wein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1752", "urn": "urn:nbn:de:kobv:b4-200905199117", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Damit ich singen lerne,", "tokens": ["Da\u00b7mit", "ich", "sin\u00b7gen", "ler\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Soll mir der Saft der Reben", "tokens": ["Soll", "mir", "der", "Saft", "der", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jtzt Muth und T\u00f6ne geben", "tokens": ["Jtzt", "Muth", "und", "T\u00f6\u00b7ne", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und neue Kunst verleihn.", "tokens": ["Und", "neu\u00b7e", "Kunst", "ver\u00b7leihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mich reizen deine Sterne,", "tokens": ["Mich", "rei\u00b7zen", "dei\u00b7ne", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Jhr Einflu\u00df wirket Wunder,", "tokens": ["Ihr", "Ein\u00b7flu\u00df", "wir\u00b7ket", "Wun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.7": {"text": "O feuriger Burgunder,", "tokens": ["O", "feu\u00b7ri\u00b7ger", "Bur\u00b7gun\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "O k\u00f6niglicher Wein!", "tokens": ["O", "k\u00f6\u00b7nig\u00b7li\u00b7cher", "Wein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+----+", "measure": "dactylic.init"}}}}}