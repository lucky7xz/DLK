{"dta.poem.8768": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Christnacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Schon les' ich in den Weiten", "tokens": ["Schon", "les'", "ich", "in", "den", "Wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des k\u00fcnft'gen Tages bang,", "tokens": ["Des", "k\u00fcnft'\u00b7gen", "Ta\u00b7ges", "bang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich h\u00f6re V\u00f6lker schreiten,", "tokens": ["Ich", "h\u00f6\u00b7re", "V\u00f6l\u00b7ker", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie athmen Untergang.", "tokens": ["Sie", "ath\u00b7men", "Un\u00b7ter\u00b7gang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es naht der m\u00fcden Erde", "tokens": ["Es", "naht", "der", "m\u00fc\u00b7den", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein frischer Morgen sich,", "tokens": ["Ein", "fri\u00b7scher", "Mor\u00b7gen", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf dieses Kindes \u201eWerde\u201c", "tokens": ["Auf", "die\u00b7ses", "Kin\u00b7des", "\u201e", "Wer\u00b7de", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$(", "VAFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erbl\u00fcht sie jugendlich.", "tokens": ["Er\u00b7bl\u00fcht", "sie", "ju\u00b7gend\u00b7lich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}