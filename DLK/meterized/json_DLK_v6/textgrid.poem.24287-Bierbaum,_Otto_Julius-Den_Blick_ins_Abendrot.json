{"textgrid.poem.24287": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Den Blick ins Abendrot", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun sieh wohl zu, mein Herz! Ein jeder Tag ist nun", "tokens": ["Nun", "sieh", "wohl", "zu", ",", "mein", "Herz", "!", "Ein", "je\u00b7der", "Tag", "ist", "nun"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "ADV", "PTKVZ", "$,", "PPOSAT", "NN", "$.", "ART", "PIAT", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kostbar und ein Geschenk und kann der letzte sein.", "tokens": ["Kost\u00b7bar", "und", "ein", "Ge\u00b7schenk", "und", "kann", "der", "letz\u00b7te", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ART", "NN", "KON", "VMFIN", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auch bist du reicher jetzt, und wenn du dich verlierst", "tokens": ["Auch", "bist", "du", "rei\u00b7cher", "jetzt", ",", "und", "wenn", "du", "dich", "ver\u00b7lierst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADV", "$,", "KON", "KOUS", "PPER", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "(du wei\u00dft, worin du ehemals gern verloren gingst:", "tokens": ["(", "du", "wei\u00dft", ",", "wo\u00b7rin", "du", "e\u00b7he\u00b7mals", "gern", "ver\u00b7lo\u00b7ren", "gingst", ":"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "PWAV", "PPER", "ADV", "ADV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Der dunkle Wald steht heute noch um dich; der See,", "tokens": ["Der", "dunk\u00b7le", "Wald", "steht", "heu\u00b7te", "noch", "um", "dich", ";", "der", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADV", "APPR", "PPER", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der laue, voller Algen, lockt noch heut, \u2013 gib acht!),", "tokens": ["Der", "lau\u00b7e", ",", "vol\u00b7ler", "Al\u00b7gen", ",", "lockt", "noch", "heut", ",", "\u2013", "gib", "acht", "!", ")", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,", "VVFIN", "ADV", "ADV", "$,", "$(", "VVIMP", "CARD", "$.", "$(", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Verlierst du mehr als fr\u00fcher und ersetzst es nie:", "tokens": ["Ver\u00b7lierst", "du", "mehr", "als", "fr\u00fc\u00b7her", "und", "er\u00b7setz\u00b7st", "es", "nie", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "KOKOM", "ADJD", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+---+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Aus dir nicht und auch nicht aus dem, das um dich ist.", "tokens": ["Aus", "dir", "nicht", "und", "auch", "nicht", "aus", "dem", ",", "das", "um", "dich", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKNEG", "KON", "ADV", "PTKNEG", "APPR", "ART", "$,", "PRELS", "APPR", "PPER", "VAFIN", "$."], "meter": "+-+------+-+", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "Sieh wohl zu, Herz! Jedoch sei wie dies Abendrot:", "tokens": ["Sieh", "wohl", "zu", ",", "Herz", "!", "Je\u00b7doch", "sei", "wie", "dies", "A\u00b7ben\u00b7drot", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKVZ", "$,", "NN", "$.", "ADV", "VAFIN", "KOKOM", "PDS", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nicht zaghaft, z\u00f6gernd, weil es bald zu scheiden gilt,", "tokens": ["Nicht", "zag\u00b7haft", ",", "z\u00f6\u00b7gernd", ",", "weil", "es", "bald", "zu", "schei\u00b7den", "gilt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "VVPP", "$,", "KOUS", "PPER", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nein: gl\u00fch gewaltig, glutenliebevoll ins All:", "tokens": ["Nein", ":", "gl\u00fch", "ge\u00b7wal\u00b7tig", ",", "glu\u00b7ten\u00b7lie\u00b7be\u00b7voll", "ins", "All", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADJD", "ADJD", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Flamm auf, flamm hin, verl\u00f6sch purpurn in Sonnenpracht!", "tokens": ["Flamm", "auf", ",", "flamm", "hin", ",", "ver\u00b7l\u00f6sch", "pur\u00b7purn", "in", "Son\u00b7nen\u00b7pracht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "ADJD", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}