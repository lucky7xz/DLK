{"textgrid.poem.34769": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich, beseligt von sch\u00f6nen K\u00fcssen,", "tokens": ["Wenn", "ich", ",", "be\u00b7se\u00b7ligt", "von", "sch\u00f6\u00b7nen", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+----+-+-", "measure": "dactylic.init"}, "line.2": {"text": "In deinen Armen mich wohl befinde,", "tokens": ["In", "dei\u00b7nen", "Ar\u00b7men", "mich", "wohl", "be\u00b7fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dann mu\u00dft du mir nie von Deutschland reden; \u2013", "tokens": ["Dann", "mu\u00dft", "du", "mir", "nie", "von", "Deutschland", "re\u00b7den", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "APPR", "NE", "VVINF", "$.", "$("], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ich kann's nicht vertragen \u2013 es hat seine Gr\u00fcnde.", "tokens": ["Ich", "kann's", "nicht", "ver\u00b7tra\u00b7gen", "\u2013", "es", "hat", "sei\u00b7ne", "Gr\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVFIN", "$(", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Ich bitte dich, la\u00df mich mit Deutschland in Frieden!", "tokens": ["Ich", "bit\u00b7te", "dich", ",", "la\u00df", "mich", "mit", "Deutschland", "in", "Frie\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "VVIMP", "PPER", "APPR", "NE", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du mu\u00dft mich nicht plagen mit ewigen Fragen", "tokens": ["Du", "mu\u00dft", "mich", "nicht", "pla\u00b7gen", "mit", "e\u00b7wi\u00b7gen", "Fra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Nach Heimat, Sippschaft und Lebensverh\u00e4ltnis; \u2013", "tokens": ["Nach", "Hei\u00b7mat", ",", "Sipp\u00b7schaft", "und", "Le\u00b7bens\u00b7ver\u00b7h\u00e4lt\u00b7nis", ";", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Es hat seine Gr\u00fcnde \u2013 ich kann's nicht vertragen.", "tokens": ["Es", "hat", "sei\u00b7ne", "Gr\u00fcn\u00b7de", "\u2013", "ich", "kann's", "nicht", "ver\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$(", "PPER", "VMFIN", "PTKNEG", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Die Eichen sind gr\u00fcn, und blau sind die Augen", "tokens": ["Die", "Ei\u00b7chen", "sind", "gr\u00fcn", ",", "und", "blau", "sind", "die", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KON", "ADJD", "VAFIN", "ART", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der deutschen Frauen, sie schmachten gelinde", "tokens": ["Der", "deut\u00b7schen", "Frau\u00b7en", ",", "sie", "schmach\u00b7ten", "ge\u00b7lin\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "ADJA"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und seufzen von Liebe, Hoffnung und Glauben; \u2013", "tokens": ["Und", "seuf\u00b7zen", "von", "Lie\u00b7be", ",", "Hoff\u00b7nung", "und", "Glau\u00b7ben", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,", "NN", "KON", "NN", "$.", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich kann's nicht vertragen \u2013 es hat seine Gr\u00fcnde.", "tokens": ["Ich", "kann's", "nicht", "ver\u00b7tra\u00b7gen", "\u2013", "es", "hat", "sei\u00b7ne", "Gr\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVFIN", "$(", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}