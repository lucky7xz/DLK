{"textgrid.poem.64109": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dort auf Tiburs steilen Felsen, wo der Anio wirbelnd rinnt,", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort auf Tiburs steilen Felsen, wo der Anio wirbelnd rinnt,", "tokens": ["Dort", "auf", "Ti\u00b7burs", "stei\u00b7len", "Fel\u00b7sen", ",", "wo", "der", "A\u00b7nio", "wir\u00b7belnd", "rinnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "ADJA", "NN", "$,", "PWAV", "ART", "NE", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Stumm, mit schmerzgebleichten Wangen, steht Germaniens stolzes Kind;", "tokens": ["Stumm", ",", "mit", "schmerz\u00b7ge\u00b7bleich\u00b7ten", "Wan\u00b7gen", ",", "steht", "Ger\u00b7ma\u00b7ni\u00b7ens", "stol\u00b7zes", "Kind", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ADJA", "NN", "$,", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "Um die hohe Stirne windet sich der Lindenbl\u00fctenkranz,", "tokens": ["Um", "die", "ho\u00b7he", "Stir\u00b7ne", "win\u00b7det", "sich", "der", "Lin\u00b7den\u00b7bl\u00fc\u00b7ten\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Von den Schl\u00e4fen zu den Knieen flie\u00dft des roten Haares Glanz,", "tokens": ["Von", "den", "Schl\u00e4\u00b7fen", "zu", "den", "Kni\u00b7e\u00b7en", "flie\u00dft", "des", "ro\u00b7ten", "Haa\u00b7res", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "--+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Und den weiten Opfermantel tr\u00e4gt sie wie im Heimatland,", "tokens": ["Und", "den", "wei\u00b7ten", "Op\u00b7fer\u00b7man\u00b7tel", "tr\u00e4gt", "sie", "wie", "im", "Hei\u00b7mat\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "KOKOM", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Aber ach, die goldne Fessel schlingt sich um die wei\u00dfe Hand. \u2013", "tokens": ["A\u00b7ber", "ach", ",", "die", "gold\u00b7ne", "Fes\u00b7sel", "schlingt", "sich", "um", "die", "wei\u00b7\u00dfe", "Hand", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "XY", "$,", "ART", "ADJA", "NN", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "\u00bbbin ich Veleda? Ach, bin ich's?\u00ab \u2013 seufzt der sch\u00f6ne, bleiche Mund \u2013", "tokens": ["\u00bb", "bin", "ich", "Ve\u00b7le\u00b7da", "?", "Ach", ",", "bin", "ich's", "?", "\u00ab", "\u2013", "seufzt", "der", "sch\u00f6\u00b7ne", ",", "blei\u00b7che", "Mund", "\u2013"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "NE", "$.", "ITJ", "$,", "VAFIN", "PIS", "$.", "$(", "$(", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Die mit G\u00f6ttern Zwiesprach tauschte auf des heil'gen Berges Rund,", "tokens": ["Die", "mit", "G\u00f6t\u00b7tern", "Zwie\u00b7sprach", "tauschte", "auf", "des", "heil'\u00b7gen", "Ber\u00b7ges", "Rund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Die in hoher Eichen Wipfel hohe Weissagung belauscht,", "tokens": ["Die", "in", "ho\u00b7her", "Ei\u00b7chen", "Wip\u00b7fel", "ho\u00b7he", "Weis\u00b7sa\u00b7gung", "be\u00b7lauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Welcher laut des Rheines Wirbel Siegverhei\u00dfung zugerauscht? \u2013", "tokens": ["Wel\u00b7cher", "laut", "des", "Rhei\u00b7nes", "Wir\u00b7bel", "Sieg\u00b7ver\u00b7hei\u00b7\u00dfung", "zu\u00b7ge\u00b7rauscht", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAT", "APPR", "ART", "ADJA", "NN", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Bin ich's, der mein Volk mit Jauchzen deinen Feldherrn, stolzes Rom,", "tokens": ["Bin", "ich's", ",", "der", "mein", "Volk", "mit", "Jauch\u00b7zen", "dei\u00b7nen", "Feld\u00b7herrn", ",", "stol\u00b7zes", "Rom", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "PRELS", "PPOSAT", "NN", "APPR", "NN", "PPOSAT", "NN", "$,", "ADJA", "NE", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Zugef\u00fchrt als Ehrenbeute auf befreitem Lippestrom?", "tokens": ["Zu\u00b7ge\u00b7f\u00fchrt", "als", "Eh\u00b7ren\u00b7beu\u00b7te", "auf", "be\u00b7frei\u00b7tem", "Lip\u00b7pes\u00b7trom", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Denn ich hatte Sieg verhei\u00dfen, Sieg in Land und Wasserschlacht, \u2013", "tokens": ["Denn", "ich", "hat\u00b7te", "Sieg", "ver\u00b7hei\u00b7\u00dfen", ",", "Sieg", "in", "Land", "und", "Was\u00b7ser\u00b7schlacht", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "VVINF", "$,", "NN", "APPR", "NN", "KON", "NN", "$,", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Und auf seiner Prunktriere ward der Pr\u00e4tor mir gebracht.", "tokens": ["Und", "auf", "sei\u00b7ner", "Prunk\u00b7trie\u00b7re", "ward", "der", "Pr\u00e4\u00b7tor", "mir", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Doch ein Tag kam \u2013 seine Schrecken k\u00fcndete kein G\u00f6tterwort \u2013", "tokens": ["Doch", "ein", "Tag", "kam", "\u2013", "sei\u00b7ne", "Schre\u00b7cken", "k\u00fcn\u00b7de\u00b7te", "kein", "G\u00f6t\u00b7ter\u00b7wort", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$(", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Weh! da scholl im heil'gen Haine Waffenl\u00e4rm und wilder Mord,", "tokens": ["Weh", "!", "da", "scholl", "im", "heil'\u00b7gen", "Hai\u00b7ne", "Waf\u00b7fen\u00b7l\u00e4rm", "und", "wil\u00b7der", "Mord", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "ADJD", "APPRART", "ADJA", "NN", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.17": {"text": "R\u00f6merhelme \u2013 rote Fackeln \u2013 Priesterblut und Waldesbrand,", "tokens": ["R\u00f6\u00b7mer\u00b7hel\u00b7me", "\u2013", "ro\u00b7te", "Fa\u00b7ckeln", "\u2013", "Pries\u00b7ter\u00b7blut", "und", "Wal\u00b7des\u00b7brand", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADJA", "NN", "$(", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.18": {"text": "Und sie schleppten mich gefangen aus dem gr\u00fcnen Bruktrerland. \u2013", "tokens": ["Und", "sie", "schlepp\u00b7ten", "mich", "ge\u00b7fan\u00b7gen", "aus", "dem", "gr\u00fc\u00b7nen", "Bruk\u00b7trer\u00b7land", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.19": {"text": "Wer vom Vaterland genommen, dem ist Licht und Luft geraubt;", "tokens": ["Wer", "vom", "Va\u00b7ter\u00b7land", "ge\u00b7nom\u00b7men", ",", "dem", "ist", "Licht", "und", "Luft", "ge\u00b7raubt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "VVPP", "$,", "PDS", "VAFIN", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.20": {"text": "Wie die ausgeriss'ne Blume neig' ich hoffnungslos das Haupt;", "tokens": ["Wie", "die", "aus\u00b7ge\u00b7riss'\u00b7ne", "Blu\u00b7me", "neig'", "ich", "hoff\u00b7nungs\u00b7los", "das", "Haupt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.21": {"text": "Ach, an dieser hei\u00dfen Sonne welkt verdorrt mein Leben bald: \u2013", "tokens": ["Ach", ",", "an", "die\u00b7ser", "hei\u00b7\u00dfen", "Son\u00b7ne", "welkt", "ver\u00b7dorrt", "mein", "Le\u00b7ben", "bald", ":", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "APPR", "PDAT", "ADJA", "NN", "VVFIN", "VVFIN", "PPOSAT", "NN", "ADV", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.22": {"text": "Wo bist du, mein dunkelk\u00fchler, ferner, sch\u00f6ner Buchenwald?\u00ab", "tokens": ["Wo", "bist", "du", ",", "mein", "dun\u00b7kel\u00b7k\u00fch\u00b7ler", ",", "fer\u00b7ner", ",", "sch\u00f6\u00b7ner", "Bu\u00b7chen\u00b7wald", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "PPOSAT", "ADJA", "$,", "ADV", "$,", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.23": {"text": "Sprach's und sah vom hohen Felsen sehnend in das Land hinaus:", "tokens": ["Sprach's", "und", "sah", "vom", "ho\u00b7hen", "Fel\u00b7sen", "seh\u00b7nend", "in", "das", "Land", "hin\u00b7aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "APPRART", "ADJA", "NN", "VVPP", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.24": {"text": "Sieh, da schritten zwei Liktoren auf sie her vom Marmorhaus,", "tokens": ["Sieh", ",", "da", "schrit\u00b7ten", "zwei", "Lik\u00b7to\u00b7ren", "auf", "sie", "her", "vom", "Mar\u00b7mor\u00b7haus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "CARD", "NN", "APPR", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.25": {"text": "Purpur brachten sie und Goldstab, und es folgt ein Kriegerschwarm,", "tokens": ["Pur\u00b7pur", "brach\u00b7ten", "sie", "und", "Gold\u00b7stab", ",", "und", "es", "folgt", "ein", "Krie\u00b7ger\u00b7schwarm", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KON", "NN", "$,", "KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.26": {"text": "Laut ihr winkend: doch die Jungfrau hebet streng den wei\u00dfen Arm.", "tokens": ["Laut", "ihr", "win\u00b7kend", ":", "doch", "die", "Jung\u00b7frau", "he\u00b7bet", "streng", "den", "wei\u00b7\u00dfen", "Arm", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$.", "ADV", "ART", "NN", "VVFIN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+---+-+-+", "measure": "unknown.measure.septa"}, "line.27": {"text": "\u00bbkomm, Veleda, steige nieder\u00ab, \u2013 ruft ihr der Centurio \u2013", "tokens": ["\u00bb", "komm", ",", "Ve\u00b7le\u00b7da", ",", "stei\u00b7ge", "nie\u00b7der", "\u00ab", ",", "\u2013", "ruft", "ihr", "der", "Cen\u00b7tu\u00b7rio", "\u2013"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "NE", "$,", "VVFIN", "PTKVZ", "$(", "$,", "$(", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+--+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "\u00bbheut erf\u00fcllt sich deine Weisheit, du Prophetin siegesfroh!", "tokens": ["\u00bb", "heut", "er\u00b7f\u00fcllt", "sich", "dei\u00b7ne", "Weis\u00b7heit", ",", "du", "Pro\u00b7phe\u00b7tin", "sie\u00b7ges\u00b7froh", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$,", "PPER", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.29": {"text": "Z\u00f6gre nicht: \u2013 der Imperator harrt: \u2013 es murrt die Menge schon: \u2013", "tokens": ["Z\u00f6g\u00b7re", "nicht", ":", "\u2013", "der", "Im\u00b7pe\u00b7ra\u00b7tor", "harrt", ":", "\u2013", "es", "murrt", "die", "Men\u00b7ge", "schon", ":", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PTKNEG", "$.", "$(", "ART", "NN", "VVFIN", "$.", "$(", "PPER", "VVFIN", "ART", "NN", "ADV", "$.", "$("], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.30": {"text": "Schon vom Palatinus nieder steigt Legion auf Legion;", "tokens": ["Schon", "vom", "Pa\u00b7la\u00b7ti\u00b7nus", "nie\u00b7der", "steigt", "Le\u00b7gi\u00b7on", "auf", "Le\u00b7gi\u00b7on", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PTKVZ", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+--+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.31": {"text": "Tuben schmettern, Opfer rauchen \u2013 nur Veleda fehlet noch.\u00ab \u2013", "tokens": ["Tu\u00b7ben", "schmet\u00b7tern", ",", "Op\u00b7fer", "rau\u00b7chen", "\u2013", "nur", "Ve\u00b7le\u00b7da", "feh\u00b7let", "noch", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVFIN", "$(", "ADV", "NE", "VVFIN", "ADV", "$.", "$(", "$("], "meter": "+-+-+-+--+--+-+", "measure": "trochaic.septa.relaxed"}, "line.32": {"text": "\u00bbsprecht, was wollt ihr?\u00ab rief's und ahnend trat sie an das Felsenjoch.", "tokens": ["\u00bb", "sprecht", ",", "was", "wollt", "ihr", "?", "\u00ab", "rie\u00b7f's", "und", "ah\u00b7nend", "trat", "sie", "an", "das", "Fel\u00b7sen\u00b7joch", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "PWS", "VMFIN", "PPER", "$.", "$(", "NE", "KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+----+-+-+-+-+-+", "measure": "dactylic.init"}, "line.33": {"text": "\u00bbwie, du fr\u00e4gst noch? Im Triumphe ziehet heut der Feldherr ein,", "tokens": ["\u00bb", "wie", ",", "du", "fr\u00e4gst", "noch", "?", "Im", "Tri\u00b7um\u00b7phe", "zie\u00b7het", "heut", "der", "Feld\u00b7herr", "ein", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "$,", "PPER", "VVFIN", "ADV", "$.", "APPRART", "NN", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.34": {"text": "Du in seiner Siegeskrone bist der sch\u00f6nste Edelstein:", "tokens": ["Du", "in", "sei\u00b7ner", "Sie\u00b7ges\u00b7kro\u00b7ne", "bist", "der", "sch\u00f6ns\u00b7te", "E\u00b7del\u00b7stein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.35": {"text": "Du, vor Cerialis Wagen, bist Germaniens Symbol.\u00ab", "tokens": ["Du", ",", "vor", "Ce\u00b7ri\u00b7a\u00b7lis", "Wa\u00b7gen", ",", "bist", "Ger\u00b7ma\u00b7ni\u00b7ens", "Sym\u00b7bol", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "$,", "APPR", "NE", "NN", "$,", "VAFIN", "NN", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.36": {"text": "\u00bbauf, Veleda,\u00ab rufen alle, \u00bbfort, hinauf zum Kapitol!\u00ab", "tokens": ["\u00bb", "auf", ",", "Ve\u00b7le\u00b7da", ",", "\u00ab", "ru\u00b7fen", "al\u00b7le", ",", "\u00bb", "fort", ",", "hin\u00b7auf", "zum", "Ka\u00b7pi\u00b7tol", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$,", "NE", "$,", "$(", "VVFIN", "PIS", "$,", "$(", "PTKVZ", "$,", "ADV", "APPRART", "NN", "$.", "$("], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.37": {"text": "Und zum Felsen, sie zu greifen, schreitet schon der R\u00f6mer vor: \u2013", "tokens": ["Und", "zum", "Fel\u00b7sen", ",", "sie", "zu", "grei\u00b7fen", ",", "schrei\u00b7tet", "schon", "der", "R\u00f6\u00b7mer", "vor", ":", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPRART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.38": {"text": "Sieh, da richtet die Prophetin majest\u00e4tisch sich empor;", "tokens": ["Sieh", ",", "da", "rich\u00b7tet", "die", "Pro\u00b7phe\u00b7tin", "ma\u00b7jes\u00b7t\u00e4\u00b7tisch", "sich", "em\u00b7por", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "NN", "ADJD", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.39": {"text": "Blaue Blitze spr\u00fcht ihr Auge und im Sturm ihr Busen wallt", "tokens": ["Blau\u00b7e", "Blit\u00b7ze", "spr\u00fcht", "ihr", "Au\u00b7ge", "und", "im", "Sturm", "ihr", "Bu\u00b7sen", "wallt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPOSAT", "NN", "KON", "APPRART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.40": {"text": "Und die Feuerlocken fliegen um die dr\u00e4uende Gestalt;", "tokens": ["Und", "die", "Feu\u00b7er\u00b7lo\u00b7cken", "flie\u00b7gen", "um", "die", "dr\u00e4u\u00b7en\u00b7de", "Ge\u00b7stalt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+---+", "measure": "unknown.measure.septa"}, "line.41": {"text": "Und zum Himmel mit der Fessel hebt sie hoch die zorn'ge Hand,", "tokens": ["Und", "zum", "Him\u00b7mel", "mit", "der", "Fes\u00b7sel", "hebt", "sie", "hoch", "die", "zorn'\u00b7ge", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "APPR", "ART", "NN", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.42": {"text": "Und zertr\u00fcmmert an den Felsen schleudert sie den goldnen Tand.", "tokens": ["Und", "zer\u00b7tr\u00fcm\u00b7mert", "an", "den", "Fel\u00b7sen", "schleu\u00b7dert", "sie", "den", "gold\u00b7nen", "Tand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.43": {"text": "Und die R\u00f6mer sehn's mit Grauen, und sie ruft hinab ins Tal:", "tokens": ["Und", "die", "R\u00f6\u00b7mer", "sehn's", "mit", "Grau\u00b7en", ",", "und", "sie", "ruft", "hin\u00b7ab", "ins", "Tal", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "APPR", "NN", "$,", "KON", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.44": {"text": "\u00bbha! ich f\u00fchl's, die G\u00f6tter steigen zu mir nieder noch einmal!", "tokens": ["\u00bb", "ha", "!", "ich", "f\u00fchl's", ",", "die", "G\u00f6t\u00b7ter", "stei\u00b7gen", "zu", "mir", "nie\u00b7der", "noch", "ein\u00b7mal", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$.", "PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "ADV", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.45": {"text": "Ja, sie nah'n in diesem Schauer, der mich zorneskalt durchrinnt,", "tokens": ["Ja", ",", "sie", "nah'n", "in", "die\u00b7sem", "Schau\u00b7er", ",", "der", "mich", "zor\u00b7nes\u00b7kalt", "durch\u00b7rinnt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.46": {"text": "Wie daheim durch Eichenwipfel weht mit Weissagung der Wind.", "tokens": ["Wie", "da\u00b7heim", "durch", "Ei\u00b7chen\u00b7wip\u00b7fel", "weht", "mit", "Weis\u00b7sa\u00b7gung", "der", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "NN", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.47": {"text": "Nicht in meinen Ketten kehrten hohe G\u00f6tter bei mir ein,", "tokens": ["Nicht", "in", "mei\u00b7nen", "Ket\u00b7ten", "kehr\u00b7ten", "ho\u00b7he", "G\u00f6t\u00b7ter", "bei", "mir", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.48": {"text": "Aber jetzt, aus freier Seele, darf ich nochmals prophezei'n;", "tokens": ["A\u00b7ber", "jetzt", ",", "aus", "frei\u00b7er", "See\u00b7le", ",", "darf", "ich", "noch\u00b7mals", "pro\u00b7phe\u00b7zei'n", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "ADJA", "NN", "$,", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.49": {"text": "Wahrheit schau' ich, Wahrheit k\u00fcnd' ich; vor mir tagt's wie Sonnenschein:", "tokens": ["Wahr\u00b7heit", "schau'", "ich", ",", "Wahr\u00b7heit", "k\u00fcnd'", "ich", ";", "vor", "mir", "tagt's", "wie", "Son\u00b7nen\u00b7schein", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "NN", "VVFIN", "PPER", "$.", "APPR", "PPER", "VVFIN", "KOKOM", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.50": {"text": "Veleda nie, nie Germania f\u00fchrt ihr im Triumphzug ein!", "tokens": ["Ve\u00b7le\u00b7da", "nie", ",", "nie", "Ger\u00b7ma\u00b7nia", "f\u00fchrt", "ihr", "im", "Tri\u00b7umph\u00b7zug", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "ADV", "NE", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.51": {"text": "Seht ihr's, R\u00f6mer? Von den Bergen dort herab ins S\u00fcdenfeld \u2013", "tokens": ["Seht", "ih\u00b7r's", ",", "R\u00f6\u00b7mer", "?", "Von", "den", "Ber\u00b7gen", "dort", "her\u00b7ab", "ins", "S\u00fc\u00b7den\u00b7feld", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$.", "APPR", "ART", "NN", "ADV", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.52": {"text": "Geht ihr's nicht? \u2013 steigt hell in Waffen eine ganze Heldenwelt!", "tokens": ["Geht", "ih\u00b7r's", "nicht", "?", "\u2013", "steigt", "hell", "in", "Waf\u00b7fen", "ei\u00b7ne", "gan\u00b7ze", "Hel\u00b7den\u00b7welt", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$.", "$(", "VVFIN", "ADJD", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.53": {"text": "Immer neue, neue Scharen! \u2013 Namen voller Siegesklang!", "tokens": ["Im\u00b7mer", "neu\u00b7e", ",", "neu\u00b7e", "Scha\u00b7ren", "!", "\u2013", "Na\u00b7men", "vol\u00b7ler", "Sie\u00b7ges\u00b7klang", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "$,", "ADJA", "NN", "$.", "$(", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.54": {"text": "Adlerhelme, blanke Schilde, H\u00f6rnerjauchzen, Schlachtgesang!", "tokens": ["Ad\u00b7ler\u00b7hel\u00b7me", ",", "blan\u00b7ke", "Schil\u00b7de", ",", "H\u00f6r\u00b7ner\u00b7jauch\u00b7zen", ",", "Schlacht\u00b7ge\u00b7sang", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.55": {"text": "Heil, du blonder Siegesk\u00f6nig! Schwing' die Streitaxt, schwing' sie wohl!", "tokens": ["Heil", ",", "du", "blon\u00b7der", "Sie\u00b7ges\u00b7k\u00f6\u00b7nig", "!", "Schwing'", "die", "Strei\u00b7taxt", ",", "schwing'", "sie", "wohl", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "NN", "$.", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.56": {"text": "Sieh, sie trifft: es f\u00e4llt in Tr\u00fcmmer Tor und Turm am Kapitol.", "tokens": ["Sieh", ",", "sie", "trifft", ":", "es", "f\u00e4llt", "in", "Tr\u00fcm\u00b7mer", "Tor", "und", "Turm", "am", "Ka\u00b7pi\u00b7tol", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "NN", "NE", "KON", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.57": {"text": "Dann zerspringt die V\u00f6lkerfessel, wie jetzt meine Fessel sprang,", "tokens": ["Dann", "zer\u00b7springt", "die", "V\u00f6l\u00b7ker\u00b7fes\u00b7sel", ",", "wie", "jetzt", "mei\u00b7ne", "Fes\u00b7sel", "sprang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PWAV", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.58": {"text": "Und es wird die Freiheit tagen, die Veleda sterbend sang!\u00ab", "tokens": ["Und", "es", "wird", "die", "Frei\u00b7heit", "ta\u00b7gen", ",", "die", "Ve\u00b7le\u00b7da", "ster\u00b7bend", "sang", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "NN", "$,", "PRELS", "NE", "ADJD", "VVFIN", "$.", "$("], "meter": "--+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.59": {"text": "Sprach's, die R\u00f6mer h\u00f6rten's schauernd \u2013 und noch eh' das Wort verhallt,", "tokens": ["Sprach's", ",", "die", "R\u00f6\u00b7mer", "h\u00f6r\u00b7ten's", "schau\u00b7ernd", "\u2013", "und", "noch", "eh'", "das", "Wort", "ver\u00b7hallt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "ADJD", "$(", "KON", "ADV", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.60": {"text": "Schwang sich nieder von dem Felsen eine leuchtende Gestalt,", "tokens": ["Schwang", "sich", "nie\u00b7der", "von", "dem", "Fel\u00b7sen", "ei\u00b7ne", "leuch\u00b7ten\u00b7de", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKVZ", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.61": {"text": "Rasch und hell, wie wenn vom Himmel hoch ein Stern gefallen w\u00e4r':", "tokens": ["Rasch", "und", "hell", ",", "wie", "wenn", "vom", "Him\u00b7mel", "hoch", "ein", "Stern", "ge\u00b7fal\u00b7len", "w\u00e4r'", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "KOKOM", "KOUS", "APPRART", "NN", "ADJD", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.62": {"text": "Und der Flu\u00dfgott trug die sch\u00f6ne