{"textgrid.poem.25305": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun schreiten wir in Abends leisem Leuchten", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun schreiten wir in Abends leisem Leuchten", "tokens": ["Nun", "schrei\u00b7ten", "wir", "in", "A\u00b7bends", "lei\u00b7sem", "Leuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "den Wiesenhang von Blumengold umsch\u00fcttet", "tokens": ["den", "Wie\u00b7sen\u00b7hang", "von", "Blu\u00b7men\u00b7gold", "um\u00b7sch\u00fct\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "den Schatten zu die von erloschnen H\u00fcgeln", "tokens": ["den", "Schat\u00b7ten", "zu", "die", "von", "er\u00b7lo\u00b7schnen", "H\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "hinsinken \u00fcber das entflammte Tal.", "tokens": ["hin\u00b7sin\u00b7ken", "\u00fc\u00b7ber", "das", "ent\u00b7flamm\u00b7te", "Tal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Uns ward die M\u00e4r von fernen Tempels Zinnen:", "tokens": ["Uns", "ward", "die", "M\u00e4r", "von", "fer\u00b7nen", "Tem\u00b7pels", "Zin\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gold sind die T\u00fcrme silbern strahlt das Tor", "tokens": ["Gold", "sind", "die", "T\u00fcr\u00b7me", "sil\u00b7bern", "strahlt", "das", "Tor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "VVINF", "VVFIN", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "wei\u00df schimmern seine Alabasters\u00e4ulen", "tokens": ["wei\u00df", "schim\u00b7mern", "sei\u00b7ne", "A\u00b7la\u00b7bas\u00b7ter\u00b7s\u00e4u\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "aus schwarzem Lorbeer vor und Rosenb\u00fcschen.", "tokens": ["aus", "schwar\u00b7zem", "Lor\u00b7beer", "vor", "und", "Ro\u00b7sen\u00b7b\u00fc\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Im Gl\u00fchen und Verrieseln dunkler Dolden", "tokens": ["Im", "Gl\u00fc\u00b7hen", "und", "Ver\u00b7rie\u00b7seln", "dunk\u00b7ler", "Dol\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "bebt zag der Schritt durch die verwunschnen Beete", "tokens": ["bebt", "zag", "der", "Schritt", "durch", "die", "ver\u00b7wun\u00b7schnen", "Bee\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "der Stufen Glanz von rotem Licht umflattert", "tokens": ["der", "Stu\u00b7fen", "Glanz", "von", "ro\u00b7tem", "Licht", "um\u00b7flat\u00b7tert"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "wo tief in klingender Gew\u00f6lbe Schauern", "tokens": ["wo", "tief", "in", "klin\u00b7gen\u00b7der", "Ge\u00b7w\u00f6l\u00b7be", "Schau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "von Purpurnacht der Decken \u00fcberstr\u00f6mt", "tokens": ["von", "Pur\u00b7pur\u00b7nacht", "der", "De\u00b7cken", "\u00fc\u00b7ber\u00b7str\u00f6mt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "auf runder Schale schl\u00e4ft der heilige Kelch.", "tokens": ["auf", "run\u00b7der", "Scha\u00b7le", "schl\u00e4ft", "der", "hei\u00b7li\u00b7ge", "Kelch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Schon tropft das Dunkel \u00fcber uns wie Tau.", "tokens": ["Schon", "tropft", "das", "Dun\u00b7kel", "\u00fc\u00b7ber", "uns", "wie", "Tau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wann rinnt es golden durch umflorte Wipfel?", "tokens": ["Wann", "rinnt", "es", "gol\u00b7den", "durch", "um\u00b7flor\u00b7te", "Wip\u00b7fel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wann lockt durch schw\u00fcle Stille s\u00fc\u00dfer Ton?", "tokens": ["Wann", "lockt", "durch", "schw\u00fc\u00b7le", "Stil\u00b7le", "s\u00fc\u00b7\u00dfer", "Ton", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}