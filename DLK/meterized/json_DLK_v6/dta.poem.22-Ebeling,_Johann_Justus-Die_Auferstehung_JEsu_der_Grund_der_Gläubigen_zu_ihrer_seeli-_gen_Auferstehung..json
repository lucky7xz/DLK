{"dta.poem.22": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n  Auferstehung JEsu  \n der  \n  Grund der Gl\u00e4ubigen zu ihrer seeli-  \n gen Auferstehung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Heiland lebt! er steht, er sieget!", "tokens": ["Der", "Hei\u00b7land", "lebt", "!", "er", "steht", ",", "er", "sie\u00b7get", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Grabes Dekkel springt entzwei,", "tokens": ["Des", "Gra\u00b7bes", "Dek\u00b7kel", "springt", "ent\u00b7zwei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum Zeugnis da\u00df die H\u00f6ll bekrieget,", "tokens": ["Zum", "Zeug\u00b7nis", "da\u00df", "die", "H\u00f6ll", "be\u00b7krie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Teuffel \u00fcberwunden sey.", "tokens": ["Der", "Teuf\u00b7fel", "\u00fc\u00b7berw\u00b7un\u00b7den", "sey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Todes Bande sind zersprungen,", "tokens": ["Des", "To\u00b7des", "Ban\u00b7de", "sind", "zer\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Feind womit der Held gerungen,", "tokens": ["Der", "Feind", "wo\u00b7mit", "der", "Held", "ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Liegt nun in blasser Ohnmacht da:", "tokens": ["Liegt", "nun", "in", "blas\u00b7ser", "Ohn\u00b7macht", "da", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das H\u00f6llen Heer fleugt aus dem Streite,", "tokens": ["Das", "H\u00f6l\u00b7len", "Heer", "fleugt", "aus", "dem", "Strei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "L\u00e4st fahren die erhaschte Beute,", "tokens": ["L\u00e4st", "fah\u00b7ren", "die", "er\u00b7haschte", "Beu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Des Glaubens Jubel-Lied heist nun Victoria.", "tokens": ["Des", "Glau\u00b7bens", "Ju\u00b7bel\u00b7Lied", "heist", "nun", "Vic\u00b7to\u00b7ria", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "NE", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}