{"textgrid.poem.53062": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was lachst, du P\u00f6fel, der Gem\u00fcter", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was lachst, du P\u00f6fel, der Gem\u00fcter", "tokens": ["Was", "lachst", ",", "du", "P\u00f6\u00b7fel", ",", "der", "Ge\u00b7m\u00fc\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "PPER", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die nur nach Kunst vnd Wei\u00dfheit stehn,", "tokens": ["Die", "nur", "nach", "Kunst", "vnd", "Wei\u00df\u00b7heit", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd wollen grosser Sch\u00e4tz' vnd G\u00fcter", "tokens": ["Vnd", "wol\u00b7len", "gros\u00b7ser", "Sch\u00e4tz'", "vnd", "G\u00fc\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dar\u00fcber gerne m\u00fcssig gehn?", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "ger\u00b7ne", "m\u00fcs\u00b7sig", "gehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Warumb wird das von dir verh\u00f6net,", "tokens": ["Wa\u00b7rumb", "wird", "das", "von", "dir", "ver\u00b7h\u00f6\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit welchem vns die Wei\u00dfheit kr\u00f6net?", "tokens": ["Mit", "wel\u00b7chem", "vns", "die", "Wei\u00df\u00b7heit", "kr\u00f6\u00b7net", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kan Midas auch ein Vrtheil fellen", "tokens": ["Kan", "Mi\u00b7das", "auch", "ein", "Vrtheil", "fel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "NE", "ADV", "ART", "NN", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "In dem, was seinem tummen Sinn", "tokens": ["In", "dem", ",", "was", "sei\u00b7nem", "tum\u00b7men", "Sinn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "$,", "PRELS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So hoch ist, al\u00df ein Stern der Hellen?", "tokens": ["So", "hoch", "ist", ",", "al\u00df", "ein", "Stern", "der", "Hel\u00b7len", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "KOUS", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O fleuch, vnd komm ja da nicht hin,", "tokens": ["O", "fleuch", ",", "vnd", "komm", "ja", "da", "nicht", "hin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "KON", "VVFIN", "ADV", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo man dein mit Apelles lachet,", "tokens": ["Wo", "man", "dein", "mit", "A\u00b7pel\u00b7les", "la\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPOSAT", "APPR", "NE", "VVFIN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Vnd die so kl\u00fcgeln schamrot machet.", "tokens": ["Vnd", "die", "so", "kl\u00fc\u00b7geln", "scham\u00b7rot", "ma\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "VVFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du prangest nur mit Goldes Tonnen", "tokens": ["Du", "pran\u00b7gest", "nur", "mit", "Gol\u00b7des", "Ton\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd grossen G\u00fctern vmb den Port,", "tokens": ["Vnd", "gros\u00b7sen", "G\u00fc\u00b7tern", "vmb", "den", "Port", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was so gar schleunig wirt gewonnen,", "tokens": ["Was", "so", "gar", "schleu\u00b7nig", "wirt", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das mu\u00df auch schleunig wieder fort,", "tokens": ["Das", "mu\u00df", "auch", "schleu\u00b7nig", "wie\u00b7der", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ADJD", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du siehst ja selbst Raub, Brand vnd kriegen", "tokens": ["Du", "siehst", "ja", "selbst", "Raub", ",", "Brand", "vnd", "krie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "NN", "$,", "NN", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vmb grosse Sch\u00e4tz' vnd G\u00fctter fliegen.", "tokens": ["Vmb", "gros\u00b7se", "Sch\u00e4tz'", "vnd", "G\u00fct\u00b7ter", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie fleucht die Zeit mit grimm' vnd toben,", "tokens": ["Wie", "fleucht", "die", "Zeit", "mit", "grimm'", "vnd", "to\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie giebt dir was sie nachmals nimpt,", "tokens": ["Sie", "giebt", "dir", "was", "sie", "nach\u00b7mals", "nimpt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jetzt hat das leichte Gl\u00fcck dir oben,", "tokens": ["Jetzt", "hat", "das", "leich\u00b7te", "Gl\u00fcck", "dir", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dann vnten gar zu sein bestimpt;", "tokens": ["Dann", "vn\u00b7ten", "gar", "zu", "sein", "be\u00b7stimpt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PTKZU", "VAINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So leichte kan kein Spiel sich enden", "tokens": ["So", "leich\u00b7te", "kan", "kein", "Spiel", "sich", "en\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "VMFIN", "PIAT", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Al\u00df wol dein Gl\u00fcck sich m\u00f6chte wenden.", "tokens": ["Al\u00df", "wol", "dein", "Gl\u00fcck", "sich", "m\u00f6ch\u00b7te", "wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "PRF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Allein die sich auff Wei\u00dfheit gr\u00fcnden,", "tokens": ["Al\u00b7lein", "die", "sich", "auff", "Wei\u00df\u00b7heit", "gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRELS", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sind von der zeiten Hochmuth frey,", "tokens": ["Sind", "von", "der", "zei\u00b7ten", "Hoch\u00b7muth", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wei\u00dfheit kan das Gl\u00fcck auch binden,", "tokens": ["Die", "Wei\u00df\u00b7heit", "kan", "das", "Gl\u00fcck", "auch", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auff da\u00df es ihr zu willen sey;", "tokens": ["Auff", "da\u00df", "es", "ihr", "zu", "wil\u00b7len", "sey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PPER", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wen hat sie je so arm gelassen,", "tokens": ["Wen", "hat", "sie", "je", "so", "arm", "ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den nachmals auch die Freunde hassen?", "tokens": ["Den", "nach\u00b7mals", "auch", "die", "Freun\u00b7de", "has\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}