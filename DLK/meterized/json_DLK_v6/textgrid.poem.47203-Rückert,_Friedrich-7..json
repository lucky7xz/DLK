{"textgrid.poem.47203": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Gestalten hab' ich euch besessen,", "tokens": ["Als", "Ge\u00b7stal\u00b7ten", "hab'", "ich", "euch", "be\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Jugendlich-belebte;", "tokens": ["Ju\u00b7gend\u00b7lich\u00b7be\u00b7leb\u00b7te", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und ich kann den Traum noch nicht vergessen,", "tokens": ["Und", "ich", "kann", "den", "Traum", "noch", "nicht", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ART", "NN", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der so schnell entschwebte.", "tokens": ["Der", "so", "schnell", "ent\u00b7schweb\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Himmlischen Beschl\u00fcssen mu\u00df mit Witzen", "tokens": ["Himm\u00b7li\u00b7schen", "Be\u00b7schl\u00fcs\u00b7sen", "mu\u00df", "mit", "Wit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VMFIN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Menschensinn sich f\u00fcgen:", "tokens": ["Men\u00b7schen\u00b7sinn", "sich", "f\u00fc\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Euch als sch\u00f6ne Bilder zu besitzen,", "tokens": ["Euch", "als", "sch\u00f6\u00b7ne", "Bil\u00b7der", "zu", "be\u00b7sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mu\u00df mir jetzt gen\u00fcgen.", "tokens": ["Mu\u00df", "mir", "jetzt", "ge\u00b7n\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}