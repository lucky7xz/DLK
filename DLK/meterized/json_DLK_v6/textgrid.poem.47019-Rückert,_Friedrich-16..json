{"textgrid.poem.47019": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn all die Schar von Monden, Wochen, Tagen,", "tokens": ["Wenn", "all", "die", "Schar", "von", "Mon\u00b7den", ",", "Wo\u00b7chen", ",", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIAT", "ART", "NN", "APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Stund', Augenblick', Minuten und Sekunde,", "tokens": ["Stund'", ",", "Au\u00b7gen\u00b7blick'", ",", "Mi\u00b7nu\u00b7ten", "und", "Se\u00b7kun\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die mir durch dich verschmachteten als Wunde,", "tokens": ["Die", "mir", "durch", "dich", "ver\u00b7schmach\u00b7te\u00b7ten", "als", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "VVFIN", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die mir durch dich verjammerten als Klagen:", "tokens": ["Die", "mir", "durch", "dich", "ver\u00b7jam\u00b7mer\u00b7ten", "als", "Kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "VVFIN", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn alle sie aus ihren Sarkophagen", "tokens": ["Wenn", "al\u00b7le", "sie", "aus", "ih\u00b7ren", "Sar\u00b7ko\u00b7pha\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erst\u00fcnden und sich stellten in die Runde", "tokens": ["Er\u00b7st\u00fcn\u00b7den", "und", "sich", "stell\u00b7ten", "in", "die", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "PRF", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um dich, und h\u00fcben an aus einem Munde,", "tokens": ["Um", "dich", ",", "und", "h\u00fc\u00b7ben", "an", "aus", "ei\u00b7nem", "Mun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "$,", "KON", "ADJD", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als ihre M\u00f6rderin dich zu verklagen:", "tokens": ["Als", "ih\u00b7re", "M\u00f6r\u00b7de\u00b7rin", "dich", "zu", "ver\u00b7kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbwir alle waren einst zur Lust geboren,", "tokens": ["\u00bb", "wir", "al\u00b7le", "wa\u00b7ren", "einst", "zur", "Lust", "ge\u00b7bo\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "PIS", "VAFIN", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Berechtigt, unser Dasein zu genie\u00dfen;", "tokens": ["Be\u00b7rech\u00b7tigt", ",", "un\u00b7ser", "Da\u00b7sein", "zu", "ge\u00b7nie\u00b7\u00dfen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Durch dich ging Dasein uns und Lust verloren!\u00ab", "tokens": ["Durch", "dich", "ging", "Da\u00b7sein", "uns", "und", "Lust", "ver\u00b7lo\u00b7ren", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "PPER", "KON", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wenn so sich all die Stimmen h\u00f6ren lie\u00dfen,", "tokens": ["Wenn", "so", "sich", "all", "die", "Stim\u00b7men", "h\u00f6\u00b7ren", "lie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PRF", "PIAT", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer wei\u00df, ob du dann w\u00fcrdest noch die Ohren", "tokens": ["Wer", "wei\u00df", ",", "ob", "du", "dann", "w\u00fcr\u00b7dest", "noch", "die", "Oh\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vor ihnen wie vor meiner einen schlie\u00dfen.", "tokens": ["Vor", "ih\u00b7nen", "wie", "vor", "mei\u00b7ner", "ei\u00b7nen", "schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KOKOM", "APPR", "PPOSAT", "ART", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}