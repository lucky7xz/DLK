{"textgrid.poem.66187": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "An die Schwaben", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr lieben Schwaben insgesammt,", "tokens": ["Ihr", "lie\u00b7ben", "Schwa\u00b7ben", "ins\u00b7ge\u00b7sammt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn noch ein F\u00fcnkchen in euch flammt", "tokens": ["Wenn", "noch", "ein", "F\u00fcnk\u00b7chen", "in", "euch", "flammt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Ahnenglut, so h\u00f6ret mich \u2013", "tokens": ["Von", "Ah\u00b7nen\u00b7glut", ",", "so", "h\u00f6\u00b7ret", "mich", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn biederb, frei und deutsch bin ich.", "tokens": ["Denn", "bie\u00b7derb", ",", "frei", "und", "deutsch", "bin", "ich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "KON", "ADJD", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Un\u00fcberwindlich, gro\u00df und stark,", "tokens": ["Un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7lich", ",", "gro\u00df", "und", "stark", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In ihrer Knochen L\u00f6wenmark", "tokens": ["In", "ih\u00b7rer", "Kno\u00b7chen", "L\u00f6\u00b7wen\u00b7mark"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "War eurer gro\u00dfen V\u00e4ter Art;", "tokens": ["War", "eu\u00b7rer", "gro\u00b7\u00dfen", "V\u00e4\u00b7ter", "Art", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Jetzt seid ihr z\u00e4rtlich, winzig, zart;", "tokens": ["Jetzt", "seid", "ihr", "z\u00e4rt\u00b7lich", ",", "win\u00b7zig", ",", "zart", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Tragt statt der Waffe Degelein", "tokens": ["Tragt", "statt", "der", "Waf\u00b7fe", "De\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mit B\u00e4ndern d'ran, gar h\u00fcbsch und fein,", "tokens": ["Mit", "B\u00e4n\u00b7dern", "d'\u00b7ran", ",", "gar", "h\u00fcbsch", "und", "fein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.11": {"text": "Und sprecht mit eurem lieben Sohn", "tokens": ["Und", "sprecht", "mit", "eu\u00b7rem", "lie\u00b7ben", "Sohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Franzosensprach' im Nasenton!", "tokens": ["Fran\u00b7zo\u00b7sen\u00b7sprach'", "im", "Na\u00b7sen\u00b7ton", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ihr lauft verbuhlt um eure Weiber,", "tokens": ["Ihr", "lauft", "ver\u00b7buhlt", "um", "eu\u00b7re", "Wei\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Wie Maulwurf, Sperling, oder T\u00e4uber.", "tokens": ["Wie", "Maul\u00b7wurf", ",", "Sper\u00b7ling", ",", "o\u00b7der", "T\u00e4u\u00b7ber", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "NN", "$,", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Wer Complimente schneiden kann,", "tokens": ["Wer", "Com\u00b7pli\u00b7men\u00b7te", "schnei\u00b7den", "kann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Wer schmeicheln, kriechen, heucheln kann,", "tokens": ["Wer", "schmei\u00b7cheln", ",", "krie\u00b7chen", ",", "heu\u00b7cheln", "kann", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "VVFIN", "$,", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Der ist bei euch ein ganzer Mann!", "tokens": ["Der", "ist", "bei", "euch", "ein", "gan\u00b7zer", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Ihr haschet nur nach Rauch und Dunst,", "tokens": ["Ihr", "ha\u00b7schet", "nur", "nach", "Rauch", "und", "Dunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und nicht nach Wissenschaft und Kunst:", "tokens": ["Und", "nicht", "nach", "Wis\u00b7sen\u00b7schaft", "und", "Kunst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Drum gilt bei euch der Gauch und Tropf", "tokens": ["Drum", "gilt", "bei", "euch", "der", "Gauch", "und", "Tropf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPR", "PPER", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Mehr als der Weise und der Kopf!", "tokens": ["Mehr", "als", "der", "Wei\u00b7se", "und", "der", "Kopf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Der J\u00fcngling sitzt beim Wein so kalt,", "tokens": ["Der", "J\u00fcng\u00b7ling", "sitzt", "beim", "Wein", "so", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Als w\u00e4r' er achtzig Jahre alt,", "tokens": ["Als", "w\u00e4r'", "er", "acht\u00b7zig", "Jah\u00b7re", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "CARD", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Und s\u00e4\u00dfe auf der Alpen H\u00f6h", "tokens": ["Und", "s\u00e4\u00b7\u00dfe", "auf", "der", "Al\u00b7pen", "H\u00f6h"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Mit blo\u00dfem A** im ew'gen Schnee.", "tokens": ["Mit", "blo\u00b7\u00dfem", "A", "*", "*", "im", "ew'\u00b7gen", "Schnee", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "XY", "XY", "XY", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "\u2013 Ists Wunder, wenn man euch entehrt,", "tokens": ["\u2013", "Ists", "Wun\u00b7der", ",", "wenn", "man", "euch", "ent\u00b7ehrt", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "NN", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Als wenn ihr Y\u00e4hoo w\u00e4rt?", "tokens": ["Als", "wenn", "ihr", "Y\u00e4\u00b7hoo", "w\u00e4rt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "NE", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.28": {"text": "Schnipst euch der Sachs' und Breme doch", "tokens": ["Schnipst", "euch", "der", "Sachs'", "und", "Bre\u00b7me", "doch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Ver\u00e4chtlich unters Nasenloch.", "tokens": ["Ver\u00b7\u00e4cht\u00b7lich", "un\u00b7ters", "Na\u00b7sen\u00b7loch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "O denkt einmal im Ernste nach,", "tokens": ["O", "denkt", "ein\u00b7mal", "im", "Erns\u00b7te", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Was einst Bohemus von uns sprach:", "tokens": ["Was", "einst", "Bo\u00b7he\u00b7mus", "von", "uns", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.32": {"text": "Ach denkt daran, 's ist hohe Zeit.", "tokens": ["Ach", "denkt", "da\u00b7ran", ",", "'s", "ist", "ho\u00b7he", "Zeit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PAV", "$,", "PPER", "VAFIN", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.33": {"text": "Seid klug, schon vor den vierzig Jahren,", "tokens": ["Seid", "klug", ",", "schon", "vor", "den", "vier\u00b7zig", "Jah\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "ADV", "APPR", "ART", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Wie's eure braven V\u00e4ter waren.", "tokens": ["Wie's", "eu\u00b7re", "bra\u00b7ven", "V\u00e4\u00b7ter", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Wie sch\u00f6n, wenn einst der Enkel spricht:", "tokens": ["Wie", "sch\u00f6n", ",", "wenn", "einst", "der", "En\u00b7kel", "spricht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Die Narrenkappe pa\u00dft mir nicht.", "tokens": ["Die", "Nar\u00b7ren\u00b7kap\u00b7pe", "pa\u00dft", "mir", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}