{"textgrid.poem.62509": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Himmelfahrt", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohin, wohin, ihr Blumen?", "tokens": ["Wo\u00b7hin", ",", "wo\u00b7hin", ",", "ihr", "Blu\u00b7men", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu Gottes Heiligthumen,", "tokens": ["Zu", "Got\u00b7tes", "Hei\u00b7lig\u00b7thu\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hinauf zum Sonnenschein!", "tokens": ["Hin\u00b7auf", "zum", "Son\u00b7nen\u00b7schein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Winter mu\u00df entweichen", "tokens": ["Der", "Win\u00b7ter", "mu\u00df", "ent\u00b7wei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Aus Gottes sch\u00f6nen Reichen,", "tokens": ["Aus", "Got\u00b7tes", "sch\u00f6\u00b7nen", "Rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das Leben mu\u00df gedeihn.", "tokens": ["Das", "Le\u00b7ben", "mu\u00df", "ge\u00b7deihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er ist vorangegangen,", "tokens": ["Er", "ist", "vor\u00b7an\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Sehnsucht, Schmerz und Bangen", "tokens": ["Und", "Sehn\u00b7sucht", ",", "Schmerz", "und", "Ban\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kennt f\u00fcrder nun die Bahn;", "tokens": ["Kennt", "f\u00fcr\u00b7der", "nun", "die", "Bahn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es mag vom Thr\u00e4nenthale", "tokens": ["Es", "mag", "vom", "Thr\u00e4\u00b7nen\u00b7tha\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zu seinem Abendmahle", "tokens": ["Zu", "sei\u00b7nem", "A\u00b7bend\u00b7mah\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nun jeder J\u00fcnger nahn.", "tokens": ["Nun", "je\u00b7der", "J\u00fcn\u00b7ger", "nahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wohin, wohin, ihr Blicke!", "tokens": ["Wo\u00b7hin", ",", "wo\u00b7hin", ",", "ihr", "Bli\u00b7cke", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hinauf zum ew'gen Gl\u00fccke!", "tokens": ["Hin\u00b7auf", "zum", "ew'\u00b7gen", "Gl\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hinauf zur ew'gen Lust!", "tokens": ["Hin\u00b7auf", "zur", "ew'\u00b7gen", "Lust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Heiland ist erhoben,", "tokens": ["Der", "Hei\u00b7land", "ist", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Heiland winkt von oben", "tokens": ["Der", "Hei\u00b7land", "winkt", "von", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die J\u00fcnger an die Brust.", "tokens": ["Die", "J\u00fcn\u00b7ger", "an", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}