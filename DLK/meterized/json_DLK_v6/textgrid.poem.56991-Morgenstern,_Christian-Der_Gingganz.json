{"textgrid.poem.56991": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Der Gingganz", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Stiefel wandern und sein Knecht", "tokens": ["Ein", "Stie\u00b7fel", "wan\u00b7dern", "und", "sein", "Knecht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "von Knickeb\u00fchl gen Entenbrecht.", "tokens": ["von", "Kni\u00b7cke\u00b7b\u00fchl", "gen", "En\u00b7ten\u00b7brecht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Urpl\u00f6tzlich auf dem Felde drau\u00df", "tokens": ["Ur\u00b7pl\u00f6tz\u00b7lich", "auf", "dem", "Fel\u00b7de", "drau\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "begehrt der Stiefel: Zieh mich aus!", "tokens": ["be\u00b7gehrt", "der", "Stie\u00b7fel", ":", "Zieh", "mich", "aus", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "$.", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Knecht drauf: Es ist nicht an dem;", "tokens": ["Der", "Knecht", "drauf", ":", "Es", "ist", "nicht", "an", "dem", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "PPER", "VAFIN", "PTKNEG", "APPR", "ART", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "doch sagt mir, lieber Herre, \u2013: wem?", "tokens": ["doch", "sagt", "mir", ",", "lie\u00b7ber", "Her\u00b7re", ",", "\u2013", ":", "wem", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "NN", "$,", "$(", "$.", "PWS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dem Stiefel gibt es einen Ruck:", "tokens": ["Dem", "Stie\u00b7fel", "gibt", "es", "ei\u00b7nen", "Ruck", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcrwahr, beim heiligen Nepomuk,", "tokens": ["F\u00fcr\u00b7wahr", ",", "beim", "hei\u00b7li\u00b7gen", "Ne\u00b7po\u00b7muk", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "ich GING GANZ in Gedanken hin ...", "tokens": ["ich", "GiNG", "GaNZ", "in", "Ge\u00b7dan\u00b7ken", "hin", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "NE", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du wei\u00dft, da\u00df ich ein andrer bin,", "tokens": ["Du", "wei\u00dft", ",", "da\u00df", "ich", "ein", "an\u00b7drer", "bin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "seitdem ich meinen Herrn verlor ...", "tokens": ["seit\u00b7dem", "ich", "mei\u00b7nen", "Herrn", "ver\u00b7lor", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Knecht wirft beide Arm empor,", "tokens": ["Der", "Knecht", "wirft", "bei\u00b7de", "Arm", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "als wollt er sagen: La\u00df doch, la\u00df!", "tokens": ["als", "wollt", "er", "sa\u00b7gen", ":", "La\u00df", "doch", ",", "la\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "VVINF", "$.", "VVIMP", "ADV", "$,", "VVIMP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und weiter zieht das Paar f\u00fcrba\u00df.", "tokens": ["Und", "wei\u00b7ter", "zieht", "das", "Paar", "f\u00fcr\u00b7ba\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}