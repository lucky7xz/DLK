{"dta.poem.15039": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Nachrichten von dem Ursprung und  \n Wachsthum der Critik bey den  \n Deutschen.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1741", "urn": "urn:nbn:de:kobv:b4-200905198526", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201emonarch da\u00df in verwehnten Z\u00fcgen", "tokens": ["\u201e", "mon\u00b7arch", "da\u00df", "in", "ver\u00b7wehn\u00b7ten", "Z\u00fc\u00b7gen"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u201emein eitler Kiel von neuem sich vergi\u00dft,", "tokens": ["\u201e", "mein", "eit\u00b7ler", "Kiel", "von", "neu\u00b7em", "sich", "ver\u00b7gi\u00dft", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u201eund deiner ", "tokens": ["\u201e", "und", "dei\u00b7ner"], "token_info": ["punct", "word", "word"], "pos": ["$(", "KON", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "\u201eden l\u00fcstern Trieb der Schnsucht zu vergn\u00fcgen;", "tokens": ["\u201e", "den", "l\u00fcs\u00b7tern", "Trieb", "der", "Schn\u00b7sucht", "zu", "ver\u00b7gn\u00fc\u00b7gen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "\u201eist deiner selbstbeliebten Huld", "tokens": ["\u201e", "ist", "dei\u00b7ner", "selbst\u00b7be\u00b7lieb\u00b7ten", "Huld"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201eund eigner Gnade mehr, als meiner Frechheit Schuld.", "tokens": ["\u201e", "und", "eig\u00b7ner", "Gna\u00b7de", "mehr", ",", "als", "mei\u00b7ner", "Frech\u00b7heit", "Schuld", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADJA", "NN", "ADV", "$,", "KOUS", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201ehie\u00df dein Befehl ", "tokens": ["\u201e", "hie\u00df", "dein", "Be\u00b7fehl"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "\u201eund an der Strahlen Gold erquicken;", "tokens": ["\u201e", "und", "an", "der", "Strah\u00b7len", "Gold", "er\u00b7qui\u00b7cken", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "\u201ewas Wunder, da\u00df sich ", "tokens": ["\u201e", "was", "Wun\u00b7der", ",", "da\u00df", "sich"], "token_info": ["punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "PWS", "NN", "$,", "KOUS", "PRF"], "meter": "-+-++", "measure": "unknown.measure.tri"}, "line.10": {"text": "\u201ean dem bisher der ferne Fu\u00df geklebet,", "tokens": ["\u201e", "an", "dem", "bis\u00b7her", "der", "fer\u00b7ne", "Fu\u00df", "ge\u00b7kle\u00b7bet", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "\u201ehiervon erhitzt ", "tokens": ["\u201e", "hier\u00b7von", "er\u00b7hitzt"], "token_info": ["punct", "word", "word"], "pos": ["$(", "PAV", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "\u201edurch einen k\u00fchnen ", "tokens": ["\u201e", "durch", "ei\u00b7nen", "k\u00fch\u00b7nen"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "APPR", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "\u201eda\u00df meiner Musen ", "tokens": ["\u201e", "da\u00df", "mei\u00b7ner", "Mu\u00b7sen"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}}}}}