{"textgrid.poem.44352": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "An Selenen bei Zur\u00fcckstellung des Buches: von der Nachfolge Christi", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fruchtet doch mein Streben nichts;", "tokens": ["Fruch\u00b7tet", "doch", "mein", "Stre\u00b7ben", "nichts", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Heimisch nur im Reich der Kl\u00e4nge,", "tokens": ["Hei\u00b7misch", "nur", "im", "Reich", "der", "Kl\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bin ich fremd im Reich des Lichts.", "tokens": ["Bin", "ich", "fremd", "im", "Reich", "des", "Lichts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Meine Augen, wie erreichten", "tokens": ["Mei\u00b7ne", "Au\u00b7gen", ",", "wie", "er\u00b7reich\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie ein Ziel so hoch und fern?", "tokens": ["Sie", "ein", "Ziel", "so", "hoch", "und", "fern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jene Strahlen, die dir leuchten,", "tokens": ["Je\u00b7ne", "Strah\u00b7len", ",", "die", "dir", "leuch\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blenden meinen tr\u00fcben Stern.", "tokens": ["Blen\u00b7den", "mei\u00b7nen", "tr\u00fc\u00b7ben", "Stern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch, h\u00fcllt Nacht mir ", "tokens": ["Doch", ",", "h\u00fcllt", "Nacht", "mir"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "VVFIN", "NN", "PPER"], "meter": "--+-", "measure": "anapaest.init"}, "line.2": {"text": "Klarer sind die ", "tokens": ["Kla\u00b7rer", "sind", "die"], "token_info": ["word", "word", "word"], "pos": ["NN", "VAFIN", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Folg du ", "tokens": ["Folg", "du"], "token_info": ["word", "word"], "pos": ["NN", "PPER"], "meter": "--", "measure": "unknown.measure.zero"}}}}}