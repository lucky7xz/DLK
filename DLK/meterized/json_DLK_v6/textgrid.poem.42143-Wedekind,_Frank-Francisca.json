{"textgrid.poem.42143": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Francisca", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Francisca, mein reizender Falter,", "tokens": ["Fran\u00b7ci\u00b7sca", ",", "mein", "rei\u00b7zen\u00b7der", "Fal\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "----+--+-", "measure": "iambic.di.relaxed"}, "line.2": {"text": "H\u00e4tt'st du nicht zu eng f\u00fcr dein Alter", "tokens": ["H\u00e4tt'st", "du", "nicht", "zu", "eng", "f\u00fcr", "dein", "Al\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "PTKA", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Den keimenden Busen geschn\u00fcrt,", "tokens": ["Den", "kei\u00b7men\u00b7den", "Bu\u00b7sen", "ge\u00b7schn\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Dann klafften wohl nicht die Gew\u00e4nder,", "tokens": ["Dann", "klaff\u00b7ten", "wohl", "nicht", "die", "Ge\u00b7w\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKNEG", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Sobald ich nur eben die B\u00e4nder", "tokens": ["So\u00b7bald", "ich", "nur", "e\u00b7ben", "die", "B\u00e4n\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Mit harmlosem Finger ber\u00fchrt.", "tokens": ["Mit", "harm\u00b7lo\u00b7sem", "Fin\u00b7ger", "be\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Nun wehr auch nicht meinem Entz\u00fccken,", "tokens": ["Nun", "wehr", "auch", "nicht", "mei\u00b7nem", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Als Erster die K\u00fcsse zu pfl\u00fccken", "tokens": ["Als", "Ers\u00b7ter", "die", "K\u00fcs\u00b7se", "zu", "pfl\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Der zarten, jungfr\u00e4ulichen Haut.", "tokens": ["Der", "zar\u00b7ten", ",", "jung\u00b7fr\u00e4u\u00b7li\u00b7chen", "Haut", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Mich blendet die schneeige Wei\u00dfe,", "tokens": ["Mich", "blen\u00b7det", "die", "schne\u00b7ei\u00b7ge", "Wei\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Solang ich das Fleisch nicht, das hei\u00dfe,", "tokens": ["So\u00b7lang", "ich", "das", "Fleisch", "nicht", ",", "das", "hei\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PTKNEG", "$,", "PDS", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Mit bebenden Lippen betaut.", "tokens": ["Mit", "be\u00b7ben\u00b7den", "Lip\u00b7pen", "be\u00b7taut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+---+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Denn gleich wie die Knospe der Blume", "tokens": ["Denn", "gleich", "wie", "die", "Knos\u00b7pe", "der", "Blu\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOKOM", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Nichts ahnt von der Pracht und dem Ruhme", "tokens": ["Nichts", "ahnt", "von", "der", "Pracht", "und", "dem", "Ruh\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN", "KON", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Der Rose am \u00fcppigen Strauch,", "tokens": ["Der", "Ro\u00b7se", "am", "\u00fcp\u00b7pi\u00b7gen", "Strauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "So seh ich bescheiden erst schwellen", "tokens": ["So", "seh", "ich", "be\u00b7schei\u00b7den", "erst", "schwel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ADV", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Die keuschen, die kindlichen Wellen,", "tokens": ["Die", "keu\u00b7schen", ",", "die", "kind\u00b7li\u00b7chen", "Wel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Umweht von berauschendem Hauch.", "tokens": ["Um\u00b7weht", "von", "be\u00b7rau\u00b7schen\u00b7dem", "Hauch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Oh! glaub mir, die Monde entfliehen,", "tokens": ["Oh", "!", "glaub", "mir", ",", "die", "Mon\u00b7de", "ent\u00b7flie\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "$,", "PRELS", "NE", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Rosen verwelken, verbl\u00fchen", "tokens": ["Die", "Ro\u00b7sen", "ver\u00b7wel\u00b7ken", ",", "ver\u00b7bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVINF", "$,", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und fallen dem Winter zum Raub.", "tokens": ["Und", "fal\u00b7len", "dem", "Win\u00b7ter", "zum", "Raub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Es kommen und gehen die Jahre,", "tokens": ["Es", "kom\u00b7men", "und", "ge\u00b7hen", "die", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Man legt deinen Leib auf die Bahre", "tokens": ["Man", "legt", "dei\u00b7nen", "Leib", "auf", "die", "Bah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Und alles wird Moder und Staub.", "tokens": ["Und", "al\u00b7les", "wird", "Mo\u00b7der", "und", "Staub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}