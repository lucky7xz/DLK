{"dta.poem.20869": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Die keine Lust ger\u00fchrt was h\u00f6hers zu geniessen/", "tokens": ["Die", "kei\u00b7ne", "Lust", "ge\u00b7r\u00fchrt", "was", "h\u00f6\u00b7hers", "zu", "ge\u00b7nies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "PRELS", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sich der Tugenden des P\u00f6fels hat befliessen/", "tokens": ["Und", "sich", "der", "Tu\u00b7gen\u00b7den", "des", "P\u00f6\u00b7fels", "hat", "be\u00b7flies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und nach gemeiner Art die Fr\u00f6mmigkeit geliebt/", "tokens": ["Und", "nach", "ge\u00b7mei\u00b7ner", "Art", "die", "Fr\u00f6m\u00b7mig\u00b7keit", "ge\u00b7liebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die aus Gewonheitszug stets recht und fromm gewesen/", "tokens": ["Die", "aus", "Ge\u00b7won\u00b7heits\u00b7zug", "stets", "recht", "und", "fromm", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADV", "ADJD", "KON", "ADJD", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zu dem gemeinen Nutz viel Gutes ausge\u00fcbt/", "tokens": ["Zu", "dem", "ge\u00b7mei\u00b7nen", "Nutz", "viel", "Gu\u00b7tes", "aus\u00b7ge\u00b7\u00fcbt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und die der armen M\u00fch erlesen.", "tokens": ["Und", "die", "der", "ar\u00b7men", "M\u00fch", "er\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Von diesen glaubet man/ da\u00df sie der Himmel treibt/", "tokens": ["Von", "die\u00b7sen", "glau\u00b7bet", "man", "/", "da\u00df", "sie", "der", "Him\u00b7mel", "treibt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PIS", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Amei\u00df\u2019 und die Bein ihr Wesen einverleibt/", "tokens": ["Der", "A\u00b7mei\u00df'", "und", "die", "Bein", "ihr", "We\u00b7sen", "ein\u00b7ver\u00b7leibt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die bey und neben uns ein stilles Leben f\u00fchren/", "tokens": ["Die", "bey", "und", "ne\u00b7ben", "uns", "ein", "stil\u00b7les", "Le\u00b7ben", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "KON", "APPR", "PPER", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und f\u00fcllen Stock und Loch mit Korn und Honig an/", "tokens": ["Und", "f\u00fcl\u00b7len", "Stock", "und", "Loch", "mit", "Korn", "und", "Ho\u00b7nig", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie schaffen hier zwar nichts was sich nicht wil geb\u00fchren/", "tokens": ["Sie", "schaf\u00b7fen", "hier", "zwar", "nichts", "was", "sich", "nicht", "wil", "ge\u00b7b\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PIS", "PWS", "PRF", "PTKNEG", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch schaut man wie di\u00df nicht zum Himmel dringen kan.", "tokens": ["Doch", "schaut", "man", "wie", "di\u00df", "nicht", "zum", "Him\u00b7mel", "drin\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "KOKOM", "PDS", "PTKNEG", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das reiche Himmelsfeld da Tausend Fackeln schweben/", "tokens": ["Das", "rei\u00b7che", "Him\u00b7mels\u00b7feld", "da", "Tau\u00b7send", "Fa\u00b7ckeln", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "CARD", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist nur der reinen Schaar der Seelen \u00fcbergeben/", "tokens": ["Ist", "nur", "der", "rei\u00b7nen", "Schaar", "der", "See\u00b7len", "\u00fc\u00b7ber\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gott hat den sch\u00f6nen Plan zu ihrer Lust erkiest/", "tokens": ["Gott", "hat", "den", "sch\u00f6\u00b7nen", "Plan", "zu", "ih\u00b7rer", "Lust", "er\u00b7kiest", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da werden wir mit ihm in Ewigkeit verbunden/", "tokens": ["Da", "wer\u00b7den", "wir", "mit", "ihm", "in", "E\u00b7wig\u00b7keit", "ver\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "APPR", "NN", "VVPP", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Dafern nur unser Geist recht wol geleutert ist/", "tokens": ["Da\u00b7fern", "nur", "un\u00b7ser", "Geist", "recht", "wol", "ge\u00b7leu\u00b7tert", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PPOSAT", "NN", "ADV", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und durch die Wissenschafft das Fleisch hat \u00fcberwunden.", "tokens": ["Und", "durch", "die", "Wis\u00b7sen\u00b7schafft", "das", "Fleisch", "hat", "\u00fc\u00b7berw\u00b7un\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}