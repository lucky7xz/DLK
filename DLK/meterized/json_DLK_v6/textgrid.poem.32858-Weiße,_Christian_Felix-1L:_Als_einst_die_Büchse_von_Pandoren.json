{"textgrid.poem.32858": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als einst die B\u00fcchse von Pandoren", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als einst die B\u00fcchse von Pandoren", "tokens": ["Als", "einst", "die", "B\u00fcch\u00b7se", "von", "Pan\u00b7do\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Unheil durch die Welt verstreut:", "tokens": ["Das", "Un\u00b7heil", "durch", "die", "Welt", "ver\u00b7streut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie kl\u00e4glich wurden da die Ohren", "tokens": ["Wie", "kl\u00e4g\u00b7lich", "wur\u00b7den", "da", "die", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Menschen Vater voll geschreyt!", "tokens": ["Der", "Men\u00b7schen", "Va\u00b7ter", "voll", "ge\u00b7schreyt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der will von Liebesflammen sterben,", "tokens": ["Der", "will", "von", "Lie\u00b7bes\u00b7flam\u00b7men", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der klagt des Nachbars Freundschaft an:", "tokens": ["Der", "klagt", "des", "Nach\u00b7bars", "Freund\u00b7schaft", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dem fehlt die Frau, und dem die Erben,", "tokens": ["Dem", "fehlt", "die", "Frau", ",", "und", "dem", "die", "Er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,", "KON", "ART", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die qu\u00e4lt den Mann, und sie der Mann.", "tokens": ["Die", "qu\u00e4lt", "den", "Mann", ",", "und", "sie", "der", "Mann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was sollte Jupiter da machen?", "tokens": ["Was", "soll\u00b7te", "Ju\u00b7pi\u00b7ter", "da", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im ganzen Himmel war kein Rath.", "tokens": ["Im", "gan\u00b7zen", "Him\u00b7mel", "war", "kein", "Rath", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Anfang mu\u00dft er dr\u00fcber lachen;", "tokens": ["Im", "An\u00b7fang", "mu\u00dft", "er", "dr\u00fc\u00b7ber", "la\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch man wird auch des Lachens satt.", "tokens": ["Doch", "man", "wird", "auch", "des", "La\u00b7chens", "satt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Zuletzt gab er der Welt die Beere,", "tokens": ["Zu\u00b7letzt", "gab", "er", "der", "Welt", "die", "Bee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Aus der man Wein durch Keltern pre\u00dft,", "tokens": ["Aus", "der", "man", "Wein", "durch", "Kel\u00b7tern", "pre\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die that mehr, als die beste Lehre,", "tokens": ["Die", "that", "mehr", ",", "als", "die", "bes\u00b7te", "Leh\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Denn jeder Klagort ward ein Fest.", "tokens": ["Denn", "je\u00b7der", "Kla\u00b7gort", "ward", "ein", "Fest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der J\u00fcngling heilt von seinen Wunden,", "tokens": ["Der", "J\u00fcng\u00b7ling", "heilt", "von", "sei\u00b7nen", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So bald der helle R\u00f6mer klingt:", "tokens": ["So", "bald", "der", "hel\u00b7le", "R\u00f6\u00b7mer", "klingt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Nachbar g\u00f6nnt man frohe Stunden,", "tokens": ["Dem", "Nach\u00b7bar", "g\u00f6nnt", "man", "fro\u00b7he", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn man in seinem Keller trinkt:", "tokens": ["Wenn", "man", "in", "sei\u00b7nem", "Kel\u00b7ler", "trinkt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Man trinkt sich stark zum Eheseegen,", "tokens": ["Man", "trinkt", "sich", "stark", "zum", "E\u00b7he\u00b7see\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und schickt zur Noth die Frau ins Bad:", "tokens": ["Und", "schickt", "zur", "Noth", "die", "Frau", "ins", "Bad", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "APPRART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Man trinkt, wenn H\u00e4ndel sich erregen,", "tokens": ["Man", "trinkt", ",", "wenn", "H\u00e4n\u00b7del", "sich", "er\u00b7re\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bis man sich fromm getrunken hat.", "tokens": ["Bis", "man", "sich", "fromm", "ge\u00b7trun\u00b7ken", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PRF", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}