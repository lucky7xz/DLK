{"dta.poem.20779": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An den Frieden .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du, den Germanien mit klagenden Ges\u00e4ngen               ", "tokens": ["Du", ",", "den", "Ger\u00b7ma\u00b7ni\u00b7en", "mit", "kla\u00b7gen\u00b7den", "Ge\u00b7s\u00e4n\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Von dem Olympus niederzog!", "tokens": ["Von", "dem", "O\u00b7lym\u00b7pus", "nie\u00b7der\u00b7zog", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df dich das Laster nicht verdr\u00e4ngen,", "tokens": ["La\u00df", "dich", "das", "Las\u00b7ter", "nicht", "ver\u00b7dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das f\u00fcr dem Kriege nicht entflog;", "tokens": ["Das", "f\u00fcr", "dem", "Krie\u00b7ge", "nicht", "ent\u00b7flog", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "La\u00df keinen Griff nach einer Krone", "tokens": ["La\u00df", "kei\u00b7nen", "Griff", "nach", "ei\u00b7ner", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und keinen F\u00fcrsten-Geiz nach einem leeren Throne", "tokens": ["Und", "kei\u00b7nen", "F\u00fcrs\u00b7ten\u00b7Geiz", "nach", "ei\u00b7nem", "lee\u00b7ren", "Thro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dich kr\u00e4nken, da\u00df du wieder fliehst", "tokens": ["Dich", "kr\u00e4n\u00b7ken", ",", "da\u00df", "du", "wie\u00b7der", "fliehst"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "$,", "KOUS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und von den Himmeln den zerst\u00f6rten Weltkreis siehst.", "tokens": ["Und", "von", "den", "Him\u00b7meln", "den", "zer\u00b7st\u00f6r\u00b7ten", "Welt\u00b7kreis", "siehst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}