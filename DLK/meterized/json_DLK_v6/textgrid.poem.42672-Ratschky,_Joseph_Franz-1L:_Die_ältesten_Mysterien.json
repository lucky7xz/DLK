{"textgrid.poem.42672": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die \u00e4ltesten Mysterien", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die \u00e4ltesten Mysterien", "tokens": ["Die", "\u00e4l\u00b7tes\u00b7ten", "Mys\u00b7te\u00b7ri\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sind wohl die eleusinischen:", "tokens": ["Sind", "wohl", "die", "e\u00b7leu\u00b7si\u00b7ni\u00b7schen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Diess soll mir niemand disputiren;", "tokens": ["Diess", "soll", "mir", "nie\u00b7mand", "dis\u00b7pu\u00b7ti\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Herr Adam stiftete sie schon,", "tokens": ["Herr", "A\u00b7dam", "stif\u00b7te\u00b7te", "sie", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und ich als Adams treuer Sohn", "tokens": ["Und", "ich", "als", "A\u00b7dams", "treu\u00b7er", "Sohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "KOUS", "NE", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Liess j\u00fcngst mich auch iniziiren.", "tokens": ["Liess", "j\u00fcngst", "mich", "auch", "i\u00b7ni\u00b7zi\u00b7i\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So wie bey uns der Neophyt", "tokens": ["So", "wie", "bey", "uns", "der", "Neo\u00b7phyt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "APPR", "PPER", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Nach Maurersitt' auf Reisen zieht,", "tokens": ["Nach", "Mau\u00b7rer\u00b7sitt'", "auf", "Rei\u00b7sen", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So musst' auch ich ein paarmal wandern,", "tokens": ["So", "musst'", "auch", "ich", "ein", "paar\u00b7mal", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPER", "ART", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bis man mich aufnahm. Eisenfest", "tokens": ["Bis", "man", "mich", "auf\u00b7nahm", ".", "Ei\u00b7sen\u00b7fest"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PIS", "PPER", "VVFIN", "$.", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wallfahrtet' ich von Ost bis West", "tokens": ["Wall\u00b7fahr\u00b7tet'", "ich", "von", "Ost", "bis", "West"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von einer Schwester zu der andern.", "tokens": ["Von", "ei\u00b7ner", "Schwes\u00b7ter", "zu", "der", "an\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Den ersten Strauss auf meiner Fahrt", "tokens": ["Den", "ers\u00b7ten", "Strauss", "auf", "mei\u00b7ner", "Fahrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wagt' ich beherzt nach Ritterart", "tokens": ["Wagt'", "ich", "be\u00b7herzt", "nach", "Rit\u00b7ter\u00b7art"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit einer grundgelehrten Schwester.", "tokens": ["Mit", "ei\u00b7ner", "grund\u00b7ge\u00b7lehr\u00b7ten", "Schwes\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie war so klug, als Salomo,", "tokens": ["Sie", "war", "so", "klug", ",", "als", "Sa\u00b7lo\u00b7mo", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sprach ihr Latein, wie Cicero,", "tokens": ["Sprach", "ihr", "La\u00b7tein", ",", "wie", "Ci\u00b7ce\u00b7ro", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "PWAV", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.6": {"text": "Und war dabey so sch\u00f6n, als Esther.", "tokens": ["Und", "war", "da\u00b7bey", "so", "sch\u00f6n", ",", "als", "E\u00b7sther", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PAV", "ADV", "ADJD", "$,", "KOUS", "NN", "$."], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}}, "stanza.4": {"line.1": {"text": "Sie h\u00f6rte Wolfs Philosophie,", "tokens": ["Sie", "h\u00f6r\u00b7te", "Wolfs", "Phi\u00b7lo\u00b7so\u00b7phie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und kannte die Geographie", "tokens": ["Und", "kann\u00b7te", "die", "Geo\u00b7gra\u00b7phie"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Von Otaheite bis nach China.", "tokens": ["Von", "O\u00b7ta\u00b7hei\u00b7te", "bis", "nach", "Chi\u00b7na", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "APPR", "NE", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch sprach ich von Mysterien,", "tokens": ["Doch", "sprach", "ich", "von", "Mys\u00b7te\u00b7ri\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NE", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "So rief sie, gleich Vestalinnen:", "tokens": ["So", "rief", "sie", ",", "gleich", "Ves\u00b7ta\u00b7lin\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Hier war ich nun, wie jedermann,", "tokens": ["Hier", "war", "ich", "nun", ",", "wie", "je\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "PWAV", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der sein Latein nur halb noch kann,", "tokens": ["Der", "sein", "La\u00b7tein", "nur", "halb", "noch", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "ADJD", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Leicht merkt, nicht auf dem rechten Wege.", "tokens": ["Leicht", "merkt", ",", "nicht", "auf", "dem", "rech\u00b7ten", "We\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verscheucht durch ihren Eigensinn,", "tokens": ["Ver\u00b7scheucht", "durch", "ih\u00b7ren", "Ei\u00b7gen\u00b7sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zog ich zu ihrer Nachbarinn:", "tokens": ["Zog", "ich", "zu", "ih\u00b7rer", "Nach\u00b7ba\u00b7rinn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch hier gieng vollends alles schr\u00e4ge.", "tokens": ["Doch", "hier", "gieng", "vol\u00b7lends", "al\u00b7les", "schr\u00e4\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Denn die war f\u00fchllos, kalt und stumm", "tokens": ["Denn", "die", "war", "f\u00fchl\u00b7los", ",", "kalt", "und", "stumm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "ADJD", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und exemplarisch fromm und dumm:", "tokens": ["Und", "ex\u00b7emp\u00b7la\u00b7risch", "fromm", "und", "dumm", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man k\u00f6nnte sie kanonisiren.", "tokens": ["Man", "k\u00f6nn\u00b7te", "sie", "ka\u00b7no\u00b7ni\u00b7si\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Umsonst sucht' ich der Sch\u00fcchternen", "tokens": ["Um\u00b7sonst", "sucht'", "ich", "der", "Sch\u00fcch\u00b7ter\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "Die Reitze der Mysterien", "tokens": ["Die", "Reit\u00b7ze", "der", "Mys\u00b7te\u00b7ri\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Mit aller Kunst zu demonstriren.", "tokens": ["Mit", "al\u00b7ler", "Kunst", "zu", "de\u00b7monst\u00b7ri\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Sie blieb trotz aller meiner M\u00fch", "tokens": ["Sie", "blieb", "trotz", "al\u00b7ler", "mei\u00b7ner", "M\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "St\u00e4ts von der Scheitel bis zum Knie", "tokens": ["St\u00e4ts", "von", "der", "Schei\u00b7tel", "bis", "zum", "Knie"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "APPRART", "NN"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Eiskalt, wie eine Marmorb\u00fcste:", "tokens": ["Eis\u00b7kalt", ",", "wie", "ei\u00b7ne", "Mar\u00b7mor\u00b7b\u00fcs\u00b7te", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf jedes Wort, das ich verlor,", "tokens": ["Auf", "je\u00b7des", "Wort", ",", "das", "ich", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kam der Bescheid, dass sie zuvor", "tokens": ["Kam", "der", "Be\u00b7scheid", ",", "dass", "sie", "zu\u00b7vor"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$,", "KOUS", "PPER", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Den Pater Rektor fragen m\u00fcsste.", "tokens": ["Den", "Pa\u00b7ter", "Rek\u00b7tor", "fra\u00b7gen", "m\u00fcss\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Hier war ich nun, wie jedermann", "tokens": ["Hier", "war", "ich", "nun", ",", "wie", "je\u00b7der\u00b7mann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "PWAV", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus ihrer Dummheit schliessen kann,", "tokens": ["Aus", "ih\u00b7rer", "Dumm\u00b7heit", "schlies\u00b7sen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl auch nicht auf dem rechten Wege.", "tokens": ["Wohl", "auch", "nicht", "auf", "dem", "rech\u00b7ten", "We\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So oft zu irren, gieng mir nah:", "tokens": ["So", "oft", "zu", "ir\u00b7ren", ",", "gieng", "mir", "nah", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKZU", "VVINF", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch hiess es, jetzt sey Hoffnung da,", "tokens": ["Doch", "hiess", "es", ",", "jetzt", "sey", "Hoff\u00b7nung", "da", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADV", "VAFIN", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dass ich darauf gelangen m\u00f6ge.", "tokens": ["Dass", "ich", "da\u00b7rauf", "ge\u00b7lan\u00b7gen", "m\u00f6\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ich wagt's, mit diesem Trost versehn,", "tokens": ["Ich", "wagt's", ",", "mit", "die\u00b7sem", "Trost", "ver\u00b7sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die dritte Schwester zu bestehn.", "tokens": ["Die", "drit\u00b7te", "Schwes\u00b7ter", "zu", "be\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An dieser fand ich Wohlbehagen;", "tokens": ["An", "die\u00b7ser", "fand", "ich", "Wohl\u00b7be\u00b7ha\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie war nicht \u00fcberklug, nicht dumm:", "tokens": ["Sie", "war", "nicht", "\u00fc\u00b7berk\u00b7lug", ",", "nicht", "dumm", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "VVFIN", "$,", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "H\u00f6rt' ich einst in der Schule sagen.", "tokens": ["H\u00f6rt'", "ich", "einst", "in", "der", "Schu\u00b7le", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Zwar that auch die mir Widerstand:", "tokens": ["Zwar", "that", "auch", "die", "mir", "Wi\u00b7der\u00b7stand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch endlich bot sie mir die Hand,", "tokens": ["Doch", "end\u00b7lich", "bot", "sie", "mir", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nahm mich huldreich in die Pflege.", "tokens": ["Und", "nahm", "mich", "huld\u00b7reich", "in", "die", "Pfle\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In ihren Armen endigte", "tokens": ["In", "ih\u00b7ren", "Ar\u00b7men", "en\u00b7dig\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Mein Weh sich, und der Leidende", "tokens": ["Mein", "Weh", "sich", ",", "und", "der", "Lei\u00b7den\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PRF", "$,", "KON", "ART", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "War endlich auf dem rechten Wege.", "tokens": ["War", "end\u00b7lich", "auf", "dem", "rech\u00b7ten", "We\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Nicht albern und nicht zu gelehrt", "tokens": ["Nicht", "al\u00b7bern", "und", "nicht", "zu", "ge\u00b7lehrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "KON", "PTKNEG", "PTKZU", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sey die Geliebte, deren Werth", "tokens": ["Sey", "die", "Ge\u00b7lieb\u00b7te", ",", "de\u00b7ren", "Werth"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Euch, Br\u00fcder, reitzt, sie zu erlangen.", "tokens": ["Euch", ",", "Br\u00fc\u00b7der", ",", "reitzt", ",", "sie", "zu", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wohlan denn! feuert rings umher", "tokens": ["Wo\u00b7hlan", "denn", "!", "feu\u00b7ert", "rings", "um\u00b7her"], "to