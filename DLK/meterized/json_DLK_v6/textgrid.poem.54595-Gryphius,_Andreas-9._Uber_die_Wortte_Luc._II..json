{"textgrid.poem.54595": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "9. Uber die Wortte Luc. II.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du komst in deine Welt/ die leider dir zu enge/", "tokens": ["Du", "komst", "in", "dei\u00b7ne", "Welt", "/", "die", "lei\u00b7der", "dir", "zu", "en\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$(", "ART", "ADV", "PPER", "APPR", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Hau\u00df und Platz versagt in rasendem gedr\u00e4nge.", "tokens": ["Und", "Hau\u00df", "und", "Platz", "ver\u00b7sagt", "in", "ra\u00b7sen\u00b7dem", "ge\u00b7dr\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Hertz das noch die Welt nicht gantz besessen hat:", "tokens": ["Mein", "Hertz", "das", "noch", "die", "Welt", "nicht", "gantz", "be\u00b7ses\u00b7sen", "hat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADV", "ART", "NN", "PTKNEG", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "R\u00e4um' ich dir Heiland ein zu einer L\u00e4ger Stadt.", "tokens": ["R\u00e4um'", "ich", "dir", "Hei\u00b7land", "ein", "zu", "ei\u00b7ner", "L\u00e4\u00b7ger", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "NN", "ART", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mir leider will die Welt auch nicht ein R\u00e4umlin g\u00f6nnen:", "tokens": ["Mir", "lei\u00b7der", "will", "die", "Welt", "auch", "nicht", "ein", "R\u00e4um\u00b7lin", "g\u00f6n\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ART", "NN", "ADV", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit mir/ wirst ausser ihr du dich behelffen k\u00f6nnen.", "tokens": ["Mit", "mir", "/", "wirst", "aus\u00b7ser", "ihr", "du", "dich", "be\u00b7hel\u00b7ffen", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$(", "VAFIN", "APPR", "PPER", "PPER", "PRF", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}