{"dta.poem.19222": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "69.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf jener Wiese, wo statt Blumen Sterne stehn,", "tokens": ["Auf", "je\u00b7ner", "Wie\u00b7se", ",", "wo", "statt", "Blu\u00b7men", "Ster\u00b7ne", "stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PWAV", "APPR", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird auch ein Fr\u00fchlingswind, der Rosen wecket, wehn,", "tokens": ["Wird", "auch", "ein", "Fr\u00fch\u00b7lings\u00b7wind", ",", "der", "Ro\u00b7sen", "we\u00b7cket", ",", "wehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und Knospen werden dort auch \u00fcber Nacht aufgehn.", "tokens": ["Und", "Knos\u00b7pen", "wer\u00b7den", "dort", "auch", "\u00fc\u00b7ber", "Nacht", "auf\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADV", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mit blo\u00dfen Augen siehst du nicht in jener Ferne,", "tokens": ["Mit", "blo\u00b7\u00dfen", "Au\u00b7gen", "siehst", "du", "nicht", "in", "je\u00b7ner", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch mit bewaffneten, o Sohn, die Nebelsterne,", "tokens": ["Doch", "mit", "be\u00b7waff\u00b7ne\u00b7ten", ",", "o", "Sohn", ",", "die", "Ne\u00b7bels\u00b7ter\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "$,", "FM", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Von au\u00dfen d\u00e4mmernd noch, doch stralend schon im Kerne.", "tokens": ["Von", "au\u00b7\u00dfen", "d\u00e4m\u00b7mernd", "noch", ",", "doch", "stra\u00b7lend", "schon", "im", "Ker\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "ADV", "$,", "ADV", "ADJD", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Das sind die Knospen, die noch nicht sind aufgegangen,", "tokens": ["Das", "sind", "die", "Knos\u00b7pen", ",", "die", "noch", "nicht", "sind", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "PTKNEG", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die aufgegangen einst als Rosen werden prangen.", "tokens": ["Die", "auf\u00b7ge\u00b7gan\u00b7gen", "einst", "als", "Ro\u00b7sen", "wer\u00b7den", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "KOUS", "NN", "VAINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wann? frage nicht. Ein Tag schm\u00fcckt hier den Rosenhag,", "tokens": ["Wann", "?", "fra\u00b7ge", "nicht", ".", "Ein", "Tag", "schm\u00fcckt", "hier", "den", "Ro\u00b7sen\u00b7hag", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PTKNEG", "$.", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch hunderttausend Jahr sind dort ein Fr\u00fchlingstag.", "tokens": ["Doch", "hun\u00b7dert\u00b7tau\u00b7send", "Jahr", "sind", "dort", "ein", "Fr\u00fch\u00b7lings\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}