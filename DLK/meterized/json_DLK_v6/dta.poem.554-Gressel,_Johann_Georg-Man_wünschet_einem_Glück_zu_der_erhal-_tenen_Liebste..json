{"dta.poem.554": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Man w\u00fcnschet einem Gl\u00fcck zu der erhal-  \n tenen Liebste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel heitert sich nach langen Blitzen aus/", "tokens": ["Der", "Him\u00b7mel", "hei\u00b7tert", "sich", "nach", "lan\u00b7gen", "Blit\u00b7zen", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und auf das Donnern folgt ein helles Sonnenscheinen/", "tokens": ["Und", "auf", "das", "Don\u00b7nern", "folgt", "ein", "hel\u00b7les", "Son\u00b7nen\u00b7schei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Gl\u00fccke lacht zuletzt nach ausgestandnen Strau\u00df/", "tokens": ["Das", "Gl\u00fc\u00b7cke", "lacht", "zu\u00b7letzt", "nach", "aus\u00b7ge\u00b7stand\u00b7nen", "Strau\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ein erfreut Gesicht beschleu\u00dft das lange Weinen.", "tokens": ["Und", "ein", "er\u00b7freut", "Ge\u00b7sicht", "be\u00b7schleu\u00dft", "das", "lan\u00b7ge", "Wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nach denen Fasten bricht die Oster-Woche an/", "tokens": ["Nach", "de\u00b7nen", "Fas\u00b7ten", "bricht", "die", "Os\u00b7ter\u00b7Wo\u00b7che", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Darinn ein Jubel-Fest die treuen Seelen halten/", "tokens": ["Da\u00b7rinn", "ein", "Ju\u00b7bel\u00b7Fest", "die", "treu\u00b7en", "See\u00b7len", "hal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die ihren Lauff gehabt auf einer Dornen Bahn/", "tokens": ["Die", "ih\u00b7ren", "Lauff", "ge\u00b7habt", "auf", "ei\u00b7ner", "Dor\u00b7nen", "Bahn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VAPP", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und mehr denn offt gemeynt sie m\u00fcsten nun erkalten.", "tokens": ["Und", "mehr", "denn", "offt", "ge\u00b7meynt", "sie", "m\u00fcs\u00b7ten", "nun", "er\u00b7kal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "VVPP", "PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Sommer s\u00fcsser Lust bricht nach dem Winter ein/", "tokens": ["Der", "Som\u00b7mer", "s\u00fcs\u00b7ser", "Lust", "bricht", "nach", "dem", "Win\u00b7ter", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und das erstorbne Feld bekommt ein neues Leben/", "tokens": ["Und", "das", "ers\u00b7torb\u00b7ne", "Feld", "be\u00b7kommt", "ein", "neu\u00b7es", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "Die Blumen zeigen an des Himmels G\u00fctig-seyn/", "tokens": ["Die", "Blu\u00b7men", "zei\u00b7gen", "an", "des", "Him\u00b7mels", "G\u00fc\u00b7tig\u00b7seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sie wollen dessen Gunst durch ihren Glantz erheben.", "tokens": ["Sie", "wol\u00b7len", "des\u00b7sen", "Gunst", "durch", "ih\u00b7ren", "Glantz", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDS", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So kehrt sein Un-stern sich auch in ein nenes Gl\u00fcck/", "tokens": ["So", "kehrt", "sein", "Un\u00b7stern", "sich", "auch", "in", "ein", "ne\u00b7nes", "Gl\u00fcck", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PRF", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Selbst das Verh\u00e4ngni\u00df will ihn jetzt mit Wollust kr\u00f6nen/", "tokens": ["Selbst", "das", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "will", "ihn", "jetzt", "mit", "Wol\u00b7lust", "kr\u00f6\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.15": {"text": "Der Himmel liebet ihn/ er giebt beliebten Blick/", "tokens": ["Der", "Him\u00b7mel", "lie\u00b7bet", "ihn", "/", "er", "giebt", "be\u00b7lieb\u00b7ten", "Blick", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$(", "PPER", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Sein W\u00fcnschen macht er wahr/ nun trifft ihm ein sein Sehnen.", "tokens": ["Sein", "W\u00fcn\u00b7schen", "macht", "er", "wahr", "/", "nun", "trifft", "ihm", "ein", "sein", "Seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$(", "ADV", "VVFIN", "PPER", "ART", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Denn seiner Augen-Lust/ des Hertzens halber Theil/", "tokens": ["Denn", "sei\u00b7ner", "Au\u00b7gen\u00b7Lust", "/", "des", "Hert\u00b7zens", "hal\u00b7ber", "Theil", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$(", "ART", "NN", "APPO", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Macht seine Freude voll/ sie h\u00e4uffet sein Vergn\u00fcgen/", "tokens": ["Macht", "sei\u00b7ne", "Freu\u00b7de", "voll", "/", "sie", "h\u00e4uf\u00b7fet", "sein", "Ver\u00b7gn\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die Seelen-Wunde ist nun aus dem Grunde heyl/", "tokens": ["Die", "See\u00b7len\u00b7Wun\u00b7de", "ist", "nun", "aus", "dem", "Grun\u00b7de", "heyl", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Sie l\u00e4sset ihn/ mein Freund/ die harte Brust befiegen.", "tokens": ["Sie", "l\u00e4s\u00b7set", "ihn", "/", "mein", "Freund", "/", "die", "har\u00b7te", "Brust", "be\u00b7fie\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "PPOSAT", "NN", "$(", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}