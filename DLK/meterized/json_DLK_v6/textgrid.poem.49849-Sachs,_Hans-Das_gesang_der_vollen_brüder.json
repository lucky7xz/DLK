{"textgrid.poem.49849": {"metadata": {"author": {"name": "Sachs, Hans", "birth": "N.A.", "death": "N.A."}, "title": "Das gesang der vollen br\u00fcder", "genre": "verse", "period": "N.A.", "pub_year": 1568, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer hie f\u00fcr ge, der schau uns an,", "tokens": ["Wer", "hie", "f\u00fcr", "ge", ",", "der", "schau", "uns", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NE", "$,", "PRELS", "ADJD", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "der vollen br\u00fcder ordn wir han,", "tokens": ["der", "vol\u00b7len", "br\u00fc\u00b7der", "ordn", "wir", "han", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und all das gaudeamus singen,", "tokens": ["und", "all", "das", "gau\u00b7dea\u00b7mus", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "das fortuna la\u00df wir erklingen,", "tokens": ["das", "for\u00b7tu\u00b7na", "la\u00df", "wir", "er\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIMP", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "wir haben Bacchum auserkorn,", "tokens": ["wir", "ha\u00b7ben", "Bac\u00b7chum", "au\u00b7ser\u00b7korn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "derselb ist unser abgot worn;", "tokens": ["der\u00b7selb", "ist", "un\u00b7ser", "ab\u00b7got", "worn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "VVPP", "VAPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "was uns verlie\u00df mutter und vater", "tokens": ["was", "uns", "ver\u00b7lie\u00df", "mut\u00b7ter", "und", "va\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "als unser einige woltater,", "tokens": ["als", "un\u00b7ser", "ei\u00b7ni\u00b7ge", "wol\u00b7ta\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "das mu\u00df als mit uns gen zu grunt", "tokens": ["das", "mu\u00df", "als", "mit", "uns", "gen", "zu", "grunt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "KOKOM", "APPR", "PPER", "APPR", "PTKA", "ADJD"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "und als faren durch unsern schlunt.", "tokens": ["und", "als", "fa\u00b7ren", "durch", "un\u00b7sern", "schlunt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "APPR", "PPOSAT", "ADJD", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "die v\u00f6gel w\u00f6ll wir la\u00dfen sorgen,", "tokens": ["die", "v\u00f6\u00b7gel", "w\u00f6ll", "wir", "la\u00b7\u00dfen", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "ein abnt ist be\u00dfer den sibn morgen", "tokens": ["ein", "abnt", "ist", "be\u00b7\u00dfer", "den", "sibn", "mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VAFIN", "ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.13": {"text": "mit schlemmerei, saufen und fre\u00dfen,", "tokens": ["mit", "schlem\u00b7me\u00b7rei", ",", "sau\u00b7fen", "und", "fre\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.14": {"text": "da alles ungl\u00fccks wir verge\u00dfen,", "tokens": ["da", "al\u00b7les", "un\u00b7gl\u00fccks", "wir", "ver\u00b7ge\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "fr\u00f6lich das gaudeamus singen", "tokens": ["fr\u00f6\u00b7lich", "das", "gau\u00b7dea\u00b7mus", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.16": {"text": "mit allen kurzweiligen dingen.", "tokens": ["mit", "al\u00b7len", "kurz\u00b7wei\u00b7li\u00b7gen", "din\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.17": {"text": "wer das sein spart, nit tag und nacht", "tokens": ["wer", "das", "sein", "spart", ",", "nit", "tag", "und", "nacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ART", "PPOSAT", "ADJD", "$,", "PTKNEG", "NN", "KON", "NN"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.18": {"text": "schlemt, derselb wirt von uns veracht;", "tokens": ["schlemt", ",", "der\u00b7selb", "wirt", "von", "uns", "ver\u00b7acht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PDS", "VAFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "wir achten keinr sitten noch tugent,", "tokens": ["wir", "ach\u00b7ten", "keinr", "sit\u00b7ten", "noch", "tu\u00b7gent", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADV", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "wie wirs triben in unser jugent,", "tokens": ["wie", "wirs", "tri\u00b7ben", "in", "un\u00b7ser", "ju\u00b7gent", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.21": {"text": "so treib wirs bi\u00df ins alter auch;", "tokens": ["so", "treib", "wirs", "bi\u00df", "ins", "al\u00b7ter", "auch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "APPRART", "ADJA", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "dasselb ist unser aller brauch,", "tokens": ["das\u00b7selb", "ist", "un\u00b7ser", "al\u00b7ler", "brauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPOSAT", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "darmit get hauptgut und der gwin", "tokens": ["dar\u00b7mit", "get", "haupt\u00b7gut", "und", "der", "gwin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADJD", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "mit unser schlemmerei dahin.", "tokens": ["mit", "un\u00b7ser", "schlem\u00b7me\u00b7rei", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "wir verzeren zwelf pfunt vom taler,", "tokens": ["wir", "ver\u00b7ze\u00b7ren", "zwelf", "pfunt", "vom", "ta\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "VVFIN", "APPRART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.26": {"text": "sint all gut borger und b\u00f6s zaler,", "tokens": ["sint", "all", "gut", "bor\u00b7ger", "und", "b\u00f6s", "za\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJD", "ADJA", "KON", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "gut gesellen und b\u00f6s kintsvetter,", "tokens": ["gut", "ge\u00b7sel\u00b7len", "und", "b\u00f6s", "kints\u00b7vet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "KON", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.28": {"text": "stanthaftig wie aprillenwetter;", "tokens": ["stant\u00b7haf\u00b7tig", "wie", "ap\u00b7ril\u00b7len\u00b7wet\u00b7ter", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "wenn es denn in das alter get,", "tokens": ["wenn", "es", "denn", "in", "das", "al\u00b7ter", "get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "ADJA", "VVPP", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.30": {"text": "erst unser sach baufellig stet,", "tokens": ["erst", "un\u00b7ser", "sach", "bau\u00b7fel\u00b7lig", "stet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "so ler und \u00f6d stet unser haus,", "tokens": ["so", "ler", "und", "\u00f6d", "stet", "un\u00b7ser", "haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "der beste hausrat ist heraus,", "tokens": ["der", "bes\u00b7te", "haus\u00b7rat", "ist", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "als silbergschirr, kupfer und zin", "tokens": ["als", "sil\u00b7berg\u00b7schirr", ",", "kup\u00b7fer", "und", "zin"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "ADJD", "KON", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "ist mit der schlemmerei dahin,", "tokens": ["ist", "mit", "der", "schlem\u00b7me\u00b7rei", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "dergleich kleider und betgewant", "tokens": ["derg\u00b7leich", "klei\u00b7der", "und", "bet\u00b7ge\u00b7want"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.36": {"text": "stet under den j\u00fcden zu pfant,", "tokens": ["stet", "un\u00b7der", "den", "j\u00fc\u00b7den", "zu", "pfant", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ART", "ADJA", "APPR", "NN", "$,"], "meter": "----+--+", "measure": "iambic.di.chol"}, "line.37": {"text": "und ist nichts da den angst und not,", "tokens": ["und", "ist", "nichts", "da", "den", "angst", "und", "not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "ADV", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "da uns erst gute h\u00fclf tut not.", "tokens": ["da", "uns", "erst", "gu\u00b7te", "h\u00fclf", "tut", "not", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "so wir sint alt, machtlos und krank,", "tokens": ["so", "wir", "sint", "alt", ",", "macht\u00b7los", "und", "krank", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.40": {"text": "so verget uns das fr\u00f6lich gsank,", "tokens": ["so", "ver\u00b7get", "uns", "das", "fr\u00f6\u00b7lich", "gsank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "und singen denn den wemmerwe,", "tokens": ["und", "sin\u00b7gen", "denn", "den", "wem\u00b7mer\u00b7we", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "bi\u00df uns die ellent sel ausge.", "tokens": ["bi\u00df", "uns", "die", "el\u00b7lent", "sel", "aus\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}