{"dta.poem.10991": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "B.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist das f\u00fcr ein sch\u00f6ner Tag! genie\u00df ihn! denke,\nda\u00df er sch\u00f6n;", "tokens": ["Was", "ist", "das", "f\u00fcr", "ein", "sch\u00f6\u00b7ner", "Tag", "!", "ge\u00b7nie\u00df", "ihn", "!", "den\u00b7ke", ",", "da\u00df", "er", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "APPR", "ART", "ADJA", "NN", "$.", "VVFIN", "PPER", "$.", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Er f\u00e4hret sonsten schnell dahin, und wird, wie ein Geschrey,", "tokens": ["Er", "f\u00e4h\u00b7ret", "sons\u00b7ten", "schnell", "da\u00b7hin", ",", "und", "wird", ",", "wie", "ein", "Ge\u00b7schrey", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PAV", "$,", "KON", "VAFIN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}