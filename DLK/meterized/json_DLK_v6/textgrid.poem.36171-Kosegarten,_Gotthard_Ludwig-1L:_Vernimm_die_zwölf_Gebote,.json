{"textgrid.poem.36171": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vernimm die zw\u00f6lf Gebote,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vernimm die zw\u00f6lf Gebote,", "tokens": ["Ver\u00b7nimm", "die", "zw\u00f6lf", "Ge\u00b7bo\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Gott dem Kriegsmann gab,", "tokens": ["Die", "Gott", "dem", "Kriegs\u00b7mann", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und f\u00fcge dich dem Gotte,", "tokens": ["Und", "f\u00fc\u00b7ge", "dich", "dem", "Got\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und folg' ihm bis ans Grab!", "tokens": ["Und", "fol\u00b7g'", "ihm", "bis", "ans", "Grab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Du sollt von Herzen lieben", "tokens": ["Du", "sollt", "von", "Her\u00b7zen", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gott und den Bruder dein;", "tokens": ["Gott", "und", "den", "Bru\u00b7der", "dein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "PPOSAT", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Den Feind sollt du betr\u00fcben,", "tokens": ["Den", "Feind", "sollt", "du", "be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem Freunde freundlich seyn!", "tokens": ["Dem", "Freun\u00b7de", "freund\u00b7lich", "seyn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du sollt auf Hut und Wache,", "tokens": ["Du", "sollt", "auf", "Hut", "und", "Wa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf Zahl und Zeug nicht bau'n;", "tokens": ["Auf", "Zahl", "und", "Zeug", "nicht", "bau'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du sollt der guten Sache", "tokens": ["Du", "sollt", "der", "gu\u00b7ten", "Sa\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und deinem Recht vertrau'n!", "tokens": ["Und", "dei\u00b7nem", "Recht", "ver\u00b7trau'n", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "F\u00fcrchten sollt du gar kindlich", "tokens": ["F\u00fcrch\u00b7ten", "sollt", "du", "gar", "kind\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "ADV", "ADJD"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Den Feldherrn, der gebeut.", "tokens": ["Den", "Feld\u00b7herrn", ",", "der", "ge\u00b7beut", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem Feind' un\u00fcberwindlich", "tokens": ["Dem", "Feind'", "un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist, wer den Feldherrn scheu't.", "tokens": ["Ist", ",", "wer", "den", "Feld\u00b7herrn", "scheu'", "t."], "token_info": ["word", "punct", "word", "word", "word", "word", "abbreviation"], "pos": ["VAFIN", "$,", "PWS", "ART", "NN", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Den B\u00fcrger und den Bauer", "tokens": ["Den", "B\u00fcr\u00b7ger", "und", "den", "Bau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sollt du in Ehren ha'n;", "tokens": ["Sollt", "du", "in", "Eh\u00b7ren", "ha'n", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sollt wie Wall und Mauer", "tokens": ["Und", "sollt", "wie", "Wall", "und", "Mau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "KOKOM", "NE", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor Heerd und Altar stahn!", "tokens": ["Vor", "Heerd", "und", "Al\u00b7tar", "stahn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Heilig sey dir vor allen", "tokens": ["Hei\u00b7lig", "sey", "dir", "vor", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "VAFIN", "PPER", "APPR", "PIAT"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Der Frau'n und Jungfrau'n Zucht;", "tokens": ["Der", "Frau'n", "und", "Jung\u00b7frau'n", "Zucht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn nichts mag Gott gefallen,", "tokens": ["Denn", "nichts", "mag", "Gott", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was schamlos und verrucht!", "tokens": ["Was", "scham\u00b7los", "und", "ver\u00b7rucht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Du sollt ein Leu mit Leuen,", "tokens": ["Du", "sollt", "ein", "Leu", "mit", "Leu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Lamm mit L\u00e4mmern seyn.", "tokens": ["Ein", "Lamm", "mit", "L\u00e4m\u00b7mern", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Wetterstrahl im Freien,", "tokens": ["Ein", "Wet\u00b7ter\u00b7strahl", "im", "Frei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Daheim wie Sonnenschein!", "tokens": ["Da\u00b7heim", "wie", "Son\u00b7nen\u00b7schein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Du sollt kein Held im Prahlen,", "tokens": ["Du", "sollt", "kein", "Held", "im", "Prah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht keck seyn mit dem Maul!", "tokens": ["Nicht", "keck", "seyn", "mit", "dem", "Maul", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Thaten sollt du zahlen.", "tokens": ["Mit", "Tha\u00b7ten", "sollt", "du", "zah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer schw\u00e4tzt' wird hohl und faul.", "tokens": ["Wer", "schw\u00e4tzt'", "wird", "hohl", "und", "faul", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Du sollt den Feind nicht l\u00e4stern,", "tokens": ["Du", "sollt", "den", "Feind", "nicht", "l\u00e4s\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ba\u00df kleiden Ma\u00df und Glimpf.", "tokens": ["Ba\u00df", "klei\u00b7den", "Ma\u00df", "und", "Glimpf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Der Feind ist nicht seit gestern", "tokens": ["Der", "Feind", "ist", "nicht", "seit", "ge\u00b7stern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "APPR", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ge\u00fcbt auf Ernst und Schimpf.", "tokens": ["Ge\u00b7\u00fcbt", "auf", "Ernst", "und", "Schimpf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Du sollt den Feind nicht richten!", "tokens": ["Du", "sollt", "den", "Feind", "nicht", "rich\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihn richten darf nur Gott!", "tokens": ["Ihn", "rich\u00b7ten", "darf", "nur", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dir ziemt ihn zu vernichten,", "tokens": ["Dir", "ziemt", "ihn", "zu", "ver\u00b7nich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Magst du's, mit Kraut und Loth.", "tokens": ["Magst", "du's", ",", "mit", "Kraut", "und", "Loth", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.11": {"line.1": {"text": "Du sollt in frommen Spr\u00fcchen", "tokens": ["Du", "sollt", "in", "from\u00b7men", "Spr\u00fc\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht sprudeln Gift und Gall;", "tokens": ["Nicht", "spru\u00b7deln", "Gift", "und", "Gall", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "NN", "KON", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gebet stimmt nicht zu Fl\u00fcchen,", "tokens": ["Ge\u00b7bet", "stimmt", "nicht", "zu", "Fl\u00fc\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Christ nicht zu Belial!", "tokens": ["Christ", "nicht", "zu", "Be\u00b7li\u00b7al", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Trag nicht das Kreuz im Munde,", "tokens": ["Trag", "nicht", "das", "Kreuz", "im", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Trag's nicht auf Hut und Latz;", "tokens": ["Trag's", "nicht", "auf", "Hut", "und", "Latz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Heimlich ins Herzens Grunde", "tokens": ["Heim\u00b7lich", "ins", "Her\u00b7zens", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Trag den verborg'nen Schatz!", "tokens": ["Trag", "den", "ver\u00b7bor\u00b7g'\u00b7nen", "Schatz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.13": {"line.1": {"text": "Und gilt es nun, ihr Br\u00fcder,", "tokens": ["Und", "gilt", "es", "nun", ",", "ihr", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zieht hin dann! Schaut nicht um!", "tokens": ["Zieht", "hin", "dann", "!", "Schaut", "nicht", "um", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$.", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer's Leben wagt, kommt wieder,", "tokens": ["Wer's", "Le\u00b7ben", "wagt", ",", "kommt", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer's Leben spart, kommt um.", "tokens": ["Wer's", "Le\u00b7ben", "spart", ",", "kommt", "um", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Die\u00df sind die zw\u00f6lf Gebote,", "tokens": ["Die\u00df", "sind", "die", "zw\u00f6lf", "Ge\u00b7bo\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gegeben von dem Gott.", "tokens": ["Ge\u00b7ge\u00b7ben", "von", "dem", "Gott", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So du nun folgst dem Gotte,", "tokens": ["So", "du", "nun", "folgst", "dem", "Got\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So schadet dir kein Tod.", "tokens": ["So", "scha\u00b7det", "dir", "kein", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}