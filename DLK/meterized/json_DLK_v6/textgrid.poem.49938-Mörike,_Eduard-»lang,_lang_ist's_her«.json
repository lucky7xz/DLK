{"textgrid.poem.49938": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "\u00bblang, lang ist's her\u00ab", "genre": "verse", "period": "N.A.", "pub_year": 1866, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es gibt ein altes Liebeslied, vom Norden kommt's,", "tokens": ["Es", "gibt", "ein", "al\u00b7tes", "Lie\u00b7bes\u00b7lied", ",", "vom", "Nor\u00b7den", "kommt's", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie ferne Glockenlaute, oder wie am Strand", "tokens": ["Wie", "fer\u00b7ne", "Glo\u00b7cken\u00b7lau\u00b7te", ",", "o\u00b7der", "wie", "am", "Strand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "$,", "KON", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Eint\u00f6nig sanfter Wellenschlag sich wiederholt,", "tokens": ["Ein\u00b7t\u00f6\u00b7nig", "sanf\u00b7ter", "Wel\u00b7len\u00b7schlag", "sich", "wie\u00b7der\u00b7holt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "PRF", "VVPP", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Dem man so gern, vergangner Zeiten denkend lauscht;", "tokens": ["Dem", "man", "so", "gern", ",", "ver\u00b7gang\u00b7ner", "Zei\u00b7ten", "den\u00b7kend", "lauscht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "ADV", "$,", "ADJA", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn endlos, s\u00fc\u00dfer Wehmut uners\u00e4ttigt, kehrt", "tokens": ["Denn", "end\u00b7los", ",", "s\u00fc\u00b7\u00dfer", "Weh\u00b7mut", "un\u00b7er\u00b7s\u00e4t\u00b7tigt", ",", "kehrt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADJD", "$,", "ADJA", "NN", "ADJD", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das immer gleiche Wort zur\u00fcck: Lang, lang ist's her.", "tokens": ["Das", "im\u00b7mer", "glei\u00b7che", "Wort", "zu\u00b7r\u00fcck", ":", "Lang", ",", "lang", "ist's", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJA", "NN", "PTKVZ", "$.", "NN", "$,", "ADJD", "VAFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u2013 Du kennst es wohl, und nie vielleicht so lieblich mehr", "tokens": ["\u2013", "Du", "kennst", "es", "wohl", ",", "und", "nie", "viel\u00b7leicht", "so", "lieb\u00b7lich", "mehr"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "$,", "KON", "ADV", "ADV", "ADV", "ADJD", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als jenen Tag aus deinem Munde h\u00f6ren wir's.", "tokens": ["Als", "je\u00b7nen", "Tag", "aus", "dei\u00b7nem", "Mun\u00b7de", "h\u00f6\u00b7ren", "wir'", "s."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "PDAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "VAFIN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie kommt es doch, da\u00df mitten hier im lauten Schwarm", "tokens": ["Wie", "kommt", "es", "doch", ",", "da\u00df", "mit\u00b7ten", "hier", "im", "lau\u00b7ten", "Schwarm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "ADV", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entz\u00fcckter G\u00e4ste, die dein Fest versammelt hat,", "tokens": ["Ent\u00b7z\u00fcck\u00b7ter", "G\u00e4s\u00b7te", ",", "die", "dein", "Fest", "ver\u00b7sam\u00b7melt", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mir insgeheim die schlichte Weise immerdar", "tokens": ["Mir", "ins\u00b7ge\u00b7heim", "die", "schlich\u00b7te", "Wei\u00b7se", "im\u00b7mer\u00b7dar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Im Ohre fl\u00fcsternd liegen mu\u00df: Lang, lang ist's her \u2013?", "tokens": ["Im", "Oh\u00b7re", "fl\u00fcs\u00b7ternd", "lie\u00b7gen", "mu\u00df", ":", "Lang", ",", "lang", "ist's", "her", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVPP", "VVINF", "VMFIN", "$.", "NN", "$,", "ADJD", "VAFIN", "ADV", "$(", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u2013 Nachdenklich auch und wie der Gegenwart entr\u00fcckt", "tokens": ["\u2013", "Nach\u00b7denk\u00b7lich", "auch", "und", "wie", "der", "Ge\u00b7gen\u00b7wart", "ent\u00b7r\u00fcckt"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "ADV", "KON", "PWAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Auf Augenblicke seh ich deinen Vater dort,", "tokens": ["Auf", "Au\u00b7gen\u00b7bli\u00b7cke", "seh", "ich", "dei\u00b7nen", "Va\u00b7ter", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den Freund, mit dem ich jung gewesen und bei dem", "tokens": ["Den", "Freund", ",", "mit", "dem", "ich", "jung", "ge\u00b7we\u00b7sen", "und", "bei", "dem"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "ADJD", "VAPP", "KON", "APPR", "ART"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Das Herz mir immer jung aufgeht, so alt es sei.", "tokens": ["Das", "Herz", "mir", "im\u00b7mer", "jung", "auf\u00b7geht", ",", "so", "alt", "es", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "ADJD", "VVFIN", "$,", "ADV", "ADJD", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was wir erstrebt, genossen beide und verschmerzt,", "tokens": ["Was", "wir", "er\u00b7strebt", ",", "ge\u00b7nos\u00b7sen", "bei\u00b7de", "und", "ver\u00b7schmerzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVFIN", "PIS", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "In tausend Bildern dr\u00e4ngt sich's vor die Seele mir:", "tokens": ["In", "tau\u00b7send", "Bil\u00b7dern", "dr\u00e4ngt", "sich's", "vor", "die", "See\u00b7le", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PIS", "APPR", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Des Scherzes F\u00fclle, dicht am Ernst, und Lieb und Ha\u00df,", "tokens": ["Des", "Scher\u00b7zes", "F\u00fcl\u00b7le", ",", "dicht", "am", "Ernst", ",", "und", "Lieb", "und", "Ha\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADJD", "APPRART", "NN", "$,", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bei vielem Irrtum vieles doch, das nicht get\u00e4uscht.", "tokens": ["Bei", "vie\u00b7lem", "Irr\u00b7tum", "vie\u00b7les", "doch", ",", "das", "nicht", "ge\u00b7t\u00e4uscht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "PIS", "ADV", "$,", "PRELS", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u2013 \u2013 Ihm selber aber, wie mu\u00df ihm zu Sinne sein,", "tokens": ["\u2013", "\u2013", "Ihm", "sel\u00b7ber", "a\u00b7ber", ",", "wie", "mu\u00df", "ihm", "zu", "Sin\u00b7ne", "sein", ","], "token_info": ["punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "PPER", "ADV", "ADV", "$,", "PWAV", "VMFIN", "PPER", "APPR", "NN", "VAINF", "$,"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Die Tochter heut an eines edeln Mannes Hand", "tokens": ["Die", "Toch\u00b7ter", "heut", "an", "ei\u00b7nes", "e\u00b7deln", "Man\u00b7nes", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Zu sehn, dein liebes Haupt, o Kind, bekr\u00e4nzt von Ihr,", "tokens": ["Zu", "sehn", ",", "dein", "lie\u00b7bes", "Haupt", ",", "o", "Kind", ",", "be\u00b7kr\u00e4nzt", "von", "Ihr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PPOSAT", "ADJA", "NN", "$,", "FM", "NN", "$,", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die l\u00e4chelnd uns in deiner br\u00e4utlichen Gestalt", "tokens": ["Die", "l\u00e4\u00b7chelnd", "uns", "in", "dei\u00b7ner", "br\u00e4ut\u00b7li\u00b7chen", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der eignen Jugend Bl\u00fcte wieder schauen l\u00e4\u00dft!", "tokens": ["Der", "eig\u00b7nen", "Ju\u00b7gend", "Bl\u00fc\u00b7te", "wie\u00b7der", "schau\u00b7en", "l\u00e4\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nun wendet sich dein Lebensweg; du gehst von uns,", "tokens": ["Nun", "wen\u00b7det", "sich", "dein", "Le\u00b7bens\u00b7weg", ";", "du", "gehst", "von", "uns", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fernhin, wo dir ein trauter Herd bereitet ist,", "tokens": ["Fern\u00b7hin", ",", "wo", "dir", "ein", "trau\u00b7ter", "Herd", "be\u00b7rei\u00b7tet", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "ART", "ADJA", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und manches Auge sieht dir schwer von Tr\u00e4nen nach.", "tokens": ["Und", "man\u00b7ches", "Au\u00b7ge", "sieht", "dir", "schwer", "von", "Tr\u00e4\u00b7nen", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "ADJD", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u2013 Noch steht die Sonne dieses Tags am Himmel und", "tokens": ["\u2013", "Noch", "steht", "die", "Son\u00b7ne", "die\u00b7ses", "Tags", "am", "Him\u00b7mel", "und"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "ART", "NN", "PDAT", "NN", "APPRART", "NN", "KON"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Noch hei\u00dft es Heute; wenn dies Heute Gestern hei\u00dft,", "tokens": ["Noch", "hei\u00dft", "es", "Heu\u00b7te", ";", "wenn", "dies", "Heu\u00b7te", "Ge\u00b7stern", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "KOUS", "PDS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Wie anders liegt die Welt bereits vor deinem Blick!", "tokens": ["Wie", "an\u00b7ders", "liegt", "die", "Welt", "be\u00b7reits", "vor", "dei\u00b7nem", "Blick", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u2013 Und Jahr um Jahr vergeht gemach mit Eile so.", "tokens": ["\u2013", "Und", "Jahr", "um", "Jahr", "ver\u00b7geht", "ge\u00b7mach", "mit", "Ei\u00b7le", "so", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "NN", "APPR", "NN", "VVFIN", "ADV", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ihr Inhalt ist zur H\u00e4lfte kaum des Menschen Wahl,", "tokens": ["Ihr", "In\u00b7halt", "ist", "zur", "H\u00e4lf\u00b7te", "kaum", "des", "Men\u00b7schen", "Wahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPRART", "NN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die andre ruht in ewiger M\u00e4chte Liebesrat.", "tokens": ["Die", "and\u00b7re", "ruht", "in", "e\u00b7wi\u00b7ger", "M\u00e4ch\u00b7te", "Lie\u00b7bes\u00b7rat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Wenn du an des Geliebten Seite k\u00fcnftighin", "tokens": ["Wenn", "du", "an", "des", "Ge\u00b7lieb\u00b7ten", "Sei\u00b7te", "k\u00fcnf\u00b7tig\u00b7hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "NN", "NE"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Des heutigen Fests Ged\u00e4chtnis ohne uns begehst,", "tokens": ["Des", "heu\u00b7ti\u00b7gen", "Fests", "Ge\u00b7d\u00e4cht\u00b7nis", "oh\u00b7ne", "uns", "be\u00b7gehst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Wenn ihr in diesen g\u00e4stereichen, heitern Saal", "tokens": ["Wenn", "ihr", "in", "die\u00b7sen", "g\u00e4s\u00b7te\u00b7rei\u00b7chen", ",", "hei\u00b7tern", "Saal"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Euch einmal wieder ganz versetzt im Geist, und all", "tokens": ["Euch", "ein\u00b7mal", "wie\u00b7der", "ganz", "ver\u00b7setzt", "im", "Geist", ",", "und", "all"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "ADV", "ADV", "VVPP", "APPRART", "NN", "$,", "KON", "PIAT"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die freundlichen Gesichter hier sich neu vor euch", "tokens": ["Die", "freund\u00b7li\u00b7chen", "Ge\u00b7sich\u00b7ter", "hier", "sich", "neu", "vor", "euch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "PRF", "ADJD", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Beleben zwischen Blumenschmuck und Gl\u00e4serklang:", "tokens": ["Be\u00b7le\u00b7ben", "zwi\u00b7schen", "Blu\u00b7men\u00b7schmuck", "und", "Gl\u00e4\u00b7ser\u00b7klang", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Dann la\u00df zur stillen Abendstunde kerzenhell", "tokens": ["Dann", "la\u00df", "zur", "stil\u00b7len", "A\u00b7bends\u00b7tun\u00b7de", "ker\u00b7zen\u00b7hell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iam