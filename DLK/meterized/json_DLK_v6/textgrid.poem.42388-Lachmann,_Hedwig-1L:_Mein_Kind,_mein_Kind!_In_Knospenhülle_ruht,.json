{"textgrid.poem.42388": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Kind, mein Kind! In Knospenh\u00fclle ruht,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Kind, mein Kind! In Knospenh\u00fclle ruht,", "tokens": ["Mein", "Kind", ",", "mein", "Kind", "!", "In", "Knos\u00b7pen\u00b7h\u00fcl\u00b7le", "ruht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$.", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem Sorgenblick entzogen, noch dein Los;", "tokens": ["Dem", "Sor\u00b7gen\u00b7blick", "ent\u00b7zo\u00b7gen", ",", "noch", "dein", "Los", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du lebst von meiner Achtsamkeit und Hut.", "tokens": ["Du", "lebst", "von", "mei\u00b7ner", "Acht\u00b7sam\u00b7keit", "und", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mein Kind, mein Kind! Du wirst nun langsam gross", "tokens": ["Mein", "Kind", ",", "mein", "Kind", "!", "Du", "wirst", "nun", "lang\u00b7sam", "gross"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "ADV", "ADJD", "ADJA"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und tust erstaunte Blicke in die Welt;", "tokens": ["Und", "tust", "er\u00b7staun\u00b7te", "Bli\u00b7cke", "in", "die", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "L\u00e4ngst zieht es dich nicht mehr nach meinem Schoss.", "tokens": ["L\u00e4ngst", "zieht", "es", "dich", "nicht", "mehr", "nach", "mei\u00b7nem", "Schoss", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mein Kind, du bist schon auf dich selbst gestellt;", "tokens": ["Mein", "Kind", ",", "du", "bist", "schon", "auf", "dich", "selbst", "ge\u00b7stellt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "ADV", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gleich einem Strom gewinnst du eignen Lauf", "tokens": ["Gleich", "ei\u00b7nem", "Strom", "ge\u00b7winnst", "du", "eig\u00b7nen", "Lauf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu Fernen, die kein Lichtstreif noch erhellt.", "tokens": ["Zu", "Fer\u00b7nen", ",", "die", "kein", "Licht\u00b7streif", "noch", "er\u00b7hellt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "PRELS", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mein Kind, ein neues Volk wuchs mit dir auf!", "tokens": ["Mein", "Kind", ",", "ein", "neu\u00b7es", "Volk", "wuchs", "mit", "dir", "auf", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wird, was wir nur geahnt, in euch Gestalt?", "tokens": ["Wird", ",", "was", "wir", "nur", "ge\u00b7ahnt", ",", "in", "euch", "Ge\u00b7stalt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zieht, was wir nur ersehnt, mit euch herauf?", "tokens": ["Zieht", ",", "was", "wir", "nur", "er\u00b7sehnt", ",", "mit", "euch", "her\u00b7auf", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Mein Kind, die Zeit zerstob, ich werde alt:", "tokens": ["Mein", "Kind", ",", "die", "Zeit", "zer\u00b7stob", ",", "ich", "wer\u00b7de", "alt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wird eurer Kinder Zukunft anders sein,", "tokens": ["Wird", "eu\u00b7rer", "Kin\u00b7der", "Zu\u00b7kunft", "an\u00b7ders", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn unsre bangen Stimmen l\u00e4ngst verhallt?", "tokens": ["Wenn", "uns\u00b7re", "ban\u00b7gen", "Stim\u00b7men", "l\u00e4ngst", "ver\u00b7hallt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wo M\u00e4nner knirschten, werdet ihr befrein?", "tokens": ["Wo", "M\u00e4n\u00b7ner", "knirschten", ",", "wer\u00b7det", "ihr", "be\u00b7fr\u00b7ein", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "$,", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+----+-+-", "measure": "dactylic.init"}}}}}