{"textgrid.poem.60433": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieben Jahre sind verflossen,", "tokens": ["Sie\u00b7ben", "Jah\u00b7re", "sind", "ver\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spurlos wie die Flut ins Meer,", "tokens": ["Spur\u00b7los", "wie", "die", "Flut", "ins", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seit der Ehbund ward geschlossen,", "tokens": ["Seit", "der", "Eh\u00b7bund", "ward", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heute ist die Jahreskehr.", "tokens": ["Heu\u00b7te", "ist", "die", "Jah\u00b7res\u00b7kehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Anna wird im Land besungen", "tokens": ["An\u00b7na", "wird", "im", "Land", "be\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als die allersch\u00f6nste Frau;", "tokens": ["Als", "die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Frau", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie empf\u00e4ngt die Huldigungen,", "tokens": ["Sie", "emp\u00b7f\u00e4ngt", "die", "Hul\u00b7di\u00b7gun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie die Rose ihren Tau.", "tokens": ["Wie", "die", "Ro\u00b7se", "ih\u00b7ren", "Tau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Keines von den s\u00fc\u00dfen Liedern", "tokens": ["Kei\u00b7nes", "von", "den", "s\u00fc\u00b7\u00dfen", "Lie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mag ein Blick ger\u00fchrter Huld,", "tokens": ["Mag", "ein", "Blick", "ge\u00b7r\u00fchr\u00b7ter", "Huld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mag ein s\u00fc\u00dfes Wort erwidern;", "tokens": ["Mag", "ein", "s\u00fc\u00b7\u00dfes", "Wort", "er\u00b7wi\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Anna tr\u00e4gt nur ", "tokens": ["An\u00b7na", "tr\u00e4gt", "nur"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVFIN", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Oftmals bei geschlo\u00dfnem Riegel", "tokens": ["Oft\u00b7mals", "bei", "ge\u00b7schlo\u00df\u00b7nem", "Rie\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist sie unbelauscht allein,", "tokens": ["Ist", "sie", "un\u00b7be\u00b7lauscht", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "St\u00fcrzt ihr Aug sich in den Spiegel,", "tokens": ["St\u00fcrzt", "ihr", "Aug", "sich", "in", "den", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwelgt in ihrem Widerschein.", "tokens": ["Schwelgt", "in", "ih\u00b7rem", "Wi\u00b7der\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Gerne mag sich Anna zieren,", "tokens": ["Ger\u00b7ne", "mag", "sich", "An\u00b7na", "zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reich geschm\u00fcckt am Spiegel stehn;", "tokens": ["Reich", "ge\u00b7schm\u00fcckt", "am", "Spie\u00b7gel", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis sie f\u00fchlt geheimes Frieren,", "tokens": ["Bis", "sie", "f\u00fchlt", "ge\u00b7hei\u00b7mes", "Frie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn sie lang hineingesehn.", "tokens": ["Wenn", "sie", "lang", "hin\u00b7ein\u00b7ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Klirrt und rauscht dann Gold und Seide,", "tokens": ["Klirrt", "und", "rauscht", "dann", "Gold", "und", "Sei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "ADV", "NN", "KON", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "D\u00fcnkt ihr oft, es werde wach", "tokens": ["D\u00fcnkt", "ihr", "oft", ",", "es", "wer\u00b7de", "wach"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jener bange Laut der Heide,", "tokens": ["Je\u00b7ner", "ban\u00b7ge", "Laut", "der", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der manchmal ihr wehte nach.", "tokens": ["Der", "manch\u00b7mal", "ihr", "weh\u00b7te", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Anna ist so sch\u00f6n geblieben,", "tokens": ["An\u00b7na", "ist", "so", "sch\u00f6n", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie als Braut einst am Altar;", "tokens": ["Wie", "als", "Braut", "einst", "am", "Al\u00b7tar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "KOUS", "NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Erich trauert, da\u00df sein Lieben", "tokens": ["E\u00b7rich", "trau\u00b7ert", ",", "da\u00df", "sein", "Lie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "KOUS", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sein Leben unfruchtbar.", "tokens": ["Und", "sein", "Le\u00b7ben", "un\u00b7frucht\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Schweigend reiten sie zum Schlosse", "tokens": ["Schwei\u00b7gend", "rei\u00b7ten", "sie", "zum", "Schlos\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heim von einer Kindestauf;", "tokens": ["Heim", "von", "ei\u00b7ner", "Kin\u00b7de\u00b7stauf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als ihr leuchtender Genosse", "tokens": ["Als", "ihr", "leuch\u00b7ten\u00b7der", "Ge\u00b7nos\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zieht der volle Mond herauf.", "tokens": ["Zieht", "der", "vol\u00b7le", "Mond", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Erich reitet in Gedanken", "tokens": ["E\u00b7rich", "rei\u00b7tet", "in", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hinter seinem Weibe fort,", "tokens": ["Hin\u00b7ter", "sei\u00b7nem", "Wei\u00b7be", "fort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieht des Waldes Schatten wanken", "tokens": ["Sieht", "des", "Wal\u00b7des", "Schat\u00b7ten", "wan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unst\u00e4t wechselnd hier und dort.", "tokens": ["Un\u00b7st\u00e4t", "wech\u00b7selnd", "hier", "und", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "KON", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Als sie weiter traben beide,", "tokens": ["Als", "sie", "wei\u00b7ter", "tra\u00b7ben", "bei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In Gedanken, ohne Laut,", "tokens": ["In", "Ge\u00b7dan\u00b7ken", ",", "oh\u00b7ne", "Laut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "APPR", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als sie kommen auf die Heide,", "tokens": ["Als", "sie", "kom\u00b7men", "auf", "die", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo sie einst geirrt als Braut:", "tokens": ["Wo", "sie", "einst", "ge\u00b7irrt", "als", "Braut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVPP", "KOKOM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Sieht er ihres Pferdes Schatten", "tokens": ["Sieht", "er", "ih\u00b7res", "Pfer\u00b7des", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um die Reiterin verk\u00fcrzt,", "tokens": ["Um", "die", "Rei\u00b7te\u00b7rin", "ver\u00b7k\u00fcrzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das Bild erschreckt den Gatten,", "tokens": ["Und", "das", "Bild", "er\u00b7schreckt", "den", "Gat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob sein Weib vom Ro\u00df gest\u00fcrzt?", "tokens": ["Ob", "sein", "Weib", "vom", "Ro\u00df", "ge\u00b7st\u00fcrzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Nein, sie sitzt! \u00bbGott sei uns gn\u00e4dig!\u00ab", "tokens": ["Nein", ",", "sie", "sitzt", "!", "\u00bb", "Gott", "sei", "uns", "gn\u00e4\u00b7dig", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "$.", "$(", "NN", "VAFIN", "PPER", "ADJD", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruft er aus \u2013 \u00bbVerfluchtes Weib!", "tokens": ["Ruft", "er", "aus", "\u2013", "\u00bb", "Ver\u00b7fluch\u00b7tes", "Weib", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$(", "$(", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur dein Ro\u00df, als ging' es ledig,", "tokens": ["Nur", "dein", "Ro\u00df", ",", "als", "ging'", "es", "le\u00b7dig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "KOUS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keinen Schatten wirft dein Leib!\u00ab", "tokens": ["Kei\u00b7nen", "Schat\u00b7ten", "wirft", "dein", "Leib", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Aber Anna treibt den Zelter,", "tokens": ["A\u00b7ber", "An\u00b7na", "treibt", "den", "Zel\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zitternd vor dem Mondenstrahl,", "tokens": ["Zit\u00b7ternd", "vor", "dem", "Mon\u00b7dens\u00b7trahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vor dem himmlischen Vergelter,", "tokens": ["Vor", "dem", "himm\u00b7li\u00b7schen", "Ver\u00b7gel\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dem z\u00fcrnenden Gemahl.", "tokens": ["Und", "dem", "z\u00fcr\u00b7nen\u00b7den", "Ge\u00b7mahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Jetzo st\u00fcrzt sie bang zu F\u00fc\u00dfen", "tokens": ["Jet\u00b7zo", "st\u00fcrzt", "sie", "bang", "zu", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihrem Herrn im Schlafgemach,", "tokens": ["Ih\u00b7rem", "Herrn", "im", "Schlaf\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie bekennt in Tr\u00e4neng\u00fcssen,", "tokens": ["Sie", "be\u00b7kennt", "in", "Tr\u00e4\u00b7nen\u00b7g\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flehend, was sie einst verbrach.", "tokens": ["Fle\u00b7hend", ",", "was", "sie", "einst", "ver\u00b7brach", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Schaudernd h\u00f6rte er ihre Kunde;", "tokens": ["Schau\u00b7dernd", "h\u00f6r\u00b7te", "er", "ih\u00b7re", "Kun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "S\u00fc\u00dfer sonst als Blumenduft,", "tokens": ["S\u00fc\u00b7\u00dfer", "sonst", "als", "Blu\u00b7men\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KOUS", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trifft der Hauch aus ihrem Munde