{"textgrid.poem.67112": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Einladung an Neuffer", "genre": "verse", "period": "N.A.", "pub_year": 1791, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Morgen, Bruder, ging so sch\u00f6n hervor,", "tokens": ["Dein", "Mor\u00b7gen", ",", "Bru\u00b7der", ",", "ging", "so", "sch\u00f6n", "her\u00b7vor", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "VVFIN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So herrlich schimmerte dein Morgenrot \u2013", "tokens": ["So", "herr\u00b7lich", "schim\u00b7mer\u00b7te", "dein", "Mor\u00b7gen\u00b7rot", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und doch \u2013 und doch besiegt ein schwarzer Sturm", "tokens": ["Und", "doch", "\u2013", "und", "doch", "be\u00b7siegt", "ein", "schwar\u00b7zer", "Sturm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$(", "KON", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das hehre Licht \u2013 und w\u00e4lzet schreckenvoll", "tokens": ["Das", "heh\u00b7re", "Licht", "\u2013", "und", "w\u00e4l\u00b7zet", "schre\u00b7cken\u00b7voll"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Den grimmen Donner auf dein sichres Haupt!", "tokens": ["Den", "grim\u00b7men", "Don\u00b7ner", "auf", "dein", "sich\u00b7res", "Haupt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "O Bruder! Bruder! da\u00df dein Bild so wahr,", "tokens": ["O", "Bru\u00b7der", "!", "Bru\u00b7der", "!", "da\u00df", "dein", "Bild", "so", "wahr", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "NN", "$.", "KOUS", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So schrecklich wahr des Lebens Wechsel deutet!", "tokens": ["So", "schreck\u00b7lich", "wahr", "des", "Le\u00b7bens", "Wech\u00b7sel", "deu\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df Disteln hinter Blumeng\u00e4ngen lauern \u2013", "tokens": ["Da\u00df", "Dis\u00b7teln", "hin\u00b7ter", "Blu\u00b7men\u00b7g\u00e4n\u00b7gen", "lau\u00b7ern", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und Jammer auf die Rosenwange schielt!", "tokens": ["Und", "Jam\u00b7mer", "auf", "die", "Ro\u00b7sen\u00b7wan\u00b7ge", "schielt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und bleicher Tod in J\u00fcnglingsadern schleicht,", "tokens": ["Und", "blei\u00b7cher", "Tod", "in", "J\u00fcng\u00b7lin\u00b7gsa\u00b7dern", "schleicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und bange Trennung treuer Freunde Los", "tokens": ["Und", "ban\u00b7ge", "Tren\u00b7nung", "treu\u00b7er", "Freun\u00b7de", "Los"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "NN", "ADJA", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und edler Herzen Schicksal Druck und Kummer ist!", "tokens": ["Und", "ed\u00b7ler", "Her\u00b7zen", "Schick\u00b7sal", "Druck", "und", "Kum\u00b7mer", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da baun wir Plane, tr\u00e4umen so entz\u00fcckt", "tokens": ["Da", "baun", "wir", "Pla\u00b7ne", ",", "tr\u00e4u\u00b7men", "so", "ent\u00b7z\u00fcckt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "VVFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Vom nahen Ziel \u2013 und pl\u00f6tzlich, pl\u00f6tzlich zuckt", "tokens": ["Vom", "na\u00b7hen", "Ziel", "\u2013", "und", "pl\u00f6tz\u00b7lich", ",", "pl\u00f6tz\u00b7lich", "zuckt"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$(", "KON", "ADJD", "$,", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Ein Blitz herab, und \u00f6ffnet uns die Augen!", "tokens": ["Ein", "Blitz", "her\u00b7ab", ",", "und", "\u00f6ff\u00b7net", "uns", "die", "Au\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Du fr\u00e4gst, warum dies all? \u2013 aus heller Laune.", "tokens": ["Du", "fr\u00e4gst", ",", "wa\u00b7rum", "dies", "all", "?", "\u2013", "aus", "hel\u00b7ler", "Lau\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PDS", "PIAT", "$.", "$(", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Ich sah im Geist sich deine Stirne w\u00f6lken,", "tokens": ["Ich", "sah", "im", "Geist", "sich", "dei\u00b7ne", "Stir\u00b7ne", "w\u00f6l\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PRF", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "In deiner Eingezogenheit \u2013 da ging", "tokens": ["In", "dei\u00b7ner", "Ein\u00b7ge\u00b7zo\u00b7gen\u00b7heit", "\u2013", "da", "ging"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$(", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Ich tr\u00fcben Blicks hinab zu meinem Neckar", "tokens": ["Ich", "tr\u00fc\u00b7ben", "Blicks", "hin\u00b7ab", "zu", "mei\u00b7nem", "Ne\u00b7ckar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Und sah in seine Wogen, bis mir schwindelte \u2013", "tokens": ["Und", "sah", "in", "sei\u00b7ne", "Wo\u00b7gen", ",", "bis", "mir", "schwin\u00b7del\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "KOUS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und kehrte still und voll der dunklen Zukunft,", "tokens": ["Und", "kehr\u00b7te", "still", "und", "voll", "der", "dunk\u00b7len", "Zu\u00b7kunft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Und voll des Schicksals, welches unsrer wartet,", "tokens": ["Und", "voll", "des", "Schick\u00b7sals", ",", "wel\u00b7ches", "uns\u00b7rer", "war\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Zur\u00fcck \u2013 und setzte mich, und also ward", "tokens": ["Zu\u00b7r\u00fcck", "\u2013", "und", "setz\u00b7te", "mich", ",", "und", "al\u00b7so", "ward"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKVZ", "$(", "KON", "VVFIN", "PPER", "$,", "KON", "ADV", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Die \u2013 freilich nicht erbauliche \u2013 Tirade", "tokens": ["Die", "\u2013", "frei\u00b7lich", "nicht", "er\u00b7bau\u00b7li\u00b7che", "\u2013", "Ti\u00b7ra\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "$(", "ADV", "PTKNEG", "ADJA", "$(", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Vom ungewissen Wechsel unsers Lebens.", "tokens": ["Vom", "un\u00b7ge\u00b7wis\u00b7sen", "Wech\u00b7sel", "un\u00b7sers", "Le\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Doch \u2013 komme du \u2013 du scherze mir Tiraden", "tokens": ["Doch", "\u2013", "kom\u00b7me", "du", "\u2013", "du", "scher\u00b7ze", "mir", "Ti\u00b7ra\u00b7den"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$(", "VVFIN", "PPER", "$(", "PPER", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Und Ahndungen der Zukunft von der Stirne weg,", "tokens": ["Und", "Ahn\u00b7dun\u00b7gen", "der", "Zu\u00b7kunft", "von", "der", "Stir\u00b7ne", "weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "O komm \u2013 es harret dein ein ", "tokens": ["O", "komm", "\u2013", "es", "har\u00b7ret", "dein", "ein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$(", "PPER", "VVFIN", "PPOSAT", "ART"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.29": {"text": "Stiefm\u00fctterlich soll wahrlich nicht mein F\u00e4\u00dfchen sein.", "tokens": ["Stief\u00b7m\u00fct\u00b7ter\u00b7lich", "soll", "wahr\u00b7lich", "nicht", "mein", "F\u00e4\u00df\u00b7chen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "ADV", "PTKNEG", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und findst du schon kein St\u00e4dtermahl, so w\u00fcrzet es", "tokens": ["Und", "findst", "du", "schon", "kein", "St\u00e4d\u00b7ter\u00b7mahl", ",", "so", "w\u00fcr\u00b7zet", "es"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Doch meine Freundschaft, und der Meinen guter Wille.", "tokens": ["Doch", "mei\u00b7ne", "Freund\u00b7schaft", ",", "und", "der", "Mei\u00b7nen", "gu\u00b7ter", "Wil\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}