{"textgrid.poem.37488": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Lohn des Flei\u00dfes", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbkomm Nero!\u00ab spricht Herr Bartel ernst,", "tokens": ["\u00bb", "komm", "Ne\u00b7ro", "!", "\u00ab", "spricht", "Herr", "Bar\u00b7tel", "ernst", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "NE", "$.", "$(", "VVFIN", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbes wird jetzt Zeit, da\u00df du was lernst!", "tokens": ["\u00bb", "es", "wird", "jetzt", "Zeit", ",", "da\u00df", "du", "was", "lernst", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "NN", "$,", "KOUS", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du willst nicht? \u2013 Gut! so hau' ich dich", "tokens": ["Du", "willst", "nicht", "?", "\u2013", "Gut", "!", "so", "hau'", "ich", "dich"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "$.", "$(", "ADJD", "$.", "ADV", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit einem Stecken f\u00fcrchterlich.\u00ab", "tokens": ["Mit", "ei\u00b7nem", "Ste\u00b7cken", "f\u00fcrch\u00b7ter\u00b7lich", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Drauf sitzt der Nero m\u00e4uschenstill", "tokens": ["Drauf", "sitzt", "der", "Ne\u00b7ro", "m\u00e4u\u00b7schen\u00b7still"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und h\u00f6rt, was man ihm sagen will.", "tokens": ["Und", "h\u00f6rt", ",", "was", "man", "ihm", "sa\u00b7gen", "will", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "PIS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbhut ab!\u00ab das ist das erste St\u00fcck,", "tokens": ["\u00bb", "hut", "ab", "!", "\u00ab", "das", "ist", "das", "ers\u00b7te", "St\u00fcck", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$.", "$(", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Nero macht es mit Geschick.", "tokens": ["Der", "Ne\u00b7ro", "macht", "es", "mit", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Zum zweiten: \u00bbJenen Stecken dort!\u00ab", "tokens": ["Zum", "zwei\u00b7ten", ":", "\u00bb", "Je\u00b7nen", "Ste\u00b7cken", "dort", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "$.", "$(", "PDAT", "NN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur munter, Nero! such! apport!\u00ab", "tokens": ["Nur", "mun\u00b7ter", ",", "Ne\u00b7ro", "!", "such", "!", "ap\u00b7port", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "$,", "NE", "$.", "VVFIN", "$.", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und jetzt: \u00bbDie T\u00fcr auf! \u2013 So, so, so!", "tokens": ["Und", "jetzt", ":", "\u00bb", "Die", "T\u00fcr", "auf", "!", "\u2013", "So", ",", "so", ",", "so", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "$.", "$(", "ART", "NN", "PTKVZ", "$.", "$(", "ADV", "$,", "ADV", "$,", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das geht ja schon! Bravissimo!\u00ab", "tokens": ["Das", "geht", "ja", "schon", "!", "Bra\u00b7vis\u00b7si\u00b7mo", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "$.", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbach!\u00ab denkt der Nero, \u00bbach, wozu", "tokens": ["\u00bb", "ach", "!", "\u00ab", "denkt", "der", "Ne\u00b7ro", ",", "\u00bb", "ach", ",", "wo\u00b7zu"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "word"], "pos": ["$(", "ITJ", "$.", "$(", "VVFIN", "ART", "NE", "$,", "$(", "ITJ", "$,", "PWAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "L\u00e4\u00dft mich mein Herr doch nicht in Ruh'?!\u00ab", "tokens": ["L\u00e4\u00dft", "mich", "mein", "Herr", "doch", "nicht", "in", "Ruh'", "?!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADV", "PTKNEG", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Da kommt, als sie spazierengingen,", "tokens": ["Da", "kommt", ",", "als", "sie", "spa\u00b7zie\u00b7ren\u00b7gin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Hundef\u00e4nger mit der Schlingen.", "tokens": ["Der", "Hun\u00b7de\u00b7f\u00e4n\u00b7ger", "mit", "der", "Schlin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "\u00bbhut ab!\u00ab ruft schnell Herr Bartel jetzt,", "tokens": ["\u00bb", "hut", "ab", "!", "\u00ab", "ruft", "schnell", "Herr", "Bar\u00b7tel", "jetzt", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$.", "$(", "VVFIN", "ADJD", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Hundef\u00e4nger ist entsetzt.", "tokens": ["Der", "Hun\u00b7de\u00b7f\u00e4n\u00b7ger", "ist", "ent\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Und l\u00e4\u00dft, dieweil der Schreck so gro\u00df,", "tokens": ["Und", "l\u00e4\u00dft", ",", "die\u00b7weil", "der", "Schreck", "so", "gro\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die festgemachte Schlinge los.", "tokens": ["Die", "fest\u00b7ge\u00b7mach\u00b7te", "Schlin\u00b7ge", "los", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Gleich sitzt der Nero mit der M\u00fctze", "tokens": ["Gleich", "sitzt", "der", "Ne\u00b7ro", "mit", "der", "M\u00fct\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In einer tiefen Wasserpf\u00fctze.", "tokens": ["In", "ei\u00b7ner", "tie\u00b7fen", "Was\u00b7ser\u00b7pf\u00fct\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Der b\u00f6se Mann, gar sehr gewandt,", "tokens": ["Der", "b\u00f6\u00b7se", "Mann", ",", "gar", "sehr", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fischt aber Nero an das Land", "tokens": ["Fischt", "a\u00b7ber", "Ne\u00b7ro", "an", "das", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Und sperrt ihn in den Gitterkasten", "tokens": ["Und", "sperrt", "ihn", "in", "den", "Git\u00b7ter\u00b7kas\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und schreit: \u00bbJetzt soll der Schlingel fasten!\u00ab", "tokens": ["Und", "schreit", ":", "\u00bb", "Jetzt", "soll", "der", "Schlin\u00b7gel", "fas\u00b7ten", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "ADV", "VMFIN", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Doch kaum hat sich der Mann entfernt,", "tokens": ["Doch", "kaum", "hat", "sich", "der", "Mann", "ent\u00b7fernt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zeigt Nero, da\u00df er was gelernt.", "tokens": ["Zeigt", "Ne\u00b7ro", ",", "da\u00df", "er", "was", "ge\u00b7lernt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "KOUS", "PPER", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Er macht die T\u00fcre auf, und dann", "tokens": ["Er", "macht", "die", "T\u00fc\u00b7re", "auf", ",", "und", "dann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "L\u00e4uft er nach Haus, so schnell er kann.", "tokens": ["L\u00e4uft", "er", "nach", "Haus", ",", "so", "schnell", "er", "kann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,", "ADV", "ADJD", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Hier kehrt er heim und ist erfreut,", "tokens": ["Hier", "kehrt", "er", "heim", "und", "ist", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das macht allein die Flei\u00dfigkeit.", "tokens": ["Das", "macht", "al\u00b7lein", "die", "Flei\u00b7\u00dfig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}