{"dta.poem.20701": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  Herrn von G.  \n den Officier und Dichter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Cyntia l\u00e4chelt uns zu,", "tokens": ["Cyn\u00b7tia", "l\u00e4\u00b7chelt", "uns", "zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Dir vor allen, Apollischer Sohn,", "tokens": ["Dir", "vor", "al\u00b7len", ",", "A\u00b7pol\u00b7li\u00b7scher", "Sohn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "APPR", "PIAT", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Denn sie w\u00e4hnt, Du seyst Endymion,", "tokens": ["Denn", "sie", "w\u00e4hnt", ",", "Du", "seyst", "En\u00b7dy\u00b7mi\u00b7on", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Er war nicht so angenehm als Du \u2014", "tokens": ["Er", "war", "nicht", "so", "an\u00b7ge\u00b7nehm", "als", "Du"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "ADJD", "KOKOM", "PPER", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Dies schwazt' ich gestern, Corillas!                 ", "tokens": ["Dies", "schwazt'", "ich", "ge\u00b7stern", ",", "Co\u00b7ril\u00b7las", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVINF", "$,", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und habe nicht gelogen;", "tokens": ["Und", "ha\u00b7be", "nicht", "ge\u00b7lo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des Tagegottes Schwester sa\u00df", "tokens": ["Des", "Ta\u00b7ge\u00b7got\u00b7tes", "Schwes\u00b7ter", "sa\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei weggelegtem Bogen", "tokens": ["Bei", "weg\u00b7ge\u00b7leg\u00b7tem", "Bo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auf dem Olympus in dem Krei\u00df", "tokens": ["Auf", "dem", "O\u00b7lym\u00b7pus", "in", "dem", "Krei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NE", "APPR", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Der dienstbestellten Nymphen,", "tokens": ["Der", "dienst\u00b7be\u00b7stell\u00b7ten", "Nym\u00b7phen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Und ihre Stirne silberwei\u00df", "tokens": ["Und", "ih\u00b7re", "Stir\u00b7ne", "sil\u00b7ber\u00b7wei\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die fing sich an zu r\u00fcmpfen,", "tokens": ["Die", "fing", "sich", "an", "zu", "r\u00fcmp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APZR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ihr sch\u00f6nes Auge zog sich klein,", "tokens": ["Ihr", "sch\u00f6\u00b7nes", "Au\u00b7ge", "zog", "sich", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wie sich ein Auge ziehet,", "tokens": ["Wie", "sich", "ein", "Au\u00b7ge", "zie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Wenn man, um recht gewi\u00df zu seyn,", "tokens": ["Wenn", "man", ",", "um", "recht", "ge\u00b7wi\u00df", "zu", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "KOUI", "ADV", "ADV", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Tief in die Ferne siehet,", "tokens": ["Tief", "in", "die", "Fer\u00b7ne", "sie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.13": {"text": "Wo etwas wankt, Mensch oder Thier \u2014", "tokens": ["Wo", "et\u00b7was", "wankt", ",", "Mensch", "o\u00b7der", "Thier"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "$,", "NN", "KON", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.14": {"text": "Sie wollte Dich erkennen,", "tokens": ["Sie", "woll\u00b7te", "Dich", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Und fing voll inniger Begier", "tokens": ["Und", "fing", "voll", "in\u00b7ni\u00b7ger", "Be\u00b7gier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ADJA", "NN"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.16": {"text": "Im Busen an zu brennen,", "tokens": ["Im", "Bu\u00b7sen", "an", "zu", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Und seufzte laut: Endymion! \u2014", "tokens": ["Und", "seufz\u00b7te", "laut", ":", "En\u00b7dy\u00b7mi\u00b7on", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$.", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du hast es nicht vernommen,", "tokens": ["Du", "hast", "es", "nicht", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir aber ist der Seufzerton", "tokens": ["Mir", "a\u00b7ber", "ist", "der", "Seuf\u00b7zer\u00b7ton"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sehr kl\u00e4glich vorgekommen,", "tokens": ["Sehr", "kl\u00e4g\u00b7lich", "vor\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Denn ich gab auf die G\u00f6ttinn Acht.", "tokens": ["Denn", "ich", "gab", "auf", "die", "G\u00f6t\u00b7tinn", "Acht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "CARD", "$."], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Sie ward, indem wir gingen,", "tokens": ["Sie", "ward", ",", "in\u00b7dem", "wir", "gin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im Antliz feuerroth gemacht,", "tokens": ["Im", "Ant\u00b7liz", "feu\u00b7er\u00b7roth", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das Herz wollt ihr zerspringen", "tokens": ["Das", "Herz", "wollt", "ihr", "zer\u00b7sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vor Schaam und Reue, da\u00df sie Dich", "tokens": ["Vor", "Schaam", "und", "Reu\u00b7e", ",", "da\u00df", "sie", "Dich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "KOUS", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Gesehen und verkannte", "tokens": ["Ge\u00b7se\u00b7hen", "und", "ver\u00b7kann\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Und mit Endymion verglich \u2014", "tokens": ["Und", "mit", "En\u00b7dy\u00b7mi\u00b7on", "ver\u00b7glich"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADJD", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Und eine Nymphe nannte,", "tokens": ["Und", "ei\u00b7ne", "Nym\u00b7phe", "nann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Zu desto gr\u00f6\u00dferen Verdru\u00df", "tokens": ["Zu", "des\u00b7to", "gr\u00f6\u00b7\u00dfe\u00b7ren", "Ver\u00b7dru\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ihr schalkhaft Deine Minne,", "tokens": ["Ihr", "schalk\u00b7haft", "Dei\u00b7ne", "Min\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Da go\u00df des Schamroths Ueberflu\u00df", "tokens": ["Da", "go\u00df", "des", "Scham\u00b7roths", "Ue\u00b7berf\u00b7lu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Urpl\u00f6tzlich von dem Kinne", "tokens": ["Ur\u00b7pl\u00f6tz\u00b7lich", "von", "dem", "Kin\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Sich auf der G\u00f6ttinn Busenraum;", "tokens": ["Sich", "auf", "der", "G\u00f6t\u00b7tinn", "Bu\u00b7sen\u00b7raum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Denn ach die Nymphe sagte,", "tokens": ["Denn", "ach", "die", "Nym\u00b7phe", "sag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Da\u00df Amors Mutter selber kaum", "tokens": ["Da\u00df", "A\u00b7mors", "Mut\u00b7ter", "sel\u00b7ber", "kaum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Sich zu vergleichen wagte", "tokens": ["Sich", "zu", "ver\u00b7glei\u00b7chen", "wag\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "PTKZU", "VVINF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Mit Deiner sch\u00f6nen Sch\u00e4ferinn,", "tokens": ["Mit", "Dei\u00b7ner", "sch\u00f6\u00b7nen", "Sch\u00e4\u00b7fe\u00b7rinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Die blond wie Ceres w\u00e4re,", "tokens": ["Die", "blond", "wie", "Ce\u00b7res", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KOKOM", "NE", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Da sank das Haupt der Luna hin,", "tokens": ["Da", "sank", "das", "Haupt", "der", "Lu\u00b7na", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Und eine bittre Z\u00e4hre", "tokens": ["Und", "ei\u00b7ne", "bitt\u00b7re", "Z\u00e4h\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Flo\u00df zitternd auf ihr Purpurpf\u00fchl,", "tokens": ["Flo\u00df", "zit\u00b7ternd", "auf", "ihr", "Pur\u00b7pur\u00b7pf\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Bis Morpheus sich erbarmte,", "tokens": ["Bis", "Mor\u00b7pheus", "sich", "er\u00b7barm\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Bis durch sein s\u00fc\u00dfes Gaukelspiel", "tokens": ["Bis", "durch", "sein", "s\u00fc\u00b7\u00dfes", "Gau\u00b7kel\u00b7spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Ihr Sch\u00e4fer sie umarmte.", "tokens": ["Ihr", "Sch\u00e4\u00b7fer", "sie", "um\u00b7arm\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}