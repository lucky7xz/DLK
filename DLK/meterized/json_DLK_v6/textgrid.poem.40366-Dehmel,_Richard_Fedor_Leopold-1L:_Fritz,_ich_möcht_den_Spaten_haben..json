{"textgrid.poem.40366": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fritz, ich m\u00f6cht den Spaten haben.", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fritz, ich m\u00f6cht den Spaten haben.", "tokens": ["Fritz", ",", "ich", "m\u00f6cht", "den", "Spa\u00b7ten", "ha\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VMFIN", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\"mutterchen, warum?\"", "tokens": ["\"", "mut\u00b7ter\u00b7chen", ",", "wa\u00b7rum", "?", "\""], "token_info": ["punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "VVINF", "$,", "PWAV", "$.", "$("], "meter": "+---+", "measure": "dactylic.init"}, "line.3": {"text": "M\u00f6chte eine Grube graben.", "tokens": ["M\u00f6ch\u00b7te", "ei\u00b7ne", "Gru\u00b7be", "gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\"mutterchen, warum?\"", "tokens": ["\"", "mut\u00b7ter\u00b7chen", ",", "wa\u00b7rum", "?", "\""], "token_info": ["punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "VVINF", "$,", "PWAV", "$.", "$("], "meter": "+---+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "M\u00f6chte drin ein B\u00e4umchen pflanzen.", "tokens": ["M\u00f6ch\u00b7te", "drin", "ein", "B\u00e4um\u00b7chen", "pflan\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\"mutterchen, warum?\"", "tokens": ["\"", "mut\u00b7ter\u00b7chen", ",", "wa\u00b7rum", "?", "\""], "token_info": ["punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "VVINF", "$,", "PWAV", "$.", "$("], "meter": "+---+", "measure": "dactylic.init"}, "line.3": {"text": "Wird mein Fritze drunter tanzen.", "tokens": ["Wird", "mein", "Frit\u00b7ze", "drun\u00b7ter", "tan\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PAV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\"mutterchen, warum?\"", "tokens": ["\"", "mut\u00b7ter\u00b7chen", ",", "wa\u00b7rum", "?", "\""], "token_info": ["punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "VVINF", "$,", "PWAV", "$.", "$("], "meter": "+---+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "\"wird das B\u00e4umchen Kirschen tragen.", "tokens": ["\"", "wird", "das", "B\u00e4um\u00b7chen", "Kir\u00b7schen", "tra\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\"mutterchen, warum?\"", "tokens": ["\"", "mut\u00b7ter\u00b7chen", ",", "wa\u00b7rum", "?", "\""], "token_info": ["punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "VVINF", "$,", "PWAV", "$.", "$("], "meter": "+---+", "measure": "dactylic.init"}, "line.3": {"text": "Ei, du mu\u00dft die Spatzen fragen,", "tokens": ["Ei", ",", "du", "mu\u00dft", "die", "Spat\u00b7zen", "fra\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "die sind nicht so dumm! \u2013", "tokens": ["die", "sind", "nicht", "so", "dumm", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "ADV", "ADJD", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Kommt die kleine Plappertasche:", "tokens": ["Kommt", "die", "klei\u00b7ne", "Plap\u00b7per\u00b7ta\u00b7sche", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\"mutterchen, nicht wahr,", "tokens": ["\"", "mut\u00b7ter\u00b7chen", ",", "nicht", "wahr", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVINF", "$,", "PTKNEG", "ADJD", "$,"], "meter": "+---+", "measure": "dactylic.init"}, "line.3": {"text": "ich bin kl\u00fcger als der Fritze,", "tokens": ["ich", "bin", "kl\u00fc\u00b7ger", "als", "der", "Frit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "bin schon bald sechs Jahr!", "tokens": ["bin", "schon", "bald", "sechs", "Jahr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "CARD", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "\"mutterchen, nicht wahr, der Fritze", "tokens": ["\"", "mut\u00b7ter\u00b7chen", ",", "nicht", "wahr", ",", "der", "Frit\u00b7ze"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "VVINF", "$,", "PTKNEG", "ADJD", "$,", "ART", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "ist ein Schaf, o jee!", "tokens": ["ist", "ein", "Schaf", ",", "o", "jee", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "FM", "FM", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ich kann schon bis zwanzig z\u00e4hlen", "tokens": ["Ich", "kann", "schon", "bis", "zwan\u00b7zig", "z\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "CARD", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "und das A-B-C!\"", "tokens": ["und", "das", "A\u00b7\u00b7B\u00b7\u00b7C", "!", "\""], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "I, du kleine Plappertasche,", "tokens": ["I", ",", "du", "klei\u00b7ne", "Plap\u00b7per\u00b7ta\u00b7sche", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["XY", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "la\u00df den Fritz in Ruh!", "tokens": ["la\u00df", "den", "Fritz", "in", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NE", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Plappertasche, wische wasche,", "tokens": ["Plap\u00b7per\u00b7ta\u00b7sche", ",", "wi\u00b7sche", "wa\u00b7sche", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "halt das M\u00e4ulchen zu!", "tokens": ["halt", "das", "M\u00e4ul\u00b7chen", "zu", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "\u00dcbermorgen in acht Wochen", "tokens": ["\u00dc\u00b7ber\u00b7mor\u00b7gen", "in", "acht", "Wo\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "kommt der Weihnachtsmann;", "tokens": ["kommt", "der", "Weih\u00b7nachts\u00b7mann", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "wenn du dann noch immer plapperst,", "tokens": ["wenn", "du", "dann", "noch", "im\u00b7mer", "plap\u00b7perst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "----+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "was bekommst du dann?", "tokens": ["was", "be\u00b7kommst", "du", "dann", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Einen gro\u00dfen Maulkorb! \u2013", "tokens": ["Ei\u00b7nen", "gro\u00b7\u00dfen", "Maul\u00b7korb", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}