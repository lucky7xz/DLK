{"textgrid.poem.24766": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Antwort gibt im Felde dort,", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Antwort gibt im Felde dort,", "tokens": ["Ant\u00b7wort", "gibt", "im", "Fel\u00b7de", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Faust, die festgeballte,", "tokens": ["Faust", ",", "die", "fest\u00b7ge\u00b7ball\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Antwort dir gibt nur ", "tokens": ["Ant\u00b7wort", "dir", "gibt", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "VVFIN", "ADV"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Jenes Gott erhalte!", "tokens": ["Je\u00b7nes", "Gott", "er\u00b7hal\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Unsern Kindern eint uns dies,", "tokens": ["Un\u00b7sern", "Kin\u00b7dern", "eint", "uns", "dies", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PDS", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie's uns eint den V\u00e4tern,", "tokens": ["Wie's", "uns", "eint", "den", "V\u00e4\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Einet heut die K\u00e4mpferschar", "tokens": ["Ei\u00b7net", "heut", "die", "K\u00e4mp\u00b7fer\u00b7schar"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier mit uns, den Betern.", "tokens": ["Hier", "mit", "uns", ",", "den", "Be\u00b7tern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$,", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Berge sind ein schwacher Wall,", "tokens": ["Ber\u00b7ge", "sind", "ein", "schwa\u00b7cher", "Wall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Haben Kluft und Spalte:", "tokens": ["Ha\u00b7ben", "Kluft", "und", "Spal\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Brust an Brust und Volk bei Volk", "tokens": ["Brust", "an", "Brust", "und", "Volk", "bei", "Volk"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schallt es: Gott erhalte!", "tokens": ["Schallt", "es", ":", "Gott", "er\u00b7hal\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Helden sind wie Kinder schlicht,", "tokens": ["Hel\u00b7den", "sind", "wie", "Kin\u00b7der", "schlicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "KOKOM", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kinder werden Helden,", "tokens": ["Kin\u00b7der", "wer\u00b7den", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Worte nicht und kein Gedicht", "tokens": ["Wor\u00b7te", "nicht", "und", "kein", "Ge\u00b7dicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "KON", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00f6nnens je vermelden.", "tokens": ["K\u00f6n\u00b7nens", "je", "ver\u00b7mel\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Ungeheueres umfa\u00dft", "tokens": ["Un\u00b7ge\u00b7heu\u00b7e\u00b7res", "um\u00b7fa\u00dft"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Heut dies heilig Alte,", "tokens": ["Heut", "dies", "hei\u00b7lig", "Al\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "ADJD", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und so dringts zum Himmel auf:", "tokens": ["Und", "so", "dringts", "zum", "Him\u00b7mel", "auf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unser Gott erhalte!", "tokens": ["Un\u00b7ser", "Gott", "er\u00b7hal\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}