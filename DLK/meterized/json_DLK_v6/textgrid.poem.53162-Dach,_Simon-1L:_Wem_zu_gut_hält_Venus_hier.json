{"textgrid.poem.53162": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wem zu gut h\u00e4lt Venus hier", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wem zu gut h\u00e4lt Venus hier", "tokens": ["Wem", "zu", "gut", "h\u00e4lt", "Ve\u00b7nus", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PTKA", "ADJD", "VVFIN", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In so sch\u00f6ner Pracht und Zier!", "tokens": ["In", "so", "sch\u00f6\u00b7ner", "Pracht", "und", "Zier", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wem doch fliegen umb sie her", "tokens": ["Wem", "doch", "flie\u00b7gen", "umb", "sie", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VVINF", "APPR", "PPER", "APZR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tausent Knaben ohn gefehr,", "tokens": ["Tau\u00b7sent", "Kna\u00b7ben", "ohn", "ge\u00b7fehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Derer leichte Fl\u00fcgel sind", "tokens": ["De\u00b7rer", "leich\u00b7te", "Fl\u00fc\u00b7gel", "sind"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "LiebesGlut und Anmuth-Wind,", "tokens": ["Lie\u00b7bes", "Glut", "und", "An\u00b7muth\u00b7Wind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ihr Gescho\u00df blinckt hell und rein", "tokens": ["Ihr", "Ge\u00b7scho\u00df", "blinckt", "hell", "und", "rein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Nur von Demant-Stein?", "tokens": ["Nur", "von", "De\u00b7mant\u00b7Stein", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Dir, du Adeliches Paar,", "tokens": ["Dir", ",", "du", "A\u00b7de\u00b7li\u00b7ches", "Paar", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir dient diese Liebes-Schaar,", "tokens": ["Dir", "dient", "die\u00b7se", "Lie\u00b7bes\u00b7Schaar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Dieser M\u00e4nge heller Glantz", "tokens": ["Die\u00b7ser", "M\u00e4n\u00b7ge", "hel\u00b7ler", "Glantz"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kr\u00f6net deinen Hochzeit-Tantz:", "tokens": ["Kr\u00f6\u00b7net", "dei\u00b7nen", "Hoch\u00b7zeit\u00b7Tantz", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was selbst Erato hie singt,", "tokens": ["Was", "selbst", "E\u00b7ra\u00b7to", "hie", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NE", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was der Seiten Kunst erzwingt,", "tokens": ["Was", "der", "Sei\u00b7ten", "Kunst", "er\u00b7zwingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Alles, ewer Tritt und Gang,", "tokens": ["Al\u00b7les", ",", "e\u00b7wer", "Tritt", "und", "Gang", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist nur Liebes-Fang.", "tokens": ["Ist", "nur", "Lie\u00b7bes\u00b7Fang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wir beschwehren Amors Pracht,", "tokens": ["Wir", "be\u00b7schweh\u00b7ren", "A\u00b7mors", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seiner Pfeil' und Herrschafft Macht,", "tokens": ["Sei\u00b7ner", "Pfeil'", "und", "Herr\u00b7schafft", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df er sag', ob eine Zeit", "tokens": ["Da\u00df", "er", "sag'", ",", "ob", "ei\u00b7ne", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein gew\u00fcnschters Paar gefreyt,", "tokens": ["Ein", "ge\u00b7w\u00fcnschters", "Paar", "ge\u00b7freyt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Das an Hertzen, Stand' und Trew", "tokens": ["Das", "an", "Hert\u00b7zen", ",", "Stand'", "und", "Trew"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "So einander \u00e4hnlich sey,", "tokens": ["So", "ein\u00b7an\u00b7der", "\u00e4hn\u00b7lich", "sey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Das so gern der Sternen Raht", "tokens": ["Das", "so", "gern", "der", "Ster\u00b7nen", "Raht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ADV", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Vntersiegelt hat?", "tokens": ["Vn\u00b7ter\u00b7sie\u00b7gelt", "hat", "?"], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Himmel, wann dir in der Welt", "tokens": ["Him\u00b7mel", ",", "wann", "dir", "in", "der", "Welt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zucht und Tugend wolgef\u00e4llt,", "tokens": ["Zucht", "und", "Tu\u00b7gend", "wol\u00b7ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wann du Lust zu Vnschuld tr\u00e4gst", "tokens": ["Wann", "du", "Lust", "zu", "Vn\u00b7schuld", "tr\u00e4gst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "NN", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vnd Belohnung f\u00fcr sie hegst:", "tokens": ["Vnd", "Be\u00b7loh\u00b7nung", "f\u00fcr", "sie", "hegst", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df dich ietzund reichlich aus", "tokens": ["La\u00df", "dich", "ie\u00b7tzund", "reich\u00b7lich", "aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADJD", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00dcber dieses Bett' und Hau\u00df!", "tokens": ["\u00dc\u00b7ber", "die\u00b7ses", "Bett'", "und", "Hau\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nie wird deiner Guttaht Hand", "tokens": ["Nie", "wird", "dei\u00b7ner", "Gut\u00b7taht", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Besser angewandt.", "tokens": ["Bes\u00b7ser", "an\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Edle Seelen, f\u00f6rtert Euch,", "tokens": ["Ed\u00b7le", "See\u00b7len", ",", "f\u00f6r\u00b7tert", "Euch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Werdet bald den Sternen gleich,", "tokens": ["Wer\u00b7det", "bald", "den", "Ster\u00b7nen", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ewer Sam' erhalte Preis", "tokens": ["E\u00b7wer", "Sam'", "er\u00b7hal\u00b7te", "Preis"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch den weiten Erden-Krey\u00df,", "tokens": ["Durch", "den", "wei\u00b7ten", "Er\u00b7den\u00b7Krey\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vnd beherrsche nach wie vor", "tokens": ["Vnd", "be\u00b7herr\u00b7sche", "nach", "wie", "vor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "KOKOM", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sein- und unsrer Feinde Thor,", "tokens": ["Sein", "und", "uns\u00b7rer", "Fein\u00b7de", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ja er m\u00fcss' an Tugend-Schein", "tokens": ["Ja", "er", "m\u00fcss'", "an", "Tu\u00b7gen\u00b7dSchein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "PPER", "VMFIN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mehr als Menschlich seyn!", "tokens": ["Mehr", "als", "Menschlich", "seyn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ADJD", "VAINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}