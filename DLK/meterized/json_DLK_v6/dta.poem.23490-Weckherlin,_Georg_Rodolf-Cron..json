{"dta.poem.23490": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Cron.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1619", "urn": "urn:nbn:de:kobv:b4-20627-4", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Musen trost/ vnd der Tugenten", "tokens": ["Der", "Mu\u00b7sen", "trost", "/", "vnd", "der", "Tu\u00b7gen\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$(", "KON", "ART", "NN"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "wohn/", "tokens": ["wohn", "/"], "token_info": ["word", "punct"], "pos": ["PWAV", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Vnd der Potentaten Exempel;", "tokens": ["Vnd", "der", "Po\u00b7ten\u00b7ta\u00b7ten", "Ex\u00b7em\u00b7pel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ja wie zu Rom der tempel Pantheon", "tokens": ["Ja", "wie", "zu", "Rom", "der", "tem\u00b7pel", "Pan\u00b7the\u00b7on"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "KOKOM", "APPR", "NE", "ART", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "War einmahl aller G\u00f6tter tempel:", "tokens": ["War", "ein\u00b7mahl", "al\u00b7ler", "G\u00f6t\u00b7ter", "tem\u00b7pel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Also ist jetz bey vns ", "tokens": ["Al\u00b7so", "ist", "jetz", "bey", "vns"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "PPER"], "meter": "+--++-", "measure": "dactylic.init"}, "line.7": {"text": "Der hat allein die Tugenten bey sich.", "tokens": ["Der", "hat", "al\u00b7lein", "die", "Tu\u00b7gen\u00b7ten", "bey", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "APPR", "PRF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Er ist Wei\u00df sich wol f\u00fcr-zu-sehen/", "tokens": ["Er", "ist", "Wei\u00df", "sich", "wol", "f\u00fcr\u00b7zu\u00b7se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "PRF", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Starck gl\u00fcck vnd vngl\u00fcck au\u00df-zu-stehen/", "tokens": ["Starck", "gl\u00fcck", "vnd", "vn\u00b7gl\u00fcck", "au\u00df\u00b7zu\u00b7ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "NN", "VVIZU", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "M\u00e4\u00dfig/ N\u00fcchtern/ Embsig zugleich/", "tokens": ["M\u00e4\u00b7\u00dfig", "/", "N\u00fcch\u00b7tern", "/", "Emb\u00b7sig", "zu\u00b7gleich", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$(", "NN", "$(", "NE", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Milt wol-zu-thun/ vnd recht vnd wol zu-", "tokens": ["Milt", "wol\u00b7zu\u00b7thun", "/", "vnd", "recht", "vnd", "wol", "zu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$(", "KON", "ADJD", "KON", "ADV", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "geben/", "tokens": ["ge\u00b7ben", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "An Gnad/ Gotsforcht vn\u0303 From\u0303keit reich/", "tokens": ["An", "Gnad", "/", "Gots\u00b7forcht", "v\u00f1", "From\u0303keit", "reich", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "NN", "APPR", "NN", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Gerecht f\u00fcr Sich vnd f\u00fcr andre zu leben/", "tokens": ["Ge\u00b7recht", "f\u00fcr", "Sich", "vnd", "f\u00fcr", "and\u00b7re", "zu", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "KON", "APPR", "PIS", "PTKZU", "VVINF", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Von Sitten die je mehr vnd mehr", "tokens": ["Von", "Sit\u00b7ten", "die", "je", "mehr", "vnd", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "ADV", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Der Menschen Hertz mit lieb vnd lob durch-", "tokens": ["Der", "Men\u00b7schen", "Hertz", "mit", "lieb", "vnd", "lob", "durch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "ADJD", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "dringen:", "tokens": ["drin\u00b7gen", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Darumb Wir billich zumahl singen/", "tokens": ["Da\u00b7rumb", "Wir", "bil\u00b7lich", "zu\u00b7mahl", "sin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADJD", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Georg-Friderich ist der sterblichen ehr.", "tokens": ["Ge\u00b7or\u00b7gFri\u00b7de\u00b7rich", "ist", "der", "sterb\u00b7li\u00b7chen", "ehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}