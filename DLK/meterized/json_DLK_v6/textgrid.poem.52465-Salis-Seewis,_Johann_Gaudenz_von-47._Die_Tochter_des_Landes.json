{"textgrid.poem.52465": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "47. Die Tochter des Landes", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Pales sich zur Pfleglingin erkoren,", "tokens": ["Die", "Pa\u00b7les", "sich", "zur", "Pfleg\u00b7lin\u00b7gin", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Tochter der Natur,", "tokens": ["Die", "Toch\u00b7ter", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wird in der H\u00fctte stillem Raum geboren,", "tokens": ["Wird", "in", "der", "H\u00fct\u00b7te", "stil\u00b7lem", "Raum", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erzogen auf der Flur.", "tokens": ["Er\u00b7zo\u00b7gen", "auf", "der", "Flur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Den Jubelton von hellen Weidenpfeifen", "tokens": ["Den", "Ju\u00b7bel\u00b7ton", "von", "hel\u00b7len", "Wei\u00b7den\u00b7pfei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Empf\u00e4ngt zuerst ihr Ohr:", "tokens": ["Emp\u00b7f\u00e4ngt", "zu\u00b7erst", "ihr", "Ohr", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr erster Blick steigt zu den Purpurstreifen", "tokens": ["Ihr", "ers\u00b7ter", "Blick", "steigt", "zu", "den", "Pur\u00b7pur\u00b7strei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am Westgew\u00f6lk empor.", "tokens": ["Am", "West\u00b7ge\u00b7w\u00f6lk", "em\u00b7por", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gl\u00fcckselig Kind, das in bekr\u00e4nzter Wiege", "tokens": ["Gl\u00fcck\u00b7se\u00b7lig", "Kind", ",", "das", "in", "be\u00b7kr\u00e4nz\u00b7ter", "Wie\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ein Bl\u00fctenwipfel k\u00fchlt,", "tokens": ["Ein", "Bl\u00fc\u00b7ten\u00b7wip\u00b7fel", "k\u00fchlt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Um das vertraut sein Lamm und seine Ziege", "tokens": ["Um", "das", "ver\u00b7traut", "sein", "Lamm", "und", "sei\u00b7ne", "Zie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJD", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im hohen Grase spielt!", "tokens": ["Im", "ho\u00b7hen", "Gra\u00b7se", "spielt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Froh klimmt sie nach des Regenbogens Farben", "tokens": ["Froh", "klimmt", "sie", "nach", "des", "Re\u00b7gen\u00b7bo\u00b7gens", "Far\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hinan des H\u00fcgels Pfad;", "tokens": ["Hi\u00b7nan", "des", "H\u00fc\u00b7gels", "Pfad", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zum Sitze w\u00e4hlt sie pralle Weizengarben,", "tokens": ["Zum", "Sit\u00b7ze", "w\u00e4hlt", "sie", "pral\u00b7le", "Wei\u00b7zen\u00b7gar\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum Pf\u00fchl der Gerste Schwad.", "tokens": ["Zum", "Pf\u00fchl", "der", "Gers\u00b7te", "Schwad", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ein Nelkenstock befriedigt ihr Begehren;", "tokens": ["Ein", "Nel\u00b7ken\u00b7stock", "be\u00b7frie\u00b7digt", "ihr", "Be\u00b7geh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit wenigem vergn\u00fcgt,", "tokens": ["Mit", "we\u00b7ni\u00b7gem", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist ihre Thr\u00e4ne, gleich Aurorens Z\u00e4hren,", "tokens": ["Ist", "ih\u00b7re", "Thr\u00e4\u00b7ne", ",", "gleich", "Au\u00b7ro\u00b7rens", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "ADV", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im ersten Strahl versiegt.", "tokens": ["Im", "ers\u00b7ten", "Strahl", "ver\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ihr singt der Hain nur mit der Freude T\u00f6nen,", "tokens": ["Ihr", "singt", "der", "Hain", "nur", "mit", "der", "Freu\u00b7de", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Leicht ist ihr Gang wie Tanz;", "tokens": ["Leicht", "ist", "ihr", "Gang", "wie", "Tanz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "KOKOM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch band sie nicht, ein teures Grab zu kr\u00f6nen,", "tokens": ["Noch", "band", "sie", "nicht", ",", "ein", "teu\u00b7res", "Grab", "zu", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den bittern Rautenkranz.", "tokens": ["Den", "bit\u00b7tern", "Rau\u00b7ten\u00b7kranz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Gl\u00fcckseliger, wenn sie nun sechzehn Lenze,", "tokens": ["Gl\u00fcck\u00b7se\u00b7li\u00b7ger", ",", "wenn", "sie", "nun", "sech\u00b7zehn", "Len\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "CARD", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Statt sechzehn Ahnen, z\u00e4hlt,", "tokens": ["Statt", "sech\u00b7zehn", "Ah\u00b7nen", ",", "z\u00e4hlt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "CARD", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem Haupt zum Kleinod goldner Primeln Kr\u00e4nze,", "tokens": ["Dem", "Haupt", "zum", "Klei\u00b7nod", "gold\u00b7ner", "Pri\u00b7meln", "Kr\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Brust Violen w\u00e4hlt.", "tokens": ["Der", "Brust", "Vi\u00b7o\u00b7len", "w\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ihr Antlitz bl\u00fcht, wenn einfach sich die Haube", "tokens": ["Ihr", "Ant\u00b7litz", "bl\u00fcht", ",", "wenn", "ein\u00b7fach", "sich", "die", "Hau\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KOUS", "PPER", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um ihre Schl\u00e4fe dr\u00e4ngt,", "tokens": ["Um", "ih\u00b7re", "Schl\u00e4\u00b7fe", "dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bescheiden, hold, wie halb versteckt im Laube", "tokens": ["Be\u00b7schei\u00b7den", ",", "hold", ",", "wie", "halb", "ver\u00b7steckt", "im", "Lau\u00b7be"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "$,", "PWAV", "ADJD", "VVPP", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Maiengl\u00f6ckchen h\u00e4ngt.", "tokens": ["Ein", "Mai\u00b7en\u00b7gl\u00f6\u00b7ck\u00b7chen", "h\u00e4ngt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Sch\u00f6n wallt ihr Haar, das sie gleich der Najade", "tokens": ["Sch\u00f6n", "wallt", "ihr", "Haar", ",", "das", "sie", "gleich", "der", "Na\u00b7ja\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Im Quell zu waschen pflegt,", "tokens": ["Im", "Quell", "zu", "wa\u00b7schen", "pflegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und, unentweiht von Puder und Pomade,", "tokens": ["Und", ",", "un\u00b7ent\u00b7weiht", "von", "Pu\u00b7der", "und", "Po\u00b7ma\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In lose Flechten legt.", "tokens": ["In", "lo\u00b7se", "Flech\u00b7ten", "legt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "So prangt sie still im heimatlichen Thale,", "tokens": ["So", "prangt", "sie", "still", "im", "hei\u00b7mat\u00b7li\u00b7chen", "Tha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von weiser Zucht bewacht,", "tokens": ["Von", "wei\u00b7ser", "Zucht", "be\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Unschuld wirft den Schleier der Vestale", "tokens": ["Und", "Un\u00b7schuld", "wirft", "den", "Schlei\u00b7er", "der", "Ves\u00b7ta\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um ihre Hirtentracht.", "tokens": ["Um", "ih\u00b7re", "Hir\u00b7ten\u00b7tracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Die H\u00e4uslichkeit birgt sie in gr\u00fcnen Hallen,", "tokens": ["Die", "H\u00e4us\u00b7lich\u00b7keit", "birgt", "sie", "in", "gr\u00fc\u00b7nen", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Vom Sonnenstrahl umblinkt,", "tokens": ["Vom", "Son\u00b7nen\u00b7strahl", "um\u00b7blinkt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis ihr beim Hochzeitlied der Nachtigallen", "tokens": ["Bis", "ihr", "beim", "Hoch\u00b7zeit\u00b7lied", "der", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Myrtenkranz entsinkt.", "tokens": ["Der", "Myr\u00b7ten\u00b7kranz", "ent\u00b7sinkt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Dann zieret sie, wenn br\u00e4utlich hold sie bebet,", "tokens": ["Dann", "zie\u00b7ret", "sie", ",", "wenn", "br\u00e4ut\u00b7lich", "hold", "sie", "be\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ADJD", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Versch\u00e4mtes Rosenlicht;", "tokens": ["Ver\u00b7sch\u00e4m\u00b7tes", "Ro\u00b7sen\u00b7licht", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und bald, was mehr ein holdes Weib noch hebet,", "tokens": ["Und", "bald", ",", "was", "mehr", "ein", "hol\u00b7des", "Weib", "noch", "he\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PRELS", "ADV", "ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erf\u00fcllte Mutterpflicht.", "tokens": ["Er\u00b7f\u00fcll\u00b7te", "Mut\u00b7ter\u00b7pflicht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}