{"textgrid.poem.49421": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "[auf irren Pfaden ohne Ende]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf irren Pfaden ohne Ende", "tokens": ["Auf", "ir\u00b7ren", "Pfa\u00b7den", "oh\u00b7ne", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schritt ich dahin in banger Qual,", "tokens": ["Schritt", "ich", "da\u00b7hin", "in", "ban\u00b7ger", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich f\u00fchrten deine lieben H\u00e4nde.", "tokens": ["Mich", "f\u00fchr\u00b7ten", "dei\u00b7ne", "lie\u00b7ben", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich sah am Horizont, dass fahl", "tokens": ["Ich", "sah", "am", "Ho\u00b7ri\u00b7zont", ",", "dass", "fahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein schwacher Schein der Hoffnung glimme,", "tokens": ["Ein", "schwa\u00b7cher", "Schein", "der", "Hoff\u00b7nung", "glim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Auge war der Morgenstrahl.", "tokens": ["Dein", "Au\u00b7ge", "war", "der", "Mor\u00b7gen\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ermut'gend durch die Nacht, die schlimme,", "tokens": ["Er\u00b7mut'\u00b7gend", "durch", "die", "Nacht", ",", "die", "schlim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kam nur der eig'nen Schritte Klang:", "tokens": ["Kam", "nur", "der", "eig'\u00b7nen", "Schrit\u00b7te", "Klang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geh weiter! sagte deine Stimme.", "tokens": ["Geh", "wei\u00b7ter", "!", "sag\u00b7te", "dei\u00b7ne", "Stim\u00b7me", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mein Herz, so d\u00fcster und so bang,", "tokens": ["Mein", "Herz", ",", "so", "d\u00fcs\u00b7ter", "und", "so", "bang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es weinte still in bitt'rem Leide,", "tokens": ["Es", "wein\u00b7te", "still", "in", "bitt'\u00b7rem", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Liebe, die den Sieg errang", "tokens": ["Die", "Lie\u00b7be", ",", "die", "den", "Sieg", "er\u00b7rang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Hat uns geeint in sel'ger Freude!", "tokens": ["Hat", "uns", "ge\u00b7eint", "in", "sel'\u00b7ger", "Freu\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}