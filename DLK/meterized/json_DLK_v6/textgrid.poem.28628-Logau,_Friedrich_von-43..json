{"textgrid.poem.28628": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "43.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Theils sucht man Wissenschaft nur blo\u00df zu schlechtem wissen,", "tokens": ["Theils", "sucht", "man", "Wis\u00b7sen\u00b7schaft", "nur", "blo\u00df", "zu", "schlech\u00b7tem", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "NN", "ADV", "ADV", "APPR", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dieses dient dahin den F\u00fcrwitz nur zu b\u00fcssen;", "tokens": ["Und", "die\u00b7ses", "dient", "da\u00b7hin", "den", "F\u00fcr\u00b7witz", "nur", "zu", "b\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PAV", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Theils sucht man Wisssenschafft, damit man sey geehrt,", "tokens": ["Theils", "sucht", "man", "Wiss\u00b7sen\u00b7schafft", ",", "da\u00b7mit", "man", "sey", "ge\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "NN", "$,", "KOUS", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und dieses thun nur die, die Eitelkeit beth\u00f6rt;", "tokens": ["Und", "die\u00b7ses", "thun", "nur", "die", ",", "die", "Ei\u00b7tel\u00b7keit", "be\u00b7th\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADV", "ART", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Theils sucht man Wissenschaft, damit man was verdiene,", "tokens": ["Theils", "sucht", "man", "Wis\u00b7sen\u00b7schaft", ",", "da\u00b7mit", "man", "was", "ver\u00b7die\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "NN", "$,", "KOUS", "PIS", "PWS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und dieses schl\u00e4gt nur au\u00df zu sch\u00e4ndlichem Gewiene;", "tokens": ["Und", "die\u00b7ses", "schl\u00e4gt", "nur", "au\u00df", "zu", "sch\u00e4nd\u00b7li\u00b7chem", "Ge\u00b7wie\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADV", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Theils sucht man Wissenschaft, dem Nechsten zum Genie\u00df,", "tokens": ["Theils", "sucht", "man", "Wis\u00b7sen\u00b7schaft", ",", "dem", "Nechs\u00b7ten", "zum", "Ge\u00b7nie\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und dieses ist ein Werk, das wahre Lieb uns hie\u00df;", "tokens": ["Und", "die\u00b7ses", "ist", "ein", "Werk", ",", "das", "wah\u00b7re", "Lieb", "uns", "hie\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Theils sucht man Wissenschaft, sich selbsten zu versorgen,", "tokens": ["Theils", "sucht", "man", "Wis\u00b7sen\u00b7schaft", ",", "sich", "selbs\u00b7ten", "zu", "ver\u00b7sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "NN", "$,", "PRF", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und di\u00df dient, da\u00df man so nicht fremde Witz darff borgen.", "tokens": ["Und", "di\u00df", "dient", ",", "da\u00df", "man", "so", "nicht", "frem\u00b7de", "Witz", "darff", "bor\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "$,", "KOUS", "PIS", "ADV", "PTKNEG", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}}}}}