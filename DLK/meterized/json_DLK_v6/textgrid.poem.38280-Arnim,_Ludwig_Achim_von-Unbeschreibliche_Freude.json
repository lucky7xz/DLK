{"textgrid.poem.38280": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Unbeschreibliche Freude", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer ist denn draussen und klopfet an?", "tokens": ["Wer", "ist", "denn", "draus\u00b7sen", "und", "klop\u00b7fet", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "VVINF", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der mich so leise wecken kann?", "tokens": ["Der", "mich", "so", "lei\u00b7se", "we\u00b7cken", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das ist der Herzallerliebste dein,", "tokens": ["Das", "ist", "der", "Her\u00b7zal\u00b7ler\u00b7liebs\u00b7te", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PPOSAT", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Steh auf und la\u00df mich zu dir ein.", "tokens": ["Steh", "auf", "und", "la\u00df", "mich", "zu", "dir", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVIMP", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das M\u00e4dchen stand auf, und lie\u00df ihn ein,", "tokens": ["Das", "M\u00e4d\u00b7chen", "stand", "auf", ",", "und", "lie\u00df", "ihn", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit seinem schneeweissen Hemdelein;", "tokens": ["Mit", "sei\u00b7nem", "schnee\u00b7weis\u00b7sen", "Hem\u00b7del\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mit seinen schneeweissen Beinen,", "tokens": ["Mit", "sei\u00b7nen", "schnee\u00b7weis\u00b7sen", "Bei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das M\u00e4dchen fing an zu weinen.", "tokens": ["Das", "M\u00e4d\u00b7chen", "fing", "an", "zu", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ach weine nicht, du Liebste mein,", "tokens": ["Ach", "wei\u00b7ne", "nicht", ",", "du", "Liebs\u00b7te", "mein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PTKNEG", "$,", "PPER", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aufs Jahr sollt du mein eigen seyn;", "tokens": ["Aufs", "Jahr", "sollt", "du", "mein", "ei\u00b7gen", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "PPOSAT", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein eigen sollt du werden,", "tokens": ["Mein", "ei\u00b7gen", "sollt", "du", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O Liebe auf gr\u00fcner Erden.", "tokens": ["O", "Lie\u00b7be", "auf", "gr\u00fc\u00b7ner", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich wollt da\u00df alle Felder w\u00e4ren Papier,", "tokens": ["Ich", "wollt", "da\u00df", "al\u00b7le", "Fel\u00b7der", "w\u00e4\u00b7ren", "Pa\u00b7pier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KOUS", "PIAT", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Und alle Studenten schrieben hier;", "tokens": ["Und", "al\u00b7le", "Stu\u00b7den\u00b7ten", "schrie\u00b7ben", "hier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie schrieben ja hier die liebe lange Nacht,", "tokens": ["Sie", "schrie\u00b7ben", "ja", "hier", "die", "lie\u00b7be", "lan\u00b7ge", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sie schrieben uns beiden die Liebe doch nicht ab.", "tokens": ["Sie", "schrie\u00b7ben", "uns", "bei\u00b7den", "die", "Lie\u00b7be", "doch", "nicht", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "ART", "NN", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}}}}