{"textgrid.poem.35708": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "32. Das Landm\u00e4dchen", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An meines Vaters H\u00fcgel,", "tokens": ["An", "mei\u00b7nes", "Va\u00b7ters", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da steht ein sch\u00f6ner Baum:", "tokens": ["Da", "steht", "ein", "sch\u00f6\u00b7ner", "Baum", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gern singt das Waldgefl\u00fcgel", "tokens": ["Gern", "singt", "das", "Wald\u00b7ge\u00b7fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An meines Vaters H\u00fcgel,", "tokens": ["An", "mei\u00b7nes", "Va\u00b7ters", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und singt mir manchen Traum.", "tokens": ["Und", "singt", "mir", "man\u00b7chen", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Man ruht auf weichem Rasen,", "tokens": ["Man", "ruht", "auf", "wei\u00b7chem", "Ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Zitterglanz erhellt;", "tokens": ["Von", "Zit\u00b7ter\u00b7glanz", "er\u00b7hellt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Schaf' und L\u00e4mmer grasen;", "tokens": ["Die", "Schaf'", "und", "L\u00e4m\u00b7mer", "gra\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Man ruht auf weichem Rasen,", "tokens": ["Man", "ruht", "auf", "wei\u00b7chem", "Ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und \u00fcberschaut das Feld.", "tokens": ["Und", "\u00fc\u00b7bersc\u00b7haut", "das", "Feld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "In gr\u00fcngew\u00f6lbtem Laube,", "tokens": ["In", "gr\u00fcn\u00b7ge\u00b7w\u00f6lb\u00b7tem", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Sonne schien so warm!", "tokens": ["Die", "Son\u00b7ne", "schien", "so", "warm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Belauscht' ich meine Taube,", "tokens": ["Be\u00b7lauscht'", "ich", "mei\u00b7ne", "Tau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In gr\u00fcngew\u00f6lbtem Laube,", "tokens": ["In", "gr\u00fcn\u00b7ge\u00b7w\u00f6lb\u00b7tem", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und froher W\u00fcrmchen Schwarm", "tokens": ["Und", "fro\u00b7her", "W\u00fcrm\u00b7chen", "Schwarm"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da kann er mit Err\u00f6ten", "tokens": ["Da", "kann", "er", "mit", "Er\u00b7r\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch hohes Gras daher;", "tokens": ["Durch", "ho\u00b7hes", "Gras", "da\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PAV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ich hatt' ihn nicht gebeten:", "tokens": ["Ich", "hatt'", "ihn", "nicht", "ge\u00b7be\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da kam er mit Err\u00f6ten,", "tokens": ["Da", "kam", "er", "mit", "Er\u00b7r\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gewi\u00df von ohngef\u00e4hr.", "tokens": ["Ge\u00b7wi\u00df", "von", "ohn\u00b7ge\u00b7f\u00e4hr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Vertraulich sank er nieder", "tokens": ["Ver\u00b7trau\u00b7lich", "sank", "er", "nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu mir auf weiches, Gras.", "tokens": ["Zu", "mir", "auf", "wei\u00b7ches", ",", "Gras", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ADJA", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir ward so eng das Mieder!", "tokens": ["Mir", "ward", "so", "eng", "das", "Mie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vertraulich sank er nieder,", "tokens": ["Ver\u00b7trau\u00b7lich", "sank", "er", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und sprach, ich wei\u00df nicht was.", "tokens": ["Und", "sprach", ",", "ich", "wei\u00df", "nicht", "was", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VVFIN", "PTKNEG", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Er w\u00e4re gern geblieben;", "tokens": ["Er", "w\u00e4\u00b7re", "gern", "ge\u00b7blie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Allein ich hie\u00df ihn gehn.", "tokens": ["Al\u00b7lein", "ich", "hie\u00df", "ihn", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mich d\u00e4ucht, er sprach von Lieben:", "tokens": ["Mich", "d\u00e4ucht", ",", "er", "sprach", "von", "Lie\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er w\u00e4re gern geblieben,", "tokens": ["Er", "w\u00e4\u00b7re", "gern", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und schmeichelte so sch\u00f6n.", "tokens": ["Und", "schmei\u00b7chel\u00b7te", "so", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wie \u00f6d' ist mir seit gestern", "tokens": ["Wie", "\u00f6d'", "ist", "mir", "seit", "ge\u00b7stern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Stell' im weichen Gras!", "tokens": ["Die", "Stell'", "im", "wei\u00b7chen", "Gras", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erz\u00e4hlt was, liebe Schwestern!", "tokens": ["Er\u00b7z\u00e4hlt", "was", ",", "lie\u00b7be", "Schwes\u00b7tern", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie \u00f6d' ist mir seit gestern", "tokens": ["Wie", "\u00f6d'", "ist", "mir", "seit", "ge\u00b7stern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die Stelle, wo er sa\u00df!", "tokens": ["Die", "Stel\u00b7le", ",", "wo", "er", "sa\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}