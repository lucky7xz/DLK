{"dta.poem.20648": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  den Ph\u00f6bus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du l\u00e4chelst, Ph\u00f6bus! diese nackten R\u00fcmpfe                 ", "tokens": ["Du", "l\u00e4\u00b7chelst", ",", "Ph\u00f6\u00b7bus", "!", "die\u00b7se", "nack\u00b7ten", "R\u00fcmp\u00b7fe"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "NE", "$.", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der hingestreckten Blumenstengel an,", "tokens": ["Der", "hin\u00b7ge\u00b7streck\u00b7ten", "Blu\u00b7men\u00b7sten\u00b7gel", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Willst du den Weinstock k\u00fcssen, der die Nymphe", "tokens": ["Willst", "du", "den", "Wein\u00b7stock", "k\u00fcs\u00b7sen", ",", "der", "die", "Nym\u00b7phe"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht mehr vor dir verstecken kann?", "tokens": ["Nicht", "mehr", "vor", "dir", "ver\u00b7ste\u00b7cken", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Soll in bedorrter Zweige Wurzel dringen", "tokens": ["Soll", "in", "be\u00b7dorr\u00b7ter", "Zwei\u00b7ge", "Wur\u00b7zel", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ADJA", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein warmer Blick, dem Winter zum Verdru\u00df,", "tokens": ["Dein", "war\u00b7mer", "Blick", ",", "dem", "Win\u00b7ter", "zum", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df tausendbl\u00e4ttrig seine Reize bringen", "tokens": ["Da\u00df", "tau\u00b7send\u00b7bl\u00e4tt\u00b7rig", "sei\u00b7ne", "Rei\u00b7ze", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Rosenstock zu Kr\u00e4nzen mu\u00df?", "tokens": ["Der", "Ro\u00b7sen\u00b7stock", "zu", "Kr\u00e4n\u00b7zen", "mu\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nein, zu gewaltig wirst du hingerissen", "tokens": ["Nein", ",", "zu", "ge\u00b7wal\u00b7tig", "wirst", "du", "hin\u00b7ge\u00b7ris\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PTKA", "ADJD", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von jener Phillis. Gro\u00dfer Ph\u00f6bus! gl\u00fch\u2019", "tokens": ["Von", "je\u00b7ner", "Phil\u00b7lis", ".", "Gro\u00b7\u00dfer", "Ph\u00f6\u00b7bus", "!", "gl\u00fch'"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "PDAT", "NE", "$.", "ADJA", "NN", "$.", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vergebens, ihrer Lippen Pracht zu k\u00fcssen:", "tokens": ["Ver\u00b7ge\u00b7bens", ",", "ih\u00b7rer", "Lip\u00b7pen", "Pracht", "zu", "k\u00fcs\u00b7sen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Hirt Alexis k\u00fcsset sie.", "tokens": ["Der", "Hirt", "A\u00b7le\u00b7xis", "k\u00fcs\u00b7set", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}