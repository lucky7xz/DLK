{"textgrid.poem.67125": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Mensch", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kaum spro\u00dften aus den Wassern, o Erde, dir", "tokens": ["Kaum", "spro\u00df\u00b7ten", "aus", "den", "Was\u00b7sern", ",", "o", "Er\u00b7de", ",", "dir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,", "FM", "NN", "$,", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der jungen Berge Gipfel und dufteten", "tokens": ["Der", "jun\u00b7gen", "Ber\u00b7ge", "Gip\u00b7fel", "und", "duf\u00b7te\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "KON", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Lustatmend, immergr\u00fcner Haine", "tokens": ["Lus\u00b7tat\u00b7mend", ",", "im\u00b7mer\u00b7gr\u00fc\u00b7ner", "Hai\u00b7ne"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVPP", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Voll, in des Ozeans grauer Wildnis", "tokens": ["Voll", ",", "in", "des", "O\u00b7ze\u00b7ans", "grau\u00b7er", "Wild\u00b7nis"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Die ersten holden Inseln; und freudig sah", "tokens": ["Die", "ers\u00b7ten", "hol\u00b7den", "In\u00b7seln", ";", "und", "freu\u00b7dig", "sah"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "$.", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Sonnengottes Auge die Neulinge,", "tokens": ["Des", "Son\u00b7nen\u00b7got\u00b7tes", "Au\u00b7ge", "die", "Neu\u00b7lin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Pflanzen, seiner ewgen Jugend", "tokens": ["Die", "Pflan\u00b7zen", ",", "sei\u00b7ner", "ew\u00b7gen", "Ju\u00b7gend"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "L\u00e4chelnde Kinder, aus dir geboren.", "tokens": ["L\u00e4\u00b7cheln\u00b7de", "Kin\u00b7der", ",", "aus", "dir", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "PPER", "VVPP", "$."], "meter": "+--+-++-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Da auf der Inseln sch\u00f6nster, wo immerhin", "tokens": ["Da", "auf", "der", "In\u00b7seln", "sch\u00f6ns\u00b7ter", ",", "wo", "im\u00b7mer\u00b7hin"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "$,", "PWAV", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Hain in zarter Ruhe die Luft umflo\u00df,", "tokens": ["Den", "Hain", "in", "zar\u00b7ter", "Ru\u00b7he", "die", "Luft", "um\u00b7flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Lag unter Trauben einst, nach lauer", "tokens": ["Lag", "un\u00b7ter", "Trau\u00b7ben", "einst", ",", "nach", "lau\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "NN", "ADV", "$,", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nacht, in der d\u00e4mmernden Morgenstunde", "tokens": ["Nacht", ",", "in", "der", "d\u00e4m\u00b7mern\u00b7den", "Mor\u00b7gen\u00b7stun\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Geboren, Mutter Erde! dein sch\u00f6nstes Kind; \u2013", "tokens": ["Ge\u00b7bo\u00b7ren", ",", "Mut\u00b7ter", "Er\u00b7de", "!", "dein", "sch\u00f6ns\u00b7tes", "Kind", ";", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "$,", "NN", "NN", "$.", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und auf zum Vater Helios sieht bekannt", "tokens": ["Und", "auf", "zum", "Va\u00b7ter", "He\u00b7li\u00b7os", "sieht", "be\u00b7kannt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "APPRART", "NN", "NE", "VVFIN", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Knab, und wacht und w\u00e4hlt, die s\u00fc\u00dfen", "tokens": ["Der", "Knab", ",", "und", "wacht", "und", "w\u00e4hlt", ",", "die", "s\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "KON", "VVFIN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beere versuchend, die heilge Rebe", "tokens": ["Bee\u00b7re", "ver\u00b7su\u00b7chend", ",", "die", "heil\u00b7ge", "Re\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVPP", "$,", "ART", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Zur Amme sich; und bald ist er gro\u00df; ihn scheun", "tokens": ["Zur", "Am\u00b7me", "sich", ";", "und", "bald", "ist", "er", "gro\u00df", ";", "ihn", "scheun"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "PRF", "$.", "KON", "ADV", "VAFIN", "PPER", "ADJD", "$.", "PPER", "VVINF"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Tiere, denn ein anderer ist, wie sie,", "tokens": ["Die", "Tie\u00b7re", ",", "denn", "ein", "an\u00b7de\u00b7rer", "ist", ",", "wie", "sie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ART", "ADJA", "VAFIN", "$,", "PWAV", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Mensch; nicht dir und nicht dem Vater", "tokens": ["Der", "Mensch", ";", "nicht", "dir", "und", "nicht", "dem", "Va\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "PTKNEG", "PPER", "KON", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gleicht er, denn k\u00fchn ist in ihm und einzig", "tokens": ["Gleicht", "er", ",", "denn", "k\u00fchn", "ist", "in", "ihm", "und", "ein\u00b7zig"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KON", "ADJD", "VAFIN", "APPR", "PPER", "KON", "ADJD"], "meter": "+--+-++-+-", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "Des Vaters hohe Seele mit deiner Lust,", "tokens": ["Des", "Va\u00b7ters", "ho\u00b7he", "See\u00b7le", "mit", "dei\u00b7ner", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "O Erd! und deiner Trauer von je vereint;", "tokens": ["O", "Erd", "!", "und", "dei\u00b7ner", "Trau\u00b7er", "von", "je", "ver\u00b7eint", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "KON", "PPOSAT", "NN", "APPR", "ADV", "VVPP", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der G\u00f6ttermutter, der Natur, der", "tokens": ["Der", "G\u00f6t\u00b7ter\u00b7mut\u00b7ter", ",", "der", "Na\u00b7tur", ",", "der"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PRELS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Allesumfassenden m\u00f6cht er gleichen!", "tokens": ["Al\u00b7les\u00b7um\u00b7fas\u00b7sen\u00b7den", "m\u00f6cht", "er", "glei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Ach! darum treibt ihn, Erde! vom Herzen dir", "tokens": ["Ach", "!", "da\u00b7rum", "treibt", "ihn", ",", "Er\u00b7de", "!", "vom", "Her\u00b7zen", "dir"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "PAV", "VVFIN", "PPER", "$,", "NN", "$.", "APPRART", "NN", "PPER"], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sein \u00dcbermut, und deine Geschenke sind", "tokens": ["Sein", "\u00dc\u00b7ber\u00b7mut", ",", "und", "dei\u00b7ne", "Ge\u00b7schen\u00b7ke", "sind"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Umsonst und deine zarten Bande;", "tokens": ["Um\u00b7sonst", "und", "dei\u00b7ne", "zar\u00b7ten", "Ban\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sucht er ein Besseres doch, der Wilde!", "tokens": ["Sucht", "er", "ein", "Bes\u00b7se\u00b7res", "doch", ",", "der", "Wil\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$,", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Von seines Ufers duftender Wiese mu\u00df", "tokens": ["Von", "sei\u00b7nes", "U\u00b7fers", "duf\u00b7ten\u00b7der", "Wie\u00b7se", "mu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ins bl\u00fctenlose Wasser hinaus der Mensch,", "tokens": ["Ins", "bl\u00fc\u00b7ten\u00b7lo\u00b7se", "Was\u00b7ser", "hin\u00b7aus", "der", "Mensch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APZR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und gl\u00e4nzt auch, wie die Sternenacht, von", "tokens": ["Und", "gl\u00e4nzt", "auch", ",", "wie", "die", "Ster\u00b7ne\u00b7nacht", ",", "von"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "ADV", "$,", "PWAV", "ART", "NN", "$,", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Goldenen Fr\u00fcchten sein Hain, doch gr\u00e4bt er", "tokens": ["Gol\u00b7de\u00b7nen", "Fr\u00fcch\u00b7ten", "sein", "Hain", ",", "doch", "gr\u00e4bt", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Sich H\u00f6hlen in den Bergen und sp\u00e4ht im Schacht,", "tokens": ["Sich", "H\u00f6h\u00b7len", "in", "den", "Ber\u00b7gen", "und", "sp\u00e4ht", "im", "Schacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "APPR", "ART", "NN", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von seines Vaters heiterem Lichte fern,", "tokens": ["Von", "sei\u00b7nes", "Va\u00b7ters", "hei\u00b7te\u00b7rem", "Lich\u00b7te", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dem Sonnengott auch ungetreu, der", "tokens": ["Dem", "Son\u00b7nen\u00b7gott", "auch", "un\u00b7ge\u00b7treu", ",", "der"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ADV", "ADJD", "$,", "PRELS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Knechte nicht liebt und der Sorge spottet.", "tokens": ["Knech\u00b7te", "nicht", "liebt", "und", "der", "Sor\u00b7ge", "spot\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "VVFIN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Denn freier atmen V\u00f6gel des Walds, wenn schon", "tokens": ["Denn", "frei\u00b7er", "at\u00b7men", "V\u00f6\u00b7gel", "des", "Walds", ",", "wenn", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "ADJA", "NN", "ART", "NN", "$,", "KOUS", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Menschen Brust sich herrlicher hebt, und der", "tokens": ["Des", "Men\u00b7schen", "Brust", "sich", "herr\u00b7li\u00b7cher", "hebt", ",", "und", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "PRF", "APZR", "VVFIN", "$,", "KON", "ART"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die dunkle Zukunft sieht, er mu\u00df auch", "tokens": ["Die", "dunk\u00b7le", "Zu\u00b7kunft", "sieht", ",", "er", "mu\u00df", "auch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "PPER", "VMFIN", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Sehen den Tod und allein ihn f\u00fcrchten.", "tokens": ["Se\u00b7hen", "den", "Tod", "und", "al\u00b7lein", "ihn", "f\u00fcrch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "ADV", "PPER", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Und Waffen wider alle, die atmen, tr\u00e4gt", "tokens": ["Und", "Waf\u00b7fen", "wi\u00b7der", "al\u00b7le", ",", "die", "at\u00b7men", ",", "tr\u00e4gt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "NN", "APPR", "PIS", "$,", "PRELS", "VVINF", "$,", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In ewigbangem Stolze der Mensch; im Zwist", "tokens": ["In", "e\u00b7wig\u00b7ban\u00b7gem", "Stol\u00b7ze", "der", "Mensch", ";", "im", "Zwist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$.", "APPRART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Verzehrt er sich und seines Friedens", "tokens": ["Ver\u00b7zehrt", "er", "sich", "und", "sei\u00b7nes", "Frie\u00b7dens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Blume, die z\u00e4rtliche, bl\u00fcht nicht lange.", "tokens": ["Blu\u00b7me", ",", "die", "z\u00e4rt\u00b7li\u00b7che", ",", "bl\u00fcht", "nicht", "lan\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "$,", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Ist er von allen Lebensgenossen nicht", "tokens": ["Ist", "er", "von", "al\u00b7len", "Le\u00b7bens\u00b7ge\u00b7nos\u00b7sen", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PIAT", "NN", "PTKNEG"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der seligste? Doch tiefer und rei\u00dfender", "tokens": ["Der", "se\u00b7ligs\u00b7te", "?", "Doch", "tie\u00b7fer", "und", "rei\u00b7\u00dfen\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$.", "KON", "ADJD", "KON", "ADJA"], "meter": "+-+--+--+--", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ergreift das Schicksal, allausgleichend,", "tokens": ["Er\u00b7greift", "das", "Schick\u00b7sal", ",", "al\u00b7laus\u00b7glei\u00b7chend", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auch die entz\u00fcndbare Brust dem Starken.", "tokens": ["Auch", "die", "ent\u00b7z\u00fcnd\u00b7ba\u00b7re", "Brust", "dem", "Star\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}