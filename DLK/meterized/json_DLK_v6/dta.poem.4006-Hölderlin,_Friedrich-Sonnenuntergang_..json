{"dta.poem.4006": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Sonnenuntergang .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Wo bist Du? trunken d\u00e4mmert die Seele mir", "tokens": ["Wo", "bist", "Du", "?", "trun\u00b7ken", "d\u00e4m\u00b7mert", "die", "See\u00b7le", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "ADJD", "VVFIN", "ART", "NN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von aller Deiner Wonne; denn eben ist's,", "tokens": ["Von", "al\u00b7ler", "Dei\u00b7ner", "Won\u00b7ne", ";", "denn", "e\u00b7ben", "ist's", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$.", "KON", "ADV", "VAFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df ich gelauscht, wie, goldner T\u00f6ne", "tokens": ["Da\u00df", "ich", "ge\u00b7lauscht", ",", "wie", ",", "gold\u00b7ner", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "$,", "PWAV", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Voll, der entz\u00fcckende Sonnenj\u00fcngling", "tokens": ["Voll", ",", "der", "ent\u00b7z\u00fc\u00b7cken\u00b7de", "Son\u00b7nen\u00b7j\u00fcng\u00b7ling"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "ART", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Sein Abendlied auf himmlischer Leyer spielt';", "tokens": ["Sein", "A\u00b7ben\u00b7dlied", "auf", "himm\u00b7li\u00b7scher", "Le\u00b7yer", "spielt'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es t\u00f6nten rings die W\u00e4lder und H\u00fcgel nach,", "tokens": ["Es", "t\u00f6n\u00b7ten", "rings", "die", "W\u00e4l\u00b7der", "und", "H\u00fc\u00b7gel", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch fern ist er zu frommen V\u00f6lkern,", "tokens": ["Doch", "fern", "ist", "er", "zu", "from\u00b7men", "V\u00f6l\u00b7kern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die ihn noch ehren, hinweggegangen.", "tokens": ["Die", "ihn", "noch", "eh\u00b7ren", ",", "hin\u00b7weg\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "$,", "VVPP", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}