{"dta.poem.19011": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Dancksagung f\u00fcr jhre erledigung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Evch will ich dienen/ loben/ ehren/", "tokens": ["Evch", "will", "ich", "die\u00b7nen", "/", "lo\u00b7ben", "/", "eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "VVINF", "$(", "VVINF", "$(", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O Nymfen/ der welt bestes glick/", "tokens": ["O", "Nym\u00b7fen", "/", "der", "welt", "bes\u00b7tes", "glick", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil (gn\u00e4dig) h\u00f6rend mein begehren", "tokens": ["Weil", "(", "gn\u00e4\u00b7dig", ")", "h\u00f6\u00b7rend", "mein", "be\u00b7geh\u00b7ren"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "$(", "ADJD", "$(", "VVPP", "PPOSAT", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jhr Vns gefreyhet von dem strick", "tokens": ["Ihr", "Vns", "ge\u00b7frey\u00b7het", "von", "dem", "strick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sich bey euch in dienst zu begeben", "tokens": ["Sich", "bey", "euch", "in", "dienst", "zu", "be\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "PPER", "APPR", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vil besser ist dan frey zu leben.", "tokens": ["Vil", "bes\u00b7ser", "ist", "dan", "frey", "zu", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein Richter war bald vberwunden", "tokens": ["Mein", "Rich\u00b7ter", "war", "bald", "vber\u00b7wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Als Er sah ewer angesicht.", "tokens": ["Als", "Er", "sah", "e\u00b7wer", "an\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "VVPP", "$."], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch bin ich noch so hart verbunden", "tokens": ["Doch", "bin", "ich", "noch", "so", "hart", "ver\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ob ich wol gefangen nicht:", "tokens": ["Ob", "ich", "wol", "ge\u00b7fan\u00b7gen", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zwar sich in ewern dienst begeben", "tokens": ["Zwar", "sich", "in", "e\u00b7wern", "dienst", "be\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PRF", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist ein glickseelig-freyes leben.", "tokens": ["Ist", "ein", "glick\u00b7see\u00b7lig\u00b7freyes", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+----", "measure": "unknown.measure.di"}}, "stanza.3": {"line.1": {"text": "F\u00fcrhin solt jhr mein Reich regieren/", "tokens": ["F\u00fcr\u00b7hin", "solt", "jhr", "mein", "Reich", "re\u00b7gie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Damit die welt glickseelig sey/", "tokens": ["Da\u00b7mit", "die", "welt", "glick\u00b7see\u00b7lig", "sey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die laster auch den schwanck verlieren", "tokens": ["Die", "las\u00b7ter", "auch", "den", "schwanck", "ver\u00b7lie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd m\u00f6niglich au\u00df frewden schrey:", "tokens": ["Vnd", "m\u00f6\u00b7nig\u00b7lich", "au\u00df", "frew\u00b7den", "schrey", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In ewern dienst sich zu begeben/", "tokens": ["In", "e\u00b7wern", "dienst", "sich", "zu", "be\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist das glickseeligiste leben.", "tokens": ["Ist", "das", "glick\u00b7see\u00b7li\u00b7gis\u00b7te", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}