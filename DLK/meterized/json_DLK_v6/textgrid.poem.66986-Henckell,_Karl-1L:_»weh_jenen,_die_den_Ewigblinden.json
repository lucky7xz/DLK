{"textgrid.poem.66986": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbweh jenen, die den Ewigblinden", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbweh jenen, die den Ewigblinden", "tokens": ["\u00bb", "weh", "je\u00b7nen", ",", "die", "den", "E\u00b7wig\u00b7blin\u00b7den"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADV", "PDS", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Lichtes Himmelsfackel leihn!\u00ab \u2013", "tokens": ["Des", "Lich\u00b7tes", "Him\u00b7mels\u00b7fa\u00b7ckel", "leihn", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weh jenen, die das Maul verbinden", "tokens": ["Weh", "je\u00b7nen", ",", "die", "das", "Maul", "ver\u00b7bin\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PDAT", "$,", "PRELS", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Menschen, die nach Wahrheit schrein!", "tokens": ["Den", "Men\u00b7schen", ",", "die", "nach", "Wahr\u00b7heit", "schrein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}