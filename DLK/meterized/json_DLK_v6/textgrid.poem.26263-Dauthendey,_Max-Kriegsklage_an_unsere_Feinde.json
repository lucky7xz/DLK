{"textgrid.poem.26263": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Kriegsklage an unsere Feinde", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kann nicht mehr fluchen. Ich mu\u00df trauern und schweigen,", "tokens": ["Kann", "nicht", "mehr", "flu\u00b7chen", ".", "Ich", "mu\u00df", "trau\u00b7ern", "und", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ADV", "VVINF", "$.", "PPER", "VMFIN", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da sich die V\u00f6lker endlos belauern, endlos die Z\u00e4hne zeigen.", "tokens": ["Da", "sich", "die", "V\u00f6l\u00b7ker", "end\u00b7los", "be\u00b7lau\u00b7ern", ",", "end\u00b7los", "die", "Z\u00e4h\u00b7ne", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADJD", "VVINF", "$,", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Mu\u00df sie bedauern. Die Tr\u00e4ne will still steigen.", "tokens": ["Mu\u00df", "sie", "be\u00b7dau\u00b7ern", ".", "Die", "Tr\u00e4\u00b7ne", "will", "still", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$.", "ART", "NN", "VMFIN", "ADJD", "VVINF", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Ohne Nutzen w\u00fchlen in Wut die Nie-Satten,", "tokens": ["Oh\u00b7ne", "Nut\u00b7zen", "w\u00fch\u00b7len", "in", "Wut", "die", "Nie\u00b7Sat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Lassen das Eisen nicht k\u00fchlen, nie ermatten.", "tokens": ["Las\u00b7sen", "das", "Ei\u00b7sen", "nicht", "k\u00fch\u00b7len", ",", "nie", "er\u00b7mat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "VVINF", "$,", "ADV", "VVFIN", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Tot ist all ihr F\u00fchlen. Es rasen nur Schatten.", "tokens": ["Tot", "ist", "all", "ihr", "F\u00fch\u00b7len", ".", "Es", "ra\u00b7sen", "nur", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ADV", "NN", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Hassende sind sie, die geistlos weitert\u00f6ten.", "tokens": ["Has\u00b7sen\u00b7de", "sind", "sie", ",", "die", "geist\u00b7los", "wei\u00b7ter\u00b7t\u00f6\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Gegenwart wird verhallen, Zukunft err\u00f6ten.", "tokens": ["Ge\u00b7gen\u00b7wart", "wird", "ver\u00b7hal\u00b7len", ",", "Zu\u00b7kunft", "er\u00b7r\u00f6\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVINF", "$,", "NN", "VVINF", "$."], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Sie lallen sinnlos, haben Vernunft vonn\u00f6ten.", "tokens": ["Sie", "lal\u00b7len", "sinn\u00b7los", ",", "ha\u00b7ben", "Ver\u00b7nunft", "von\u00b7n\u00f6\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "VAFIN", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Ihr G\u00f6tze lacht, der Neid, den Ohnmacht geboren.", "tokens": ["Ihr", "G\u00f6t\u00b7ze", "lacht", ",", "der", "Neid", ",", "den", "Ohn\u00b7macht", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "ART", "NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie haben f\u00fcr Lust und Leid den Sinn verloren,", "tokens": ["Sie", "ha\u00b7ben", "f\u00fcr", "Lust", "und", "Leid", "den", "Sinn", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Abgestumpft vom Streit, dem Hunger vor den Toren.", "tokens": ["Ab\u00b7ge\u00b7stumpft", "vom", "Streit", ",", "dem", "Hun\u00b7ger", "vor", "den", "To\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.5": {"line.1": {"text": "Der Erde wachsen Sorgen wie Leichenhaufen.", "tokens": ["Der", "Er\u00b7de", "wach\u00b7sen", "Sor\u00b7gen", "wie", "Lei\u00b7chen\u00b7hau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KOKOM", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Alle wollen das Morgen im Blut ersaufen.", "tokens": ["Al\u00b7le", "wol\u00b7len", "das", "Mor\u00b7gen", "im", "Blut", "er\u00b7sau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Doch vom Tod ist nichts zu borgen, nichts zu kaufen.", "tokens": ["Doch", "vom", "Tod", "ist", "nichts", "zu", "bor\u00b7gen", ",", "nichts", "zu", "kau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "PIS", "PTKZU", "VVINF", "$,", "PIS", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}