{"dta.poem.9577": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An ihre entz\u00fcndende sch\u00f6nheit.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was geist und leben nur in seinen adern tr\u00e4gt/", "tokens": ["Was", "geist", "und", "le\u00b7ben", "nur", "in", "sei\u00b7nen", "a\u00b7dern", "tr\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kennt die vollkommenheit der sch\u00f6nsten Leonoren/", "tokens": ["Kennt", "die", "voll\u00b7kom\u00b7men\u00b7heit", "der", "sch\u00f6ns\u00b7ten", "Le\u00b7o\u00b7no\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die zur verwunderung den menschen ist gebohren/", "tokens": ["Die", "zur", "ver\u00b7wun\u00b7de\u00b7rung", "den", "men\u00b7schen", "ist", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ART", "ADJA", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und fast die halbe welt durch ihren blick bewegt/", "tokens": ["Und", "fast", "die", "hal\u00b7be", "welt", "durch", "ih\u00b7ren", "blick", "be\u00b7wegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die durch ein s\u00fcsses wort die hertzen gantz zerschl\u00e4gt/", "tokens": ["Die", "durch", "ein", "s\u00fcs\u00b7ses", "wort", "die", "hert\u00b7zen", "gantz", "zer\u00b7schl\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und t\u00e4glich frische glut in unsrer brust erregt;", "tokens": ["Und", "t\u00e4g\u00b7lich", "fri\u00b7sche", "glut", "in", "uns\u00b7rer", "brust", "er\u00b7regt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ja/ die der himmel nur uns zum verderb erkohren/", "tokens": ["Ja", "/", "die", "der", "him\u00b7mel", "nur", "uns", "zum", "ver\u00b7derb", "er\u00b7koh\u00b7ren", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PRELS", "ART", "NN", "ADV", "PPER", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df durch ihr heisses feur gieng stadt und land verlohren.", "tokens": ["Da\u00df", "durch", "ihr", "heis\u00b7ses", "feur", "gieng", "stadt", "und", "land", "ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Saat mir: wie hei\u00df\u2019 ich wohl die ungl\u00fccks-stiffterin/", "tokens": ["Saat", "mir", ":", "wie", "hei\u00df'", "ich", "wohl", "die", "un\u00b7gl\u00fccks\u00b7stiff\u00b7te\u00b7rin", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PWAV", "VVFIN", "PPER", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Durch die ich selbst zur glut und aschen worden bin/", "tokens": ["Durch", "die", "ich", "selbst", "zur", "glut", "und", "asc\u00b7hen", "wor\u00b7den", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "APPRART", "NN", "KON", "ADJA", "VAPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie wird auffs grausamste sie doch von mir benennet?", "tokens": ["Wie", "wird", "auffs", "grau\u00b7sams\u00b7te", "sie", "doch", "von", "mir", "be\u00b7nen\u00b7net", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "APPRART", "ADJA", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.12": {"text": "Es gleichet ihrem brand kein blitz noch sonnen-schein;", "tokens": ["Es", "glei\u00b7chet", "ih\u00b7rem", "brand", "kein", "blitz", "noch", "son\u00b7nen\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PIAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sie kan mit be\u00dfrem fug ein h\u00f6ll\u2019sches feuer seyn/", "tokens": ["Sie", "kan", "mit", "be\u00df\u00b7rem", "fug", "ein", "h\u00f6ll'\u00b7sches", "feu\u00b7er", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das tag und nacht uns brennt/ und selber doch nicht brennet.", "tokens": ["Das", "tag", "und", "nacht", "uns", "brennt", "/", "und", "sel\u00b7ber", "doch", "nicht", "bren\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PPER", "VVFIN", "$(", "KON", "ADV", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}