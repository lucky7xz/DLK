{"dta.poem.1413": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Tauff- und Pathen-W\u00fcnsche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Der Tauffe Flutt befreyet aller Flecken/", "tokens": ["Der", "Tauf\u00b7fe", "Flutt", "be\u00b7fre\u00b7yet", "al\u00b7ler", "Fle\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Womit uns pflegt die Erb-Schuld anzustecken/", "tokens": ["Wo\u00b7mit", "uns", "pflegt", "die", "Er\u00b7bSchuld", "an\u00b7zu\u00b7ste\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "NN", "VVIZU", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auff da\u00df du auch erlangest solches Gutt/", "tokens": ["Auff", "da\u00df", "du", "auch", "er\u00b7lan\u00b7gest", "sol\u00b7ches", "Gutt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADV", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nezt/ Pathe/ dich der heilgen Tauffe Flutt.", "tokens": ["Nezt", "/", "Pa\u00b7the", "/", "dich", "der", "heil\u00b7gen", "Tauf\u00b7fe", "Flutt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "NE", "$(", "PPER", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Des H\u00f6chsten Gunst/ woraus der Brunn geflossen/", "tokens": ["Des", "H\u00f6chs\u00b7ten", "Gunst", "/", "wo\u00b7raus", "der", "Brunn", "ge\u00b7flos\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PWAV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Macht dich dadurch zu seinem Reichs-Genossen/", "tokens": ["Macht", "dich", "da\u00b7durch", "zu", "sei\u00b7nem", "Reichs\u00b7Ge\u00b7nos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "PAV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Es raube dir kein Erden-Koth noch Dunst/", "tokens": ["Es", "rau\u00b7be", "dir", "kein", "Er\u00b7den\u00b7Koth", "noch", "Dunst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "ADV", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So lange du hier lebst/ des H\u00f6chsten Gunst.", "tokens": ["So", "lan\u00b7ge", "du", "hier", "lebst", "/", "des", "H\u00f6chs\u00b7ten", "Gunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "VVFIN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}