{"textgrid.poem.50411": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "69. Gedancken zur Abendzeit bey Licht", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich werd' an dir dass ich, wie du, auch ", "tokens": ["Ich", "werd'", "an", "dir", "dass", "ich", ",", "wie", "du", ",", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "KOUS", "PPER", "$,", "PWAV", "PPER", "$,", "ADV"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "So ", "tokens": ["So"], "token_info": ["word"], "pos": ["ADV"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Dass ich den ", "tokens": ["Dass", "ich", "den"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Ich schreib' in dem du brennst, und sorg' in dem ich schreib',", "tokens": ["Ich", "schreib'", "in", "dem", "du", "brennst", ",", "und", "sor\u00b7g'", "in", "dem", "ich", "schreib'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRELS", "PPER", "VVFIN", "$,", "KON", "ADV", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Dass ich bey ", "tokens": ["Dass", "ich", "bey"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Dass ich ", "tokens": ["Dass", "ich"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Und dass weil meine Tag' als wie dein Dunst verfliehn,", "tokens": ["Und", "dass", "weil", "mei\u00b7ne", "Tag'", "als", "wie", "dein", "Dunst", "ver\u00b7fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "KOUS", "PPOSAT", "NN", "KOUS", "KOKOM", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich so wie du ", "tokens": ["Ich", "so", "wie", "du"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "KOKOM", "PPER"], "meter": "-+-+", "measure": "iambic.di"}}}}}