{"dta.poem.12461": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Im Fr\u00fchjahr.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lustig auf! die Erde gl\u00e4nzt,", "tokens": ["Lus\u00b7tig", "auf", "!", "die", "Er\u00b7de", "gl\u00e4nzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein gef\u00fcllter Freudenbecher,", "tokens": ["Ein", "ge\u00b7f\u00fcll\u00b7ter", "Freu\u00b7den\u00b7be\u00b7cher", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der trunkne Himmel kr\u00e4nzt", "tokens": ["Und", "der", "trunk\u00b7ne", "Him\u00b7mel", "kr\u00e4nzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Sich sein Haubt, ein froher Zecher.", "tokens": ["Sich", "sein", "Haubt", ",", "ein", "fro\u00b7her", "Ze\u00b7cher", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ueppig hat ein Bl\u00fcthenleib", "tokens": ["Uep\u00b7pig", "hat", "ein", "Bl\u00fc\u00b7then\u00b7leib"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Um die B\u00e4ume sich ergossen,", "tokens": ["Um", "die", "B\u00e4u\u00b7me", "sich", "er\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gleich als hielt' ein junges Weib", "tokens": ["Gleich", "als", "hielt'", "ein", "jun\u00b7ges", "Weib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jeder in den Arm geschlossen.", "tokens": ["Je\u00b7der", "in", "den", "Arm", "ge\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sternenauf und sternenab", "tokens": ["Ster\u00b7nen\u00b7auf", "und", "ster\u00b7nen\u00b7ab"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "XY"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tausend leuchtende Gefieder,", "tokens": ["Tau\u00b7send", "leuch\u00b7ten\u00b7de", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Rosen tr\u00e4gt das finstre Grab", "tokens": ["Ro\u00b7sen", "tr\u00e4gt", "das", "finst\u00b7re", "Grab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Kreuze sinken nieder.", "tokens": ["Und", "die", "Kreu\u00b7ze", "sin\u00b7ken", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Duft und Klang und Vogelflug,", "tokens": ["Duft", "und", "Klang", "und", "Vo\u00b7gel\u00b7flug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Balsam, wo die Blicke weilen,", "tokens": ["Bal\u00b7sam", ",", "wo", "die", "Bli\u00b7cke", "wei\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und doch Alles nicht genug,", "tokens": ["Und", "doch", "Al\u00b7les", "nicht", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Um \u2014 ein krankes Volk zu heilen.", "tokens": ["Um", "ein", "kran\u00b7kes", "Volk", "zu", "hei\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "$(", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}