{"textgrid.poem.65375": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Schiffer auf dem Festlande", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor meines Vaters Hause", "tokens": ["Vor", "mei\u00b7nes", "Va\u00b7ters", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht ferne von dem Strand,", "tokens": ["Nicht", "fer\u00b7ne", "von", "dem", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da liegt ein alter Nachen,", "tokens": ["Da", "liegt", "ein", "al\u00b7ter", "Na\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bedeckt mit Schilf und Sand.", "tokens": ["Be\u00b7deckt", "mit", "Schilf", "und", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und wenn die Boote segeln", "tokens": ["Und", "wenn", "die", "Boo\u00b7te", "se\u00b7geln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hinaus zum Heringsfang,", "tokens": ["Hin\u00b7aus", "zum", "He\u00b7rings\u00b7fang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann kracht der alte Nachen", "tokens": ["Dann", "kracht", "der", "al\u00b7te", "Na\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und macht die Fischer bang'.", "tokens": ["Und", "macht", "die", "Fi\u00b7scher", "bang'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "War einst der sch\u00f6nste Nachen,", "tokens": ["War", "einst", "der", "sch\u00f6ns\u00b7te", "Na\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Trug einst den sch\u00f6nsten Mann:", "tokens": ["Trug", "einst", "den", "sch\u00f6ns\u00b7ten", "Mann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Mann verschlang die Woge,", "tokens": ["Den", "Mann", "ver\u00b7schlang", "die", "Wo\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Nachen trieb sie an.", "tokens": ["Den", "Na\u00b7chen", "trieb", "sie", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da lie\u00dfen sie ihn liegen,", "tokens": ["Da", "lie\u00b7\u00dfen", "sie", "ihn", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohin ihn warf die Fluth \u2013", "tokens": ["Wo\u00b7hin", "ihn", "warf", "die", "Fluth", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie l\u00e4g' ich still im Lande", "tokens": ["Wie", "l\u00e4g'", "ich", "still", "im", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit meinem Schifferblut?", "tokens": ["Mit", "mei\u00b7nem", "Schif\u00b7fer\u00b7blut", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}