{"textgrid.poem.35190": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1902", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Horch! Klingt das nicht wie ferner Glockenklang?", "tokens": ["Horch", "!", "Klingt", "das", "nicht", "wie", "fer\u00b7ner", "Glo\u00b7cken\u00b7klang", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PDS", "PTKNEG", "KOKOM", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von Thurm zu Thurm schwingt sich die ernste Kunde:", "tokens": ["Von", "Thurm", "zu", "Thurm", "schwingt", "sich", "die", "erns\u00b7te", "Kun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbes rief mich der, den Keiner noch bezwang;", "tokens": ["\u00bb", "es", "rief", "mich", "der", ",", "den", "Kei\u00b7ner", "noch", "be\u00b7zwang", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ART", "$,", "PRELS", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich segne Dich, mein Volk, in letzter Stunde!\u00ab", "tokens": ["Ich", "seg\u00b7ne", "Dich", ",", "mein", "Volk", ",", "in", "letz\u00b7ter", "Stun\u00b7de", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dort, gegen Morgen, wo der Strahl erwacht,", "tokens": ["Dort", ",", "ge\u00b7gen", "Mor\u00b7gen", ",", "wo", "der", "Strahl", "er\u00b7wacht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "NN", "$,", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den t\u00e4glich uns der lichte Himmel sendet,", "tokens": ["Den", "t\u00e4g\u00b7lich", "uns", "der", "lich\u00b7te", "Him\u00b7mel", "sen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da hat im Kampfe mit der Todesnacht", "tokens": ["Da", "hat", "im", "Kamp\u00b7fe", "mit", "der", "To\u00b7des\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein deutscher Held den schwersten Sieg vollendet.", "tokens": ["Ein", "deut\u00b7scher", "Held", "den", "schwers\u00b7ten", "Sieg", "voll\u00b7en\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Den S\u00e4bel nicht, die Palme in der Hand,", "tokens": ["Den", "S\u00e4\u00b7bel", "nicht", ",", "die", "Pal\u00b7me", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist er uns abermals vorangegangen,", "tokens": ["Ist", "er", "uns", "a\u00b7ber\u00b7mals", "vor\u00b7an\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um dort im hei\u00df erk\u00e4mpften Vaterland", "tokens": ["Um", "dort", "im", "hei\u00df", "er\u00b7k\u00e4mpf\u00b7ten", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ADV", "APPRART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von Gott, wie hier, die Krone zu empfangen.", "tokens": ["Von", "Gott", ",", "wie", "hier", ",", "die", "Kro\u00b7ne", "zu", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch, stieg sein Geist empor im Abendroth,", "tokens": ["Doch", ",", "stieg", "sein", "Geist", "em\u00b7por", "im", "A\u00b7ben\u00b7droth", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPOSAT", "NN", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So kehrt er mit der Morgenr\u00f6the wieder,", "tokens": ["So", "kehrt", "er", "mit", "der", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sieht die Heimath er vom Feind bedroht,", "tokens": ["Und", "sieht", "die", "Hei\u00b7math", "er", "vom", "Feind", "be\u00b7droht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kommt er in heilger R\u00fcstung zu uns nieder.", "tokens": ["Kommt", "er", "in", "heil\u00b7ger", "R\u00fcs\u00b7tung", "zu", "uns", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Er zieht mit seinen Mannen uns voran,", "tokens": ["Er", "zieht", "mit", "sei\u00b7nen", "Man\u00b7nen", "uns", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df er noch lebe, uns zu offenbaren,", "tokens": ["Da\u00df", "er", "noch", "le\u00b7be", ",", "uns", "zu", "of\u00b7fen\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und durch den Kampf f\u00fchrt er zum Sieg uns dann", "tokens": ["Und", "durch", "den", "Kampf", "f\u00fchrt", "er", "zum", "Sieg", "uns", "dann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als Deutschlands Heldengeist mit seinen Schaaren.", "tokens": ["Als", "Deutschlands", "Hel\u00b7den\u00b7geist", "mit", "sei\u00b7nen", "Schaa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}}}}}