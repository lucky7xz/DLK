{"textgrid.poem.42337": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Auge alles Lebens folgt uns nach.", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Auge alles Lebens folgt uns nach.", "tokens": ["Das", "Au\u00b7ge", "al\u00b7les", "Le\u00b7bens", "folgt", "uns", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So wie man oftmals geht und kann nicht scheiden,", "tokens": ["So", "wie", "man", "oft\u00b7mals", "geht", "und", "kann", "nicht", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PIS", "ADV", "VVFIN", "KON", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Worte in sich h\u00f6rt, die niemand sprach,", "tokens": ["Und", "Wor\u00b7te", "in", "sich", "h\u00f6rt", ",", "die", "nie\u00b7mand", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PRF", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und lange fortf\u00e4hrt ein Geschick zu leiden,", "tokens": ["Und", "lan\u00b7ge", "fort\u00b7f\u00e4hrt", "ein", "Ge\u00b7schick", "zu", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wiewohl es l\u00e4ngst sein B\u00fcndnis mit uns brach \u2013", "tokens": ["Wie\u00b7wohl", "es", "l\u00e4ngst", "sein", "B\u00fcnd\u00b7nis", "mit", "uns", "brach", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So h\u00e4lt uns auch im Banne fremdes Sein,", "tokens": ["So", "h\u00e4lt", "uns", "auch", "im", "Ban\u00b7ne", "frem\u00b7des", "Sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "An dem wir eben nur vor\u00fcbergingen,", "tokens": ["An", "dem", "wir", "e\u00b7ben", "nur", "vor\u00b7\u00fc\u00b7ber\u00b7gin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von tausend Bildern lebt ein Widerschein", "tokens": ["Von", "tau\u00b7send", "Bil\u00b7dern", "lebt", "ein", "Wi\u00b7der\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In uns, und von den fernsten Erdendingen", "tokens": ["In", "uns", ",", "und", "von", "den", "ferns\u00b7ten", "Er\u00b7den\u00b7din\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Pr\u00e4gt sich ein sicheres Ged\u00e4chtnis ein.", "tokens": ["Pr\u00e4gt", "sich", "ein", "si\u00b7che\u00b7res", "Ge\u00b7d\u00e4cht\u00b7nis", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Was sich in Dunkel h\u00fcllt, du ahnst es gut:", "tokens": ["Was", "sich", "in", "Dun\u00b7kel", "h\u00fcllt", ",", "du", "ahnst", "es", "gut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die eigne Inbrunst brandet um das Leben.", "tokens": ["Die", "eig\u00b7ne", "In\u00b7brunst", "bran\u00b7det", "um", "das", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie taucht zu dir aus Strom und Meeresflut,", "tokens": ["Sie", "taucht", "zu", "dir", "aus", "Strom", "und", "Mee\u00b7res\u00b7flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mahnt dich im Sturm der Nacht, dich hinzugeben,", "tokens": ["Mahnt", "dich", "im", "Sturm", "der", "Nacht", ",", "dich", "hin\u00b7zu\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und in dir bebt es: Nimm, o nimm mein Blut!", "tokens": ["Und", "in", "dir", "bebt", "es", ":", "Nimm", ",", "o", "nimm", "mein", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PPER", "$.", "NE", "$,", "FM", "VVIMP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}