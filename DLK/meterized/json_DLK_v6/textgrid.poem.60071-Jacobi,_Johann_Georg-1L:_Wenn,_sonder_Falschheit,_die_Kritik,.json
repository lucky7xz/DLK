{"textgrid.poem.60071": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn, sonder Falschheit, die Kritik,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn, sonder Falschheit, die Kritik,", "tokens": ["Wenn", ",", "son\u00b7der", "Falschheit", ",", "die", "Kri\u00b7tik", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "$,", "KON", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Wie du, mit Silbert\u00f6nen redte;", "tokens": ["Wie", "du", ",", "mit", "Sil\u00b7ber\u00b7t\u00f6\u00b7nen", "red\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bey Lob und Tadel deinen Blick", "tokens": ["Bey", "Lob", "und", "Ta\u00b7del", "dei\u00b7nen", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dein getreues L\u00e4cheln h\u00e4tte,", "tokens": ["Und", "dein", "ge\u00b7treu\u00b7es", "L\u00e4\u00b7cheln", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So k\u00f6nnte sie der Musen Schwester seyn,", "tokens": ["So", "k\u00f6nn\u00b7te", "sie", "der", "Mu\u00b7sen", "Schwes\u00b7ter", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Herzen alle sich gewinnen;", "tokens": ["Die", "Her\u00b7zen", "al\u00b7le", "sich", "ge\u00b7win\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So ladeten die Huldg\u00f6ttinnen", "tokens": ["So", "la\u00b7de\u00b7ten", "die", "Huld\u00b7g\u00f6t\u00b7tin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sie selbst zu ihren T\u00e4nzen ein.", "tokens": ["Sie", "selbst", "zu", "ih\u00b7ren", "T\u00e4n\u00b7zen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}