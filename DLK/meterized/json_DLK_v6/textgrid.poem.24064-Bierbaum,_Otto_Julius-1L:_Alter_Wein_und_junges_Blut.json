{"textgrid.poem.24064": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Alter Wein und junges Blut", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alter Wein und junges Blut", "tokens": ["Al\u00b7ter", "Wein", "und", "jun\u00b7ges", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tanzen durcheinander.", "tokens": ["Tan\u00b7zen", "durch\u00b7ein\u00b7an\u00b7der", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Blumenm\u00e4dchen, wirf die Rosen,", "tokens": ["Blu\u00b7men\u00b7m\u00e4d\u00b7chen", ",", "wirf", "die", "Ro\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wirf die Rosen aus dem Schurze,", "tokens": ["Wirf", "die", "Ro\u00b7sen", "aus", "dem", "Schur\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wirf die Rosen auf den Weg!", "tokens": ["Wirf", "die", "Ro\u00b7sen", "auf", "den", "Weg", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sieh, wir taumeln in geschienten", "tokens": ["Sieh", ",", "wir", "tau\u00b7meln", "in", "ge\u00b7schien\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Gliedern mit dem blauen, schweren", "tokens": ["Glie\u00b7dern", "mit", "dem", "blau\u00b7en", ",", "schwe\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Stahlhelm, taumeln wie am hohen", "tokens": ["Stahl\u00b7helm", ",", "tau\u00b7meln", "wie", "am", "ho\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "KOKOM", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Himmel die betrunkenen grauen", "tokens": ["Him\u00b7mel", "die", "be\u00b7trun\u00b7ke\u00b7nen", "grau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "ADJA"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Wolken. (Oder find es Schl\u00e4uche?)", "tokens": ["Wol\u00b7ken", ".", "(", "O\u00b7der", "find", "es", "Schl\u00e4u\u00b7che", "?", ")"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "$(", "KON", "VVFIN", "PPER", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Pfui, wer wird hier stille stehn?", "tokens": ["Pfui", ",", "wer", "wird", "hier", "stil\u00b7le", "stehn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VAFIN", "ADV", "ADJA", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Zwar, dort in den Rhododendren", "tokens": ["Zwar", ",", "dort", "in", "den", "Rho\u00b7do\u00b7den\u00b7dren"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lauern niedertr\u00e4chtige Dornen,", "tokens": ["Lau\u00b7ern", "nie\u00b7der\u00b7tr\u00e4ch\u00b7ti\u00b7ge", "Dor\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und es scheint mir, hopla, M\u00e4dchen,", "tokens": ["Und", "es", "scheint", "mir", ",", "ho\u00b7pla", ",", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "NE", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hinter ihnen schnarcht der Tod.", "tokens": ["Hin\u00b7ter", "ih\u00b7nen", "schnarcht", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Pst! Kommt! Auf den Zehenspitzen", "tokens": ["Pst", "!", "Kommt", "!", "Auf", "den", "Ze\u00b7hen\u00b7spit\u00b7zen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "$.", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schl\u00fcpfen leis wir in den Keller!", "tokens": ["Schl\u00fcp\u00b7fen", "leis", "wir", "in", "den", "Kel\u00b7ler", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo im Sand die spitzen Kr\u00fcge", "tokens": ["Wo", "im", "Sand", "die", "spit\u00b7zen", "Kr\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit den dicken B\u00e4uchen stehn.", "tokens": ["Mit", "den", "di\u00b7cken", "B\u00e4u\u00b7chen", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kommt, wir machen eine Kette!", "tokens": ["Kommt", ",", "wir", "ma\u00b7chen", "ei\u00b7ne", "Ket\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Erst zwei stahlbeschiente Beine,", "tokens": ["Erst", "zwei", "stahl\u00b7be\u00b7schien\u00b7te", "Bei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Dann zwei nackte, schlanke, weiche,", "tokens": ["Dann", "zwei", "nack\u00b7te", ",", "schlan\u00b7ke", ",", "wei\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "CARD", "VVFIN", "$,", "VVFIN", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und so nach der Regel weiter,", "tokens": ["Und", "so", "nach", "der", "Re\u00b7gel", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Die Ovid beschrieben hat.", "tokens": ["Die", "O\u00b7vid", "be\u00b7schrie\u00b7ben", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVPP", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "La\u00dft den Tod im Busche schnarchen!", "tokens": ["La\u00dft", "den", "Tod", "im", "Bu\u00b7sche", "schnar\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn die sehr betrunknen Wolken", "tokens": ["Wenn", "die", "sehr", "be\u00b7trunk\u00b7nen", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich auf Jovis Knieen lagern,", "tokens": ["Sich", "auf", "Jo\u00b7vis", "Kni\u00b7e\u00b7en", "la\u00b7gern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Legen wir uns neben ihn.", "tokens": ["Le\u00b7gen", "wir", "uns", "ne\u00b7ben", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}