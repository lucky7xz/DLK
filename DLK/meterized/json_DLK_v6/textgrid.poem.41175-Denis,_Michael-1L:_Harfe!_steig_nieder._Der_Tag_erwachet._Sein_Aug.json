{"textgrid.poem.41175": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Harfe! steig nieder. Der Tag erwachet. Sein Aug", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Harfe! steig nieder. Der Tag erwachet. Sein Aug", "tokens": ["Har\u00b7fe", "!", "steig", "nie\u00b7der", ".", "Der", "Tag", "er\u00b7wa\u00b7chet", ".", "Sein", "Aug"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$.", "VVFIN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "$.", "PPOSAT", "NN"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Blicket aus Osten auf dich.", "tokens": ["Bli\u00b7cket", "aus", "Os\u00b7ten", "auf", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "APPR", "PPER", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "H\u00f6rst du der Schwalbe Gesch\u00e4ft? Sie lobet schon lang'", "tokens": ["H\u00f6rst", "du", "der", "Schwal\u00b7be", "Ge\u00b7sch\u00e4ft", "?", "Sie", "lo\u00b7bet", "schon", "lang'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "ADV", "ADV"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Oben am Giebel das Licht.", "tokens": ["O\u00b7ben", "am", "Gie\u00b7bel", "das", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "H\u00f6rst du den Morgenhauch", "tokens": ["H\u00f6rst", "du", "den", "Mor\u00b7gen\u00b7hauch"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In dem Gezweig' umher?", "tokens": ["In", "dem", "Ge\u00b7zweig'", "um\u00b7her", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Harfe! steig nieder zu mir, begleite mein Lied!", "tokens": ["Har\u00b7fe", "!", "steig", "nie\u00b7der", "zu", "mir", ",", "be\u00b7glei\u00b7te", "mein", "Lied", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PTKVZ", "APPR", "PPER", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Rein ist das Obergew\u00f6lb der Sch\u00f6pfung und blau,", "tokens": ["Rein", "ist", "das", "O\u00b7ber\u00b7ge\u00b7w\u00f6lb", "der", "Sch\u00f6p\u00b7fung", "und", "blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN", "KON", "ADJD", "$,"], "meter": "++-+--+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "K\u00fchl ist der Odem der Luft.", "tokens": ["K\u00fchl", "ist", "der", "O\u00b7dem", "der", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "D\u00fcnn ist der Schleier von Duft, der \u00fcber der Flur", "tokens": ["D\u00fcnn", "ist", "der", "Schlei\u00b7er", "von", "Duft", ",", "der", "\u00fc\u00b7ber", "der", "Flur"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "APPR", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Tr\u00e4chtigem Busen sich dehnt.", "tokens": ["Tr\u00e4ch\u00b7ti\u00b7gem", "Bu\u00b7sen", "sich", "dehnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Bunt ist der frische Thau,", "tokens": ["Bunt", "ist", "der", "fri\u00b7sche", "Thau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der durch den Schleier blitzt.", "tokens": ["Der", "durch", "den", "Schlei\u00b7er", "blitzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hold ist der Morgen, und hold auch, Barde! f\u00fcr dich.", "tokens": ["Hold", "ist", "der", "Mor\u00b7gen", ",", "und", "hold", "auch", ",", "Bar\u00b7de", "!", "f\u00fcr", "dich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "KON", "ADJD", "ADV", "$,", "NN", "$.", "APPR", "PPER", "$."], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Als dich noch gestern zu Nacht dein Lager umfing,", "tokens": ["Als", "dich", "noch", "ge\u00b7stern", "zu", "Nacht", "dein", "La\u00b7ger", "um\u00b7fing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.2": {"text": "Warst du des Morgens gewi\u00df?", "tokens": ["Warst", "du", "des", "Mor\u00b7gens", "ge\u00b7wi\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADV", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Konnte dein Leben nicht gleich der Rose verbl\u00fch'n,", "tokens": ["Konn\u00b7te", "dein", "Le\u00b7ben", "nicht", "gleich", "der", "Ro\u00b7se", "ver\u00b7bl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Die sich nun nimmer erneut?", "tokens": ["Die", "sich", "nun", "nim\u00b7mer", "er\u00b7neut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "ADV", "ADJD", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Aber Allvaters Huld", "tokens": ["A\u00b7ber", "All\u00b7va\u00b7ters", "Huld"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "L\u00e4\u00dft dich auch heute noch", "tokens": ["L\u00e4\u00dft", "dich", "auch", "heu\u00b7te", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Trinken vom Strome der Lust, der alles berauscht.", "tokens": ["Trin\u00b7ken", "vom", "Stro\u00b7me", "der", "Lust", ",", "der", "al\u00b7les", "be\u00b7rauscht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$,", "PRELS", "PIS", "VVPP", "$."], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "H\u00f6re die Barden der Luft!", "tokens": ["H\u00f6\u00b7re", "die", "Bar\u00b7den", "der", "Luft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Und der hochragende Wald,", "tokens": ["Und", "der", "hoch\u00b7ra\u00b7gen\u00b7de", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Aecker und Ufer und Sumpf, und H\u00fctten und Stadt", "tokens": ["A\u00b7e\u00b7cker", "und", "U\u00b7fer", "und", "Sumpf", ",", "und", "H\u00fct\u00b7ten", "und", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN", "$,", "KON", "NN", "KON", "NN"], "meter": "-+--+--+-+--+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Senden ihr Danklied empor.", "tokens": ["Sen\u00b7den", "ihr", "Dank\u00b7lied", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00f6re der Heerden Dank,", "tokens": ["H\u00f6\u00b7re", "der", "Heer\u00b7den", "Dank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Der sich aus w\u00e4\u00dfrigen", "tokens": ["Der", "sich", "aus", "w\u00e4\u00df\u00b7ri\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Th\u00e4lern, von grasiger Trift, vom H\u00fcgel erhebt.", "tokens": ["Th\u00e4\u00b7lern", ",", "von", "gra\u00b7si\u00b7ger", "Trift", ",", "vom", "H\u00fc\u00b7gel", "er\u00b7hebt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJA", "NN", "$,", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "H\u00f6re den Pfl\u00fcger. Dem Mund entstr\u00f6met ein Lied,", "tokens": ["H\u00f6\u00b7re", "den", "Pfl\u00fc\u00b7ger", ".", "Dem", "Mund", "ent\u00b7str\u00f6\u00b7met", "ein", "Lied", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$.", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.2": {"text": "So wie der Saamen der Hand.", "tokens": ["So", "wie", "der", "Saa\u00b7men", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "H\u00f6re den Hirten. Sein Rohr bl\u00e4st Freuden in's Land.", "tokens": ["H\u00f6\u00b7re", "den", "Hir\u00b7ten", ".", "Sein", "Rohr", "bl\u00e4st", "Freu\u00b7den", "in's", "Land", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$.", "PPOSAT", "NN", "VVFIN", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.4": {"text": "H\u00f6re den Winzer. Er singt.", "tokens": ["H\u00f6\u00b7re", "den", "Win\u00b7zer", ".", "Er", "singt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$.", "PPER", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "H\u00f6re des Hornes Schall,", "tokens": ["H\u00f6\u00b7re", "des", "Hor\u00b7nes", "Schall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Welches der Waidmann weckt.", "tokens": ["Wel\u00b7ches", "der", "Waid\u00b7mann", "weckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "H\u00f6re den Morgengesang der S\u00f6hne der Schlacht!", "tokens": ["H\u00f6\u00b7re", "den", "Mor\u00b7gen\u00b7ge\u00b7sang", "der", "S\u00f6h\u00b7ne", "der", "Schlacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Geber des Morgens! auch ich, ich menge mein Lied", "tokens": ["Ge\u00b7ber", "des", "Mor\u00b7gens", "!", "auch", "ich", ",", "ich", "men\u00b7ge", "mein", "Lied"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADV", "$.", "ADV", "PPER", "$,", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "In die erwachte Natur,", "tokens": ["In", "die", "er\u00b7wach\u00b7te", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich dein geringstes Gesch\u00f6pf! So thaut ein Gew\u00f6lk", "tokens": ["Ich", "dein", "ge\u00b7rings\u00b7tes", "Ge\u00b7sch\u00f6pf", "!", "So", "thaut", "ein", "Ge\u00b7w\u00f6lk"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Einen der Tropfen in's Meer.", "tokens": ["Ei\u00b7nen", "der", "Trop\u00b7fen", "in's", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "H\u00f6re den fr\u00fchen Dank,", "tokens": ["H\u00f6\u00b7re", "den", "fr\u00fc\u00b7hen", "Dank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Den dir mein Harfenspiel,", "tokens": ["Den", "dir", "mein", "Har\u00b7fen\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den dir der lodernde Geist des Barden emp\u00f6rt.", "tokens": ["Den", "dir", "der", "lo\u00b7dern\u00b7de", "Geist", "des", "Bar\u00b7den", "em\u00b7p\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Dein sind die Wunder, womit der Morgen sich schm\u00fcckt,", "tokens": ["Dein", "sind", "die", "Wun\u00b7der", ",", "wo\u00b7mit", "der", "Mor\u00b7gen", "sich", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "$,", "PWAV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Mittag und Abend sich schm\u00fcckt.", "tokens": ["Mit\u00b7tag", "und", "A\u00b7bend", "sich", "schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PRF", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Dein ist der Tag und die Nacht. Du winktest: Sie geh'n.", "tokens": ["Dein", "ist", "der", "Tag", "und", "die", "Nacht", ".", "Du", "wink\u00b7test", ":", "Sie", "geh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "abbreviation"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "KON", "ART", "NN", "$.", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "NE"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Winkest einst wieder: Sie steh'n.", "tokens": ["Win\u00b7kest", "einst", "wie\u00b7der", ":", "Sie", "steh'", "n."], "token_info": ["word", "word", "word", "punct", "word", "word", "abbreviation"], "pos": ["NN", "ADV", "ADV", "$.", "PPER", "VVFIN", "NE"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "O so verschm\u00e4h' es nicht,", "tokens": ["O", "so", "ver\u00b7schm\u00e4h'", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Wenn dir dein S\u00e4nger weiht,", "tokens": ["Wenn", "dir", "dein", "S\u00e4n\u00b7ger", "weiht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Was er aus Gnade von dir, Allvater! geneu\u00dft.", "tokens": ["Was", "er", "aus", "Gna\u00b7de", "von", "dir", ",", "All\u00b7va\u00b7ter", "!", "ge\u00b7neu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "APPR", "PPER", "$,", "NN", "$.", "VVPP", "$."], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}}, "stanza.8": {"line.1": {"text": "Dein sey der heutige Tag. Dein wachendes Aug,", "tokens": ["Dein", "sey", "der", "heu\u00b7ti\u00b7ge", "Tag", ".", "Dein", "wa\u00b7chen\u00b7des", "Aug", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "ADJA", "NN", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das mir den Busen durchschaut,", "tokens": ["Das", "mir", "den", "Bu\u00b7sen", "durch\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sehe kein steigend Gew\u00f6lk von niedriger Lust,", "tokens": ["Se\u00b7he", "kein", "stei\u00b7gend", "Ge\u00b7w\u00f6lk", "von", "nied\u00b7ri\u00b7ger", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJD", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Keines von schn\u00f6der Begier.", "tokens": ["Kei\u00b7nes", "von", "schn\u00f6\u00b7der", "Be\u00b7gier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Ueber sein still Gesicht", "tokens": ["Ue\u00b7ber", "sein", "still", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Schwelle kein brausender", "tokens": ["Schwel\u00b7le", "kein", "brau\u00b7sen\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["NN", "PIAT", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Hochmuth, und kreuze kein Blitz des Zornes vor dir!", "tokens": ["Hoch\u00b7muth", ",", "und", "kreu\u00b7ze", "kein", "Blitz", "des", "Zor\u00b7nes", "vor", "dir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "VVFIN", "PIAT", "NN", "ART", "NN", "APPR", "PPER", "$."], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Dein sey mein Denken, und dein sey jeglicher Trieb,", "tokens": ["Dein", "sey", "mein", "Den\u00b7ken", ",", "und", "dein", "sey", "jeg\u00b7li\u00b7cher", "Trieb", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der mir im Herzen erwacht.", "tokens": ["Der", "mir", "im", "Her\u00b7zen", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Reift der Gedanke, der Trieb zur Stimme, zur That.", "tokens": ["Reift", "der", "Ge\u00b7dan\u00b7ke", ",", "der", "Trieb", "zur", "Stim\u00b7me", ",", "zur", "That", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Dein sey die Stimme, die That.", "tokens": ["Dein", "sey", "die", "Stim\u00b7me", ",", "die", "That", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Du hast den Baum gepflanzt,", "tokens": ["Du", "hast", "den", "Baum", "ge\u00b7pflanzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "W\u00e4ren die Fr\u00fcchte nun,", "tokens": ["W\u00e4\u00b7ren", "die", "Fr\u00fcch\u00b7te", "nun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Die er, gepflogen von dir, erzeuget, nicht dein?", "tokens": ["Die", "er", ",", "ge\u00b7pflo\u00b7gen", "von", "dir", ",", "er\u00b7zeu\u00b7get", ",", "nicht", "dein", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "VVPP", "APPR", "PPER", "$,", "VVFIN", "$,", "PTKNEG", "PPOSAT", "$."], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "La\u00df ihn auch schatten den Baum, gepflanzet von dir,", "tokens": ["La\u00df", "ihn", "auch", "schat\u00b7ten", "den", "Baum", ",", "ge\u00b7pflan\u00b7zet", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "VVFIN", "ART", "NN", "$,", "VVPP", "APPR", "PPER", "$,"], "meter": "---+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Schatten, gepflogen von dir;", "tokens": ["Schat\u00b7ten", ",", "ge\u00b7pflo\u00b7gen", "von", "dir", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "APPR", "PPER", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Da\u00df ihn der Wandrer und Hirt erkiese zu ruh'n,", "tokens": ["Da\u00df", "ihn", "der", "Wand\u00b7rer", "und", "Hirt", "er\u00b7kie\u00b7se", "zu", "ruh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "KON", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wenn sich der Mittag entflammt.", "tokens": ["Wenn", "sich", "der", "Mit\u00b7tag", "ent\u00b7flammt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Gib mir die seligste", "tokens": ["Gib", "mir", "die", "se\u00b7ligs\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wollust auch heut, wie du,", "tokens": ["Wol\u00b7lust", "auch", "heut", ",", "wie", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$,", "PWAV", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Meinem Geschlechte zu Rath und Hilfe zu seyn!", "tokens": ["Mei\u00b7nem", "Ge\u00b7schlech\u00b7te", "zu", "Rath", "und", "Hil\u00b7fe", "zu", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "KON", "NN", "PTKZU", "VAINF", "$."], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "S\u00e4\u00df' ich auch denkend, und schw\u00f6ll' ein r\u00fchmliches Lied", "tokens": ["S\u00e4\u00df'", "ich", "auch", "den\u00b7kend", ",", "und", "schw\u00f6ll'", "ein", "r\u00fchm\u00b7li\u00b7ches", "Lied"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "VVPP", "$,", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Hoch mir im Busen, und itzt", "tokens": ["Hoch", "mir", "im", "Bu\u00b7sen", ",", "und", "itzt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "PPER", "APPRART", "NN", "$,", "KON", "ADV"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "F\u00fchr' ich zur Harfe, da tr\u00e4t' ein J\u00fcngling zu mir", "tokens": ["F\u00fchr'", "ich", "zur", "Har\u00b7fe", ",", "da", "tr\u00e4t'", "ein", "J\u00fcng\u00b7ling", "zu", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPRART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "APPR", "PPER"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Lehrbed\u00fcrftig heran;", "tokens": ["Lehr\u00b7be\u00b7d\u00fcrf\u00b7tig", "he\u00b7ran", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "La\u00df mich den Durst nach Ruhm,", "tokens": ["La\u00df", "mich", "den", "Durst", "nach", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Welcher im Liede wohnt,", "tokens": ["Wel\u00b7cher", "im", "Lie\u00b7de", "wohnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPRART", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "D\u00e4mpfen, ein Leiter und Aug der Jugend zu seyn.", "tokens": ["D\u00e4mp\u00b7fen", ",", "ein", "Lei\u00b7ter", "und", "Aug", "der", "Ju\u00b7gend", "zu", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "KON", "NN", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Strecke den schirmenden Arm, Allvater! auch heut", "tokens": ["Stre\u00b7cke", "den", "schir\u00b7men\u00b7den", "Arm", ",", "All\u00b7va\u00b7ter", "!", "auch", "heut"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "NN", "$.", "ADV", "ADV"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ueber die Mutter des Volks,", "tokens": ["Ue\u00b7ber", "die", "Mut\u00b7ter", "des", "Volks", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Ueber Theresien aus. Sie sehe der Tag", "tokens": ["Ue\u00b7ber", "The\u00b7re\u00b7si\u00b7en", "aus", ".", "Sie", "se\u00b7he", "der", "Tag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "PTKVZ", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Wieder dir gleicher durch Huld.", "tokens": ["Wie\u00b7der", "dir", "glei\u00b7cher", "durch", "Huld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Ueber den Heldenfreund", "tokens": ["Ue\u00b7ber", "den", "Hel\u00b7den\u00b7freund"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Joseph erhelle sich,", "tokens": ["Jo\u00b7se\u00b7ph", "er\u00b7hel\u00b7le", "sich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "So wie der Morgen, der Ruhm im Erbe von Teut.", "tokens": ["So", "wie", "der", "Mor\u00b7gen", ",", "der", "Ruhm", "im", "Er\u00b7be", "von", "Teut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "$,", "ART", "NN", "APPRART", "NN", "APPR", "NE", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.13": {"line.1": {"text": "Lenke die M\u00e4nner des Raths, Allweiser! auch heut,", "tokens": ["Len\u00b7ke", "die", "M\u00e4n\u00b7ner", "des", "Raths", ",", "All\u00b7wei\u00b7ser", "!", "auch", "heut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "NN", "$,", "NN", "$.", "ADV", "ADV", "$,"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Lenke die M\u00e4nner des Rechts,", "tokens": ["Len\u00b7ke", "die", "M\u00e4n\u00b7ner", "des", "Rechts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Lenke die Lehrer des Volks, da\u00df keinen sein Herz", "tokens": ["Len\u00b7ke", "die", "Leh\u00b7rer", "des", "Volks", ",", "da\u00df", "kei\u00b7nen", "sein", "Herz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "ART", "NN", "$,", "KOUS", "PIAT", "PPOSAT", "NN"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Einstens verklage vor dir.", "tokens": ["Eins\u00b7tens", "ver\u00b7kla\u00b7ge", "vor", "dir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Trockne der leidenden", "tokens": ["Trock\u00b7ne", "der", "lei\u00b7den\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "ADJA"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.6": {"text": "Unschuld die Thr\u00e4nen heut.", "tokens": ["Un\u00b7schuld", "die", "Thr\u00e4\u00b7nen", "heut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "H\u00f6re die Stimme der Noth, Erbarmer! und hilf!", "tokens": ["H\u00f6\u00b7re", "die", "Stim\u00b7me", "der", "Noth", ",", "Er\u00b7bar\u00b7mer", "!", "und", "hilf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "NN", "$,", "NN", "$.", "KON", "VVIMP", "$."], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}}, "stanza.14": {"line.1": {"text": "Also beginn' ich den Tag, Allvater! vor dir!", "tokens": ["Al\u00b7so", "be\u00b7ginn'", "ich", "den", "Tag", ",", "All\u00b7va\u00b7ter", "!", "vor", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "NN", "$.", "APPR", "PPER", "$."], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ist es mein letzter, wohlan!", "tokens": ["Ist", "es", "mein", "letz\u00b7ter", ",", "wo\u00b7hlan", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "ADJA", "$,", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Nimm ihn den betenden Geist. Du schufst ihn. Und nimm,", "tokens": ["Nimm", "ihn", "den", "be\u00b7ten\u00b7den", "Geist", ".", "Du", "schufst", "ihn", ".", "Und", "nimm", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPER", "$.", "KON", "VVIMP", "$,"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Erde! du gabst ihn, den Leib.", "tokens": ["Er\u00b7de", "!", "du", "gabst", "ihn", ",", "den", "Leib", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPER", "$,", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Aber erg\u00f6tzet mich", "tokens": ["A\u00b7ber", "er\u00b7g\u00f6t\u00b7zet", "mich"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PPER"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Nochmal ein Abendroth,", "tokens": ["Noch\u00b7mal", "ein", "A\u00b7ben\u00b7droth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.7": {"text": "O dann erhebt sich mein Dank auf Schwingen des Wests.", "tokens": ["O", "dann", "er\u00b7hebt", "sich", "mein", "Dank", "auf", "Schwin\u00b7gen", "des", "Wests", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "PRF", "PPOSAT", "NN", "APPR", "NN", "ART", "NN", "$."], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}}}}}