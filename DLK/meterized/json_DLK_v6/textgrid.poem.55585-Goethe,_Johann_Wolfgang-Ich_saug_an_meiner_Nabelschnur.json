{"textgrid.poem.55585": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Ich saug an meiner Nabelschnur", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich saug an meiner Nabelschnur", "tokens": ["Ich", "saug", "an", "mei\u00b7ner", "Na\u00b7bel\u00b7schnur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun Nahrung aus der Welt.", "tokens": ["Nun", "Nah\u00b7rung", "aus", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und herrlich rings ist die Natur,", "tokens": ["Und", "herr\u00b7lich", "rings", "ist", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die mich am Busen h\u00e4lt!", "tokens": ["Die", "mich", "am", "Bu\u00b7sen", "h\u00e4lt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Welle wieget unsern Kahn", "tokens": ["Die", "Wel\u00b7le", "wie\u00b7get", "un\u00b7sern", "Kahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Rudertakt hinauf,", "tokens": ["Im", "Ru\u00b7der\u00b7takt", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und Berge, wolkenangetan,", "tokens": ["Und", "Ber\u00b7ge", ",", "wol\u00b7ken\u00b7an\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "$,", "PWAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Entgegnen unserm Lauf.", "tokens": ["Ent\u00b7geg\u00b7nen", "un\u00b7serm", "Lauf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Aug, mein Aug, was sinkst du nieder?", "tokens": ["Aug", ",", "mein", "Aug", ",", "was", "sinkst", "du", "nie\u00b7der", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "PWS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Goldne Tr\u00e4ume, kommt ihr wieder?", "tokens": ["Gold\u00b7ne", "Tr\u00e4u\u00b7me", ",", "kommt", "ihr", "wie\u00b7der", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weg, du Traum! so gold du bist;", "tokens": ["Weg", ",", "du", "Traum", "!", "so", "gold", "du", "bist", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "NN", "$.", "ADV", "ADJD", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier auch Lieb und Leben ist.", "tokens": ["Hier", "auch", "Lieb", "und", "Le\u00b7ben", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "KON", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf der Welle blinken", "tokens": ["Auf", "der", "Wel\u00b7le", "blin\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Tausend schwebende Sterne,", "tokens": ["Tau\u00b7send", "schwe\u00b7ben\u00b7de", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Liebe Nebel trinken", "tokens": ["Lie\u00b7be", "Ne\u00b7bel", "trin\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["NN", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Rings die t\u00fcrmende Ferne,", "tokens": ["Rings", "die", "t\u00fcr\u00b7men\u00b7de", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Morgenwind umfl\u00fcgelt,", "tokens": ["Mor\u00b7gen\u00b7wind", "um\u00b7fl\u00fc\u00b7gelt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Die beschattete Bucht,", "tokens": ["Die", "be\u00b7schat\u00b7te\u00b7te", "Bucht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Und im See bespiegelt", "tokens": ["Und", "im", "See", "be\u00b7spie\u00b7gelt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Sich die reifende Frucht.", "tokens": ["Sich", "die", "rei\u00b7fen\u00b7de", "Frucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}