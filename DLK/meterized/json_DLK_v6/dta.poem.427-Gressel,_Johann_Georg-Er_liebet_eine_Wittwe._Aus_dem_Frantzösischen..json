{"dta.poem.427": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er liebet eine Wittwe.  \n Aus dem Frantz\u00f6sischen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Liebet welche die ihr wolt/ euch sind alle frey gegeben/", "tokens": ["Lie\u00b7bet", "wel\u00b7che", "die", "ihr", "wolt", "/", "euch", "sind", "al\u00b7le", "frey", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRELS", "PPER", "VMFIN", "$(", "PPER", "VAFIN", "PIS", "ADJD", "VVPP", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Mein Hertz aber soll allzeit nur in einer Wittwen leben/", "tokens": ["Mein", "Hertz", "a\u00b7ber", "soll", "all\u00b7zeit", "nur", "in", "ei\u00b7ner", "Witt\u00b7wen", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VMFIN", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Der ein heisses Seuffzen steitig aus der Quell des Hertzens", "tokens": ["Der", "ein", "heis\u00b7ses", "Seuff\u00b7zen", "stei\u00b7tig", "aus", "der", "Quell", "des", "Hert\u00b7zens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "steigt/", "tokens": ["steigt", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Und die sich als schon bewandert besser zu den L\u00fcsten neigt.", "tokens": ["Und", "die", "sich", "als", "schon", "be\u00b7wan\u00b7dert", "bes\u00b7ser", "zu", "den", "L\u00fcs\u00b7ten", "neigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PRF", "KOKOM", "ADV", "ADJD", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Wenn der Wein die Wirckung thut/ da\u00df er in die Geister", "tokens": ["Wenn", "der", "Wein", "die", "Wir\u00b7ckung", "thut", "/", "da\u00df", "er", "in", "die", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$(", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "kommen/", "tokens": ["kom\u00b7men", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Denn laufft man der Tonnen zu/ wo er ist heraus genommen/", "tokens": ["Denn", "laufft", "man", "der", "Ton\u00b7nen", "zu", "/", "wo", "er", "ist", "he\u00b7raus", "ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ART", "NN", "PTKZU", "$(", "PWAV", "PPER", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Und man macht sich kein Bedencken braff zu trincken von dem", "tokens": ["Und", "man", "macht", "sich", "kein", "Be\u00b7den\u00b7cken", "braff", "zu", "trin\u00b7cken", "von", "dem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "PRF", "PIAT", "NN", "VVFIN", "PTKZU", "VVINF", "APPR", "ART"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.10": {"text": "Wein/", "tokens": ["Wein", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Ob ein ander schon der Tonnen erst der Bohrer m\u00fcssen seyn.", "tokens": ["Ob", "ein", "an\u00b7der", "schon", "der", "Ton\u00b7nen", "erst", "der", "Boh\u00b7rer", "m\u00fcs\u00b7sen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "ADV", "ART", "NN", "ADV", "ART", "NN", "VMFIN", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}