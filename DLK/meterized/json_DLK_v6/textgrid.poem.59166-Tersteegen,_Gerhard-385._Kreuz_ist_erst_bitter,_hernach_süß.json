{"textgrid.poem.59166": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "385. Kreuz ist erst bitter, hernach s\u00fc\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn 's Kreuz im Hause nicht regiert,", "tokens": ["Wenn", "'s", "Kreuz", "im", "Hau\u00b7se", "nicht", "re\u00b7giert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "APPRART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "So hat dein H\u00fcttchen keine Freuden;", "tokens": ["So", "hat", "dein", "H\u00fctt\u00b7chen", "kei\u00b7ne", "Freu\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Bitterkeit wird's drein gef\u00fchrt,", "tokens": ["Mit", "Bit\u00b7ter\u00b7keit", "wird's", "drein", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch, lieb es nur, so bringt es S\u00fc\u00dfigkeiten!", "tokens": ["Doch", ",", "lieb", "es", "nur", ",", "so", "bringt", "es", "S\u00fc\u00b7\u00dfig\u00b7kei\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "PPER", "ADV", "$,", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}