{"dta.poem.6848": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Ort ist aber nicht gar leicht zu finden,", "tokens": ["Der", "Ort", "ist", "a\u00b7ber", "nicht", "gar", "leicht", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKNEG", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch eigentlich die Stelle zu ergr\u00fcnden.", "tokens": ["Noch", "ei\u00b7gent\u00b7lich", "die", "Stel\u00b7le", "zu", "er\u00b7gr\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Doch mag es, oder nicht die Glandul seyn,", "tokens": ["Doch", "mag", "es", ",", "o\u00b7der", "nicht", "die", "Glan\u00b7dul", "seyn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "$,", "KON", "PTKNEG", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Pinealis insgemein", "tokens": ["Die", "Pi\u00b7nea\u00b7lis", "ins\u00b7ge\u00b7mein"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Genennet wird, so stimmt doch dem ein jeder bey,", "tokens": ["Ge\u00b7nen\u00b7net", "wird", ",", "so", "stimmt", "doch", "dem", "ein", "je\u00b7der", "bey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "ADV", "VVFIN", "ADV", "ART", "ART", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df es ein solcher Ort, der ihr nicht ungleich, sey.", "tokens": ["Da\u00df", "es", "ein", "sol\u00b7cher", "Ort", ",", "der", "ihr", "nicht", "un\u00b7gleich", ",", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "PIAT", "NN", "$,", "PRELS", "PPER", "PTKNEG", "ADJD", "$,", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Nerven F\u00e4den sind allhier so zart und klein,", "tokens": ["Der", "Ner\u00b7ven", "F\u00e4\u00b7den", "sind", "all\u00b7hier", "so", "zart", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Verwirren sich und sind fast nicht zu sp\u00fcren,", "tokens": ["Ver\u00b7wir\u00b7ren", "sich", "und", "sind", "fast", "nicht", "zu", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "KON", "VAFIN", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ja scheinen sich fast g\u00e4ntzlich zu verlieren.", "tokens": ["Ja", "schei\u00b7nen", "sich", "fast", "g\u00e4ntz\u00b7lich", "zu", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "PRF", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Dis mu\u00df nun eigentlich der unsichtbare Sitz,", "tokens": ["Dis", "mu\u00df", "nun", "ei\u00b7gent\u00b7lich", "der", "un\u00b7sicht\u00b7ba\u00b7re", "Sitz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+--+---++--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Das P\u00fcnctgen der Verbindung seyn,", "tokens": ["Das", "P\u00fcnct\u00b7gen", "der", "Ver\u00b7bin\u00b7dung", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Woselbst, da sie des Eindrucks W\u00fcrckung sp\u00fcret,", "tokens": ["Wo\u00b7selbst", ",", "da", "sie", "des", "Ein\u00b7drucks", "W\u00fcr\u00b7ckung", "sp\u00fc\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Die Seele f\u00fchlbar ist, und wo sie wird ger\u00fchret:", "tokens": ["Die", "See\u00b7le", "f\u00fchl\u00b7bar", "ist", ",", "und", "wo", "sie", "wird", "ge\u00b7r\u00fch\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "$,", "KON", "PWAV", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Bewegungen, von c\u00f6rperlichen Sachen", "tokens": ["Be\u00b7we\u00b7gun\u00b7gen", ",", "von", "c\u00f6r\u00b7per\u00b7li\u00b7chen", "Sa\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ADJA", "NN"], "meter": "--+--+---+-", "measure": "anapaest.di.plus"}, "line.15": {"text": "Die m\u00fcssen dort ein geistigs Mahlwerck machen,", "tokens": ["Die", "m\u00fcs\u00b7sen", "dort", "ein", "geis\u00b7tigs", "Mahl\u00b7werck", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "So da\u00df die Seele hier, die mit dem Leib sich bindet,", "tokens": ["So", "da\u00df", "die", "See\u00b7le", "hier", ",", "die", "mit", "dem", "Leib", "sich", "bin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "ADV", "$,", "PRELS", "APPR", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der Sinnen Harmonie, auch ihre selbst, empfindet.", "tokens": ["Der", "Sin\u00b7nen", "Har\u00b7mo\u00b7nie", ",", "auch", "ih\u00b7re", "selbst", ",", "emp\u00b7fin\u00b7det", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADV", "PPOSAT", "ADV", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}