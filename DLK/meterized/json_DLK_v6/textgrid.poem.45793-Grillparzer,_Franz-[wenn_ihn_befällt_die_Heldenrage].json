{"textgrid.poem.45793": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[wenn ihn bef\u00e4llt die Heldenrage]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ihn bef\u00e4llt die Heldenrage,", "tokens": ["Wenn", "ihn", "be\u00b7f\u00e4llt", "die", "Hel\u00b7den\u00b7ra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Trotzt er der wilden Meeresflut,", "tokens": ["Trotzt", "er", "der", "wil\u00b7den", "Mee\u00b7res\u00b7flut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Die Unbesonnenheit gibt Courage,", "tokens": ["Die", "Un\u00b7be\u00b7son\u00b7nen\u00b7heit", "gibt", "Cou\u00b7ra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nur der Besonnene hat Mut.", "tokens": ["Nur", "der", "Be\u00b7son\u00b7ne\u00b7ne", "hat", "Mut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}