{"textgrid.poem.47176": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zwei Paar schwarze Augen haben", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei Paar schwarze Augen haben", "tokens": ["Zwei", "Paar", "schwar\u00b7ze", "Au\u00b7gen", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "ADJA", "NN", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wir gebracht zusammen,", "tokens": ["Wir", "ge\u00b7bracht", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wie sie uns die M\u00fctter gaben,", "tokens": ["Wie", "sie", "uns", "die", "M\u00fct\u00b7ter", "ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denen wir entstammen.", "tokens": ["De\u00b7nen", "wir", "ent\u00b7stam\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Und so ist es nun kein Wunder,", "tokens": ["Und", "so", "ist", "es", "nun", "kein", "Wun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df auch diese Knaben", "tokens": ["Da\u00df", "auch", "die\u00b7se", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PDAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mit den Feuerblicken zunder-", "tokens": ["Mit", "den", "Feu\u00b7er\u00b7bli\u00b7cken", "zun\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "TRUNC"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "schwarze Augen haben.", "tokens": ["schwar\u00b7ze", "Au\u00b7gen", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Nur dem einen sind die Sterne", "tokens": ["Nur", "dem", "ei\u00b7nen", "sind", "die", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "PIS", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hell im Blau erglommen,", "tokens": ["Hell", "im", "Blau", "er\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und du m\u00f6chtest wissen gerne,", "tokens": ["Und", "du", "m\u00f6ch\u00b7test", "wis\u00b7sen", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "VVINF", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo er's her genommen.", "tokens": ["Wo", "er's", "her", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Wiss' es denn! von meinem Vater,", "tokens": ["Wiss'", "es", "denn", "!", "von", "mei\u00b7nem", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$.", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der sie so einst hatte.", "tokens": ["Der", "sie", "so", "einst", "hat\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VAFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Freu' im Himmel ihn sein spater", "tokens": ["Freu'", "im", "Him\u00b7mel", "ihn", "sein", "spa\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "PPER", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wiederschein und Schatte!", "tokens": ["Wie\u00b7der\u00b7schein", "und", "Schat\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "--+-+-", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Tr\u00f6stlich ist's und l\u00e4sset ahnen", "tokens": ["Tr\u00f6st\u00b7lich", "ist's", "und", "l\u00e4s\u00b7set", "ah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "KON", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Irdisch-ew'ges Leben,", "tokens": ["Ir\u00b7dische\u00b7w'\u00b7ges", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wie in Enkelz\u00fcgen Ahnen-", "tokens": ["Wie", "in", "En\u00b7kel\u00b7z\u00fc\u00b7gen", "Ah\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "TRUNC"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bilder fort sich weben.", "tokens": ["Bil\u00b7der", "fort", "sich", "we\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "PRF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Wenn ich selbst vom Vater habe", "tokens": ["Wenn", "ich", "selbst", "vom", "Va\u00b7ter", "ha\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wen'ger miterhalten,", "tokens": ["Wen'\u00b7ger", "mi\u00b7ter\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Freut's mich mehr nur, da\u00df der Knabe", "tokens": ["Freut's", "mich", "mehr", "nur", ",", "da\u00df", "der", "Kna\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPER", "ADV", "ADV", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mehr hat von dem Alten.", "tokens": ["Mehr", "hat", "von", "dem", "Al\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Und wie ich mich selbst vor Augen", "tokens": ["Und", "wie", "ich", "mich", "selbst", "vor", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "PRF", "ADV", "APPR", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Hab' in meinen Knaben,", "tokens": ["Hab'", "in", "mei\u00b7nen", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Will in einem ich vor Augen", "tokens": ["Will", "in", "ei\u00b7nem", "ich", "vor", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ART", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meinen Vater haben.", "tokens": ["Mei\u00b7nen", "Va\u00b7ter", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}