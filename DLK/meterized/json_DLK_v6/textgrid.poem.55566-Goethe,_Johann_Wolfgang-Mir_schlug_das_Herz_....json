{"textgrid.poem.55566": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Mir schlug das Herz ...", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir schlug das Herz, geschwind zu Pferde!", "tokens": ["Mir", "schlug", "das", "Herz", ",", "ge\u00b7schwind", "zu", "Pfer\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und fort! wild, wie ein Held zur Schlacht.", "tokens": ["Und", "fort", "!", "wild", ",", "wie", "ein", "Held", "zur", "Schlacht", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "$.", "ADJD", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Abend wiegte schon die Erde,", "tokens": ["Der", "A\u00b7bend", "wieg\u00b7te", "schon", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und an den Bergen hing die Nacht;", "tokens": ["Und", "an", "den", "Ber\u00b7gen", "hing", "die", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schon stund im Nebelkleid die Eiche", "tokens": ["Schon", "stund", "im", "Ne\u00b7bel\u00b7kleid", "die", "Ei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein aufget\u00fcrmter Riese da,", "tokens": ["Ein", "auf\u00b7ge\u00b7t\u00fcrm\u00b7ter", "Rie\u00b7se", "da", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo Finsternis aus dem Gestr\u00e4uche", "tokens": ["Wo", "Fins\u00b7ter\u00b7nis", "aus", "dem", "Ge\u00b7str\u00e4u\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit hundert schwarzen Augen sah.", "tokens": ["Mit", "hun\u00b7dert", "schwar\u00b7zen", "Au\u00b7gen", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Mond von seinem Wolkenh\u00fcgel", "tokens": ["Der", "Mond", "von", "sei\u00b7nem", "Wol\u00b7ken\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schien kl\u00e4glich aus dem Duft hervor,", "tokens": ["Schien", "kl\u00e4g\u00b7lich", "aus", "dem", "Duft", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Winde schwangen leise Fl\u00fcgel,", "tokens": ["Die", "Win\u00b7de", "schwan\u00b7gen", "lei\u00b7se", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Umsausten schauerlich mein Ohr;", "tokens": ["Um\u00b7saus\u00b7ten", "schau\u00b7er\u00b7lich", "mein", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Nacht schuf tausend Ungeheuer;", "tokens": ["Die", "Nacht", "schuf", "tau\u00b7send", "Un\u00b7ge\u00b7heu\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch tausendfacher war mein Mut:", "tokens": ["Doch", "tau\u00b7send\u00b7fa\u00b7cher", "war", "mein", "Mut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mein Geist war ein verzehrend Feuer,", "tokens": ["Mein", "Geist", "war", "ein", "ver\u00b7zeh\u00b7rend", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mein ganzes Herz zerflo\u00df in Glut.", "tokens": ["Mein", "gan\u00b7zes", "Herz", "zer\u00b7flo\u00df", "in", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich sah dich, und die milde Freude", "tokens": ["Ich", "sah", "dich", ",", "und", "die", "mil\u00b7de", "Freu\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Flo\u00df aus dem s\u00fc\u00dfen Blick auf mich;", "tokens": ["Flo\u00df", "aus", "dem", "s\u00fc\u00b7\u00dfen", "Blick", "auf", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ganz war mein Herz an deiner Seite", "tokens": ["Ganz", "war", "mein", "Herz", "an", "dei\u00b7ner", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und jeder Atemzug f\u00fcr dich.", "tokens": ["Und", "je\u00b7der", "A\u00b7tem\u00b7zug", "f\u00fcr", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein rosenfarbes Fr\u00fchlingswetter", "tokens": ["Ein", "ro\u00b7sen\u00b7far\u00b7bes", "Fr\u00fch\u00b7lings\u00b7wet\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Lag auf dem lieblichen Gesicht,", "tokens": ["Lag", "auf", "dem", "lieb\u00b7li\u00b7chen", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und Z\u00e4rtlichkeit f\u00fcr mich \u2013 ihr G\u00f6tter!", "tokens": ["Und", "Z\u00e4rt\u00b7lich\u00b7keit", "f\u00fcr", "mich", "\u2013", "ihr", "G\u00f6t\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPER", "$(", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich hofft es, ich verdient es nicht!", "tokens": ["Ich", "hofft", "es", ",", "ich", "ver\u00b7dient", "es", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Abschied, wie bedr\u00e4ngt, wie tr\u00fcbe!", "tokens": ["Der", "Ab\u00b7schied", ",", "wie", "be\u00b7dr\u00e4ngt", ",", "wie", "tr\u00fc\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "VVPP", "$,", "PWAV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus deinen Blicken sprach dein Herz.", "tokens": ["Aus", "dei\u00b7nen", "Bli\u00b7cken", "sprach", "dein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In deinen K\u00fcssen welche Liebe!", "tokens": ["In", "dei\u00b7nen", "K\u00fcs\u00b7sen", "wel\u00b7che", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PWAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O welche Wonne, welcher Schmerz!", "tokens": ["O", "wel\u00b7che", "Won\u00b7ne", ",", "wel\u00b7cher", "Schmerz", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PWAT", "NN", "$,", "PWAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du gingst, ich stund und sah zur Erden", "tokens": ["Du", "gingst", ",", "ich", "stund", "und", "sah", "zur", "Er\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "ADJD", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und sah dir nach mit nassem Blick;", "tokens": ["Und", "sah", "dir", "nach", "mit", "nas\u00b7sem", "Blick", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und doch, welch Gl\u00fcck! geliebt zu werden,", "tokens": ["Und", "doch", ",", "welch", "Gl\u00fcck", "!", "ge\u00b7liebt", "zu", "wer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAT", "NN", "$.", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und lieben, G\u00f6tter, welch ein Gl\u00fcck!", "tokens": ["Und", "lie\u00b7ben", ",", "G\u00f6t\u00b7ter", ",", "welch", "ein", "Gl\u00fcck", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "NN", "$,", "PWAT", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}