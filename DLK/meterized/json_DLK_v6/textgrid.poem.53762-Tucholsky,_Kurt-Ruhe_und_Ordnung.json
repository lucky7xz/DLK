{"textgrid.poem.53762": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Ruhe und Ordnung", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Millionen arbeiten, ohne zu leben,", "tokens": ["Wenn", "Mil\u00b7lion\u00b7en", "ar\u00b7bei\u00b7ten", ",", "oh\u00b7ne", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVINF", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "wenn M\u00fctter den Kindern nur Milchwasser geben \u2013", "tokens": ["wenn", "M\u00fct\u00b7ter", "den", "Kin\u00b7dern", "nur", "Milch\u00b7was\u00b7ser", "ge\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "ADV", "NN", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "das ist Ordnung.", "tokens": ["das", "ist", "Ord\u00b7nung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Wenn Werkleute rufen: \u00bbLa\u00dft uns ans Licht!", "tokens": ["Wenn", "Wer\u00b7kleu\u00b7te", "ru\u00b7fen", ":", "\u00bb", "La\u00dft", "uns", "ans", "Licht", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVINF", "$.", "$(", "VVIMP", "PPER", "APPRART", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wer Arbeit stiehlt, der mu\u00df vors Gericht!\u00ab", "tokens": ["Wer", "Ar\u00b7beit", "stiehlt", ",", "der", "mu\u00df", "vors", "Ge\u00b7richt", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "NN", "VVFIN", "$,", "ART", "VMFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.6": {"text": "Das ist Unordnung.", "tokens": ["Das", "ist", "Un\u00b7ord\u00b7nung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wenn Tuberkul\u00f6se zur Drehbank rennen,", "tokens": ["Wenn", "Tu\u00b7ber\u00b7ku\u00b7l\u00f6\u00b7se", "zur", "Dreh\u00b7bank", "ren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "wenn dreizehn in einer Stube pennen \u2013", "tokens": ["wenn", "drei\u00b7zehn", "in", "ei\u00b7ner", "Stu\u00b7be", "pen\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "das ist Ordnung.", "tokens": ["das", "ist", "Ord\u00b7nung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Wenn einer ausbricht mit Gebr\u00fcll,", "tokens": ["Wenn", "ei\u00b7ner", "aus\u00b7bricht", "mit", "Ge\u00b7br\u00fcll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "weil er sein Alter sichern will \u2013", "tokens": ["weil", "er", "sein", "Al\u00b7ter", "si\u00b7chern", "will", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "das ist Unordnung.", "tokens": ["das", "ist", "Un\u00b7ord\u00b7nung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Wenn reiche Erben im schweizer Schnee", "tokens": ["Wenn", "rei\u00b7che", "Er\u00b7ben", "im", "schwei\u00b7zer", "Schnee"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "jubeln \u2013 und sommers am Comer See \u2013", "tokens": ["ju\u00b7beln", "\u2013", "und", "som\u00b7mers", "am", "Co\u00b7mer", "See", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KON", "ADV", "APPRART", "NN", "NN", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "dann herrscht Ruhe.", "tokens": ["dann", "herrscht", "Ru\u00b7he", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Wenn Gefahr besteht, da\u00df sich Dinge wandeln,", "tokens": ["Wenn", "Ge\u00b7fahr", "be\u00b7steht", ",", "da\u00df", "sich", "Din\u00b7ge", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$,", "KOUS", "PRF", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.5": {"text": "wenn verboten wird, mit dem Boden zu handeln \u2013", "tokens": ["wenn", "ver\u00b7bo\u00b7ten", "wird", ",", "mit", "dem", "Bo\u00b7den", "zu", "han\u00b7deln", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "VAFIN", "$,", "APPR", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "dann herrscht Unordnung.", "tokens": ["dann", "herrscht", "Un\u00b7ord\u00b7nung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Die Hauptsache ist: Nicht auf Hungernde h\u00f6ren.", "tokens": ["Die", "Haupt\u00b7sa\u00b7che", "ist", ":", "Nicht", "auf", "Hun\u00b7gern\u00b7de", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$.", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Hauptsache ist: Nicht das Stra\u00dfenbild st\u00f6ren.", "tokens": ["Die", "Haupt\u00b7sa\u00b7che", "ist", ":", "Nicht", "das", "Stra\u00b7\u00dfen\u00b7bild", "st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$.", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+--++-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nur nicht schrein.", "tokens": ["Nur", "nicht", "schrein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VAINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Mit der Zeit wird das schon.", "tokens": ["Mit", "der", "Zeit", "wird", "das", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ART", "ADV", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Alles bringt euch die Evolution.", "tokens": ["Al\u00b7les", "bringt", "euch", "die", "E\u00b7vo\u00b7lu\u00b7ti\u00b7on", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "So hats euer Volksvertreter entdeckt.", "tokens": ["So", "hats", "eu\u00b7er", "Volks\u00b7ver\u00b7tre\u00b7ter", "ent\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Seid ihr bis dahin alle verreckt?", "tokens": ["Seid", "ihr", "bis", "da\u00b7hin", "al\u00b7le", "ver\u00b7reckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ADV", "PAV", "PIS", "VVPP", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.8": {"text": "So wird man auf euern Gr\u00e4bern doch lesen:", "tokens": ["So", "wird", "man", "auf", "eu\u00b7ern", "Gr\u00e4\u00b7bern", "doch", "le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "APPR", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "sie sind immer ruhig und ordentlich gewesen.", "tokens": ["sie", "sind", "im\u00b7mer", "ru\u00b7hig", "und", "or\u00b7dent\u00b7lich", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "VAPP", "$."], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}}}}