{"textgrid.poem.25905": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Augustmond im Weinberg", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Angustmond durch den Weinberg streicht", "tokens": ["An\u00b7gust\u00b7mond", "durch", "den", "Wein\u00b7berg", "streicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Und hat den Reben die Brust gereicht.", "tokens": ["Und", "hat", "den", "Re\u00b7ben", "die", "Brust", "ge\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er, der hinter Verliebten schleicht,", "tokens": ["Er", ",", "der", "hin\u00b7ter", "Ver\u00b7lieb\u00b7ten", "schleicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Will R\u00e4usche Verliebter den Trauben geben;", "tokens": ["Will", "R\u00e4u\u00b7sche", "Ver\u00b7lieb\u00b7ter", "den", "Trau\u00b7ben", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Wo zwei ein volles Glas dann heben,", "tokens": ["Wo", "zwei", "ein", "vol\u00b7les", "Glas", "dann", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wird ihnen ihre Erde leicht,", "tokens": ["Wird", "ih\u00b7nen", "ih\u00b7re", "Er\u00b7de", "leicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df sie als Mond dar\u00fcber schweben.", "tokens": ["Da\u00df", "sie", "als", "Mond", "da\u00b7r\u00fc\u00b7ber", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "NN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}