{"textgrid.poem.25517": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie war ich sonst dem Wachen doch so gram,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie war ich sonst dem Wachen doch so gram,", "tokens": ["Wie", "war", "ich", "sonst", "dem", "Wa\u00b7chen", "doch", "so", "gram", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ART", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem Schlafe wie so gut!", "tokens": ["Dem", "Schla\u00b7fe", "wie", "so", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn ungelockt er auf die Augen kam,", "tokens": ["Wenn", "un\u00b7ge\u00b7lockt", "er", "auf", "die", "Au\u00b7gen", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Noch unbenetzt von s\u00fc\u00dfer Thr\u00e4nen Fluth.", "tokens": ["Noch", "un\u00b7be\u00b7netzt", "von", "s\u00fc\u00b7\u00dfer", "Thr\u00e4\u00b7nen", "Fluth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich g\u00e4hnte schon, sobald der Hesperus", "tokens": ["Ich", "g\u00e4hn\u00b7te", "schon", ",", "so\u00b7bald", "der", "Hes\u00b7pe\u00b7rus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "ART", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Am Horizonte stand,", "tokens": ["Am", "Ho\u00b7ri\u00b7zon\u00b7te", "stand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gab nickend oft dem N\u00e4hpult' einen Ku\u00df,", "tokens": ["Gab", "ni\u00b7ckend", "oft", "dem", "N\u00e4h\u00b7pult'", "ei\u00b7nen", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und leise fiel mein Strickzeug aus der Hand.", "tokens": ["Und", "lei\u00b7se", "fiel", "mein", "Strick\u00b7zeug", "aus", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Fand ich nicht oft am Abend' meinen Kopf,", "tokens": ["Fand", "ich", "nicht", "oft", "am", "A\u00b7bend'", "mei\u00b7nen", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Auf meinen Arm gelegt,", "tokens": ["Auf", "mei\u00b7nen", "Arm", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "An dem Klavier; und sucht' ihn, wie ein Tropf,", "tokens": ["An", "dem", "Kla\u00b7vier", ";", "und", "sucht'", "ihn", ",", "wie", "ein", "Tropf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "KON", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.12": {"text": "Wenn's vor ihm steht, das Gl\u00fcck zu suchen pflegt?", "tokens": ["Wenn's", "vor", "ihm", "steht", ",", "das", "Gl\u00fcck", "zu", "su\u00b7chen", "pflegt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPER", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Wie bin ich nun dem Schlafe doch so gram,", "tokens": ["Wie", "bin", "ich", "nun", "dem", "Schla\u00b7fe", "doch", "so", "gram", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ART", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Dem Wachen, wie so gut!", "tokens": ["Dem", "Wa\u00b7chen", ",", "wie", "so", "gut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Itzt, Lucifer, siehst du am N\u00e4herahm'", "tokens": ["Itzt", ",", "Lu\u00b7ci\u00b7fer", ",", "siehst", "du", "am", "N\u00e4\u00b7herahm'"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "NE", "$,", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.16": {"text": "Mich noch so gl\u00fch als h\u00e4tt' ich sanft geruht.", "tokens": ["Mich", "noch", "so", "gl\u00fch", "als", "h\u00e4tt'", "ich", "sanft", "ge\u00b7ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJD", "KOKOM", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Hier ist dein Bild, mein zweites liebes Du!", "tokens": ["Hier", "ist", "dein", "Bild", ",", "mein", "zwei\u00b7tes", "lie\u00b7bes", "Du", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "ADJA", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Ich werfe weinend dann", "tokens": ["Ich", "wer\u00b7fe", "wei\u00b7nend", "dann"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Ihm Ku\u00df auf Ku\u00df von meinen Lippen zu;", "tokens": ["Ihm", "Ku\u00df", "auf", "Ku\u00df", "von", "mei\u00b7nen", "Lip\u00b7pen", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Wie l\u00e4chelt's mich so innig dankbar an!", "tokens": ["Wie", "l\u00e4\u00b7chelt's", "mich", "so", "in\u00b7nig", "dank\u00b7bar", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Ich fl\u00fcstre gar, als k\u00f6nnt' es mich verstehn,", "tokens": ["Ich", "fl\u00fcst\u00b7re", "gar", ",", "als", "k\u00f6nnt'", "es", "mich", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Ihm meine Seufzer vor,", "tokens": ["Ihm", "mei\u00b7ne", "Seuf\u00b7zer", "vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Denk' als ein Kind: (auch der Betrug ist sch\u00f6n!)", "tokens": ["Denk'", "als", "ein", "Kind", ":(", "auch", "der", "Be\u00b7trug", "ist", "sch\u00f6n", "!", ")"], "token_info": ["word", "word", "word", "word", "emoticon", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "KOKOM", "ART", "NN", "$(", "ADV", "ART", "NN", "VAFIN", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Nun klingt ihm itzt vielleicht sein rechtes Ohr.", "tokens": ["Nun", "klingt", "ihm", "itzt", "viel\u00b7leicht", "sein", "rech\u00b7tes", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Wenn auch der Schlaf die Augenlieder treu", "tokens": ["Wenn", "auch", "der", "Schlaf", "die", "Au\u00b7gen\u00b7lie\u00b7der", "treu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Mit Schwanenfl\u00fcgeln streicht,", "tokens": ["Mit", "Schwa\u00b7nen\u00b7fl\u00fc\u00b7geln", "streicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Macht meine Hand ihn endlich doch so scheu,", "tokens": ["Macht", "mei\u00b7ne", "Hand", "ihn", "end\u00b7lich", "doch", "so", "scheu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PPER", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.28": {"text": "Da\u00df er verwirrt zu meiner ", "tokens": ["Da\u00df", "er", "ver\u00b7wirrt", "zu", "mei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Denn so der Schlaf dich meinem Geist' entri\u00df:", "tokens": ["Denn", "so", "der", "Schlaf", "dich", "mei\u00b7nem", "Geist'", "ent\u00b7ri\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Ach, ach! was h\u00e4tt' ich dann?", "tokens": ["Ach", ",", "ach", "!", "was", "h\u00e4tt'", "ich", "dann", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ITJ", "$.", "PWS", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "Ob dich ein Traum mir zeig', ist ungewi\u00df,", "tokens": ["Ob", "dich", "ein", "Traum", "mir", "zeig'", ",", "ist", "un\u00b7ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "VVFIN", "$,", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Drum schmieg' ich mich im Wachen an dich an.", "tokens": ["Drum", "schmieg'", "ich", "mich", "im", "Wa\u00b7chen", "an", "dich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}