{"textgrid.poem.62141": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[kennt ihr sie nicht die b\u00f6se bunte Schlange]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kennt ihr sie nicht die b\u00f6se bunte Schlange,", "tokens": ["Kennt", "ihr", "sie", "nicht", "die", "b\u00f6\u00b7se", "bun\u00b7te", "Schlan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die vom Geb\u00fcsch die Ferse sticht?", "tokens": ["Die", "vom", "Ge\u00b7b\u00fcsch", "die", "Fer\u00b7se", "sticht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie schleicht verderbend auf dem Gange,", "tokens": ["Sie", "schleicht", "ver\u00b7der\u00b7bend", "auf", "dem", "Gan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und tretet nie vor's Angesicht.", "tokens": ["Und", "tre\u00b7tet", "nie", "vor's", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr Weg ist Mord, allein ganz ungef\u00e4hrdet", "tokens": ["Ihr", "Weg", "ist", "Mord", ",", "al\u00b7lein", "ganz", "un\u00b7ge\u00b7f\u00e4hr\u00b7det"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vergiftet sie aus dem Versteck,", "tokens": ["Ver\u00b7gif\u00b7tet", "sie", "aus", "dem", "Ver\u00b7steck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Horch, zischend sie im Staube sich geberdet:", "tokens": ["Horch", ",", "zi\u00b7schend", "sie", "im", "Stau\u00b7be", "sich", "ge\u00b7ber\u00b7det", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "APPRART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O Menschen, schafft das Monstrum weg!", "tokens": ["O", "Men\u00b7schen", ",", "schafft", "das", "Monst\u00b7rum", "weg", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}