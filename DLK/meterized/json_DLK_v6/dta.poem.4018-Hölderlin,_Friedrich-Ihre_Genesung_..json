{"dta.poem.4018": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Ihre Genesung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Deine Freundin, Natur! leidet und schl\u00e4ft, und du", "tokens": ["Dei\u00b7ne", "Freun\u00b7din", ",", "Na\u00b7tur", "!", "lei\u00b7det", "und", "schl\u00e4ft", ",", "und", "du"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "NN", "$.", "VVFIN", "KON", "VVFIN", "$,", "KON", "PPER"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Allbelebende s\u00e4umst? ach, und ihr heilt sie nicht", "tokens": ["All\u00b7be\u00b7le\u00b7ben\u00b7de", "s\u00e4umst", "?", "ach", ",", "und", "ihr", "heilt", "sie", "nicht"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$.", "XY", "$,", "KON", "PPER", "VVFIN", "PPER", "PTKNEG"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "M\u00e4cht'ge L\u00fcfte des Aethers,", "tokens": ["M\u00e4cht'\u00b7ge", "L\u00fcf\u00b7te", "des", "A\u00b7e\u00b7thers", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht ihr Quellen des Sonnenlichts?", "tokens": ["Nicht", "ihr", "Quel\u00b7len", "des", "Son\u00b7nen\u00b7lichts", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Alle Blumen der Erd', alle die fr\u00f6hlichen", "tokens": ["Al\u00b7le", "Blu\u00b7men", "der", "Erd'", ",", "al\u00b7le", "die", "fr\u00f6h\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "ART", "NN", "$,", "PIS", "ART", "ADJA"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Sch\u00f6nen Fr\u00fcchte des Hains, heitern sie alle nicht", "tokens": ["Sch\u00f6\u00b7nen", "Fr\u00fcch\u00b7te", "des", "Hains", ",", "hei\u00b7tern", "sie", "al\u00b7le", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "PIS", "PTKNEG"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Dieses Leben, ihr G\u00f6tter,", "tokens": ["Die\u00b7ses", "Le\u00b7ben", ",", "ihr", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Das ihr selber in Lieb' erzogt?", "tokens": ["Das", "ihr", "sel\u00b7ber", "in", "Lieb'", "er\u00b7zogt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Ach! schon athmet und t\u00f6nt heilige Lebenslust", "tokens": ["Ach", "!", "schon", "ath\u00b7met", "und", "t\u00f6nt", "hei\u00b7li\u00b7ge", "Le\u00b7bens\u00b7lust"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "KON", "VVFIN", "ADJA", "NN"], "meter": "+-+--++--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ihr im reizenden Wort wieder, wie sonst, und", "tokens": ["Ihr", "im", "rei\u00b7zen\u00b7den", "Wort", "wie\u00b7der", ",", "wie", "sonst", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "APPRART", "ADJA", "NN", "ADV", "$,", "PWAV", "ADV", "$,", "KON"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "schon", "tokens": ["schon"], "token_info": ["word"], "pos": ["ADV"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Gl\u00e4nzt das Auge des Lieblings", "tokens": ["Gl\u00e4nzt", "das", "Au\u00b7ge", "des", "Lieb\u00b7lings"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Freundlich offen, Natur! Dich an.", "tokens": ["Freund\u00b7lich", "of\u00b7fen", ",", "Na\u00b7tur", "!", "Dich", "an", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "NN", "$.", "PPER", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}