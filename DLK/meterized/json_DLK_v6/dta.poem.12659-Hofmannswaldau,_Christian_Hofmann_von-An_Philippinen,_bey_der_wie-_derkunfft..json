{"dta.poem.12659": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An Philippinen, bey der wie-  \n derkunfft.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein Damon schreibet hier, geliebte Philippine!", "tokens": ["Dein", "Da\u00b7mon", "schrei\u00b7bet", "hier", ",", "ge\u00b7lieb\u00b7te", "Phil\u00b7ip\u00b7pi\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der deine gunst weit mehr, als alle sch\u00e4tze liebt,", "tokens": ["Der", "dei\u00b7ne", "gunst", "weit", "mehr", ",", "als", "al\u00b7le", "sch\u00e4t\u00b7ze", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "ADV", "$,", "KOUS", "PIS", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du weist wohl, wie ich dich mit steter treu bediene,", "tokens": ["Du", "weist", "wohl", ",", "wie", "ich", "dich", "mit", "ste\u00b7ter", "treu", "be\u00b7die\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "PPER", "PRF", "APPR", "ADJA", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wie mein hertze sich dir gantz zu eigen giebt.", "tokens": ["Und", "wie", "mein", "hert\u00b7ze", "sich", "dir", "gantz", "zu", "ei\u00b7gen", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "VVFIN", "PRF", "PPER", "ADV", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich ware voller lust, als nur dein holdes blicken,", "tokens": ["Ich", "wa\u00b7re", "vol\u00b7ler", "lust", ",", "als", "nur", "dein", "hol\u00b7des", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$,", "KOUS", "ADV", "PPOSAT", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bey meiner wiederkunfft mir in die augen fiel;", "tokens": ["Bey", "mei\u00b7ner", "wie\u00b7der\u00b7kunfft", "mir", "in", "die", "au\u00b7gen", "fiel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ach! dacht ich, d\u00fcrfft ich dich an meine brust nur dr\u00fccken,", "tokens": ["Ach", "!", "dacht", "ich", ",", "d\u00fcrfft", "ich", "dich", "an", "mei\u00b7ne", "brust", "nur", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dich sch\u00f6nstes Venus bild! dich meiner liebe ziel!", "tokens": ["Dich", "sch\u00f6ns\u00b7tes", "Ve\u00b7nus", "bild", "!", "dich", "mei\u00b7ner", "lie\u00b7be", "ziel", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "NN", "$.", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mein wunsch ward mir gew\u00e4hrt, als ich nach zweyen tagen", "tokens": ["Mein", "wunsch", "ward", "mir", "ge\u00b7w\u00e4hrt", ",", "als", "ich", "nach", "zwe\u00b7yen", "ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "VAFIN", "PPER", "VVPP", "$,", "KOUS", "PPER", "APPR", "VVFIN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "In voller gluth und pein an deiner seiten stund,", "tokens": ["In", "vol\u00b7ler", "gluth", "und", "pein", "an", "dei\u00b7ner", "sei\u00b7ten", "stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich konte l\u00e4nger nicht der sonnen-strahl vertragen", "tokens": ["Ich", "kon\u00b7te", "l\u00e4n\u00b7ger", "nicht", "der", "son\u00b7nen\u00b7strahl", "ver\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "PTKNEG", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Drum wurde meine Qvaal durch sanffte k\u00fcsse kund.", "tokens": ["Drum", "wur\u00b7de", "mei\u00b7ne", "Qvaal", "durch", "sanff\u00b7te", "k\u00fcs\u00b7se", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Erz\u00fcrne dich doch nicht, o G\u00f6ttin meiner seelen!", "tokens": ["Er\u00b7z\u00fcr\u00b7ne", "dich", "doch", "nicht", ",", "o", "G\u00f6t\u00b7tin", "mei\u00b7ner", "see\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PTKNEG", "$,", "FM", "NN", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und sto\u00df den treuen knecht nicht stracks von dir hinweg.", "tokens": ["Und", "sto\u00df", "den", "treu\u00b7en", "knecht", "nicht", "stracks", "von", "dir", "hin\u00b7weg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Soll ich, o \u00e4rmster! denn mich annoch l\u00e4nger qv\u00e4len?", "tokens": ["Soll", "ich", ",", "o", "\u00e4rms\u00b7ter", "!", "denn", "mich", "an\u00b7noch", "l\u00e4n\u00b7ger", "qv\u00e4\u00b7len", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "FM", "FM", "$.", "KON", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So geh ich nur getrost den finstern todes-steg?", "tokens": ["So", "geh", "ich", "nur", "ge\u00b7trost", "den", "fins\u00b7tern", "to\u00b7des\u00b7steg", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Willst du denn diesen trieb als einen frevel achten?", "tokens": ["Willst", "du", "denn", "die\u00b7sen", "trieb", "als", "ei\u00b7nen", "fre\u00b7vel", "ach\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PDS", "VVFIN", "KOKOM", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ist ein verliebter ku\u00df nichts als der folter werth?", "tokens": ["Ist", "ein", "ver\u00b7lieb\u00b7ter", "ku\u00df", "nichts", "als", "der", "fol\u00b7ter", "werth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PIS", "KOKOM", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ach lernesch\u00f6nste doch die hei\u00dfe treu betrachten,", "tokens": ["Ach", "ler\u00b7ne\u00b7sch\u00f6ns\u00b7te", "doch", "die", "hei\u00b7\u00dfe", "treu", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "ART", "ADJA", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Erwege nur die pflicht, die dir dein Damon schwert,", "tokens": ["Er\u00b7we\u00b7ge", "nur", "die", "pflicht", ",", "die", "dir", "dein", "Da\u00b7mon", "schwert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Drum Sch\u00f6nste! suche doch die k\u00fcsse nicht zu meiden,", "tokens": ["Drum", "Sch\u00f6ns\u00b7te", "!", "su\u00b7che", "doch", "die", "k\u00fcs\u00b7se", "nicht", "zu", "mei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "$.", "VVFIN", "ADV", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Sieh diesen, der sie giebt mit holden blicken an,", "tokens": ["Sieh", "die\u00b7sen", ",", "der", "sie", "giebt", "mit", "hol\u00b7den", "bli\u00b7cken", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "$,", "PRELS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und weil ein ieder mu\u00df auf dieser welt was leiden.", "tokens": ["Und", "weil", "ein", "ie\u00b7der", "mu\u00df", "auf", "die\u00b7ser", "welt", "was", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "PIS", "VMFIN", "APPR", "PDAT", "NN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So leide doch mein licht, da\u00df ich dich lieben kan.", "tokens": ["So", "lei\u00b7de", "doch", "mein", "licht", ",", "da\u00df", "ich", "dich", "lie\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}