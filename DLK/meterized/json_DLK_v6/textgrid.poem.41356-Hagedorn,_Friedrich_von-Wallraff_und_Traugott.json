{"textgrid.poem.41356": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Wallraff und Traugott", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heulend drang sich Boreas in die dichtverz\u00e4unten Felder,", "tokens": ["Heu\u00b7lend", "drang", "sich", "Bo\u00b7re\u00b7as", "in", "die", "dicht\u00b7ver\u00b7z\u00e4un\u00b7ten", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PRF", "NE", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Ueberraschte Berg und Thal, beugte, brach, zerri\u00df die W\u00e4lder.", "tokens": ["Ue\u00b7ber\u00b7raschte", "Berg", "und", "Thal", ",", "beug\u00b7te", ",", "brach", ",", "zer\u00b7ri\u00df", "die", "W\u00e4l\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+--+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Durch die r\u00e4uberischen Winde ward in einer Ungl\u00fccksnacht", "tokens": ["Durch", "die", "r\u00e4u\u00b7be\u00b7ri\u00b7schen", "Win\u00b7de", "ward", "in", "ei\u00b7ner", "Un\u00b7gl\u00fccks\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Nordens ewigbanger W\u00fcste manches Tempe gleich gemacht.", "tokens": ["Nor\u00b7dens", "e\u00b7wig\u00b7ban\u00b7ger", "W\u00fcs\u00b7te", "man\u00b7ches", "Tem\u00b7pe", "gleich", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PIAT", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Rauhe Furchen, wei\u00df von Reif, \u00f6de h\u00f6ckerichte Fluren,", "tokens": ["Rau\u00b7he", "Fur\u00b7chen", ",", "wei\u00df", "von", "Reif", ",", "\u00f6\u00b7de", "h\u00f6\u00b7cke\u00b7rich\u00b7te", "Flu\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "APPR", "NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Leere Wiesen, fallend Laub, des entbl\u00f6\u00dften Winters Spuren", "tokens": ["Lee\u00b7re", "Wie\u00b7sen", ",", "fal\u00b7lend", "Laub", ",", "des", "ent\u00b7bl\u00f6\u00df\u00b7ten", "Win\u00b7ters", "Spu\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KOUS", "NN", "$,", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Droheten mit starrem Schrecken, wurden doppelt f\u00fcrchterlich,", "tokens": ["Dro\u00b7he\u00b7ten", "mit", "star\u00b7rem", "Schre\u00b7cken", ",", "wur\u00b7den", "dop\u00b7pelt", "f\u00fcrch\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "Als die neue Wuth der St\u00fcrme das betr\u00fcbte Land durchstrich.", "tokens": ["Als", "die", "neu\u00b7e", "Wuth", "der", "St\u00fcr\u00b7me", "das", "be\u00b7tr\u00fcb\u00b7te", "Land", "durch\u00b7strich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Was des Pachters wacher Flei\u00df wohl verpflegt und eingeschlossen,", "tokens": ["Was", "des", "Pach\u00b7ters", "wa\u00b7cher", "Flei\u00df", "wohl", "ver\u00b7pflegt", "und", "ein\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADJA", "NN", "ADV", "VVPP", "KON", "VVPP", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Hohe Ranken an dem Ulm, in den Beeten zarte Sprossen,", "tokens": ["Ho\u00b7he", "Ran\u00b7ken", "an", "dem", "Ulm", ",", "in", "den", "Bee\u00b7ten", "zar\u00b7te", "Spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Zweige starker junger B\u00e4ume, die man alten eingesetzt,", "tokens": ["Zwei\u00b7ge", "star\u00b7ker", "jun\u00b7ger", "B\u00e4u\u00b7me", ",", "die", "man", "al\u00b7ten", "ein\u00b7ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "ADJA", "NN", "$,", "PRELS", "PIS", "ADJA", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Hoffnungvolle frische Pflanzen, die der Frost noch nicht verletzt.", "tokens": ["Hoff\u00b7nung\u00b7vol\u00b7le", "fri\u00b7sche", "Pflan\u00b7zen", ",", "die", "der", "Frost", "noch", "nicht", "ver\u00b7letzt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "ADV", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Was des rauhen Herbstes Grimm vielen Aesten lassen m\u00fcssen,", "tokens": ["Was", "des", "rau\u00b7hen", "Herbs\u00b7tes", "Grimm", "vie\u00b7len", "A\u00b7es\u00b7ten", "las\u00b7sen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "NE", "PIAT", "NN", "VVINF", "VMINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Ward geknickt, gebeugt, zerstreut, abgeschlagen, umgerissen.", "tokens": ["Ward", "ge\u00b7knickt", ",", "ge\u00b7beugt", ",", "zer\u00b7streut", ",", "ab\u00b7ge\u00b7schla\u00b7gen", ",", "um\u00b7ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "VVPP", "$,", "VVPP", "$,", "VVPP", "$,", "VVPP", "$."], "meter": "+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.15": {"text": "Endlich bringt der Tag die Stille: jeder eilt, um selbst zu sehn,", "tokens": ["End\u00b7lich", "bringt", "der", "Tag", "die", "Stil\u00b7le", ":", "je\u00b7der", "eilt", ",", "um", "selbst", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$.", "PIS", "VVFIN", "$,", "KOUI", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Welche B\u00e4ume noch zu st\u00fctzen, welche noch zu retten stehn;", "tokens": ["Wel\u00b7che", "B\u00e4u\u00b7me", "noch", "zu", "st\u00fct\u00b7zen", ",", "wel\u00b7che", "noch", "zu", "ret\u00b7ten", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "ADV", "PTKZU", "VVINF", "$,", "PRELS", "ADV", "PTKZU", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Hausherr, Frau und Knecht und Magd macht sich auf, und forscht und z\u00e4hlet", "tokens": ["Haus\u00b7herr", ",", "Frau", "und", "Knecht", "und", "Magd", "macht", "sich", "auf", ",", "und", "forscht", "und", "z\u00e4h\u00b7let"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN", "KON", "NN", "VVFIN", "PRF", "PTKVZ", "$,", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-++-+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.18": {"text": "Ranken, Sprossen, Baum und Stock, die der Nordwind jetzt verfehlet.", "tokens": ["Ran\u00b7ken", ",", "Spros\u00b7sen", ",", "Baum", "und", "Stock", ",", "die", "der", "Nord\u00b7wind", "jetzt", "ver\u00b7feh\u00b7let", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NE", "KON", "NN", "$,", "PRELS", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.19": {"text": "Zur Erhaltung der Gew\u00e4chse lehren alle, was zu thun;", "tokens": ["Zur", "Er\u00b7hal\u00b7tung", "der", "Ge\u00b7w\u00e4ch\u00b7se", "leh\u00b7ren", "al\u00b7le", ",", "was", "zu", "thun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PIS", "$,", "PRELS", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.20": {"text": "Jeder gibt dem Nachbar Anschlag; weder Witz noch Zunge ruhn.", "tokens": ["Je\u00b7der", "gibt", "dem", "Nach\u00b7bar", "An\u00b7schlag", ";", "we\u00b7der", "Witz", "noch", "Zun\u00b7ge", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "NN", "$.", "KON", "NN", "ADV", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Wallraff nur fa\u00dft den Entschlu\u00df, seine B\u00e4ume zu behauen,", "tokens": ["Wall\u00b7raff", "nur", "fa\u00dft", "den", "Ent\u00b7schlu\u00df", ",", "sei\u00b7ne", "B\u00e4u\u00b7me", "zu", "be\u00b7hau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.2": {"text": "Und weit emsiger, als sonst, das beraubte Feld zu bauen,", "tokens": ["Und", "weit", "em\u00b7si\u00b7ger", ",", "als", "sonst", ",", "das", "be\u00b7raub\u00b7te", "Feld", "zu", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "$,", "KOUS", "ADV", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Greift zur n\u00e4chsten Axt und Hacke, schneidet, pfl\u00f6cket, k\u00fcrzt und bricht;", "tokens": ["Greift", "zur", "n\u00e4chs\u00b7ten", "Axt", "und", "Ha\u00b7cke", ",", "schnei\u00b7det", ",", "pfl\u00f6\u00b7cket", ",", "k\u00fcrzt", "und", "bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "KON", "NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Aber k\u00fcrzt und bricht zu heftig, und verschont fast keinen nicht.", "tokens": ["A\u00b7ber", "k\u00fcrzt", "und", "bricht", "zu", "hef\u00b7tig", ",", "und", "ver\u00b7schont", "fast", "kei\u00b7nen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PTKA", "ADJD", "$,", "KON", "VVFIN", "ADV", "PIAT", "PTKNEG", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Zwar sein Nachbar Traugott k\u00f6mmt, aus Erfahrung ihn zu lehren,", "tokens": ["Zwar", "sein", "Nach\u00b7bar", "Trau\u00b7gott", "k\u00f6mmt", ",", "aus", "Er\u00b7fah\u00b7rung", "ihn", "zu", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "VVFIN", "$,", "APPR", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Nicht durch Eile noch Gewalt Ordnung und Natur zu st\u00f6ren.", "tokens": ["Nicht", "durch", "Ei\u00b7le", "noch", "Ge\u00b7walt", "Ord\u00b7nung", "und", "Na\u00b7tur", "zu", "st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "ADV", "NN", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Schone, spricht er, deiner B\u00e4ume: glaube mir, allein die Zeit", "tokens": ["Scho\u00b7ne", ",", "spricht", "er", ",", "dei\u00b7ner", "B\u00e4u\u00b7me", ":", "glau\u00b7be", "mir", ",", "al\u00b7lein", "die", "Zeit"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "$,", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Schaffet, ohne solche Mittel, die erw\u00fcnschte Fruchtbarkeit.", "tokens": ["Schaf\u00b7fet", ",", "oh\u00b7ne", "sol\u00b7che", "Mit\u00b7tel", ",", "die", "er\u00b7w\u00fcnschte", "Frucht\u00b7bar\u00b7keit", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUI", "PIAT", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-++-+-+", "measure": "unknown.measure.octa.plus"}, "line.9": {"text": "Aber Wallraff h\u00f6rt ihn nicht. Als hierauf der Lenz erschienen,", "tokens": ["A\u00b7ber", "Wall\u00b7raff", "h\u00f6rt", "ihn", "nicht", ".", "Als", "hier\u00b7auf", "der", "Lenz", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "PTKNEG", "$.", "KOUS", "PAV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Sahe man fast jeden Baum, nur nicht die gekappten, gr\u00fcnen,", "tokens": ["Sa\u00b7he", "man", "fast", "je\u00b7den", "Baum", ",", "nur", "nicht", "die", "ge\u00b7kapp\u00b7ten", ",", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PIAT", "NN", "$,", "ADV", "PTKNEG", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Und des weisen Alten St\u00e4mme voller, als man sonst gesehn,", "tokens": ["Und", "des", "wei\u00b7sen", "Al\u00b7ten", "St\u00e4m\u00b7me", "vol\u00b7ler", ",", "als", "man", "sonst", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "NN", "ADJA", "$,", "KOUS", "PIS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Reich an unerzwungnen Fr\u00fcchten, ungek\u00fcnstelt pr\u00e4chtig stehn.", "tokens": ["Reich", "an", "un\u00b7er\u00b7zwung\u00b7nen", "Fr\u00fcch\u00b7ten", ",", "un\u00b7ge\u00b7k\u00fcns\u00b7telt", "pr\u00e4ch\u00b7tig", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "ADJD", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Diesen B\u00e4umen gleicht der Witz; sucht ihn nicht zu \u00fcbertreiben;", "tokens": ["Die\u00b7sen", "B\u00e4u\u00b7men", "gleicht", "der", "Witz", ";", "sucht", "ihn", "nicht", "zu", "\u00fc\u00b7bert\u00b7rei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "ART", "NN", "$.", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Ehrt die wirkende Natur; la\u00dft das K\u00fcnsteln ferne bleiben.", "tokens": ["Ehrt", "die", "wir\u00b7ken\u00b7de", "Na\u00b7tur", ";", "la\u00dft", "das", "K\u00fcns\u00b7teln", "fer\u00b7ne", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$.", "VVIMP", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Soll die Seele sich entwickeln, und in rechter Gr\u00f6\u00dfe bl\u00fchn,", "tokens": ["Soll", "die", "See\u00b7le", "sich", "ent\u00b7wi\u00b7ckeln", ",", "und", "in", "rech\u00b7ter", "Gr\u00f6\u00b7\u00dfe", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PRF", "VVINF", "$,", "KON", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "troch