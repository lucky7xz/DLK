{"dta.poem.14567": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der S\u00e4nger.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wie Wald und Strom im Rauschen", "tokens": ["Wie", "Wald", "und", "Strom", "im", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verlockend Worte tauschen!", "tokens": ["Ver\u00b7lo\u00b7ckend", "Wor\u00b7te", "tau\u00b7schen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was ist's, da\u00df ich ergrause? \u2014", "tokens": ["Was", "ist's", ",", "da\u00df", "ich", "er\u00b7grau\u00b7se", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "$,", "KOUS", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fchrt doch aus stillem Hause", "tokens": ["F\u00fchrt", "doch", "aus", "stil\u00b7lem", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Hirt die gold'ne Heerde,", "tokens": ["Der", "Hirt", "die", "gold'\u00b7ne", "Heer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und h\u00fctet treu und wacht,", "tokens": ["Und", "h\u00fc\u00b7tet", "treu", "und", "wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So lieblich weht die Nacht,", "tokens": ["So", "lieb\u00b7lich", "weht", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Lind s\u00e4uselt kaum die Erde.", "tokens": ["Lind", "s\u00e4u\u00b7selt", "kaum", "die", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}}}}}