{"dta.poem.11126": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An einen neuen ehmann.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wen hast du denn gefreyt? die beste von den schlimmsten.", "tokens": ["Wen", "hast", "du", "denn", "ge\u00b7freyt", "?", "die", "bes\u00b7te", "von", "den", "schlimms\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VVPP", "$.", "ART", "ADJA", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie ist sie am verstand? die kl\u00fcgste von den t\u00fcmmsten.", "tokens": ["Wie", "ist", "sie", "am", "ver\u00b7stand", "?", "die", "kl\u00fcgs\u00b7te", "von", "den", "t\u00fcmms\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "APPRART", "NN", "$.", "ART", "ADJA", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist sie auch sch\u00f6n genung? Ja wie ein a. b. c.", "tokens": ["Ist", "sie", "auch", "sch\u00f6n", "ge\u00b7nung", "?", "Ja", "wie", "ein", "a.", "b.", "c."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "ADV", "$.", "PTKANT", "KOKOM", "ART", "APPRART", "ADJA", "NE"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Was kriegst du denn mit ihr? Ein ", "tokens": ["Was", "kriegst", "du", "denn", "mit", "ihr", "?", "Ein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["P