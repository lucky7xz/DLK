{"textgrid.poem.24089": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir wird es schwer, ", "tokens": ["Mir", "wird", "es", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wer wei\u00df, in welche schmutzig dumpfen Gassen", "tokens": ["Wer", "wei\u00df", ",", "in", "wel\u00b7che", "schmut\u00b7zig", "dum\u00b7pfen", "Gas\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "APPR", "PRELS", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Flug sie tr\u00e4gt. \u2013 Mut, Herz, gieb sie dem Wind!", "tokens": ["Ihr", "Flug", "sie", "tr\u00e4gt", ".", "\u2013", "Mut", ",", "Herz", ",", "gieb", "sie", "dem", "Wind", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "$.", "$(", "NN", "$,", "NN", "$,", "VVIMP", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du bist befreit, wenn sie ins Blaue schwinden.", "tokens": ["Du", "bist", "be\u00b7freit", ",", "wenn", "sie", "ins", "Blau\u00b7e", "schwin\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KOUS", "PPER", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da fliegen sie und segeln mit den Winden,", "tokens": ["Da", "flie\u00b7gen", "sie", "und", "se\u00b7geln", "mit", "den", "Win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die deines Wahns die letzten Fetzen sind:", "tokens": ["Die", "dei\u00b7nes", "Wahns", "die", "letz\u00b7ten", "Fet\u00b7zen", "sind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}