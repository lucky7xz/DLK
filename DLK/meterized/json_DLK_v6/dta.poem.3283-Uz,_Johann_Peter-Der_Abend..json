{"dta.poem.3283": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Der Abend.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Mit finstrer Stirne stehn wir da,", "tokens": ["Mit", "finst\u00b7rer", "Stir\u00b7ne", "stehn", "wir", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ordnen das Geschick der Staaten,", "tokens": ["Und", "ord\u00b7nen", "das", "Ge\u00b7schick", "der", "Staa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wissen, was bey Sorr geschah,", "tokens": ["Und", "wis\u00b7sen", ",", "was", "bey", "Sorr", "ge\u00b7schah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PRELS", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wissen Oesterreich zu rathen.", "tokens": ["Und", "wis\u00b7sen", "O\u00b7es\u00b7ter\u00b7reich", "zu", "ra\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Inde\u00df verschlie\u00dft sich unsre Brust", "tokens": ["In\u00b7de\u00df", "ver\u00b7schlie\u00dft", "sich", "uns\u00b7re", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Ruf der lockenden Cythere:", "tokens": ["Dem", "Ruf", "der", "lo\u00b7cken\u00b7den", "Cy\u00b7the\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn steigt nicht schon, zu Amors Lust,", "tokens": ["Denn", "steigt", "nicht", "schon", ",", "zu", "A\u00b7mors", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "$,", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Abend aus dem k\u00fchlen Meere?", "tokens": ["Der", "A\u00b7bend", "aus", "dem", "k\u00fch\u00b7len", "Mee\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Erkennet euern Eigensinn", "tokens": ["Er\u00b7ken\u00b7net", "eu\u00b7ern", "Ei\u00b7gen\u00b7sinn"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und da\u00df die Zeit gefl\u00fcgelt scheide!", "tokens": ["Und", "da\u00df", "die", "Zeit", "ge\u00b7fl\u00fc\u00b7gelt", "schei\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Jhr schwatzt, sie fliegt, sie ist dahin", "tokens": ["Ihr", "schwatzt", ",", "sie", "fliegt", ",", "sie", "ist", "da\u00b7hin"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit aller angebothnen Freude.", "tokens": ["Mit", "al\u00b7ler", "an\u00b7ge\u00b7both\u00b7nen", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich will zu jenen B\u00fcschen gehn,", "tokens": ["Ich", "will", "zu", "je\u00b7nen", "B\u00fc\u00b7schen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die sanft von Zephyrs Ankunft beben.", "tokens": ["Die", "sanft", "von", "Ze\u00b7phyrs", "An\u00b7kunft", "be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NE", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da hoff ich Lesbien zu sehn,", "tokens": ["Da", "hoff", "ich", "Les\u00b7bi\u00b7en", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn sichre Schatten uns umgeben.", "tokens": ["Wenn", "sich\u00b7re", "Schat\u00b7ten", "uns", "um\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Bereits ert\u00f6nt in stiller Luft", "tokens": ["Be\u00b7reits", "er\u00b7t\u00f6nt", "in", "stil\u00b7ler", "Luft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Nachtigall verliebte Klage:", "tokens": ["Der", "Nach\u00b7ti\u00b7gall", "ver\u00b7lieb\u00b7te", "Kla\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie h\u00fcpft von Zweig auf Zweig und ruft", "tokens": ["Sie", "h\u00fcpft", "von", "Zweig", "auf", "Zweig", "und", "ruft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit s\u00fcssern Liedern, als am Tage.", "tokens": ["Mit", "s\u00fcs\u00b7sern", "Lie\u00b7dern", ",", "als", "am", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Was Wunder, wenn sie br\u00fcnstig girrt,", "tokens": ["Was", "Wun\u00b7der", ",", "wenn", "sie", "br\u00fcns\u00b7tig", "girrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seit Amor mit gespanntem Bogen,", "tokens": ["Seit", "A\u00b7mor", "mit", "ge\u00b7spann\u00b7tem", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bey dem ein voller K\u00f6cher schwirrt,", "tokens": ["Bey", "dem", "ein", "vol\u00b7ler", "K\u00f6\u00b7cher", "schwirrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem jungen Fr\u00fchling nachgeflogen!", "tokens": ["Dem", "jun\u00b7gen", "Fr\u00fch\u00b7ling", "nach\u00b7ge\u00b7flo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}