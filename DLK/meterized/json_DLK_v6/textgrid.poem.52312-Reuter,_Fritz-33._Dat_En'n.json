{"textgrid.poem.52312": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "33. Dat En'n", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "As ick tau Rostock in de Kopmannslihr,", "tokens": ["As", "ick", "tau", "Ros\u00b7tock", "in", "de", "Kop\u00b7manns\u00b7lihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Hadd ick en gauden Fr\u00fcnd, de 'n beten d\u00fcsig wir.", "tokens": ["Hadd", "ick", "en", "gau\u00b7den", "Fr\u00fcnd", ",", "de", "'n", "be\u00b7ten", "d\u00fc\u00b7sig", "wir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "NE", "NE", "VVFIN", "ADJD", "PPER", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Mit den'n gung ick des S\u00fcnndags mal", "tokens": ["Mit", "den'n", "gung", "ick", "des", "S\u00fcnn\u00b7dags", "mal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPER", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verluren an den Strand hendal,", "tokens": ["Ver\u00b7lu\u00b7ren", "an", "den", "Strand", "hen\u00b7dal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00dcm uns de Sch\u00e4p mal antauseihn.", "tokens": ["\u00dcm", "uns", "de", "Sch\u00e4p", "mal", "an\u00b7tau\u00b7seihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "NE", "NE", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Na gaud, wi g\u00fcngen r\u00fcm un st\u00fcn'n", "tokens": ["Na", "gaud", ",", "wi", "g\u00fcn\u00b7gen", "r\u00fcm", "un", "st\u00fcn'n"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Un keken tau, wo up dat ein", "tokens": ["Un", "ke\u00b7ken", "tau", ",", "wo", "up", "dat", "ein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NE", "$,", "PWAV", "NE", "VVFIN", "ART"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "De L\u00fcd en Tau herupper w\u00fcnn'n.", "tokens": ["De", "L\u00fcd", "en", "Tau", "he\u00b7rup\u00b7per", "w\u00fcnn'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "De Kirls, de r\u00f6gten fix de H\u00e4n'n,", "tokens": ["De", "Kirls", ",", "de", "r\u00f6g\u00b7ten", "fix", "de", "H\u00e4n'n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Doch hadd dat Tau noch \u00fcmmer nich en En'n.", "tokens": ["Doch", "hadd", "dat", "Tau", "noch", "\u00fcm\u00b7mer", "nich", "en", "En'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "NE", "ART", "NN", "ADV", "ADV", "PTKNEG", "NE", "NE", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "\u00bbne\u00ab, segg ick, \u00bbwill'n man wider gahn!", "tokens": ["\u00bb", "ne", "\u00ab", ",", "segg", "ick", ",", "\u00bb", "will'n", "man", "wi\u00b7der", "gahn", "!"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "VMFIN", "PIS", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Wat s\u00e4l wi hir as Nahren stahn?", "tokens": ["Wat", "s\u00e4l", "wi", "hir", "as", "Nah\u00b7ren", "stahn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "De Sak schint h\u00fct kein En'n tau finnen,", "tokens": ["De", "Sak", "schint", "h\u00fct", "kein", "En'n", "tau", "fin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADJD", "PIAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "De k\u00e4n'n noch \u00e4wermorgen winnen.\u00ab", "tokens": ["De", "k\u00e4n'n", "noch", "\u00e4\u00b7wer\u00b7mor\u00b7gen", "win\u00b7nen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "ADV", "VVINF", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "\u00bbna\u00ab, seggt min Fr\u00fcnd un makt en klauk Gesicht,", "tokens": ["\u00bb", "na", "\u00ab", ",", "seggt", "min", "Fr\u00fcnd", "un", "makt", "en", "klauk", "Ge\u00b7sicht", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$(", "$,", "VVFIN", "PPOSAT", "NN", "FM", "FM", "FM", "FM", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.16": {"text": "\u00bbpa\u00df up, dit ward 'ne snurrige Geschicht.", "tokens": ["\u00bb", "pa\u00df", "up", ",", "dit", "ward", "'ne", "snur\u00b7ri\u00b7ge", "Ge\u00b7schicht", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "$,", "NE", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Dat En'n, dat krigen s' nich tau St\u00e4den;", "tokens": ["Dat", "En'n", ",", "dat", "kri\u00b7gen", "s'", "nich", "tau", "St\u00e4\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "NE", "PTKNEG", "CARD", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Ick wedd dorup, sei finnen kein,", "tokens": ["Ick", "wedd", "do\u00b7rup", ",", "sei", "fin\u00b7nen", "kein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "VAFIN", "VVFIN", "PIAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Pa\u00df du mal up, du wardst dat seihn:", "tokens": ["Pa\u00df", "du", "mal", "up", ",", "du", "wardst", "dat", "seihn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "NE", "$,", "PPER", "VAFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "De Jung's, de heww'n dat En'n afsn\u00e4den.\u00ab", "tokens": ["De", "Jung's", ",", "de", "hew\u00b7w'n", "dat", "En'n", "afs\u00b7n\u00e4\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "$,", "NE", "NE", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}