{"dta.poem.11825": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Leiter unterm Baum liegt umgest\u00fcrzt im Graben,", "tokens": ["Die", "Lei\u00b7ter", "un\u00b7term", "Baum", "liegt", "um\u00b7ge\u00b7st\u00fcrzt", "im", "Gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An der heut auf und ab geklettert unsre Knaben,", "tokens": ["An", "der", "heut", "auf", "und", "ab", "ge\u00b7klet\u00b7tert", "uns\u00b7re", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "PTKVZ", "KON", "PTKVZ", "VVPP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Jakobsleiter gleich, auf welcher Engel stiegen,", "tokens": ["Der", "Ja\u00b7kobs\u00b7lei\u00b7ter", "gleich", ",", "auf", "wel\u00b7cher", "En\u00b7gel", "stie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "APPR", "PWAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von der, ich wei\u00df nicht wo, bewahrt die Sprossen liegen.", "tokens": ["Von", "der", ",", "ich", "wei\u00df", "nicht", "wo", ",", "be\u00b7wahrt", "die", "Spros\u00b7sen", "lie\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PPER", "VVFIN", "PTKNEG", "PWAV", "$,", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Engel stiegen dort herab vom Himmelsraum,", "tokens": ["Die", "En\u00b7gel", "stie\u00b7gen", "dort", "her\u00b7ab", "vom", "Him\u00b7mels\u00b7raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Bengel stiegen hier hinauf zum Apfelbaum;", "tokens": ["Die", "Ben\u00b7gel", "stie\u00b7gen", "hier", "hin\u00b7auf", "zum", "Ap\u00b7fel\u00b7baum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Hier sch\u00f6ne Wirklichkeit, und dort ein sch\u00f6ner Traum.", "tokens": ["Hier", "sch\u00f6\u00b7ne", "Wirk\u00b7lich\u00b7keit", ",", "und", "dort", "ein", "sch\u00f6\u00b7ner", "Traum", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}