{"textgrid.poem.32551": {"metadata": {"author": {"name": "Scheffner, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: H\u00f6r Afterwelt mein Lied! es prei\u00dft", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6r Afterwelt mein Lied! es prei\u00dft", "tokens": ["H\u00f6r", "Af\u00b7ter\u00b7welt", "mein", "Lied", "!", "es", "prei\u00dft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "PPOSAT", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das sch\u00f6nste Kleinod keuscher Musen,", "tokens": ["Das", "sch\u00f6ns\u00b7te", "Klei\u00b7nod", "keu\u00b7scher", "Mu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Apoll doch nein \u2013 st\u00e4rk' dich mein Geist", "tokens": ["A\u00b7poll", "doch", "nein", "\u2013", "st\u00e4rk'", "dich", "mein", "Geist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADV", "PTKANT", "$(", "VVIMP", "PPER", "PPOSAT", "NN"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Durch Blicke auf de sch\u00f6nsten Busen!", "tokens": ["Durch", "Bli\u00b7cke", "auf", "de", "sch\u00f6ns\u00b7ten", "Bu\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der V\u00f6lkerschaften Lieblich dich,", "tokens": ["Der", "V\u00f6l\u00b7ker\u00b7schaf\u00b7ten", "Lieb\u00b7lich", "dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O k\u00f6nnt ich w\u00fcrdig dich besingen!", "tokens": ["O", "k\u00f6nnt", "ich", "w\u00fcr\u00b7dig", "dich", "be\u00b7sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dies Lied, voll deines Ruhms, w\u00fcrd' mich", "tokens": ["Dies", "Lied", ",", "voll", "dei\u00b7nes", "Ruhms", ",", "w\u00fcrd'", "mich"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "NN", "$,", "ADJD", "PPOSAT", "NN", "$,", "VAFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dann auf der Enkel Enkel bringen.", "tokens": ["Dann", "auf", "der", "En\u00b7kel", "En\u00b7kel", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00dft bey verschwendter Odenwut", "tokens": ["La\u00dft", "bey", "ver\u00b7schwend\u00b7ter", "O\u00b7den\u00b7wut"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um Eselsscheiteln Lorbeer gr\u00fcnen,", "tokens": ["Um", "E\u00b7sels\u00b7schei\u00b7teln", "Lor\u00b7beer", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Singt Schlachten, singt der Reben Blut", "tokens": ["Singt", "Schlach\u00b7ten", ",", "singt", "der", "Re\u00b7ben", "Blut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um Brod und Titel zu verdienen", "tokens": ["Um", "Brod", "und", "Ti\u00b7tel", "zu", "ver\u00b7die\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "NN", "KON", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und bleibt stets n\u00fcchtern \u2013 Mein Gesang", "tokens": ["Und", "bleibt", "stets", "n\u00fcch\u00b7tern", "\u2013", "Mein", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$(", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Prei\u00dft stolz das Schoo\u00dfkind junger Sch\u00f6nen,", "tokens": ["Prei\u00dft", "stolz", "das", "Schoo\u00df\u00b7kind", "jun\u00b7ger", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und w\u00fcnscht sich nur ", "tokens": ["Und", "w\u00fcnscht", "sich", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Die noch der Unschuld Myrthen kr\u00f6nen.", "tokens": ["Die", "noch", "der", "Un\u00b7schuld", "Myr\u00b7then", "kr\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O Guth, f\u00fcr das die L\u00fcsternheit", "tokens": ["O", "Guth", ",", "f\u00fcr", "das", "die", "L\u00fcs\u00b7tern\u00b7heit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "APPR", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Rausch oft hundert Welten g\u00e4be,", "tokens": ["Im", "Rausch", "oft", "hun\u00b7dert", "Wel\u00b7ten", "g\u00e4\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dich sing ich, Prei\u00df der Z\u00e4rtlichkeit,", "tokens": ["Dich", "sing", "ich", ",", "Prei\u00df", "der", "Z\u00e4rt\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich, erster Keim der Ehstandsrebe;", "tokens": ["Dich", ",", "ers\u00b7ter", "Keim", "der", "Eh\u00b7stands\u00b7re\u00b7be", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dich Guth, das einmal nur erg\u00f6tzt,", "tokens": ["Dich", "Guth", ",", "das", "ein\u00b7mal", "nur", "er\u00b7g\u00f6tzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Amors Sieg kr\u00e4nzt und vollendet,", "tokens": ["Das", "A\u00b7mors", "Sieg", "kr\u00e4nzt", "und", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "F\u00fcr das, oft zehnmal schon ersetzt", "tokens": ["F\u00fcr", "das", ",", "oft", "zehn\u00b7mal", "schon", "er\u00b7setzt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "$,", "ADV", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Britte selbst sein Gold verschwendet.", "tokens": ["Der", "Brit\u00b7te", "selbst", "sein", "Gold", "ver\u00b7schwen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dich Kleinod, oft schlecht angebracht,", "tokens": ["Dich", "Klei\u00b7nod", ",", "oft", "schlecht", "an\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und oft im ", "tokens": ["Und", "oft", "im"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "APPRART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Bey allen M\u00e4dchen zwar gedacht,", "tokens": ["Bey", "al\u00b7len", "M\u00e4d\u00b7chen", "zwar", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bey vierzehnj\u00e4hrgen kaum gefunden,", "tokens": ["Bey", "vier\u00b7zehn\u00b7j\u00e4hr\u00b7gen", "kaum", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O Gabe, die die Clerisey", "tokens": ["O", "Ga\u00b7be", ",", "die", "die", "Cle\u00b7ri\u00b7sey"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gern statt des ersten Beichtgelds n\u00e4hme,", "tokens": ["Gern", "statt", "des", "ers\u00b7ten", "Beicht\u00b7gelds", "n\u00e4h\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die jeder Mann, wie Weibertreu,", "tokens": ["Die", "je\u00b7der", "Mann", ",", "wie", "Wei\u00b7ber\u00b7treu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "PWAV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sehr gern zum Brautschatz mit bek\u00e4hme.", "tokens": ["Sehr", "gern", "zum", "Braut\u00b7schatz", "mit", "be\u00b7k\u00e4h\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "APPR", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dich Bl\u00fchmchen, das der Rose gleicht,", "tokens": ["Dich", "Bl\u00fch\u00b7mchen", ",", "das", "der", "Ro\u00b7se", "gleicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die roth und frisch die Sonne gr\u00fc\u00dfet,", "tokens": ["Die", "roth", "und", "frisch", "die", "Son\u00b7ne", "gr\u00fc\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vom Mittagsstral, verbleicht,", "tokens": ["Vom", "Mit\u00b7tags\u00b7stral", ",", "ver\u00b7bleicht", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und nie sich mehr als Knospe schlie\u00dfet;", "tokens": ["Und", "nie", "sich", "mehr", "als", "Knos\u00b7pe", "schlie\u00b7\u00dfet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PRF", "ADV", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dich Guth, das B\u00fcrgerm\u00e4dchen ziert,", "tokens": ["Dich", "Guth", ",", "das", "B\u00fcr\u00b7ger\u00b7m\u00e4d\u00b7chen", "ziert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und stolz die K\u00f6nigst\u00f6chter schm\u00fccket,", "tokens": ["Und", "stolz", "die", "K\u00f6\u00b7nig\u00b7st\u00f6ch\u00b7ter", "schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das jenen oft ein Prinz entf\u00fchrt,", "tokens": ["Das", "je\u00b7nen", "oft", "ein", "Prinz", "ent\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PDS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und hier ein Kammerdiener pfl\u00fccket.", "tokens": ["Und", "hier", "ein", "Kam\u00b7mer\u00b7die\u00b7ner", "pfl\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Magnet von seltner Anzugskraft,", "tokens": ["Mag\u00b7net", "von", "selt\u00b7ner", "An\u00b7zugs\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der sich nach allen Polen bieget,", "tokens": ["Der", "sich", "nach", "al\u00b7len", "Po\u00b7len", "bie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Heil dir kranzw\u00fcrd'ge Jungferschaft,", "tokens": ["Heil", "dir", "kran\u00b7zw\u00fcrd'\u00b7ge", "Jung\u00b7fer\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Heil dem, der blutig dich ersieget!", "tokens": ["Heil", "dem", ",", "der", "blu\u00b7tig", "dich", "er\u00b7sie\u00b7get", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O M\u00e4dchen, lernt des Kleinods Werth,", "tokens": ["O", "M\u00e4d\u00b7chen", ",", "lernt", "des", "Klein\u00b7ods", "Werth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Lernt mit der Myrthenkrone geitzen,", "tokens": ["Lernt", "mit", "der", "Myr\u00b7then\u00b7kro\u00b7ne", "geit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Doch nicht zu lang, sonst f\u00e4llt ihr Werth,", "tokens": ["Doch", "nicht", "zu", "lang", ",", "sonst", "f\u00e4llt", "ihr", "Werth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PTKA", "ADJD", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und ihre Kraft zum Kauf zu reitzen.", "tokens": ["Und", "ih\u00b7re", "Kraft", "zum", "Kauf", "zu", "reit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nur la\u00dft von wilder L\u00fcsternheit", "tokens": ["Nur", "la\u00dft", "von", "wil\u00b7der", "L\u00fcs\u00b7tern\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Euch nie den Zauberg\u00fcrtel l\u00f6sen,", "tokens": ["Euch", "nie", "den", "Zau\u00b7ber\u00b7g\u00fcr\u00b7tel", "l\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dem nur, der eurer Z\u00e4rtlichkeit", "tokens": ["Dem", "nur", ",", "der", "eu\u00b7rer", "Z\u00e4rt\u00b7lich\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ADV", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ganz w\u00fcrdig ist, dem la\u00dft ihn l\u00f6sen: \u2013", "tokens": ["Ganz", "w\u00fcr\u00b7dig", "ist", ",", "dem", "la\u00dft", "ihn", "l\u00f6\u00b7sen", ":", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "PRELS", "VVFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und sollt ihr nach des Schicksals Schlu\u00df", "tokens": ["Und", "sollt", "ihr", "nach", "des", "Schick\u00b7sals", "Schlu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Euch fromm als Priesterfraun einst br\u00fcsten;",