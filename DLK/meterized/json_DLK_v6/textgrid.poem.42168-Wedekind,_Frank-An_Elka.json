{"textgrid.poem.42168": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "An Elka", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Elka, l\u00e4nger kann ich mich nicht halten,", "tokens": ["El\u00b7ka", ",", "l\u00e4n\u00b7ger", "kann", "ich", "mich", "nicht", "hal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Meine Sinne toben allzu wild;", "tokens": ["Mei\u00b7ne", "Sin\u00b7ne", "to\u00b7ben", "all\u00b7zu", "wild", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKA", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und in allen weiblichen Gestalten", "tokens": ["Und", "in", "al\u00b7len", "weib\u00b7li\u00b7chen", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Seh ich schon dein G\u00f6tterbild!", "tokens": ["Seh", "ich", "schon", "dein", "G\u00f6t\u00b7ter\u00b7bild", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auch im Traum bist du mir schon erschienen,", "tokens": ["Auch", "im", "Traum", "bist", "du", "mir", "schon", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dich entkleidend; oh wie ward mir da!", "tokens": ["Dich", "ent\u00b7klei\u00b7dend", ";", "oh", "wie", "ward", "mir", "da", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$.", "ADJD", "KOKOM", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Schwindlig ward mir hinter den Gardinen,", "tokens": ["Schwind\u00b7lig", "ward", "mir", "hin\u00b7ter", "den", "Gar\u00b7di\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Als ich deinen Busen sah.", "tokens": ["Als", "ich", "dei\u00b7nen", "Bu\u00b7sen", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Meine beiden Knie wurden br\u00fcchig,", "tokens": ["Mei\u00b7ne", "bei\u00b7den", "Knie", "wur\u00b7den", "br\u00fc\u00b7chig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PIAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Von der Stirne triefte mir das Fett.", "tokens": ["Von", "der", "Stir\u00b7ne", "trief\u00b7te", "mir", "das", "Fett", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Als das Hemd du abgetan, da schlich ich", "tokens": ["Als", "das", "Hemd", "du", "ab\u00b7ge\u00b7tan", ",", "da", "schlich", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "$,", "ADV", "VVFIN", "PPER"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wonneschauernd an dein Bett.", "tokens": ["Won\u00b7ne\u00b7schau\u00b7ernd", "an", "dein", "Bett", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mach, da\u00df dieser Traum sich bald erf\u00fclle;", "tokens": ["Mach", ",", "da\u00df", "die\u00b7ser", "Traum", "sich", "bald", "er\u00b7f\u00fcl\u00b7le", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PDAT", "NN", "PRF", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mach, erhabne K\u00f6nigin,", "tokens": ["Mach", ",", "er\u00b7hab\u00b7ne", "K\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df bei dir ich vor Behagen br\u00fclle,", "tokens": ["Da\u00df", "bei", "dir", "ich", "vor", "Be\u00b7ha\u00b7gen", "br\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nicht vor Wut, weil ich dir ferne bin.", "tokens": ["Nicht", "vor", "Wut", ",", "weil", "ich", "dir", "fer\u00b7ne", "bin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$,", "KOUS", "PPER", "PPER", "ADV", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}