{"textgrid.poem.48847": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "72. An seine erste Freundin", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du aber, edler Geist, gedenkst noch nicht zu glauben,", "tokens": ["Du", "a\u00b7ber", ",", "ed\u00b7ler", "Geist", ",", "ge\u00b7denkst", "noch", "nicht", "zu", "glau\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJA", "NN", "$,", "VVFIN", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "was mein getreuer Mund dir oft und viel verspricht.", "tokens": ["was", "mein", "ge\u00b7treu\u00b7er", "Mund", "dir", "oft", "und", "viel", "ver\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "PPER", "ADV", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Herz, h\u00f6r' es doch einmal, weil ich bin bei dir nicht,", "tokens": ["Herz", ",", "h\u00f6r'", "es", "doch", "ein\u00b7mal", ",", "weil", "ich", "bin", "bei", "dir", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "ADV", "$,", "KOUS", "PPER", "VAFIN", "APPR", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "so kan ich nicht vorbei, ich mu\u00df es an dich schreiben.", "tokens": ["so", "kan", "ich", "nicht", "vor\u00b7bei", ",", "ich", "mu\u00df", "es", "an", "dich", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "PTKVZ", "$,", "PPER", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du bist die Liebste noch und wirst die Liebste bleiben.", "tokens": ["Du", "bist", "die", "Liebs\u00b7te", "noch", "und", "wirst", "die", "Liebs\u00b7te", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "KON", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob das Verh\u00e4ngn\u00fc\u00df gleich uns von einander bricht,", "tokens": ["Ob", "das", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "gleich", "uns", "von", "ein\u00b7an\u00b7der", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PPER", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und g\u00f6nnet uns nicht uns, so bleibt doch unser Pflicht,", "tokens": ["und", "g\u00f6n\u00b7net", "uns", "nicht", "uns", ",", "so", "bleibt", "doch", "un\u00b7ser", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "PPER", "$,", "ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "so lange werden stehn des runden Himmels Scheiben.", "tokens": ["so", "lan\u00b7ge", "wer\u00b7den", "stehn", "des", "run\u00b7den", "Him\u00b7mels", "Schei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAINF", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Bezwinge dich durch dich und fall dir selbsten bei,", "tokens": ["Be\u00b7zwin\u00b7ge", "dich", "durch", "dich", "und", "fall", "dir", "selbs\u00b7ten", "bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "KON", "NN", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "gedenke meines Eids und sei des Zweifelns frei,", "tokens": ["ge\u00b7den\u00b7ke", "mei\u00b7nes", "Eids", "und", "sei", "des", "Zwei\u00b7felns", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "des Zweifelns, das, Lieb, dich mit diesem Trauren plaget.", "tokens": ["des", "Zwei\u00b7felns", ",", "das", ",", "Lieb", ",", "dich", "mit", "die\u00b7sem", "Trau\u00b7ren", "pla\u00b7get", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "$,", "NN", "$,", "PRF", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich will dein Treuer sein, dieweil ich werde sein.", "tokens": ["Ich", "will", "dein", "Treu\u00b7er", "sein", ",", "die\u00b7weil", "ich", "wer\u00b7de", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VAINF", "$,", "KOUS", "PPER", "VAFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wilst du denn \u00fcber di\u00df noch haben einen Schein,", "tokens": ["Wilst", "du", "denn", "\u00fc\u00b7ber", "di\u00df", "noch", "ha\u00b7ben", "ei\u00b7nen", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PDS", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so frag die Liebste selbst, ich habs ihr oft gesaget.", "tokens": ["so", "frag", "die", "Liebs\u00b7te", "selbst", ",", "ich", "habs", "ihr", "oft", "ge\u00b7sa\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}