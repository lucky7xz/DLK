{"textgrid.poem.38204": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach edler Schatz verzeih es mir,", "tokens": ["Ach", "ed\u00b7ler", "Schatz", "ver\u00b7zeih", "es", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich so sp\u00e4t bin kommen,", "tokens": ["Da\u00df", "ich", "so", "sp\u00e4t", "bin", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die grosse Lieb aus Herzens Begier", "tokens": ["Die", "gros\u00b7se", "Lieb", "aus", "Her\u00b7zens", "Be\u00b7gier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Hat mich dazu gezwungen.", "tokens": ["Hat", "mich", "da\u00b7zu", "ge\u00b7zwun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PAV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Jezt liegt mein Schatz, mein edler Schatz", "tokens": ["Jezt", "liegt", "mein", "Schatz", ",", "mein", "ed\u00b7ler", "Schatz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gar sanft in seinem Bettchen,", "tokens": ["Gar", "sanft", "in", "sei\u00b7nem", "Bett\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ey m\u00f6cht ich ihn ganz inniglich", "tokens": ["Ey", "m\u00f6cht", "ich", "ihn", "ganz", "in\u00b7nig\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit meiner Musik wecken.", "tokens": ["Mit", "mei\u00b7ner", "Mu\u00b7sik", "we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Erweck ich sie, erschreck ich sie,", "tokens": ["Er\u00b7weck", "ich", "sie", ",", "er\u00b7schreck", "ich", "sie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "$,", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es m\u00fc\u00dft mich herzlich reuen,", "tokens": ["Es", "m\u00fc\u00dft", "mich", "herz\u00b7lich", "reu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ach k\u00f6nnte ich doch bey ihr seyn,", "tokens": ["Ach", "k\u00f6nn\u00b7te", "ich", "doch", "bey", "ihr", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ruhn in ihren Armen.", "tokens": ["Und", "ruhn", "in", "ih\u00b7ren", "Ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie hat zwey Cristallinisch Stein,", "tokens": ["Sie", "hat", "zwey", "Cris\u00b7tal\u00b7li\u00b7nisch", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "NN", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Auf Elfenbein auch Purpur,", "tokens": ["Auf", "El\u00b7fen\u00b7bein", "auch", "Pur\u00b7pur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Solt ihr geschehn daran ein Leid,", "tokens": ["Solt", "ihr", "ge\u00b7schehn", "da\u00b7ran", "ein", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "PAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So spr\u00e4ng mein Herz in St\u00fccken.", "tokens": ["So", "spr\u00e4ng", "mein", "Herz", "in", "St\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}