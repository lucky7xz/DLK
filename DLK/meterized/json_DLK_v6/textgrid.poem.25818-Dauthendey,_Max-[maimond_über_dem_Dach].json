{"textgrid.poem.25818": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[maimond \u00fcber dem Dach]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Maimond \u00fcber dem Dach,", "tokens": ["Mai\u00b7mond", "\u00fc\u00b7ber", "dem", "Dach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Maimond sieht in das Haus,", "tokens": ["Mai\u00b7mond", "sieht", "in", "das", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Golden stehen die Scheiben,", "tokens": ["Gol\u00b7den", "ste\u00b7hen", "die", "Schei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Sehnsucht leuchtet heraus.", "tokens": ["Sehn\u00b7sucht", "leuch\u00b7tet", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Drau\u00dfen Blatt bei Blatt", "tokens": ["Drau\u00b7\u00dfen", "Blatt", "bei", "Blatt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Schlafen dunkel die B\u00e4ume,", "tokens": ["Schla\u00b7fen", "dun\u00b7kel", "die", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Drinnen unter dem Dach", "tokens": ["Drin\u00b7nen", "un\u00b7ter", "dem", "Dach"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Liegt die Liebe wach.", "tokens": ["Liegt", "die", "Lie\u00b7be", "wach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Schw\u00fcre gl\u00fchen im Dunkel,", "tokens": ["Schw\u00fc\u00b7re", "gl\u00fc\u00b7hen", "im", "Dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.10": {"text": "Funkeln hinaus in die Nacht.", "tokens": ["Fun\u00b7keln", "hin\u00b7aus", "in", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APZR", "APPR", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}