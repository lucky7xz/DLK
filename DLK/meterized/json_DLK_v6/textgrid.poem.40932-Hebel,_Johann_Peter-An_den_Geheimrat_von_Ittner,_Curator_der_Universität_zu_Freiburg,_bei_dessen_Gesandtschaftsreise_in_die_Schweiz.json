{"textgrid.poem.40932": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An den Geheimrat von Ittner, Curator der Universit\u00e4t zu Freiburg, bei dessen Gesandtschaftsreise in die Schweiz", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Se bh\u00fcetich Gott der Her, und z\u00fcrnet n\u00fct!", "tokens": ["Se", "bh\u00fce\u00b7tich", "Gott", "der", "Her", ",", "und", "z\u00fcr\u00b7net", "n\u00fct", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "ART", "NN", "$,", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Me schwezt, wie eim der Schnabel gwachsen isch.", "tokens": ["Me", "schwezt", ",", "wie", "eim", "der", "Schna\u00b7bel", "gwach\u00b7sen", "isch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PWAV", "ADJD", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gern ch\u00f6nnti's besser, aber 's will nit goh.", "tokens": ["Gern", "ch\u00f6nn\u00b7ti's", "bes\u00b7ser", ",", "a\u00b7ber", "'s", "will", "nit", "goh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "ADJD", "$,", "KON", "PPER", "VMFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Doch was vom Herze chunnt, isch au nit schlecht.", "tokens": ["Doch", "was", "vom", "Her\u00b7ze", "chunnt", ",", "isch", "au", "nit", "schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "VVFIN", "ADJD", "$,", "ADJD", "NE", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Chr\u00fcterma vo Badewiler het", "tokens": ["Der", "Chr\u00fc\u00b7ter\u00b7ma", "vo", "Ba\u00b7de\u00b7wi\u00b7ler", "het"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "NE", "NE", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "mer's mengmal gseit, und gfluecht derzu, es soll", "tokens": ["mer's", "meng\u00b7mal", "gseit", ",", "und", "gfluecht", "der\u00b7zu", ",", "es", "soll"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "KON", "VVFIN", "PAV", "$,", "PPER", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "kei Hypnum meh, kei Carex in der Welt", "tokens": ["kei", "Hyp\u00b7num", "meh", ",", "kei", "Ca\u00b7rex", "in", "der", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "NE", "$,", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "vor sini Auge cho (der Teufel wei\u00df,", "tokens": ["vor", "si\u00b7ni", "Au\u00b7ge", "cho", "(", "der", "Teu\u00b7fel", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "NE", "$(", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.9": {"text": "sin's Buben oder Meidli), wenn e Ma", "tokens": ["sin's", "Bu\u00b7ben", "o\u00b7der", "Meid\u00b7li", ")", ",", "wenn", "e", "Ma"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "NE", "$(", "$,", "KOUS", "NE", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "wie Ihr in siebe Here L\u00e4ndere seig.", "tokens": ["wie", "Ihr", "in", "sie\u00b7be", "He\u00b7re", "L\u00e4n\u00b7de\u00b7re", "seig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "I will's nit repetiere. Besser w\u00e4r's,", "tokens": ["I", "will's", "nit", "re\u00b7pe\u00b7tie\u00b7re", ".", "Bes\u00b7ser", "w\u00e4r's", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$.", "NN", "VAFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "der Chr\u00fcterma h\u00e4tt's au nit gseit; es isch", "tokens": ["der", "Chr\u00fc\u00b7ter\u00b7ma", "h\u00e4tt's", "au", "nit", "gseit", ";", "es", "isch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "NE", "PTKNEG", "NN", "$.", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "mit so me Fluech nit z'spasse. Het's der Recht", "tokens": ["mit", "so", "me", "Flu\u00b7ech", "nit", "z'\u00b7spas\u00b7se", ".", "Het's", "der", "Recht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN", "PTKNEG", "VVFIN", "$.", "NE", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "zum Ungl\u00fcck gh\u00f6rt, se gl\u00e4nzt mim Chr\u00fcterma", "tokens": ["zum", "Un\u00b7gl\u00fcck", "gh\u00f6rt", ",", "se", "gl\u00e4nzt", "mim", "Chr\u00fc\u00b7ter\u00b7ma"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "$,", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "kei Sternli meh vom blaue Himmelszelt,", "tokens": ["kei", "Stern\u00b7li", "meh", "vom", "blau\u00b7e", "Him\u00b7mels\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NE", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "kei Bl\u00fcemli meh im gr\u00fcene Mattegrund.", "tokens": ["kei", "Bl\u00fcem\u00b7li", "meh", "im", "gr\u00fce\u00b7ne", "Mat\u00b7te\u00b7grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NE", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Du arme Chetzer, Carex, Hypnum schie\u00dft", "tokens": ["Du", "ar\u00b7me", "Chet\u00b7zer", ",", "Ca\u00b7rex", ",", "Hyp\u00b7num", "schie\u00dft"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "NE", "$,", "NE", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "dim Aug eggege, wo de stohsch und gohsch.", "tokens": ["dim", "Aug", "eg\u00b7ge\u00b7ge", ",", "wo", "de", "stohsch", "und", "goh\u00b7sch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "PWAV", "NE", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "I mach kei Gspa\u00df, es isch mer selber so,", "tokens": ["I", "mach", "kei", "Gs\u00b7pa\u00df", ",", "es", "isch", "mer", "sel\u00b7ber", "so", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VVFIN", "PIAT", "NN", "$,", "PPER", "ADJD", "ADV", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.20": {"text": "und woni n\u00e4umen ane lueg, se stoht", "tokens": ["und", "wo\u00b7ni", "n\u00e4u\u00b7men", "a\u00b7ne", "lu\u00b7eg", ",", "se", "stoht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "FM", "FM", "FM", "FM", "$,", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "was hent der gmeint? E Hypnum? Nei, se stoht", "tokens": ["was", "hent", "der", "gmeint", "?", "E", "Hyp\u00b7num", "?", "Nei", ",", "se", "stoht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "VVFIN", "$.", "NN", "NE", "$.", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "libhaftig Euer Bildnu\u00df vor mim Aug,", "tokens": ["lib\u00b7haf\u00b7tig", "Eu\u00b7er", "Bild\u00b7nu\u00df", "vor", "mim", "Aug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "so fr\u00fcndlig und so lieb, und stirbi morn,", "tokens": ["so", "fr\u00fcnd\u00b7lig", "und", "so", "lieb", ",", "und", "stir\u00b7bi", "morn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$,", "KON", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "und siehnich n\u00fcmme, bis am j\u00fcngste Tag,", "tokens": ["und", "sieh\u00b7nich", "n\u00fcm\u00b7me", ",", "bis", "am", "j\u00fcngs\u00b7te", "Tag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "se chummi in mim goldne Sunntigrock,", "tokens": ["se", "chum\u00b7mi", "in", "mim", "gold\u00b7ne", "Sunn\u00b7ti\u00b7grock", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "(es hei\u00dft, mer werden alli neu gstaffiert),", "tokens": ["(", "es", "hei\u00dft", ",", "mer", "wer\u00b7den", "al\u00b7li", "neu", "gstaf\u00b7fiert", ")", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "ADV", "ADJD", "VVPP", "$(", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "und sag mim Kamerad, wo mit mer goht:", "tokens": ["und", "sag", "mim", "Ka\u00b7me\u00b7rad", ",", "wo", "mit", "mer", "goht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,", "PWAV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "\u00bbisch sel nit der Her Ittner, wo im Duft", "tokens": ["\u00bb", "isch", "sel", "nit", "der", "Her", "Itt\u00b7ner", ",", "wo", "im", "Duft"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADJD", "ADV", "PTKNEG", "ART", "NN", "NE", "$,", "PWAV", "APPRART", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.29": {"text": "d\u00f6rt an der Milchstro\u00df goht? Jez buckt er si,", "tokens": ["d\u00f6rt", "an", "der", "Milch\u00b7stro\u00df", "goht", "?", "Jez", "buckt", "er", "si", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$.", "NE", "VVFIN", "PPER", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "und bschaut e Bl\u00fcemli, 's wird Dudaim si.\u00ab", "tokens": ["und", "bschaut", "e", "Bl\u00fcem\u00b7li", ",", "'s", "wird", "Du\u00b7daim", "si", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "NE", "NE", "$,", "PPER", "VAFIN", "NE", "NE", "$.", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.31": {"text": "Drum laufi, was i laufe cha, d'Stro\u00df uf;", "tokens": ["Drum", "lau\u00b7fi", ",", "was", "i", "lau\u00b7fe", "cha", ",", "d'\u00b7St\u00b7ro\u00df", "uf", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "PWS", "FM", "FM", "FM", "$,", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "der Kamerad blibt zruck, er chunnt nit no.", "tokens": ["der", "Ka\u00b7me\u00b7rad", "blibt", "zruck", ",", "er", "chunnt", "nit", "no", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PTKNEG", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Druf sagi: \u00bbMit Verlaub! I mein emol,", "tokens": ["Druf", "sa\u00b7gi", ":", "\u00bb", "Mit", "Ver\u00b7laub", "!", "I", "mein", "e\u00b7mol", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$.", "$(", "APPR", "NN", "$.", "XY", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "der seiget's. Hani nit vor langer Zit", "tokens": ["der", "sei\u00b7get'", "s.", "Ha\u00b7ni", "nit", "vor", "lan\u00b7ger", "Zit"], "token_info": ["word", "word", "abbreviation", "word", "word", "word", "word", "word"], "pos": ["ART", "VVFIN", "NE", "NE", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "bim Kaiserwirt e Sch\u00f6pli mitich gha?", "tokens": ["bim", "Kai\u00b7ser\u00b7wirt", "e", "Sch\u00f6p\u00b7li", "mi\u00b7tich", "gha", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Wie hent der gschlofe? Wohl? Der Morgen isch", "tokens": ["Wie", "hent", "der", "gschlo\u00b7fe", "?", "Wohl", "?", "Der", "Mor\u00b7gen", "isch"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$.", "ADV", "$.", "ART", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "so heiter. Wemmer nit e wengeli", "tokens": ["so", "hei\u00b7ter", ".", "Wem\u00b7mer", "nit", "e", "wen\u00b7ge\u00b7li"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$.", "NN", "PTKNEG", "NE", "VVFIN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.38": {"text": "do ane sitze zue dem Amarant?\u00ab", "tokens": ["do", "a\u00b7ne", "sit\u00b7ze", "zue", "dem", "A\u00b7ma\u00b7rant", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "NE", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Jez bh\u00fcetich Gott, und spar ich frisch und gsund", "tokens": ["Jez", "bh\u00fce\u00b7tich", "Gott", ",", "und", "spar", "ich", "frisch", "und", "gsund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "NN", "$,", "KON", "VVFIN", "PPER", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "uf euer lange Berg- und Schwizerreis.", "tokens": ["uf", "eu\u00b7er", "lan\u00b7ge", "Ber\u00b7g", "und", "Schwi\u00b7zer\u00b7reis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.41": {"text": "'s het d'Milchstro\u00df uf, am j\u00fcngste Tag, no Zit", "tokens": ["'s", "het", "d'\u00b7Milch\u00b7stro\u00df", "uf", ",", "am", "j\u00fcngs\u00b7te", "Tag", ",", "no", "Zit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PDS", "PTKVZ", "$,", "APPRART", "ADJA", "NN", "$,", "NE", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "wohl hunderttausig Johr, und isch's denn d\u00f6rt", "tokens": ["wohl", "hun\u00b7dert\u00b7tau\u00b7sig", "Johr", ",", "und", "isch's", "denn", "d\u00f6rt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "CARD", "NN", "$,", "KON", "NE", "KON", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "viel sch\u00f6ner echt, as an der Limmat Gstad?", "tokens": ["viel", "sch\u00f6\u00b7ner", "echt", ",", "as", "an", "der", "Lim\u00b7mat", "Gstad", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$,", "PRELS", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Wie glitzert uffem See der Silberstaub!", "tokens": ["Wie", "glit\u00b7zert", "uf\u00b7fem", "See", "der", "Sil\u00b7ber\u00b7staub", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Wie wechsle hundertf\u00e4ltig F\u00e4rb und Glanz,", "tokens": ["Wie", "wechs\u00b7le", "hun\u00b7dert\u00b7f\u00e4l\u00b7tig", "F\u00e4rb", "und", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Pal\u00e4stl