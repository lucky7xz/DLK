{"textgrid.poem.59089": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "308. Der Reine bei den Reinen", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du des H\u00f6chsten Wohnhaus sein,", "tokens": ["Willst", "du", "des", "H\u00f6chs\u00b7ten", "Wohn\u00b7haus", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Halt Fleisch und Geist von Flecken rein,", "tokens": ["Halt", "Fleisch", "und", "Geist", "von", "Fle\u00b7cken", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sanft eingekehrt, ganz abgeschieden!", "tokens": ["Sanft", "ein\u00b7ge\u00b7kehrt", ",", "ganz", "ab\u00b7ge\u00b7schie\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gott wohnt in Reinheit und im Frieden.", "tokens": ["Gott", "wohnt", "in", "Rein\u00b7heit", "und", "im", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}