{"textgrid.poem.62504": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "3. Simeon", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr, ich kann in Frieden fahren,", "tokens": ["Herr", ",", "ich", "kann", "in", "Frie\u00b7den", "fah\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn dein Morgen r\u00f6thet sich,", "tokens": ["Denn", "dein", "Mor\u00b7gen", "r\u00f6\u00b7thet", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Hab' erharrt in langen Jahren,", "tokens": ["Hab'", "er\u00b7harrt", "in", "lan\u00b7gen", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was ich schaue sichtbarlich;", "tokens": ["Was", "ich", "schau\u00b7e", "sicht\u00b7bar\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was uns heilig zugeschworen,", "tokens": ["Was", "uns", "hei\u00b7lig", "zu\u00b7ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist wahrhaftig auch geschehn;", "tokens": ["Ist", "wahr\u00b7haf\u00b7tig", "auch", "ge\u00b7schehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieses Zeichen war erkoren", "tokens": ["Die\u00b7ses", "Zei\u00b7chen", "war", "er\u00b7ko\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vieler Fall und Auferstehn.", "tokens": ["Vie\u00b7ler", "Fall", "und", "Auf\u00b7er\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mag das Schwert zum Herzen dringen,", "tokens": ["Mag", "das", "Schwert", "zum", "Her\u00b7zen", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schallen soll der Glockenklang;", "tokens": ["Schal\u00b7len", "soll", "der", "Glo\u00b7cken\u00b7klang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hell und muthig will ich singen", "tokens": ["Hell", "und", "mut\u00b7hig", "will", "ich", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meinen letzten Schwanensang.", "tokens": ["Mei\u00b7nen", "letz\u00b7ten", "Schwa\u00b7nen\u00b7sang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Neues Leben hat begonnen,", "tokens": ["Neu\u00b7es", "Le\u00b7ben", "hat", "be\u00b7gon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jung und sch\u00f6n und wunderbar,", "tokens": ["Jung", "und", "sch\u00f6n", "und", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "All die alten Liebesbronnen", "tokens": ["All", "die", "al\u00b7ten", "Lie\u00b7bes\u00b7bron\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flie\u00dfen auch noch s\u00fc\u00df und klar.", "tokens": ["Flie\u00b7\u00dfen", "auch", "noch", "s\u00fc\u00df", "und", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn die Greise Kinder werden,", "tokens": ["Wenn", "die", "Grei\u00b7se", "Kin\u00b7der", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weisheit aus den Kindern spricht,", "tokens": ["Weis\u00b7heit", "aus", "den", "Kin\u00b7dern", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Spielet wieder auf der Erden", "tokens": ["Spie\u00b7let", "wie\u00b7der", "auf", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hell und frisch das Himmelslicht.", "tokens": ["Hell", "und", "frisch", "das", "Him\u00b7mels\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Herr, nun la\u00df den Diener ziehen,", "tokens": ["Herr", ",", "nun", "la\u00df", "den", "Die\u00b7ner", "zie\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVIMP", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df ihn von dem langen Thun,", "tokens": ["La\u00df", "ihn", "von", "dem", "lan\u00b7gen", "Thun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von den Sorgen, von den M\u00fchen", "tokens": ["Von", "den", "Sor\u00b7gen", ",", "von", "den", "M\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sanft in seinem Erbtheil ruhn.", "tokens": ["Sanft", "in", "sei\u00b7nem", "E\u00b7rbtheil", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}