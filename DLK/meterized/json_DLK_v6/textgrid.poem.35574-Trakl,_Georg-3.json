{"textgrid.poem.35574": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "3", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rosige Osterglocke im Grabgew\u00f6lbe der Nacht", "tokens": ["Ro\u00b7si\u00b7ge", "Os\u00b7ter\u00b7glo\u00b7cke", "im", "Grab\u00b7ge\u00b7w\u00f6l\u00b7be", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "NN", "ART", "NN"], "meter": "+--+-+--+-+--+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und die Silberstimmen der Sterne,", "tokens": ["Und", "die", "Sil\u00b7ber\u00b7stim\u00b7men", "der", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da\u00df in Schauern ein dunkler Wahnsinn von der Stirne des Schl\u00e4fers sank.", "tokens": ["Da\u00df", "in", "Schau\u00b7ern", "ein", "dunk\u00b7ler", "Wahn\u00b7sinn", "von", "der", "Stir\u00b7ne", "des", "Schl\u00e4\u00b7fers", "sank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "O wie stille ein Gang den blauen Flu\u00df hinab", "tokens": ["O", "wie", "stil\u00b7le", "ein", "Gang", "den", "blau\u00b7en", "Flu\u00df", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Vergessenes sinnend, da im gr\u00fcnen Ge\u00e4st", "tokens": ["Ver\u00b7ges\u00b7se\u00b7nes", "sin\u00b7nend", ",", "da", "im", "gr\u00fc\u00b7nen", "Ge\u00b7\u00e4st"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "$,", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Drossel ein Fremdes in den Untergang rief.", "tokens": ["Die", "Dros\u00b7sel", "ein", "Frem\u00b7des", "in", "den", "Un\u00b7ter\u00b7gang", "rief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Oder wenn er an der kn\u00f6chernen Hand des Greisen", "tokens": ["O\u00b7der", "wenn", "er", "an", "der", "kn\u00f6\u00b7cher\u00b7nen", "Hand", "des", "Grei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Abends vor die verfallene Mauer der Stadt ging", "tokens": ["A\u00b7bends", "vor", "die", "ver\u00b7fal\u00b7le\u00b7ne", "Mau\u00b7er", "der", "Stadt", "ging"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und jener in schwarzem Mantel ein rosiges Kindlein trug,", "tokens": ["Und", "je\u00b7ner", "in", "schwar\u00b7zem", "Man\u00b7tel", "ein", "ro\u00b7si\u00b7ges", "Kin\u00b7dlein", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Im Schatten des Nu\u00dfbaums der Geist des B\u00f6sen erschien.", "tokens": ["Im", "Schat\u00b7ten", "des", "Nu\u00df\u00b7baums", "der", "Geist", "des", "B\u00f6\u00b7sen", "er\u00b7schien", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+--+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Tasten \u00fcber die gr\u00fcnen Stufen des Sommers. O wie leise", "tokens": ["Tas\u00b7ten", "\u00fc\u00b7ber", "die", "gr\u00fc\u00b7nen", "Stu\u00b7fen", "des", "Som\u00b7mers", ".", "O", "wie", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$.", "NE", "KOKOM", "ADJD"], "meter": "+-+--+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Verfiel der Garten in der braunen Stille des Herbstes,", "tokens": ["Ver\u00b7fiel", "der", "Gar\u00b7ten", "in", "der", "brau\u00b7nen", "Stil\u00b7le", "des", "Herbs\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Duft und Schwermut des alten Hollunders,", "tokens": ["Duft", "und", "Schwer\u00b7mut", "des", "al\u00b7ten", "Hol\u00b7lun\u00b7ders", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Da in Sebastians Schatten die Silberstimme des Engels erstarb.", "tokens": ["Da", "in", "Se\u00b7bas\u00b7ti\u00b7ans", "Schat\u00b7ten", "die", "Sil\u00b7ber\u00b7stim\u00b7me", "des", "En\u00b7gels", "er\u00b7starb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+--+--+", "measure": "iambic.septa.relaxed"}}}}}