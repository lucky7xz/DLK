{"textgrid.poem.66562": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie du's ihnen Einmal recht gemacht,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie du's ihnen Einmal recht gemacht,", "tokens": ["Wie", "du's", "ih\u00b7nen", "Ein\u00b7mal", "recht", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "So wollen sie's immer haben,", "tokens": ["So", "wol\u00b7len", "sie's", "im\u00b7mer", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "VAFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und ob du zehnmal bessres erdacht,", "tokens": ["Und", "ob", "du", "zehn\u00b7mal", "bess\u00b7res", "er\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Sie hadern mit deinen Gaben.", "tokens": ["Sie", "ha\u00b7dern", "mit", "dei\u00b7nen", "Ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Was schiert sie, da\u00df dich das Leben gef\u00fchrt,", "tokens": ["Was", "schiert", "sie", ",", "da\u00df", "dich", "das", "Le\u00b7ben", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und anders dein M\u00fcssen und Sollen!", "tokens": ["Und", "an\u00b7ders", "dein", "M\u00fcs\u00b7sen", "und", "Sol\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Du sollst nur k\u00f6nnen, was sie ber\u00fchrt,", "tokens": ["Du", "sollst", "nur", "k\u00f6n\u00b7nen", ",", "was", "sie", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VMFIN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und kannst nichts, was sie nicht wollen.", "tokens": ["Und", "kannst", "nichts", ",", "was", "sie", "nicht", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "$,", "PRELS", "PPER", "PTKNEG", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Da\u00df du sie f\u00fchrest so wie du mu\u00dft,", "tokens": ["Da\u00df", "du", "sie", "f\u00fch\u00b7rest", "so", "wie", "du", "mu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "ADV", "KOKOM", "PPER", "VMFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nie werden sie dir's erlauben!", "tokens": ["Nie", "wer\u00b7den", "sie", "dir's", "er\u00b7lau\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "VVINF", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Das alte Lied und der alte Wust,", "tokens": ["Das", "al\u00b7te", "Lied", "und", "der", "al\u00b7te", "Wust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Man predigt Blinden und Tauben!", "tokens": ["Man", "pre\u00b7digt", "Blin\u00b7den", "und", "Tau\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "KON", "NN", "$."], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}