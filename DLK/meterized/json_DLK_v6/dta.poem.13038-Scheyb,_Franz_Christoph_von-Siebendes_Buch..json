{"dta.poem.13038": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ejhr kennt mich insgesammt, mich, jenen ", "tokens": ["\u201e", "jhr", "kennt", "mich", "ins\u00b7ge\u00b7sammt", ",", "mich", ",", "je\u00b7nen"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "$,", "PDAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u201ef\u00fcr den ", "tokens": ["\u201e", "f\u00fcr", "den"], "token_info": ["punct", "word", "word"], "pos": ["$(", "APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "\u201edaher bin ich von euch, ist es nicht so? beruffen,", "tokens": ["\u201e", "da\u00b7her", "bin", "ich", "von", "euch", ",", "ist", "es", "nicht", "so", "?", "be\u00b7ruf\u00b7fen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PAV", "VAFIN", "PPER", "APPR", "PPER", "$,", "VAFIN", "PPER", "PTKNEG", "ADV", "$.", "VVPP", "$,"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "\u201eda\u00df ich entschliessen soll, wer zu den Ehren-Stuffen", "tokens": ["\u201e", "da\u00df", "ich", "ent\u00b7schlies\u00b7sen", "soll", ",", "wer", "zu", "den", "Eh\u00b7ren\u00b7Stuf\u00b7fen"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "PPER", "VVINF", "VMFIN", "$,", "PWS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "265\u201dMit Recht versehen sey? was in dem Fall zu thun?", "tokens": ["\"", "Mit", "Recht", "ver\u00b7se\u00b7hen", "sey", "?", "was", "in", "dem", "Fall", "zu", "thun", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "VVPP", "VAFIN", "$.", "PWS", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ebey welcher mehr Verdienst\u2019, als bey der andern, ruhn?", "tokens": ["\u201e", "bey", "wel\u00b7cher", "mehr", "Ver\u00b7dienst'", ",", "als", "bey", "der", "an\u00b7dern", ",", "ruhn", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "PIAT", "NN", "$,", "KOUS", "APPR", "ART", "ADJA", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eso will ich ungef\u00e4rbt nur was ich dencke, sagen,", "tokens": ["\u201e", "so", "will", "ich", "un\u00b7ge\u00b7f\u00e4rbt", "nur", "was", "ich", "den\u00b7cke", ",", "sa\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "ADJD", "ADV", "PWS", "PPER", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ejedoch nicht meinen Rath als Richtschnur vorzuschlagen.", "tokens": ["\u201e", "je\u00b7doch", "nicht", "mei\u00b7nen", "Rath", "als", "Richt\u00b7schnur", "vor\u00b7zu\u00b7schla\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PTKNEG", "PPOSAT", "NN", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}