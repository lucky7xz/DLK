{"textgrid.poem.62982": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Drey Br\u00fcder schifften nach der Insel", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drey Br\u00fcder schifften nach der Insel", "tokens": ["Drey", "Br\u00fc\u00b7der", "schiff\u00b7ten", "nach", "der", "In\u00b7sel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Weisheit, die der blasse Pinsel", "tokens": ["Der", "Weis\u00b7heit", ",", "die", "der", "blas\u00b7se", "Pin\u00b7sel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Erdensohns nicht malen kann.", "tokens": ["Des", "Er\u00b7den\u00b7sohns", "nicht", "ma\u00b7len", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Itzt landen sie nach langem Pfl\u00fcgen", "tokens": ["Itzt", "lan\u00b7den", "sie", "nach", "lan\u00b7gem", "Pfl\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Des Oceans am Ufer an.", "tokens": ["Des", "O\u00b7ceans", "am", "U\u00b7fer", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Es war im Fr\u00fchling. Voll Vergn\u00fcgen", "tokens": ["Es", "war", "im", "Fr\u00fch\u00b7ling", ".", "Voll", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "$.", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sehn sie den Felsen vor sich liegen,", "tokens": ["Sehn", "sie", "den", "Fel\u00b7sen", "vor", "sich", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Auf dessen Scheitel der Altar", "tokens": ["Auf", "des\u00b7sen", "Schei\u00b7tel", "der", "Al\u00b7tar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der G\u00f6ttlichen gegr\u00fcndet war,", "tokens": ["Der", "G\u00f6tt\u00b7li\u00b7chen", "ge\u00b7gr\u00fcn\u00b7det", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Den Davids Sohn ihr einst erbaute.", "tokens": ["Den", "Da\u00b7vids", "Sohn", "ihr", "einst", "er\u00b7bau\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Er trug den goldenen Pokal,", "tokens": ["Er", "trug", "den", "gol\u00b7de\u00b7nen", "Po\u00b7kal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "In den ein fl\u00fc\u00dfiger Crystal", "tokens": ["In", "den", "ein", "fl\u00fc\u00b7\u00dfi\u00b7ger", "Crys\u00b7tal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Aus einer Rosenwolke thaute.", "tokens": ["Aus", "ei\u00b7ner", "Ro\u00b7sen\u00b7wol\u00b7ke", "thau\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Der j\u00fcngste Bruder eilt und klimmt", "tokens": ["Der", "j\u00fcngs\u00b7te", "Bru\u00b7der", "eilt", "und", "klimmt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Zuerst hinauf, springt hin und nimmt,", "tokens": ["Zu\u00b7erst", "hin\u00b7auf", ",", "springt", "hin", "und", "nimmt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "VVFIN", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Um ihn auf einmal auszuleeren,", "tokens": ["Um", "ihn", "auf", "ein\u00b7mal", "aus\u00b7zu\u00b7lee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "APPR", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Den Kelch und st\u00fcrzt ihn gierig ein.", "tokens": ["Den", "Kelch", "und", "st\u00fcrzt", "ihn", "gie\u00b7rig", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Doch schnell gerann der Trank zum Stein.", "tokens": ["Doch", "schnell", "ge\u00b7rann", "der", "Trank", "zum", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Umsonst war schl\u00fcrfen, r\u00fctteln, kehren;", "tokens": ["Um\u00b7sonst", "war", "schl\u00fcr\u00b7fen", ",", "r\u00fct\u00b7teln", ",", "keh\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Er setzt den Kelch verdrie\u00dflich hin,", "tokens": ["Er", "setzt", "den", "Kelch", "ver\u00b7drie\u00df\u00b7lich", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Entschlossen wieder heimzuziehn.", "tokens": ["Ent\u00b7schlos\u00b7sen", "wie\u00b7der", "heim\u00b7zu\u00b7ziehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Ich gehe mit, versetzt der zweyte;", "tokens": ["Ich", "ge\u00b7he", "mit", ",", "ver\u00b7setzt", "der", "zwey\u00b7te", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "VVFIN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Allein den Kelch nehm ich als Beute", "tokens": ["Al\u00b7lein", "den", "Kelch", "nehm", "ich", "als", "Beu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Von meiner Wallfahrt mit nach Haus", "tokens": ["Von", "mei\u00b7ner", "Wall\u00b7fahrt", "mit", "nach", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Und trink auf meinem Ruhebette,", "tokens": ["Und", "trink", "auf", "mei\u00b7nem", "Ru\u00b7he\u00b7bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Mit Sirup wohl versetzt, ihn aus.", "tokens": ["Mit", "Si\u00b7rup", "wohl", "ver\u00b7setzt", ",", "ihn", "aus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "VVPP", "$,", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Er fa\u00dft ihn; doch die Demantkette", "tokens": ["Er", "fa\u00dft", "ihn", ";", "doch", "die", "De\u00b7mant\u00b7ket\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "ADV", "ART", "NN"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.28": {"text": "Des Schicksals h\u00e4lt ihn mauerfest", "tokens": ["Des", "Schick\u00b7sals", "h\u00e4lt", "ihn", "mau\u00b7er\u00b7fest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "An dem Altar. Da\u00df dich die Pest!", "tokens": ["An", "dem", "Al\u00b7tar", ".", "Da\u00df", "dich", "die", "Pest", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "KOUS", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Rief er, kommt Br\u00fcder, la\u00dft uns eilen:", "tokens": ["Rief", "er", ",", "kommt", "Br\u00fc\u00b7der", ",", "la\u00dft", "uns", "ei\u00b7len", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "NN", "$,", "VVIMP", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Hier spuckt der Satan und sein Heer.", "tokens": ["Hier", "spuckt", "der", "Sa\u00b7tan", "und", "sein", "Heer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Ich, sprach der dritte, will hier weilen!", "tokens": ["Ich", ",", "sprach", "der", "drit\u00b7te", ",", "will", "hier", "wei\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVFIN", "ART", "ADJA", "$,", "VMFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Vielleicht \u2013 Schon h\u00f6ren sie nicht mehr.", "tokens": ["Viel\u00b7leicht", "\u2013", "Schon", "h\u00f6\u00b7ren", "sie", "nicht", "mehr", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Der neue Siedler l\u00e4\u00dft die Gecken", "tokens": ["Der", "neu\u00b7e", "Sied\u00b7ler", "l\u00e4\u00dft", "die", "Ge\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Von hinnen ziehn, baut sich ein Haus", "tokens": ["Von", "hin\u00b7nen", "ziehn", ",", "baut", "sich", "ein", "Haus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VVINF", "$,", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Von laubichten Wachholderhecken,", "tokens": ["Von", "lau\u00b7bich\u00b7ten", "Wach\u00b7hol\u00b7der\u00b7he\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-++-+--+-", "measure": "iambic.tetra.relaxed"}, "line.37": {"text": "Sucht Schw\u00e4mme, gr\u00e4bt sich Wurzeln aus,", "tokens": ["Sucht", "Schw\u00e4m\u00b7me", ",", "gr\u00e4bt", "sich", "Wur\u00b7zeln", "aus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "PRF", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Wovon ihm auch die bittern schmecken", "tokens": ["Wo\u00b7von", "ihm", "auch", "die", "bit\u00b7tern", "schme\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ART", "ADJA", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Und lechzet er im Mittagsstrahl", "tokens": ["Und", "lech\u00b7zet", "er", "im", "Mit\u00b7tags\u00b7strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Nach einem Trunk zum kargen Mahl;", "tokens": ["Nach", "ei\u00b7nem", "Trunk", "zum", "kar\u00b7gen", "Mahl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "So f\u00e4llt er zu des Altars F\u00fc\u00dfen", "tokens": ["So", "f\u00e4llt", "er", "zu", "des", "Al\u00b7tars", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Und ruft voll Inbrunst: la\u00df, o la\u00df", "tokens": ["Und", "ruft", "voll", "In\u00b7brunst", ":", "la\u00df", ",", "o", "la\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "NN", "$.", "VVIMP", "$,", "FM", "FM"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Zum Labsal vom geweihten Na\u00df", "tokens": ["Zum", "Lab\u00b7sal", "vom", "ge\u00b7weih\u00b7ten", "Na\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Mir, G\u00f6ttin, ein Paar Tropfen flie\u00dfen!", "tokens": ["Mir", ",", "G\u00f6t\u00b7tin", ",", "ein", "Paar", "Trop\u00b7fen", "flie\u00b7\u00dfen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.45": {"text": "Nie bat der biedre junge Mann", "tokens": ["Nie", "bat", "der", "bied\u00b7re", "jun\u00b7ge", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Vergebens; mit gest\u00e4rkter Seele", "tokens": ["Ver\u00b7ge\u00b7bens", ";", "mit", "ge\u00b7st\u00e4rk\u00b7ter", "See\u00b7le"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Griff er den Kelch. Der Balsam rann", "tokens": ["Griff", "er", "den", "Kelch", ".", "Der", "Bal\u00b7sam", "rann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.48": {"text": "Wie Muttermilch in seine Kehle,", "tokens": ["Wie", "Mut\u00b7ter\u00b7milch", "in", "sei\u00b7ne", "Keh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Und ehe noch vom Felsenhang", "tokens": ["Und", "e\u00b7he", "noch", "vom", "Fel\u00b7sen\u00b7hang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Das Lied der himmelblauen Meise", "tokens": ["Das", "Lied", "der", "him\u00b7mel\u00b7blau\u00b7en", "Mei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Den traubenreichen Herbst besang,", "tokens": ["Den", "trau\u00b7ben\u00b7rei\u00b7chen", "Herbst", "be\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "War er, was wenig werden \u2013 weise.", "tokens": ["War", "er", ",", "was", "we\u00b7nig", "wer\u00b7den", "\u2013", "wei\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PRELS", "PIS", "VAINF", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "O glaube, glaube mir, mein Sohn;", "tokens": ["O", "glau\u00b7be", ",", "glau\u00b7be", "mir", ",", "mein", "Sohn", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Uns f\u00fchrt kein flacher Sammeth\u00fcgel", "tokens": ["Uns", "f\u00fchrt", "kein", "fla\u00b7cher", "Sam\u00b7me\u00b7th\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, 