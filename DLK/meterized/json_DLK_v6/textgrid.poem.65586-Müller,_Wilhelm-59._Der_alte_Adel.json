{"textgrid.poem.65586": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "59. Der alte Adel", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst sprach zu mir ein faules Holz: Ich bin des Pfirsichstammes Sohn,", "tokens": ["J\u00fcngst", "sprach", "zu", "mir", "ein", "fau\u00b7les", "Holz", ":", "Ich", "bin", "des", "Pfir\u00b7sichs\u00b7tam\u00b7mes", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Der viel der edlen Fr\u00fcchte trug vor mehr als tausend Jahren schon.", "tokens": ["Der", "viel", "der", "ed\u00b7len", "Fr\u00fcch\u00b7te", "trug", "vor", "mehr", "als", "tau\u00b7send", "Jah\u00b7ren", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VVFIN", "APPR", "PIAT", "KOKOM", "CARD", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Ich warf es lachend in's Kamin. Was thu' ich mit dem leeren Wicht,", "tokens": ["Ich", "warf", "es", "la\u00b7chend", "in's", "Ka\u00b7min", ".", "Was", "thu'", "ich", "mit", "dem", "lee\u00b7ren", "Wicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$.", "PWS", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Der prahlerisch zu seinem Ruhm von alter Ahnen Thaten spricht?", "tokens": ["Der", "prah\u00b7le\u00b7risch", "zu", "sei\u00b7nem", "Ruhm", "von", "al\u00b7ter", "Ah\u00b7nen", "Tha\u00b7ten", "spricht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}}}}