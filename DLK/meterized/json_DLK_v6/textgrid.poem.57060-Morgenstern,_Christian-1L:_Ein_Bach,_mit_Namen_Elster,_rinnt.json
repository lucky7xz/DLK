{"textgrid.poem.57060": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Bach, mit Namen Elster, rinnt", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Bach, mit Namen Elster, rinnt", "tokens": ["Ein", "Bach", ",", "mit", "Na\u00b7men", "Els\u00b7ter", ",", "rinnt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "APPR", "NN", "NE", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "durch Nacht und Nebel und besinnt", "tokens": ["durch", "Nacht", "und", "Ne\u00b7bel", "und", "be\u00b7sinnt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "inmitten dieser stillen Handlung", "tokens": ["in\u00b7mit\u00b7ten", "die\u00b7ser", "stil\u00b7len", "Hand\u00b7lung"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "sich seiner einstigen Verwandlung,", "tokens": ["sich", "sei\u00b7ner", "eins\u00b7ti\u00b7gen", "Ver\u00b7wand\u00b7lung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "die ihm vor mehr als tausend Jahren", "tokens": ["die", "ihm", "vor", "mehr", "als", "tau\u00b7send", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PIAT", "KOKOM", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "von einem Magier widerfahren.", "tokens": ["von", "ei\u00b7nem", "Ma\u00b7gier", "wi\u00b7der\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wie so Nacht und Nebel weben,", "tokens": ["Und", "wie", "so", "Nacht", "und", "Ne\u00b7bel", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "erwacht in ihm das alte Leben.", "tokens": ["er\u00b7wacht", "in", "ihm", "das", "al\u00b7te", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er f\u00e4hrt in eine in der N\u00e4he", "tokens": ["Er", "f\u00e4hrt", "in", "ei\u00b7ne", "in", "der", "N\u00e4\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "zuf\u00e4llig eingeschlafne Kr\u00e4he", "tokens": ["zu\u00b7f\u00e4l\u00b7lig", "ein\u00b7ge\u00b7schlaf\u00b7ne", "Kr\u00e4\u00b7he"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "und fliegt, dieweil sein Bett verdorrt,", "tokens": ["und", "fliegt", ",", "die\u00b7weil", "sein", "Bett", "ver\u00b7dorrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "wie dermaleinst als Vogel fort.", "tokens": ["wie", "der\u00b7ma\u00b7leinst", "als", "Vo\u00b7gel", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "KOUS", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}