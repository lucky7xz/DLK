{"dta.poem.2080": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Katze.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die kleinen sind recht l\u00e4cherlich, ihr Gaukeln, ihr pos-", "tokens": ["Die", "klei\u00b7nen", "sind", "recht", "l\u00e4\u00b7cher\u00b7lich", ",", "ihr", "Gau\u00b7keln", ",", "ihr", "pos"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "VAFIN", "ADV", "ADJD", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "sierlichs Springen", "tokens": ["sier\u00b7lichs", "Sprin\u00b7gen"], "token_info": ["word", "word"], "pos": ["CARD", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Kann oft den, dem es nicht ums Herz zu lachen, doch", "tokens": ["Kann", "oft", "den", ",", "dem", "es", "nicht", "ums", "Herz", "zu", "la\u00b7chen", ",", "doch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VMFIN", "ADV", "ART", "$,", "PRELS", "PPER", "PTKNEG", "APPRART", "NN", "PTKZU", "VVINF", "$,", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "zum Lachen zwingen.", "tokens": ["zum", "La\u00b7chen", "zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Man findet wilde Katzen auch, die in den dicken W\u00e4ldern", "tokens": ["Man", "fin\u00b7det", "wil\u00b7de", "Kat\u00b7zen", "auch", ",", "die", "in", "den", "di\u00b7cken", "W\u00e4l\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADJA", "NN", "ADV", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "wohnen", "tokens": ["woh\u00b7nen"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Und das, was sie erbeuten k\u00f6nnen, als sehr gefr\u00e4\u00dfig,", "tokens": ["Und", "das", ",", "was", "sie", "er\u00b7beu\u00b7ten", "k\u00f6n\u00b7nen", ",", "als", "sehr", "ge\u00b7fr\u00e4\u00b7\u00dfig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "PRELS", "PPER", "VVINF", "VMINF", "$,", "KOUS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "nicht verschonen:", "tokens": ["nicht", "ver\u00b7scho\u00b7nen", ":"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Doch werden ihre sch\u00f6ne B\u00e4lge mit gro\u00dfem Nutzen ab-", "tokens": ["Doch", "wer\u00b7den", "ih\u00b7re", "sch\u00f6\u00b7ne", "B\u00e4l\u00b7ge", "mit", "gro\u00b7\u00dfem", "Nut\u00b7zen", "ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "gesetzt,", "tokens": ["ge\u00b7setzt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Zumal man selbe gegen Fl\u00fcsse f\u00fcr ein bewehrtes Mittel", "tokens": ["Zu\u00b7mal", "man", "sel\u00b7be", "ge\u00b7gen", "Fl\u00fcs\u00b7se", "f\u00fcr", "ein", "be\u00b7wehr\u00b7tes", "Mit\u00b7tel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVFIN", "APPR", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "sch\u00e4tzt,", "tokens": ["sch\u00e4tzt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Jmgleichen f\u00fcr die Wassersucht. Da\u00df also Katzen auch", "tokens": ["Jm\u00b7glei\u00b7chen", "f\u00fcr", "die", "Was\u00b7ser\u00b7sucht", ".", "Da\u00df", "al\u00b7so", "Kat\u00b7zen", "auch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$.", "KOUS", "ADV", "NN", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "imgleichen,", "tokens": ["im\u00b7glei\u00b7chen", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Nicht weniger als andre Thier, uns mannichfache Vor-", "tokens": ["Nicht", "we\u00b7ni\u00b7ger", "als", "and\u00b7re", "Thier", ",", "uns", "man\u00b7nich\u00b7fa\u00b7che", "Vor"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "KOUS", "ADJA", "NN", "$,", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.16": {"text": "thel reichen.", "tokens": ["thel", "rei\u00b7chen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}