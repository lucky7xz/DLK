{"dta.poem.13179": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eso zog der Prinz das Heer aus seinem Lorber-Wald.", "tokens": ["\u201e", "so", "zog", "der", "Prinz", "das", "Heer", "aus", "sei\u00b7nem", "Lor\u00b7ber\u00b7Wald", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eauch da war er begl\u00fcckt; der Feind erfuhr es bald.", "tokens": ["\u201e", "auch", "da", "war", "er", "be\u00b7gl\u00fcckt", ";", "der", "Feind", "er\u00b7fuhr", "es", "bald", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "VAFIN", "PPER", "VVPP", "$.", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "\u201eder Prinz schwung seinen Fahn so k\u00fchn auf dessen Schaaren,", "tokens": ["\u201e", "der", "Prinz", "schwung", "sei\u00b7nen", "Fahn", "so", "k\u00fchn", "auf", "des\u00b7sen", "Schaa\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "APPR", "PRELAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eda\u00df sie von ihrem Zweck, vom Land verdrungen waren.", "tokens": ["\u201e", "da\u00df", "sie", "von", "ih\u00b7rem", "Zweck", ",", "vom", "Land", "ver\u00b7drun\u00b7gen", "wa\u00b7ren", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "$,", "APPRART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "505\u201dKurz: dieses ist der Held, den keine Macht verlezt,", "tokens": ["\"", "Kurz", ":", "die\u00b7ses", "ist", "der", "Held", ",", "den", "kei\u00b7ne", "Macht", "ver\u00b7lezt", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$.", "PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ewann er sich seinem Feind mit Wiz entgegen sezt.", "tokens": ["\u201e", "wann", "er", "sich", "sei\u00b7nem", "Feind", "mit", "Wiz", "ent\u00b7ge\u00b7gen", "sezt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "PRF", "PPOSAT", "NN", "APPR", "NN", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eviel lieber wollt er sich mit eignem Blut besprizen,", "tokens": ["\u201e", "viel", "lie\u00b7ber", "wollt", "er", "sich", "mit", "eig\u00b7nem", "Blut", "be\u00b7spri\u00b7zen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "VMFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eals nicht die K\u00f6niginn mit seinem Arm besch\u00fczen.", "tokens": ["\u201e", "als", "nicht", "die", "K\u00f6\u00b7ni\u00b7ginn", "mit", "sei\u00b7nem", "Arm", "be\u00b7sch\u00fc\u00b7zen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PTKNEG", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}