{"textgrid.poem.24894": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Rot schimmert durch das Laubgr\u00fcn der Platane", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rot schimmert durch das Laubgr\u00fcn der Platane", "tokens": ["Rot", "schim\u00b7mert", "durch", "das", "Laub\u00b7gr\u00fcn", "der", "Pla\u00b7ta\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Mohrenburg, auf der die Halbmondfahne", "tokens": ["Die", "Moh\u00b7ren\u00b7burg", ",", "auf", "der", "die", "Halb\u00b7mond\u00b7fah\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "ART", "NN"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Durch acht Jahrhunderte geweht;", "tokens": ["Durch", "acht", "Jahr\u00b7hun\u00b7der\u00b7te", "ge\u00b7weht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Noch flammen Koranspr\u00fcche an dem Thore,", "tokens": ["Noch", "flam\u00b7men", "Ko\u00b7rans\u00b7pr\u00fc\u00b7che", "an", "dem", "Tho\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Noch an der Mauer rauscht die Sykomore", "tokens": ["Noch", "an", "der", "Mau\u00b7er", "rauscht", "die", "Sy\u00b7ko\u00b7mo\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zu Allah ein Gebet.", "tokens": ["Zu", "Al\u00b7lah", "ein", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ich schritt hinan; ringsum in Sprudelbronnen", "tokens": ["Ich", "schritt", "hi\u00b7nan", ";", "ring\u00b7sum", "in", "Spru\u00b7del\u00b7bron\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Silberb\u00e4chen rieselten die Wonnen,", "tokens": ["Und", "Sil\u00b7ber\u00b7b\u00e4\u00b7chen", "rie\u00b7sel\u00b7ten", "die", "Won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Die der Prophet verhei\u00dfen hat,", "tokens": ["Die", "der", "Pro\u00b7phet", "ver\u00b7hei\u00b7\u00dfen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wie ein Zauberschlo\u00df verschollner Sagen", "tokens": ["Und", "wie", "ein", "Zau\u00b7ber\u00b7schlo\u00df", "ver\u00b7scholl\u00b7ner", "Sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sah ich Gew\u00f6lbe, luft'ge Pfeiler ragen,", "tokens": ["Sah", "ich", "Ge\u00b7w\u00f6l\u00b7be", ",", "luft'\u00b7ge", "Pfei\u00b7ler", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als ich den Myrtenhof betrat.", "tokens": ["Als", "ich", "den", "Myr\u00b7ten\u00b7hof", "be\u00b7trat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Im Lichtglanz, der von Saal zu Saale spr\u00fchte,", "tokens": ["Im", "Licht\u00b7glanz", ",", "der", "von", "Saal", "zu", "Saa\u00b7le", "spr\u00fch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "APPR", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erschlo\u00df sich knospend das Gestein und bl\u00fchte", "tokens": ["Er\u00b7schlo\u00df", "sich", "knos\u00b7pend", "das", "Ge\u00b7stein", "und", "bl\u00fch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADJD", "ART", "NN", "KON", "VVFIN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Farbreich um Wand und S\u00e4ulenknauf;", "tokens": ["Farb\u00b7reich", "um", "Wand", "und", "S\u00e4u\u00b7len\u00b7knauf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ew'gem Klingen sprudelten Kaskaden", "tokens": ["Mit", "ew'\u00b7gem", "Klin\u00b7gen", "spru\u00b7del\u00b7ten", "Kas\u00b7ka\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zum Laubendach der schlanken Kolonnaden", "tokens": ["Zum", "Lau\u00b7ben\u00b7dach", "der", "schlan\u00b7ken", "Ko\u00b7lon\u00b7na\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den Silberregen auf.", "tokens": ["Den", "Sil\u00b7ber\u00b7re\u00b7gen", "auf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ein Hauch von Eden, Bote ew'ger Freude,", "tokens": ["Ein", "Hauch", "von", "E\u00b7den", ",", "Bo\u00b7te", "ew'\u00b7ger", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durchzitterte das blitzende Gest\u00e4ude;", "tokens": ["Durch\u00b7zit\u00b7ter\u00b7te", "das", "blit\u00b7zen\u00b7de", "Ge\u00b7st\u00e4u\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Der Bogengang am L\u00f6wenhof", "tokens": ["Der", "Bo\u00b7gen\u00b7gang", "am", "L\u00f6\u00b7wen\u00b7hof"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schien Nebeln gleich im Morgenwind zu schwanken,", "tokens": ["Schien", "Ne\u00b7beln", "gleich", "im", "Mor\u00b7gen\u00b7wind", "zu", "schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Indessen schimmernd von Gezweig und Ranken", "tokens": ["In\u00b7des\u00b7sen", "schim\u00b7mernd", "von", "Ge\u00b7zweig", "und", "Ran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Tau herniedertroff.", "tokens": ["Der", "Tau", "her\u00b7nie\u00b7der\u00b7troff", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Aus Rosenkelchen str\u00f6mte sinnbet\u00e4ubend", "tokens": ["Aus", "Ro\u00b7sen\u00b7kel\u00b7chen", "str\u00f6m\u00b7te", "sinn\u00b7be\u00b7t\u00e4u\u00b7bend"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Woll\u00fcst'ger Duft, in leichten Flocken st\u00e4ubend,", "tokens": ["Wol\u00b7l\u00fcst'\u00b7ger", "Duft", ",", "in", "leich\u00b7ten", "Flo\u00b7cken", "st\u00e4u\u00b7bend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Wie K\u00fcsse von dem Mund der Braut;", "tokens": ["Wie", "K\u00fcs\u00b7se", "von", "dem", "Mund", "der", "Braut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und an der Wand die rankenden Gedichte,", "tokens": ["Und", "an", "der", "Wand", "die", "ran\u00b7ken\u00b7den", "Ge\u00b7dich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sich l\u00f6send, atmend in dem Morgenlichte,", "tokens": ["Sich", "l\u00f6\u00b7send", ",", "at\u00b7mend", "in", "dem", "Mor\u00b7gen\u00b7lich\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Entsandten einen Jubellaut.", "tokens": ["Ent\u00b7sand\u00b7ten", "ei\u00b7nen", "Ju\u00b7bel\u00b7laut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O Ton, der meiner Kindheit oft erklungen,", "tokens": ["O", "Ton", ",", "der", "mei\u00b7ner", "Kind\u00b7heit", "oft", "er\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit dem mich Geister oft in Schlaf gesungen", "tokens": ["Mit", "dem", "mich", "Geis\u00b7ter", "oft", "in", "Schlaf", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "NN", "ADV", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im sonn'gen Thal und dunklen Hain:", "tokens": ["Im", "sonn'\u00b7gen", "Thal", "und", "dunk\u00b7len", "Hain", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hier t\u00f6nst du, lang verstummter, mir entgegen,", "tokens": ["Hier", "t\u00f6nst", "du", ",", "lang", "ver\u00b7stumm\u00b7ter", ",", "mir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "ADJA", "$,", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und jauchzend f\u00e4llt mein Herz mit schnellern Schl\u00e4gen", "tokens": ["Und", "jauch\u00b7zend", "f\u00e4llt", "mein", "Herz", "mit", "schnel\u00b7lern", "Schl\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In deinen Jubel ein.", "tokens": ["In", "dei\u00b7nen", "Ju\u00b7bel", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}