{"dta.poem.7772": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Die Geschichte des Davids,  \n K\u00f6nigs in Juda.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198429", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weil aber es also nun mit dem K\u00f6nig stuhnde,", "tokens": ["Weil", "a\u00b7ber", "es", "al\u00b7so", "nun", "mit", "dem", "K\u00f6\u00b7nig", "stuhn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "ADJA", "$,"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ahinoam, die nicht f\u00fcr rathsamlich befunde,", "tokens": ["A\u00b7hi\u00b7no\u00b7am", ",", "die", "nicht", "f\u00fcr", "rath\u00b7sam\u00b7lich", "be\u00b7fun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PTKNEG", "APPR", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df das ihr Ehgemahl, wie es in Juda war,", "tokens": ["Da\u00df", "das", "ihr", "Eh\u00b7ge\u00b7mahl", ",", "wie", "es", "in", "Ju\u00b7da", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPOSAT", "NN", "$,", "PWAV", "PPER", "APPR", "NE", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sogleich erfahren m\u00fc\u00dft, wolt bergen die Gefahr;", "tokens": ["Sog\u00b7leich", "er\u00b7fah\u00b7ren", "m\u00fc\u00dft", ",", "wolt", "ber\u00b7gen", "die", "Ge\u00b7fahr", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "$,", "VMFIN", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.5": {"text": "Weil sie gar wohl bedacht, es w\u00fcrde weiter kommen", "tokens": ["Weil", "sie", "gar", "wohl", "be\u00b7dacht", ",", "es", "w\u00fcr\u00b7de", "wei\u00b7ter", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVPP", "$,", "PPER", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Des K\u00f6nigs Traurigkeit, wann er di\u00df h\u00e4tt vernommen,", "tokens": ["Des", "K\u00f6\u00b7nigs", "Trau\u00b7rig\u00b7keit", ",", "wann", "er", "di\u00df", "h\u00e4tt", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PWAV", "PPER", "PDS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und wo Betr\u00fcbni\u00df ist, folgt eine Furcht hernach,", "tokens": ["Und", "wo", "Be\u00b7tr\u00fcb\u00b7ni\u00df", "ist", ",", "folgt", "ei\u00b7ne", "Furcht", "her\u00b7nach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "VAFIN", "$,", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df uns offt allzuschwer scheint eine leichte Sach.", "tokens": ["Da\u00df", "uns", "offt", "all\u00b7zu\u00b7schwer", "scheint", "ei\u00b7ne", "leich\u00b7te", "Sach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Drum solte Jonathan allein dem Feind entgegen.", "tokens": ["Drum", "sol\u00b7te", "Jo\u00b7na\u00b7than", "al\u00b7lein", "dem", "Feind", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "NE", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dennoch nach langem Rath, nach vielem Ueberlegen,", "tokens": ["Den\u00b7noch", "nach", "lan\u00b7gem", "Rath", ",", "nach", "vie\u00b7lem", "Ue\u00b7ber\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Findt Abner es f\u00fcr gut, da\u00df ers dem K\u00f6nig sagt.", "tokens": ["Findt", "Ab\u00b7ner", "es", "f\u00fcr", "gut", ",", "da\u00df", "ers", "dem", "K\u00f6\u00b7nig", "sagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPER", "APPR", "ADJD", "$,", "KOUS", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Er gieng in sein Gemach des Morgens unbefragt,", "tokens": ["Er", "gieng", "in", "sein", "Ge\u00b7mach", "des", "Mor\u00b7gens", "un\u00b7be\u00b7fragt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Als Saul noch lag zur Ruh: doch wann nur Ruh zu nennen", "tokens": ["Als", "Saul", "noch", "lag", "zur", "Ruh", ":", "doch", "wann", "nur", "Ruh", "zu", "nen\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADV", "VVFIN", "APPRART", "NN", "$.", "KON", "PWAV", "ADV", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Schreckens-volle Tr\u00e4um, das innerliche Brennen,", "tokens": ["Die", "Schre\u00b7ckens\u00b7vol\u00b7le", "Tr\u00e4um", ",", "das", "in\u00b7ner\u00b7li\u00b7che", "Bren\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die Schwermuth seines Geists, die ihn niemahls verlie\u00df,", "tokens": ["Die", "Schwer\u00b7muth", "sei\u00b7nes", "Geists", ",", "die", "ihn", "nie\u00b7mahls", "ver\u00b7lie\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "So da\u00df sein Schlaffen recht ein stetes Wachen hie\u00df.", "tokens": ["So", "da\u00df", "sein", "Schlaf\u00b7fen", "recht", "ein", "ste\u00b7tes", "Wa\u00b7chen", "hie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPOSAT", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Sobald er Abner sah, fieng sein Hertz an zu schlagen,", "tokens": ["So\u00b7bald", "er", "Ab\u00b7ner", "sah", ",", "fi\u00b7eng", "sein", "Hertz", "an", "zu", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Und furchte sich f\u00fcr was, das er nicht konnte sagen,", "tokens": ["Und", "furch\u00b7te", "sich", "f\u00fcr", "was", ",", "das", "er", "nicht", "konn\u00b7te", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PRELS", "$,", "PRELS", "PPER", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Er reichte ihm die Hand, und sah ihn zitternd an,", "tokens": ["Er", "reich\u00b7te", "ihm", "die", "Hand", ",", "und", "sah", "ihn", "zit\u00b7ternd", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und Abner bringt es f\u00fcr, so gut er immer kan.", "tokens": ["Und", "Ab\u00b7ner", "bringt", "es", "f\u00fcr", ",", "so", "gut", "er", "im\u00b7mer", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "APPR", "$,", "ADV", "ADJD", "PPER", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Mein K\u00f6nig, sagte er, es giebt ein neues Siegen,", "tokens": ["Mein", "K\u00f6\u00b7nig", ",", "sag\u00b7te", "er", ",", "es", "giebt", "ein", "neu\u00b7es", "Sie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Gott hat dir was geschickt, dein Hertze zu vergn\u00fcgen,", "tokens": ["Gott", "hat", "dir", "was", "ge\u00b7schickt", ",", "dein", "Hert\u00b7ze", "zu", "ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PIS", "VVPP", "$,", "PPOSAT", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Man sieht, dir dienet nicht die stille Einsamkeit,", "tokens": ["Man", "sieht", ",", "dir", "die\u00b7net", "nicht", "die", "stil\u00b7le", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PPER", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Du bist nur kranck, weil du nicht mehr ziehst in den Streit.", "tokens": ["Du", "bist", "nur", "kranck", ",", "weil", "du", "nicht", "mehr", "ziehst", "in", "den", "Streit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Schau doch ein Volck von Gad k\u00f6m\u0303t an, und will es wagen,", "tokens": ["Schau", "doch", "ein", "Volck", "von", "Gad", "k\u00f6m\u0303t", "an", ",", "und", "will", "es", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "APPR", "NE", "VVFIN", "PTKVZ", "$,", "KON", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Sich mit dir, Grosser Saul! in einem Streit zu schlagen,", "tokens": ["Sich", "mit", "dir", ",", "Gros\u00b7ser", "Saul", "!", "in", "ei\u00b7nem", "Streit", "zu", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "$,", "ADJA", "NN", "$.", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Bey Socho stehen sie, und seynd dazu bestimmt,", "tokens": ["Bey", "So\u00b7cho", "ste\u00b7hen", "sie", ",", "und", "seynd", "da\u00b7zu", "be\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPER", "$,", "KON", "VAFIN", "PAV", "VVPP", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.28": {"text": "Da\u00df dort dein Helden-Muth ihr Toben ihnen nimmt.", "tokens": ["Da\u00df", "dort", "dein", "Hel\u00b7den\u00b7Muth", "ihr", "To\u00b7ben", "ih\u00b7nen", "nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der K\u00f6nig schwiege still, und dacht in seinen Sinnen,", "tokens": ["Der", "K\u00f6\u00b7nig", "schwie\u00b7ge", "still", ",", "und", "dacht", "in", "sei\u00b7nen", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wo ist mein erster Muth, mein tapferes Beginnen?", "tokens": ["Wo", "ist", "mein", "ers\u00b7ter", "Muth", ",", "mein", "tap\u00b7fe\u00b7res", "Be\u00b7gin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wo find ich Saul in mir? ich bin sein Bild allein,", "tokens": ["Wo", "find", "ich", "Saul", "in", "mir", "?", "ich", "bin", "sein", "Bild", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "NE", "APPR", "PPER", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Was ehmahls war mein Wunsch, macht mir anjetzo Pein.", "tokens": ["Was", "eh\u00b7mahls", "war", "mein", "Wunsch", ",", "macht", "mir", "an\u00b7je\u00b7tzo", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Als Abner nun kein Wort vom K\u00f6nig konnte bringen,", "tokens": ["Als", "Ab\u00b7ner", "nun", "kein", "Wort", "vom", "K\u00f6\u00b7nig", "konn\u00b7te", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "PIAT", "NN", "APPRART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Vergieng ihm schier der Muth, da\u00df es ihm w\u00fcrd gelingen.", "tokens": ["Ver\u00b7gieng", "ihm", "schier", "der", "Muth", ",", "da\u00df", "es", "ihm", "w\u00fcrd", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "$,", "KOUS", "PPER", "PPER", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Doch sagt er noch einmahl, wie ist dann Saul verkehrt,", "tokens": ["Doch", "sagt", "er", "noch", "ein\u00b7mahl", ",", "wie", "ist", "dann", "Saul", "ver\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "$,", "PWAV", "VAFIN", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da\u00df er sein Volck nicht mehr zu sch\u00fctzen nun begehrt?", "tokens": ["Da\u00df", "er", "sein", "Volck", "nicht", "mehr", "zu", "sch\u00fct\u00b7zen", "nun", "be\u00b7gehrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "ADV", "PTKZU", "VVINF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Auf K\u00f6nig thu dein Amt, da\u00df Juda werd befreyet.", "tokens": ["Auf", "K\u00f6\u00b7nig", "thu", "dein", "Amt", ",", "da\u00df", "Ju\u00b7da", "werd", "be\u00b7fre\u00b7yet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "NE", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.38": {"text": "Ach! brache Saul heraus, ob zwar mein Hertz nicht scheuet", "tokens": ["Ach", "!", "bra\u00b7che", "Saul", "he\u00b7raus", ",", "ob", "zwar", "mein", "Hertz", "nicht", "scheu\u00b7et"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "NE", "PTKVZ", "$,", "KOUS", "ADV", "PPOSAT", "NN", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Di\u00df unbeschnittne Volck, so ist doch Gott von mir;", "tokens": ["Di\u00df", "un\u00b7be\u00b7schnitt\u00b7ne", "Volck", ",", "so", "ist", "doch", "Gott", "von", "mir", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "$,", "ADV", "VAFIN", "ADV", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Was kan ich ohne Gott? Gott sagte mir sonst f\u00fcr,", "tokens": ["Was", "kan", "ich", "oh\u00b7ne", "Gott", "?", "Gott", "sag\u00b7te", "mir", "sonst", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "NN", "$.", "NN", "VVFIN", "PPER", "ADV", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Des Siegs gewi\u00df zu seyn; der hat mich nun verlassen,", "tokens": ["Des", "Siegs", "ge\u00b7wi\u00df", "zu", "seyn", ";", "der", "hat", "mich", "nun", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VAINF", "$.", "ART", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Sein Geist denckt nicht an mich, sein Will ist mich zu hassen.", "tokens": ["Sein", "Geist", "denckt", "nicht", "an", "mich", ",", "sein", "Will", "ist", "mich", "zu", "has\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "APPR", "PPER", "$,", "VAINF", "VMFIN", "VAFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Ey! sprache Abner drauf, das ist ja nur ein Wahn.", "tokens": ["Ey", "!", "spra\u00b7che", "Ab\u00b7ner", "drauf", ",", "das", "ist", "ja", "nur", "ein", "Wahn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADJA", "NN", "PTKVZ", "$,", "PDS", "VAFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Ach! sagte Saul, wer es also empfinden kan", "tokens": ["Ach", "!", "sag\u00b7te", "Saul", ",", "wer", "es", "al\u00b7so", "emp\u00b7fin\u00b7den", "kan"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "NE", "$,", "PWS", "PPER", "ADV", "VVINF", "VMFIN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.45": {"text": "Als ich, der wird gewi\u00df mit mir nur dahin zielen,", "tokens": ["Als", "ich", ",", "der", "wird", "ge\u00b7wi\u00df", "mit", "mir", "nur", "da\u00b7hin", "zie\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PRELS", "VAFIN", "ADV", "APPR", "PPER", "ADV", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Was H\u00f6llenangst es sey, nicht Gottes Gnad zu f\u00fchlen.", "tokens": ["Was", "H\u00f6l\u00b7len\u00b7angst", "es", "sey", ",", "nicht", "Got\u00b7tes", "Gnad", "zu", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "VAFIN", "$,", "PTKNEG", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Hiemit erbla\u00dfte Saul, er \u00e4chzt und seufzte tief,", "tokens": ["Hie\u00b7mit", "er\u00b7bla\u00df\u00b7te", "Saul", ",", "er", "\u00e4chzt", "und", "seufz\u00b7te", "tief", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$,", "PPER", "VVFIN", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Ein starcker Thr\u00e4nen-Bach ihm aus den Augen lief.", "tokens": ["Ein", "star\u00b7cker", "Thr\u00e4\u00b7nen\u00b7Bach", "ihm", "aus", "den", "Au\u00b7gen", "lief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Sein gantzer Leib erstarrt, sein Hertz hub an zu springen,", "tokens": ["Sein", "gant\u00b7zer", "Leib", "er\u00b7starrt", ",", "sein", "Hertz", "hub", "an", "zu", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$,", "PPOSAT", "NN", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Die Lippen r\u00fchrten sich ohn etwas f\u00fcrzubringen.", "tokens": ["Die", "Lip\u00b7pen", "r\u00fchr\u00b7ten", "sich", "ohn", "et\u00b7was", "f\u00fcr\u00b7zu\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Dem Abner wurde bang, er rieffe oft und viel,", "tokens": ["Dem", "Ab\u00b7ner", "wur\u00b7de", "bang", ",", "er", "rief\u00b7fe", "oft", "und", "viel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PPER", "VVFIN", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Da\u00df doch der Knab bald k\u00e4m mit seinem Saitenspiel.", "tokens": ["Da\u00df", "doch", "der", "Knab", "bald", "k\u00e4m", "mit", "sei\u00b7nem", "Sai\u00b7ten\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Der sch\u00f6ne David kam und r\u00fchrte seine Saiten,", "tokens": ["Der", "sch\u00f6\u00b7ne", "Da\u00b7vid", "kam", "und", "r\u00fchr\u00b7te", "sei\u00b7ne", "Sai\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Damit die Lieblichkeit sich mehrt, stuhnd er von weiten,", "tokens": ["Da\u00b7mit", "die", "Lieb\u00b7lich\u00b7keit", "sich", "mehrt", ",", "stuhnd", "er", "von", "wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PRF", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.55": {"text": "Und als der Harffenklang zum K\u00f6nig drang hinein,", "tokens": ["Und", "als", "der", "Harf\u00b7fen\u00b7klang", "zum", "K\u00f6\u00b7nig", "drang", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Entwich der Trauer-Geist und liesse Saul allein.", "tokens": ["Ent\u00b7wich", "der", "Trau\u00b7e\u00b7rGeist", "und", "lies\u00b7se", "Saul", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "KON", "VVFIN", "NE", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Er richtet sich empor, und seine matte Glieder", "tokens": ["Er", "rich\u00b7tet", "sich", "em\u00b7por", ",", "und", "sei\u00b7ne", "mat\u00b7te", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Bekamen ihre Ruh und neue St\u00e4rcke wieder,", "tokens": ["Be\u00b7ka\u00b7men", "ih\u00b7re", "Ruh", "und", "neu\u00b7e", "St\u00e4r\u00b7cke", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KON", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Er lachte David an, den er gantz innig liebt,", "tokens": ["Er", "lach\u00b7te", "Da\u00b7vid", "an", ",", "den", "er", "gantz", "in\u00b7nig", "liebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "PTKVZ", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.60": {"text": "Weil dessen Saitenspiel, ihm s\u00fcssen Labsaal giebt.", "tokens": ["Weil", "des\u00b7sen", "Sai\u00b7ten\u00b7spiel", ",", "ihm", "s\u00fcs\u00b7sen", "Lab\u00b7saal", "giebt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELAT", "NN", "$,", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Wo warst du doch mein Sohn, sprach er zu diesem Knaben,", "tokens": ["Wo", "warst", "du", "doch", "mein", "Sohn", ",", "sprach", "er", "zu", "die\u00b7sem", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Ohn dich kan ja mein Hertz nie wahre Ruhe haben;", "tokens": ["Ohn", "dich", "kan", "ja", "mein", "Hertz", "nie", "wah\u00b7re", "Ru\u00b7he", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VMFIN", "ADV", "PPOSAT", "NN", "ADV", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Herr K\u00f6nig, sprach der Knab aus f\u00fcrgenommnen Rath,", "tokens": ["Herr", "K\u00f6\u00b7nig", ",", "sprach", "der", "Knab", "aus", "f\u00fcr\u00b7ge\u00b7nomm\u00b7nen", "Rath", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Ich h\u00f6rte an wie man zum Streit Beliebung hat,", "tokens": ["Ich", "h\u00f6r\u00b7te", "an", "wie", "man", "zum", "Streit", "Be\u00b7lie\u00b7bung", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KOKOM", "PIS", "APPRART", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Wie Jonathan der Printz, und deiner Helden Schaaren,", "tokens": ["Wie", "Jo\u00b7na\u00b7than", "der", "Printz", ",", "und", "dei\u00b7ner", "Hel\u00b7den", "Schaa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ART", "NN", "$,", "KON", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Nach Socho hin verlangt, da Ecron soll erfahren,", "tokens": ["Nach", "So\u00b7cho", "hin", "ver\u00b7langt", ",", "da", "E\u00b7cron", "soll", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "VVPP", "$,", "KOUS", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.67": {"text": "Was Jacobs Gott vermag. Ja! trat hie Abner zu,", "tokens": ["Was", "Ja\u00b7cobs", "Gott", "ver\u00b7mag", ".", "Ja", "!", "trat", "hie", "Ab\u00b7ner", "zu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "NN", "VVFIN", "$.", "PTKANT", "$.", "VVFIN", "ADV", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Und sprach, all deine Leut die haben keine Ruh,", "tokens": ["Und", "sprach", ",", "all", "dei\u00b7ne", "Leut", "die", "ha\u00b7ben", "kei\u00b7ne", "Ruh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PIAT", "PPOSAT", "NN", "ART", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Sie wollen f\u00fcr den Feind und mit dir K\u00f6nig siegen,", "tokens": ["Sie", "wol\u00b7len", "f\u00fcr", "den", "Feind", "und", "mit", "dir", "K\u00f6\u00b7nig", "sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "KON", "APPR", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "Wann Saul nur bey uns ist, so mu\u00df der Feind erliegen.", "tokens": ["Wann", "Saul", "nur", "bey", "uns", "ist", ",", "so", "mu\u00df", "der", "Feind", "er\u00b7lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADV", "APPR", "PPER", "VAFIN", "$,", "ADV", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "Hiezu kam Jonathan und stuhnde Abner bey,", "tokens": ["Hie\u00b7zu", "kam", "Jo\u00b7na\u00b7than", "und", "stuhn\u00b7de", "Ab\u00b7ner", "bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NE", "KON", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "Auch selbst Ahinoam sagt, da\u00df es n\u00f6thig sey.", "tokens": ["Auch", "selbst", "A\u00b7hi\u00b7no\u00b7am", "sagt", ",", "da\u00df", "es", "n\u00f6\u00b7thig", "sey", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NE", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.73": {"text": "Der K\u00f6nig der f\u00fcr sie wolt keine Zagheit weisen,", "tokens": ["Der", "K\u00f6\u00b7nig", "der", "f\u00fcr", "sie", "wolt", "kei\u00b7ne", "Zag\u00b7heit", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPR", "PPER", "VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.74": {"text": "Die er gar nicht besa\u00df, hub nunmehr an zu preisen,", "tokens": ["Die", "er", "gar", "nicht", "be\u00b7sa\u00df", ",", "hub", "nun\u00b7mehr", "an", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PTKNEG", "VVFIN", "$,", "VVFIN", "ADV", "APPR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.75": {"text": "Der andern ihren Muth und gienge alles ein,", "tokens": ["Der", "an\u00b7dern", "ih\u00b7ren", "Muth", "und", "gien\u00b7ge", "al\u00b7les", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPOSAT", "NN", "KON", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.76": {"text": "Er wolte f\u00fcr dem Heer auch selbst der Erste seyn.", "tokens": ["Er", "wol\u00b7te", "f\u00fcr", "dem", "Heer", "auch", "selbst", "der", "Ers\u00b7te", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "ADV", "ADV", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.77": {"text": "Ahinoam erfreut, so muthig ihn zu sehen,", "tokens": ["A\u00b7hi\u00b7no\u00b7am", "er\u00b7freut", ",", "so", "mut\u00b7hig", "ihn", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ADV", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.78": {"text": "Sah niemahls ihn so gern in eine Feldschlacht gehen.", "tokens": ["Sah", "nie\u00b7mahls", "ihn", "so", "gern", "in", "ei\u00b7ne", "Feld\u00b7schlacht", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.79": {"text": "Thalmais, Jonathans vertrautes Ehgemahl,", "tokens": ["Thal\u00b7mais", ",", "Jo\u00b7na\u00b7thans", "ver\u00b7trau\u00b7tes", "Eh\u00b7ge\u00b7mahl", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.80": {"text": "H\u00f6rt diesen Feldzug an gleich einen Donnerknall.", "tokens": ["H\u00f6rt", "die\u00b7sen", "Feld\u00b7zug", "an", "gleich", "ei\u00b7nen", "Don\u00b7ner\u00b7knall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDAT", "NN", "APPR", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.81": {"text": "Sie wu\u00dft wie Jonathan sich pflegte nie zu scheuen,", "tokens": ["Sie", "wu\u00dft", "wie", "Jo\u00b7na\u00b7than", "sich", "pfleg\u00b7te", "nie", "zu", "scheu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NE", "PRF", "VVFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.82": {"text": "Und h\u00f6rt aus seinem Wort, aus seinem dapfern Dr\u00e4uen,", "tokens": ["Und", "h\u00f6rt", "aus", "sei\u00b7nem", "Wort", ",", "aus", "sei\u00b7nem", "dap\u00b7fern", "Dr\u00e4u\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.83": {"text": "Wie es solt Ecron gehn, drum war ihr Hertz und Sinn", "tokens": ["Wie", "es", "solt", "E\u00b7cron", "gehn", ",", "drum", "war", "ihr", "Hertz", "und", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VMFIN", "NN", "VVINF", "$,", "PAV", "VAFIN", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.84": {"text": "Voll Liebes-Traurigkeit, doch weil sie sich darinn", "tokens": ["Voll", "Lie\u00b7bes\u00b7Trau\u00b7rig\u00b7keit", ",", "doch", "weil", "sie", "sich", "da\u00b7rinn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "$,", "ADV", "KOUS", "PPER", "PRF", "PAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.85": {"text": "F\u00fcr ihme bergen must, weil er nicht wol kont leiden,", "tokens": ["F\u00fcr", "ih\u00b7me", "ber\u00b7gen", "must", ",", "weil", "er", "nicht", "wol", "kont", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "VMFIN", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.86": {"text": "Da\u00df das ihr Klagen war, was er vollf\u00fchrt mit Freuden;", "tokens": ["Da\u00df", "das", "ihr", "Kla\u00b7gen", "war", ",", "was", "er", "voll\u00b7f\u00fchrt", "mit", "Freu\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPOSAT", "NN", "VAFIN", "$,", "PWS", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.87": {"text": "Daher verschwieg sie nur, was ihr ein Trauren bracht,", "tokens": ["Da\u00b7her", "ver\u00b7schwieg", "sie", "nur", ",", "was", "ihr", "ein", "Trau\u00b7ren", "bracht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "$,", "PWS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.88": {"text": "Und was ihr Hertz beweint, dasselb ihr Mund belacht.", "tokens": ["Und", "was", "ihr", "Hertz", "be\u00b7weint", ",", "das\u00b7selb", "ihr", "Mund", "be\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "NN", "VVPP", "$,", "PDAT", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.89": {"text": "Die Merob und Michal die wollen bey ihr bleiben,", "tokens": ["Die", "Me\u00b7rob", "und", "Mic\u00b7hal", "die", "wol\u00b7len", "bey", "ihr", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NE", "ART", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+--++-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.90": {"text": "Mit ihr die lange Weil indessen zu vertreiben,", "tokens": ["Mit", "ihr", "die", "lan\u00b7ge", "Weil", "in\u00b7des\u00b7sen", "zu", "ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.91": {"text": "Und weil nun Jonathan den Abschied nehmen will,", "tokens": ["Und", "weil", "nun", "Jo\u00b7na\u00b7than", "den", "Ab\u00b7schied", "neh\u00b7men", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "NE", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.92": {"text": "Umf\u00e4ht sie ihn und spricht: Wag dich auch nicht zu viel!", "tokens": ["Um\u00b7f\u00e4ht", "sie", "ihn", "und", "spricht", ":", "Wag", "dich", "auch", "nicht", "zu", "viel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "KON", "VVFIN", "$.", "VVIMP", "PPER", "ADV", "PTKNEG", "PTKA", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.93": {"text": "Er schaut sie lieblich an, und ohn ein Wort zu sprechen", "tokens": ["Er", "schaut", "sie", "lieb\u00b7lich", "an", ",", "und", "ohn", "ein", "Wort", "zu", "spre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "KON", "APPR", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.94": {"text": "Setzt er sich auf sein Pferd, den Unmuth ihr zu brechen,", "tokens": ["Setzt", "er", "sich", "auf", "sein", "Pferd", ",", "den", "Un\u00b7muth", "ihr", "zu", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.95": {"text": "So ritt er was er kan, gantz Jsrael war da,", "tokens": ["So", "ritt", "er", "was", "er", "kan", ",", "gantz", "Js\u00b7rael", "war", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PWS", "PPER", "VMFIN", "$,", "ADV", "NE", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.96": {"text": "Ein jeder r\u00fcstet sich, weil schon der Feind so nah.", "tokens": ["Ein", "je\u00b7der", "r\u00fcs\u00b7tet", "sich", ",", "weil", "schon", "der", "Feind", "so", "nah", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PRF", "$,", "KOUS", "ADV", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}