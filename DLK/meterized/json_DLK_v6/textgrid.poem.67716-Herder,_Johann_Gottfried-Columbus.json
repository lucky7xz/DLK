{"textgrid.poem.67716": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Columbus", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ha, Sch\u00f6pfer Colon! Ha, wie hast Du uns die Welt", "tokens": ["Ha", ",", "Sch\u00f6p\u00b7fer", "Co\u00b7lon", "!", "Ha", ",", "wie", "hast", "Du", "uns", "die", "Welt"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "NN", "NE", "$.", "ITJ", "$,", "PWAV", "VAFIN", "PPER", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Land und Volk und Silbergeld", "tokens": ["Mit", "Land", "und", "Volk", "und", "Sil\u00b7ber\u00b7geld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Schmuck und Zier und Wissenschaft", "tokens": ["Und", "Schmuck", "und", "Zier", "und", "Wis\u00b7sen\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ums Viertheil uns vermehret!", "tokens": ["Ums", "Vier\u00b7theil", "uns", "ver\u00b7meh\u00b7ret", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ach, M\u00f6rder Colon! ach, wie hast Du uns die Welt", "tokens": ["Ach", ",", "M\u00f6r\u00b7der", "Co\u00b7lon", "!", "ach", ",", "wie", "hast", "Du", "uns", "die", "Welt"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "NN", "NE", "$.", "XY", "$,", "PWAV", "VAFIN", "PPER", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Alles, was sie Sch\u00f6nes h\u00e4lt,", "tokens": ["Und", "Al\u00b7les", ",", "was", "sie", "Sch\u00f6\u00b7nes", "h\u00e4lt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "PPER", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Reiz, Sitte, Leben, Jugendkraft,", "tokens": ["Reiz", ",", "Sit\u00b7te", ",", "Le\u00b7ben", ",", "Ju\u00b7gend\u00b7kraft", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Deinem Gift verheeret!", "tokens": ["Mit", "Dei\u00b7nem", "Gift", "ver\u00b7hee\u00b7ret", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}