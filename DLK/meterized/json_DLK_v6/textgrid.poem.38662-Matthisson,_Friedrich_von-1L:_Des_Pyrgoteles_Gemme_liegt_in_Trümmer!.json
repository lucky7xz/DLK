{"textgrid.poem.38662": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Des Pyrgoteles Gemme liegt in Tr\u00fcmmer!", "genre": "verse", "period": "N.A.", "pub_year": 1791, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Pyrgoteles Gemme liegt in Tr\u00fcmmer!", "tokens": ["Des", "Pyr\u00b7go\u00b7te\u00b7les", "Gem\u00b7me", "liegt", "in", "Tr\u00fcm\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lang' am Nektarpokal der Huldg\u00f6ttinnen", "tokens": ["Lang'", "am", "Nek\u00b7tar\u00b7po\u00b7kal", "der", "Huld\u00b7g\u00f6t\u00b7tin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Selbst den Musen ein Wunder schien Homeros", "tokens": ["Selbst", "den", "Mu\u00b7sen", "ein", "Wun\u00b7der", "schien", "Ho\u00b7me\u00b7ros"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VVFIN", "NE"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Apotheose.", "tokens": ["A\u00b7pot\u00b7he\u00b7o\u00b7se", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Diesen k\u00f6stlichen Stein, o K\u00fcnstler, senden", "tokens": ["Die\u00b7sen", "k\u00f6st\u00b7li\u00b7chen", "Stein", ",", "o", "K\u00fcnst\u00b7ler", ",", "sen\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PDAT", "ADJA", "NN", "$,", "FM", "NN", "$,", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Dir die Grazien, eine Tempelrunde", "tokens": ["Dir", "die", "Gra\u00b7zi\u00b7en", ",", "ei\u00b7ne", "Tem\u00b7pel\u00b7run\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ART", "NN", "$,", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Auf geschlifnem Azur dem Kelch zur neuen", "tokens": ["Auf", "ge\u00b7schlif\u00b7nem", "A\u00b7zur", "dem", "Kelch", "zur", "neu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "APPRART", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Zierde zu bilden.", "tokens": ["Zier\u00b7de", "zu", "bil\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Auf jonischen S\u00e4ulen la\u00df die Kuppel", "tokens": ["Auf", "jo\u00b7ni\u00b7schen", "S\u00e4u\u00b7len", "la\u00df", "die", "Kup\u00b7pel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVIMP", "ART", "NN"], "meter": "++--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ruhn; am Blumenaltar erheb' ein Brustbild,", "tokens": ["Ruhn", ";", "am", "Blu\u00b7men\u00b7al\u00b7tar", "er\u00b7heb'", "ein", "Brust\u00b7bild", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Das mit heiligem Laub' Apoll und Paphos", "tokens": ["Das", "mit", "hei\u00b7li\u00b7gem", "Laub'", "A\u00b7poll", "und", "Pa\u00b7phos"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ADJA", "NN", "NE", "KON", "NN"], "meter": "+-+--++-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Herrscherin kr\u00e4nzen.", "tokens": ["Herr\u00b7sche\u00b7rin", "kr\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Dieser Adern Gewebe m\u00fc\u00df' als Lorbeer", "tokens": ["Die\u00b7ser", "A\u00b7dern", "Ge\u00b7we\u00b7be", "m\u00fc\u00df'", "als", "Lor\u00b7beer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "NN", "VMFIN", "KOUS", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Des Unsterblichen Denkmal \u00fcberbreiten;", "tokens": ["Des", "U\u00b7nsterb\u00b7li\u00b7chen", "Denk\u00b7mal", "\u00fc\u00b7berb\u00b7rei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bildwerk schm\u00fccke den Fries, den Altar Wielands", "tokens": ["Bild\u00b7werk", "schm\u00fc\u00b7cke", "den", "Fries", ",", "den", "Al\u00b7tar", "Wie\u00b7lands"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Ewiger Name.", "tokens": ["E\u00b7wi\u00b7ger", "Na\u00b7me", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}